    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4f6b2639-24fe-3474-9d0d-60cd6793183d",
                    "sense_definition": "Дать имя или название кому-либо или чему-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "naming_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Родители долго спорили, как <назвать> своего первенца.",
                            "target_lang_translations": {
                                "en": "The parents argued for a long time about how to <w>name</w> their firstborn."
                            }
                        },
                        {
                            "text": "Мы решили <назвать> нашу новую лодку «Победа».",
                            "target_lang_translations": {
                                "en": "We decided to <w>name</w> our new boat \"Victory\"."
                            }
                        }
                    ],
                    "synonyms": [
                        "наречь",
                        "поименовать"
                    ],
                    "common_phrases": [
                        "назвать сына",
                        "назвать улицу",
                        "назвать в честь"
                    ],
                    "target_lang_definitions": {
                        "en": "To give a name or title to someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "name",
                                "target_lang_sense_clarification": "to give a name to"
                            },
                            {
                                "target_lang_word": "call",
                                "target_lang_sense_clarification": "to give a name to"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5991676-9319-39ab-9678-3fa365187fc2",
                    "sense_definition": "Произнести вслух, сообщить или упомянуть (имя, цену, дату и т.п.).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "naming_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Продавец отказался <назвать> точную цену товара без осмотра.",
                            "target_lang_translations": {
                                "en": "The seller refused to <w>name</w> the exact price of the item without an inspection."
                            }
                        },
                        {
                            "text": "Свидетель не смог <назвать> имя человека, которого видел ночью.",
                            "target_lang_translations": {
                                "en": "The witness could not <w>give</w> the name of the person he saw at night."
                            }
                        }
                    ],
                    "common_phrases": [
                        "назвать цену",
                        "назвать причину",
                        "назвать дату",
                        "назвать имя"
                    ],
                    "target_lang_definitions": {
                        "en": "To say aloud, communicate, or mention (a name, price, date, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "name",
                                "target_lang_sense_clarification": "to mention or specify (a price, date, etc.)"
                            },
                            {
                                "target_lang_word": "state",
                                "target_lang_sense_clarification": "to express definitely"
                            },
                            {
                                "target_lang_word": "give",
                                "target_lang_sense_clarification": "to provide (a name, reason, etc.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fa37831-5960-3bdf-945d-6f19a1896010",
                    "sense_definition": "Охарактеризовать кого-либо или что-либо, дать оценку, счесть кем-либо или чем-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "evaluation_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я не могу <назвать> этот фильм интересным, он довольно скучный.",
                            "target_lang_translations": {
                                "en": "I cannot <w>call</w> this film interesting; it is quite boring."
                            }
                        },
                        {
                            "text": "Его поступок трудно <назвать> умным, скорее это была ошибка.",
                            "target_lang_translations": {
                                "en": "His action is hard to <w>call</w> smart; rather, it was a mistake."
                            }
                        }
                    ],
                    "synonyms": [
                        "охарактеризовать"
                    ],
                    "common_phrases": [
                        "назвать ошибкой",
                        "назвать глупостью",
                        "назвать другом"
                    ],
                    "target_lang_definitions": {
                        "en": "To characterize someone or something; to evaluate or consider as."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "call",
                                "target_lang_sense_clarification": "to describe or characterize as"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dae13f17-3d4b-3a38-ae5b-f9ff1d7a1f38",
                    "sense_definition": "Пригласить, созвать большое количество людей (обычно гостей).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gathering_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На свой юбилей дедушка решил <назвать> полный дом гостей.",
                            "target_lang_translations": {
                                "en": "For his jubilee, Grandpa decided to <w>invite</w> a full house of guests."
                            }
                        },
                        {
                            "text": "Хозяйка умудрилась <назвать> столько народу, что всем не хватило стульев.",
                            "target_lang_translations": {
                                "en": "The hostess managed to <w>invite</w> so many people that there weren't enough chairs for everyone."
                            }
                        }
                    ],
                    "common_phrases": [
                        "назвать гостей"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies inviting a large quantity of people"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To invite or summon a large number of people (usually guests)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "invite",
                                "target_lang_sense_clarification": "to ask to come"
                            },
                            {
                                "target_lang_word": "summon",
                                "target_lang_sense_clarification": "to call people together"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
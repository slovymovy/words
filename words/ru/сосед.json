    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cd690280-a6f8-32d8-a1f7-457b326ea570",
                    "sense_definition": "Человек, который живёт вблизи, рядом с кем-либо (в одном доме, на одной лестничной клетке, на одной улице).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "person_nearby",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наш новый <сосед> оказался очень вежливым и тихим человеком.",
                            "target_lang_translations": {
                                "en": "Our new <neighbor> turned out to be a very polite and quiet person."
                            }
                        },
                        {
                            "text": "Я часто встречаю своего <соседа> в магазине рядом с домом.",
                            "target_lang_translations": {
                                "en": "I often meet my <neighbor> at the store near my house."
                            }
                        }
                    ],
                    "common_phrases": [
                        "новый сосед",
                        "сосед по квартире",
                        "сосед по комнате",
                        "сосед по даче",
                        "шумный сосед"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who lives near or next to someone else (in the same house, apartment building, or street)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "neighbor",
                                "target_lang_sense_clarification": "person living nearby (US spelling)"
                            },
                            {
                                "target_lang_word": "neighbour",
                                "target_lang_sense_clarification": "person living nearby (UK spelling)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd4b2737-7aa0-3af7-ab8b-39bf76f2164f",
                    "sense_definition": "Человек, который находится или занимает место рядом с кем-либо (в транспорте, театре, учебном классе или за столом).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_nearby",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой <сосед> по парте помог мне решить сложную задачу по математике.",
                            "target_lang_translations": {
                                "en": "My <neighbor> at the desk helped me solve a difficult math problem."
                            }
                        },
                        {
                            "text": "В самолёте мой <сосед> спал почти весь полёт.",
                            "target_lang_translations": {
                                "en": "On the plane, my <neighbor> slept for almost the entire flight."
                            }
                        }
                    ],
                    "common_phrases": [
                        "сосед по парте",
                        "сосед по столу",
                        "сосед справа",
                        "сосед слева",
                        "случайный сосед"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who is sitting or standing next to someone (in transport, a theater, a classroom, or at a table)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "neighbor",
                                "target_lang_sense_clarification": "person sitting next to someone"
                            },
                            {
                                "target_lang_word": "seatmate",
                                "target_lang_sense_clarification": "person sharing a seat or sitting nearby"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "483cdde7-dd38-3bf9-ae32-c4f5a7c07a20",
                    "sense_definition": "Государство или страна, имеющая общую границу с другой страной.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Китай — это наш южный <сосед> и важный торговый партнёр.",
                            "target_lang_translations": {
                                "en": "China is our southern <neighbor> and an important trading partner."
                            }
                        },
                        {
                            "text": "Правительство старается поддерживать мирные отношения со всеми <соседями>.",
                            "target_lang_translations": {
                                "en": "The government tries to maintain peaceful relations with all <neighbors>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "восточный сосед",
                        "ближайший сосед",
                        "мирные соседи"
                    ],
                    "target_lang_definitions": {
                        "en": "A state or country that shares a common border with another country."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "neighbor",
                                "target_lang_sense_clarification": "adjacent country"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "1f2fae5d-4c48-3d23-b231-a7ac3f5b95cf",
                    "sense_definition": "W sposób niegodziwy, hańbiący lub łamiący zasady moralne.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "moral_wickedness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zdrajca postąpił <bezecnie>, wydając swoich dawnych towarzyszy w ręce wroga.",
                            "target_lang_translations": {
                                "ru": "Предатель поступил <w>гнусно</w>, выдав своих бывших товарищей в руки врага."
                            }
                        },
                        {
                            "text": "Oskarżono go o to, że <bezecnie> wykorzystał naiwność i zaufanie ubogich ludzi.",
                            "target_lang_translations": {
                                "ru": "Его обвинили в том, что он <w>бесчестно</w> воспользовался наивностью и доверием бедных людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "niecnie"
                    ],
                    "antonyms": [
                        "szlachetnie",
                        "godnie"
                    ],
                    "common_phrases": [
                        "postąpić bezecnie",
                        "zachować się bezecnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подлым, позорным образом или нарушая моральные принципы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гнусно",
                                "target_lang_sense_clarification": "вызывая отвращение низостью поступка"
                            },
                            {
                                "target_lang_word": "бесчестно",
                                "target_lang_sense_clarification": "поступая против чести и совести"
                            },
                            {
                                "target_lang_word": "подло",
                                "target_lang_sense_clarification": "низко и вероломно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9d354e2-ad52-3265-8c28-3985a9e59511",
                    "sense_definition": "W sposób nieprzyzwoity, gorszący lub wywołujący oburzenie z powodu naruszenia norm obyczajowych.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "obscenity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna uśmiechał się <bezecnie>, opowiadając gorszące historie przy wspólnym stole.",
                            "target_lang_translations": {
                                "ru": "Мужчина <w>непристойно</w> ухмылялся, рассказывая возмутительные истории за общим столом."
                            }
                        },
                        {
                            "text": "Nie wypada odzywać się tak <bezecnie> i wulgarnie w obecności starszych osób.",
                            "target_lang_translations": {
                                "ru": "Не подобает выражаться так <w>непотребно</w> и вульгарно в присутствии пожилых людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieprzyzwoicie",
                        "sprośnie",
                        "zdrożnie"
                    ],
                    "antonyms": [
                        "przyzwoicie"
                    ],
                    "common_phrases": [
                        "żartować bezecnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неприличным, возмутительным образом или нарушая нормы приличия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непристойно",
                                "target_lang_sense_clarification": "нарушая правила приличия"
                            },
                            {
                                "target_lang_word": "бесстыдно",
                                "target_lang_sense_clarification": "не испытывая стыда, нагло"
                            },
                            {
                                "target_lang_word": "непотребно",
                                "target_lang_sense_clarification": "развратно или вульгарно (устар./книжн.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

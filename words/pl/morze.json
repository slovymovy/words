            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b863c06d-56e3-3efd-8d3c-384107783866",
                    "sense_definition": "Duży, naturalny zbiornik słonej wody, będący częścią oceanu lub z nim połączony.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "water_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W te wakacje całą rodziną jedziemy na urlop nad <w>morze</w>.",
                            "target_lang_translations": {
                                "ru": "В эти каникулы мы всей семьей едем в отпуск на <w>море</w>."
                            }
                        },
                        {
                            "text": "Statek wypłynął na otwarte <w>morze</w> wczesnym rankiem.",
                            "target_lang_translations": {
                                "ru": "Корабль вышел в открытое <w>море</w> рано утром."
                            }
                        }
                    ],
                    "antonyms": [
                        "ląd"
                    ],
                    "common_phrases": [
                        "nad morzem",
                        "otwarte morze",
                        "poziom morza",
                        "wilk morski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупный естественный водоем с соленой водой, являющийся частью океана или соединенный с ним."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "море",
                                "target_lang_sense_clarification": "часть океана"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ff80b08-2696-35d0-9537-7d9c6994bbb6",
                    "sense_definition": "Bardzo duża, trudna do policzenia ilość czegoś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Powszechnie wiadomo, że studenci potrafią wypić <w>morze</w> piwa.",
                            "target_lang_translations": {
                                "ru": "Всем известно, что студенты могут выпить <w>море</w> пива."
                            }
                        },
                        {
                            "text": "Dziewczyna wylała <w>morze</w> łez po rozstaniu z chłopakiem.",
                            "target_lang_translations": {
                                "ru": "Девушка пролила <w>море</w> слез после расставания с парнем."
                            }
                        }
                    ],
                    "synonyms": [
                        "mnóstwo",
                        "ogrom",
                        "masa",
                        "bezlik",
                        "bezmiar"
                    ],
                    "common_phrases": [
                        "morze łez",
                        "kropla w morzu",
                        "morze ognia",
                        "morze głów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень большое, неисчислимое количество чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "море",
                                "target_lang_sense_clarification": "огромное количество"
                            },
                            {
                                "target_lang_word": "океан",
                                "target_lang_sense_clarification": "перен. изобилие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04163d03-594f-33ca-8bd4-7f895ba92406",
                    "sense_definition": "Forma celownika i miejscownika liczby pojedynczej rzeczownika <w>mora</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W językoznawstwie poświęcono wiele uwagi <w>morze</w> jako jednostce iloczasu.",
                            "target_lang_translations": {
                                "ru": "В языкознании уделено много внимания <w>море</w> как единице долготы звука."
                            }
                        },
                        {
                            "text": "Przyglądał się tej dziwnej <w>morze</w> na tkaninie, zastanawiając się nad jej wzorem.",
                            "target_lang_translations": {
                                "ru": "Он приглядывался к этому странному <w>муару</w> на ткани, раздумывая над его узором."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "od: mora"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма дательного и местного падежей единственного числа существительного <w>mora</w> (мора или муар)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "море",
                                "target_lang_sense_clarification": "форма слова «мора» (лингвистика)"
                            },
                            {
                                "target_lang_word": "муару",
                                "target_lang_sense_clarification": "форма слова «муар» (ткань)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65548790-a12d-3377-8f32-c4db548254dd",
                    "sense_definition": "Forma miejscownika i wołacza liczby pojedynczej rzeczownika <w>mór</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starych kronikach czytamy o strasznym <w>morze</w>, który zdziesiątkował ludność miasta.",
                            "target_lang_translations": {
                                "ru": "В старых хрониках мы читаем о страшном <w>море</w>, который выкосил население города."
                            }
                        },
                        {
                            "text": "O <w>morze</w>, tyś zabrał nam wszystko, co kochaliśmy!",
                            "target_lang_translations": {
                                "ru": "О <w>мор</w>, ты забрал у нас всё, что мы любили!"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "od: mór"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма местного и звательного падежей единственного числа существительного <w>mór</w> (мор, эпидемия)."

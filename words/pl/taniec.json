    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "805dca59-fa2b-3ff4-a984-eba1a7f088df",
                    "sense_definition": "Zespół rytmicznych ruchów ciała wykonywanych zazwyczaj w takt muzyki, służący rozrywce lub wyrażaniu emocji.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_core",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ich pierwszy <taniec> na weselu wzruszył wszystkich gości.",
                            "target_lang_translations": {
                                "ru": "Их первый <w>танец</w> на свадьбе растрогал всех гостей."
                            }
                        },
                        {
                            "text": "Ten energiczny <taniec> wymaga od tancerzy doskonałej kondycji.",
                            "target_lang_translations": {
                                "ru": "Этот энергичный <w>танец</w> требует от танцоров отличной физической формы."
                            }
                        }
                    ],
                    "synonyms": [
                        "pląs",
                        "tan"
                    ],
                    "common_phrases": [
                        "szkoła tańca",
                        "lekcja tańca",
                        "prosić do tańca",
                        "nauka tańca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ритмичные движения тела, выполняемые обычно под музыку для развлечения или выражения эмоций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "танец",
                                "target_lang_sense_clarification": "движение под музыку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34e72724-dece-3e3d-a71d-df014ce4c8cc",
                    "sense_definition": "Konkretny rodzaj lub styl układu tanecznego, charakterystyczny dla danej kultury, epoki lub gatunku muzycznego.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_core",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Walc to bardzo elegancki <taniec> wykonywany w parach.",
                            "target_lang_translations": {
                                "ru": "Вальс — это очень элегантный <w>танец</w>, исполняемый в парах."
                            }
                        },
                        {
                            "text": "Każdy region Polski ma swój tradycyjny <taniec> ludowy.",
                            "target_lang_translations": {
                                "ru": "У каждого региона Польши есть свой традиционный народный <w>танец</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "taniec towarzyski",
                        "taniec ludowy",
                        "taniec nowoczesny",
                        "taniec brzucha"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный вид или стиль танцевальной композиции, характерный для определенной культуры или эпохи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "танец",
                                "target_lang_sense_clarification": "стиль/вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ef2d3d3-d2f6-3f47-aae0-a6fcafe3f0c4",
                    "sense_definition": "Spotkanie towarzyskie lub impreza, której głównym celem jest wspólne tańczenie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W każdy piątek chodzimy ze znajomymi na <tańce> do klubu.",
                            "target_lang_translations": {
                                "ru": "Каждую пятницу мы с друзьями ходим на <w>танцы</w> в клуб."
                            }
                        },
                        {
                            "text": "W remizie strażackiej zorganizowano wiejski <taniec> dla mieszkańców.",
                            "target_lang_translations": {
                                "ru": "В пожарном депо организовали деревенские <w>танцы</w> для жителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "potańcówka",
                        "zabawa"
                    ],
                    "common_phrases": [
                        "iść na tańce",
                        "grać do tańca",
                        "zaproszenie na tańce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общественное мероприятие или вечеринка, где главным занятием являются танцы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "танцы",
                                "target_lang_sense_clarification": "мероприятие (мн.ч.)"
                            },
                            {
                                "target_lang_word": "вечер танцев",
                                "target_lang_sense_clarification": "мероприятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4631d40-7967-337c-8d7b-13fd3c495858",
                    "sense_definition": "Specyficzne zachowanie zwierząt, najczęściej ptaków lub owadów, służące przyciągnięciu partnera lub komunikacji.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Skomplikowany <taniec> godowy żurawi jest fascynującym widowiskiem przyrodniczym.",
                            "target_lang_translations": {
                                "ru": "Сложный брачный <w>танец</w> журавлей — это захватывающее природное зрелище."
                            }
                        },
                        {
                            "text": "Pszczoły wykonują specjalny <taniec>, aby wskazać innym drogę do kwiatów.",
                            "target_lang_translations": {
                                "ru": "Пчелы исполняют особый <w>танец</w>, чтобы указать другим дорогу к цветам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "taniec godowy",
                        "taniec pszczół"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специфическое поведение животных (обычно птиц или насекомых) для привлечения партнера или коммуникации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "танец",
                                "target_lang_sense_clarification": "у животных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e4b6537-6ae1-30d9-bfa3-83c0bbb5f66b",
                    "sense_definition": "Gwałtowne wydarzenia, walka lub bitwa (znaczenie przenośne).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na polu bitwy rozpoczął się krwawy <taniec> śmierci.",
                            "target_lang_translations": {
                                "ru": "На поле битвы начался кровавый <w>танец</w> смерти."
                            }
                        },
                        {
                            "text": "Polityczny <taniec> mocarstw trwał przez wiele lat.",
                            "target_lang_translations": {
                                "ru": "Политический <w>танец</w> держав продолжался много лет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "taniec śmierci",
                        "chocholi taniec"

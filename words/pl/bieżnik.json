{
    "word_family": [
        "bieżnikować",
        "bieżnikowanie"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "51124702-4699-376a-92a0-17f8d7801f02",
                    "sense_definition": "Zewnętrzna warstwa opony z rowkami, która styka się z nawierzchnią i zapewnia przyczepność.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "tread",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę wymienić opony, ponieważ ich <w>bieżnik</w> jest już prawie całkowicie starty.",
                            "target_lang_translations": {
                                "ru": "Мне нужно заменить шины, так как их <w>протектор</w> уже почти полностью стёрся."
                            }
                        },
                        {
                            "text": "Policjant podczas kontroli drogowej zmierzył głębokość <w>bieżnika</w> w moim samochodzie.",
                            "target_lang_translations": {
                                "ru": "Полицейский во время проверки на дороге измерил глубину <w>протектора</w> в моей машине."
                            }
                        }
                    ],
                    "synonyms": [
                        "protektor"
                    ],
                    "common_phrases": [
                        "głębokość bieżnika",
                        "zużyty bieżnik",
                        "rzeźba bieżnika",
                        "bieżnik opony"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внешний слой шины с канавками, который соприкасается с дорогой и обеспечивает сцепление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "протектор",
                                "target_lang_sense_clarification": "шины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4382cd27-53ce-35f9-a533-9ae28c04eb7f",
                    "sense_definition": "Wąski, długi pas tkaniny ozdobnej kładziony na środek stołu, często na obrus.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "textile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na białym obrusie mama położyła czerwony <w>bieżnik</w>, aby udekorować stół na święta.",
                            "target_lang_translations": {
                                "ru": "На белую скатерть мама положила красную <w>дорожку</w>, чтобы украсить стол к праздникам."
                            }
                        },
                        {
                            "text": "Ten haftowany <w>bieżnik</w> idealnie pasuje do drewnianego blatu w jadalni.",
                            "target_lang_translations": {
                                "ru": "Эта вышитая <w>дорожка</w> идеально подходит к деревянной столешнице в столовой."
                            }
                        }
                    ],
                    "synonyms": [
                        "laufer"
                    ],
                    "common_phrases": [
                        "lniany bieżnik",
                        "bieżnik na stół",
                        "świąteczny bieżnik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Узкая длинная полоса декоративной ткани, которую кладут на середину стола, часто поверх скатерти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дорожка",
                                "target_lang_sense_clarification": "на стол"
                            },
                            {
                                "target_lang_word": "раннер",
                                "target_lang_sense_clarification": "столовый текстиль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14b85fee-bf03-326e-9822-4ec35f4513f4",
                    "sense_definition": "Wypukły wzór na spodzie podeszwy buta, który zapobiega ślizganiu się.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tread",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W góry najlepiej zabrać buty trekkingowe, które mają solidny i głęboki <w>bieżnik</w>.",
                            "target_lang_translations": {
                                "ru": "В горы лучше всего брать треккинговые ботинки, имеющие надежный и глубокий <w>протектор</w>."
                            }
                        },
                        {
                            "text": "Zimą łatwo się przewrócić, jeśli twoje buty mają płaską podeszwę bez wyraźnego <w>bieżnika</w>.",
                            "target_lang_translations": {
                                "ru": "Зимой легко упасть, если у твоей обуви плоская подошва без выраженного <w>протектора</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "protektor"
                    ],
                    "common_phrases": [
                        "agresywny bieżnik",
                        "podeszwa z bieżnikiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рельефный рисунок на подошве обуви, предотвращающий скольжение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "протектор",
                                "target_lang_sense_clarification": "подошвы"
                            },
                            {
                                "target_lang_word": "рифление",
                                "target_lang_sense_clarification": "узор подошвы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
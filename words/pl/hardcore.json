                },
                {
                    "sense_id": "11863c7b-d183-3b23-ab6d-70d632c32c69",
                    "sense_definition": "Gwałtowna bójka, awantura lub zamieszanie, często z użyciem przemocy.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "fight_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pod klubem wybuchł niezły <w>hardcore</w> i musiała przyjechać policja.",
                            "target_lang_translations": {
                                "ru": "У клуба начался лютый <w>замес</w>, и пришлось приехать полиции."
                            }

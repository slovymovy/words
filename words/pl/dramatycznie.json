            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ef60e611-74da-3a32-b11d-7b4ff99900a4",
                    "sense_definition": "W sposób gwałtowny, skrajny lub w bardzo dużym stopniu, często oznaczający nagłą zmianę.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Liczba bezrobotnych w tym regionie <w>dramatycznie</w> wzrosła w ciągu ostatniego roku.",
                            "target_lang_translations": {
                                "ru": "Число безработных в этом регионе <w>резко</w> возросло за последний год."
                            }
                        },
                        {
                            "text": "Poziom wody w rzece <w>dramatycznie</w> się obniżył z powodu suszy.",
                            "target_lang_translations": {
                                "ru": "Уровень воды в реке <w>критически</w> снизился из-за засухи."
                            }
                        }
                    ],
                    "synonyms": [
                        "drastycznie",
                        "gwałtownie",
                        "radykalnie",
                        "znacząco"
                    ],
                    "antonyms": [
                        "nieznacznie",
                        "lekko",
                        "stopniowo"
                    ],
                    "common_phrases": [
                        "dramatycznie wzrosnąć",
                        "dramatycznie spaść",
                        "dramatycznie niski",
                        "dramatycznie zmienić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Резко, кардинально или в очень сильной степени, часто подразумевает внезапное изменение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "резко",
                                "target_lang_sense_clarification": "внезапно и сильно"
                            },
                            {
                                "target_lang_word": "кардинально",
                                "target_lang_sense_clarification": "полностью, коренным образом"
                            },
                            {
                                "target_lang_word": "драматически",
                                "target_lang_sense_clarification": "о силе изменения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb9598a1-9ee5-33d8-8a71-0e6b4605e295",
                    "sense_definition": "W sposób bardzo poważny, tragiczny, smutny lub budzący grozę.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "tragedy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sytuacja mieszkańców po trzęsieniu ziemi wygląda <w>dramatycznie</w>.",
                            "target_lang_translations": {
                                "ru": "Ситуация жителей после землетрясения выглядит <w>драматично</w>."
                            }
                        },
                        {
                            "text": "Wyprawa w góry skończyła się <w>dramatycznie</w> dla niedoświadczonych turystów.",
                            "target_lang_translations": {
                                "ru": "Поход в горы закончился <w>трагически</w> для неопытных туристов."
                            }
                        }
                    ],
                    "synonyms": [
                        "tragicznie",
                        "katastrofalnie",
                        "wstrząsająco",
                        "okropnie"
                    ],
                    "antonyms": [
                        "szczęśliwie",
                        "pomyślnie",
                        "dobrze"
                    ],
                    "common_phrases": [

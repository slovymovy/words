{
    "word_family": [
        "уговор",
        "уговаривание"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4a9f5b97-24c0-3809-9d4f-f020884e71b6",
                    "sense_definition": "Склонить кого-либо к какому-либо действию или согласию посредством просьб, убеждения или доводов.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я наконец смог <уговорить> родителей купить мне собаку.",
                            "target_lang_translations": {
                                "en": "I finally managed to <persuade> my parents to buy me a dog."
                            }
                        },
                        {
                            "text": "Мы пытались <уговорить> его пойти с нами в кино, но он отказался.",
                            "target_lang_translations": {
                                "en": "We tried to <talk> him <into> going to the movies with us, but he refused."
                            }
                        }
                    ],
                    "synonyms": [
                        "убедить"
                    ],
                    "antonyms": [
                        "отговорить"
                    ],
                    "common_phrases": [
                        "уговорить друга",
                        "еле уговорить",
                        "попытаться уговорить"
                    ],
                    "target_lang_definitions": {
                        "en": "To induce someone to take a specific action or agree to something through requests, persuasion, or arguments."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "persuade",
                                "target_lang_sense_clarification": "to induce someone to do something through reasoning or argument"
                            },
                            {
                                "target_lang_word": "talk into",
                                "target_lang_sense_clarification": "to persuade someone to do something, often against their initial reluctance"
                            },
                            {
                                "target_lang_word": "convince",
                                "target_lang_sense_clarification": "to cause someone to agree to do something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "710cbdf9-4b75-3345-9880-4ca38382d2e5",
                    "sense_definition": "Полностью выпить или съесть что-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За вечер они <уговорили> две бутылки вина.",
                            "target_lang_translations": {
                                "en": "Over the evening, they <polished off> two bottles of wine."
                            }
                        },
                        {
                            "text": "Голодные туристы быстро <уговорили> весь обед.",
                            "target_lang_translations": {
                                "en": "The hungry tourists quickly <finished off> the entire lunch."
                            }
                        }
                    ],
                    "common_phrases": [
                        "уговорить бутылочку"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To completely drink or eat something (colloquial)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "polish off",
                                "target_lang_sense_clarification": "to finish eating or drinking something quickly or easily (informal)"
                            },
                            {
                                "target_lang_word": "finish off",
                                "target_lang_sense_clarification": "to consume the remainder of something"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
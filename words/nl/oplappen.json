    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b5a789cd-ad7f-3d5c-a769-abdba680f4f9",
                    "sense_definition": "Zieke mensen of dieren verzorgen en weer gezond maken",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "healing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De artsen in het ziekenhuis zullen de gewonde patiënt wel weer <w>oplappen</w>.",
                            "target_lang_translations": {
                                "en": "The doctors in the hospital will <w>patch</w> the injured patient <w>up</w>.",
                                "ru": "Врачи в больнице наверняка снова <w>поставят на ноги</w> раненого пациента."
                            }
                        },
                        {
                            "text": "Na die zware griep moest mijn moeder me echt even <w>oplappen</w> met goede soep.",
                            "target_lang_translations": {
                                "en": "After that bad flu, my mother really had to <w>nurse</w> me <w>back to health</w> with good soup.",
                                "ru": "После того тяжелого гриппа маме пришлось <w>выхаживать</w> меня хорошим супом."
                            }
                        }
                    ],
                    "synonyms": [
                        "opknappen",
                        "genezen",
                        "verplegen",
                        "herstellen"
                    ],
                    "common_phrases": [
                        "iemand weer oplappen"
                    ],
                    "target_lang_definitions": {
                        "en": "To care for sick people or animals and restore them to health",
                        "ru": "Ухаживать за больными людьми или животными, возвращая им здоровье"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "patch up",
                                "target_lang_sense_clarification": "to treat injuries or restore health, often implying a rough or quick fix"
                            },
                            {
                                "target_lang_word": "nurse back to health",
                                "target_lang_sense_clarification": "to care for someone until they recover"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выхаживать",
                                "target_lang_sense_clarification": "заботиться до выздоровления"
                            },
                            {
                                "target_lang_word": "ставить на ноги",
                                "target_lang_sense_clarification": "возвращать здоровье"
                            },
                            {
                                "target_lang_word": "подлечивать",
                                "target_lang_sense_clarification": "разг. лечить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f18b3b5-6420-3001-a520-3aab8d6ea135",
                    "sense_definition": "Dingen die kapot of versleten zijn repareren en weer bruikbaar maken",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "repairing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft het hele weekend besteed aan het <w>oplappen</w> van zijn oude boot.",
                            "target_lang_translations": {
                                "en": "He spent the whole weekend <w>fixing up</w> his old boat.",
                                "ru": "Он потратил все выходные на то, чтобы <w>подлатать</w> свою старую лодку."
                            }
                        },
                        {
                            "text": "We gaan die kapotte fiets <w>oplappen</w> zodat hij weer een tijdje mee kan.",
                            "target_lang_translations": {
                                "en": "We are going to <w>patch up</w> that broken bike so it lasts a while longer.",
                                "ru": "Мы собираемся <w>подремонтировать</w> этот сломанный велосипед, чтобы он прослужил еще какое-то время."
                            }
                        }
                    ],
                    "synonyms": [
                        "repareren",
                        "opknappen",
                        "herstellen",
                        "opkalefateren",
                        "restaureren",
                        "fiksen"
                    ],
                    "antonyms": [
                        "slijten",
                        "kapotgaan"
                    ],
                    "common_phrases": [
                        "een auto oplappen",
                        "het huis oplappen"
                    ],
                    "target_lang_definitions": {
                        "en": "To repair broken or worn-out things to make them usable again",
                        "ru": "Ремонтировать сломанные или изношенные вещи, делая их снова пригодными к использованию"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "patch up",
                                "target_lang_sense_clarification": "to repair something quickly or temporarily"
                            },
                            {
                                "target_lang_word": "fix up",
                                "target_lang_sense_clarification": "to repair, renovate, or restore something old"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "подлатывать",
                                "target_lang_sense_clarification": "чинить на скорую руку"
                            },
                            {
                                "target_lang_word": "ремонтировать",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "чинить",
                                "target_lang_sense_clarification": "исправлять поломку"
                            }
                        ]
                    }
                }
            ]
        }
    ]

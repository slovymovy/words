{
    "word_family": [
        "wisieć",
        "wieszać",
        "zawiesić",
        "powiesić",
        "zwisać",
        "podwiesić",
        "przewiesić",
        "nawis",
        "zwis",
        "zawieszenie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c574439b-42f6-360a-8052-33f3fdaca8d1",
                    "sense_definition": "Zostać umieszczonym w pozycji wiszącej lub zaczepić się o coś, nie dotykając podłoża.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_hanging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy obraz w końcu <zawisł> na ścianie w salonie.",
                            "target_lang_translations": {
                                "ru": "Новая картина наконец <повисла> на стене в гостиной."
                            }
                        },
                        {
                            "text": "Jego życie <zawisło> na włosku po tym groźnym wypadku.",
                            "target_lang_translations": {
                                "ru": "Его жизнь <повисла> на волоске после этой страшной аварии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zawisnąć na szyi",
                        "zawisnąć na włosku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказаться в висячем положении или зацепиться за что-то, не касаясь опоры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повиснуть",
                                "target_lang_sense_clarification": "начать висеть; зацепиться и висеть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e509c2eb-5f59-3480-9c01-27106cc2a35b",
                    "sense_definition": "Zatrzymać się w locie i unosić się w jednym miejscu (o ptakach, owadach, maszynach) lub wypełnić przestrzeń bez ruchu (o mgle, dymie, ciszy).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hovering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Helikopter policyjny <zawisł> nad budynkiem, obserwując sytuację na dole.",
                            "target_lang_translations": {
                                "ru": "Полицейский вертолет <завис> над зданием, наблюдая за ситуацией внизу."
                            }
                        },
                        {
                            "text": "Ciężka i niezręczna cisza <zawisła> w pokoju po jego słowach.",
                            "target_lang_translations": {
                                "ru": "Тяжелая и неловкая тишина <повисла> в комнате после его слов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zawisnąć w powietrzu",
                        "zawisnąć w próżni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Остановиться в воздухе над одним местом (о птицах, технике) или заполнить пространство (о тишине, дыме)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зависнуть",
                                "target_lang_sense_clarification": "остановиться в полете (о вертолете, птице)"
                            },
                            {
                                "target_lang_word": "повиснуть",
                                "target_lang_sense_clarification": "о тишине, тумане, атмосфере"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db08f9cd-88c3-3c03-98e1-1addaae6972d",
                    "sense_definition": "Pojawić się jako bliskie, nieuchronne zagrożenie lub niebezpieczeństwo (zwykle w połączeniu z przyimkiem \"nad\").",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "looming_threat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Groźba bankructwa <zawisła> nad firmą z powodu złego zarządzania.",
                            "target_lang_translations": {
                                "ru": "Угроза банкротства <нависла> над фирмой из-за плохого управления."
                            }
                        },
                        {
                            "text": "Czarne chmury <zawisły> nad naszą przyszłością w tym kraju.",
                            "target_lang_translations": {
                                "ru": "Черные тучи <нависли> над нашим будущим в этой стране."
                            }
                        }
                    ],
                    "common_phrases": [
                        "groźba zawisła",
                        "niebezpieczeństwo zawisło"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возникнуть как близкая, неизбежная угроза или опасность (обычно с предлогом \"над\")."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нависнуть",
                                "target_lang_sense_clarification": "об угрозе, опасности, тучах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6dc297c5-2b81-35c4-8ff5-4c45ec3dd44c",
                    "sense_definition": "Stać się zależnym od czyjejś decyzji, woli lub zewnętrznych okoliczności.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dependency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Los całego projektu <zawisł> na decyzji jednego urzędnika.",
                            "target_lang_translations": {
                                "ru": "Судьба всего проекта <зависла> на решении одного чиновника."
                            }
                        },
                        {
                            "text": "Sprawa sądowa <zawisła> w martwym punkcie z braku dowodów.",
                            "target_lang_translations": {
                                "ru": "Судебное дело <зависло> в мертвой точке из-за отсутствия улик."
                            }
                        }
                    ],
                    "synonyms": [
                        "uzależnić się"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказаться в состоянии неопределенности или стать зависимым от внешних обстоятельств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зависнуть",
                                "target_lang_sense_clarification": "застопориться, остаться нерешенным"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "everything",
                                "target_lang_sense_clarification": "used in pronominal adverb constructions"
                            },
                            {
                                "target_lang_word": "anything",
                                "target_lang_sense_clarification": "used in negative or interrogative contexts"

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1741a9e2-3f60-3c33-ac02-fe47d5f15ecc",
                    "sense_definition": "Считать кого-либо виновным, ответственным за совершение чего-либо дурного.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Во всём <w>вините</w> только себя самого.",
                            "target_lang_translations": {
                                "en": "<w>Blame</w> only yourself for everything."
                            }
                        },
                        {
                            "text": "Не стоит <w>винить</w> обстоятельства в своих личных неудачах.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>blame</w> circumstances for your personal failures."
                            }
                        }
                    ],
                    "synonyms": [
                        "обвинять"
                    ],
                    "antonyms": [
                        "восхвалять"
                    ],
                    "common_phrases": [
                        "винить себя",
                        "винить во всём",
                        "некого винить"
                    ],
                    "target_lang_definitions": {
                        "en": "To consider someone guilty or responsible for doing something bad"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blame",
                                "target_lang_sense_clarification": "to hold responsible"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cbbbf3f-5318-3972-9d18-f80362d26c33",
                    "sense_definition": "Высказывать упрёки, выражать недовольство чьими-либо действиями.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец начал <w>винить</w> сына за постоянное безделье.",
                            "target_lang_translations": {
                                "en": "The father started <w>blaming</w> his son for constant idleness."
                            }
                        },
                        {
                            "text": "Она привыкла <w>винить</w> мужа по всяким пустякам.",
                            "target_lang_translations": {
                                "en": "She got used to <w>blaming</w> her husband for every trifle."
                            }
                        }
                    ],
                    "synonyms": [
                        "упрекать",
                        "укорять"
                    ],
                    "common_phrases": [
                        "перестать винить",
                        "винить за ошибки"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To express reproaches or dissatisfaction with someone's actions"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blame",
                                "target_lang_sense_clarification": "to criticize or censure"
                            },
                            {
                                "target_lang_word": "accuse",
                                "target_lang_sense_clarification": "to find fault with"
                            }
                        ]
                    }
                }
            ]
        }
    ]

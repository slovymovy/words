    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "04db9762-0006-35c4-b8a7-052a1ec1eeb3",
                    "sense_definition": "Substancja w stanie ciekłym, która nie ma własnego kształtu i przybiera formę naczynia; w życiu codziennym często oznacza napój lub płynny środek czystości.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "substance_liquid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zalecił pacjentowi przyjmować dużo <w>płynów</w> w czasie gorączki.",
                            "target_lang_translations": {
                                "ru": "Врач порекомендовал пациенту пить много <w>жидкости</w> во время жара."
                            }
                        },
                        {
                            "text": "Muszę iść do sklepu, bo skończył się <w>płyn</w> do mycia naczyń.",
                            "target_lang_translations": {
                                "ru": "Мне нужно пойти в магазин, так как закончилась <w>жидкость</w> для мытья посуды."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciecz"
                    ],
                    "common_phrases": [
                        "płyn do naczyń",
                        "płyn do kąpieli",
                        "płyn hamulcowy",
                        "płyn do płukania",
                        "przyjmować płyny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещество в жидком состоянии, которое не имеет собственной формы и принимает форму сосуда; в повседневной жизни часто означает напиток или жидкое чистящее средство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жидкость",
                                "target_lang_sense_clarification": "вещество или напиток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "660688ca-e309-3f1e-96a2-46ca33c17281",
                    "sense_definition": "W fizyce: każda substancja zdolna do swobodnego przepływu, obejmująca zarówno ciecze, jak i gazy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physics_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mechanika <w>płynów</w> zajmuje się badaniem zachowania cieczy i gazów w ruchu.",
                            "target_lang_translations": {
                                "ru": "Механика <w>жидкостей</w> занимается изучением поведения жидкостей и газов в движении."
                            }
                        },
                        {
                            "text": "Powietrze jest traktowane w aerodynamice jako ściśliwy <w>płyn</w>.",
                            "target_lang_translations": {
                                "ru": "В аэродинамике воздух рассматривается как сжимаемая <w>жидкость</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mechanika płynów",
                        "dynamika płynów",
                        "przepływ płynu"
                    ],
                    "target_lang_definitions": {
                        "ru": "В физике: любое вещество, способное свободно течь, включая как жидкости, так и газы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жидкость",
                                "target_lang_sense_clarification": "физический термин"
                            },
                            {
                                "target_lang_word": "текучая среда",
                                "target_lang_sense_clarification": "термин механики сплошных сред"
                            }
                        ]
                    }
                }
            ]
        }

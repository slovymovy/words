    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "65f449d6-625f-37d7-9501-af68a5964756",
                    "sense_definition": "To bring goods or services into a country from abroad, especially for sale or trade.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The country has to <w>import</w> most of its oil from abroad.",
                            "target_lang_translations": {
                                "ru": "Страна вынуждена <w>импортировать</w> большую часть нефти из-за границы."
                            }
                        },
                        {
                            "text": "We plan to <w>import</w> high-quality fabrics for our new clothing line.",
                            "target_lang_translations": {
                                "ru": "Мы планируем <w>закупать</w> высококачественные ткани для нашей новой линии одежды."
                            }
                        }
                    ],
                    "antonyms": [
                        "export"
                    ],
                    "common_phrases": [
                        "import goods",
                        "import from",
                        "import duties"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ввозить товары или услуги в страну из-за границы, особенно для продажи или торговли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "импортировать",
                                "target_lang_sense_clarification": "экономический термин"
                            },
                            {
                                "target_lang_word": "ввозить",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56028cb4-3e1f-34e6-8356-a24fcbb7b274",
                    "sense_definition": "To load a file or data into a software application from another version or system.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can easily <w>import</w> your contacts from your old phone to the new one.",
                            "target_lang_translations": {
                                "ru": "Вы можете легко <w>импортировать</w> контакты со старого телефона на новый."
                            }
                        },
                        {
                            "text": "The program allows users to <w>import</w> images directly into the document.",
                            "target_lang_translations": {
                                "ru": "Программа позволяет пользователям <w>импортировать</w> изображения прямо в документ."
                            }
                        }
                    ],
                    "antonyms": [
                        "export"
                    ],
                    "common_phrases": [
                        "import data",
                        "import files",
                        "import settings"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Загружать файл или данные в программное приложение из другой версии или системы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "импортировать",
                                "target_lang_sense_clarification": "информатика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cac9a8ec-de86-3069-b621-2338d561de6a",
                    "sense_definition": "To be of importance or consequence; to signify.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It <w>imports</w> us little what others may think of our decision.",
                            "target_lang_translations": {
                                "ru": "Нам не <w>важно</w>, что другие могут подумать о нашем решении."
                            }
                        },
                        {
                            "text": "What <w>imports</w> it if we fail, as long as we have tried our best?",
                            "target_lang_translations": {
                                "ru": "Какое это имеет <w>значение</w>, если мы потерпим неудачу, пока мы старались изо всех сил?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь значение или важность; значить (устаревшее или книжное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иметь значение",
                                "target_lang_sense_clarification": "фраза"
                            },
                            {
                                "target_lang_word": "быть важным",
                                "target_lang_sense_clarification": "фраза"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c97b64d5-88ff-3308-b187-6507681cf1fd",
                    "sense_definition": "A product or good brought into a country from another source, usually for sale.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The shop sells a variety of cheese <w>imports</w> from France and Italy.",
                            "target_lang_translations": {
                                "ru": "Магазин продает разнообразные <w>импортные сыры</w> из Франции и Италии."
                            }
                        },
                        {
                            "text": "Cheap <w>imports</w> have made it difficult for local manufacturers to compete.",
                            "target_lang_translations": {
                                "ru": "Дешевый <w>импорт</w> затруднил конкуренцию для местных производителей."
                            }
                        }
                    ],
                    "antonyms": [
                        "export"
                    ],
                    "common_phrases": [
                        "cheap imports",
                        "foreign imports",
                        "net import"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продукт или товар, ввезенный в страну из другого источника, обычно для продажи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "импорт",
                                "target_lang_sense_clarification": "собирательное, ввозимые товары"
                            },
                            {
                                "target_lang_word": "импортный товар",
                                "target_lang_sense_clarification": "конкретный предмет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19c6fd26-1caa-3ea3-953a-e1ff5c00adf3",
                    "sense_definition": "The action or process of bringing goods or services into a country from abroad.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government has placed a ban on the <w>import</w> of ivory products.",
                            "target_lang_translations": {
                                "ru": "Правительство ввело запрет на <w>ввоз</w> изделий из слоновой кости."
                            }
                        },
                        {
                            "text": "They specialize in the <w>import</w> and distribution of electronic components.",
                            "target_lang_translations": {
                                "ru": "Они специализируются на <w>импорте</w> и дистрибуции электронных компонентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "importation"
                    ],
                    "antonyms": [
                        "export"
                    ],
                    "common_phrases": [
                        "import duty",
                        "import license",
                        "import tax"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или процесс ввоза товаров или услуг в страну из-за границы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "импорт",
                                "target_lang_sense_clarification": "экономика"
                            },
                            {
                                "target_lang_word": "ввоз",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96a8d123-52f1-39ba-b44a-c4a6b2d715aa",
                    "sense_definition": "The implicit meaning, significance, or importance of something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This is a matter of great <w>import</w> to the future of our organization.",
                            "target_lang_translations": {
                                "ru": "Это вопрос огромной <w>важности</w> для будущего нашей организации."
                            }
                        },
                        {
                            "text": "She did not immediately grasp the full <w>import</w> of the message.",
                            "target_lang_translations": {
                                "ru": "Она не сразу поняла полный <w>смысл</w> сообщения."
                            }
                        }
                    ],
                    "synonyms": [
                        "significance",
                        "importance",
                        "meaning",
                        "weight"
                    ],
                    "antonyms": [
                        "insignificance"
                    ],
                    "common_phrases": [
                        "of great import",
                        "full import",
                        "grave import"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Скрытый смысл, значимость или важность чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "значение",
                                "target_lang_sense_clarification": "значимость"
                            },
                            {
                                "target_lang_word": "смысл",
                                "target_lang_sense_clarification": "суть"
                            },
                            {
                                "target_lang_word": "важность",
                                "target_lang_sense_clarification": "вес, влияние"
                            }
                        ]
                    }
                }
            ]
        }
    ]

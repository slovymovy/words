    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "473a25be-c8ff-3d7a-af68-7f340b9085f6",
                    "sense_definition": "Clothes, sheets, and towels that need to be washed or have just been washed.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You've left your dirty <w>laundry</w> all over the house.",
                            "target_lang_translations": {
                                "ru": "Ты разбросал своё грязное <w>бельё</w> по всему дому."
                            }
                        },
                        {
                            "text": "I spent the whole afternoon folding clean <w>laundry</w>.",
                            "target_lang_translations": {
                                "ru": "Я провёл весь день, складывая чистое <w>бельё</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "washing"
                    ],
                    "common_phrases": [
                        "dirty laundry",
                        "clean laundry",
                        "pile of laundry",
                        "fold the laundry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одежда, простыни и полотенца, которые нужно постирать или которые только что были постираны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бельё",
                                "target_lang_sense_clarification": "одежда для стирки"
                            },
                            {
                                "target_lang_word": "стирка",
                                "target_lang_sense_clarification": "вещи в стирке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5f4c584-9a85-338c-90cd-99be91e523b6",
                    "sense_definition": "The act or process of washing clothes and linens.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In our family of five, we have to do the <w>laundry</w> every other day.",
                            "target_lang_translations": {
                                "ru": "В нашей семье из пяти человек нам приходится заниматься <w>стиркой</w> через день."
                            }
                        },
                        {
                            "text": "She is responsible for the cooking and the <w>laundry</w>.",
                            "target_lang_translations": {
                                "ru": "Она отвечает за готовку и <w>стирку</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "washing"
                    ],
                    "common_phrases": [
                        "do the laundry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или процесс стирки одежды и белья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стирка",
                                "target_lang_sense_clarification": "процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6e1ea7e-39aa-3250-bffa-9176af6e6331",
                    "sense_definition": "A business or room where clothes are washed and dried.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I dropped off my shirts at the <w>laundry</w> this morning.",
                            "target_lang_translations": {
                                "ru": "Я занёс свои рубашки в <w>прачечную</w> сегодня утром."
                            }
                        },
                        {
                            "text": "Our apartment building has a shared <w>laundry</w> in the basement.",
                            "target_lang_translations": {
                                "ru": "В нашем многоквартирном доме есть общая <w>прачечная</w> в подвале."
                            }
                        }
                    ],
                    "synonyms": [
                        "laundromat",
                        "laundrette",
                        "washateria"
                    ],
                    "common_phrases": [
                        "laundry room",
                        "at the laundry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предприятие или помещение, где стирают и сушат одежду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прачечная",
                                "target_lang_sense_clarification": "заведение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9aeda15e-4daa-3cfa-89aa-51b54d50c8bb",
                    "sense_definition": "A business established primarily to hide the source of illegal money.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car wash served as a <w>laundry</w> for the gang's drug money.",
                            "target_lang_translations": {
                                "ru": "Автомойка служила <w>прачечной</w> для наркоденег банды."
                            }
                        },
                        {
                            "text": "Investigators discovered the shop was just a <w>laundry</w> for illicit funds.",
                            "target_lang_translations": {
                                "ru": "Следователи обнаружили, что магазин был просто <w>прачечной</w> для незаконных средств."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бизнес, созданный в первую очередь для сокрытия источника незаконных денег."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прачечная",
                                "target_lang_sense_clarification": "сленг: отмывание денег"
                            },
                            {
                                "target_lang_word": "отмывочная",
                                "target_lang_sense_clarification": "сленг: фирма для отмывания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07a7f20f-1868-3b06-bc82-92ee8a03b1b2",

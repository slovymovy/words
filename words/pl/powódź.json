    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e1d166b4-782e-3a8d-a884-225b3f44fed6",
                    "sense_definition": "Gwałtowne zalanie wodą znacznego obszaru lądu, występujące zazwyczaj na skutek intensywnych opadów, roztopów lub wylania rzeki.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielka <powódź> zniszczyła dorobek całego ich życia.",
                            "target_lang_translations": {
                                "ru": "Сильное <наводнение> уничтожило всё, что они наживали всю жизнь."
                            }
                        },
                        {
                            "text": "Mieszkańcy obawiają się, że nadejdzie kolejna fala powodziowa.",
                            "target_lang_translations": {
                                "ru": "Жители опасаются, что придёт новая волна <наводнения>."
                            }
                        }
                    ],
                    "synonyms": [
                        "potop",
                        "zalew",
                        "wylew",
                        "rozlew"
                    ],
                    "antonyms": [
                        "susza"
                    ],
                    "common_phrases": [
                        "fala powodziowa",
                        "klęska powodzi",
                        "skutki powodzi",
                        "zagrożenie powodzią"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапное затопление значительной территории суши водой, обычно из-за сильных дождей, таяния снегов или разлива рек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наводнение",
                                "target_lang_sense_clarification": "стихийное бедствие"
                            },
                            {
                                "target_lang_word": "потоп",
                                "target_lang_sense_clarification": "разговорное или о масштабном бедствии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e186e613-3431-3308-80e5-9ed354a62c27",
                    "sense_definition": "Bardzo duża ilość czegoś, co pojawia się nagle i w nadmiarze; nawał.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wykładzie prelegenta zalała <powódź> pytań od publiczności.",
                            "target_lang_translations": {
                                "ru": "После лекции докладчика засыпал <поток> вопросов от публики."
                            }
                        },
                        {
                            "text": "W internecie panuje prawdziwa <powódź> niesprawdzonych informacji.",
                            "target_lang_translations": {
                                "ru": "В интернете царит настоящий <поток> непроверенной информации."
                            }
                        }
                    ],
                    "synonyms": [
                        "potok",
                        "mnogość"
                    ],
                    "antonyms": [
                        "posucha"
                    ],
                    "common_phrases": [
                        "powódź słów",
                        "powódź łez",
                        "powódź pytań"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень большое количество чего-либо, появляющееся внезапно и в избытке; лавина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поток",
                                "target_lang_sense_clarification": "переносное значение: непрерывная масса чего-либо"
                            },
                            {
                                "target_lang_word": "наплыв",
                                "target_lang_sense_clarification": "скопление, появление в большом количестве"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4700ae64-7f56-32b0-a8aa-b14d56b33403",
                    "sense_definition": "Druga osoba liczby pojedynczej trybu rozkazującego czasownika <w>powodzić</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Powódź> palcem po mapie, a szybko znajdziesz szukane miasto.",
                            "target_lang_translations": {
                                "ru": "<Поводи> пальцем по карте, и ты быстро найдёшь искомый город."
                            }
                        },
                        {
                            "text": "<Powódź> wzrokiem po sali i spróbuj kogoś rozpoznać.",
                            "target_lang_translations": {
                                "ru": "<Поводи> взглядом по залу и попробуй кого-нибудь узнать."
                            }
                        }
                    ],
                    "traits": [

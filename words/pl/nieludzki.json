    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f5ad4a2a-946f-366e-ae47-c75f636b123d",
                    "sense_definition": "Okrutny, bezwzględny, pozbawiony współczucia lub naruszający godność człowieka.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "moral_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Więźniowie byli przetrzymywani w ciasnych celach, a strażnicy stosowali wobec nich <w>nieludzkie</w> kary.",
                            "target_lang_translations": {
                                "ru": "Заключенных держали в тесных камерах, а охранники применяли к ним <w>бесчеловечные</w> наказания."
                            }
                        },
                        {
                            "text": "To <w>nieludzkie</w> traktowanie zwierząt w hodowlach przemysłowych musi się wreszcie skończyć.",
                            "target_lang_translations": {
                                "ru": "Это <w>бесчеловечное</w> обращение с животными на промышленных фермах должно наконец прекратиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "okrutny",
                        "bezlitosny"
                    ],
                    "antonyms": [
                        "ludzki"
                    ],
                    "common_phrases": [
                        "nieludzkie traktowanie",
                        "nieludzkie warunki",
                        "nieludzki system",
                        "nieludzkie prawo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жестокий, безжалостный, лишенный сострадания или нарушающий человеческое достоинство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бесчеловечный",
                                "target_lang_sense_clarification": "жестокий, лишенный гуманности"
                            },
                            {
                                "target_lang_word": "жестокий",
                                "target_lang_sense_clarification": "безжалостный, суровый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db555ee4-9e97-350c-8825-cd6dc6e10b50",
                    "sense_definition": "Przekraczający zwykłe ludzkie możliwości, siły lub wytrzymałość; niezwykle intensywny lub wielki.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Maratończyk włożył <w>nieludzki</w> wysiłek, aby dobiec do mety mimo bolesnej kontuzji.",
                            "target_lang_translations": {
                                "ru": "Марафонец приложил <w>нечеловеческое</w> усилие, чтобы добежать до финиша, несмотря на болезненную травму."
                            }
                        },
                        {
                            "text": "Nagły, <w>nieludzki</w> ból głowy sprawił, że mężczyzna musiał natychmiast usiąść na ławce.",
                            "target_lang_translations": {
                                "ru": "Внезапная, <w>нечеловеческая</w> головная боль заставила мужчину немедленно сесть на скамейку."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogromny",
                        "nadludzki",
                        "wielki"
                    ],
                    "antonyms": [
                        "zwyczajny",
                        "normalny"
                    ],
                    "common_phrases": [
                        "nieludzki wysiłek",
                        "nieludzki ból",
                        "nieludzki krzyk",
                        "nieludzkie zmęczenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Превышающий обычные человеческие возможности, силы или выносливость; чрезвычайно интенсивный или великий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нечеловеческий",
                                "target_lang_sense_clarification": "чрезвычайно сильный, за пределами обычных возможностей"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
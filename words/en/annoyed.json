            "senses": [
                {
                    "sense_id": "3ecbdff4-0184-3d9c-b5b8-cca6ef610b1f",
                    "sense_definition": "Troubled or irritated by something unwanted or unliked.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Judy is <w>annoyed</w> at Jim because he forgot to put the bins out again.",
                            "target_lang_translations": {
                                "ru": "Джуди <w>раздражена</w> на Джима, потому что он снова забыл выставить мусорные баки."
                            }
                        },
                        {
                            "text": "He gets <w>annoyed</w> when people talk loudly on the bus.",
                            "target_lang_translations": {
                                "ru": "Он <w>раздражается</w>, когда люди громко разговаривают в автобусе."
                            }
                        }
                    ],
                    "synonyms": [
                        "irritated",
                        "bothered",
                        "vexed",
                        "irked"
                    ],
                    "antonyms": [
                        "pleased"
                    ],
                    "common_phrases": [
                        "get annoyed",
                        "annoyed with someone",
                        "annoyed at something",
                        "look annoyed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывающий раздражение или недовольство чем-либо нежелательным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раздражённый",
                                "target_lang_sense_clarification": "находящийся в состоянии раздражения"
                            },
                            {
                                "target_lang_word": "раздосадованный",
                                "target_lang_sense_clarification": "испытывающий чувство досады или огорчения"
                            },
                            {
                                "target_lang_word": "сердитый",
                                "target_lang_sense_clarification": "испытывающий гнев или недовольство кем-то"
                            }
                        ]
                    }
                }
            ]
        },
        {

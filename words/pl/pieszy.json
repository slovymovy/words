    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "72d85ad9-e560-3fe0-aba8-f7410c0813d0",
                    "sense_definition": "Osoba, która porusza się po drodze na własnych nogach, a nie w pojeździe.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_person_moving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca zatrzymał się przed pasami, aby przepuścić <w>pieszego</w>.",
                            "target_lang_translations": {
                                "ru": "Водитель остановился перед переходом, чтобы пропустить <w>пешехода</w>."
                            }
                        },
                        {
                            "text": "W strefie zamieszkania <w>piesi</w> mają zawsze pierwszeństwo przed samochodami.",
                            "target_lang_translations": {
                                "ru": "В жилой зоне <w>пешеходы</w> всегда имеют преимущество перед автомобилями."
                            }
                        }
                    ],
                    "synonyms": [
                        "piechur",
                        "niezmotoryzowany",
                        "piechotny"
                    ],
                    "antonyms": [
                        "zmotoryzowany"
                    ],
                    "common_phrases": [
                        "przejście dla pieszych",
                        "bezpieczeństwo pieszych",
                        "pierwszeństwo pieszych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который передвигается по дороге пешком, а не в транспортном средстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пешеход",
                                "target_lang_sense_clarification": "человек, идущий пешком"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c4c98a22-6841-3204-a787-0e60917677ef",
                    "sense_definition": "Taki, który odbywa się na piechotę lub jest przeznaczony dla osób chodzących, a nie dla pojazdów.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_activity_infra",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W weekend planujemy długą <w>pieszą</w> wycieczkę do lasu.",
                            "target_lang_translations": {
                                "ru": "В выходные мы планируем долгую <w>пешую</w> прогулку в лес."
                            }
                        },
                        {
                            "text": "Ten szlak <w>pieszy</w> jest zbyt stromy dla rowerzystów.",
                            "target_lang_translations": {
                                "ru": "Эта <w>пешеходная</w> тропа слишком крутая для велосипедистов."
                            }
                        }
                    ],
                    "antonyms": [
                        "kołowy",
                        "zmotoryzowany"
                    ],
                    "common_phrases": [
                        "ruch pieszy",
                        "szlak pieszy",
                        "wycieczka piesza",
                        "przejście piesze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершаемый пешком или предназначенный для ходьбы, а не для транспорта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пеший",
                                "target_lang_sense_clarification": "совершаемый пешком (о прогулке, походе)"
                            },
                            {
                                "target_lang_word": "пешеходный",
                                "target_lang_sense_clarification": "предназначенный для пешеходов (о маршруте, переходе)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02d04695-0a7e-3b7c-b75e-6f4a33e41b8f",
                    "sense_definition": "Taki, który porusza się o własnych siłach na nogach, a nie jedzie pojazdem.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_person_moving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Pieszy</w> patrol policji pilnuje porządku w parku miejskim.",
                            "target_lang_translations": {
                                "ru": "<w>Пеший</w> патруль полиции следит за порядком в городском парке."
                            }
                        },
                        {
                            "text": "Jako <w>pieszy</w> turysta zobaczysz w górach więcej szczegółów niż z okna samochodu.",
                            "target_lang_translations": {
                                "ru": "Как <w>пеший</w> турист ты увидишь в горах больше деталей, чем из окна автомобиля."
                            }
                        }
                    ],
                    "synonyms": [
                        "niezmotoryzowany"
                    ],
                    "antonyms": [
                        "zmotoryzowany"
                    ],
                    "common_phrases": [
                        "patrol pieszy",
                        "turysta pieszy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передвигающийся собственными силами на ногах, не использующий транспорт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пеший",
                                "target_lang_sense_clarification": "передвигающийся на ногах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2e6aa74-92c9-3c4b-b84a-101a9b995791",
                    "sense_definition": "Dotyczący piechoty – formacji wojskowej walczącej na lądzie bez użycia pojazdów bojowych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Historyczny pułk <w>pieszy</w> brał udział w wielu bitwach dziewiętnastego wieku.",
                            "target_lang_translations": {
                                "ru": "Исторический <w>пехотный</w> полк принимал участие во многих битвах девятнадцатого века."
                            }
                        },
                        {
                            "text": "Dawna walka <w>piesza</w> wymagała od żołnierzy dużej wytrzymałości fizycznej.",
                            "target_lang_translations": {
                                "ru": "Древний <w>пеший</w> бой требовал от солдат большой физической выносливости."
                            }
                        }
                    ],
                    "synonyms": [
                        "piechotny"
                    ],
                    "common_phrases": [
                        "pułk pieszy",
                        "strzelec pieszy"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "współcześnie rzadziej używane, częściej w kontekście historycznym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к пехоте — роду войск, сражающемуся на суше без боевых машин."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пехотный",
                                "target_lang_sense_clarification": "относящийся к пехоте"
                            },
                            {
                                "target_lang_word": "пеший",
                                "target_lang_sense_clarification": "сражающийся в пешем строю"
                            }
                        ]
                    }
                }
            ]
        }

            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4d00bdc4-22e3-3295-ba2e-a4a17153bae5",
                    "sense_definition": "Mający niewielką wysokość; znajdujący się blisko ziemi lub poziomu odniesienia (o przedmiotach, budynkach, roślinach itp.)",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_dimension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <niski> płot można bardzo łatwo przeskoczyć.",
                            "target_lang_translations": {
                                "ru": "Через этот <низкий> забор можно очень легко перепрыгнуть."
                            }
                        },
                        {
                            "text": "W starym domu był bardzo <niski> sufit, o który uderzyłem głową.",
                            "target_lang_translations": {
                                "ru": "В старом доме был очень <низкий> потолок, о который я ударился головой."
                            }
                        }
                    ],
                    "synonyms": [
                        "następujący",
                        "gruby"
                    ],
                    "antonyms": [
                        "wysoki"
                    ],
                    "common_phrases": [
                        "niski budynek",
                        "niski stół",
                        "niski poziom",
                        "niski parter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий небольшую высоту; находящийся близко к земле (о предметах, зданиях, растениях)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "низкий",
                                "target_lang_sense_clarification": "о физической высоте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4d9fc66-b434-3490-9b4f-c9f5863a4405",
                    "sense_definition": "O człowieku: mający mały wzrost",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_dimension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat jest <niski>, ale gra świetnie w koszykówkę.",
                            "target_lang_translations": {
                                "ru": "Мой брат <низкий> (невысокий), но отлично играет в баскетбол."
                            }
                        },
                        {
                            "text": "Ta <niska> dziewczyna w czerwonej sukience to moja siostra.",
                            "target_lang_translations": {
                                "ru": "Эта <низкая> (невысокая) девушка в красном платье — моя сестра."
                            }
                        }
                    ],
                    "antonyms": [
                        "wysoki"
                    ],
                    "common_phrases": [
                        "niski wzrost",
                        "niski mężczyzna",
                        "niska kobieta"
                    ],
                    "target_lang_definitions": {
                        "ru": "О человеке: имеющий маленький рост"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "низкий",
                                "target_lang_sense_clarification": "о росте"
                            },
                            {
                                "target_lang_word": "невысокий",
                                "target_lang_sense_clarification": "о росте (более мягко)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08449ff1-ebb7-3bc5-8fed-7fc59e4e7efe",
                    "sense_definition": "Mały pod względem liczby, natężenia, wartości, stopnia lub jakości",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "value_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem ten samochód za wyjątkowo <niską> cenę.",
                            "target_lang_translations": {
                                "ru": "Я купил эту машину по исключительно <низкой> цене."
                            }
                        },
                        {
                            "text": "Dziś rano była bardzo <niska> temperatura i padał śnieg.",
                            "target_lang_translations": {
                                "ru": "Сегодня утром была очень <низкая> температура и шёл снег."
                            }
                        }
                    ],
                    "antonyms": [
                        "wysoki"
                    ],
                    "common_phrases": [

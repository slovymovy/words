        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "15be6ae0-5268-3601-a19f-29e5d17827df",
                    "sense_definition": "To schedule or plan an event to happen at a specific time.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "planning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new software update is <w>slated</w> for release next month.",
                            "target_lang_translations": {
                                "ru": "Выпуск обновления программного обеспечения <w>запланирован</w> на следующий месяц."
                            }
                        },
                        {
                            "text": "The committee meeting has been <w>slated</w> for Tuesday afternoon.",
                            "target_lang_translations": {
                                "ru": "Заседание комитета <w>назначено</w> на вторую половину дня вторника."
                            }
                        }
                    ],
                    "common_phrases": [
                        "slated for",
                        "slated to"

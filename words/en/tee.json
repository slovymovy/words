        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3cc44ac6-2782-3474-9f17-d52efa6beb59",
                    "sense_definition": "A casual short-sleeved shirt, usually made of cotton; short for <w>tee-shirt</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wore a simple white <w>tee</w> and jeans to the party.",
                            "target_lang_translations": {
                                "ru": "Она надела простую белую <w>футболку</w> и джинсы на вечеринку."
                            }
                        },
                        {
                            "text": "I bought a souvenir <w>tee</w> from the concert.",
                            "target_lang_translations": {
                                "ru": "Я купил сувенирную <w>футболку</w> на концерте."
                            }
                        }
                    ],
                    "synonyms": [
                        "tee up"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bd36884c-1bec-37f2-b172-439715aa700c",
                    "sense_definition": "Wyciągać rękę lub inny przedmiot w jakimś kierunku, aby czegoś dotknąć lub to chwycić.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko próbowało <w>sięgać</w> po zabawkę leżącą na stole.",
                            "target_lang_translations": {
                                "ru": "Ребенок пытался <w>тянуться</w> за игрушкой, лежащей на столе."
                            }
                        },
                        {
                            "text": "Musisz <w>sięgać</w> wysoko, żeby zdjąć ten karton z szafy.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно <w>тянуться</w> высоко, чтобы снять эту коробку со шкафа."
                            }
                        }
                    ],
                    "synonyms": [
                        "dosięgać"
                    ],
                    "common_phrases": [
                        "sięgać po książkę",
                        "sięgać ręką",
                        "sięgać do kieszeni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Протягивать руку или предмет в каком-либо направлении, чтобы коснуться чего-то или схватить это."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тянуться",
                                "target_lang_sense_clarification": "протягивать руку к чему-либо"
                            },
                            {
                                "target_lang_word": "доставать",
                                "target_lang_sense_clarification": "дотягиваться, брать что-то, находящееся далеко или высоко"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e64cd9c9-490f-3d64-a763-8a4da2581e80",
                    "sense_definition": "Rozciągać się do jakiegoś miejsca, mieć określony zasięg, wysokość lub głębokość.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "extent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Woda w rzece po ulewie zaczęła <w>sięgać</w> niebezpiecznego poziomu.",
                            "target_lang_translations": {
                                "ru": "Вода в реке после ливня начала <w>достигать</w> опасного уровня."
                            }
                        },
                        {
                            "text": "Jej długie włosy potrafiły <w>sięgać</w> aż do pasa.",
                            "target_lang_translations": {
                                "ru": "Ее длинные волосы могли <w>доходить</w> аж до пояса."
                            }
                        }
                    ],
                    "synonyms": [
                        "dosięgać"
                    ],
                    "common_phrases": [
                        "sięgać do kolan",
                        "sięgać horyzontu",
                        "sięgać sufitu",
                        "sięgać zenitu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Простираться до какого-либо места, достигать определенного уровня, высоты или глубины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "достигать",
                                "target_lang_sense_clarification": "доходить до определенного уровня или предела"
                            },
                            {
                                "target_lang_word": "доходить",
                                "target_lang_sense_clarification": "простираться до определенного места"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5813ae27-2d4a-337a-b114-5fbbdba0bbc5",
                    "sense_definition": "Starać się coś zdobyć, uzyskać lub wykorzystać (często w połączeniu z przyimkiem \"po\").",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W trudnych chwilach warto <w>sięgać</w> po radę doświadczonego specjalisty.",
                            "target_lang_translations": {
                                "ru": "В трудные моменты стоит <w>обращаться</w> за советом к опытному специалисту."
                            }
                        },
                        {
                            "text": "Politycy w czasie kryzysu są gotowi <w>sięgać</w> po drastyczne środki.",
                            "target_lang_translations": {
                                "ru": "Политики во время кризиса готовы <w>прибегать</w> к решительным мерам."
                            }
                        }
                    ],
                    "synonyms": [
                        "osiągać"
                    ],
                    "common_phrases": [
                        "sięgać po pomoc",
                        "sięgać po władzę",
                        "sięgać po środki",
                        "sięgać po broń"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пытаться что-то получить, добыть или использовать (часто в сочетании с предлогом «po» — за/к)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прибегать",
                                "target_lang_sense_clarification": "использовать средство или способ (sięgać po środki)"
                            },
                            {
                                "target_lang_word": "обращаться",
                                "target_lang_sense_clarification": "искать помощи или совета (sięgać po radę)"
                            },
                            {
                                "target_lang_word": "стремиться",
                                "target_lang_sense_clarification": "пытаться получить власть или статус"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ba970bc-ecfd-39d8-bda6-8e6a1a9a7300",
                    "sense_definition": "Mieć swój początek w przeszłości, odnosić się do dawnych czasów.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Historia tego średniowiecznego zamku może <w>sięgać</w> czternastego wieku.",
                            "target_lang_translations": {
                                "ru": "История этого средневекового замка может <w>восходить</w> к четырнадцатому веку."
                            }
                        },
                        {
                            "text": "Musimy <w>sięgać</w> pamięcią wstecz, aby zrozumieć przyczyny tego konfliktu.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>возвращаться</w> памятью назад, чтобы понять причины этого конфликта."
                            }
                        }
                    ],
                    "synonyms": [
                        "datować się"
                    ],
                    "common_phrases": [
                        "sięgać pamięcią",
                        "sięgać korzeniami",
                        "sięgać czasów",
                        "sięgać historii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Брать свое начало в прошлом, относиться к давним временам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "восходить",
                                "target_lang_sense_clarification": "относиться к определенному времени в прошлом"
                            },
                            {
                                "target_lang_word": "уходить корнями",
                                "target_lang_sense_clarification": "иметь происхождение в прошлом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee1b1975-ebfb-3bec-a0b8-ae8b727ea4be",
                    "sense_definition": "Korzystać z czyjegoś dorobku artystycznego lub naukowego, czerpać z jakiegoś źródła.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "intellectual_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Współcześni pisarze lubią <w>sięgać</w> do mitologii w swoich powieściach.",
                            "target_lang_translations": {
                                "ru": "Современные писатели любят <w>обращаться</w> к мифологии в своих романах."
                            }
                        },
                        {
                            "text": "Student piszący pracę magisterską musi <w>sięgać</w> do wiarygodnych źródeł.",
                            "target_lang_translations": {
                                "ru": "Студент, пишущий магистерскую диссертацию, должен <w>обращаться</w> к достоверным источникам."
                            }
                        }
                    ],
                    "synonyms": [
                        "czerpać",
                        "zapożyczać"
                    ],
                    "common_phrases": [
                        "sięgać do źródeł",
                        "sięgać do tradycji",
                        "sięgać do klasyków"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать чьи-либо художественные или научные достижения, черпать из какого-либо источника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обращаться",
                                "target_lang_sense_clarification": "использовать источники или традиции"
                            },
                            {
                                "target_lang_word": "черпать",
                                "target_lang_sense_clarification": "заимствовать идеи или вдохновение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d8e94ccc-5c12-31f1-8083-01630b1df53a",
                    "sense_definition": "Having a thick edge or point; not sharp.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_edge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The knife was too <w>blunt</w> to cut the tough meat.",
                            "target_lang_translations": {
                                "ru": "Нож был слишком <w>тупым</w>, чтобы разрезать жесткое мясо."
                            }
                        },
                        {
                            "text": "He was struck on the head by a heavy, <w>blunt</w> object.",
                            "target_lang_translations": {
                                "ru": "Его ударили по голове тяжелым <w>тупым</w> предметом."
                            }
                        }
                    ],
                    "synonyms": [
                        "dull"
                    ],
                    "antonyms": [
                        "sharp"
                    ],
                    "common_phrases": [
                        "blunt object",
                        "blunt instrument",
                        "blunt knife",
                        "blunt edge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий толстый край или острие; неострый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тупой",
                                "target_lang_sense_clarification": "о лезвии или предмете"
                            },
                            {
                                "target_lang_word": "неострый",
                                "target_lang_sense_clarification": "о режущей кромке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9390cfc0-cd71-3d44-8078-a89ff2bdc476",
                    "sense_definition": "Abrupt in address; plain and direct, often to the point of being rude.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To be <w>blunt</w>, I do not think you are qualified for this job.",
                            "target_lang_translations": {
                                "ru": "Буду <w>прямолинеен</w>, я не думаю, что вы подходите для этой работы."
                            }
                        },
                        {
                            "text": "She gave a <w>blunt</w> refusal to his request for a loan.",
                            "target_lang_translations": {
                                "ru": "Она ответила <w>резким</w> отказом на его просьбу о займе."
                            }
                        }
                    ],
                    "synonyms": [
                        "curt",
                        "brusque",
                        "direct",
                        "frank",
                        "rude"
                    ],
                    "antonyms": [
                        "polite",
                        "tactful",
                        "subtle"
                    ],
                    "common_phrases": [
                        "to be blunt",
                        "blunt refusal",
                        "blunt truth",
                        "blunt admission"
                    ],
                    "target_lang_definitions": {
                        "ru": "Резкий в обращении; прямой и откровенный, часто до грубости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прямолинейный",
                                "target_lang_sense_clarification": "откровенный"
                            },
                            {
                                "target_lang_word": "резкий",
                                "target_lang_sense_clarification": "грубоватый"
                            },
                            {
                                "target_lang_word": "грубый",
                                "target_lang_sense_clarification": "невежливый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce5b22b1-485a-3fbc-b135-095d0655052b",
                    "sense_definition": "Dull in understanding; slow to perceive or discern.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "intelligence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His senses had become <w>blunt</w> with age and fatigue.",
                            "target_lang_translations": {
                                "ru": "Его чувства <w>притупились</w> от возраста и усталости."
                            }
                        },
                        {
                            "text": "Despite his <w>blunt</w> wits, he understood the danger immediately.",
                            "target_lang_translations": {
                                "ru": "Несмотря на свой <w>тупой</w> ум, он сразу понял опасность."
                            }
                        }
                    ],
                    "synonyms": [
                        "obtuse",
                        "stupid",
                        "slow"
                    ],
                    "antonyms": [
                        "acute",
                        "sharp",
                        "keen"
                    ],
                    "target_lang_definitions": {

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e167089c-ee83-3130-a480-69f76ab14f6a",
                    "sense_definition": "Разговорное сокращение от слова «специалист»; тот, кто обладает глубокими знаниями, опытом и мастерством в каком-либо деле.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "expert_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нам срочно нужен хороший <w>спец</w>, чтобы починить этот сложный прибор.",
                            "target_lang_translations": {
                                "en": "We urgently need a good <w>expert</w> to fix this complex device."
                            }
                        },
                        {
                            "text": "Мой брат — настоящий <w>спец</w> в программировании, он поможет тебе с кодом.",
                            "target_lang_translations": {
                                "en": "My brother is a real <w>pro</w> at programming; he'll help you with the code."
                            }
                        }
                    ],
                    "synonyms": [
                        "профессионал",
                        "эксперт",
                        "мастер",
                        "дока"
                    ],
                    "antonyms": [
                        "дилетант",
                        "любитель",
                        "неуч",
                        "неумейка"
                    ],
                    "common_phrases": [
                        "классный спец",
                        "крутой спец",
                        "спец по компьютерам",
                        "настоящий спец"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Colloquial abbreviation for \"specialist\"; someone possessing deep knowledge, experience, and skill in a particular field."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pro",
                                "target_lang_sense_clarification": "informal, short for professional"
                            },
                            {
                                "target_lang_word": "expert",
                                "target_lang_sense_clarification": "general meaning"
                            },
                            {
                                "target_lang_word": "whiz",
                                "target_lang_sense_clarification": "someone extremely clever or skillful"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9881d729-cedb-36dc-aa55-758722a0b4f0",
                    "sense_definition": "Боец подразделения специального назначения.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "military_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В здание ворвались вооружённые <w>спецы</w> и быстро освободили заложников.",
                            "target_lang_translations": {
                                "en": "Armed <w>commandos</w> burst into the building and quickly freed the hostages."
                            }
                        },
                        {
                            "text": "Этот опытный <w>спец</w> прошёл множество горячих точек и секретных операций.",
                            "target_lang_translations": {
                                "en": "This experienced <w>operative</w> has been through many hot spots and secret operations."
                            }
                        }
                    ],

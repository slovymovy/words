                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To arrive or show up somewhere unexpectedly (slang)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "roll up",
                                "target_lang_sense_clarification": "informal arrival"
                            },
                            {
                                "target_lang_word": "show up",
                                "target_lang_sense_clarification": "appear unexpectedly"
                            },
                            {
                                "target_lang_word": "descend upon",
                                "target_lang_sense_clarification": "arrive in a group or overwhelmingly"
                            }

            "senses": [
                {
                    "sense_id": "51cb0ab4-7ba1-3dcc-8ed7-be26f24df3ea",
                    "sense_definition": "specjalne nosze używane do przenoszenia trumny lub zwłok",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "funeral_bier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żałobnicy powoli nieśli <mary> z ciałem zmarłego do kaplicy.",
                            "target_lang_translations": {
                                "ru": "Скорбящие медленно несли <погребальные носилки> с телом покойного в часовню."
                            }
                        },
                        {
                            "text": "Ciało złożono na <marach>, aby przenieść je na cmentarz.",
                            "target_lang_translations": {
                                "ru": "Тело положили на <дроги>, чтобы перенести его на кладбище."
                            }
                        }
                    ],
                    "synonyms": [
                        "tragi"
                    ],
                    "common_phrases": [
                        "leżeć na marach"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plurale tantum"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Специальные носилки, используемые для переноски гроба или тела умершего"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "погребальные носилки",
                                "target_lang_sense_clarification": "устройство для переноски"
                            },
                            {
                                "target_lang_word": "дроги",
                                "target_lang_sense_clarification": "повозка или носилки для покойника"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60099878-e8fb-3a8c-86aa-26b39158ecc0",
                    "sense_definition": "podwyższenie lub stojak, na którym stawia się trumnę podczas uroczystości pogrzebowych",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "funeral_bier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trumna stała na wysokich <marach> otoczona wieńcami kwiatów.",
                            "target_lang_translations": {
                                "ru": "Гроб стоял на высоком <катафалке>, окруженный венками цветов."
                            }
                        },
                        {
                            "text": "W kościele ustawiono czarne <mary>, na których spoczął zmarły.",
                            "target_lang_translations": {
                                "ru": "В церкви установили черный <помост>, на котором покоился усопший."
                            }
                        }
                    ],
                    "synonyms": [
                        "katafalk"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plurale tantum"
                        }
                    ],
                    "target_lang_definitions": {

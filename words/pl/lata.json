                {
                    "sense_id": "b1c365a7-070c-318f-8a98-f5d15d603077",
                    "sense_definition": "Forma odmienna (dopełniacz liczby pojedynczej lub mianownik liczby mnogiej) rzeczownika <w>lato</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "season",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie lubię końca <w>lata</w>, bo dni stają się krótsze.",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fd72092b-1e9d-3f5a-9840-ef16fd0ba405",
                    "sense_definition": "Содействовать кому-либо в каком-либо деле, принимать участие в чьей-либо деятельности, облегчая работу или усилия.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "help_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я стараюсь каждый день <помогать> маме по дому."
                        },
                        {
                            "text": "Мой старший брат часто <помогает> мне решать сложные задачи по математике."
                        }
                    ],
                    "synonyms": [
                        "содействовать",
                        "способствовать",
                        "оказывать помощь",
                        "подсоблять"
                    ],
                    "antonyms": [
                        "мешать",
                        "препятствовать"
                    ],
                    "common_phrases": [
                        "помогать людям",
                        "помогать по дому",
                        "помогать в работе",
                        "помогать друг другу"
                    ]
                },
                {
                    "sense_id": "10a9e412-4b1e-3a5e-bc3a-825d3dea9bee",
                    "sense_definition": "Приносить пользу, давать желаемый результат, избавлять от боли или проблем (обычно о лекарствах или методах).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это новое лекарство отлично <помогает> от головной боли."
                        },
                        {
                            "text": "Врач сказал, что отдых и свежий воздух <помогают> лучше любых таблеток."
                        }
                    ],
                    "common_phrases": [
                        "помогать от боли",
                        "лекарство помогает",
                        "ничего не помогает",
                        "хорошо помогать"
                    ]
                },
                {
                    "sense_id": "44a501dd-4ff5-365a-8e3a-2aac9aa4d94f",
                    "sense_definition": "Поддерживать материально, предоставлять деньги или иные средства для жизни.",

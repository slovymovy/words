{
    "word_family": [
        "thruster"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7012fada-65d9-36c0-8011-14a510542933",
                    "sense_definition": "To push something or someone suddenly or violently in a specific direction.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "physical_push",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>thrust</w> his hands deep into his coat pockets to keep them warm.",
                            "target_lang_translations": {
                                "ru": "Он <w>сунул</w> руки глубоко в карманы пальто, чтобы согреть их."
                            }
                        },
                        {
                            "text": "She <w>thrust</w> the papers into my hand and ran away before I could speak.",
                            "target_lang_translations": {
                                "ru": "Она <w>сунула</w> бумаги мне в руку и убежала, прежде чем я успел что-то сказать."
                            }
                        }
                    ],
                    "synonyms": [
                        "push",
                        "shove",
                        "drive",
                        "force"
                    ],
                    "antonyms": [
                        "pull"
                    ],
                    "common_phrases": [
                        "thrust forward",
                        "thrust aside",
                        "thrust into"
                    ],
                    "target_lang_definitions": {
                        "ru": "Резко или с силой толкать что-то или кого-то в определенном направлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "толкать",
                                "target_lang_sense_clarification": "резко двигать от себя"
                            },
                            {
                                "target_lang_word": "сунуть",
                                "target_lang_sense_clarification": "быстро и небрежно положить или вставить"
                            },
                            {
                                "target_lang_word": "пихнуть",
                                "target_lang_sense_clarification": "грубо толкнуть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4815acf-a09d-35b4-8dea-d9581cfce75e",
                    "sense_definition": "To make a sudden forward movement with a weapon or object to pierce or attack.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_push",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The knight <w>thrust</w> his sword at the dragon's heart with all his might.",
                            "target_lang_translations": {
                                "ru": "Рыцарь изо всех сил <w>нанес удар</w> мечом в сердце дракона."
                            }
                        },
                        {
                            "text": "He <w>thrust</w> at the target with the fencing foil to score a point.",
                            "target_lang_translations": {
                                "ru": "Он <w>сделал выпад</w> рапирой в сторону мишени, чтобы заработать очко."
                            }
                        }
                    ],
                    "synonyms": [
                        "stab",
                        "jab",
                        "pierce",
                        "lunge",
                        "spear"
                    ],
                    "common_phrases": [
                        "thrust at",
                        "thrust through"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать резкое движение вперед оружием или предметом с целью пронзить или атаковать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делать выпад",
                                "target_lang_sense_clarification": "в фехтовании или бою"
                            },
                            {
                                "target_lang_word": "вонзать",
                                "target_lang_sense_clarification": "протыкать острием"
                            },
                            {
                                "target_lang_word": "наносить удар",
                                "target_lang_sense_clarification": "атаковать оружием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5963a1e-10a0-3153-a7cf-34de808acf10",
                    "sense_definition": "To force someone to accept something or to put someone in a situation they did not choose.",
                    "learner_level": "C2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Fame was suddenly <w>thrust</w> upon the young actor after his first movie.",
                            "target_lang_translations": {
                                "ru": "Слава внезапно <w>обрушилась</w> на молодого актера после его первого фильма."
                            }
                        },
                        {
                            "text": "I do not want to <w>thrust</w> my personal problems on you right now.",
                            "target_lang_translations": {
                                "ru": "Я не хочу сейчас <w>навязывать</w> тебе свои личные проблемы."
                            }
                        }
                    ],
                    "synonyms": [
                        "force",
                        "impose",
                        "compel"
                    ],
                    "common_phrases": [
                        "thrust upon",
                        "thrust on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять кого-то принять что-то или ставить кого-то в ситуацию против его воли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "навязывать",
                                "target_lang_sense_clarification": "принуждать принять"
                            },
                            {
                                "target_lang_word": "взваливать",
                                "target_lang_sense_clarification": "перекладывать ответственность или проблемы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "843f696f-a94b-3877-9e23-599228204244",
                    "sense_definition": "To extend or stick out prominently, often vertically.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "extension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mountain peak <w>thrust</w> through the clouds like a giant spear.",
                            "target_lang_translations": {
                                "ru": "Горная вершина <w>пронзала</w> облака, словно гигантское копье."
                            }
                        },
                        {
                            "text": "Tall skyscrapers <w>thrust</w> into the sky above the busy city streets.",
                            "target_lang_translations": {
                                "ru": "Высокие небоскребы <w>взмывали</w> в небо над оживленными городскими улицами."
                            }
                        }
                    ],
                    "synonyms": [
                        "stick out",
                        "extend",
                        "reach"
                    ],
                    "common_phrases": [
                        "thrust upward",
                        "thrust out"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often literary or descriptive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выступать или выдаваться вперед, часто вертикально вверх."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вздыматься",
                                "target_lang_sense_clarification": "возвышаться (о горах, зданиях)"
                            },
                            {
                                "target_lang_word": "пронзать",
                                "target_lang_sense_clarification": "проходить сквозь (облака, небо)"
                            },
                            {
                                "target_lang_word": "выступать",
                                "target_lang_sense_clarification": "выдаваться вперед"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6c7f5b8e-ee14-3896-ab6b-443911087a59",
                    "sense_definition": "A sudden, strong push or lunge, often with a weapon or forward body movement.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_push",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "With one powerful <w>thrust</w>, he managed to open the heavy stuck door.",
                            "target_lang_translations": {
                                "ru": "Одним мощным <w>толчком</w> ему удалось открыть тяжелую застрявшую дверь."
                            }
                        },
                        {
                            "text": "The fencer made a quick <w>thrust</w> to score the winning point.",
                            "target_lang_translations": {
                                "ru": "Фехтовальщик сделал быстрый <w>выпад</w>, чтобы заработать победное очко."
                            }
                        }
                    ],
                    "synonyms": [
                        "push",
                        "lunge",
                        "stab",
                        "jab"
                    ],
                    "common_phrases": [
                        "a sudden thrust",
                        "sword thrust"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапный сильный толчок или выпад, часто с оружием или движением тела вперед."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "толчок",
                                "target_lang_sense_clarification": "резкое движение"
                            },
                            {
                                "target_lang_word": "выпад",
                                "target_lang_sense_clarification": "движение в фехтовании"
                            },
                            {
                                "target_lang_word": "удар",
                                "target_lang_sense_clarification": "атакующее действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29ef2ab0-7fb4-3861-9237-c461bb67ba62",
                    "sense_definition": "The main idea, purpose, or essential part of something such as an argument or policy.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The main <w>thrust</w> of his speech was the need for better education funding.",
                            "target_lang_translations": {
                                "ru": "Основной <w>идеей</w> его речи была необходимость лучшего финансирования образования."
                            }
                        },
                        {
                            "text": "We agree with the general <w>thrust</w> of the new proposal, but not the details.",
                            "target_lang_translations": {
                                "ru": "Мы согласны с общей <w>направленностью</w> нового предложения, но не с деталями."
                            }
                        }
                    ],
                    "synonyms": [
                        "gist",
                        "point",
                        "focus",
                        "core"
                    ],
                    "common_phrases": [
                        "main thrust",
                        "general thrust"
                    ],
                    "target_lang_definitions": {
                        "ru": "Главная идея, цель или суть чего-либо, например, аргумента или политики."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "суть",
                                "target_lang_sense_clarification": "главное содержание"
                            },
                            {
                                "target_lang_word": "основная идея",
                                "target_lang_sense_clarification": "главная мысль"
                            },
                            {
                                "target_lang_word": "направленность",
                                "target_lang_sense_clarification": "общий вектор или тенденция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00a659fc-8c5c-33fa-a2c3-f09f0098febe",
                    "sense_definition": "The propulsive force produced by a jet or rocket engine that drives a vehicle forward.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mechanical_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rocket engines generated enough <w>thrust</w> to leave the atmosphere.",
                            "target_lang_translations": {
                                "ru": "Ракетные двигатели создали достаточную <w>тягу</w>, чтобы покинуть атмосферу."
                            }
                        },
                        {
                            "text": "The pilot increased the <w>thrust</w> to make the plane climb higher.",
                            "target_lang_translations": {
                                "ru": "Пилот увеличил <w>тягу</w>, чтобы самолет набрал высоту."
                            }
                        }
                    ],
                    "synonyms": [
                        "propulsion",
                        "force",
                        "drive"
                    ],
                    "antonyms": [
                        "drag"
                    ],
                    "common_phrases": [
                        "engine thrust",
                        "generate thrust"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Движущая сила, создаваемая реактивным или ракетным двигателем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тяга",
                                "target_lang_sense_clarification": "сила двигателя"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
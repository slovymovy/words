    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d02240c9-fc8a-3745-b116-835e0c7c01b1",
                    "sense_definition": "Not having been hauled.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The massive logs lay <w>unhauled</w> by the side of the river for weeks.",
                            "target_lang_translations": {
                                "ru": "Массивные бревна неделями лежали <w>невывезенными</w> на берегу реки."
                            }
                        },
                        {
                            "text": "Due to the sudden strike, the city's garbage remained <w>unhauled</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за внезапной забастовки городской мусор остался <w>невывезенным</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "untransported",
                        "unmoved"
                    ],
                    "antonyms": [
                        "hauled",
                        "transported"
                    ],
                    "common_phrases": [
                        "remained unhauled",
                        "left unhauled"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не перевезенный, не вывезенный или не перемещенный тягой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невывезенный",
                                "target_lang_sense_clarification": "о грузе, мусоре или материалах"
                            },
                            {
                                "target_lang_word": "неперевезенный",
                                "target_lang_sense_clarification": "общий термин для транспортировки"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1e7f7d63-a0c3-3ca7-958c-50ad941e0134",
                    "sense_definition": "Simple past and past participle of <w>unhaul</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sailor <w>unhauled</w> the line to reduce the tension on the sail.",
                            "target_lang_translations": {
                                "ru": "Моряк <w>ослабил</w> трос, чтобы уменьшить натяжение паруса."
                            }
                        },
                        {
                            "text": "He <w>unhauled</w> the heavy gear back from the loading dock.",
                            "target_lang_translations": {
                                "ru": "Он <w>оттащил</w> тяжелое снаряжение обратно от погрузочной платформы."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время от глагола unhaul (ослаблять натяжение, оттаскивать назад)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ослабил",
                                "target_lang_sense_clarification": "о натяжении троса или веревки"
                            },
                            {
                                "target_lang_word": "оттащил",
                                "target_lang_sense_clarification": "переместил волоком обратно"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "83574b12-3830-36fc-b9d7-8f47a3a7c129",
                    "sense_definition": "Идти, распространяться от кого-либо или чего-либо (о запахе, тепле, звуке, свете или информации).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "origin_emanation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "От старой печи <исходило> приятное и уютное тепло.",
                            "target_lang_translations": {
                                "en": "A pleasant and cozy warmth <emanated> from the old stove."
                            }
                        },
                        {
                            "text": "Странный шум <исходил> из соседней комнаты, пугая кошку.",
                            "target_lang_translations": {
                                "en": "A strange noise <was coming> from the next room, scaring the cat."
                            }
                        }
                    ],
                    "synonyms": [
                        "обойти",
                        "обойти",
                        "обойти"
                    ],
                    "common_phrases": [
                        "исходить от человека",
                        "исходить от источника",
                        "исходить паром"
                    ],
                    "target_lang_definitions": {
                        "en": "To originate, flow, or spread from a source (regarding smells, heat, sound, light, or information)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come from",
                                "target_lang_sense_clarification": "general term for origin"
                            },
                            {
                                "target_lang_word": "emanate",
                                "target_lang_sense_clarification": "formal, often used for smells, light, or qualities"
                            },
                            {
                                "target_lang_word": "issue",
                                "target_lang_sense_clarification": "literary, for sounds or orders coming from a source"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4178aa52-b1d6-34f2-ba09-fcad60401c83",
                    "sense_definition": "Брать что-либо за основу для своих действий, решений или рассуждений; руководствоваться чем-либо.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "logical_basis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "При планировании бюджета мы будем <исходить> из реальных доходов.",
                            "target_lang_translations": {
                                "en": "When planning the budget, we will <proceed> from real income."
                            }
                        },
                        {
                            "text": "Адвокат в своей речи <исходил> из показаний главного свидетеля.",
                            "target_lang_translations": {
                                "en": "In his speech, the lawyer <proceeded> from the main witness's testimony."
                            }
                        }
                    ],
                    "synonyms": [
                        "обойти",
                        "обойти",
                        "обойти"
                    ],
                    "common_phrases": [
                        "исходить из того",
                        "исходить из опыта",
                        "исходить из предположения",
                        "исходя из этого"
                    ],
                    "target_lang_definitions": {
                        "en": "To use something as a starting point or basis for reasoning, decisions, or actions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "proceed from",
                                "target_lang_sense_clarification": "to base logic or action on a specific premise"
                            },
                            {
                                "target_lang_word": "start from",
                                "target_lang_sense_clarification": "to begin with a certain assumption"
                            },
                            {
                                "target_lang_word": "be based on",
                                "target_lang_sense_clarification": "to rely on as a foundation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a5df3c4-17d8-3e10-9fe6-9acb7f9e234b",
                    "sense_definition": "Побывать во многих местах, обойти большое пространство пешком (обычно совершенный вид).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За один день туристы <исходили> весь исторический центр города.",
                            "target_lang_translations": {
                                "en": "In one day, the tourists <walked all over> the entire historic city center."
                            }
                        },
                        {
                            "text": "Мы <исходили> всё поле, но так и не нашли потерянные ключи.",
                            "target_lang_translations": {
                                "en": "We <tramped> the whole field but never found the lost keys."
                            }
                        }
                    ],
                    "synonyms": [
                        "обойти"
                    ],
                    "common_phrases": [
                        "исходить всё поле",
                        "исходить весь город",
                        "исходить все тропинки"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies extensive or tiring walking"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To walk through or cover a large area on foot; to traverse completely."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "walk all over",
                                "target_lang_sense_clarification": "to walk extensively across an area"
                            },
                            {
                                "target_lang_word": "traverse",
                                "target_lang_sense_clarification": "formal, to travel across or through"
                            },
                            {
                                "target_lang_word": "crisscross",
                                "target_lang_sense_clarification": "to move back and forth over an area"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3ed6260-2e58-3541-9490-318f38d4a50b",
                    "sense_definition": "Сильно истекать чем-либо (обычно кровью, слезами, потом), терять много сил или жидкости.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physiological_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Раненый боец <исходил> кровью, ожидая помощи медиков.",
                            "target_lang_translations": {
                                "en": "The wounded soldier was <bleeding out> while waiting for medical help."
                            }
                        },
                        {
                            "text": "Мать <исходила> слезами, провожая сына в дальнюю дорогу.",
                            "target_lang_translations": {
                                "en": "The mother was <drowning in tears> as she saw her son off on his long journey."
                            }
                        }
                    ],
                    "synonyms": [
                        "обойти",
                        "обойти",
                        "обойти"
                    ],
                    "common_phrases": [
                        "исходить кровью",
                        "исходить слезами",
                        "исходить криком",
                        "исходить потом"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often found in literary contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To lose a large amount of bodily fluid (blood, tears, sweat) or strength; to flow profusely."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bleed out",
                                "target_lang_sense_clarification": "specifically losing blood"
                            },
                            {
                                "target_lang_word": "be bathed in",
                                "target_lang_sense_clarification": "covered in sweat or tears"
                            },
                            {
                                "target_lang_word": "drain",
                                "target_lang_sense_clarification": "to lose strength or fluid"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac9b30f0-401e-380d-b089-29dc172dbb47",
                    "sense_definition": "Заканчиваться, подходить к концу (о времени, сроке).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "temporal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Срок ультиматума <исходит> завтра на рассвете.",
                            "target_lang_translations": {
                                "en": "The ultimatum <expires> tomorrow at dawn."
                            }
                        },
                        {
                            "text": "Время ожидания уже <исходит>, а ответа всё нет.",
                            "target_lang_translations": {
                                "en": "The waiting time is already <running out>, but there is still no answer."
                            }
                        }
                    ],
                    "synonyms": [
                        "обойти",
                        "обойти",
                        "обойти"
                    ],
                    "common_phrases": [
                        "срок исходит",
                        "время исходит"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern Russian usually uses 'истекать'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To come to an end; to expire (regarding a period of time)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "expire",
                                "target_lang_sense_clarification": "formal, for deadlines or time limits"
                            },
                            {
                                "target_lang_word": "run out",
                                "target_lang_sense_clarification": "general, for time or supplies"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от существительного \"цинк\" (виды цинка).",
                        "en": "Plural of the noun \"zinc\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "виды цинка",
                                "target_lang_sense_clarification": "разновидности металла"

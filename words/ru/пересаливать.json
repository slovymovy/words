                            {
                                "target_lang_word": "salt",
                                "target_lang_sense_clarification": "process a large quantity"
                            },
                            {
                                "target_lang_word": "re-salt",
                                "target_lang_sense_clarification": "salt again"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5010e648-2af8-3ca5-80e3-b538e03bf869",

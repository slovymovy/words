            "pos": "verb",
            "senses": [
                {
                    "sense_id": "aa766532-3e94-3b81-9ffc-7499712a7a19",
                    "sense_definition": "Iemand of iets na een bepaalde tijd opnieuw zien of ontmoeten.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na tien jaar <w>zie</w> ik je eindelijk <w>terug</w>!",
                            "target_lang_translations": {
                                "en": "After ten years I finally <w>see</w> you <w>again</w>!"
                            }
                        },
                        {
                            "text": "Ik hoop dat we elkaar snel zullen <w>terugzien</w>.",
                            "target_lang_translations": {
                                "en": "I hope we will <w>see</w> each other <w>again</w> soon."
                            }
                        },
                        {
                            "text": "Ze wisten dat ze hen nooit meer zouden <w>terugzien</w>.",
                            "target_lang_translations": {
                                "en": "They knew they would never <w>see</w> them <w>again</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "weerzien",
                        "ontmoeten"
                    ],
                    "common_phrases": [
                        "elkaar terugzien",
                        "graag terugzien",
                        "nooit meer terugzien"
                    ],
                    "target_lang_definitions": {
                        "en": "To see or meet someone or something again after a period of time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "see again",
                                "target_lang_sense_clarification": "general meaning"
                            },
                            {
                                "target_lang_word": "meet again",
                                "target_lang_sense_clarification": "specifically regarding people"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2027a6c-ed4a-39b8-a39e-30ae8802f27d",
                    "sense_definition": "Iets herkennen of weerspiegeld zien in iets anders, zoals een resultaat of gedrag.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn liefde voor de natuur liet hij <w>terugzien</w> in zijn werk.",
                            "target_lang_translations": {
                                "en": "He let his love for nature <w>be reflected</w> in his work."
                            }
                        },
                        {
                            "text": "De goede resultaten <w>zie</w> je direct <w>terug</w> in de winstcijfers.",
                            "target_lang_translations": {
                                "en": "You <w>see</w> the good results directly <w>reflected</w> in the profit figures."
                            }
                        }
                    ],
                    "synonyms": [
                        "weerspiegelen",
                        "herkennen",
                        "tonen"
                    ],
                    "common_phrases": [
                        "terugzien in",
                        "laten terugzien"
                    ],
                    "target_lang_definitions": {
                        "en": "To recognize something or see it reflected in something else, such as a result or behavior."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "see reflected",
                                "target_lang_sense_clarification": "to see the effect or result of something"
                            },
                            {
                                "target_lang_word": "recognize",
                                "target_lang_sense_clarification": "to identify traits or characteristics"
                            }
                        ]
                    }
                }
            ]
        }
    ]

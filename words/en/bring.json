{
    "word_family": [
        "bringer",
        "upbringing"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e5427480-0af1-3337-92f5-097b422a9673",
                    "sense_definition": "To carry, convey, or transport someone or something to the place where the speaker is or intends to be.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>bring</w> me a glass of water from the kitchen.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>принеси</w> мне стакан воды из кухни."
                            }
                        },
                        {
                            "text": "Don't forget to <w>bring</w> your books to class tomorrow.",
                            "target_lang_translations": {
                                "ru": "Не забудь <w>принести</w> свои книги завтра на урок."
                            }
                        }
                    ],
                    "synonyms": [
                        "tingaling"
                    ],
                    "antonyms": [
                        "take"
                    ],
                    "common_phrases": [
                        "bring a friend",
                        "bring food",
                        "bring it here"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать, доставлять или транспортировать кого-либо или что-либо в место, где находится или планирует быть говорящий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приносить",
                                "target_lang_sense_clarification": "доставлять что-либо, неся в руках"
                            },
                            {
                                "target_lang_word": "приводить",
                                "target_lang_sense_clarification": "доставлять кого-либо, сопровождая (о людях или животных)"
                            },
                            {
                                "target_lang_word": "привозить",
                                "target_lang_sense_clarification": "доставлять с помощью транспорта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90087dcb-34dc-3f5f-ba46-f4bc8b28fa5d",
                    "sense_definition": "To cause something to happen, exist, or occur as a consequence.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "causative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy rain <w>brought</w> floods to the valley.",
                            "target_lang_translations": {
                                "ru": "Сильный дождь <w>привёл</w> к наводнению в долине."
                            }
                        },
                        {
                            "text": "Her visit <w>brought</w> a lot of joy to the family.",
                            "target_lang_translations": {
                                "ru": "Её визит <w>принёс</w> семье много радости."
                            }
                        }
                    ],
                    "synonyms": [
                        "ringaling"
                    ],
                    "common_phrases": [
                        "bring change",
                        "bring luck",
                        "bring peace",
                        "bring trouble"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть причиной того, что что-то происходит, существует или случается; вызывать последствия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приносить",
                                "target_lang_sense_clarification": "быть причиной появления чего-либо (радости, удачи, пользы)"
                            },
                            {
                                "target_lang_word": "приводить",
                                "target_lang_sense_clarification": "служить причиной, вызывать последствия (обычно с предлогом 'к')"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7bb4dcd2-c56d-378d-9675-1bcec3df54bf",
                    "sense_definition": "To contribute or provide a particular quality, skill, or idea to a situation or group.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "contribute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>brings</w> years of experience to the new job.",
                            "target_lang_translations": {
                                "ru": "Она <w>привносит</w> многолетний опыт в новую работу."
                            }
                        },
                        {
                            "text": "The new director <w>brought</w> a fresh perspective to the team.",
                            "target_lang_translations": {
                                "ru": "Новый директор <w>привнёс</w> свежий взгляд в команду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bring experience",
                        "bring skills",
                        "bring value"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вносить вклад или предоставлять определенное качество, навык или идею в ситуацию или группу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привносить",
                                "target_lang_sense_clarification": "добавлять, вносить что-то новое (идеи, опыт, качества)"
                            },
                            {
                                "target_lang_word": "приносить",
                                "target_lang_sense_clarification": "давать в качестве результата или вклада"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6241403f-96fb-3663-a503-0326326bef95",
                    "sense_definition": "To force or persuade oneself to do something, usually something unpleasant or difficult.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "persuade",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I could not <w>bring</w> myself to tell him the bad news.",
                            "target_lang_translations": {
                                "ru": "Я не мог <w>заставить</w> себя сообщить ему плохие новости."
                            }
                        },
                        {
                            "text": "She finally <w>brought</w> herself to apologize for the mistake.",
                            "target_lang_translations": {
                                "ru": "Она наконец <w>заставила</w> себя извиниться за ошибку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bring myself to",
                        "bring himself to",
                        "bring herself to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять или убеждать себя сделать что-то, обычно неприятное или трудное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заставлять",
                                "target_lang_sense_clarification": "в сочетании 'bring oneself to' — принудить себя сделать что-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b3e1cfb-cd09-3b7c-be5c-f69e756c5cff",
                    "sense_definition": "To formally initiate legal action, such as a lawsuit or charges, against someone.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police decided to <w>bring</w> charges against the suspect.",
                            "target_lang_translations": {
                                "ru": "Полиция решила <w>предъявить</w> обвинения подозреваемому."
                            }
                        },
                        {
                            "text": "They plan to <w>bring</w> a lawsuit against the company for damages.",
                            "target_lang_translations": {
                                "ru": "Они планируют <w>подать</w> иск против компании о возмещении ущерба."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bring charges",
                        "bring a case",
                        "bring suit",
                        "bring an action"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально инициировать юридические действия, такие как судебный процесс или обвинения, против кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предъявлять",
                                "target_lang_sense_clarification": "официально выдвигать (обвинения)"
                            },
                            {
                                "target_lang_word": "подавать",
                                "target_lang_sense_clarification": "направлять официальное заявление (иск)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef4eb199-85eb-30a9-b6f4-df91bc59ae63",
                    "sense_definition": "To sell for a specific amount of money; to fetch a price.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "commercial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The antique painting should <w>bring</w> a high price at the auction.",
                            "target_lang_translations": {
                                "ru": "Старинная картина должна <w>принести</w> высокую цену на аукционе."
                            }
                        },
                        {
                            "text": "How much did your old car <w>bring</w> when you sold it?",
                            "target_lang_translations": {
                                "ru": "Сколько <w>принесла</w> твоя старая машина, когда ты её продал?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "bring a price",
                        "bring money"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть проданным за определенную сумму денег; приносить выручку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приносить",
                                "target_lang_sense_clarification": "давать прибыль или выручку при продаже"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b40b5881-0895-3d9c-84e2-fe1fc36ddd60",
                    "sense_definition": "To perform with great energy, skill, or intensity, often used in sports contexts.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slang_performance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pitcher can really <w>bring</w> it in the final innings.",
                            "target_lang_translations": {
                                "ru": "Питчер действительно может <w>показать класс</w> в последних иннингах."
                            }
                        },
                        {
                            "text": "If you want to win this game, you have to <w>bring</w> it.",
                            "target_lang_translations": {
                                "ru": "Если хочешь выиграть эту игру, ты должен <w>выложиться</w> на полную."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bring it",
                        "bring it on"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "often used in sports"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выступать с большой энергией, мастерством или интенсивностью (часто в спорте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выкладываться",
                                "target_lang_sense_clarification": "действовать с полной отдачей (сленг)"
                            },
                            {
                                "target_lang_word": "показывать класс",
                                "target_lang_sense_clarification": "проявлять высокое мастерство"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "7a0c2f03-53fe-31ad-ab9d-2b2f5dfa2ec8",
                    "sense_definition": "Used to represent the sound of a telephone ringing.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The phone went <w>bring</w> <w>bring</w> in the middle of the night.",
                            "target_lang_translations": {
                                "ru": "Телефон зазвенел <w>дзынь</w>-<w>дзынь</w> посреди ночи."
                            }
                        },
                        {
                            "text": "I heard a loud <w>bring</w> from the hallway and ran to answer it.",
                            "target_lang_translations": {
                                "ru": "Я услышал громкое <w>дзынь</w> из прихожей и побежал отвечать."
                            }
                        }
                    ],
                    "synonyms": [
                        "ringaling",
                        "tingaling"
                    ],
                    "common_phrases": [
                        "bring bring"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для обозначения звука телефонного звонка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дзынь",
                                "target_lang_sense_clarification": "звукоподражание звонку"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
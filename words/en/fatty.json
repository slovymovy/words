    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "de19bddb-7771-3eba-a242-6c27a15918f3",
                    "sense_definition": "Containing or consisting of a large amount of fat.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "content",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doctors often advise patients to reduce their intake of <w>fatty</w> foods.",
                            "target_lang_translations": {
                                "ru": "Врачи часто советуют пациентам сократить потребление <w>жирной</w> пищи."
                            }
                        },
                        {
                            "text": "The meat was too <w>fatty</w> for my taste, so I cut away the edges.",
                            "target_lang_translations": {
                                "ru": "Мясо было слишком <w>жирным</w> на мой вкус, поэтому я срезал края."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fatty food",
                        "fatty tissue",
                        "fatty liver",
                        "fatty meat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержащий большое количество жира или состоящий из него."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жирный",
                                "target_lang_sense_clarification": "о пище"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea65fa26-5e06-3567-b8c2-a0b8511a6a86",
                    "sense_definition": "Relating to the chemical structure or biological properties of fat.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "content",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Omega-3 is a type of essential <w>fatty</w> acid found in fish oil.",
                            "target_lang_translations": {
                                "ru": "Омега-3 — это тип незаменимой <w>жирной</w> кислоты, содержащейся в рыбьем жире."
                            }
                        },
                        {
                            "text": "The scientist analyzed the <w>fatty</w> composition of the substance.",
                            "target_lang_translations": {
                                "ru": "Ученый проанализировал <w>жировой</w> состав вещества."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fatty acid",
                        "fatty alcohol"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к химической структуре или биологическим свойствам жира."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жирный",
                                "target_lang_sense_clarification": "о кислотах"
                            },
                            {
                                "target_lang_word": "жировой",
                                "target_lang_sense_clarification": "о ткани или составе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1110a17e-dc27-356b-9e58-d5a449ee7d65",
                    "sense_definition": "Resembling fat in texture, appearance, or consistency; greasy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "resemblance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soup had a <w>fatty</w> layer floating on the surface.",
                            "target_lang_translations": {
                                "ru": "На поверхности супа плавал <w>жирный</w> слой."
                            }
                        },
                        {
                            "text": "He wiped the <w>fatty</w> residue off the kitchen counter.",
                            "target_lang_translations": {
                                "ru": "Он вытер <w>жирный</w> налет с кухонного стола."
                            }
                        }
                    ],
                    "synonyms": [
                        "fatlike"
                    ],
                    "target_lang_definitions": {
                        "ru": "Напоминающий жир по текстуре, внешнему виду или консистенции; маслянистый."
                    },
                    "translations": {
                        "ru": [
                            {

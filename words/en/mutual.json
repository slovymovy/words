    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8020ab3f-f767-33a7-8532-adf4a59f739d",
                    "sense_definition": "Experienced or done by each of two or more parties toward the other or others.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "reciprocal_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two leaders signed a treaty of <w>mutual</w> support and cooperation.",
                            "target_lang_translations": {
                                "ru": "Два лидера подписали договор о <w>взаимной</w> поддержке и сотрудничестве."
                            }
                        },
                        {
                            "text": "After years of fighting, they finally reached a <w>mutual</w> understanding.",
                            "target_lang_translations": {
                                "ru": "После долгих лет вражды они наконец достигли <w>взаимного</w> понимания."
                            }
                        }
                    ],
                    "synonyms": [
                        "reciprocal",
                        "reciprocative"
                    ],
                    "common_phrases": [
                        "mutual respect",
                        "mutual benefit",
                        "mutual understanding",
                        "mutual agreement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытываемый или совершаемый обеими сторонами по отношению друг к другу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взаимный",
                                "target_lang_sense_clarification": "совершаемый в ответ, обоюдный"
                            },
                            {
                                "target_lang_word": "обоюдный",
                                "target_lang_sense_clarification": "касающийся обеих сторон, общий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2737f2b7-6420-3aa8-81b0-933579be42c2",
                    "sense_definition": "Held in common by two or more parties.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "shared_possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We discovered that we have a <w>mutual</w> friend who lives in Chicago.",
                            "target_lang_translations": {
                                "ru": "Мы обнаружили, что у нас есть <w>общий</w> друг, который живет в Чикаго."
                            }
                        },
                        {
                            "text": "Their <w>mutual</w> interest in classical music brought them together.",
                            "target_lang_translations": {
                                "ru": "Их <w>общий</w> интерес к классической музыке сблизил их."
                            }
                        }
                    ],
                    "synonyms": [
                        "shared",
                        "joint"
                    ],
                    "common_phrases": [
                        "mutual friend",
                        "mutual interest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принадлежащий двум или более сторонам; совместный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "общий",
                                "target_lang_sense_clarification": "принадлежащий всем участникам"
                            },
                            {
                                "target_lang_word": "совместный",
                                "target_lang_sense_clarification": "разделяемый с кем-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "590a4c42-0377-3ac9-b4eb-c5ac077b3603",
                    "sense_definition": "Relating to a company or organization owned by its members rather than shareholders.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "business_type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>mutual</w> insurance company distributes its profits to policyholders.",
                            "target_lang_translations": {
                                "ru": "Компания <w>взаимного</w> страхования распределяет прибыль между страхователями."
                            }
                        },
                        {
                            "text": "They decided to invest their savings in a <w>mutual</w> fund for better stability.",
                            "target_lang_translations": {
                                "ru": "Они решили вложить свои сбережения во <w>взаимный</w> фонд ради большей стабильности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mutual fund",
                        "mutual insurance",
                        "mutual company"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к организации, которой владеют её члены, а не акционеры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взаимный",
                                "target_lang_sense_clarification": "в контексте фондов или страхования"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "621c40a2-0111-345d-9890-bc0fb808c7f3",
                    "sense_definition": "A person on social media who follows you and whom you follow back.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_media_user",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was excited when my favorite artist became a <w>mutual</w> on Twitter.",
                            "target_lang_translations": {
                                "ru": "Я был в восторге, когда мой любимый художник стал моим <w>мьючуалом</w> в Твиттере."
                            }
                        },
                        {
                            "text": "She loves interacting with her <w>mutuals</w> in the comments section.",
                            "target_lang_translations": {
                                "ru": "Она любит общаться со своими <w>мьючуалами</w> в комментариях."

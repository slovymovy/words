                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Worm</w> was listed in the old town directory.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Ворм</w> был указан в старом городском справочнике."
                            }
                        },
                        {
                            "text": "We met a family named <w>Worm</w> during our travels in Germany.",
                            "target_lang_translations": {
                                "ru": "Мы встретили семью по фамилии <w>Ворм</w> во время путешествия по Германии."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {

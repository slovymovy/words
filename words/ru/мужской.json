    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "397f436e-007a-3cea-9824-081e597d3067",
                    "sense_definition": "Связанный с мужчиной, принадлежащий ему или предназначенный для него.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "general_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В магазине продаётся только <мужская> одежда и обувь.",
                            "target_lang_translations": {
                                "en": "The store sells only <men's> clothing and footwear."
                            }
                        },
                        {
                            "text": "За стеной послышался низкий <мужской> голос.",
                            "target_lang_translations": {
                                "en": "A low <male> voice was heard from behind the wall."
                            }
                        }
                    ],
                    "antonyms": [
                        "женский"
                    ],
                    "common_phrases": [
                        "мужская одежда",
                        "мужской голос",
                        "мужской туалет",
                        "мужской монастырь"
                    ],
                    "target_lang_definitions": {
                        "en": "Related to a man, belonging to him, or intended for him."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "men's",
                                "target_lang_sense_clarification": "intended for men (e.g. clothing)"
                            },
                            {
                                "target_lang_word": "male",
                                "target_lang_sense_clarification": "characteristic of a man (e.g. voice)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed172778-6e17-3266-aad8-71affd729bbb",
                    "sense_definition": "Грамматический род, к которому относятся слова, согласующиеся с местоимением «он».",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Слово «стол» в русском языке имеет <мужской> род.",
                            "target_lang_translations": {
                                "en": "The word \"table\" in Russian has <masculine> gender."
                            }
                        },
                        {
                            "text": "Прилагательные изменяются по родам: <мужской>, женский и средний.",
                            "target_lang_translations": {
                                "en": "Adjectives change by gender: <masculine>, feminine, and neuter."
                            }
                        }
                    ],
                    "antonyms": [
                        "женский",
                        "женский"
                    ],
                    "common_phrases": [
                        "мужской род",
                        "существительное мужского рода"
                    ],
                    "target_lang_definitions": {
                        "en": "The grammatical gender to which words agreeing with the pronoun \"he\" belong."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "masculine",
                                "target_lang_sense_clarification": "grammatical gender"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc385235-d522-3f0d-a8c2-7b6b2c77c534",
                    "sense_definition": "Свойственный мужчине; выражающий твёрдость, силу, решительность (о характере или поступках).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_qualities",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это был честный и смелый <мужской> поступок.",
                            "target_lang_translations": {
                                "en": "It was an honest and brave <manly> deed."
                            }
                        },
                        {
                            "text": "У неё был твёрдый <мужской> характер, который помогал ей в бизнесе.",
                            "target_lang_translations": {
                                "en": "She had a firm <masculine> character that helped her in business."
                            }
                        }
                    ],
                    "antonyms": [
                        "женский"
                    ],
                    "common_phrases": [
                        "мужской характер",
                        "мужской поступок",
                        "мужской разговор"
                    ],
                    "target_lang_definitions": {
                        "en": "Characteristic of a man; expressing firmness, strength, determination."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "masculine",
                                "target_lang_sense_clarification": "having qualities associated with men"
                            },
                            {
                                "target_lang_word": "manly",
                                "target_lang_sense_clarification": "brave, strong, or determined"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a69d241b-8052-3346-a75f-8ef621c11883",
                    "sense_definition": "Относящийся к мужскому полу в биологии; свойственный самцам животных или растений.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В лаборатории изучали <мужские> половые клетки.",
                            "target_lang_translations": {
                                "en": "In the laboratory, they studied <male> sex cells."
                            }
                        },
                        {
                            "text": "Некоторые растения имеют только <мужские> цветки.",
                            "target_lang_translations": {
                                "en": "Some plants have only <male> flowers."
                            }
                        }
                    ],
                    "synonyms": [
                        "♂"
                    ],
                    "antonyms": [
                        "женский"
                    ],
                    "common_phrases": [
                        "мужские гормоны",
                        "мужская особь",
                        "мужские хромосомы"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to the male sex in biology; characteristic of male animals or plants."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "male",
                                "target_lang_sense_clarification": "biological sex"
                            }
                        ]
                    }
                }
            ]
        }
    ]

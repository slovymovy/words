    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d7a161c3-8d59-316a-952a-3a96e29a0a75",
                    "sense_definition": "The state or quality of being a nation, often characterized by a shared identity, culture, or history among a group of people.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "state_of_being",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The president gave a speech to strengthen the country's sense of <w>nationhood</w>.",
                            "target_lang_translations": {
                                "ru": "Президент выступил с речью, чтобы укрепить чувство <w>национальной идентичности</w> страны."
                            }
                        },
                        {
                            "text": "Shared traditions and language are essential elements of our distinct <w>nationhood</w>.",
                            "target_lang_translations": {
                                "ru": "Общие традиции и язык являются неотъемлемыми элементами нашей уникальной <w>национальной самобытности</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sense of nationhood",
                        "spirit of nationhood",
                        "concept of nationhood"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние или качество нации, часто характеризующееся общей идентичностью, культурой или историей группы людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "национальная самобытность",
                                "target_lang_sense_clarification": "уникальные культурные черты"
                            },
                            {
                                "target_lang_word": "национальная идентичность",
                                "target_lang_sense_clarification": "осознание принадлежности к нации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "362b3acc-513f-3717-b773-a7847e373f0c",
                    "sense_definition": "The status of having achieved political independence or autonomy as a separate nation.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "state_of_being",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many former colonies struggled for years before they finally achieved <w>nationhood</w>.",
                            "target_lang_translations": {
                                "ru": "Многие бывшие колонии годами боролись, прежде чем наконец обрели <w>государственность</w>."
                            }
                        },
                        {
                            "text": "The treaty officially recognized the territory's transition to full <w>nationhood</w>.",
                            "target_lang_translations": {
                                "ru": "Договор официально признал переход территории к полноценной <w>государственности</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "independence",
                        "sovereignty",
                        "statehood"
                    ],
                    "antonyms": [
                        "dependence"
                    ],
                    "common_phrases": [
                        "achieve nationhood",
                        "path to nationhood",
                        "grant nationhood"
                    ],
                    "target_lang_definitions": {
                        "ru": "Статус достижения политической независимости или автономии как отдельного государства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "государственность",
                                "target_lang_sense_clarification": "политический статус независимости"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "sense_id": "ca685a55-5685-31f0-8c9d-96fb72d8366b",
                    "sense_definition": "Een samenvatting of schematische weergave van de belangrijkste punten of inhoud.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "summary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In dit document vind je een duidelijk <w>overzicht</w> van alle gemaakte kosten.",
                            "target_lang_translations": {
                                "en": "In this document you will find a clear <w>overview</w> of all incurred costs.",
                                "ru": "В этом документе вы найдете четкий <w>обзор</w> всех понесенных расходов."
                            }
                        },
                        {
                            "text": "De leraar gaf een kort <w>overzicht</w> van de geschiedenis van de stad.",
                            "target_lang_translations": {
                                "en": "The teacher gave a short <w>overview</w> of the city's history.",
                                "ru": "Учитель дал краткий <w>обзор</w> истории города."
                            }
                        }
                    ],
                    "synonyms": [
                        "samenvatting",
                        "resumé",
                        "uittreksel",
                        "excerpt"
                    ],
                    "common_phrases": [
                        "een duidelijk overzicht",
                        "financieel overzicht",
                        "compleet overzicht",
                        "overzicht van"
                    ],
                    "target_lang_definitions": {

                {
                    "sense_id": "7beb3168-8096-31b8-8698-3eb568e3f7df",
                    "sense_definition": "Begrijpen hoe iets in elkaar zit of zich bewust worden van een feit of situatie",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "realize",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je zult uiteindelijk wel <inzien> dat dit de beste keuze is voor iedereen.",
                            "target_lang_translations": {
                                "en": "You will eventually <w>see</w> that this is the best choice for everyone.",
                                "ru": "В конце концов ты <поймешь>, что это лучший выбор для всех."
                            }
                        },
                        {
                            "text": "Hij heeft zijn vergissing gelukkig op tijd <ingezien> en zijn excuses aangeboden.",
                            "target_lang_translations": {
                                "en": "Fortunately, he <w>realized</w> his mistake in time and apologized.",
                                "ru": "К счастью, он вовремя <осознал> свою ошибку и принес извинения."
                            }
                        }
                    ],
                    "synonyms": [
                        "beseffen",
                        "begrijpen",
                        "doorhebben",
                        "realiseren"
                    ],
                    "antonyms": [
                        "miskennen"
                    ],
                    "common_phrases": [
                        "zijn fout inzien",
                        "het nut inzien",
                        "de ernst inzien"
                    ],
                    "target_lang_definitions": {
                        "en": "To understand the nature of something or become aware of a fact or situation",
                        "ru": "Понимать суть чего-либо или осознавать факт или ситуацию"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "realize",
                                "target_lang_sense_clarification": "to become fully aware of something as a fact"
                            },
                            {
                                "target_lang_word": "see",
                                "target_lang_sense_clarification": "to understand or comprehend"
                            },
                            {
                                "target_lang_word": "recognize",
                                "target_lang_sense_clarification": "to acknowledge the validity or reality of"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "осознавать",
                                "target_lang_sense_clarification": "понимать, отдавать себе отчет"
                            },
                            {
                                "target_lang_word": "понимать",
                                "target_lang_sense_clarification": "постигать смысл"
                            },
                            {
                                "target_lang_word": "признавать",
                                "target_lang_sense_clarification": "соглашаться с фактом (например, ошибкой)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31a18f10-c9b2-331e-b9fe-4b836a1893cd",
                    "sense_definition": "Een geschrift, document of dossier bekijken om de inhoud te leren kennen of te controleren",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "inspect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De patiënt heeft het recht om zijn eigen medisch dossier <in> te <zien>.",
                            "target_lang_translations": {
                                "en": "The patient has the right to <w>view</w> their own medical file.",
                                "ru": "Пациент имеет право <ознакомиться> со своим медицинским досье."
                            }
                        },
                        {
                            "text": "Wij willen de financiële stukken graag even <inzien> voordat we een beslissing nemen.",
                            "target_lang_translations": {
                                "en": "We would like to <w>have a look at</w> the financial documents before we make a decision.",
                                "ru": "Мы хотели бы <просмотреть> финансовые документы, прежде чем принимать решение."
                            }
                        }
                    ],
                    "synonyms": [
                        "doornemen",
                        "bekijken",
                        "inspecteren",
                        "raadplegen"
                    ],
                    "common_phrases": [
                        "een dossier inzien",
                        "stukken inzien"
                    ],
                    "target_lang_definitions": {
                        "en": "To look at or examine a document or file to learn its contents",
                        "ru": "Просматривать документ или досье для ознакомления с содержанием или проверки"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "inspect",

{
    "word_family": [
        "remark",
        "remarkable"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "834b005e-efb0-3756-a043-1e7c1ae73f37",
                    "sense_definition": "To an unusual or surprising degree.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The weather has been <w>remarkably</w> warm for the middle of winter.",
                            "target_lang_translations": {
                                "ru": "Погода стояла <w>необычайно</w> теплая для середины зимы."
                            }
                        },
                        {
                            "text": "She remained <w>remarkably</w> calm despite the chaos around her.",
                            "target_lang_translations": {
                                "ru": "Она оставалась <w>удивительно</w> спокойной, несмотря на хаос вокруг."
                            }
                        }
                    ],
                    "synonyms": [
                        "surprisingly",
                        "exceptionally",
                        "extraordinarily",
                        "unusually",
                        "extremely"
                    ],
                    "antonyms": [
                        "unremarkably",
                        "ordinarily",
                        "slightly"
                    ],
                    "common_phrases": [
                        "remarkably well",
                        "remarkably similar",
                        "remarkably consistent",
                        "remarkably easy"
                    ],
                    "target_lang_definitions": {
                        "ru": "В необычной или удивительной степени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "необычайно",
                                "target_lang_sense_clarification": "в высшей степени, очень"
                            },
                            {
                                "target_lang_word": "удивительно",
                                "target_lang_sense_clarification": "вызывая удивление"
                            },
                            {
                                "target_lang_word": "исключительно",
                                "target_lang_sense_clarification": "особенно, крайне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ad237d1-2f62-39a1-bafd-663656c249d6",
                    "sense_definition": "Used to emphasize that a statement is surprising or worth noting.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Remarkably</w>, no one was injured in the accident on the highway.",
                            "target_lang_translations": {
                                "ru": "<w>Удивительно</w>, но никто не пострадал в аварии на шоссе."
                            }
                        },
                        {
                            "text": "<w>Remarkably</w>, the ancient vase survived the earthquake without a scratch.",
                            "target_lang_translations": {
                                "ru": "<w>Поразительно</w>, но древняя ваза пережила землетрясение без единой царапины."
                            }
                        }
                    ],
                    "synonyms": [
                        "surprisingly",
                        "amazingly",
                        "astonishingly",
                        "incredibly"
                    ],
                    "antonyms": [
                        "unsurprisingly",
                        "predictably"
                    ],
                    "common_phrases": [
                        "quite remarkably",
                        "most remarkably"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для подчеркивания того, что сказанное удивительно или заслуживает внимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удивительно",
                                "target_lang_sense_clarification": "вводное слово"
                            },
                            {
                                "target_lang_word": "поразительно",
                                "target_lang_sense_clarification": "выражение сильного удивления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa7d6255-3459-3c7d-af59-385838d96c9d",
                    "sense_definition": "In a way that is worthy of attention or striking.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The young pianist performed the complex piece <w>remarkably</w>.",
                            "target_lang_translations": {
                                "ru": "Молодой пианист <w>замечательно</w> исполнил это сложное произведение."
                            }
                        },
                        {
                            "text": "He contributed <w>remarkably</w> to the success of the project.",
                            "target_lang_translations": {
                                "ru": "Он <w>значительно</w> способствовал успеху проекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "notably",
                        "strikingly",
                        "impressively"
                    ],
                    "antonyms": [
                        "unremarkably",
                        "poorly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образом, достойным внимания; впечатляюще."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замечательно",
                                "target_lang_sense_clarification": "очень хорошо, превосходно"
                            },
                            {
                                "target_lang_word": "впечатляюще",
                                "target_lang_sense_clarification": "производя сильное впечатление"
                            },
                            {
                                "target_lang_word": "значительно",
                                "target_lang_sense_clarification": "существенно, заметно"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
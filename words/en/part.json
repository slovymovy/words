    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "13f0a8f3-551f-37dc-8b46-7bcd871a7feb",
                    "sense_definition": "A piece or segment of something such as an object, activity, or period of time, which combined with others makes up the whole.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "component_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This is the most difficult <w>part</w> of the exam.",
                            "target_lang_translations": {
                                "ru": "Это самая сложная <w>часть</w> экзамена."
                            }
                        },
                        {
                            "text": "We spent <w>part</w> of the afternoon cleaning the house.",
                            "target_lang_translations": {
                                "ru": "Мы провели <w>часть</w> дня за уборкой дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "piece",
                        "portion",
                        "component",
                        "element"
                    ],
                    "common_phrases": [
                        "part of",
                        "for the most part",
                        "in part",
                        "large part"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кусок или сегмент чего-либо (объекта, деятельности или периода времени), который в сочетании с другими составляет целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "часть",
                                "target_lang_sense_clarification": "общее значение; элемент целого"
                            },
                            {
                                "target_lang_word": "доля",
                                "target_lang_sense_clarification": "часть, приходящаяся на кого-либо; процентное содержание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16d0d5d9-2d0f-36d7-ac0b-783b1761cfb2",
                    "sense_definition": "A distinct physical component of a machine, vehicle, or body.",

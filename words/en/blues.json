            "senses": [
                {
                    "sense_id": "769d7d3c-d940-32cb-bc59-7c6cfd878ab4",
                    "sense_definition": "Plural of <w>blue</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist used several different <w>blues</w> to paint the ocean.",
                            "target_lang_translations": {
                                "ru": "Художник использовал несколько разных <w>оттенков синего</w>, чтобы нарисовать океан."
                            }
                        },
                        {
                            "text": "I prefer the lighter <w>blues</w> over the darker shades for this room.",
                            "target_lang_translations": {
                                "ru": "Для этой комнаты я предпочитаю более светлые <w>синие тона</w>, чем темные."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова \"синий\"; различные оттенки синего цвета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оттенки синего",
                                "target_lang_sense_clarification": "цвета"
                            },
                            {
                                "target_lang_word": "синие тона",
                                "target_lang_sense_clarification": "цветовая гамма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6936cb7b-75ac-3085-8c6c-23aae243e99b",
                    "sense_definition": "A style of music of African-American origin, often expressing sadness and characterized by a specific rhythm and scale.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "music_genre",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He learned to play the <w>blues</w> on his old guitar.",
                            "target_lang_translations": {
                                "ru": "Он научился играть <w>блюз</w> на своей старой гитаре."
                            }
                        },
                        {
                            "text": "Many modern rock songs have their roots in the <w>blues</w>.",
                            "target_lang_translations": {
                                "ru": "Многие современные рок-песни уходят корнями в <w>блюз</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rhythm and blues",
                        "sing the blues",
                        "blues musician"
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкальный стиль афроамериканского происхождения, часто выражающий грусть и характеризующийся специфическим ритмом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блюз",
                                "target_lang_sense_clarification": "музыкальный жанр"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b19c3b5-0008-391d-840a-695bfcf9e92d",
                    "sense_definition": "A feeling of sadness, depression, or melancholy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have got the <w>blues</w> because my holiday is over.",
                            "target_lang_translations": {
                                "ru": "На меня напала <w>хандра</w>, потому что мой отпуск закончился."
                            }
                        },
                        {
                            "text": "Long winters often give people the <w>blues</w> due to the lack of sunlight.",
                            "target_lang_translations": {
                                "ru": "Долгие зимы часто нагоняют на людей <w>тоску</w> из-за недостатка солнечного света."
                            }
                        }
                    ],
                    "synonyms": [
                        "sadness",
                        "depression",
                        "melancholy",
                        "unhappiness",
                        "misery",
                        "dejection"
                    ],
                    "common_phrases": [
                        "have the blues",
                        "beat the blues",
                        "winter blues"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство грусти, уныния или меланхолии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хандра",
                                "target_lang_sense_clarification": "уныние"
                            },
                            {
                                "target_lang_word": "тоска",
                                "target_lang_sense_clarification": "грусть"
                            },
                            {
                                "target_lang_word": "меланхолия",
                                "target_lang_sense_clarification": "психологическое состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "860f8380-e81a-37c7-bc3d-6cb28820a9b6",
                    "sense_definition": "A formal uniform made of blue fabric, often worn by military personnel.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers looked very smart marching in their dress <w>blues</w>.",
                            "target_lang_translations": {

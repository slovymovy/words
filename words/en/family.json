{
    "word_family": [
        "familial",
        "familiar",
        "familiarity",
        "familiarize",
        "unfamiliar"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "88635fb1-ae8f-39cb-a8fc-127ff03632f9",
                    "sense_definition": "A group of people who are closely related to one another by blood, marriage, or adoption, typically consisting of parents and children.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "kinship_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My <w>family</w> likes to go camping together in the mountains every summer.",
                            "target_lang_translations": {
                                "ru": "Моя <w>семья</w> любит каждое лето ходить вместе в походы в горы."
                            }
                        },
                        {
                            "text": "She wants to introduce her new boyfriend to her immediate <w>family</w>.",
                            "target_lang_translations": {
                                "ru": "Она хочет представить своего нового парня своим ближайшим <w>родственникам</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "relatives",
                        "kin",
                        "kinfolk",
                        "flesh and blood",
                        "bloodline",
                        "fam"
                    ],
                    "common_phrases": [
                        "immediate family",
                        "extended family",
                        "family member",
                        "whole family",
                        "family tree"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, тесно связанных кровным родством, браком или усыновлением, обычно состоящая из родителей и детей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "семья",
                                "target_lang_sense_clarification": "группа родственников"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7eb2912b-c66d-32d6-8dc7-6d93a057814f",
                    "sense_definition": "Members of one's family considered collectively, often referring to relatives living in different places.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "kinship_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have a lot of <w>family</w> living in Australia and New Zealand.",
                            "target_lang_translations": {
                                "ru": "У меня много <w>родни</w> в Австралии и Новой Зеландии."
                            }
                        },
                        {
                            "text": "He does not have much <w>family</w> left in this city anymore.",
                            "target_lang_translations": {
                                "ru": "У него в этом городе почти не осталось <w>родных</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "relatives",
                        "kin"
                    ],
                    "common_phrases": [
                        "have family",
                        "visit family",
                        "family in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Члены семьи как совокупность, часто имеются в виду родственники, живущие в разных местах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "семья",
                                "target_lang_sense_clarification": "родственники, родня"
                            },
                            {
                                "target_lang_word": "родня",
                                "target_lang_sense_clarification": "собирательное, разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76f50689-8762-38cf-8331-5e4ab9a18038",
                    "sense_definition": "A close-knit group of people related by friendship, work, or common goals; also used to refer to a criminal organization.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The small company treats all its employees like one big happy <w>family</w>.",
                            "target_lang_translations": {
                                "ru": "Эта маленькая компания относится ко всем своим сотрудникам как к одной большой счастливой <w>семье</w>."
                            }
                        },
                        {
                            "text": "Police are investigating the activities of a powerful crime <w>family</w>.",
                            "target_lang_translations": {
                                "ru": "Полиция расследует деятельность могущественной преступной <w>семьи</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "crime family",
                        "big happy family"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сплоченная группа людей, связанных дружбой, работой или общими целями; также используется для обозначения преступной организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "семья",
                                "target_lang_sense_clarification": "сообщество, мафия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47206fb0-ff97-32ff-8f71-9e2b7f891e7b",
                    "sense_definition": "A category in the classification of organisms, ranking below order and above genus.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "taxonomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lions, tigers, and domestic cats all belong to the cat <w>family</w>.",
                            "target_lang_translations": {
                                "ru": "Львы, тигры и домашние кошки принадлежат к <w>семейству</w> кошачьих."
                            }
                        },
                        {
                            "text": "This specific plant is a member of the rose <w>family</w>.",
                            "target_lang_translations": {
                                "ru": "Это конкретное растение относится к <w>семейству</w> розовых."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cat family",
                        "rose family",
                        "plant family"
                    ],
                    "target_lang_definitions": {
                        "ru": "Категория в классификации организмов, рангом ниже отряда и выше рода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "семейство",
                                "target_lang_sense_clarification": "в биологии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ab0b6fe-950d-3967-aa9e-9b8ad7193304",
                    "sense_definition": "A group of languages believed to have descended from the same ancestral language.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "taxonomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "English and German are both part of the Germanic <w>family</w> of languages.",
                            "target_lang_translations": {
                                "ru": "Английский и немецкий языки входят в германскую языковую <w>семью</w>."
                            }
                        },
                        {
                            "text": "Linguists study how words evolve within a specific language <w>family</w>.",
                            "target_lang_translations": {
                                "ru": "Лингвисты изучают, как слова эволюционируют внутри конкретной языковой <w>семьи</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "language family"
                    ],
                    "common_phrases": [
                        "language family",
                        "Indo-European family"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа языков, предположительно произошедших от одного языка-предка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "семья",
                                "target_lang_sense_clarification": "языковая"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "008e574a-c2b9-3445-bb14-6d0b8561d6ee",
                    "sense_definition": "Suitable for or designed to be enjoyed by both children and adults.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to eat at a <w>family</w> restaurant because it had a play area.",
                            "target_lang_translations": {
                                "ru": "Мы решили поесть в <w>семейном</w> ресторане, потому что там была игровая зона."
                            }
                        },
                        {
                            "text": "This cinema shows many <w>family</w> movies during the holiday season.",
                            "target_lang_translations": {
                                "ru": "В праздничный сезон этот кинотеатр показывает много <w>семейных</w> фильмов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "family restaurant",
                        "family movie",
                        "family fun",
                        "family friendly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подходящий для детей и взрослых; предназначенный для семейного досуга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "семейный",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
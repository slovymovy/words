                    "sense_id": "b6d620af-95b9-3666-9ce2-d29e7aded771",
                    "sense_definition": "jednostka miary długości używana w krajach anglosaskich, równa 2,54 centymetra",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "units_of_measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój nowy telewizor ma pięćdziesiąt pięć <w>cali</w> i świetną rozdzielczość.",
                            "target_lang_translations": {
                                "ru": "У моего нового телевизора пятьдесят пять <w>дюймов</w> и отличное разрешение."
                            }
                        },
                        {
                            "text": "Hydraulik poprosił o rurę o średnicy pół <w>cala</w> do naprawy zlewu.",
                            "target_lang_translations": {
                                "ru": "Сантехник попросил трубу диаметром в пол<w>дюйма</w> для ремонта раковины."
                            }
                        }
                    ],
                    "common_phrases": [
                        "telewizor 50 cali",
                        "gwint pół cala",
                        "felgi 17 cali"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b85e040e-11b0-30ca-b2d8-4ed626d1788d",
                    "sense_definition": "De handeling waarbij men iets ergens van afhakt.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>afhouwing</w> van de rotspunten was noodzakelijk voor de veiligheid van de klimmers.",
                            "target_lang_translations": {
                                "en": "The <w>hewing off</w> of the jagged rocks was necessary for the safety of the climbers.",
                                "ru": "<w>Скалывание</w> острых выступов скалы было необходимо для безопасности альпинистов."
                            }
                        },
                        {
                            "text": "Tijdens de <w>afhouwing</w> van het marmer moet de beeldhouwer uiterst voorzichtig te werk gaan.",
                            "target_lang_translations": {
                                "en": "During the <w>hewing</w> of the marble, the sculptor must proceed with extreme caution.",
                                "ru": "Во время <w>скалывания</w> мрамора скульптор должен действовать крайне осторожно."
                            }
                        }
                    ],
                    "synonyms": [
                        "afhakking"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of chopping or hewing something off.",
                        "ru": "Действие, при котором что-либо отрубают, отсекают или скалывают."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hewing off",
                                "target_lang_sense_clarification": "action of cutting with blows"
                            },
                            {
                                "target_lang_word": "chopping off",
                                "target_lang_sense_clarification": "action of severing"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отрубание",
                                "target_lang_sense_clarification": "процесс отделения части ударами"
                            },
                            {
                                "target_lang_word": "скалывание",
                                "target_lang_sense_clarification": "для камня, льда или твердых пород"
                            },
                            {
                                "target_lang_word": "отсечение",
                                "target_lang_sense_clarification": "резкое отделение части"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98fa1b97-d273-396c-a93a-945a29433426",
                    "sense_definition": "Het stuk materiaal dat ergens van afgehakt is.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zware <w>afhouwing</w> viel met een doffe klap op de grond naast de boomstam.",
                            "target_lang_translations": {
                                "en": "The heavy <w>hewn piece</w> fell with a dull thud on the ground next to the tree trunk.",
                                "ru": "Тяжелый <w>отрубок</w> с глухим стуком упал на землю рядом со стволом дерева."
                            }
                        },
                        {
                            "text": "Na het werk in de mijn ruimden de arbeiders elke losse <w>afhouwing</w> netjes op.",
                            "target_lang_translations": {
                                "en": "After the work in the mine, the workers tidied up every loose <w>fragment</w>.",
                                "ru": "После работы в шахте рабочие аккуратно убрали каждый валяющийся <w>обломок</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "afhaksel",
                        "brokstuk"
                    ],
                    "target_lang_definitions": {
                        "en": "The piece of material that has been chopped off.",
                        "ru": "Кусок материала, который был отрублен или отколот от чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hewn piece",
                                "target_lang_sense_clarification": "result of hewing"
                            },
                            {
                                "target_lang_word": "fragment",
                                "target_lang_sense_clarification": "broken off piece"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отрубок",
                                "target_lang_sense_clarification": "отрубленный кусок, часто дерева"
                            },
                            {
                                "target_lang_word": "обломок",
                                "target_lang_sense_clarification": "отделившийся кусок"
                            },
                            {
                                "target_lang_word": "скол",
                                "target_lang_sense_clarification": "отколотый кусок твердого материала"

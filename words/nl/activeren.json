{
    "word_family": [
        "activatie",
        "activering",
        "activator",
        "actief"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f69d2b43-1bf3-3adf-a773-1478a762e93a",
                    "sense_definition": "Iets in werking stellen of aanzetten tot actie",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet op de link in de e-mail klikken om je account te <w>activeren</w>.",
                            "target_lang_translations": {
                                "ru": "Вам нужно нажать на ссылку в электронном письме, чтобы <w>активировать</w> свою учетную запись.",
                                "en": "You must click the link in the email to <w>activate</w> your account."
                            }
                        },
                        {
                            "text": "De leraar probeerde de leerlingen te <w>activeren</w> door vragen te stellen.",
                            "target_lang_translations": {
                                "ru": "Учитель пытался <w>активизировать</w> учеников, задавая вопросы.",
                                "en": "The teacher tried to <w>activate</w> the students by asking questions."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanzetten",
                        "starten"
                    ],
                    "antonyms": [
                        "deactiveren"
                    ],
                    "common_phrases": [
                        "een account activeren",
                        "een kaart activeren",
                        "kennis activeren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить что-либо в действие или побуждать к активности",
                        "en": "To put something into operation or incite action"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "активировать",
                                "target_lang_sense_clarification": "привести в действие (учетную запись, карту)"
                            },
                            {
                                "target_lang_word": "активизировать",
                                "target_lang_sense_clarification": "сделать более активным, оживить (внимание, людей)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "activate",
                                "target_lang_sense_clarification": "to make valid or operational (e.g. an account)"
                            },
                            {
                                "target_lang_word": "stimulate",
                                "target_lang_sense_clarification": "to encourage activity or interest"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de7d3abe-acc1-3819-a214-fb3673deaea8",
                    "sense_definition": "Een functie of apparaat inschakelen zodat het werkt",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het inbraakalarm wordt automatisch <w>geactiveerd</w> als je de deur op slot doet.",
                            "target_lang_translations": {
                                "ru": "Охранная сигнализация автоматически <w>активируется</w>, когда вы запираете дверь.",
                                "en": "The burglar alarm is automatically <w>activated</w> when you lock the door."
                            }
                        },
                        {
                            "text": "Druk op deze knop om de bluetooth-functie op je telefoon te <w>activeren</w>.",
                            "target_lang_translations": {
                                "ru": "Нажмите эту кнопку, чтобы <w>активировать</w> функцию Bluetooth на вашем телефоне.",
                                "en": "Press this button to <w>activate</w> the bluetooth function on your phone."
                            }
                        }
                    ],
                    "synonyms": [
                        "inschakelen",
                        "aanzetten"
                    ],
                    "antonyms": [
                        "deactiveren"
                    ],
                    "common_phrases": [
                        "het alarm activeren",
                        "software activeren",
                        "een optie activeren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Включать функцию или устройство для работы",
                        "en": "To switch on a function or device so it works"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "включать",
                                "target_lang_sense_clarification": "запустить устройство или механизм"
                            },
                            {
                                "target_lang_word": "активировать",
                                "target_lang_sense_clarification": "задействовать функцию или режим"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "activate",
                                "target_lang_sense_clarification": "to start a device or system"
                            },
                            {
                                "target_lang_word": "enable",
                                "target_lang_sense_clarification": "to switch on a software function"
                            },
                            {
                                "target_lang_word": "turn on",
                                "target_lang_sense_clarification": "general term for switching on"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "wrakowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ac14ee10-1b4c-33a2-baaa-e059d823dcd6",
                    "sense_definition": "Pozostałości zniszczonego statku, samolotu lub innego pojazdu, który uległ katastrofie lub poważnemu uszkodzeniu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "zniszczony_obiekt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nurkowie zeszli głęboko pod wodę, aby zbadać <w>wrak</w> Titanica.",
                            "target_lang_translations": {
                                "ru": "Водолазы спустились глубоко под воду, чтобы исследовать <w>остов</w> «Титаника»."
                            }
                        },
                        {
                            "text": "Ratownicy odnaleźli <w>wrak</w> samolotu w gęstym lesie.",
                            "target_lang_translations": {
                                "ru": "Спасатели нашли <w>обломки</w> самолета в густом лесу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wrak statku",
                        "wrak samolotu",
                        "wydobyć wrak",
                        "wrak na dnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Остатки разрушенного корабля, самолета или другого транспортного средства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остов",
                                "target_lang_sense_clarification": "каркас разрушенного судна или машины"
                            },
                            {
                                "target_lang_word": "обломки",
                                "target_lang_sense_clarification": "остатки после крушения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2a388fb-31f7-3e28-93b4-bcc6e4b769c5",
                    "sense_definition": "Stary, bardzo zniszczony i często niesprawny samochód lub inna maszyna.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "zniszczony_obiekt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten pordzewiały <w>wrak</w> stojący w garażu nie nadaje się już do jazdy.",
                            "target_lang_translations": {
                                "ru": "Эта ржавая <w>развалина</w>, стоящая в гараже, уже не годится для езды."
                            }
                        },
                        {
                            "text": "Sąsiad próbuje sprzedać swój stary <w>wrak</w> za niewielkie pieniądze.",
                            "target_lang_translations": {
                                "ru": "Сосед пытается продать свою старую <w>развалину</w> за небольшие деньги."
                            }
                        }
                    ],
                    "synonyms": [
                        "grat",
                        "złom",
                        "rupieć",
                        "trup"
                    ],
                    "common_phrases": [
                        "stary wrak",
                        "jeździć wrakiem",
                        "kompletny wrak"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "o pojeździe"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старый, сильно изношенный автомобиль или неисправный механизм."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "развалина",
                                "target_lang_sense_clarification": "о ветхом автомобиле"
                            },
                            {
                                "target_lang_word": "драндулет",
                                "target_lang_sense_clarification": "разг., о старой машине"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86442000-f1a8-3f68-a713-c46873fbd9b6",
                    "sense_definition": "Człowiek całkowicie wyczerpany, schorowany lub wyniszczony psychicznie, często na skutek nałogów lub traumatycznych przeżyć.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Długoletnia choroba i stres sprawiły, że stał się <w>wrakiem</w> człowieka.",
                            "target_lang_translations": {
                                "ru": "Долгая болезнь и стресс превратили его в <w>развалину</w>."
                            }
                        },
                        {
                            "text": "Po śmierci żony czuł się jak psychiczny <w>wrak</w> i unikał ludzi.",
                            "target_lang_translations": {
                                "ru": "После смерти жены он чувствовал себя как душевная <w>развалина</w> и избегал людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "ruina",
                        "żywy trup",
                        "strzęp ludzki",
                        "strzęp człowieka"
                    ],
                    "common_phrases": [
                        "wrak człowieka",
                        "wrak nerwowy",
                        "stać się wrakiem"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "o człowieku"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, физически или морально истощенный, потерявший здоровье."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "развалина",
                                "target_lang_sense_clarification": "о больном или слабом человеке"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
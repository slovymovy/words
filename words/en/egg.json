                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ноль",
                                "target_lang_sense_clarification": "счёт"
                            },
                            {
                                "target_lang_word": "баранка",
                                "target_lang_sense_clarification": "слэнг"
                            }
                        ]
                    }
                }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7218fc31-d71a-332e-8420-15ea1a9acd02",
                    "sense_definition": "Ogół zabiegów i czynności wykonywanych przy hodowli roślin oraz przygotowaniu gleby.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cultivation_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W Polsce <uprawa> roślin zbożowych jest bardzo ważną gałęzią rolnictwa.",
                            "target_lang_translations": {
                                "ru": "В Польше <возделывание> зерновых культур является очень важной отраслью сельского хозяйства."
                            }
                        },
                        {
                            "text": "Mój dziadek uważa, że ręczna <uprawa> ziemi daje najlepsze rezultaty.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка считает, что ручная <обработка> земли дает лучшие результаты."
                            }
                        }
                    ],
                    "synonyms": [
                        "agrokultura",
                        "agrokultura"
                    ],
                    "common_phrases": [
                        "uprawa ziemi",
                        "uprawa roślin",
                        "uprawa roli",
                        "ekologiczna uprawa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность действий по выращиванию растений и обработке почвы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выращивание",
                                "target_lang_sense_clarification": "процесс роста и ухода за растениями"
                            },
                            {
                                "target_lang_word": "возделывание",
                                "target_lang_sense_clarification": "сельскохозяйственная обработка культур"
                            },
                            {
                                "target_lang_word": "обработка",
                                "target_lang_sense_clarification": "подготовка почвы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6fe2efb3-61ba-319e-8be4-0bfba14dc89d",
                    "sense_definition": "Rośliny posiane lub posadzone na określonym terenie w celach gospodarczych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cultivation_plants",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gwałtowna burza i gradobicie zniszczyły wszystkie <uprawy> w naszej okolicy.",
                            "target_lang_translations": {
                                "ru": "Сильная буря и град уничтожили все <посевы> в нашей округе."
                            }
                        },
                        {
                            "text": "Rolnicy martwią się, że tegoroczne <uprawy> zbożowe będą bardzo słabe przez suszę.",
                            "target_lang_translations": {
                                "ru": "Фермеры беспокоятся, что в этом году зерновые <культуры> будут очень слабыми из-за засухи."
                            }
                        }
                    ],
                    "synonyms": [
                        "agrokultura",
                        "agrokultura"
                    ],
                    "common_phrases": [
                        "zniszczone uprawy",
                        "uprawy zbożowe",
                        "stan upraw"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сельскохозяйственные культуры, посаженные на определенной территории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посевы",
                                "target_lang_sense_clarification": "засеянные растения"
                            },
                            {
                                "target_lang_word": "культуры",
                                "target_lang_sense_clarification": "виды выращиваемых растений"
                            },
                            {
                                "target_lang_word": "насаждения",
                                "target_lang_sense_clarification": "посаженные растения (часто о деревьях или кустарниках)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b27007c0-d841-3eb7-ad89-165d573aa556",
                    "sense_definition": "Wydzielony obszar gruntu przeznaczony pod hodowlę konkretnych roślin.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cultivation_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Weszliśmy na teren leśnej <uprawy>, gdzie leśnicy sadzili młode dęby.",
                            "target_lang_translations": {
                                "ru": "Мы вошли на территорию лесной <посадки>, где лесники сажали молодые дубы."
                            }
                        },
                        {
                            "text": "Ta wielka <uprawa> kukurydzy ciągnie się aż po horyzont.",
                            "target_lang_translations": {
                                "ru": "Эта огромная <плантация> кукурузы тянется до самого горизонта."
                            }
                        }
                    ],
                    "synonyms": [
                        "agrokultura",
                        "agrokultura"
                    ],
                    "common_phrases": [
                        "leśna uprawa",
                        "teren uprawy",
                        "wielka uprawa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участок земли, предназначенный для выращивания конкретных растений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плантация",
                                "target_lang_sense_clarification": "большой участок с одной культурой"
                            },
                            {
                                "target_lang_word": "посадка",
                                "target_lang_sense_clarification": "место, где посажены растения (часто лесные)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                        {
                            "trait_type": "form",
                            "comment": "textiles"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Волокно хлопка, шерсти или льна, классифицируемое по длине и качеству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штапель",

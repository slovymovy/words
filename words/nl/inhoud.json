                        "ru": [
                            {
                                "target_lang_word": "содержимое",
                                "target_lang_sense_clarification": "то, что внутри"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33198b03-6521-3f7d-bd23-cd440bc7624c",
                    "sense_definition": "De onderwerpen, ideeën of het verhaal waar een tekst, film of gesprek over gaat.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "content_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar stelde vragen over de <w>inhoud</w> van het boek.",
                            "target_lang_translations": {
                                "en": "The teacher asked questions about the <w>content</w> of the book.",
                                "ru": "Учитель задавал вопросы о <w>содержании</w> книги."
                            }
                        },
                        {
                            "text": "We hebben lang gepraat over de <w>inhoud</w> van de presentatie.",
                            "target_lang_translations": {
                                "en": "We talked for a long time about the <w>content</w> of the presentation.",
                                "ru": "Мы долго говорили о <w>содержании</w> презентации."
                            }
                        }
                    ],
                    "common_phrases": [
                        "inhoud en vorm",
                        "zakelijke inhoud"
                    ],
                    "target_lang_definitions": {
                        "en": "The topics, ideas, or story that a text, film, or conversation is about.",
                        "ru": "Темы, идеи или сюжет, о которых идет речь в тексте, фильме или разговоре."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "content",
                                "target_lang_sense_clarification": "subject matter or ideas"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "содержание",
                                "target_lang_sense_clarification": "смысл, тема"

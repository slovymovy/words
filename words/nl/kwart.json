            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bfa94077-2c6e-36f5-b4fd-b753ecf8ba70",
                    "sense_definition": "Een van de vier gelijke delen van een geheel.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft ongeveer een <w>kwart</w> van zijn huiswerk af.",
                            "target_lang_translations": {
                                "ru": "Он сделал примерно <w>четверть</w> своего домашнего задания."
                            }
                        },
                        {
                            "text": "Het is al <w>kwart</w> over acht, dus we moeten opschieten.",
                            "target_lang_translations": {
                                "ru": "Уже <w>четверть</w> девятого, так что нам нужно поторопиться."
                            }
                        },
                        {
                            "text": "Om het aantal ongelukken te verminderen, wil de minister dat een <w>kwart</w> van de fietsers een helm draagt.",
                            "target_lang_translations": {
                                "ru": "Чтобы сократить количество несчастных случаев, министр хочет, чтобы <w>четверть</w> велосипедистов носила шлемы."
                            }
                        }
                    ],
                    "synonyms": [
                        "vierde"
                    ],
                    "common_phrases": [
                        "een kwart van",
                        "drie kwart",
                        "kwart over",
                        "kwart voor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из четырех равных частей целого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "четверть",
                                "target_lang_sense_clarification": "часть целого или времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98fc2b27-e388-3aba-ba26-3c2e067280e2",
                    "sense_definition": "Een interval met een toonafstand zoals die van de eerste naar de vierde toon van een diatonische toonladder.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "music_interval",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zangeres had moeite om de zuivere <w>kwart</w> te zingen.",
                            "target_lang_translations": {
                                "ru": "Певице было трудно спеть чистую <w>кварту</w>."
                            }
                        },
                        {
                            "text": "Veel blaasinstrumenten hebben voor een interval van een <w>kwart</w> een apart ventiel.",
                            "target_lang_translations": {
                                "ru": "У многих духовых инструментов есть отдельный клапан для интервала <w>кварта</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкальный интервал шириной в четыре ступени звукоряда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кварта",
                                "target_lang_sense_clarification": "музыкальный интервал"
                            }
                        ]
                    }
                }
            ]
        }
    ]

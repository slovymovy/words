                        "spirit"
                    ],
                    "common_phrases": [
                        "mentale veerkracht",
                        "veerkracht tonen"
                    ],
                    "target_lang_definitions": {
                        "en": "The ability to recover quickly from difficulties, stress, or illness.",
                        "ru": "Способность человека быстро восстанавливаться после неудач, стресса или болезни."
                    },
                    "translations": {

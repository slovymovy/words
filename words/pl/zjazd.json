    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f433b83a-a28f-39f1-8758-efa874884785",
                    "sense_definition": "Spotkanie wielu osób w jednym miejscu, często o charakterze oficjalnym, rodzinnym, politycznym lub zawodowym.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "gathering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W przyszłym miesiącu organizujemy wielki <w>zjazd</w> rodzinny u babci na wsi.",
                            "target_lang_translations": {
                                "ru": "В следующем месяце мы организуем большой семейный <w>сбор</w> у бабушки в деревне."
                            }
                        },
                        {
                            "text": "Na <w>zjazd</w> absolwentów przyszło o wiele więcej osób, niż się spodziewaliśmy.",
                            "target_lang_translations": {
                                "ru": "На <w>встречу</w> выпускников пришло гораздо больше людей, чем мы ожидали."
                            }
                        }
                    ],
                    "synonyms": [
                        "zlot",
                        "kongres",
                        "spotkanie",
                        "zebranie",
                        "zgrupowanie",
                        "konwencja",
                        "walne zgromadzenie"
                    ],
                    "common_phrases": [
                        "zjazd rodzinny",
                        "zjazd absolwentów",
                        "zjazd partii",
                        "doroczny zjazd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Встреча большого количества людей в одном месте, часто официального, семейного, политического или профессионального характера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "съезд",
                                "target_lang_sense_clarification": "официальное собрание делегатов"
                            },
                            {
                                "target_lang_word": "слёт",
                                "target_lang_sense_clarification": "массовое собрание членов организации"
                            },
                            {
                                "target_lang_word": "сбор",
                                "target_lang_sense_clarification": "собрание, встреча"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2bfc2610-403c-331e-b098-8e843f5ee561",
                    "sense_definition": "Czynność przemieszczania się w dół, na przykład windą, pojazdem lub na nartach.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "motion_down",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stromy <w>zjazd</w> na rowerze był bardzo ekscytujący i trochę niebezpieczny.",
                            "target_lang_translations": {
                                "ru": "Этот крутой <w>спуск</w> на велосипеде был очень захватывающим и немного опасным."
                            }
                        },
                        {
                            "text": "Kierowca autobusu ogłosił <w>zjazd</w> do zajezdni i wyprosił wszystkich pasażerów.",
                            "target_lang_translations": {
                                "ru": "Водитель автобуса объявил <w>съезд</w> в депо и высадил всех пассажиров."
                            }
                        }
                    ],
                    "synonyms": [
                        "zjeżdżanie"
                    ],
                    "common_phrases": [
                        "szybki zjazd",
                        "zjazd do bazy",
                        "zjazd do zajezdni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Движение вниз или съезд с основного маршрута."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спуск",
                                "target_lang_sense_clarification": "движение вниз"
                            },
                            {
                                "target_lang_word": "съезд",
                                "target_lang_sense_clarification": "действие по глаголу съезжать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ab99213-3b3a-3ebc-9995-47022d40b622",
                    "sense_definition": "Miejsce na drodze szybkiego ruchu lub autostradzie, które umożliwia jej opuszczenie i zmianę kierunku jazdy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "infrastructure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przegapiłem właściwy <w>zjazd</w> z autostrady i musiałem nadrobić sporo drogi.",
                            "target_lang_translations": {
                                "ru": "Я пропустил нужный <w>съезд</w> с автострады, и мне пришлось сделать большой крюк."
                            }
                        },
                        {
                            "text": "Najbliższy <w>zjazd</w> jest zablokowany z powodu trwających robót drogowych.",
                            "target_lang_translations": {
                                "ru": "Ближайший <w>съезд</w> заблокирован из-за дорожных работ."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zjazd z autostrady",
                        "minąć zjazd",
                        "zjazd na Kraków"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место на скоростной дороге или автомагистрали, предназначенное для съезда с неё."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "съезд",
                                "target_lang_sense_clarification": "место поворота с дороги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3448227f-4491-374a-81a1-a0035f4739b1",
                    "sense_definition": "Pochylony fragment terenu lub konstrukcja prowadząca w dół, na przykład do garażu lub piwnicy.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "infrastructure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą ten stromy <w>zjazd</w> pokrywa się lodem i trudno go bezpiecznie pokonać.",
                            "target_lang_translations": {
                                "ru": "Зимой этот крутой <w>спуск</w> покрывается льдом, и по нему трудно безопасно проехать."
                            }
                        },
                        {
                            "text": "<w>Zjazd</w> do podziemnego garażu jest bardzo wąski i wymaga dużej ostrożności.",
                            "target_lang_translations": {
                                "ru": "<w>Спуск</w> в подземный гараж очень узкий и требует большой осторожности."
                            }
                        }
                    ],
                    "synonyms": [
                        "spadek"
                    ],
                    "common_phrases": [
                        "zjazd do garażu",
                        "stromy zjazd",
                        "zjazd dla wózków"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наклонный участок или конструкция, ведущая вниз, например, в гараж."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спуск",
                                "target_lang_sense_clarification": "наклонная поверхность"
                            },
                            {
                                "target_lang_word": "съезд",
                                "target_lang_sense_clarification": "наклонный путь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bec5f584-8f4e-3bfb-bcd2-6637c63d9ee5",
                    "sense_definition": "Konkurencja narciarska polegająca na jak najszybszym pokonaniu wyznaczonej trasy w dół stoku.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polski zawodnik zajął wysokie miejsce w <w>zjeździe</w> podczas ostatnich zawodów zimowych.",
                            "target_lang_translations": {
                                "ru": "Польский спортсмен занял высокое место в <w>скоростном спуске</w> на последних зимних соревнованиях."
                            }
                        },
                        {
                            "text": "Oglądaliśmy w telewizji emocjonujący <w>zjazd</w> narciarski z alpejskiego szczytu.",
                            "target_lang_translations": {
                                "ru": "Мы смотрели по телевизору захватывающий лыжный <w>спуск</w> с альпийской вершины."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zjazd mężczyzn",
                        "mistrz w zjeździe",
                        "zjazd narciarski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лыжная дисциплина, заключающаяся в скоростном прохождении трассы вниз по склону."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скоростной спуск",
                                "target_lang_sense_clarification": "спортивная дисциплина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43c17005-83ac-306a-b81c-00337965ac4c",
                    "sense_definition": "Stan złego samopoczucia fizycznego i psychicznego po ustaniu euforycznego działania narkotyków.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slang_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po całonocnej imprezie chłopak miał straszny <w>zjazd</w> i nie mógł wstać z łóżka.",
                            "target_lang_translations": {
                                "ru": "После вечеринки, длившейся всю ночь, у парня был жуткий <w>отходняк</w>, и он не мог встать с кровати."
                            }
                        },
                        {
                            "text": "Ostrzegali go, że <w>zjazd</w> po tej substancji może być bardzo nieprzyjemny i bolesny.",
                            "target_lang_translations": {
                                "ru": "Его предупреждали, что <w>отходняк</w> после этого вещества может быть очень неприятным и болезненным."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mieć zjazd"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "używane w środowisku osób zażywających substancje psychoaktywne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Плохое физическое и психическое состояние после окончания действия наркотиков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отходняк",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }
            ]
        }
    ]

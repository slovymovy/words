    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b7777b18-c595-36e2-9624-7aee5d4dbbff",
                    "sense_definition": "Совместная с кем-либо деятельность, вовлечённость в какое-либо дело или событие.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы приняли активное <w>участие</w> в обсуждении нового проекта.",
                            "target_lang_translations": {
                                "en": "We took an active <w>part</w> in the discussion of the new project."
                            }
                        },
                        {
                            "text": "Студенты должны подтвердить своё <w>участие</w> в конференции до пятницы.",
                            "target_lang_translations": {
                                "en": "Students must confirm their <w>participation</w> in the conference by Friday."
                            }
                        }
                    ],
                    "synonyms": [
                        "вовлечённость",
                        "сотрудничество",
                        "причастность"
                    ],
                    "antonyms": [
                        "отстранённость",
                        "невмешательство"
                    ],
                    "common_phrases": [
                        "принять участие",
                        "принимать участие",
                        "активное участие"
                    ],
                    "target_lang_definitions": {
                        "en": "Joint activity with others; the act of taking part in an event, business, or matter."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "participation",
                                "target_lang_sense_clarification": "the act of taking part in an activity or event"
                            },
                            {
                                "target_lang_word": "involvement",
                                "target_lang_sense_clarification": "the state of being included or engaged in something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f96fc9f-f27a-3944-93af-c8027888d7b1",
                    "sense_definition": "Сочувственное отношение к чужому горю или неприятностям, внутренняя поддержка.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В её голосе звучало искреннее <w>участие</w> и теплота.",
                            "target_lang_translations": {
                                "en": "There was sincere <w>sympathy</w> and warmth in her voice."
                            }
                        },
                        {
                            "text": "Врач проявил большое <w>участие</w> к состоянию больного пациента.",
                            "target_lang_translations": {
                                "en": "The doctor showed great <w>concern</w> for the sick patient's condition."
                            }
                        }
                    ],
                    "synonyms": [
                        "сочувствие",
                        "сострадание",
                        "жалость"
                    ],
                    "antonyms": [
                        "безразличие",
                        "грубость"
                    ],
                    "common_phrases": [
                        "с участием",
                        "проявить участие",
                        "полный участия"
                    ],
                    "target_lang_definitions": {
                        "en": "A sympathetic attitude towards someone else's grief or trouble; emotional support."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sympathy",
                                "target_lang_sense_clarification": "feelings of pity and sorrow for someone else's misfortune"
                            },
                            {
                                "target_lang_word": "concern",
                                "target_lang_sense_clarification": "care or interest in someone's well-being"
                            },
                            {
                                "target_lang_word": "compassion",
                                "target_lang_sense_clarification": "sympathetic pity and concern for the sufferings of others"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a49a48c4-1ae3-33ab-b76a-e7dabb203941",
                    "sense_definition": "Обладание долей или паем в капитале предприятия, компании или коммерческом проекте.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Банк решил сократить своё <w>участие</w> в уставном капитале завода.",
                            "target_lang_translations": {
                                "en": "The bank decided to reduce its <w>stake</w> in the plant's authorized capital."
                            }
                        },
                        {
                            "text": "Иностранное <w>участие</w> в стратегических компаниях строго регулируется законом.",
                            "target_lang_translations": {
                                "en": "Foreign <w>participation</w> in strategic companies is strictly regulated by law."
                            }
                        }
                    ],
                    "synonyms": [
                        "доля"
                    ],
                    "common_phrases": [
                        "долевое участие"
                    ],
                    "target_lang_definitions": {
                        "en": "Possession of a portion, share, or financial interest in the capital of a company or project."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stake",
                                "target_lang_sense_clarification": "a share or interest in a business or venture"
                            },
                            {
                                "target_lang_word": "share",
                                "target_lang_sense_clarification": "a portion of ownership in a company"
                            },
                            {
                                "target_lang_word": "interest",
                                "target_lang_sense_clarification": "a financial share or legal right in a business"
                            }
                        ]
                    }
                }
            ]
        }
    ]

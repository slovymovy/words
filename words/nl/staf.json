            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2bbba9a6-9e23-32fe-8fb2-9b5cdeef8248",
                    "sense_definition": "Het personeel dat de leiding van een organisatie of afdeling ondersteunt en adviseert.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "personnel_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De medische <w>staf</w> bespreekt de toestand van de patiënten.",
                            "target_lang_translations": {
                                "ru": "Медицинский <w>персонал</w> обсуждает состояние пациентов.",
                                "en": "The medical <w>staff</w> discusses the condition of the patients."
                            }
                        },
                        {
                            "text": "Hij is onlangs toegetreden tot de technische <w>staf</w> van de voetbalclub.",
                            "target_lang_translations": {
                                "ru": "Он недавно вошел в технический <w>штаб</w> футбольного клуба.",
                                "en": "He recently joined the technical <w>staff</w> of the football club."
                            }
                        }
                    ],
                    "synonyms": [
                        "personeel",
                        "kader"
                    ],
                    "common_phrases": [
                        "medische staf",
                        "technische staf",
                        "lid van de staf"
                    ],
                    "target_lang_definitions": {
                        "ru": "Персонал, который поддерживает и консультирует руководство организации или отдела.",
                        "en": "The personnel that supports and advises the management of an organization or department."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "персонал",
                                "target_lang_sense_clarification": "сотрудники организации"
                            },
                            {
                                "target_lang_word": "штат",
                                "target_lang_sense_clarification": "кадровый состав"
                            },
                            {
                                "target_lang_word": "штаб",
                                "target_lang_sense_clarification": "группа специалистов (например, в спорте)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "staff",
                                "target_lang_sense_clarification": "employees of an organization"
                            },
                            {
                                "target_lang_word": "personnel",
                                "target_lang_sense_clarification": "workforce"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7092714-b6fe-3d02-84af-9481525f2c39",
                    "sense_definition": "Een lange stok die dient als steun bij het lopen of als symbool van waardigheid en macht.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "object_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sinterklaas houdt zijn gouden <w>staf</w> altijd in zijn linkerhand.",
                            "target_lang_translations": {
                                "ru": "Синтаклаас всегда держит свой золотой <w>посох</w> в левой руке.",
                                "en": "Sinterklaas always holds his golden <w>staff</w> in his left hand."
                            }
                        },
                        {
                            "text": "De oude herder leunde vermoeid op zijn houten <w>staf</w>.",
                            "target_lang_translations": {
                                "ru": "Старый пастух устало опирался на свой деревянный <w>посох</w>.",
                                "en": "The old shepherd leaned wearily on his wooden <w>staff</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stok"
                    ],
                    "common_phrases": [
                        "de staf van Sinterklaas",
                        "herdersstaf"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная палка, служащая опорой при ходьбе или символом достоинства и власти.",
                        "en": "A long stick serving as support when walking or as a symbol of dignity and power."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посох",
                                "target_lang_sense_clarification": "палка для опоры или символ сана"
                            },
                            {
                                "target_lang_word": "жезл",
                                "target_lang_sense_clarification": "символ власти"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "staff",
                                "target_lang_sense_clarification": "long stick or symbol of authority"
                            },
                            {
                                "target_lang_word": "rod",
                                "target_lang_sense_clarification": "straight stick"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d52c22c1-3b16-38d9-89fa-b674f634557e",
                    "sense_definition": "De groep officieren die de bevelhebber van een legereenheid bijstaat bij de leiding.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "personnel_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De generaal overlegde urenlang met zijn <w>staf</w> over de aanvalsstrategie.",
                            "target_lang_translations": {
                                "ru": "Генерал часами совещался со своим <w>штабом</w> о стратегии нападения.",
                                "en": "The general consulted for hours with his <w>staff</w> about the attack strategy."
                            }
                        },
                        {
                            "text": "De generale <w>staf</w> is gevestigd in het hoofdkwartier van het leger.",
                            "target_lang_translations": {
                                "ru": "Генеральный <w>штаб</w> расположен в штаб-квартире армии.",
                                "en": "The general <w>staff</w> is located at the army headquarters."
                            }
                        }
                    ],
                    "synonyms": [
                        "legerleiding"
                    ],
                    "common_phrases": [
                        "generale staf",
                        "chef van de staf"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа офицеров, помогающая командиру в управлении воинской частью.",
                        "en": "The group of officers assisting the commander of an army unit."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штаб",
                                "target_lang_sense_clarification": "военный орган управления"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "staff",
                                "target_lang_sense_clarification": "military officers"
                            }
                        ]
                    }
                }
            ]
        }
    ]

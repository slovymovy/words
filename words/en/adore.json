{
    "word_family": [
        "adoration",
        "adorable",
        "adorer",
        "adorably"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "801a0a80-f62e-30b3-a4dd-567d50e6745f",
                    "sense_definition": "To love with one's entire heart and soul; to regard with deep respect and affection.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "love_affection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>adores</w> his wife and brings her flowers every week.",
                            "target_lang_translations": {
                                "ru": "Он <w>обожает</w> свою жену и дарит ей цветы каждую неделю."
                            }
                        },
                        {
                            "text": "The children <w>adore</w> their grandmother because she is so kind to them.",
                            "target_lang_translations": {
                                "ru": "Дети <w>обожают</w> свою бабушку, потому что она так добра к ним."
                            }
                        }
                    ],
                    "synonyms": [
                        "love",
                        "cherish",
                        "treasure"
                    ],
                    "antonyms": [
                        "disdain"
                    ],
                    "common_phrases": [
                        "simply adore",
                        "adore him",
                        "adore her"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любить всем сердцем и душой; относиться с глубоким уважением и нежностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обожать",
                                "target_lang_sense_clarification": "очень сильно любить, души не чаять"
                            },
                            {
                                "target_lang_word": "боготворить",
                                "target_lang_sense_clarification": "относиться как к божеству, любить без памяти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "760dc8c4-9874-3c0b-ab02-6b9ae25bec22",
                    "sense_definition": "To be very fond of something or someone.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "love_affection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I absolutely <w>adore</w> the dress you are wearing tonight.",
                            "target_lang_translations": {
                                "ru": "Я просто <w>обожаю</w> платье, которое на тебе сегодня вечером."
                            }
                        },
                        {
                            "text": "She <w>adores</w> Italian food and cooks pasta almost every day.",
                            "target_lang_translations": {
                                "ru": "Она <w>обожает</w> итальянскую еду и готовит пасту почти каждый день."
                            }
                        }
                    ],
                    "synonyms": [
                        "like",
                        "enjoy"
                    ],
                    "common_phrases": [
                        "adore chocolate",
                        "adore music"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень любить что-то или кого-то; испытывать огромную симпатию или удовольствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обожать",
                                "target_lang_sense_clarification": "разговорное: очень любить (еду, одежду, занятия)"
                            },
                            {
                                "target_lang_word": "очень любить",
                                "target_lang_sense_clarification": "испытывать сильную симпатию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd7f5376-d41e-32e6-b976-5d645dc8f505",
                    "sense_definition": "To worship or honor as a deity.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "worship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pilgrims traveled far to <w>adore</w> the sacred relic at the shrine.",
                            "target_lang_translations": {
                                "ru": "Паломники проделали долгий путь, чтобы <w>поклониться</w> священной реликвии в храме."
                            }
                        },
                        {
                            "text": "O come, let us <w>adore</w> Him, Christ the Lord.",
                            "target_lang_translations": {
                                "ru": "О, придите, <w>поклонимся</w> Ему, Христу Господу."
                            }
                        }
                    ],
                    "synonyms": [
                        "worship",
                        "venerate"
                    ],
                    "antonyms": [
                        "disdain"
                    ],
                    "common_phrases": [
                        "come let us adore"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поклоняться или почитать как божество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поклоняться",
                                "target_lang_sense_clarification": "чтить как божество, совершать обряд поклонения"
                            },
                            {
                                "target_lang_word": "благоговеть",
                                "target_lang_sense_clarification": "относиться с глубочайшим почтением"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "928a52db-21e1-39e8-918c-c77d42bafe6f",
                    "sense_definition": "To adorn or decorate.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Precious jewels <w>adore</w> the ancient crown in the museum display.",
                            "target_lang_translations": {
                                "ru": "Драгоценные камни <w>украшают</w> древнюю корону в музейной витрине."
                            }
                        },
                        {
                            "text": "Bright flowers did <w>adore</w> the meadow in the morning light.",
                            "target_lang_translations": {
                                "ru": "Яркие цветы <w>украшали</w> луг в утреннем свете."
                            }
                        }
                    ],
                    "synonyms": [
                        "adorn",
                        "decorate"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "This usage is obsolete and found in older literature."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Украшать или декорировать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "украшать",
                                "target_lang_sense_clarification": "делать красивым, наряжать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
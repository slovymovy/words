            "senses": [
                {
                    "sense_id": "4b2973e7-2f7a-3928-b9d5-0229f4582dda",
                    "sense_definition": "A person who likes, knows about, and appreciates a particular interest or activity; a fan or devotee.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is a true <w>aficionado</w> of classical music and attends every concert in town.",
                            "target_lang_translations": {
                                "ru": "Она истинный <w>ценитель</w> классической музыки и посещает каждый концерт в городе."
                            }
                        },
                        {
                            "text": "As a coffee <w>aficionado</w>, he travels across the city to find the perfect roast.",
                            "target_lang_translations": {
                                "ru": "Будучи <w>знатоком</w> кофе, он ездит по всему городу в поисках идеальной обжарки."
                            }
                        }
                    ],
                    "synonyms": [
                        "enthusiast",
                        "devotee",
                        "connoisseur",
                        "buff",
                        "fan"
                    ],
                    "common_phrases": [
                        "wine aficionado",
                        "jazz aficionado",
                        "art aficionado",
                        "cigar aficionado"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который страстно увлекается определенным занятием, хорошо в нем разбирается и ценит его; фанат или знаток."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ценитель",
                                "target_lang_sense_clarification": "человек, умеющий ценить что-либо, обладающий вкусом"
                            },
                            {
                                "target_lang_word": "знаток",
                                "target_lang_sense_clarification": "человек, обладающий глубокими знаниями в чем-либо"
                            },
                            {
                                "target_lang_word": "страстный поклонник",
                                "target_lang_sense_clarification": "преданный фанат"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6a6e2db-ba91-36e6-adb2-f4f81818787d",
                    "sense_definition": "An amateur bullfighter.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The brave <w>aficionado</w> stepped into the ring to test his skills against the bull.",
                            "target_lang_translations": {
                                "ru": "Храбрый <w>афисьонадо</w> вышел на арену, чтобы испытать свои навыки против быка."
                            }
                        },
                        {
                            "text": "Hemingway wrote extensively about the traditions of the Spanish <w>aficionado</w>.",
                            "target_lang_translations": {

            "senses": [
                {
                    "sense_id": "ab96a1a1-eb3b-3fdd-b713-e2a2586bb4e3",
                    "sense_definition": "Causing feelings of sadness, hopelessness, or low spirits.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotional_effect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The television news was <w>depressing</w>, so I turned it off to feel better.",
                            "target_lang_translations": {
                                "ru": "Телевизионные новости были <w>удручающими</w>, поэтому я выключил их, чтобы почувствовать себя лучше."
                            }
                        },
                        {
                            "text": "It is <w>depressing</w> to see the beautiful old forest being cut down.",
                            "target_lang_translations": {
                                "ru": "<w>Удручающе</w> видеть, как вырубают красивый старый лес."
                            }
                        }
                    ],
                    "synonyms": [
                        "saddening",
                        "gloomy",
                        "dismal",
                        "bleak",
                        "cheerless",
                        "miserable",
                        "disheartening"
                    ],
                    "antonyms": [

{
    "word_family": [
        "vestiging"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4d5c5837-163a-30cd-97b2-5961e7eeada4",
                    "sense_definition": "Ergens gaan wonen of een bedrijf starten op een vaste plek (wederkerend gebruikt).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "settle_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na hun pensioen <w>vestigden</w> zij zich in een klein dorpje in Frankrijk.",
                            "target_lang_translations": {
                                "en": "After their retirement, they <w>settled</w> in a small village in France.",
                                "ru": "После выхода на пенсию они <w>поселились</w> в маленькой деревушке во Франции."
                            }
                        },
                        {
                            "text": "Veel jonge kunstenaars <w>vestigen</w> zich in deze goedkope wijk.",
                            "target_lang_translations": {
                                "en": "Many young artists <w>settle</w> in this cheap neighborhood.",
                                "ru": "Многие молодые художники <w>обосновываются</w> в этом дешевом районе."
                            }
                        }
                    ],
                    "synonyms": [
                        "gaan wonen",
                        "neerstrijken"
                    ],
                    "antonyms": [
                        "vertrekken",
                        "verhuizen"
                    ],
                    "common_phrases": [
                        "zich ergens vestigen",
                        "zich in het buitenland vestigen"
                    ],
                    "target_lang_definitions": {
                        "en": "To go live somewhere or start a business in a fixed place (used reflexively).",
                        "ru": "Поселиться где-либо или начать бизнес на постоянном месте (используется как возвратный глагол)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "settle",
                                "target_lang_sense_clarification": "to move to a place and establish a home"
                            },
                            {
                                "target_lang_word": "establish oneself",
                                "target_lang_sense_clarification": "to set up a business or residence"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "поселиться",
                                "target_lang_sense_clarification": "начать жить в определенном месте"
                            },
                            {
                                "target_lang_word": "обосноваться",
                                "target_lang_sense_clarification": "устроиться на новом месте жительства или работы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34325c4a-0736-3ada-8ddc-ed4b408d920a",
                    "sense_definition": "Iets nieuws, zoals een bedrijf, instelling of record, oprichten of tot stand brengen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "establish_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het internationale bedrijf wil een nieuw kantoor <w>vestigen</w> in Amsterdam.",
                            "target_lang_translations": {
                                "en": "The international company wants to <w>establish</w> a new office in Amsterdam.",
                                "ru": "Международная компания хочет <w>открыть</w> новый офис в Амстердаме."
                            }
                        },
                        {
                            "text": "De atleet trainde hard om een nieuw wereldrecord te <w>vestigen</w>.",
                            "target_lang_translations": {
                                "en": "The athlete trained hard to <w>set</w> a new world record.",
                                "ru": "Атлет упорно тренировался, чтобы <w>установить</w> новый мировой рекорд."
                            }
                        }
                    ],
                    "synonyms": [
                        "oprichten",
                        "stichten",
                        "instellen"
                    ],
                    "antonyms": [
                        "opheffen",
                        "sluiten"
                    ],
                    "common_phrases": [
                        "een record vestigen",
                        "een bedrijf vestigen",
                        "een kantoor vestigen"
                    ],
                    "target_lang_definitions": {
                        "en": "To found or bring about something new, such as a company, institution, or record.",
                        "ru": "Основать или создать что-то новое, например, компанию, учреждение или рекорд."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "establish",
                                "target_lang_sense_clarification": "to set up an organization or business"
                            },
                            {
                                "target_lang_word": "set",
                                "target_lang_sense_clarification": "to achieve a new record"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "основать",
                                "target_lang_sense_clarification": "создать организацию или бизнес"
                            },
                            {
                                "target_lang_word": "установить",
                                "target_lang_sense_clarification": "достичь нового рекорда"
                            },
                            {
                                "target_lang_word": "открыть",
                                "target_lang_sense_clarification": "создать филиал или офис"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f7164f4-5520-358c-a51a-8456dae6ae4a",
                    "sense_definition": "De aandacht of hoop specifiek op iets of iemand richten.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "focus_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De voorzitter wilde de aandacht <w>vestigen</w> op de nieuwe veiligheidsregels.",
                            "target_lang_translations": {
                                "en": "The chairman wanted to <w>draw</w> attention to the new safety rules.",
                                "ru": "Председатель хотел <w>обратить</w> внимание на новые правила безопасности."
                            }
                        },
                        {
                            "text": "Alle hoop is nu op de nieuwe directeur <w>gevestigd</w>.",
                            "target_lang_translations": {
                                "en": "All hope is now <w>pinned</w> on the new director.",
                                "ru": "Все надежды теперь <w>возлагаются</w> на нового директора."
                            }
                        }
                    ],
                    "synonyms": [
                        "richten",
                        "focussen"
                    ],
                    "antonyms": [
                        "afleiden"
                    ],
                    "common_phrases": [
                        "de aandacht vestigen op",
                        "zijn hoop vestigen op"
                    ],
                    "target_lang_definitions": {
                        "en": "To direct attention or hope specifically towards something or someone.",
                        "ru": "Направить внимание или надежды конкретно на что-то или кого-то."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "draw",
                                "target_lang_sense_clarification": "to direct (attention)"
                            },
                            {
                                "target_lang_word": "focus",
                                "target_lang_sense_clarification": "to concentrate (attention or effort)"
                            },
                            {
                                "target_lang_word": "pin",
                                "target_lang_sense_clarification": "to place (hope) on someone"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "обратить",
                                "target_lang_sense_clarification": "направить (внимание)"
                            },
                            {
                                "target_lang_word": "возлагать",
                                "target_lang_sense_clarification": "связывать (надежды) с кем-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
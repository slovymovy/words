                    ],
                    "synonyms": [
                        "вручать",
                        "отдавать"
                    ],
                    "antonyms": [
                        "забирать",
                        "принимать",
                        "брать"
                    ],
                    "common_phrases": [
                        "передавать соль",
                        "передавать деньги",
                        "передавать документы",
                        "передавать из рук в руки"

                            {
                                "target_lang_word": "tough",
                                "target_lang_sense_clarification": "strong, resilient, or acting confident"
                            },
                            {
                                "target_lang_word": "brave",
                                "target_lang_sense_clarification": "showing courage; often used for children"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "крутой",
                                "target_lang_sense_clarification": "сильный, уверенный в себе (разг.)"
                            },
                            {
                                "target_lang_word": "брутальный",
                                "target_lang_sense_clarification": "мужественный, грубоватый"
                            },
                            {
                                "target_lang_word": "смелый",
                                "target_lang_sense_clarification": "храбрый, отважный"

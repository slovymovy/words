    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c022b92c-93ef-398b-8276-e2c5b9a5d020",
                    "sense_definition": "Увезти кого-либо или что-либо из какого-то места на транспортном средстве.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <вывезти> старую мебель на дачу в эти выходные.",
                            "target_lang_translations": {
                                "en": "We decided to <haul> the old furniture <out> to the dacha this weekend."
                            }
                        },
                        {
                            "text": "Спасатели успели <вывезти> всех жителей из опасной зоны.",
                            "target_lang_translations": {
                                "en": "Rescuers managed to <evacuate> all residents from the danger zone."
                            }
                        }
                    ],
                    "common_phrases": [
                        "вывезти мусор",
                        "вывезти вещи",
                        "вывезти семью"
                    ],
                    "target_lang_definitions": {
                        "en": "To transport someone or something away from a place using a vehicle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take out",
                                "target_lang_sense_clarification": "general term for moving something away"
                            },
                            {
                                "target_lang_word": "haul away",
                                "target_lang_sense_clarification": "often used for heavy items, furniture, or trash"
                            },
                            {
                                "target_lang_word": "evacuate",
                                "target_lang_sense_clarification": "specifically for moving people from danger"
                            },
                            {
                                "target_lang_word": "remove",
                                "target_lang_sense_clarification": "formal, to take something away"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f3fa991-f0aa-3799-b8a2-c326b5957e50",
                    "sense_definition": "Отправить товары или ресурсы в другую страну для продажи.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Компания планирует <вывезти> крупную партию зерна за границу.",
                            "target_lang_translations": {
                                "en": "The company plans to <export> a large consignment of grain abroad."
                            }
                        },
                        {
                            "text": "Без разрешения таможни нельзя <вывезти> культурные ценности из страны.",
                            "target_lang_translations": {
                                "en": "You cannot <take> cultural valuables <out> of the country without customs permission."
                            }
                        }
                    ],
                    "synonyms": [
                        "экспорти́ровать"
                    ],
                    "common_phrases": [
                        "вывезти капитал",
                        "вывезти за границу",
                        "вывезти сырьё"
                    ],
                    "target_lang_definitions": {
                        "en": "To send goods or resources to another country for sale; to export."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "export",
                                "target_lang_sense_clarification": "commercial context"
                            },
                            {
                                "target_lang_word": "take out",
                                "target_lang_sense_clarification": "moving items across a border"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec563250-cb3b-33b1-851e-278868f50975",
                    "sense_definition": "Помочь выйти из трудного положения, спасти от беды или неудачи.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_help",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он надеялся, что счастливый случай снова <вывезет> его из неприятностей.",
                            "target_lang_translations": {
                                "en": "He hoped that a lucky break would <get> him <out> of trouble again."
                            }
                        },
                        {
                            "text": "Только твой опыт и спокойствие могут <вывезти> нас в этой ситуации.",
                            "target_lang_translations": {
                                "en": "Only your experience and calmness can <pull> us <through> in this situation."
                            }
                        }
                    ],
                    "synonyms": [
                        "вы́ручить"
                    ],
                    "common_phrases": [
                        "кривая вывезет",
                        "авось вывезет"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in figurative speech"
                        }
                    ],
                    "target_lang_definitions": {

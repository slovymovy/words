    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "39fb365c-3a41-3ca0-ab5f-7add65bb06ed",
                    "sense_definition": "Связанный с торговлей, бизнесом и деловыми отношениями.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наш <w>коммерческий</w> директор предложил новую стратегию развития.",
                            "target_lang_translations": {
                                "en": "Our <w>commercial</w> director proposed a new development strategy."
                            }
                        },
                        {
                            "text": "Фильм имел огромный <w>коммерческий</w> успех во всём мире.",
                            "target_lang_translations": {
                                "en": "The film was a huge <w>commercial</w> success all over the world."
                            }
                        }
                    ],
                    "synonyms": [
                        "торговый",
                        "деловой"
                    ],
                    "antonyms": [
                        "некомме́рческий"
                    ],
                    "common_phrases": [
                        "коммерческий директор",
                        "коммерческий успех",
                        "коммерческая тайна",
                        "коммерческое предложение"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to trade, business activities, and the making of profit."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "commercial",
                                "target_lang_sense_clarification": "related to commerce or market success"
                            },
                            {
                                "target_lang_word": "business",
                                "target_lang_sense_clarification": "related to professional or trade activities"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dafd4f89-a3df-388c-9381-e771fe458d6e",
                    "sense_definition": "Созданный для получения прибыли, занимающийся частным предпринимательством (об организациях и предприятиях).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Крупный <w>коммерческий</w> банк открыл филиал в нашем районе.",
                            "target_lang_translations": {
                                "en": "A large <w>commercial</w> bank opened a branch in our district."
                            }
                        },
                        {
                            "text": "Это частная <w>коммерческая</w> организация, а не государственное учреждение.",
                            "target_lang_translations": {
                                "en": "This is a private <w>commercial</w> organization, not a government institution."
                            }
                        }
                    ],
                    "synonyms": [
                        "доходный"
                    ],
                    "antonyms": [
                        "государственный",
                        "благотворительный",
                        "муниципальный",
                        "некомме́рческий"
                    ],
                    "common_phrases": [
                        "коммерческий банк",
                        "коммерческая организация",
                        "коммерческое предприятие",
                        "коммерческая структура"
                    ],
                    "target_lang_definitions": {
                        "en": "Established for the purpose of making a profit; private rather than state-run (regarding organizations)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "commercial",
                                "target_lang_sense_clarification": "operating for profit"
                            },
                            {
                                "target_lang_word": "for-profit",
                                "target_lang_sense_clarification": "aimed at earning money"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99b34bc9-9513-3f4f-aa7f-1d22a233878b",
                    "sense_definition": "Осуществляемый на платной основе; предоставляемый за деньги (об услугах, обучении, медицине).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студент не прошел на бюджет и поступил на <w>коммерческое</w> отделение.",
                            "target_lang_translations": {
                                "en": "The student didn't get a state-funded spot and enrolled in the <w>fee-paying</w> department."
                            }
                        },
                        {
                            "text": "В этой клинике есть <w>коммерческие</w> палаты повышенной комфортности.",
                            "target_lang_translations": {
                                "en": "This clinic has <w>private</w> rooms with increased comfort."
                            }
                        }
                    ],
                    "synonyms": [
                        "доходный"
                    ],
                    "antonyms": [
                        "государственный",
                        "благотворительный"
                    ],
                    "common_phrases": [
                        "коммерческая основа",
                        "коммерческое отделение",
                        "коммерческая медицина"
                    ],
                    "target_lang_definitions": {
                        "en": "Provided in exchange for payment; not free or state-funded (regarding services, education, or medicine)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fee-paying",
                                "target_lang_sense_clarification": "requiring payment (e.g., education)"
                            },
                            {
                                "target_lang_word": "private",
                                "target_lang_sense_clarification": "paid for personally, not public"
                            },
                            {
                                "target_lang_word": "commercial",
                                "target_lang_sense_clarification": "available for a price"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e293a1e9-9f3f-3112-881b-d21aaad4b4f8",
                    "sense_definition": "Предназначенный для перевозки грузов или использования в торговом обороте (о транспорте).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Завод начал выпускать легкий <w>коммерческий</w> транспорт для малого бизнеса.",
                            "target_lang_translations": {
                                "en": "The factory started producing light <w>commercial</w> vehicles for small businesses."
                            }
                        },
                        {
                            "text": "В порт зашли иностранные <w>коммерческие</w> суда с товарами.",
                            "target_lang_translations": {
                                "en": "Foreign <w>merchant</w> vessels with goods entered the port."
                            }
                        }
                    ],
                    "synonyms": [
                        "грузовой",
                        "товарный"
                    ],
                    "common_phrases": [
                        "коммерческий транспорт",
                        "коммерческий автомобиль",
                        "коммерческий рейс"
                    ],
                    "target_lang_definitions": {
                        "en": "Designed for transporting goods or used for business purposes (regarding vehicles)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "commercial",
                                "target_lang_sense_clarification": "used for business transport"
                            },
                            {
                                "target_lang_word": "merchant",
                                "target_lang_sense_clarification": "related to shipping goods (e.g., vessels)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dfbd94e0-cdc7-3a68-8f37-47262d715808",
                    "sense_definition": "Связанный с незаконным получением денег или услуг в сфере управления организацией (юридический термин).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Директора фирмы обвинили в преступлении, которое квалифицируется как <w>коммерческий</w> подкуп.",
                            "target_lang_translations": {
                                "en": "The firm's director was accused of a crime qualified as <w>commercial</w> bribery."
                            }
                        },
                        {
                            "text": "Статья уголовного кодекса предусматривает штраф за <w>коммерческий</w> подкуп.",
                            "target_lang_translations": {
                                "en": "The criminal code article provides for a fine for <w>commercial</w> bribery."
                            }
                        }
                    ],
                    "common_phrases": [
                        "коммерческий подкуп"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to illegal payments or corruption within a corporation (legal term)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "commercial",
                                "target_lang_sense_clarification": "in legal contexts like bribery"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
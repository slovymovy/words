{
    "word_family": [
        "kościół",
        "kościółek"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "64b45902-211b-3207-9840-0f23899b0b76",
                    "sense_definition": "Związany z kościołem jako instytucją religijną, wspólnotą wiernych lub budynkiem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "religion_rel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młoda para zdecydowała się na tradycyjny ślub <kościelny> w swojej parafii.",
                            "target_lang_translations": {
                                "ru": "Молодая пара решилась на традиционное <церковное> венчание в своем приходе."
                            }
                        },
                        {
                            "text": "W każdą niedzielę rano słychać głośne dzwony <kościelne> w całej okolicy.",
                            "target_lang_translations": {
                                "ru": "Каждое воскресное утро во всей округе слышны громкие <церковные> колокола."
                            }
                        }
                    ],
                    "antonyms": [
                        "świecki",
                        "cywilny"
                    ],
                    "common_phrases": [
                        "ślub kościelny",
                        "dzwon kościelny",
                        "prawo kościelne",
                        "muzyka kościelna",
                        "służba kościelna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с церковью как религиозным учреждением, общиной верующих или зданием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "церковный",
                                "target_lang_sense_clarification": "относящийся к церкви"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "42c7ce59-41c2-3c6d-a554-945e08ac4be8",
                    "sense_definition": "Osoba świecka (zwykle mężczyzna) opiekująca się budynkiem kościoła, przygotowująca ołtarz i szaty liturgiczne do nabożeństw.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "church_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz <kościelny> zawsze otwiera drzwi świątyni pół godziny przed pierwszą mszą.",
                            "target_lang_translations": {
                                "ru": "Наш <ризничий> всегда открывает двери храма за полчаса до первой мессы."
                            }
                        },
                        {
                            "text": "Ksiądz poprosił <kościelnego> o zapalenie wszystkich świec na ołtarzu.",
                            "target_lang_translations": {
                                "ru": "Священник попросил <ризничего> зажечь все свечи на алтаре."
                            }
                        }
                    ],
                    "synonyms": [
                        "zakrystian"
                    ],
                    "common_phrases": [
                        "obowiązki kościelnego",
                        "pan kościelny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Светское лицо (обычно мужчина), присматривающее за зданием церкви, подготавливающее алтарь и литургические облачения к богослужениям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ризничий",
                                "target_lang_sense_clarification": "служитель, ответственный за церковную утварь и облачение"
                            },
                            {
                                "target_lang_word": "пономарь",
                                "target_lang_sense_clarification": "служитель в православной церкви"
                            },
                            {
                                "target_lang_word": "церковный служитель",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "aaf461f9-e925-3e88-90f0-6c61996064cb",
                    "sense_definition": "Polskie nazwisko męskie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <Kościelny> mieszka w domu naprzeciwko urzędu gminy.",
                            "target_lang_translations": {
                                "ru": "Господин <Косцельный> живет в доме напротив здания гмины."
                            }
                        },
                        {
                            "text": "Nie znalazłem nazwiska <Kościelny> na liście zaproszonych gości.",
                            "target_lang_translations": {
                                "ru": "Я не нашел фамилии <Косцельный> в списке приглашенных гостей."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская мужская фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Косцельный",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
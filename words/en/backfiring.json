                    ],
                    "synonyms": [
                        "failure",
                        "fiasco"
                    ],
                    "antonyms": [
                        "success"
                    ],
                    "target_lang_definitions": {
                        "ru": "Провал плана или действия, приводящий к результату, противоположному ожидаемому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обратный эффект",
                                "target_lang_sense_clarification": "нежелательный результат"
                            },
                            {
                                "target_lang_word": "неожиданный провал",
                                "target_lang_sense_clarification": "неудача стратегии"
                            }
                        ]
                    }
                }
            ]
        }

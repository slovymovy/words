    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c5f13dc2-e69e-3291-965b-8325cb43fee9",
                    "sense_definition": "Without specific purpose, intent, or effort.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "aimless",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She sat on the porch and <w>idly</w> watched the cars pass by.",
                            "target_lang_translations": {
                                "ru": "Она сидела на крыльце и <w>лениво</w> наблюдала за проезжающими машинами."
                            }
                        },
                        {
                            "text": "He <w>idly</w> flipped through the pages of the magazine without reading anything.",
                            "target_lang_translations": {
                                "ru": "Он <w>рассеянно</w> листал страницы журнала, ничего не читая."
                            }
                        }
                    ],
                    "synonyms": [
                        "aimlessly",
                        "casually",
                        "absent-mindedly"
                    ],
                    "antonyms": [
                        "purposefully",
                        "intently"
                    ],
                    "common_phrases": [
                        "chat idly",
                        "wonder idly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без определенной цели, намерения или усилий; просто так."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лениво",
                                "target_lang_sense_clarification": "без напряжения, нехотя"
                            },
                            {
                                "target_lang_word": "бесцельно",
                                "target_lang_sense_clarification": "без конкретной цели"
                            },
                            {
                                "target_lang_word": "рассеянно",
                                "target_lang_sense_clarification": "невнимательно, механически"
                            },
                            {
                                "target_lang_word": "праздно",
                                "target_lang_sense_clarification": "без дела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "034c1e89-c04e-3724-b9e5-930f7f000a65",
                    "sense_definition": "In an inactive or lazy way; without taking action.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "inactive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We cannot stand <w>idly</w> by while innocent people are suffering.",
                            "target_lang_translations": {
                                "ru": "Мы не можем стоять <w>сложа руки</w>, пока страдают невинные люди."
                            }
                        },
                        {
                            "text": "The guards sat <w>idly</w> at the gate, ignoring the commotion outside.",
                            "target_lang_translations": {
                                "ru": "Охранники <w>бездеятельно</w> сидели у ворот, игнорируя шум снаружи."
                            }
                        }
                    ],
                    "synonyms": [
                        "passively",
                        "inactively",
                        "lazily"
                    ],
                    "antonyms": [
                        "actively",
                        "busily"
                    ],
                    "common_phrases": [
                        "stand idly by",
                        "sit idly by"
                    ],
                    "target_lang_definitions": {
                        "ru": "В состоянии бездействия или пассивности; не предпринимая никаких мер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бездеятельно",
                                "target_lang_sense_clarification": "не совершая действий"
                            },
                            {
                                "target_lang_word": "безучастно",
                                "target_lang_sense_clarification": "равнодушно, не вмешиваясь"
                            },
                            {
                                "target_lang_word": "сложа руки",
                                "target_lang_sense_clarification": "идиома: в полном бездействии"
                            }
                        ]
                    }
                }
            ]
        },

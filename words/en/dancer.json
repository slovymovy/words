    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e29b72ca-8f25-378b-9c4a-c4078d63f64a",
                    "sense_definition": "A person who dances, usually as a hobby, an occupation, or a profession.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is a talented <w>dancer</w> who performs in the theater.",
                            "target_lang_translations": {
                                "ru": "Она талантливая <w>танцовщица</w>, выступающая в театре."
                            }
                        },
                        {
                            "text": "He is a professional <w>dancer</w> with a famous ballet company.",
                            "target_lang_translations": {
                                "ru": "Он профессиональный <w>танцовщик</w> в известной балетной труппе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "professional dancer",
                        "ballet dancer",
                        "tap dancer",
                        "good dancer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который танцует, обычно в качестве хобби или профессии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "танцор",
                                "target_lang_sense_clarification": "общее название для мужчины или человека любого пола"
                            },
                            {
                                "target_lang_word": "танцовщица",
                                "target_lang_sense_clarification": "женский род"
                            },
                            {
                                "target_lang_word": "танцовщик",
                                "target_lang_sense_clarification": "профессиональный артист балета или танцев"
                            },
                            {
                                "target_lang_word": "балерина",
                                "target_lang_sense_clarification": "артистка балета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0871ef45-3f5b-336c-b9b3-377b4ec83222",
                    "sense_definition": "A person who performs striptease; a stripper.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She works as an exotic <w>dancer</w> at a club downtown.",
                            "target_lang_translations": {
                                "ru": "Она работает экзотической <w>танцовщицей</w> в клубе в центре города."
                            }
                        },
                        {
                            "text": "The <w>dancer</w> entertained the guests late into the night.",
                            "target_lang_translations": {
                                "ru": "<w>Танцовщица</w> развлекала гостей до поздней ночи."
                            }
                        }
                    ],
                    "synonyms": [
                        "exotic dancer"
                    ],
                    "common_phrases": [
                        "exotic dancer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, исполняющий стриптиз; стриптизёр."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стриптизёрша",
                                "target_lang_sense_clarification": "женский род"
                            },
                            {
                                "target_lang_word": "экзотическая танцовщица",
                                "target_lang_sense_clarification": "эвфемизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8c86cc8-dae6-32f5-b0a6-db1f98e258a8",
                    "sense_definition": "A thief who uses rooftops to enter buildings through attic windows.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>dancer</w> slipped through the open garret window.",
                            "target_lang_translations": {
                                "ru": "<w>Вор-верхолаз</w> проскользнул в открытое чердачное окно."
                            }
                        },
                        {
                            "text": "In the past, a <w>dancer</w> was a thief who climbed roofs.",
                            "target_lang_translations": {
                                "ru": "В прошлом <w>вором-верхолазом</w> называли вора, который лазил по крышам."
                            }
                        }
                    ],
                    "synonyms": [
                        "garreter"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "thieves' cant"
                        },
                        {
                            "trait_type": "obsolete",
                            "comment": "historical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вор, проникающий в здания через крыши и чердачные окна."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вор-верхолаз",
                                "target_lang_sense_clarification": "описательный перевод"
                            },
                            {
                                "target_lang_word": "форточник",
                                "target_lang_sense_clarification": "вор, проникающий через окна (близкое по смыслу)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "0519d4f5-6c83-3e65-9fa2-100e885ac21f",
                    "sense_definition": "The name of the second reindeer of Santa Claus.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "mythological_or_astrological_entity",
                    "examples": [
                        {
                            "text": "Dasher and <w>Dancer</w> led the sleigh through the snow.",
                            "target_lang_translations": {
                                "ru": "Дэшер и <w>Танцор</w> вели сани сквозь снег."
                            }
                        },
                        {
                            "text": "Children sing about <w>Dancer</w> in Christmas songs.",
                            "target_lang_translations": {
                                "ru": "Дети поют о <w>Танцоре</w> в рождественских песнях."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Имя второго оленя в упряжке Санта-Клауса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Танцор",
                                "target_lang_sense_clarification": "имя собственное"
                            }
                        ]

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "524c0b4e-22bd-3d8a-b959-e6800c631871",
                    "sense_definition": "Opdienen of als eten serveren; voornamelijk gebruikt in vaste uitdrukkingen over de dagelijkse maaltijd.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "food_serving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben geen zin om te koken, dus we eten wat de pot <w>schaft</w>.",
                            "target_lang_translations": {
                                "en": "We don't feel like cooking, so we are eating whatever the pot <w>provides</w>."
                            }
                        },
                        {
                            "text": "Mijn vader bepaalt iedere avond wat de pot <w>schaft</w>.",
                            "target_lang_translations": {
                                "en": "Every evening my father decides what the pot <w>offers</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "versieren"
                    ],
                    "common_phrases": [
                        "wat de pot schaft"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "voornamelijk gebruikt in de uitdrukking 'wat de pot schaft'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To serve or provide food; primarily used in fixed expressions regarding the daily meal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "provide",
                                "target_lang_sense_clarification": "in the context of food or meals"
                            },
                            {
                                "target_lang_word": "offer",
                                "target_lang_sense_clarification": "regarding what is available to eat"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "282d8033-4bc3-35d1-9435-b6059a3335e3",
                    "sense_definition": "Ervoor zorgen dat iets er is; beschikbaar maken of verschaffen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "provision",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De grote eik <w>schafte</w> de wandelaars een aangename schaduw.",
                            "target_lang_translations": {
                                "en": "The large oak <w>afforded</w> the hikers a pleasant shade."
                            }
                        },
                        {
                            "text": "Zijn inspanningen <w>schaften</w> hem uiteindelijk weinig voordeel.",
                            "target_lang_translations": {
                                "en": "His efforts ultimately <w>yielded</w> him little benefit."
                            }
                        }
                    ],
                    "traits": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "72971d0f-1384-322b-af39-910b0331a057",
                    "sense_definition": "The flat surface on an electronic device, such as a television, computer, or smartphone, where images and text appear.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "display_surface",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She stared at the computer <w>screen</w> for hours while working on her project.",
                            "target_lang_translations": {
                                "ru": "Она часами смотрела на <w>экран</w> компьютера, работая над своим проектом."
                            }
                        },
                        {
                            "text": "I dropped my phone and the <w>screen</w> cracked.",
                            "target_lang_translations": {
                                "ru": "Я уронил телефон, и <w>экран</w> треснул."
                            }
                        }
                    ],
                    "synonyms": [
                        "pick"
                    ],
                    "common_phrases": [
                        "touch screen",
                        "flat screen",
                        "on screen",
                        "blank screen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоская поверхность электронного устройства, такого как телевизор, компьютер или смартфон, на которой появляются изображения и текст."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экра́н",
                                "target_lang_sense_clarification": "поверхность устройства"
                            },
                            {
                                "target_lang_word": "диспле́й",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1074c331-3dce-3279-82ad-6dd9bc1583c0",
                    "sense_definition": "The specific visual image or interface displayed on a computer or phone at a particular moment.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "display_content",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Enter your password on the login <w>screen</w> to access your account.",
                            "target_lang_translations": {
                                "ru": "Введите пароль на <w>экране</w> входа в систему, чтобы получить доступ к учетной записи."
                            }
                        },
                        {
                            "text": "The game has a loading <w>screen</w> that appears between levels.",
                            "target_lang_translations": {
                                "ru": "В игре есть <w>экран</w> загрузки, который появляется между уровнями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "login screen",
                        "home screen",
                        "full screen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретное визуальное изображение или интерфейс, отображаемый на компьютере или телефоне в определенный момент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экра́н",
                                "target_lang_sense_clarification": "изображение интерфейса"
                            },
                            {
                                "target_lang_word": "окно́",
                                "target_lang_sense_clarification": "часть интерфейса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c7a64c6-e2b2-3964-8905-a9fd44e334e1",
                    "sense_definition": "The large white surface in a cinema where movies are projected, or the movie industry in general.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "display_surface",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The action movie looks amazing on the big <w>screen</w>.",
                            "target_lang_translations": {
                                "ru": "Этот боевик потрясающе смотрится на большом <w>экране</w>."
                            }
                        },
                        {
                            "text": "Many stars of the silver <w>screen</w> attended the film festival.",
                            "target_lang_translations": {
                                "ru": "Многие звезды серебряного <w>экрана</w> посетили кинофестиваль."
                            }
                        }
                    ],
                    "common_phrases": [
                        "big screen",
                        "silver screen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая белая поверхность в кинотеатре, на которую проецируются фильмы, или киноиндустрия в целом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экра́н",
                                "target_lang_sense_clarification": "в кинотеатре"
                            },
                            {
                                "target_lang_word": "киноэкра́н",
                                "target_lang_sense_clarification": "полотно для проекции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa6d65c2-8b70-336d-8f1b-eaf941f2af92",
                    "sense_definition": "A movable upright partition used to divide a room, conceal an area, or provide protection.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_barrier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She changed her clothes behind a folding <w>screen</w> in the corner of the room.",
                            "target_lang_translations": {
                                "ru": "Она переоделась за складной <w>ширмой</w> в углу комнаты."
                            }
                        },
                        {
                            "text": "The nurses put a <w>screen</w> around the patient's bed for privacy.",
                            "target_lang_translations": {
                                "ru": "Медсестры поставили <w>ширму</w> вокруг кровати пациента для уединения."
                            }
                        }
                    ],
                    "synonyms": [
                        "pick"
                    ],
                    "common_phrases": [
                        "folding screen",
                        "privacy screen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передвижная вертикальная перегородка, используемая для разделения комнаты, скрытия какой-либо зоны или обеспечения защиты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ши́рма",
                                "target_lang_sense_clarification": "складная перегородка"
                            },
                            {
                                "target_lang_word": "перегоро́дка",
                                "target_lang_sense_clarification": "разделитель пространства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cbabb3a6-eaf5-3821-93a0-ab904095edc4",
                    "sense_definition": "A frame containing a mesh of wire or plastic, placed over a window or door to keep insects out while allowing air to pass.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_barrier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to repair the torn <w>screen</w> on the back door before summer starts.",
                            "target_lang_translations": {
                                "ru": "Нам нужно починить порванную <w>сетку</w> на задней двери до начала лета."
                            }
                        },
                        {
                            "text": "Flies came into the kitchen because there was no <w>screen</w> on the window.",
                            "target_lang_translations": {
                                "ru": "Мухи залетели на кухню, потому что на окне не было <w>сетки</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "window screen",
                        "fly screen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рама с сеткой из проволоки или пластика, устанавливаемая на окно или дверь для защиты от насекомых при проветривании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "се́тка",
                                "target_lang_sense_clarification": "на окне"
                            },
                            {
                                "target_lang_word": "моски́тная се́тка",
                                "target_lang_sense_clarification": "от насекомых"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62cbe47d-dd65-30cf-b0ff-fbf397634bf8",
                    "sense_definition": "A systematic process of testing or examining people or things to detect a specific condition or problem.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "examination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doctors recommend a regular health <w>screen</w> for people over the age of fifty.",
                            "target_lang_translations": {
                                "ru": "Врачи рекомендуют регулярное медицинское <w>обследование</w> людям старше пятидесяти лет."
                            }
                        },
                        {
                            "text": "The airport security <w>screen</w> took longer than we expected.",
                            "target_lang_translations": {
                                "ru": "<w>Досмотр</w> службы безопасности в аэропорту занял больше времени, чем мы ожидали."
                            }
                        }
                    ],
                    "common_phrases": [
                        "health screen",
                        "drug screen",
                        "security screen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Систематический процесс проверки или обследования людей или предметов для выявления определенного состояния или проблемы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прове́рка",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "досмо́тр",
                                "target_lang_sense_clarification": "в целях безопасности"
                            },
                            {
                                "target_lang_word": "скри́нинг",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7da91288-0a85-339c-9e53-55b4141c01ef",
                    "sense_definition": "An offensive tactic in sports where a player stands in the way of a defender to block them and help a teammate.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sports_tactic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The player set a perfect <w>screen</w> to free his teammate for a shot.",
                            "target_lang_translations": {
                                "ru": "Игрок поставил идеальный <w>заслон</w>, чтобы освободить товарища по команде для броска."
                            }
                        },
                        {
                            "text": "The coach designed a play that relies on a solid <w>screen</w> near the basket.",
                            "target_lang_translations": {
                                "ru": "Тренер разработал комбинацию, которая опирается на надежный <w>заслон</w> у корзины."
                            }
                        }
                    ],
                    "synonyms": [
                        "pick"
                    ],
                    "common_phrases": [
                        "set a screen"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "sports"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Атакующая тактика в спорте, когда игрок встает на пути защитника, чтобы заблокировать его и помочь товарищу по команде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "засло́н",
                                "target_lang_sense_clarification": "спортивный термин"
                            },
                            {
                                "target_lang_word": "бло́к",
                                "target_lang_sense_clarification": "блокировка игрока"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dbeae6be-ac50-3b3c-a637-22bb211076ba",
                    "sense_definition": "To show a movie, television program, or video to an audience.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "show_media",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new documentary will be <w>screened</w> at the film festival next week.",
                            "target_lang_translations": {
                                "ru": "Новый документальный фильм будет <w>показан</w> на кинофестивале на следующей неделе."
                            }
                        },
                        {
                            "text": "They <w>screened</w> the final episode of the series to a packed theater.",
                            "target_lang_translations": {
                                "ru": "Они <w>показали</w> финальную серию сериала в переполненном кинотеатре."
                            }
                        }
                    ],
                    "common_phrases": [
                        "screen a movie",
                        "screen a film"
                    ],
                    "target_lang_definitions": {
                        "ru": "Показывать фильм, телепрограмму или видео аудитории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пока́зывать",
                                "target_lang_sense_clarification": "демонстрировать фильм"
                            },
                            {
                                "target_lang_word": "демонстри́ровать",
                                "target_lang_sense_clarification": "официальный показ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0947ab4-d6c8-370e-9a19-cacdf91d2732",
                    "sense_definition": "To examine or test people, things, or information to decide if they are suitable or to find potential problems.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "examine_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "All luggage is <w>screened</w> by security before being loaded onto the plane.",
                            "target_lang_translations": {
                                "ru": "Весь багаж <w>досматривается</w> службой безопасности перед погрузкой в самолет."
                            }
                        },
                        {
                            "text": "The company <w>screens</w> all candidates for their language skills before the interview.",
                            "target_lang_translations": {
                                "ru": "Компания <w>проверяет</w> всех кандидатов на знание языка перед собеседованием."
                            }
                        }
                    ],
                    "synonyms": [
                        "pick"
                    ],
                    "common_phrases": [
                        "screen candidates",
                        "screen for drugs",
                        "screen luggage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проверять или тестировать людей, предметы или информацию для определения их пригодности или выявления потенциальных проблем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проверя́ть",
                                "target_lang_sense_clarification": "обследовать"
                            },
                            {
                                "target_lang_word": "досма́тривать",
                                "target_lang_sense_clarification": "проводить досмотр"
                            },
                            {
                                "target_lang_word": "отбира́ть",
                                "target_lang_sense_clarification": "кандидатов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5107cd0e-c58c-32ca-a195-b4dbd9beda2f",
                    "sense_definition": "To check the source of a telephone call before deciding whether to answer it.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "examine_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I usually <w>screen</w> my calls when I am busy working.",
                            "target_lang_translations": {
                                "ru": "Я обычно <w>фильтрую</w> звонки, когда занят работой."
                            }
                        },
                        {
                            "text": "The assistant <w>screens</w> all visitors and calls before they reach the director.",
                            "target_lang_translations": {
                                "ru": "Ассистент <w>отсеивает</w> всех посетителей и звонки, прежде чем они попадут к директору."
                            }
                        }
                    ],
                    "common_phrases": [
                        "screen calls"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проверять источник телефонного звонка перед тем, как решить, отвечать ли на него."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фильтрова́ть",
                                "target_lang_sense_clarification": "звонки"
                            },
                            {
                                "target_lang_word": "отсе́ивать",
                                "target_lang_sense_clarification": "нежелательные вызовы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27ebf185-0e2c-35ae-8560-7f925d2a716a",
                    "sense_definition": "To conceal, shelter, or protect something by placing a barrier in front of it.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "protect_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tall trees <w>screen</w> the house from the noise of the busy road.",
                            "target_lang_translations": {
                                "ru": "Высокие деревья <w>заслоняют</w> дом от шума оживленной дороги."
                            }
                        },
                        {
                            "text": "He raised his hand to <w>screen</w> his eyes from the bright sunlight.",
                            "target_lang_translations": {
                                "ru": "Он поднял руку, чтобы <w>заслонить</w> глаза от яркого солнечного света."
                            }
                        }
                    ],
                    "common_phrases": [
                        "screen from view"
                    ],
                    "target_lang_definitions": {
                        "ru": "Скрывать, укрывать или защищать что-либо, помещая перед ним преграду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заслоня́ть",
                                "target_lang_sense_clarification": "загораживать вид"
                            },
                            {
                                "target_lang_word": "скрыва́ть",
                                "target_lang_sense_clarification": "прятать за чем-то"
                            },
                            {
                                "target_lang_word": "экрани́ровать",
                                "target_lang_sense_clarification": "защищать от воздействия"
                            }
                        ]
                    }
                }
            ]
        },
        {

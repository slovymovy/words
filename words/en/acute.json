    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d71edf76-c3b6-34a7-b127-199590b08bf6",
                    "sense_definition": "Very serious, extreme, or severe, often describing a problem, pain, or shortage.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The country is suffering from an <w>acute</w> shortage of food and water.",
                            "target_lang_translations": {
                                "ru": "Страна страдает от <w>острой</w> нехватки еды и воды."
                            }
                        },
                        {
                            "text": "He felt an <w>acute</w> pain in his chest and called for a doctor immediately.",
                            "target_lang_translations": {
                                "ru": "Он почувствовал <w>острую</w> боль в груди и немедленно вызвал врача."
                            }
                        }
                    ],
                    "synonyms": [
                        "pressing",
                        "strong",
                        "powerful"
                    ],
                    "antonyms": [
                        "slow",
                        "leisurely"
                    ],
                    "common_phrases": [
                        "acute pain",
                        "acute shortage",
                        "acute crisis",
                        "acute embarrassment"
                    ],
                    "target_lang_definitions": {

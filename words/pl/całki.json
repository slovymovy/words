    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3bbe5fbb-dbd9-3727-8292-c7dd034e1fb7",
                    "sense_definition": "Forma gramatyczna (dopełniacz liczby pojedynczej oraz mianownik, biernik i wołacz liczby mnogiej) rzeczownika <w>całka</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "math_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obliczanie tej <w>całki</w> wymagało od studentów zaawansowanej wiedzy.",
                            "target_lang_translations": {
                                "ru": "Вычисление этого <w>интеграла</w> требовало от студентов глубоких знаний."
                            }
                        },
                        {
                            "text": "Na egzaminie pojawiły się skomplikowane <w>całki</w> do rozwiązania.",
                            "target_lang_translations": {
                                "ru": "На экзамене попались сложные <w>интегралы</w> для решения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "liczenie całki",
                        "całki oznaczone"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflection of całka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грамматическая форма существительного \"całka\" (интеграл)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "интеграл",
                                "target_lang_sense_clarification": "математическое понятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1886e504-9626-306a-a7fb-2221a51b9edd",
                    "sense_definition": "Regionalne określenie na warzywo korzeniowe, znane ogólnie jako rzepa.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "vegetable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodyni dodała do zupy pokrojone <w>całki</w> dla smaku.",
                            "target_lang_translations": {
                                "ru": "Хозяйка добавила в суп нарезанную <w>репу</w> для вкуса."
                            }
                        },
                        {
                            "text": "Na targu sprzedawano świeże <w>całki</w> prosto z pola.",
                            "target_lang_translations": {
                                "ru": "На рынке продавали свежую <w>репу</w> прямо с поля."
                            }
                        }
                    ],
                    "synonyms": [
                        "rzepa"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "dialectal usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Региональное название корнеплода, известного как репа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "репа",
                                "target_lang_sense_clarification": "овощ"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b5ba1c7e-18fa-3b0a-9671-621246c33ff0",
                    "sense_definition": "Gwarowy wariant przymiotnika oznaczający: kompletny, nienaruszony, wszystek.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "whole_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zjadł <w>całki</w> bochenek chleba i wciąż był głodny.",
                            "target_lang_translations": {
                                "ru": "Он съел <w>целый</w> батон хлеба и всё ещё был голоден."
                            }
                        },
                        {
                            "text": "Ten dzień był <w>całki</w> zmarnowany przez brzydką pogodę.",
                            "target_lang_translations": {
                                "ru": "Этот день был <w>весь</w> испорчен из-за плохой погоды."
                            }
                        }
                    ],
                    "synonyms": [

                        "verdelen",
                        "afzonderen"
                    ],
                    "antonyms": [
                        "verbinden",
                        "verenigen",
                        "samenvoegen"
                    ],
                    "common_phrases": [
                        "afval scheiden",
                        "de geesten scheiden"

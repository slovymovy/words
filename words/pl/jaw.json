                    "sense_id": "083d4d21-349f-3131-af16-85a6f05e9f45",
                    "sense_definition": "stan bycia jawnym, wiadomym; współcześnie słowo używane niemal wyłącznie w zwrocie 'na jaw'",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "noun_idiom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prawda o jego przeszłości w końcu wyszła na <w>jaw</w>.",
                            "target_lang_translations": {
                                "ru": "Правда о его прошлом в конце концов выплыла <w>наружу</w>."
                            }
                        },
                        {
                            "text": "Dziennikarze starali się wydobyć na <w>jaw</w> skrywane przez lata afery.",
                            "target_lang_translations": {
                                "ru": "Журналисты старались вытащить на <w>свет</w> скрываемые годами аферы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyjść na jaw",
                        "wydobyć na jaw"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "fixed phrase"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "состояние, когда что-то становится явным или известным; в настоящее время слово используется почти исключительно в обороте 'na jaw'"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наружу",
                                "target_lang_sense_clarification": "в контексте 'выйти на явь' — стать известным"
                            },
                            {
                                "target_lang_word": "свет",
                                "target_lang_sense_clarification": "в контексте 'вытащить на явь' — обнародовать"
                            }
                        ]
                    }
                },

                    ],
                    "antonyms": [
                        "późno"
                    ],
                    "common_phrases": [
                        "za wcześnie",
                        "zbyt wcześnie",
                        "dość wcześnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Раньше положенного времени, до срока или быстрее, чем обычно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рано",
                                "target_lang_sense_clarification": "до срока"
                            },
                            {
                                "target_lang_word": "преждевременно",
                                "target_lang_sense_clarification": "раньше времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ca2f33e-fefa-3c1f-92ac-faef449be2bf",
                    "sense_definition": "Podczas początkowego etapu trwania jakiegoś procesu, zjawiska lub życia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {

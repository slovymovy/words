    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5257b5ce-38ab-3d43-8e7e-edd33d8c3019",
                    "sense_definition": "De stad of het dorp waar iemand woont en officieel ingeschreven staat.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "residence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vul op dit formulier uw naam en <w>woonplaats</w> in.",
                            "target_lang_translations": {
                                "ru": "Впишите в эту форму ваше имя и <w>место жительства</w>."
                            }
                        },
                        {
                            "text": "Zij is verhuisd en haar nieuwe <w>woonplaats</w> is Utrecht.",
                            "target_lang_translations": {
                                "ru": "Она переехала, и её новое <w>место жительства</w> — Утрехт."
                            }
                        }
                    ],
                    "synonyms": [
                        "verblijfplaats",
                        "domicilie"
                    ],
                    "common_phrases": [
                        "vaste woonplaats",
                        "naam en woonplaats"
                    ],
                    "target_lang_definitions": {
                        "ru": "Город или деревня, где человек живет и официально зарегистрирован."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "место жительства",
                                "target_lang_sense_clarification": "официальный адрес проживания"
                            },
                            {
                                "target_lang_word": "местожительство",
                                "target_lang_sense_clarification": "место постоянного проживания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4dc70f9e-fcb2-3640-8dc7-688e70c4412d",
                    "sense_definition": "Een bewoonde plek met een eigen naam, zoals een stad, dorp of gehucht.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "settlement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In deze regio is elke kleine <w>woonplaats</w> met de bus bereikbaar.",
                            "target_lang_translations": {
                                "ru": "В этом регионе до каждого малого <w>населённого пункта</w> можно добраться на автобусе."
                            }
                        },
                        {
                            "text": "Het is een rustige <w>woonplaats</w> met veel groen en weinig verkeer.",
                            "target_lang_translations": {
                                "ru": "Это тихий <w>населённый пункт</w> с обилием зелени и спокойным движением."
                            }
                        }
                    ],
                    "synonyms": [
                        "plaats",
                        "nederzetting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Населенная местность с собственным названием, например город, деревня или поселок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "населённый пункт",
                                "target_lang_sense_clarification": "географический объект (город, село и т.д.)"
                            },
                            {
                                "target_lang_word": "поселение",
                                "target_lang_sense_clarification": "место, где живут люди"
                            }
                        ]
                    }
                }
            ]
        }
    ]

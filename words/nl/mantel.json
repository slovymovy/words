                {
                    "sense_id": "bc347998-6f2b-3685-9374-ed5f7fd8cff1",
                    "sense_definition": "Een lang, loshangend kledingstuk dat men buiten over de andere kleding draagt, vaak ter bescherming tegen kou.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij sloeg een warme <w>mantel</w> om haar schouders toen ze naar buiten ging.",
                            "target_lang_translations": {
                                "en": "She wrapped a warm <w>cloak</w> around her shoulders when she went outside."
                            }
                        },
                        {
                            "text": "In de winter draagt mijn grootvader altijd een lange grijze <w>mantel</w>.",
                            "target_lang_translations": {
                                "en": "In winter, my grandfather always wears a long grey <w>coat</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "regenmantel",
                        "regenjas"
                    ],
                    "common_phrases": [
                        "een wollen mantel",
                        "de rode mantel"
                    ],
                    "target_lang_definitions": {
                        "en": "A long, loose outer garment worn over other clothes, often to protect against the cold."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "coat",
                                "target_lang_sense_clarification": "modern long outer garment"
                            },
                            {
                                "target_lang_word": "cloak",
                                "target_lang_sense_clarification": "loose sleeveless outer garment"
                            },
                            {
                                "target_lang_word": "mantle",
                                "target_lang_sense_clarification": "literary or historical term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25047166-6550-370d-ae56-77a2d678f403",
                    "sense_definition": "Een beschermende buitenlaag of omhulsel rondom een voorwerp, kabel of machine.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "technical_casing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rubberen <w>mantel</w> van de kabel is beschadigd en moet worden vervangen.",
                            "target_lang_translations": {
                                "en": "The rubber <w>sheath</w> of the cable is damaged and needs to be replaced."
                            }
                        },
                        {
                            "text": "Deze machine heeft een stalen <w>mantel</w> die het binnenwerk beschermt tegen stof.",
                            "target_lang_translations": {

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0d7b7034-a6af-3690-9d3f-e0a7d34991a3",
                    "sense_definition": "Happening or coming at the end of a series, process, or event.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This is the <w>final</w> call for passengers boarding the flight to London.",
                            "target_lang_translations": {
                                "ru": "Это <w>после́днее</w> объявление о посадке на рейс в Лондон."
                            }
                        },
                        {
                            "text": "The <w>final</w> chapter of the book was the most exciting one.",
                            "target_lang_translations": {
                                "ru": "<w>Заключи́тельная</w> глава книги была самой захватывающей."
                            }
                        }
                    ],
                    "synonyms": [
                        "last",
                        "ultimate"
                    ],
                    "antonyms": [
                        "first",
                        "initial"
                    ],
                    "common_phrases": [
                        "final stage",
                        "final destination",
                        "final warning"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий или наступающий в конце серии, процесса или события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "после́дний",
                                "target_lang_sense_clarification": "самый общий перевод; находящийся в конце"
                            },
                            {
                                "target_lang_word": "заключи́тельный",
                                "target_lang_sense_clarification": "завершающий собой что-либо (речь, концерт, главу)"
                            },
                            {
                                "target_lang_word": "фина́льный",
                                "target_lang_sense_clarification": "относящийся к финалу, завершающий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b70e19d-a25c-3f2f-ab82-8b3203cc20f8",
                    "sense_definition": "Impossible to change or argue with; allowing no further doubt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The judge's decision is <w>final</w> and cannot be appealed.",
                            "target_lang_translations": {
                                "ru": "Решение судьи <w>оконча́тельное</w> и не подлежит обжалованию."
                            }
                        },
                        {
                            "text": "We need a <w>final</w> answer from you by tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "Нам нужен ваш <w>оконча́тельный</w> ответ к завтрашнему утру."
                            }
                        }
                    ],
                    "synonyms": [
                        "conclusive",
                        "decisive",
                        "definitive"
                    ],
                    "common_phrases": [
                        "final decision",
                        "final answer",
                        "final say"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не подлежащий изменению или обсуждению; не допускающий сомнений."

                            {
                                "target_lang_word": "неподкупность",
                                "target_lang_sense_clarification": "способность не поддаваться коррупции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72df924d-7186-32ab-bdc4-516cf44b87e3",
                    "sense_definition": "De toestand waarin iets geheel, volledig en onbeschadigd is.",

                {
                    "sense_id": "adff4ccb-eee6-3c82-901e-b2ea05151c39",
                    "sense_definition": "drapieżny ptak nocny o dużej głowie i oczach umieszczonych z przodu, symbol mądrości",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starym lesie pohukiwała głośno <w>sowa</w>.",
                            "target_lang_translations": {
                                "ru": "В старом лесу громко ухала <w>сова</w>."
                            }
                        },
                        {
                            "text": "Każda <w>sowa</w> doskonale widzi w ciemnościach.",
                            "target_lang_translations": {
                                "ru": "Каждая <w>сова</w> прекрасно видит в темноте."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mądry jak sowa",
                        "oczy jak u sowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "хищная ночная птица с большой головой и глазами, расположенными спереди, символ мудрости"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сова",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26e044c4-2819-3cb9-94cc-3a51c2c5aeb6",
                    "sense_definition": "osoba, która jest aktywna w nocy i chodzi spać bardzo późno",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat to typowa <w>sowa</w>, najlepiej pracuje mu się w nocy.",
                            "target_lang_translations": {
                                "ru": "Мой брат — типичная <w>сова</w>, ему лучше всего работается ночью."
                            }
                        },
                        {
                            "text": "Jako <w>sowa</w> mam rano duże problemy ze wstawaniem.",
                            "target_lang_translations": {
                                "ru": "Как <w>сова</w>, я испытываю большие проблемы с подъёмом по утрам."
                            }
                        }
                    ],
                    "synonyms": [
                        "nocny marek"
                    ],
                    "antonyms": [
                        "ranny ptaszek",
                        "skowronek"
                    ],
                    "common_phrases": [
                        "być sową"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "человек, который активен ночью и ложится спать очень поздно"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сова",
                                "target_lang_sense_clarification": "о человеке"
                            }
                        ]

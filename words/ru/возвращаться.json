    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1682f296-c39c-3727-997e-ed22b36a75be",
                    "sense_definition": "Приходить или приезжать обратно в то место, откуда ушёл или уехал.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый вечер папа <возвращается> с работы домой в шесть часов.",
                            "target_lang_translations": {
                                "en": "Every evening dad <returns> home from work at six o'clock."
                            }
                        },
                        {
                            "text": "Мы планируем <возвращаться> из отпуска в следующее воскресенье.",
                            "target_lang_translations": {
                                "en": "We plan to <return> from vacation next Sunday."
                            }
                        }
                    ],
                    "synonyms": [
                        "приходить назад",
                        "приезжать назад"
                    ],
                    "antonyms": [
                        "уходить",
                        "уезжать",
                        "покидать"
                    ],
                    "common_phrases": [
                        "возвращаться домой",
                        "возвращаться назад",
                        "возвращаться поздно",
                        "возвращаться с работы"
                    ],
                    "target_lang_definitions": {
                        "en": "To come or go back to a place where one was before."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "return",
                                "target_lang_sense_clarification": "general term for going back"
                            },
                            {
                                "target_lang_word": "come back",
                                "target_lang_sense_clarification": "to return towards the speaker"
                            },
                            {
                                "target_lang_word": "go back",
                                "target_lang_sense_clarification": "to return away from the speaker"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09bc211e-689c-382a-94dd-a1e35ca15d8a",
                    "sense_definition": "Снова начинать обсуждать, обдумывать или рассматривать какую-либо тему или вопрос.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_return",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Давайте не будем <возвращаться> к этому неприятному разговору.",
                            "target_lang_translations": {
                                "en": "Let's not <return> to this unpleasant conversation."
                            }
                        },
                        {
                            "text": "Учитель постоянно <возвращается> к правилам грамматики, чтобы мы их запомнили.",
                            "target_lang_translations": {
                                "en": "The teacher constantly <returns> to grammar rules so that we remember them."
                            }
                        }
                    ],
                    "synonyms": [
                        "возобновлять"
                    ],
                    "common_phrases": [
                        "возвращаться к теме",
                        "возвращаться к вопросу",
                        "возвращаться к разговору"
                    ],
                    "target_lang_definitions": {
                        "en": "To begin discussing, thinking about, or considering a subject again."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "return",
                                "target_lang_sense_clarification": "to a topic or conversation"
                            },
                            {
                                "target_lang_word": "go back",
                                "target_lang_sense_clarification": "to a previous point in discussion"
                            },
                            {
                                "target_lang_word": "revisit",
                                "target_lang_sense_clarification": "to consider a subject again"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63c85168-29a6-376c-8e56-9152064cc6ca",
                    "sense_definition": "Снова оказываться в прежнем состоянии, ситуации или приступать к прежним занятиям.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_return",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После долгих праздников всегда трудно <возвращаться> к работе.",
                            "target_lang_translations": {
                                "en": "It is always hard to <return> to work after long holidays."
                            }
                        },
                        {
                            "text": "Город постепенно <возвращается> к нормальной жизни после сильного урагана.",
                            "target_lang_translations": {
                                "en": "The city is gradually <returning> to normal life after the strong hurricane."
                            }
                        }
                    ],
                    "common_phrases": [
                        "возвращаться к жизни",
                        "возвращаться к работе",
                        "возвращаться в строй",
                        "возвращаться к реальности"
                    ],
                    "target_lang_definitions": {
                        "en": "To be in a previous state or situation again, or to resume an activity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "return",
                                "target_lang_sense_clarification": "to a state, habit, or job"
                            },
                            {
                                "target_lang_word": "go back",
                                "target_lang_sense_clarification": "to a former condition"
                            },
                            {
                                "target_lang_word": "revert",
                                "target_lang_sense_clarification": "to a former state or type"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "297b002e-ff89-36e4-be26-be3d2ec1461e",
                    "sense_definition": "Передаваться обратно прежнему владельцу или отправителю (о вещах, письмах, деньгах).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если адрес указан неверно, заказное письмо <возвращается> отправителю.",
                            "target_lang_translations": {
                                "en": "If the address is incorrect, the registered letter <returns> to the sender."
                            }
                        },
                        {
                            "text": "Потерянные в аэропорту вещи редко <возвращаются> к своим хозяевам.",
                            "target_lang_translations": {
                                "en": "Things lost at the airport rarely <return> to their owners."
                            }
                        }
                    ],
                    "common_phrases": [
                        "письмо возвращается",
                        "деньги возвращаются",
                        "всё возвращается"
                    ],
                    "target_lang_definitions": {
                        "en": "To go back to the previous owner or sender (of things, letters, or money)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "return",
                                "target_lang_sense_clarification": "to the owner or sender"
                            },
                            {
                                "target_lang_word": "come back",
                                "target_lang_sense_clarification": "to the owner"
                            },
                            {
                                "target_lang_word": "find one's way back",
                                "target_lang_sense_clarification": "idiomatic, for lost items"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
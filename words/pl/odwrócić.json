    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "028b7c9b-5900-3a39-ac4f-941e60b35886",
                    "sense_definition": "Zmienić pozycję swojego ciała, aby skierować się w inną stronę, zazwyczaj do tyłu (używane z zaimkiem „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Usłyszał swoje imię i natychmiast <w>odwrócił</w> się w stronę kolegi.",
                            "target_lang_translations": {
                                "ru": "Он услышал свое имя и тут же <w>обернулся</w> к коллеге."
                            }
                        },
                        {
                            "text": "Obrażona dziewczyna postanowiła <w>odwrócić</w> się na pięcie i odejść bez słowa.",
                            "target_lang_translations": {
                                "ru": "Обиженная девушка решила <w>развернуться</w> на каблуках и уйти без слов."
                            }
                        }
                    ],
                    "synonyms": [
                        "obrócić się",
                        "obejrzeć się"
                    ],
                    "common_phrases": [
                        "odwrócić się na pięcie",
                        "odwrócić się plecami",
                        "odwrócić się za siebie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменить положение своего тела, чтобы повернуться в другую сторону, обычно назад (используется с местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обернуться",
                                "target_lang_sense_clarification": "посмотреть назад"
                            },
                            {
                                "target_lang_word": "повернуться",
                                "target_lang_sense_clarification": "изменить положение тела"
                            },
                            {
                                "target_lang_word": "отвернуться",
                                "target_lang_sense_clarification": "перестать смотреть на кого-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a32066aa-1277-313d-ad4d-6c4747e50ca2",
                    "sense_definition": "Zmienić położenie przedmiotu na drugą stronę lub skierować go w przeciwnym kierunku.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę <w>odwrócić</w> kartkę na drugą stronę i zacząć pisać test.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>переверните</w> листок на другую сторону и начинайте писать тест."
                            }
                        },
                        {
                            "text": "Musisz ostrożnie <w>odwrócić</w> mięso na patelni, żeby się nie przypaliło.",
                            "target_lang_translations": {
                                "ru": "Ты должен осторожно <w>перевернуть</w> мясо на сковороде, чтобы оно не пригорело."
                            }
                        }
                    ],
                    "synonyms": [
                        "przekręcić",
                        "obrócić"
                    ],
                    "common_phrases": [
                        "odwrócić kartkę",
                        "odwrócić na drugą stronę",
                        "odwrócić kota ogonem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменить положение предмета на другую сторону или направить его в противоположном направлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перевернуть",
                                "target_lang_sense_clarification": "положить другой стороной"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c3d4a57-10ab-3257-abe2-e3f688d1d29e",
                    "sense_definition": "Skierować coś w inną stronę (np. wzrok, uwagę) lub zapobiec czemuś niepożądanemu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Głośny hałas na ulicy pomógł złodziejowi <w>odwrócić</w> uwagę przechodniów.",
                            "target_lang_translations": {
                                "ru": "Громкий шум на улице помог вору <w>отвлечь</w> внимание прохожих."
                            }
                        },
                        {
                            "text": "Lekarze robili wszystko, co w ich mocy, aby <w>odwrócić</w> zagrożenie życia pacjenta.",
                            "target_lang_translations": {
                                "ru": "Врачи делали все возможное, чтобы <w>отвести</w> угрозу жизни пациента."
                            }
                        }
                    ],
                    "synonyms": [
                        "zapobiec",
                        "oddalić"
                    ],
                    "antonyms": [
                        "przyciągnąć",
                        "skupić"
                    ],
                    "common_phrases": [
                        "odwrócić uwagę",
                        "odwrócić wzrok",
                        "odwrócić los",
                        "odwrócić bieg zdarzeń"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направить что-то в другую сторону (например, взгляд, внимание) или предотвратить что-то нежелательное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отвлечь",
                                "target_lang_sense_clarification": "внимание"
                            },
                            {
                                "target_lang_word": "отвести",
                                "target_lang_sense_clarification": "взгляд или угрозу"
                            },
                            {
                                "target_lang_word": "предотвратить",
                                "target_lang_sense_clarification": "не допустить чего-то"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
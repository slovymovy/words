{
    "word_family": [
        "prądnica",
        "prądowy",
        "bezprądowy",
        "prądnicowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e9aa9a04-6acb-3b8f-b55a-485761394986",
                    "sense_definition": "Energia elektryczna zasilająca urządzenia, budynki i oświetlenie lub fizyczny przepływ ładunków elektrycznych.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "electricity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj w nocy podczas burzy nagle zabrakło nam w domu <w>prądu</w>.",
                            "target_lang_translations": {
                                "ru": "Вчера ночью во время грозы у нас дома внезапно пропало <w>электричество</w>."
                            }
                        },
                        {
                            "text": "Pamiętaj, aby wyłączyć <w>prąd</w> przed naprawą gniazdka elektrycznego.",
                            "target_lang_translations": {
                                "ru": "Не забудь отключить <w>электричество</w> перед ремонтом розетки."
                            }
                        }
                    ],
                    "synonyms": [
                        "elektryczność",
                        "energia",
                        "światło",
                        "napięcie"
                    ],
                    "common_phrases": [
                        "brak prądu",
                        "wyłączyć prąd",
                        "rachunek za prąd",
                        "porazić prądem",
                        "podłączyć do prądu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Электрическая энергия, питающая приборы, здания и освещение, или физический поток электрических зарядов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "электричество",
                                "target_lang_sense_clarification": "энергия для бытовых нужд"
                            },
                            {
                                "target_lang_word": "ток",
                                "target_lang_sense_clarification": "физическое явление"
                            },
                            {
                                "target_lang_word": "электроэнергия",
                                "target_lang_sense_clarification": "ресурс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b230cbbb-85cd-3123-b200-d597f4be5dcf",
                    "sense_definition": "Ukierunkowany, ciągły ruch wody w rzece lub morzu, a także ruch powietrza.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "flow_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Silny <w>prąd</w> rzeki zniósł naszą łódkę daleko od brzegu.",
                            "target_lang_translations": {
                                "ru": "Сильное <w>течение</w> реки унесло нашу лодку далеко от берега."
                            }
                        },
                        {
                            "text": "Pływanie w tym miejscu jest niebezpieczne ze względu na wsteczne <w>prądy</w> morskie.",
                            "target_lang_translations": {
                                "ru": "Купаться в этом месте опасно из-за обратных морских <w>течений</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "nurt"
                    ],
                    "common_phrases": [
                        "prąd morski",
                        "płynąć z prądem",
                        "płynąć pod prąd",
                        "silny prąd",
                        "prąd rzeki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направленное, непрерывное движение воды в реке или море, а также движение воздуха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "течение",
                                "target_lang_sense_clarification": "движение воды или воздуха"
                            },
                            {
                                "target_lang_word": "поток",
                                "target_lang_sense_clarification": "стремительное движение массы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14a4418e-609e-35d0-99aa-61a2a293e40d",
                    "sense_definition": "Wyraźny kierunek zmian, styl lub tendencja w sztuce, literaturze, polityce lub myśleniu społecznym.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "flow_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Romantyzm to niezwykle ważny <w>prąd</w> w historii literatury europejskiej.",
                            "target_lang_translations": {
                                "ru": "Романтизм — это чрезвычайно важное <w>течение</w> в истории европейской литературы."
                            }
                        },
                        {
                            "text": "Nowy <w>prąd</w> myślowy szybko zyskał popularność wśród młodych studentów.",
                            "target_lang_translations": {
                                "ru": "Новое мыслительное <w>течение</w> быстро завоевало популярность среди молодых студентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "kierunek",
                        "tendencja",
                        "trend",
                        "nurt"
                    ],
                    "common_phrases": [
                        "prąd literacki",
                        "prąd umysłowy",
                        "nowy prąd",
                        "prąd polityczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Четкое направление изменений, стиль или тенденция в искусстве, литературе, политике или общественной мысли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "течение",
                                "target_lang_sense_clarification": "направление в искусстве или мысли"
                            },
                            {
                                "target_lang_word": "направление",
                                "target_lang_sense_clarification": "вектор развития"
                            },
                            {
                                "target_lang_word": "тенденция",
                                "target_lang_sense_clarification": "склонность или стремление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16c5c18b-d1da-356b-958c-33ab670421a1",
                    "sense_definition": "Potoczne określenie na napój alkoholowy, zazwyczaj wódkę.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_alcohol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na imprezie szybko skończył się <w>prąd</w> i musieliśmy iść do sklepu.",
                            "target_lang_translations": {
                                "ru": "На вечеринке быстро закончилось <w>горючее</w>, и нам пришлось идти в магазин."
                            }
                        },
                        {
                            "text": "Czy masz może jakiś <w>prąd</w>, żebyśmy mogli się napić za twoje zdrowie?",
                            "target_lang_translations": {
                                "ru": "У тебя есть какая-нибудь <w>выпивка</w>, чтобы мы могли выпить за твое здоровье?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название алкогольных напитков, обычно водки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горючее",
                                "target_lang_sense_clarification": "сленг, алкоголь"
                            },
                            {
                                "target_lang_word": "выпивка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f2d5e9a-d05d-3056-97ba-00104c1b7d84",
                    "sense_definition": "Drzewo, korzeń lub inna przeszkoda tkwiąca w dnie rzeki, niebezpieczna dla łodzi i tratw.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "river_obstacle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony flisak potrafił z daleka dostrzec niebezpieczny <w>prąd</w> ukryty pod wodą.",
                            "target_lang_translations": {
                                "ru": "Опытный плотогон мог издалека заметить опасную <w>корягу</w>, скрытую под водой."
                            }
                        },
                        {
                            "text": "Nasza łódź uderzyła w ukryty <w>prąd</w> i prawie zatonęła na środku rzeki.",
                            "target_lang_translations": {
                                "ru": "Наша лодка ударилась о скрытый <w>топляк</w> и почти затонула посреди реки."
                            }
                        }
                    ],
                    "synonyms": [
                        "wilk"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "terminologia flisacka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дерево, корень или другое препятствие на дне реки, опасное для лодок и плотов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коряга",
                                "target_lang_sense_clarification": "затонувшее дерево"
                            },
                            {
                                "target_lang_word": "топляк",
                                "target_lang_sense_clarification": "бревно под водой"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
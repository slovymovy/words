    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "64b3a554-a083-3ccd-8a00-dcfa16e8ac5d",
                    "sense_definition": "Cykliczne zawody sportowe organizowane dla uczczenia pamięci wybitnej osoby, zazwyczaj zasłużonego sportowca lub trenera.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W przyszłym tygodniu odbędzie się coroczny <w>memoriał</w> poświęcony pamięci naszego mistrza olimpijskiego.",
                            "target_lang_translations": {
                                "ru": "На следующей неделе состоится ежегодный <w>мемориал</w>, посвященный памяти нашего олимпийского чемпиона."
                            }
                        },
                        {
                            "text": "Nasi siatkarze wygrali prestiżowy <w>memoriał</w> i zdobyli główną nagrodę turnieju.",
                            "target_lang_translations": {
                                "ru": "Наши волейболисты выиграли престижный <w>мемориал</w> и завоевали главный приз турнира."
                            }
                        }
                    ],
                    "synonyms": [
                        "turniej",
                        "zawody"
                    ],
                    "common_phrases": [
                        "coroczny memoriał",
                        "wygrać memoriał",
                        "wziąć udział w memoriale"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спортивные соревнования, проводимые в память о выдающемся человеке, обычно спортсмене или тренере."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мемориал",
                                "target_lang_sense_clarification": "спортивный турнир"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "236294a2-4096-36e1-a89e-bd42ea9cbd4f",
                    "sense_definition": "Oficjalne pismo skierowane do władz, zawierające prośby, postulaty lub uwagi dotyczące spraw ważnych dla ogółu lub państwa.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Grupa naukowców podpisała <w>memoriał</w> wzywający rząd do ochrony puszczy.",
                            "target_lang_translations": {
                                "ru": "Группа ученых подписала <w>петицию</w>, призывающую правительство защитить пущу."
                            }
                        },
                        {
                            "text": "Ambasador przekazał królowi tajny <w>memoriał</w> dotyczący sytuacji politycznej w regionie.",
                            "target_lang_translations": {
                                "ru": "Посол передал королю тайный <w>меморандум</w> касательно политической ситуации в регионе."
                            }
                        }
                    ],
                    "synonyms": [
                        "petycja",
                        "nota",
                        "pismo"
                    ],
                    "common_phrases": [
                        "złożyć memoriał",
                        "opracować memoriał",
                        "wystosować memoriał"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное письмо или обращение к властям, содержащее просьбы или предложения по важным вопросам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "меморандум",
                                "target_lang_sense_clarification": "дипломатический документ"
                            },
                            {
                                "target_lang_word": "петиция",
                                "target_lang_sense_clarification": "коллективное прошение"
                            },
                            {
                                "target_lang_word": "докладная записка",
                                "target_lang_sense_clarification": "служебный документ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e35c66c3-9c02-3aba-a1e3-8408454e9602",
                    "sense_definition": "Księga handlowa przeznaczona do ewidencjonowania operacji bezgotówkowych lub przeksięgowań.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Księgowy starannie wprowadził wszystkie zaległe operacje do firmowego <w>memoriału</w>.",
                            "target_lang_translations": {
                                "ru": "Бухгалтер тщательно внес все просроченные операции в <w>мемориальный ордер</w> фирмы."
                            }
                        },
                        {
                            "text": "Błędy w <w>memoriale</w> mogą utrudnić zamknięcie roku obrachunkowego.",
                            "target_lang_translations": {
                                "ru": "Ошибки в <w>мемориальном ордере</w> могут затруднить закрытие отчетного года."
                            }
                        }
                    ],
                    "synonyms": [
                        "dziennik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бухгалтерская книга или документ для регистрации безналичных операций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мемориальный ордер",
                                "target_lang_sense_clarification": "бухгалтерский документ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c81e6ca3-9827-3f01-939c-120d900d2743",
                    "sense_definition": "Pamięć o kimś lub o czymś; wspomnienie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego bohaterskie czyny na zawsze pozostaną w naszym <w>memoriale</w>.",
                            "target_lang_translations": {
                                "ru": "Его героические поступки навсегда останутся в нашей <w>памяти</w>."
                            }
                        },
                        {
                            "text": "Ten stary dokument przywołuje <w>memoriał</w> dawnych, lepszych czasów.",
                            "target_lang_translations": {
                                "ru": "Этот старый документ пробуждает <w>воспоминание</w> о давних, лучших временах."
                            }
                        }
                    ],
                    "synonyms": [
                        "pamięć",
                        "wspomnienie"
                    ],
                    "antonyms": [
                        "zapomnienie"
                    ],

                    "target_lang_definitions": {
                        "ru": "Выдержать что-то трудное, болезненное или неприятное; справиться с психологической или физической нагрузкой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вынести",
                                "target_lang_sense_clarification": "выдержать испытание"
                            },
                            {
                                "target_lang_word": "перенести",
                                "target_lang_sense_clarification": "пережить трудность"
                            },
                            {
                                "target_lang_word": "вытерпеть",
                                "target_lang_sense_clarification": "сдержаться, не сдаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a7d6c26-fcc1-3ba7-a55b-401a198b0458",
                    "sense_definition": "Sprawić, że prawo, przepis, zakaz lub instytucja przestaje obowiązywać lub istnieć.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "law_and_rules",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rząd postanowił <znieść> podatek od najniższych dochodów.",
                            "target_lang_translations": {
                                "ru": "Правительство решило <отменить> налог на самые низкие доходы."
                            }
                        },
                        {
                            "text": "Władze miasta chcą <znieść> zakaz wjazdu do centrum w weekendy.",
                            "target_lang_translations": {
                                "ru": "Городские власти хотят <снять> запрет на въезд в центр по выходным."
                            }
                        }
                    ],
                    "synonyms": [
                        "unieważnić",
                        "uchylić",
                        "zlikwidować",
                        "odwołać"
                    ],
                    "antonyms": [
                        "ustanowić",
                        "wprowadzić"
                    ],
                    "common_phrases": [
                        "znieść zakaz",
                        "znieść sankcje",
                        "znieść karę",
                        "znieść prawo"
                    ],
                    "target_lang_definitions": {

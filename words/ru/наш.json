    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "729dfed6-45c9-3c58-8c2e-b587a8de5de9",
                    "sense_definition": "Принадлежащий нам или имеющий отношение к нам.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это <w>наш</w> новый дом.",
                            "target_lang_translations": {
                                "en": "This is <w>our</w> new house."
                            }
                        },
                        {
                            "text": "<w>Наши</w> дети ходят в одну школу.",
                            "target_lang_translations": {
                                "en": "<w>Our</w> children go to the same school."
                            }
                        },
                        {
                            "text": "Мы защищаем <w>нашу</w> страну.",
                            "target_lang_translations": {
                                "en": "We are defending <w>our</w> country."
                            }
                        }
                    ],
                    "antonyms": [
                        "ваш",
                        "их",
                        "чужой"
                    ],
                    "common_phrases": [
                        "наш дом",
                        "наша страна",
                        "наше время",
                        "наш город"
                    ],
                    "target_lang_definitions": {
                        "en": "Belonging to us or relating to us."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "our",
                                "target_lang_sense_clarification": "determiner indicating possession"
                            },
                            {
                                "target_lang_word": "ours",
                                "target_lang_sense_clarification": "pronoun indicating possession"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf399b98-dfba-3aa0-9645-c75980f85846",
                    "sense_definition": "Тот, о котором идёт речь; упоминаемый или подразумеваемый в данном контексте.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "contextual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "А вот и <w>наш</w> герой вернулся.",
                            "target_lang_translations": {
                                "en": "And here <w>our</w> hero has returned."
                            }
                        },
                        {
                            "text": "<w>Наш</w> пациент чувствует себя намного лучше.",
                            "target_lang_translations": {
                                "en": "<w>Our</w> patient is feeling much better."
                            }
                        }
                    ],
                    "common_phrases": [
                        "наш герой",
                        "наш случай",
                        "наш клиент"
                    ],
                    "target_lang_definitions": {
                        "en": "The one being discussed; mentioned or implied in the given context."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "our",
                                "target_lang_sense_clarification": "referring to a specific person mentioned or implied"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9dbca003-ad08-3473-b09d-127fe1d5a917",
                    "sense_definition": "Близкие нам люди: родственники, друзья, соотечественники или солдаты своей армии.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "people_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вчера <w>наши</w> выиграли важный матч.",
                            "target_lang_translations": {
                                "en": "Yesterday <w>our team</w> won an important match."
                            }
                        },
                        {
                            "text": "Скоро <w>наши</w> вернутся с дачи.",
                            "target_lang_translations": {
                                "en": "Soon <w>our folks</w> will return from the dacha."
                            }
                        },
                        {
                            "text": "Он сразу стал для нас <w>нашим</w>.",
                            "target_lang_translations": {
                                "en": "He immediately became one of <w>us</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "чужой",
                        "враг"
                    ],
                    "common_phrases": [
                        "наши победили",
                        "и вашим и нашим"
                    ],
                    "target_lang_definitions": {
                        "en": "People close to us: relatives, friends, compatriots, or soldiers of one's own army."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ours",
                                "target_lang_sense_clarification": "referring to one's own group, team, or troops"
                            },
                            {
                                "target_lang_word": "our people",
                                "target_lang_sense_clarification": "relatives, friends, or compatriots"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4659190a-7c0d-37f0-85ac-75a5bb871701",
                    "sense_definition": "То, что принадлежит нам, свойственно нам или является нашей долей.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "substantivized_property",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы <w>нашего</w> никому не отдадим.",
                            "target_lang_translations": {
                                "en": "We won't give <w>ours</w> to anyone."
                            }
                        },
                        {
                            "text": "Поживи с <w>наше</w>, тогда и будешь судить.",
                            "target_lang_translations": {
                                "en": "Live as long as <w>us</w>, then you can judge."
                            }
                        }
                    ],
                    "antonyms": [
                        "чужой"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "That which belongs to us, is characteristic of us, or is our share."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ours",
                                "target_lang_sense_clarification": "that which belongs to us"
                            },
                            {
                                "target_lang_word": "what is ours",
                                "target_lang_sense_clarification": "our property or share"
                            }
                        ]
                    }
                }
            ]
        },

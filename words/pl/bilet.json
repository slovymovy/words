{
    "word_family": [
        "bilecik",
        "biletowy",
        "biletomat",
        "bileter",
        "bileterka"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "feb94656-a8a5-3eb1-8088-2f7d538cfdff",
                    "sense_definition": "Dokument potwierdzający opłatę za przejazd środkami transportu lub uprawniający do wstępu do kina, teatru, muzeum itp.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "ticket_document",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę kupić <w>bilet</w> miesięczny na autobus, aby codziennie dojeżdżać do pracy.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить месячный проездной <w>билет</w> на автобус, чтобы каждый день ездить на работу."
                            }
                        },
                        {
                            "text": "Czy masz już <w>bilet</w> na dzisiejszy wieczorny seans w kinie?",
                            "target_lang_translations": {
                                "ru": "У тебя уже есть <w>билет</w> на сегодняшний вечерний сеанс в кино?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "bilet ulgowy",
                        "bilet normalny",
                        "bilet miesięczny",
                        "kupić bilet",
                        "bilet lotniczy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Документ, подтверждающий оплату проезда в транспорте или право на посещение кино, театра, музея и т.п."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "билет",
                                "target_lang_sense_clarification": "проездной или входной документ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "307b0227-8331-316f-8a19-a7844dfbf05b",
                    "sense_definition": "Potoczne określenie oficjalnego wezwania do odbycia obowiązkowej służby wojskowej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_callup",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego plany na studia zostały nagle przerwane, gdy listonosz przyniósł mu <w>bilet</w>.",
                            "target_lang_translations": {
                                "ru": "Его планы на учебу были внезапно прерваны, когда почтальон принес ему <w>повестку</w>."
                            }
                        },
                        {
                            "text": "W dawnych czasach młodzi mężczyźni często obawiali się, że dostaną <w>bilet</w> do armii.",
                            "target_lang_translations": {
                                "ru": "В прежние времена молодые люди часто боялись, что получат <w>повестку</w> в армию."
                            }
                        }
                    ],
                    "synonyms": [
                        "wezwanie do wojska",
                        "powołanie",
                        "karta mobilizacyjna"
                    ],
                    "common_phrases": [
                        "dostać bilet",
                        "bilet do wojska",
                        "bilet do armii"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in context of conscription"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название официального вызова (повестки) для прохождения обязательной военной службы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повестка",
                                "target_lang_sense_clarification": "вызов в военкомат"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f75a147-95e8-32a6-bd3f-ddf238325efa",
                    "sense_definition": "Przestarzałe lub historyczne określenie banknotu albo papierowego znaku pieniężnego.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "money_note",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W muzeum numizmatycznym oglądaliśmy rzadki <w>bilet</w> skarbowy z czasów powstania kościuszkowskiego.",
                            "target_lang_translations": {
                                "ru": "В нумизматическом музее мы рассматривали редкий казначейский <w>билет</w> времен восстания Костюшко."
                            }
                        },
                        {
                            "text": "Rząd wyemitował nowe <w>bilety</w> bankowe, aby pokryć rosnące wydatki państwa.",
                            "target_lang_translations": {
                                "ru": "Правительство выпустило новые банковские <w>билеты</w>, чтобы покрыть растущие расходы государства."
                            }
                        }
                    ],
                    "synonyms": [
                        "banknot",
                        "asygnata"
                    ],
                    "common_phrases": [
                        "bilet skarbowy",
                        "bilet bankowy",
                        "emisja biletów"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее или историческое название банкноты или бумажного денежного знака."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "билет",
                                "target_lang_sense_clarification": "денежный знак (напр. банковский билет)"
                            },
                            {
                                "target_lang_word": "банкнота",
                                "target_lang_sense_clarification": "бумажные деньги"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
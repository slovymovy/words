    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fc379c2e-b49e-36a0-8383-e89bf0d8cffa",
                    "sense_definition": "Matka ojca lub matki.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "family",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja <babka> ma siedemdziesiąt lat i mieszka na wsi.",
                            "target_lang_translations": {
                                "ru": "Моей <бабушке> семьдесят лет, и она живет в деревне."
                            }
                        },
                        {
                            "text": "W każde wakacje odwiedzamy naszą <babkę> i dziadka.",
                            "target_lang_translations": {
                                "ru": "Каждые каникулы мы навещаем нашу <бабушку> и дедушку."
                            }
                        }
                    ],
                    "synonyms": [
                        "babcia",
                        "babunia"
                    ],
                    "antonyms": [
                        "wnuczka",
                        "dziadek"
                    ],
                    "common_phrases": [
                        "moja babka",
                        "babka macierzysta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мать отца или матери."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бабушка",
                                "target_lang_sense_clarification": "основное значение"
                            },
                            {
                                "target_lang_word": "бабуля",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f8f4b97-74c3-3c5f-b601-51da80b7bf29",
                    "sense_definition": "Słodkie ciasto pieczone w formie o kształcie ściętego stożka, często z dziurą w środku.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na deser mama podała pyszną <babkę> z lukrem.",
                            "target_lang_translations": {
                                "ru": "На десерт мама подала вкусный <кекс> с глазурью."
                            }
                        },
                        {
                            "text": "Tradycyjna <babka> wielkanocna musi być dobrze wyrośnięta.",
                            "target_lang_translations": {
                                "ru": "Традиционный пасхальный <кулич> должен хорошо подняться."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciasto"
                    ],
                    "common_phrases": [
                        "babka piaskowa",
                        "babka drożdżowa",
                        "babka wielkanocna",
                        "babka marmurkowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сладкий пирог, выпекаемый в форме усеченного конуса, часто с отверстием посередине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кекс",
                                "target_lang_sense_clarification": "общий термин для выпечки такой формы"
                            },
                            {
                                "target_lang_word": "кулич",
                                "target_lang_sense_clarification": "пасхальная выпечка"
                            },
                            {
                                "target_lang_word": "ромовая баба",
                                "target_lang_sense_clarification": "вид кондитерского изделия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "beda7ba7-c594-3346-8a2e-6b85cb799add",
                    "sense_definition": "Potoczne określenie kobiety, często budzącej sympatię lub atrakcyjnej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest naprawdę równa <babka>, można z nią o wszystkim pogadać.",
                            "target_lang_translations": {
                                "ru": "Это действительно мировая <тётка>, с ней можно обо всем поговорить."
                            }
                        },
                        {
                            "text": "Jakaś nieznajoma <babka> pytała o ciebie w recepcji.",
                            "target_lang_translations": {
                                "ru": "Какая-то незнакомая <женщина> спрашивала о тебе на ресепшене."
                            }
                        }
                    ],
                    "synonyms": [
                        "babeczka",
                        "kobieta",
                        "laska"
                    ],
                    "antonyms": [
                        "facet"
                    ],
                    "common_phrases": [
                        "równa babka",
                        "fajna babka",
                        "niezła babka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название женщины, часто вызывающей симпатию или привлекательной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тётка",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "женщина",
                                "target_lang_sense_clarification": "нейтральное"
                            },
                            {
                                "target_lang_word": "баба",
                                "target_lang_sense_clarification": "просторечное, может быть грубым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "769b9725-d7ef-368c-9b1f-5f189e5bebe4",
                    "sense_definition": "Uformowana z wilgotnego piasku bryła, zwykle w kształcie odwróconego wiaderka lub foremki.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "play",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzieci bawiły się w piaskownicy i robiły <babki>.",
                            "target_lang_translations": {
                                "ru": "Дети играли в песочнице и делали <куличики>."
                            }
                        },
                        {
                            "text": "Twoja <babka> z piasku jest o wiele większa niż moja.",
                            "target_lang_translations": {
                                "ru": "Твой <куличик> из песка намного больше моего."
                            }
                        }
                    ],
                    "common_phrases": [
                        "babka z piasku",
                        "robić babki",
                        "stawiać babki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фигура из влажного песка, обычно в форме перевернутого ведерка или формочки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "куличик",
                                "target_lang_sense_clarification": "детская игра в песке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b020ca17-1a40-3d0d-8762-36e0669b3d9c",
                    "sense_definition": "Pospolita roślina zielna o przyziemnej rozetce liści i niepozornych kwiatach, stosowana w ziołolecznictwie.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Liście <babki> przykłada się na drobne rany i stłuczenia.",
                            "target_lang_translations": {
                                "ru": "Листья <подорожника> прикладывают к мелким ранам и ушибам."
                            }
                        },
                        {
                            "text": "Przy polnej drodze rosła <babka> lancetowata.",
                            "target_lang_translations": {
                                "ru": "У проселочной дороги рос <подорожник> ланцетолистный."
                            }
                        }
                    ],
                    "common_phrases": [
                        "babka lancetowata",
                        "babka zwyczajna",
                        "liść babki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространенное травянистое растение с прикорневой розеткой листьев, используемое в фитотерапии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подорожник",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3fc0c7e-9ee9-36e1-8c36-a72856cad6e4",
                    "sense_definition": "Jadalny grzyb z rodziny borowikowatych o brązowym kapeluszu; koźlarz babka.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mycology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W brzozowym lesie znaleźliśmy dwie dorodne <babki>.",
                            "target_lang_translations": {
                                "ru": "В березовом лесу мы нашли два дородных <подберёзовика>."
                            }
                        },
                        {
                            "text": "Sos z <babek> jest doskonałym dodatkiem do obiadu.",
                            "target_lang_translations": {
                                "ru": "Соус из <подберёзовиков> — отличное дополнение к обеду."
                            }
                        }
                    ],
                    "synonyms": [
                        "koźlarz",
                        "kozak"
                    ],
                    "common_phrases": [
                        "koźlarz babka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Съедобный гриб из семейства болетовых с коричневой шляпкой; подберезовик."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подберёзовик",
                                "target_lang_sense_clarification": "гриб"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "64b42302-95f5-32f7-97da-303e57e4736b",
                    "sense_definition": "Wiejska kobieta zajmująca się leczeniem ziołami, zamawianiem chorób i pomaganiem przy porodach.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "folklore",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dawniej chorzy chodzili po pomoc do wiejskiej <babki>.",
                            "target_lang_translations": {
                                "ru": "Раньше больные ходили за помощью к деревенской <знахарке>."
                            }
                        },
                        {
                            "text": "Stara <babka> znała zioła na każdą dolegliwość.",
                            "target_lang_translations": {
                                "ru": "Старая <бабка> знала травы от любого недуга."
                            }
                        }
                    ],
                    "synonyms": [
                        "znachorka",
                        "szeptucha",
                        "zielarka",
                        "akuszerka"
                    ],
                    "common_phrases": [
                        "wiejska babka",
                        "iść do babki"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical or ethnographic context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Деревенская женщина, занимающаяся лечением травами, заговорами и помощью при родах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знахарка",
                                "target_lang_sense_clarification": "народная целительница"
                            },
                            {
                                "target_lang_word": "бабка",
                                "target_lang_sense_clarification": "разговорное название целительницы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5848a288-521d-372c-a5af-80fe4ed83f78",
                    "sense_definition": "Małe kowadełko używane przez rolników do klepania kosy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "tools",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik wbił <babkę> w pień i zaczął przygotowywać kosę do żniw.",
                            "target_lang_translations": {
                                "ru": "Фермер вбил <бабку> в пень и начал готовить косу к жатве."
                            }
                        },
                        {
                            "text": "Dźwięk uderzeń młotka o <babkę> niósł się po całej łące.",
                            "target_lang_translations": {
                                "ru": "Звук ударов молотка о <бабку> разносился по всему лугу."
                            }
                        }
                    ],
                    "synonyms": [
                        "kowadełko"
                    ],
                    "common_phrases": [
                        "klepać na babce"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "rural/technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленькая наковальня, используемая косарями для отбивания косы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бабка",
                                "target_lang_sense_clarification": "инструмент для отбивания косы"
                            }
                        ]
                    }
                }
            ]

{
    "word_family": [
        "data",
        "base",
        "databank",
        "subdatabase",
        "metadatabase"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c87ab08f-1e88-306d-9277-9e1c128e13ae",
                    "sense_definition": "A collection of organized information in a regular structure, usually accessible by a computer.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "info_collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have a <w>database</w> of all my contacts in my personal organizer.",
                            "target_lang_translations": {
                                "ru": "У меня есть <w>база данных</w> всех моих контактов в личном органайзере."
                            }
                        },
                        {
                            "text": "The police checked the national <w>database</w> to see if the suspect had a criminal record.",
                            "target_lang_translations": {
                                "ru": "Полиция проверила национальную <w>базу данных</w>, чтобы узнать, есть ли у подозреваемого судимость."
                            }
                        }
                    ],
                    "synonyms": [
                        "databank",
                        "data set",
                        "dataset"
                    ],
                    "common_phrases": [
                        "relational database",
                        "online database",
                        "search the database",
                        "comprehensive database"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность организованной информации, имеющая определенную структуру и обычно доступная с помощью компьютера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ба́за да́нных",
                                "target_lang_sense_clarification": "совокупность информации"
                            },
                            {
                                "target_lang_word": "ба́за",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02a3ef72-3375-3916-8b7b-7bc0694cafd7",
                    "sense_definition": "A software program used for storing, retrieving, and manipulating structured data.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "software_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Which <w>database</w> do you use for your web application: MySQL or Oracle?",
                            "target_lang_translations": {
                                "ru": "Какую <w>базу данных</w> вы используете для своего веб-приложения: MySQL или Oracle?"
                            }
                        },
                        {
                            "text": "The company decided to migrate to a cloud-based <w>database</w> to improve performance.",
                            "target_lang_translations": {
                                "ru": "Компания решила перейти на облачную <w>базу данных</w> для повышения производительности."
                            }
                        }
                    ],
                    "synonyms": [
                        "database management system",
                        "DB"
                    ],
                    "common_phrases": [
                        "database server",
                        "database administrator",
                        "open-source database"
                    ],
                    "target_lang_definitions": {
                        "ru": "Программное обеспечение, используемое для хранения, поиска и обработки структурированных данных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ба́за да́нных",
                                "target_lang_sense_clarification": "программное обеспечение"
                            },
                            {
                                "target_lang_word": "СУБД",
                                "target_lang_sense_clarification": "система управления базами данных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0228b038-ae02-38f6-a623-72af11ae77b2",
                    "sense_definition": "A specific set of tables and objects containing structured data within a system.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "info_collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The books <w>database</w> will have three tables, and the customers one will have two.",
                            "target_lang_translations": {
                                "ru": "<w>База данных</w> книг будет содержать три таблицы, а база клиентов — две."
                            }
                        },
                        {
                            "text": "We need to back up the production <w>database</w> every night to prevent data loss.",
                            "target_lang_translations": {
                                "ru": "Нам нужно делать резервную копию рабочей <w>базы данных</w> каждую ночь, чтобы предотвратить потерю данных."
                            }
                        }
                    ],
                    "synonyms": [
                        "dataset"
                    ],
                    "common_phrases": [
                        "database schema",
                        "database table",
                        "corrupt database"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный набор таблиц и объектов, содержащих структурированные данные внутри системы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ба́за да́нных",
                                "target_lang_sense_clarification": "конкретный экземпляр"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "27ff7300-f982-3697-b19e-a942b0d5f78b",
                    "sense_definition": "To enter data into a database.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The resulting citation collection was <w>databased</w> and coded for meaning.",
                            "target_lang_translations": {
                                "ru": "Полученная коллекция цитат была <w>занесена в базу данных</w> и закодирована по смыслу."
                            }
                        },
                        {
                            "text": "All the survey results need to be <w>databased</w> by the end of the week for analysis.",
                            "target_lang_translations": {
                                "ru": "Все результаты опроса должны быть <w>внесены в базу данных</w> до конца недели для анализа."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вносить данные в базу данных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заноси́ть в ба́зу да́нных",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "систематизи́ровать",
                                "target_lang_sense_clarification": "упорядочивать данные"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
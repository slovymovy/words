{
    "word_family": [
        "temperaturowy",
        "temperaturka"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b136718e-f919-3eb0-ba87-51ab8f43ecf5",
                    "sense_definition": "Wielkość fizyczna określająca stopień nagrzania ciała, substancji lub powietrza.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Woda osiąga <w>temperaturę</w> wrzenia przy stu stopniach Celsjusza.",
                            "target_lang_translations": {
                                "ru": "Вода достигает <w>температуры</w> кипения при ста градусах Цельсия."
                            }
                        },
                        {
                            "text": "Zimą w wysokich górach panuje bardzo niska <w>temperatura</w>.",
                            "target_lang_translations": {
                                "ru": "Зимой высоко в горах стоит очень низкая <w>температура</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "temperatura pokojowa",
                        "temperatura powietrza",
                        "temperatura wrzenia",
                        "wysoka temperatura"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическая величина, определяющая степень нагретости тела, вещества или воздуха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "температура",
                                "target_lang_sense_clarification": "физическая величина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b51f933d-505e-3139-aae6-fc8747ad9f85",
                    "sense_definition": "Podwyższona ciepłota ciała będąca objawem choroby lub infekcji.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "health",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zalecił pacjentowi regularnie mierzyć <w>temperaturę</w> termometrem.",
                            "target_lang_translations": {
                                "ru": "Врач посоветовал пациенту регулярно измерять <w>температуру</w> градусником."
                            }
                        },
                        {
                            "text": "Dziecko źle się czuje i chyba ma wysoką <w>temperaturę</w>.",
                            "target_lang_translations": {
                                "ru": "Ребёнок плохо себя чувствует, и, кажется, у него высокая <w>температура</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gorączka"
                    ],
                    "common_phrases": [
                        "mieć temperaturę",
                        "mierzyć temperaturę",
                        "zbijać temperaturę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повышенная температура тела как симптом болезни или инфекции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "температура",
                                "target_lang_sense_clarification": "жар, лихорадка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8e2ff3a-05ff-35d9-8dee-a560c437c8f4",
                    "sense_definition": "Napięcie emocjonalne lub ogólny klimat panujący w grupie ludzi.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas debaty politycznej <w>temperatura</w> dyskusji znacznie wzrosła.",
                            "target_lang_translations": {
                                "ru": "Во время политических дебатов <w>накал</w> дискуссии значительно возрос."
                            }
                        },
                        {
                            "text": "Jego spokojne słowa natychmiast obniżyły <w>temperaturę</w> sporu.",
                            "target_lang_translations": {
                                "ru": "Его спокойные слова немедленно снизили <w>градус</w> спора."
                            }
                        }
                    ],
                    "synonyms": [
                        "nastrój"
                    ],
                    "common_phrases": [
                        "temperatura sporu",
                        "temperatura uczuć",
                        "podgrzewać temperaturę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Эмоциональное напряжение или общая атмосфера в группе людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "накал",
                                "target_lang_sense_clarification": "напряжение"
                            },
                            {
                                "target_lang_word": "градус",
                                "target_lang_sense_clarification": "уровень напряжения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a9efe284-ae7e-32ff-8eee-ee545f5c84a2",
                    "sense_definition": "Полностью, совершенно, без изъятия или исключений.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_completeness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы <w>целиком</w> и полностью поддерживаем это мудрое решение.",
                            "target_lang_translations": {
                                "en": "We <w>fully</w> and completely support this wise decision."
                            }
                        },
                        {
                            "text": "Автор <w>целиком</w> посвятил свою новую книгу истории родного края.",
                            "target_lang_translations": {
                                "en": "The author <w>entirely</w> dedicated his new book to the history of his native land."
                            }
                        }
                    ],
                    "synonyms": [
                        "полностью",
                        "совершенно",
                        "безраздельно"
                    ],
                    "antonyms": [
                        "частично"
                    ],
                    "common_phrases": [
                        "целиком и полностью",
                        "посвятить себя целиком",
                        "зависеть целиком"
                    ],
                    "target_lang_definitions": {
                        "en": "Completely, entirely, without exception or reservation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "entirely",
                                "target_lang_sense_clarification": "to the full extent; completely"
                            },
                            {
                                "target_lang_word": "wholly",
                                "target_lang_sense_clarification": "exclusively or completely; often formal"
                            },
                            {
                                "target_lang_word": "completely",
                                "target_lang_sense_clarification": "totally; in every way"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec083168-a72b-3ef3-b777-87408b9cd041",
                    "sense_definition": "В полном объёме, в нераздробленном виде, одним куском.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_wholeness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Змея может проглотить <w>целиком</w> даже крупное животное.",
                            "target_lang_translations": {
                                "en": "A snake can swallow even a large animal <w>whole</w>."
                            }
                        },
                        {
                            "text": "К праздничному столу мы решили зажарить гуся <w>целиком</w>.",
                            "target_lang_translations": {
                                "en": "For the festive table, we decided to roast the goose <w>whole</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "полностью"
                    ],
                    "antonyms": [
                        "частично"
                    ],
                    "common_phrases": [
                        "проглотить целиком",
                        "зажарить целиком",
                        "сохраниться целиком"
                    ],
                    "target_lang_definitions": {
                        "en": "In its entirety, as a single unit, not broken into pieces."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "whole",
                                "target_lang_sense_clarification": "in one piece; unfragmented (e.g., swallow whole)"
                            },
                            {
                                "target_lang_word": "in one piece",
                                "target_lang_sense_clarification": "intact; not divided or cut"
                            }
                        ]
                    }
                }
            ]
        }
    ]

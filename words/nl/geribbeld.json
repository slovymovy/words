            "senses": [
                {
                    "sense_id": "8b98f14e-ec1d-3d24-975b-187ca3ede7cd",
                    "sense_definition": "Voorzien van kleine, langwerpige verhogingen of groeven op het oppervlak; niet glad.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "texture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met <w>geribbelde</w> tegels worden blinden naar de perrons geleid.",
                            "target_lang_translations": {
                                "en": "Blind people are guided to the platforms by <w>ribbed</w> tiles.",
                                "ru": "С помощью <w>рифлёной</w> плитки слепых направляют к платформам."
                            }
                        },
                        {
                            "text": "Ik koop liever <w>geribbelde</w> chips omdat die vaak krokanter zijn.",
                            "target_lang_translations": {
                                "en": "I prefer buying <w>ridged</w> chips because they are often crunchier.",
                                "ru": "Я предпочитаю покупать <w>рифлёные</w> чипсы, так как они часто более хрустящие."
                            }
                        }
                    ],
                    "synonyms": [
                        "gegroefd"
                    ],
                    "antonyms": [
                        "glad",
                        "egaal"
                    ],
                    "common_phrases": [
                        "geribbelde chips",
                        "geribbeld oppervlak",
                        "geribbeld karton"
                    ],
                    "target_lang_definitions": {
                        "en": "Provided with small, elongated ridges or grooves on the surface; not smooth.",
                        "ru": "Имеющий небольшие продолговатые выступы или канавки на поверхности; не гладкий."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ribbed",
                                "target_lang_sense_clarification": "having a pattern of raised bands or lines (e.g., fabric, cardboard)"
                            },
                            {
                                "target_lang_word": "ridged",
                                "target_lang_sense_clarification": "having raised strips or ridges (e.g., potato chips)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "рифлёный",
                                "target_lang_sense_clarification": "с бороздками (чипсы, стекло, металл)"
                            },
                            {
                                "target_lang_word": "ребристый",
                                "target_lang_sense_clarification": "имеющий ребра, неровный"
                            }
                        ]
                    }
                }
            ]
        },

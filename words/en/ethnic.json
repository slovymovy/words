        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "90a0bb46-506d-3c1f-9536-b05cde339ed1",
                    "sense_definition": "Of or relating to a group of people having common racial, ancestral, national, religious, or cultural origins.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_identity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The city is known for its diverse <w>ethnic</w> communities.",
                            "target_lang_translations": {
                                "ru": "Город известен своими разнообразными <w>этническими</w> общинами."
                            }
                        },
                        {
                            "text": "She is proud of her <w>ethnic</w> background and family traditions.",
                            "target_lang_translations": {
                                "ru": "Она гордится своим <w>этническим</w> происхождением и семейными традициями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ethnic group",
                        "ethnic minority",
                        "ethnic background",
                        "ethnic origin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к группе людей, имеющих общее расовое, национальное, религиозное или культурное происхождение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "этнический",
                                "target_lang_sense_clarification": "относящийся к этносу"
                            },
                            {
                                "target_lang_word": "национальный",
                                "target_lang_sense_clarification": "относящийся к нации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26055972-aa9a-395a-b925-65f358c5705a",
                    "sense_definition": "Characteristic of a foreign or a minority group's culture, often referring to food, clothing, or customs.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "cultural_style",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to try a new <w>ethnic</w> restaurant downtown.",
                            "target_lang_translations": {
                                "ru": "Мы решили попробовать новый ресторан <w>этнической</w> кухни в центре города."
                            }
                        },
                        {
                            "text": "The shop sells <w>ethnic</w> clothing from various countries.",

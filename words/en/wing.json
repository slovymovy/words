    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9172687c-49d6-3f4f-9f92-0d349080aa12",
                    "sense_definition": "An appendage of an animal's (bird, bat, insect) body that enables it to fly.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "flight_appendage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bird spread its <w>wings</w> and flew away.",
                            "target_lang_translations": {
                                "ru": "Птица расправила <w>крылья</w> и улетела."
                            }
                        },
                        {
                            "text": "Butterflies have beautiful colorful <w>wings</w>.",
                            "target_lang_translations": {
                                "ru": "У бабочек красивые разноцветные <w>крылья</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "spread one's wings",
                        "on the wing",
                        "take wing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть тела животного (птицы, летучей мыши, насекомого), позволяющая ему летать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крыло",
                                "target_lang_sense_clarification": "часть тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "567eb419-b0bf-3662-92a6-2e79b79abe89",
                    "sense_definition": "Part of an aircraft that produces the lift for rising into the air.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "flight_appendage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I took my seat on the plane, overlooking the <w>wing</w>.",
                            "target_lang_translations": {
                                "ru": "Я занял своё место в самолёте с видом на <w>крыло</w>."
                            }
                        },
                        {
                            "text": "The pilot checked the left <w>wing</w> before takeoff.",
                            "target_lang_translations": {
                                "ru": "Пилот проверил левое <w>крыло</w> перед взлётом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "airplane wing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть самолета, создающая подъемную силу для взлета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крыло",
                                "target_lang_sense_clarification": "самолета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12b03eb5-eabb-3053-b55d-8a9c24d095b0",
                    "sense_definition": "A part of a building that is lesser in size than the main body, or located at the side.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "side_extension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The west <w>wing</w> of the hospital is used for emergencies.",
                            "target_lang_translations": {
                                "ru": "Западное <w>крыло</w> больницы используется для экстренных случаев."
                            }
                        },
                        {
                            "text": "They are building a new <w>wing</w> for the library.",
                            "target_lang_translations": {
                                "ru": "Они строят новое <w>крыло</w> для библиотеки."
                            }
                        }
                    ],
                    "synonyms": [
                        "annex",
                        "extension"
                    ],
                    "common_phrases": [
                        "west wing",
                        "east wing",
                        "hospital wing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть здания, меньшая по размеру, чем основной корпус, или расположенная сбоку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крыло",
                                "target_lang_sense_clarification": "здания"
                            },
                            {
                                "target_lang_word": "флигель",
                                "target_lang_sense_clarification": "пристройка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4937f4b-cfc3-3a44-a7c3-b535fc5b33b7",
                    "sense_definition": "A faction of a political movement, usually implying a position apart from the mainstream center.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "side_extension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The liberal <w>wing</w> of the party voted against the bill.",
                            "target_lang_translations": {
                                "ru": "Либеральное <w>крыло</w> партии проголосовало против законопроекта."
                            }
                        },
                        {
                            "text": "He belongs to the radical <w>wing</w> of the movement.",
                            "target_lang_translations": {
                                "ru": "Он принадлежит к радикальному <w>крылу</w> движения."
                            }
                        }
                    ],
                    "synonyms": [
                        "faction",
                        "camp"
                    ],
                    "common_phrases": [
                        "left wing",
                        "right wing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фракция политического движения, обычно занимающая позицию, отличную от основного центра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крыло",
                                "target_lang_sense_clarification": "политическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "859edd39-d702-3edc-a317-ab8fc45005a3",
                    "sense_definition": "A position in several field games on either side of the field.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "side_extension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Smith started the game in the centre but moved to the <w>wing</w> later.",
                            "target_lang_translations": {
                                "ru": "Смит начал игру в центре, но позже сместился на <w>фланг</w>."
                            }
                        },
                        {
                            "text": "The coach needs a fast player on the left <w>wing</w>.",
                            "target_lang_translations": {
                                "ru": "Тренеру нужен быстрый игрок на левый <w>фланг</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "flank"
                    ],
                    "common_phrases": [
                        "play on the wing",
                        "left wing",
                        "right wing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Позиция в некоторых спортивных играх на одной из сторон поля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фланг",
                                "target_lang_sense_clarification": "спорт"
                            },
                            {
                                "target_lang_word": "край",
                                "target_lang_sense_clarification": "позиция на поле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0abb6465-2704-37a5-bfc8-e8dfd62c53fd",
                    "sense_definition": "One of the unseen areas on the side of the stage in a theatre.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "side_extension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The actors waited nervously in the <w>wings</w> for their cue.",
                            "target_lang_translations": {
                                "ru": "Актёры нервно ждали своего выхода за <w>кулисами</w>."
                            }
                        },
                        {
                            "text": "She watched the performance from the <w>wings</w>.",
                            "target_lang_translations": {
                                "ru": "Она смотрела представление из-за <w>кулис</w>."
                            }
                        }
                    ],
                    "common_phrases": [

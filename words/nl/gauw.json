                        "vlug",
                        "rap"
                    ],
                    "antonyms": [
                        "langzaam",
                        "traag"
                    ],
                    "common_phrases": [
                        "zo gauw mogelijk",
                        "even gauw",
                        "niet zo gauw"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро, поспешно; за короткое время.",
                        "en": "In a quick manner; within a short timeframe."

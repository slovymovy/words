    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f0814402-3e76-3133-b833-f98969124c4c",
                    "sense_definition": "To engage in communication and other shared activities with someone.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "social_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher encouraged the students to <w>interact</w> with each other during the project.",
                            "target_lang_translations": {
                                "ru": "Учитель поощрял студентов <w>взаимодействовать</w> друг с другом во время работы над проектом."
                            }
                        },
                        {
                            "text": "It is important to learn how to <w>interact</w> socially in different situations.",
                            "target_lang_translations": {
                                "ru": "Важно научиться <w>общаться</w> в различных социальных ситуациях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "interact with others",
                        "interact socially",
                        "interact with people"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вступать в общение или совместную деятельность с кем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взаимодействовать",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "общаться",
                                "target_lang_sense_clarification": "социальный контекст"
                            },
                            {
                                "target_lang_word": "контактировать",
                                "target_lang_sense_clarification": "устанавливать связь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "140c7ba8-aea9-397e-ba40-3d20452f667e",
                    "sense_definition": "To have an effect on each other or work together.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should ask your doctor if this medicine will <w>interact</w> with your current diet.",
                            "target_lang_translations": {
                                "ru": "Вам следует спросить врача, будет ли это лекарство <w>взаимодействовать</w> с вашей текущей диетой."
                            }
                        },
                        {
                            "text": "The two chemicals <w>interact</w> to create a powerful cleaning solution.",
                            "target_lang_translations": {
                                "ru": "Два химиката <w>взаимодействуют</w>, создавая мощный чистящий раствор."
                            }
                        }
                    ],
                    "common_phrases": [
                        "interact with",
                        "interact closely"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказывать влияние друг на друга или вступать в реакцию (о веществах, системах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взаимодействовать",
                                "target_lang_sense_clarification": "влиять друг на друга"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d25d33c7-0261-343a-8177-e56d2c1c5260",
                    "sense_definition": "A short act or piece between others, as in a play; a break between acts.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "theater",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The audience went to the lobby to buy refreshments during the <w>interact</w>.",
                            "target_lang_translations": {
                                "ru": "Зрители вышли в фойе, чтобы купить напитки во время <w>антракта</w>."
                            }
                        },
                        {
                            "text": "Soft music was played during the <w>interact</w> to keep the mood of the play.",
                            "target_lang_translations": {
                                "ru": "Во время <w>интермедии</w> играла тихая музыка, чтобы сохранить настроение пьесы."
                            }
                        }

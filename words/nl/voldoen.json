                    "sense_id": "841c8d7f-2b5d-3e2d-a890-8c04e6c60f9a",
                    "sense_definition": "Aan een eis, wens, verzoek of verwachting beantwoorden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met dit goede examenresultaat <w>voldoet</w> hij ruimschoots aan de verwachtingen van zijn ouders.",
                            "target_lang_translations": {
                                "en": "With this good exam result, he amply <w>meets</w> his parents' expectations.",
                                "ru": "С этим хорошим результатом экзамена он с лихвой <w>оправдывает</w> ожидания своих родителей."
                            }
                        },
                        {
                            "text": "Het nieuwe gebouw moet aan strenge veiligheidseisen <w>voldoen</w> voordat het open mag.",
                            "target_lang_translations": {
                                "en": "The new building must <w>comply with</w> strict safety requirements before it is allowed to open.",
                                "ru": "Новое здание должно <w>соответствовать</w> строгим требованиям безопасности перед открытием."
                            }
                        },
                        {
                            "text": "Helaas <w>voldoet</w> dit product niet aan de kwaliteitseisen die wij als bedrijf stellen.",
                            "target_lang_translations": {
                                "en": "Unfortunately, this product does not <w>meet</w> the quality requirements that we set as a company.",
                                "ru": "К сожалению, этот продукт не <w>отвечает</w> требованиям качества, которые мы устанавливаем как компания."
                            }
                        }
                    ],
                    "synonyms": [
                        "beantwoorden",
                        "nakomen",
                        "vervullen"
                    ],
                    "antonyms": [
                        "verzaken",
                        "teleurstellen"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e07878e3-59db-3b54-b4e5-1c68c764d64a",
                    "sense_definition": "Een vuur of gloed feller laten branden door er lucht op te blazen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "kindle_incite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet de houtskool goed <w>aanblazen</w> zodat de barbecue heet genoeg wordt.",
                            "target_lang_translations": {
                                "en": "You need to <w>blow on</w> the charcoal well so the barbecue gets hot enough.",
                                "ru": "Тебе нужно хорошенько <w>раздуть</w> угли, чтобы барбекю достаточно нагрелось."
                            }
                        },
                        {
                            "text": "De smid heeft het vuur in de oven krachtig <w>aangeblazen</w>.",
                            "target_lang_translations": {
                                "en": "The blacksmith vigorously <w>fanned</w> the fire in the furnace.",
                                "ru": "Кузнец сильно <w>раздул</w> огонь в печи."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanwakkeren"
                    ],
                    "antonyms": [
                        "doven",
                        "uitblazen"
                    ],
                    "common_phrases": [
                        "het vuur aanblazen"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a fire or glow burn more intensely by blowing air onto it.",
                        "ru": "Заставить огонь или угли гореть ярче, нагнетая на них воздух."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fan",
                                "target_lang_sense_clarification": "a fire"
                            },
                            {
                                "target_lang_word": "blow on",
                                "target_lang_sense_clarification": "embers"
                            },
                            {
                                "target_lang_word": "kindle",
                                "target_lang_sense_clarification": "a flame"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "раздувать",
                                "target_lang_sense_clarification": "огонь, угли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b833a44-f745-3795-8b6f-194a1d0654aa",
                    "sense_definition": "Een conflict, emotie of gerucht versterken of stimuleren.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "kindle_incite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn boze woorden zullen de ruzie alleen maar verder <w>aanblazen</w>.",
                            "target_lang_translations": {
                                "en": "His angry words will only further <w>fuel</w> the argument.",
                                "ru": "Его злые слова только сильнее <w>раздуют</w> ссору."
                            }
                        },
                        {
                            "text": "Het gerucht werd door de roddelpers flink <w>aangeblazen</w>.",
                            "target_lang_translations": {
                                "en": "The rumor was heavily <w>fueled</w> by the tabloids.",
                                "ru": "Слух был сильно <w>раздут</w> желтой прессой."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanwakkeren",
                        "opstoken"
                    ],
                    "antonyms": [
                        "sussen",
                        "kalmeren"
                    ],
                    "common_phrases": [
                        "een conflict aanblazen"
                    ],
                    "target_lang_definitions": {
                        "en": "To strengthen or stimulate a conflict, emotion, or rumor.",
                        "ru": "Усиливать или провоцировать конфликт, эмоцию или слух."
                    },

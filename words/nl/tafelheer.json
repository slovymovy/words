    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "08299088-cf26-3513-801e-a7e656b1e128",
                    "sense_definition": "Man die de presentator van een talkshow assisteert en actief deelneemt aan het gesprek aan tafel.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De presentator en zijn vaste <w>tafelheer</w> bespraken het nieuws van de dag.",
                            "target_lang_translations": {
                                "ru": "Ведущий и его постоянный <w>соведущий</w> обсуждали новости дня."
                            }
                        },
                        {
                            "text": "Als <w>tafelheer</w> mag hij regelmatig zijn mening geven over de gasten.",
                            "target_lang_translations": {
                                "ru": "Как <w>соведущий</w>, он может регулярно высказывать свое мнение о гостях."
                            }
                        }
                    ],
                    "synonyms": [
                        "sidekick"
                    ],
                    "common_phrases": [
                        "vaste tafelheer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, ассистирующий ведущему ток-шоу и активно участвующий в беседе за столом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соведущий",
                                "target_lang_sense_clarification": "на ток-шоу"
                            },
                            {
                                "target_lang_word": "помощник ведущего",
                                "target_lang_sense_clarification": "ассистент в эфире"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8ae3f3c-23f8-3ce9-a6e6-9af7087dd2eb",
                    "sense_definition": "Man die tijdens een formeel diner naast een dame zit en geacht wordt haar gezelschap te houden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens het galadiner schonk haar <w>tafelheer</w> beleefd haar glas bij.",
                            "target_lang_translations": {
                                "ru": "Во время гала-ужина её <w>сосед по столу</w> вежливо наполнил её бокал."
                            }
                        },
                        {
                            "text": "Zij wist niet wie haar <w>tafelheer</w> zou zijn tot ze het naamkaartje zag.",
                            "target_lang_translations": {
                                "ru": "Она не знала, кто будет её <w>соседом по столу</w>, пока не увидела карточку с именем."
                            }
                        }
                    ],
                    "antonyms": [
                        "tafeldame"
                    ],
                    "common_phrases": [
                        "fungeren als tafelheer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, который сидит рядом с дамой на официальном ужине и должен развлекать её."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сосед по столу",
                                "target_lang_sense_clarification": "на званом ужине"
                            },
                            {
                                "target_lang_word": "кавалер",
                                "target_lang_sense_clarification": "сопровождающий даму за столом"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
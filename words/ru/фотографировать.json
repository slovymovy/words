                    "frequency": "High",
                    "semantic_group_id": "action_photo",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я обожаю <фотографировать>, повсюду таскаю с собой фотоаппарат.",
                            "target_lang_translations": {
                                "en": "I love <w>taking pictures</w>, I carry my camera with me everywhere."
                            }
                        },
                        {
                            "text": "Туристы любят <фотографировать> красивые здания в центре города.",
                            "target_lang_translations": {
                                "en": "Tourists love <w>taking pictures of</w> beautiful buildings in the city center."
                            }
                        }
                    ],
                    "synonyms": [
                        "снимать",
                        "фоткать"
                    ],
                    "common_phrases": [
                        "фотографировать природу",
                        "фотографировать друзей",
                        "фотографировать на память",
                        "фотографировать документы"
                    ],
                    "target_lang_definitions": {
                        "en": "To create images (shots) using a camera."
                    },
                    "translations": {

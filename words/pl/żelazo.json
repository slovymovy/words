                },
                {
                    "sense_id": "2a00aead-4de5-3347-b52f-ac40c53f5b32",
                    "sense_definition": "Kilof górniczy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "5",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Górnik chwycił <w>żelazo</w> i ruszył do pracy w kopalni.",
                            "target_lang_translations": {
                                "ru": "Шахтер схватил <w>кайло</w> и отправился работать в шахту."
                            }
                        },
                        {
                            "text": "Stare <w>żelazo</w> służyło mu wiernie przez wiele lat pod ziemią.",
                            "target_lang_translations": {
                                "ru": "Старое <w>кайло</w> верно служило ему много лет под землей."
                            }
                        }

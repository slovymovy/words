    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "09025c0b-39c3-3546-bc85-80b062ed4d9f",
                    "sense_definition": "Человек, который находится в состоянии вражды с кем-либо, желает зла.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "person_hostile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У этого доброго человека совсем нет <w>врагов</w>.",
                            "target_lang_translations": {
                                "en": "This kind man has no <w>enemies</w> at all."
                            }
                        },
                        {
                            "text": "Он мой старый друг, а не <w>враг</w>.",
                            "target_lang_translations": {
                                "en": "He is my old friend, not an <w>enemy</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "недруг",
                        "противник"
                    ],
                    "antonyms": [
                        "друг"
                    ],
                    "common_phrases": [
                        "заклятый враг",
                        "личный враг",
                        "нажить врагов",
                        "смертельный враг"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who is hostile towards someone and wishes them harm."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "enemy",
                                "target_lang_sense_clarification": "personal opponent"
                            },
                            {
                                "target_lang_word": "foe",
                                "target_lang_sense_clarification": "literary or formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1969b74b-3154-38eb-b0e3-cc80c8312770",
                    "sense_definition": "Военный противник; армия или страна, с которой ведётся война.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "military_hostile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наши солдаты смело пошли в атаку на <w>врага</w>.",
                            "target_lang_translations": {
                                "en": "Our soldiers bravely went on the attack against the <w>enemy</w>."
                            }
                        },
                        {
                            "text": "Армия <w>врага</w> отступила далеко от границы.",
                            "target_lang_translations": {
                                "en": "The <w>enemy</w> army retreated far from the border."
                            }
                        }
                    ],
                    "synonyms": [
                        "противник",
                        "неприятель"
                    ],
                    "antonyms": [
                        "союзник"
                    ],
                    "common_phrases": [
                        "тыл врага",
                        "разбить врага",
                        "окружить врага",
                        "наступление врага"
                    ],
                    "target_lang_definitions": {
                        "en": "A military opponent; an army or country with which a war is being waged."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "enemy",
                                "target_lang_sense_clarification": "military force"
                            },
                            {
                                "target_lang_word": "foe",
                                "target_lang_sense_clarification": "adversary"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77b184be-e591-373b-843c-5b82a16cf729",
                    "sense_definition": "То, что приносит вред, зло или является помехой (в переносном смысле).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_harm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Курение — это опасный <w>враг</w> твоего здоровья.",
                            "target_lang_translations": {
                                "en": "Smoking is a dangerous <w>enemy</w> of your health."
                            }
                        },
                        {
                            "text": "Лень часто становится главным <w>врагом</w> успеха.",
                            "target_lang_translations": {
                                "en": "Laziness often becomes the main <w>enemy</w> of success."
                            }
                        }
                    ],
                    "common_phrases": [
                        "враг народа",
                        "лучшее — враг хорошего",
                        "враг здоровья"
                    ],
                    "target_lang_definitions": {
                        "en": "Something that causes harm, evil, or acts as a hindrance (figuratively)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "enemy",
                                "target_lang_sense_clarification": "harmful influence"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "15970525-ad6c-3a89-9493-a95814829581",
                    "sense_definition": "A stupid person, idiot, or fool.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "stupidity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I felt like a complete <w>moron</w> when I realized I had been wearing my shirt inside out all day.",
                            "target_lang_translations": {
                                "ru": "Я чувствовал себя полным <w>идиотом</w>, когда понял, что весь день носил рубашку наизнанку."
                            }
                        },
                        {
                            "text": "Don't be such a <w>moron</w> and put your seatbelt on before we start driving.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>придурком</w>, пристегни ремень безопасности, прежде чем мы поедем."
                            }
                        }
                    ],
                    "synonyms": [
                        "idiot",
                        "fool",
                        "imbecile",
                        "dolt",
                        "blockhead",
                        "dunce",
                        "nitwit",
                        "dimwit",
                        "simpleton",
                        "numbskull"
                    ],
                    "antonyms": [
                        "genius",
                        "intellectual",
                        "sage"
                    ],
                    "common_phrases": [
                        "complete moron",
                        "total moron",
                        "act like a moron",
                        "bunch of morons"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "derogatory"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Глупый человек, идиот или дурак."

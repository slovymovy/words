    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "dc705631-4415-3cd1-a975-fc909da40dc6",
                    "sense_definition": "związany z porą roku – latem, występujący w lecie lub do niego przystosowany",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "season_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uwielbiam długie <w>letnie</w> wieczory spędzane w ogrodzie.",
                            "target_lang_translations": {
                                "ru": "Я обожаю долгие <w>летние</w> вечера, проводимые в саду."
                            }
                        },
                        {
                            "text": "W Polsce obowiązuje czas <w>letni</w> od marca do października.",
                            "target_lang_translations": {
                                "ru": "В Польше действует <w>летнее</w> время с марта по октябрь."
                            }
                        }
                    ],
                    "synonyms": [
                        "reg. śl. latowy"
                    ],
                    "antonyms": [
                        "zimowy"
                    ],
                    "common_phrases": [
                        "czas letni",
                        "letnia sukienka",
                        "letni deszcz",
                        "domek letni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с временем года — летом; происходящий летом или приспособленный для него"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "летний",
                                "target_lang_sense_clarification": "о времени года"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "682398fc-4167-3e67-a9dc-4d21be9fc342",
                    "sense_definition": "lekko ciepły, ale nie gorący; o umiarkowanej temperaturze (zwykle o płynach lub powietrzu)",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "temperature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do ciasta należy dodać szklankę <w>letniej</w> wody.",
                            "target_lang_translations": {
                                "ru": "В тесто нужно добавить стакан <w>тепловатой</w> воды."
                            }
                        },
                        {
                            "text": "Herbata jest już <w>letnia</w>, więc możesz ją spokojnie wypić.",
                            "target_lang_translations": {
                                "ru": "Чай уже <w>тепловатый</w>, так что можешь спокойно его выпить."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciepławy",
                        "letny"
                    ],
                    "antonyms": [
                        "gorliwy",
                        "zimowy"
                    ],
                    "common_phrases": [
                        "letnia woda",
                        "letni prysznic",
                        "letnia temperatura"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слегка тёплый, но не горячий; умеренной температуры (обычно о жидкостях или воздухе)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тепловатый",
                                "target_lang_sense_clarification": "о температуре"
                            },
                            {
                                "target_lang_word": "чуть тёплый",
                                "target_lang_sense_clarification": "не горячий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9255ec75-3906-3165-a4fa-9bfa54c4d801",
                    "sense_definition": "pozbawiony zapału, entuzjazmu lub silnych emocji; umiarkowany w uczuciach",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego pomysł spotkał się z dość <w>letnim</w> przyjęciem przez zespół.",
                            "target_lang_translations": {
                                "ru": "Его идея встретила довольно <w>прохладный</w> приём со стороны команды."
                            }
                        },
                        {
                            "text": "Mimo naszych starań, ich reakcja była <w>letnia</w> i bez emocji.",
                            "target_lang_translations": {
                                "ru": "Несмотря на наши старания, их реакция была <w>сдержанной</w> и безэмоциональной."
                            }
                        }
                    ],
                    "synonyms": [
                        "obojętny"
                    ],
                    "antonyms": [
                        "gorliwy"
                    ],
                    "common_phrases": [
                        "letnie przyjęcie",
                        "letni stosunek",
                        "letni entuzjazm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишённый энтузиазма или сильных эмоций; сдержанный в чувствах"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прохладный",
                                "target_lang_sense_clarification": "об отношении или приёме"
                            },
                            {
                                "target_lang_word": "сдержанный",
                                "target_lang_sense_clarification": "об эмоциях"
                            }
                        ]
                    }
                }
            ]
        }
    ]

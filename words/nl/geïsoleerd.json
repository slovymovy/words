                {
                    "sense_id": "40fb0650-757f-3fd4-b770-e97c9560ed0b",
                    "sense_definition": "Afgeschermd van de omgeving om invloeden zoals kou, warmte of geluid tegen te houden, of om contact te voorkomen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "separated_protected",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ons nieuwe huis is uitstekend <w>geïsoleerd</w> tegen de winterkou.",
                            "target_lang_translations": {
                                "en": "Our new house is excellently <w>insulated</w> against the winter cold.",
                                "ru": "Наш новый дом отлично <w>изолирован</w> от зимнего холода."
                            }
                        },
                        {
                            "text": "De patiënt werd in een <w>geïsoleerde</w> ruimte verpleegd om besmetting te voorkomen.",
                            "target_lang_translations": {
                                "en": "The patient was nursed in an <w>isolated</w> room to prevent infection.",
                                "ru": "Пациента лечили в <w>изолированной</w> палате, чтобы предотвратить заражение."
                            }
                        }
                    ],
                    "synonyms": [
                        "afgeschermd",
                        "afgezonderd",
                        "beschermd"
                    ],
                    "antonyms": [
                        "open",
                        "toegankelijk"
                    ],
                    "common_phrases": [
                        "goed geïsoleerd",
                        "slecht geïsoleerd",
                        "geluid-geïsoleerd"
                    ],
                    "target_lang_definitions": {

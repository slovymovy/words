                {
                    "sense_id": "2c911ffc-47ef-3b1c-aa3b-3ca6f7b86dd0",
                    "sense_definition": "Having a color; not black, white, or transparent.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "literal_color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She used <w>colored</w> pencils to draw a rainbow.",
                            "target_lang_translations": {
                                "ru": "Она использовала <w>цветные</w> карандаши, чтобы нарисовать радугу."
                            }
                        },
                        {
                            "text": "The bird has brightly <w>colored</w> feathers that attract a mate.",
                            "target_lang_translations": {
                                "ru": "У этой птицы ярко <w>окрашенные</w> перья, которые привлекают партнера."
                            }
                        }
                    ],
                    "synonyms": [
                        "hued"
                    ],
                    "antonyms": [
                        "colorless",
                        "achromatic"
                    ],
                    "common_phrases": [
                        "colored pencils",
                        "brightly colored",
                        "dark-colored",
                        "cream-colored"
                    ],
                    "target_lang_definitions": {

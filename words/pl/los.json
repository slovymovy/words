    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7c657d61-66f0-3aba-9faa-eb8ce3b4b945",
                    "sense_definition": "Mała kartka pełniąca rolę kuponu, decydująca o wygranej lub przegranej w loterii lub grze.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_ticket",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem w kiosku <w>los</w> na loterii i czekam na wyniki.",
                            "target_lang_translations": {
                                "ru": "Я купил в киоске <w>лотерейный билет</w> и жду результатов."
                            }
                        },
                        {
                            "text": "Niestety trafił mi się pusty <w>los</w>, więc nic nie wygrałem.",
                            "target_lang_translations": {
                                "ru": "К сожалению, мне попался пустой <w>билет</w>, поэтому я ничего не выиграл."
                            }
                        }
                    ],
                    "synonyms": [
                        "kupon"
                    ],
                    "common_phrases": [
                        "kupić los",
                        "pusty los",
                        "ciągnąć losy",
                        "szczęśliwy los"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой листок бумаги, выполняющий роль купона, который определяет выигрыш или проигрыш в лотерее или игре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лотерейный билет",
                                "target_lang_sense_clarification": "полное название"
                            },
                            {
                                "target_lang_word": "билет",
                                "target_lang_sense_clarification": "разговорное сокращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52eb01b3-c99b-3389-b28b-31e6cd61fa18",
                    "sense_definition": "Siła wyższa, która rzekomo decyduje o życiu człowieka i przyszłych wydarzeniach; przeznaczenie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_fate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To musi być zrządzenie <w>losu</w>, że spotkaliśmy się w tym miejscu.",
                            "target_lang_translations": {
                                "ru": "Должно быть, это знак <w>судьбы</w>, что мы встретились в этом месте."
                            }
                        },
                        {
                            "text": "Nie można uciec przed swoim <w>losem</w>, cokolwiek by się robiło.",
                            "target_lang_translations": {
                                "ru": "Нельзя уйти от своей <w>судьбы</w>, что бы ты ни делал."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeznaczenie",
                        "dola"
                    ],
                    "common_phrases": [
                        "zrządzenie losu",
                        "ironia losu",
                        "ślepy los",
                        "pogodzić się z losem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высшая сила, которая якобы определяет жизнь человека и будущие события; предназначение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "судьба",
                                "target_lang_sense_clarification": "высшая сила, предопределение"
                            },
                            {
                                "target_lang_word": "рок",
                                "target_lang_sense_clarification": "неизбежная, часто трагическая судьба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ee4f16c-34de-3acb-bbbb-039cb601d41e",
                    "sense_definition": "Wydarzenia, przebieg czyjegoś życia lub historia jakiejś grupy czy miejsca.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_fate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Książka opowiada o powojennych <w>losach</w> pewnej rodziny.",
                            "target_lang_translations": {
                                "ru": "Книга рассказывает о послевоенной <w>судьбе</w> одной семьи."
                            }
                        },
                        {
                            "text": "Musimy zrobić wszystko, aby poprawić <w>los</w> najbiedniejszych ludzi.",
                            "target_lang_translations": {
                                "ru": "Мы должны сделать всё, чтобы улучшить <w>участь</w> самых бедных людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "dola"
                    ],
                    "common_phrases": [
                        "losy kraju",
                        "dzielić los",
                        "poprawić swój los"
                    ],
                    "target_lang_definitions": {
                        "ru": "События, ход чьей-либо жизни или история какой-либо группы или места."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "судьба",
                                "target_lang_sense_clarification": "жизненный путь, история"
                            },
                            {
                                "target_lang_word": "участь",
                                "target_lang_sense_clarification": "доля, положение (обычно тяжелое)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "432af8fb-770a-3a5f-80de-d6fe88a5cab5",
                    "sense_definition": "Przypadek lub zbieg okoliczności, na który nie mamy wpływu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_fate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej trudnej sytuacji jesteśmy zdani wyłącznie na <w>los</w>.",
                            "target_lang_translations": {
                                "ru": "В этой трудной ситуации мы полагаемся исключительно на <w>судьбу</w>."
                            }
                        },
                        {
                            "text": "O twoim sukcesie zadecydował <w>los</w>, a nie twoje umiejętności.",
                            "target_lang_translations": {
                                "ru": "Твой успех определил <w>случай</w>, а не твои навыки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zdany na los",
                        "gra losu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Случай или стечение обстоятельств, на которые мы не можем повлиять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "случай",
                                "target_lang_sense_clarification": "стечение обстоятельств"
                            },
                            {
                                "target_lang_word": "жребий",
                                "target_lang_sense_clarification": "случайный выбор"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
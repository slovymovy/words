    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b9a0939a-f58f-3721-92e3-406cb6b1c470",
                    "sense_definition": "Przerwanie ruchu, działania lub procesu; sprawienie, że ktoś lub coś przestaje się poruszać.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "stopping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nagłe <w>zatrzymanie</w> pociągu przestraszyło wszystkich pasażerów.",
                            "target_lang_translations": {
                                "ru": "Внезапная <w>остановка</w> поезда напугала всех пассажиров."
                            }
                        },
                        {
                            "text": "Lekarze robili wszystko, aby nie doszło do <w>zatrzymania</w> akcji serca.",
                            "target_lang_translations": {
                                "ru": "Врачи делали всё, чтобы не допустить <w>остановки</w> сердца."
                            }
                        }
                    ],
                    "synonyms": [
                        "postój",
                        "wstrzymanie",
                        "stop",
                        "pauza"
                    ],
                    "antonyms": [
                        "uruchomienie",
                        "start",
                        "ruszenie"
                    ],
                    "common_phrases": [
                        "nagłe zatrzymanie",
                        "zatrzymanie krążenia",
                        "całkowite zatrzymanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прекращение движения, действия или процесса; принуждение кого-то или чего-то остановиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остановка",
                                "target_lang_sense_clarification": "прекращение движения или работы"
                            },
                            {
                                "target_lang_word": "прекращение",
                                "target_lang_sense_clarification": "окончание действия или процесса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4dc4c9ba-8f7b-38c9-b8fd-53bf5469c13e",
                    "sense_definition": "Pozbawienie kogoś wolności przez policję lub inne służby w związku z podejrzeniem popełnienia przestępstwa.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "arrest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja potwierdziła wczorajsze <w>zatrzymanie</w> podejrzanego o kradzież.",
                            "target_lang_translations": {
                                "ru": "Полиция подтвердила вчерашнее <w>задержание</w> подозреваемого в краже."
                            }
                        },
                        {
                            "text": "Sąd wydał nakaz <w>zatrzymania</w> mężczyzny, który ukrywał się za granicą.",
                            "target_lang_translations": {
                                "ru": "Суд выдал ордер на <w>задержание</w> мужчины, который скрывался за границей."
                            }
                        }
                    ],
                    "synonyms": [
                        "aresztowanie",
                        "ujęcie",
                        "areszt"
                    ],
                    "antonyms": [
                        "zwolnienie",
                        "wypuszczenie"
                    ],
                    "common_phrases": [
                        "nakaz zatrzymania",
                        "dokonać zatrzymania",
                        "protokół zatrzymania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишение свободы полицией или другими службами в связи с подозрением в совершении преступления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задержание",
                                "target_lang_sense_clarification": "временное лишение свободы подозреваемого"
                            },
                            {
                                "target_lang_word": "арест",
                                "target_lang_sense_clarification": "взятие под стражу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f764d40-90c5-36db-9090-7542f637eff6",
                    "sense_definition": "Zachowanie czegoś dla siebie, nieoddanie czegoś właścicielowi lub niewypłacenie części pieniędzy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "retaining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Właściciel mieszkania zdecydował o <w>zatrzymaniu</w> kaucji na pokrycie szkód.",
                            "target_lang_translations": {
                                "ru": "Владелец квартиры принял решение об <w>удержании</w> залога для покрытия ущерба."
                            }
                        },
                        {
                            "text": "Policjant ma prawo do <w>zatrzymania</w> dowodu rejestracyjnego, jeśli pojazd jest niesprawny.",
                            "target_lang_translations": {
                                "ru": "Полицейский имеет право на <w>изъятие</w> свидетельства о регистрации, если транспортное средство неисправно."
                            }
                        }
                    ],
                    "synonyms": [
                        "zachowanie",
                        "przytrzymanie"
                    ],
                    "antonyms": [
                        "zwrot",
                        "oddanie"
                    ],
                    "common_phrases": [
                        "zatrzymanie kaucji",
                        "zatrzymanie dowodu rejestracyjnego",
                        "zatrzymanie prawa jazdy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сохранение чего-либо у себя, невозвращение владельцу или невыплата части денег."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удержание",
                                "target_lang_sense_clarification": "невыплата денег или сохранение имущества"
                            },
                            {
                                "target_lang_word": "изъятие",
                                "target_lang_sense_clarification": "официальное изъятие документов"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                },
                {
                    "sense_id": "3232a67c-cedb-3953-bbea-14f562e6cccc",
                    "sense_definition": "A rhythmic phrase or slogan shouted by a group, especially in support of a sports team.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_cheer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cheerleaders taught the crowd a new <w>yell</w> before the game started.",
                            "target_lang_translations": {
                                "ru": "Чирлидеры научили толпу новой <w>кричалке</w> перед началом игры."
                            }
                        },
                        {
                            "text": "Every student knew the school <w>yell</w> by heart and shouted it proudly.",
                            "target_lang_translations": {
                                "ru": "Каждый ученик знал школьную <w>кричалку</w> наизусть и гордо выкрикивал её."
                            }
                        }
                    ],
                    "common_phrases": [
                        "college yell",
                        "rebel yell"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ритмичная фраза или лозунг, выкрикиваемый группой, особенно в поддержку спортивной команды."
                    },
                    "translations": {

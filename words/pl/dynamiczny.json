    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9abb73ad-df64-346d-8ea5-f981287b2b73",
                    "sense_definition": "Pełen energii, witalności i ciągłego ruchu lub charakteryzujący się szybkimi zmianami i rozwojem.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "cecha_aktywności",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Firma zanotowała bardzo <w>dynamiczny</w> wzrost sprzedaży w ostatnim kwartale roku.",
                            "target_lang_translations": {
                                "ru": "Компания зафиксировала очень <w>динамичный</w> рост продаж в последнем квартале года."
                            }
                        },
                        {
                            "text": "Ten film ma niezwykle <w>dynamiczną</w> akcję, która trzyma widza w napięciu do samego końca.",
                            "target_lang_translations": {
                                "ru": "У этого фильма чрезвычайно <w>динамичное</w> действие, которое держит зрителя в напряжении до самого конца."
                            }
                        }
                    ],
                    "synonyms": [
                        "ruchliwy",
                        "żywiołowy",
                        "prężny",
                        "zmienny"
                    ],
                    "antonyms": [
                        "statyczny",
                        "bierny",
                        "monotonny"
                    ],
                    "common_phrases": [
                        "dynamiczny rozwój",
                        "dynamiczny wzrost",
                        "dynamiczna akcja",
                        "człowiek dynamiczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полный энергии, жизненных сил и постоянного движения или характеризующийся быстрыми изменениями и развитием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "динамичный",
                                "target_lang_sense_clarification": "pełen energii, żywiołowy"
                            },
                            {
                                "target_lang_word": "динамический",
                                "target_lang_sense_clarification": "dotyczący dynamiki zmian, rozwojowy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b75e488c-55b4-3e89-8a23-758d33e14959",
                    "sense_definition": "Dotyczący sił powodujących ruch lub działu fizyki zajmującego się badaniem tych sił.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fizyka",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Inżynierowie musieli dokładnie obliczyć <w>dynamiczne</w> obciążenie mostu podczas silnego wiatru.",
                            "target_lang_translations": {
                                "ru": "Инженерам пришлось точно рассчитать <w>динамическую</w> нагрузку на мост при сильном ветре."
                            }
                        },
                        {
                            "text": "W fizyce opis <w>dynamiczny</w> zjawiska uwzględnia przyczyny ruchu, czyli działające siły.",
                            "target_lang_translations": {
                                "ru": "В физике <w>динамическое</w> описание явления учитывает причины движения, то есть действующие силы."
                            }
                        }
                    ],
                    "antonyms": [
                        "statyczny"
                    ],
                    "common_phrases": [
                        "układ dynamiczny",
                        "obciążenie dynamiczne",
                        "opór dynamiczny"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "fizyka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к силам, вызывающим движение, или к разделу физики, изучающему эти силы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "динамический",
                                "target_lang_sense_clarification": "w kontekście fizyki"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04153db4-2401-3249-8755-bfb8a8cfeb0e",
                    "sense_definition": "Odnoszący się do siły natężenia dźwięku lub zakresu głośności w utworze muzycznym lub sprzęcie audio.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "muzyka",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten utwór jest trudny do wykonania ze względu na liczne i gwałtowne zmiany <w>dynamiczne</w>.",
                            "target_lang_translations": {
                                "ru": "Это произведение сложно для исполнения из-за многочисленных и резких <w>динамических</w> изменений."
                            }
                        },
                        {
                            "text": "Mikrofon <w>dynamiczny</w> jest często używany na scenie ze względu na swoją dużą wytrzymałość.",
                            "target_lang_translations": {
                                "ru": "<w>Динамический</w> микрофон часто используется на сцене из-за своей высокой прочности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zakres dynamiczny",
                        "mikrofon dynamiczny",
                        "głośnik dynamiczny"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "muzyka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к силе звука или диапазону громкости в музыкальном произведении или аудиооборудовании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "динамический",
                                "target_lang_sense_clarification": "w kontekście muzyki i sprzętu audio"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b31f3d42-9c52-3b43-b48d-c767ec8a320a",
                    "sense_definition": "Становиться равным, одинаковым по размеру, величине, количеству или объёму.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Давление внутри и снаружи сосуда постепенно <w>уравнивается</w>.",
                            "target_lang_translations": {
                                "en": "The pressure inside and outside the vessel gradually <w>equalizes</w>."
                            }
                        },
                        {
                            "text": "К концу матча счёт на табло наконец <w>уравнивается</w>.",
                            "target_lang_translations": {
                                "en": "By the end of the match, the score on the board finally <w>levels</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "сравниваться",
                        "выравниваться"
                    ],
                    "antonyms": [
                        "различаться"
                    ],
                    "common_phrases": [
                        "счёт уравнивается",
                        "силы уравниваются",
                        "температура уравнивается"
                    ],
                    "target_lang_definitions": {
                        "en": "To become equal in size, magnitude, quantity, or volume."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "equalize",
                                "target_lang_sense_clarification": "to become equal in value or amount"
                            },
                            {
                                "target_lang_word": "level out",
                                "target_lang_sense_clarification": "to become steady or equal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b18011ff-7b2f-3253-a7ec-3f6106bb4609",
                    "sense_definition": "Становиться равным с кем-либо в правах, общественном положении или возможностях.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В современном обществе мужчины и женщины <w>уравниваются</w> в правах.",
                            "target_lang_translations": {
                                "en": "In modern society, men and women <w>become equal</w> in rights."
                            }
                        },
                        {
                            "text": "Благодаря новым правилам шансы всех участников <w>уравниваются</w>.",
                            "target_lang_translations": {
                                "en": "Thanks to the new rules, the chances of all participants <w>are equalized</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "сравниваться"
                    ],
                    "common_phrases": [
                        "уравниваться в правах",
                        "шансы уравниваются"
                    ],
                    "target_lang_definitions": {
                        "en": "To become equal with someone in rights, social status, or opportunities."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "become equal",
                                "target_lang_sense_clarification": "regarding status or rights"
                            },
                            {
                                "target_lang_word": "be placed on an equal footing",
                                "target_lang_sense_clarification": "idiomatic"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25636ac7-3636-3701-b984-16795ae648ac",
                    "sense_definition": "Страдательный залог к глаголу <w>уравнивать</w>; подвергаться действию по созданию равенства или гладкости.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Поверхность дороги тщательно <w>уравнивается</w> перед укладкой асфальта.",
                            "target_lang_translations": {
                                "en": "The road surface <w>is leveled</w> thoroughly before laying asphalt."
                            }
                        },
                        {
                            "text": "Этот недостаток <w>уравнивается</w> другими важными достоинствами проекта.",
                            "target_lang_translations": {
                                "en": "This drawback <w>is balanced</w> by other important merits of the project."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "passive voice"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Passive voice of 'уравнивать'; to be made equal or smooth."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be leveled",
                                "target_lang_sense_clarification": "made flat or smooth"
                            },
                            {
                                "target_lang_word": "be balanced",
                                "target_lang_sense_clarification": "compensated for"
                            },
                            {
                                "target_lang_word": "be equalized",
                                "target_lang_sense_clarification": "made equal by an external agent"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
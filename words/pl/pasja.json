{
    "word_family": [
        "pasjonat",
        "pasjonować"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "86e1ef68-1daa-3ceb-bd2c-939ea93326a0",
                    "sense_definition": "Silne, wielkie zamiłowanie do czegoś; zajęcie lub przedmiot zainteresowań, któremu poświęca się dużo czasu i uwagi.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "hobby_interest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego największą <w>pasją</w> jest sklejanie modeli samolotów.",
                            "target_lang_translations": {
                                "ru": "Его самой большой <w>страстью</w> является склеивание моделей самолетов."
                            }
                        },
                        {
                            "text": "Ona zawsze opowiada o swojej pracy z wielką <w>pasją</w>.",
                            "target_lang_translations": {
                                "ru": "Она всегда рассказывает о своей работе с большой <w>страстью</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "hobby",
                        "konik",
                        "zainteresowanie",
                        "zajawka",
                        "zamiłowanie"
                    ],
                    "common_phrases": [
                        "rozwijać pasję",
                        "człowiek z pasją",
                        "robić coś z pasją",
                        "wielka pasja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильная любовь к чему-либо; занятие или предмет интереса, которому уделяется много времени и внимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страсть",
                                "target_lang_sense_clarification": "сильное увлечение"
                            },
                            {
                                "target_lang_word": "увлечение",
                                "target_lang_sense_clarification": "хобби, интерес"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f7b6baa-893d-3bf4-b974-621d1c639940",
                    "sense_definition": "Stan silnego wzburzenia, gwałtowny wybuch gniewu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "anger_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To ciągłe czekanie doprowadza mnie do szewskiej <w>pasji</w>.",
                            "target_lang_translations": {
                                "ru": "Это постоянное ожидание доводит меня до <w>бешенства</w>."
                            }
                        },
                        {
                            "text": "Kiedy zobaczył bałagan w pokoju, wpadł w <w>pasję</w>.",
                            "target_lang_translations": {
                                "ru": "Когда он увидел беспорядок в комнате, он пришел в <w>ярость</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gniew",
                        "złość",
                        "furia"
                    ],
                    "antonyms": [
                        "spokój",
                        "opanowanie"
                    ],
                    "common_phrases": [
                        "szewska pasja",
                        "wpaść w pasję",
                        "doprowadzić do pasji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние сильного возбуждения, внезапная вспышка гнева."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бешенство",
                                "target_lang_sense_clarification": "сильный гнев"
                            },
                            {
                                "target_lang_word": "ярость",
                                "target_lang_sense_clarification": "неконтролируемый гнев"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dfcafe73-a18d-3824-8f8c-29668fff803a",
                    "sense_definition": "Męka i cierpienie Jezusa Chrystusa (w kontekście religijnym).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "religious_passion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W okresie Wielkiego Postu wierni rozważają <w>pasję</w> Chrystusa.",
                            "target_lang_translations": {
                                "ru": "Во время Великого поста верующие размышляют о <w>Страстях</w> Христовых."
                            }
                        },
                        {
                            "text": "Obrazy w tym kościele przedstawiają sceny z <w>pasji</w>.",
                            "target_lang_translations": {
                                "ru": "Картины в этой церкви изображают сцены из <w>Страстей</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "męka"
                    ],
                    "common_phrases": [
                        "Pasja Chrystusa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мучения и страдания Иисуса Христа (в религиозном контексте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Страсти",
                                "target_lang_sense_clarification": "страдания Христа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11d13067-bfd9-3121-8938-85980fa6bd50",
                    "sense_definition": "Utwór muzyczny, literacki lub teatralny przedstawiający mękę Jezusa Chrystusa.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "religious_passion",
                    "name_type": "work_of_art_name",
                    "examples": [
                        {
                            "text": "W filharmonii wykonano słynną <w>pasję</w> Jana Sebastiana Bacha.",
                            "target_lang_translations": {
                                "ru": "В филармонии исполнили знаменитые <w>Страсти</w> Иоганна Себастьяна Баха."
                            }
                        },
                        {
                            "text": "To była najpiękniejsza <w>pasja</w>, jaką kiedykolwiek widziałem w teatrze.",
                            "target_lang_translations": {
                                "ru": "Это была самая прекрасная <w>постановка Страстей</w>, которую я когда-либо видел в театре."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pasja według świętego Mateusza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкальное, литературное или театральное произведение, изображающее мучения Христа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Страсти",
                                "target_lang_sense_clarification": "жанр произведения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "anatomy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшое отверстие в анатомической структуре, например, в среднем ухе или сердце."

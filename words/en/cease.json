                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие остановки или завершения чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прекращение",
                                "target_lang_sense_clarification": "остановка процесса или действия"
                            },
                            {
                                "target_lang_word": "остановка",
                                "target_lang_sense_clarification": "перерыв, пауза"
                            }

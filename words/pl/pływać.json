    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8b0385eb-6ec2-3757-a14d-64af743c71dc",
                    "sense_definition": "Poruszać się w wodzie dzięki ruchom ciała (o ludziach i zwierzętach).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion_in_water",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Latem bardzo lubię <w>pływać</w> w ciepłym morzu.",
                            "target_lang_translations": {
                                "ru": "Летом я очень люблю <w>плавать</w> в тёплом море."
                            }
                        },
                        {
                            "text": "Mój młodszy brat uczy się <w>pływać</w> żabką na basenie.",
                            "target_lang_translations": {
                                "ru": "Мой младший брат учится <w>плавать</w> брассом в бассейне."
                            }
                        }
                    ],
                    "antonyms": [
                        "tonąć"
                    ],
                    "common_phrases": [
                        "pływać żabką",
                        "pływać kraulem",
                        "pływać na plecach",
                        "umieć pływać",
                        "pływać pod wodą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передвигаться в воде с помощью движений тела (о людях и животных)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плавать",
                                "target_lang_sense_clarification": "передвигаться в воде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06639648-47cb-314d-9d04-283089422f0b",
                    "sense_definition": "Przemieszczać się po wodzie środkiem transportu (o statkach) lub podróżować nim (o ludziach).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "motion_in_water",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten duży prom pasażerski <w>pływa</w> do Szwecji dwa razy w tygodniu.",
                            "target_lang_translations": {
                                "ru": "Этот большой пассажирский паром <w>ходит</w> в Швецию два раза в неделю."
                            }
                        },
                        {
                            "text": "Mój dziadek był marynarzem i <w>pływał</w> na statkach handlowych.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка был моряком и <w>плавал</w> на торговых судах."
                            }
                        }
                    ],
                    "synonyms": [
                        "kursować",
                        "żeglować"
                    ],
                    "common_phrases": [
                        "pływać statkiem",
                        "pływać promem",
                        "pływać po morzu",
                        "pływać łódką"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаться по воде на транспортном средстве (о судах) или путешествовать на нём (о людях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ходить",
                                "target_lang_sense_clarification": "о судах (курсировать)"
                            },
                            {
                                "target_lang_word": "плавать",
                                "target_lang_sense_clarification": "о моряках и путешествиях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0d58a2d-3318-3614-ac11-3174bd42e61b",
                    "sense_definition": "Utrzymywać się na powierzchni cieczy i nie tonąć.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "state_in_liquid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekki korek <w>pływa</w> w wodzie, a ciężki kamień od razu tonie.",
                            "target_lang_translations": {
                                "ru": "Лёгкая пробка <w>плавает</w> в воде, а тяжёлый камень сразу тонет."
                            }
                        },
                        {
                            "text": "Zauważyłem, że w mojej zupie <w>pływa</w> kawałek marchewki.",
                            "target_lang_translations": {
                                "ru": "Я заметил, что в моем супе <w>плавает</w> кусочек морковки."
                            }
                        }
                    ],
                    "synonyms": [
                        "unosić się"
                    ],
                    "antonyms": [
                        "tonąć"
                    ],
                    "common_phrases": [
                        "pływać na powierzchni",
                        "pływać w wodzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Держаться на поверхности жидкости и не тонуть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плавать",
                                "target_lang_sense_clarification": "держаться на поверхности"
                            }
                        ]
                    }
                }
            ]
        }
    ]

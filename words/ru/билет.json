    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "010a25ca-ecf9-3462-838a-f7fa22a5f4f6",
                    "sense_definition": "Документ, дающий право пользоваться транспортом или посещать мероприятия (театр, кино, музей и т.д.)",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "docs_rights",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы заранее купили <билет> на поезд, чтобы поехать к морю.",
                            "target_lang_translations": {
                                "en": "We bought a train <ticket> in advance to go to the sea."
                            }
                        },
                        {
                            "text": "Контролёр в автобусе попросил пассажира предъявить <билет>.",
                            "target_lang_translations": {
                                "en": "The conductor on the bus asked the passenger to show their <ticket>."
                            }
                        },
                        {
                            "text": "Входной <билет> в этот музей стоит совсем недорого.",
                            "target_lang_translations": {
                                "en": "The entrance <ticket> to this museum is quite cheap."
                            }
                        }
                    ],
                    "synonyms": [
                        "проездной",
                        "талон",
                        "контрамарка",
                        "абонемент"
                    ],
                    "common_phrases": [
                        "купить билет",
                        "входной билет",
                        "билет на самолёт",
                        "обратный билет",
                        "лотерейный билет"
                    ],
                    "target_lang_definitions": {
                        "en": "A document giving the right to use transport or attend events (theater, cinema, museum, etc.)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ticket",
                                "target_lang_sense_clarification": "transport or event"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96361822-8d05-3432-8457-e807734f3f34",
                    "sense_definition": "Документ, подтверждающий личность и принадлежность человека к какой-либо организации",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "docs_rights",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студенческий <билет> позволяет получать скидки на проезд.",
                            "target_lang_translations": {
                                "en": "A student <card> allows you to get discounts on travel."
                            }
                        },
                        {
                            "text": "Охранник проверил мой пропуск и попросил показать военный <билет>.",
                            "target_lang_translations": {
                                "en": "The guard checked my pass and asked to see my military <ID>."
                            }
                        }
                    ],
                    "synonyms": [
                        "удостоверение",
                        "пропуск",
                        "корочка"
                    ],
                    "common_phrases": [
                        "студенческий билет",
                        "военный билет",
                        "читательский билет",
                        "партийный билет",
                        "белый билет"
                    ],
                    "target_lang_definitions": {
                        "en": "A document confirming a person's identity and membership in an organization"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "card",
                                "target_lang_sense_clarification": "membership or identity"
                            },
                            {
                                "target_lang_word": "ID",
                                "target_lang_sense_clarification": "identity document"
                            },
                            {
                                "target_lang_word": "pass",
                                "target_lang_sense_clarification": "access document"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0df69f10-ea79-3dc6-a46c-6bebbb6ba67f",
                    "sense_definition": "Листок с вопросами или заданиями, который студент выбирает случайным образом на экзамене",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студент очень волновался, когда подходил к столу, чтобы вытянуть <билет>.",
                            "target_lang_translations": {
                                "en": "The student was very nervous when approaching the desk to draw a <question card>."
                            }
                        },
                        {
                            "text": "Мне попался лёгкий экзаменационный <билет>, и я получил отличную оценку.",
                            "target_lang_translations": {
                                "en": "I got an easy <exam question>, and I received an excellent grade."
                            }
                        }
                    ],
                    "common_phrases": [
                        "экзаменационный билет",
                        "вытянуть билет",
                        "счастливый билет",
                        "учить билеты"
                    ],
                    "target_lang_definitions": {
                        "en": "A slip of paper with questions or tasks that a student chooses randomly during an exam"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "question card",
                                "target_lang_sense_clarification": "exam"
                            },
                            {
                                "target_lang_word": "exam paper",
                                "target_lang_sense_clarification": "set of questions"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6dbd081-ffea-320e-8e12-532723ab1933",
                    "sense_definition": "Бумажный денежный знак",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На каждой российской купюре написано: «<Билет> Банка России».",
                            "target_lang_translations": {
                                "en": "On every Russian banknote it is written: \"Bank of Russia <Note>\"."
                            }
                        },
                        {
                            "text": "В старинной книге герой расплачивался кредитными <билетами>.",
                            "target_lang_translations": {
                                "en": "In the old book, the hero paid with credit <notes>."
                            }
                        }
                    ],
                    "synonyms": [
                        "банкнота",
                        "купюра",
                        "ассигнация"
                    ],
                    "antonyms": [
                        "монета"
                    ],
                    "common_phrases": [
                        "билет Банка России",
                        "кредитный билет",
                        "казначейский билет"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "в разговорной речи обычно используется слово «купюра» или «деньги»"
                        },
                        {
                            "trait_type": "form",
                            "comment": "используется в официальных названиях денежных знаков"
                        }
                    ],

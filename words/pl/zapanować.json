    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b6101bf8-716d-36d3-b43a-01d2e1204a58",
                    "sense_definition": "Zacząć istnieć, pojawić się i trwać (zwykle o ciszy, nastroju, zjawiskach atmosferycznych lub stanach społecznych).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "state_onset",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po burzliwej kłótni w całym domu nagle <zapanowała> głucha cisza.",
                            "target_lang_translations": {
                                "ru": "После бурной ссоры во всём доме внезапно <воцарилась> мёртвая тишина."
                            }
                        },
                        {
                            "text": "Wszyscy mają nadzieję, że wkrótce w naszym kraju <zapanuje> spokój i dobrobyt.",
                            "target_lang_translations": {
                                "ru": "Все надеются, что вскоре в нашей стране <наступит> спокойствие и процветание."
                            }
                        }
                    ],
                    "synonyms": [
                        "nastać"
                    ],
                    "common_phrases": [
                        "zapanowała cisza",
                        "zapanował spokój",
                        "zapanowała ciemność",
                        "zapanował chaos"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать существовать, появиться и длиться (обычно о тишине, настроении, атмосферных явлениях или общественных состояниях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воцариться",
                                "target_lang_sense_clarification": "о тишине, покое, хаосе"
                            },
                            {
                                "target_lang_word": "наступить",
                                "target_lang_sense_clarification": "о состоянии, времени"
                            },
                            {
                                "target_lang_word": "установиться",
                                "target_lang_sense_clarification": "о погоде, порядке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea2d2773-7cac-3d58-ad64-c69eacae2e3b",
                    "sense_definition": "Zdobyć kontrolę nad czymś lub kimś; zdołać powstrzymać gwałtowne emocje, reakcje lub zjawiska.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <zapanować> nad swoimi nerwami, zanim wejdziesz do gabinetu szefa.",
                            "target_lang_translations": {
                                "ru": "Ты должен <совладать> со своими нервами, прежде чем войдёшь в кабинет начальника."
                            }
                        },
                        {
                            "text": "Doświadczonemu kierowcy udało się <zapanować> nad pojazdem mimo bardzo śliskiej nawierzchni.",
                            "target_lang_translations": {
                                "ru": "Опытному водителю удалось <справиться> с управлением автомобилем, несмотря на очень скользкую дорогу."
                            }
                        }
                    ],
                    "synonyms": [
                        "opanować"
                    ],
                    "common_phrases": [
                        "zapanować nad emocjami",
                        "zapanować nad sytuacją",
                        "zapanować nad strachem",
                        "zapanować nad sobą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взять под контроль что-либо или кого-либо; суметь сдержать сильные эмоции, реакции или явления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совладать",
                                "target_lang_sense_clarification": "с эмоциями, с собой"
                            },
                            {
                                "target_lang_word": "справиться",
                                "target_lang_sense_clarification": "с управлением, с ситуацией"
                            },
                            {
                                "target_lang_word": "взять под контроль",
                                "target_lang_sense_clarification": "общий смысл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18fb8b2f-cbfa-3ad8-87e2-13e8d59576bf",
                    "sense_definition": "Rozpocząć rządy na danym terytorium; objąć władzę królewską lub przywódczą.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "rule",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po śmierci ojca młody książę <zapanował> w królestwie, wprowadzając wiele nowych praw.",
                            "target_lang_translations": {
                                "ru": "После смерти отца молодой принц <воцарился> в королевстве, введя множество новых законов."
                            }
                        },
                        {
                            "text": "Historycy nie są zgodni co do roku, w którym ten władca <zapanował> nad imperium.",
                            "target_lang_translations": {
                                "ru": "Историки не сходятся во мнении относительно года, в котором этот правитель <начал править> империей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zapanować na tronie",
                        "zapanować nad królestwem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начать правление на определенной территории; вступить на престол."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воцариться",
                                "target_lang_sense_clarification": "на троне"
                            },
                            {
                                "target_lang_word": "начать править",
                                "target_lang_sense_clarification": "страной, народом"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "41695092-ad61-328c-9757-141de5875ece",
                    "sense_definition": "Metalowa konstrukcja na podwórku składająca się z rur, służąca do wieszania i czyszczenia dywanów, często wykorzystywana przez dzieci do zabawy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cleaning_equipment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Anka jest na podwórku, robi fikołki na <w>trzepaku</w>.",
                            "target_lang_translations": {
                                "ru": "Анка во дворе, кувыркается на <w>выбивалке</w>."
                            }
                        },
                        {
                            "text": "Muszę wyjść przed blok, żeby wyczyścić stary dywan na <w>trzepaku</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно выйти во двор, чтобы почистить старый ковер на <w>выбивалке</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "reg. śl. klopsztanga",
                        "reg. pozn. sztender"
                    ],
                    "common_phrases": [
                        "wisieć na trzepaku",
                        "trzepać dywan na trzepaku",
                        "spotkać się pod trzepakiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Металлическая конструкция во дворе, состоящая из труб, предназначенная для выбивания ковров, часто используемая детьми для игр."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбивалка",
                                "target_lang_sense_clarification": "конструкция во дворе"
                            },
                            {
                                "target_lang_word": "стойка для выбивания ковров",
                                "target_lang_sense_clarification": "официальное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6a8cbd1-f0b9-3b07-b398-350165d06ba5",
                    "sense_definition": "Potoczne określenie osoby roztrzepanej, lekkomyślnej, zakręconej lub mówiącej bardzo szybko i chaotycznie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_trait",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat to straszny <w>trzepak</w>, znowu zapomniał, gdzie położył klucze.",
                            "target_lang_translations": {
                                "ru": "Мой брат — ужасный <w>растяпа</w>, он снова забыл, куда положил ключи."
                            }
                        },
                        {
                            "text": "Nie bądź taki <w>trzepak</w> i skup się wreszcie na tym, co do ciebie mówię.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>разиней</w> и сосредоточься наконец на том, что я тебе говорю."
                            }
                        }
                    ],
                    "synonyms": [
                        "reg. pozn. sztender"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "28c30b76-b128-3a85-9672-a70a7dc39be8",
                    "sense_definition": "Dać komuś prezent lub ofiarować coś wartościowego, materialnego lub duchowego.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "giving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Święty Mikołaj postanowił hojnie <w>obdarować</w> wszystkie grzeczne dzieci.",
                            "target_lang_translations": {
                                "ru": "Санта-Клаус решил щедро <w>одарить</w> всех послушных детей."
                            }
                        },
                        {
                            "text": "Zwycięzca konkursu został <w>obdarowany</w> cennymi nagrodami rzeczowymi.",
                            "target_lang_translations": {
                                "ru": "Победитель конкурса был <w>награжден</w> ценными вещественными призами."
                            }
                        }
                    ],
                    "synonyms": [
                        "obdarzyć"
                    ],
                    "common_phrases": [
                        "hojnie obdarować",
                        "obdarować prezentami",
                        "zostać obdarowanym"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дать кому-то подарок или преподнести что-то ценное, материальное или духовное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одарить",
                                "target_lang_sense_clarification": "преподнести подарок"
                            },
                            {
                                "target_lang_word": "наградить",
                                "target_lang_sense_clarification": "вручить награду или приз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69394772-3dac-3b34-aa41-fd8919e03c8e",
                    "sense_definition": "Skierować do kogoś gest, spojrzenie lub uśmiech wyrażający pozytywne emocje.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "feelings",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziewczyna <w>obdarowała</w> go promiennym uśmiechem na powitanie.",
                            "target_lang_translations": {
                                "ru": "Девушка <w>одарила</w> его лучезарной улыбкой при встрече."
                            }
                        },
                        {
                            "text": "Starsza pani <w>obdarowała</w> mnie życzliwym spojrzeniem, gdy ustąpiłem jej miejsca.",
                            "target_lang_translations": {
                                "ru": "Пожилая дама <w>одарила</w> меня доброжелательным взглядом, когда я уступил ей место."
                            }
                        }
                    ],
                    "synonyms": [
                        "obdarzyć"
                    ],
                    "common_phrases": [
                        "obdarować uśmiechem",
                        "obdarować spojrzeniem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Адресовать кому-то жест, взгляд или улыбку, выражающие положительные эмоции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одарить",
                                "target_lang_sense_clarification": "взглядом или улыбкой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abb96068-c4af-3e32-bc0c-d90d995cb9db",
                    "sense_definition": "Wręczyć sobie nawzajem prezenty lub upominki (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "giving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W Wigilię wszyscy domownicy <w>obdarowali</w> się drobnymi upominkami.",
                            "target_lang_translations": {
                                "ru": "В сочельник все домочадцы <w>обменялись</w> небольшими подарками."
                            }
                        },
                        {
                            "text": "Przyjaciele <w>obdarowali</w> się pamiątkami przywiezionymi z wakacji.",
                            "target_lang_translations": {
                                "ru": "Друзья <w>обменялись</w> сувенирами, привезенными из отпуска."
                            }
                        }
                    ],
                    "common_phrases": [
                        "obdarować się prezentami",
                        "nawzajem się obdarować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вручить друг другу подарки или сувениры (взаимное действие)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обменяться",
                                "target_lang_sense_clarification": "подарками"
                            },
                            {
                                "target_lang_word": "одарить",
                                "target_lang_sense_clarification": "друг друга"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "156d2ecd-b220-3e4d-9f20-6f06859b8c0a",
                    "sense_definition": "Okazać sobie wzajemnie pozytywne uczucia lub gesty (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "feelings",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Małżonkowie <w>obdarowali</w> się czułymi słowami w rocznicę ślubu.",
                            "target_lang_translations": {
                                "ru": "Супруги <w>обменялись</w> нежными словами в годовщину свадьбы."
                            }
                        },
                        {
                            "text": "Sąsiedzi <w>obdarowali</w> się uśmiechami na znak zgody.",
                            "target_lang_translations": {
                                "ru": "Соседи <w>обменялись</w> улыбками в знак согласия."
                            }
                        }
                    ],
                    "common_phrases": [
                        "obdarować się uśmiechem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проявить по отношению друг к другу положительные чувства или жесты (взаимное действие)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обменяться",
                                "target_lang_sense_clarification": "улыбками или любезностями"
                            }
                        ]
                    }
                }
            ]
        }
    ]

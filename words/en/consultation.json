{
    "word_family": [
        "consult",
        "consultant",
        "consultancy",
        "consultative"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b078b078-c5a0-35ca-b696-20c1f1220d21",
                    "sense_definition": "An appointment or meeting with a professional person, such as a doctor or lawyer, to seek advice.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "meeting_types",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor charges a small fee for the initial <w>consultation</w>.",
                            "target_lang_translations": {
                                "ru": "Врач взимает небольшую плату за первичную <w>консультацию</w>."
                            }
                        },
                        {
                            "text": "She scheduled a private <w>consultation</w> with a financial advisor to discuss her investments.",
                            "target_lang_translations": {
                                "ru": "Она записалась на частную <w>консультацию</w> к финансовому советнику, чтобы обсудить свои инвестиции."
                            }
                        }
                    ],
                    "synonyms": [
                        "appointment",
                        "interview",
                        "session"
                    ],
                    "common_phrases": [
                        "medical consultation",
                        "initial consultation",
                        "private consultation",
                        "free consultation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прием или встреча со специалистом, например врачом или юристом, для получения совета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "консультация",
                                "target_lang_sense_clarification": "встреча со специалистом"
                            },
                            {
                                "target_lang_word": "приём",
                                "target_lang_sense_clarification": "посещение врача или специалиста"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14126e61-b8bc-3247-b4cd-21cb82145e34",
                    "sense_definition": "The act of formally discussing something with someone to get their advice or opinion.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "action_of_consulting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new policy was developed in close <w>consultation</w> with local business owners.",
                            "target_lang_translations": {
                                "ru": "Новая политика была разработана в ходе тесных <w>консультаций</w> с местными владельцами бизнеса."
                            }
                        },
                        {
                            "text": "After a short <w>consultation</w> between the referees, the player was sent off the field.",
                            "target_lang_translations": {
                                "ru": "После короткого <w>совещания</w> судей игрок был удален с поля."
                            }
                        }
                    ],
                    "synonyms": [
                        "discussion",
                        "deliberation",
                        "conferring"
                    ],
                    "common_phrases": [
                        "in consultation with",
                        "prior consultation",
                        "public consultation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс официального обсуждения чего-либо с кем-либо для получения совета или мнения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совещание",
                                "target_lang_sense_clarification": "обсуждение, обмен мнениями"
                            },
                            {
                                "target_lang_word": "консультация",
                                "target_lang_sense_clarification": "процесс получения совета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac682189-67bf-37ef-a2b1-0ccb98452f19",
                    "sense_definition": "A formal conference or meeting organized to exchange information and advice on a specific topic.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "meeting_types",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government launched a public <w>consultation</w> to gather views on the proposed law.",
                            "target_lang_translations": {
                                "ru": "Правительство начало общественное <w>обсуждение</w> для сбора мнений по предложенному закону."
                            }
                        },
                        {
                            "text": "He attended a <w>consultation</w> on environmental issues held at the city hall.",
                            "target_lang_translations": {
                                "ru": "Он посетил <w>консультацию</w> по экологическим вопросам, проходившую в мэрии."
                            }
                        }
                    ],
                    "synonyms": [
                        "conference",
                        "forum"
                    ],
                    "common_phrases": [
                        "consultation document",
                        "consultation period",
                        "consultation process"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальная конференция или собрание, организованное для обмена информацией и советами по конкретной теме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обсуждение",
                                "target_lang_sense_clarification": "публичные слушания"
                            },
                            {
                                "target_lang_word": "консультация",
                                "target_lang_sense_clarification": "официальная встреча"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "9ce0de94-a7f3-3d68-bf9e-05cb3aab210d",
                    "sense_definition": "The 42nd sura (chapter) of the Qur'an.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "document_or_program_name",
                    "examples": [
                        {
                            "text": "The scholar recited verses from the <w>Consultation</w> during the evening prayer.",
                            "target_lang_translations": {
                                "ru": "Ученый читал стихи из суры <w>Аш-Шура</w> во время вечерней молитвы."
                            }
                        },
                        {
                            "text": "This chapter is known as the <w>Consultation</w> because it emphasizes the importance of mutual advice.",
                            "target_lang_translations": {
                                "ru": "Эта глава известна как <w>Совет</w>, потому что она подчеркивает важность взаимного совета."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "42-я сура (глава) Корана."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Аш-Шура",
                                "target_lang_sense_clarification": "название суры"
                            },
                            {
                                "target_lang_word": "Совет",
                                "target_lang_sense_clarification": "перевод названия суры"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
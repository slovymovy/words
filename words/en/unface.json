    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "58d7c837-1c73-35c0-99d7-51162c1a2708",
                    "sense_definition": "To remove the face or covering from something; to unmask or expose.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "expose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The strong wind threatened to <w>unface</w> the old building by stripping away its cladding.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер грозил <w>лишить облицовки</w> старое здание, сорвав с него обшивку."
                            }
                        },
                        {
                            "text": "He sought to <w>unface</w> the deception and reveal the truth to everyone.",
                            "target_lang_translations": {
                                "ru": "Он стремился <w>разоблачить</w> обман и открыть всем правду."
                            }
                        }
                    ],
                    "synonyms": [
                        "unmask",
                        "expose",
                        "uncover",
                        "reveal"
                    ],
                    "antonyms": [
                        "mask",
                        "cover",
                        "conceal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снять внешнее покрытие или маску; разоблачить или выставить напоказ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обнажать",
                                "target_lang_sense_clarification": "снимать покров или облицовку"
                            },
                            {
                                "target_lang_word": "разоблачать",
                                "target_lang_sense_clarification": "раскрывать правду или обман"
                            },
                            {
                                "target_lang_word": "срывать маску",
                                "target_lang_sense_clarification": "метафорическое значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a32c30e9-02e1-373c-b5b6-056cd8234ca5",
                    "sense_definition": "To turn away so that one is no longer facing a specific direction or person.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "turn_away",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to <w>unface</w> the mirror to avoid looking at her reflection.",
                            "target_lang_translations": {
                                "ru": "Она решила <w>отвернуться</w> от зеркала, чтобы не видеть своего отражения."
                            }
                        },
                        {
                            "text": "The soldiers were ordered to <w>unface</w> the enemy and retreat slowly.",
                            "target_lang_translations": {
                                "ru": "Солдатам приказали <w>отвернуться</w> от врага и медленно отступать."
                            }
                        }
                    ],
                    "synonyms": [
                        "turn away",
                        "avert"
                    ],
                    "antonyms": [
                        "face",
                        "confront"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отвернуться, перестать стоять лицом к кому-либо или чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отворачиваться",
                                "target_lang_sense_clarification": "перестать смотреть"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "klezmerka",
        "klezmerski",
        "klezmerstwo"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "64477365-6b75-3321-9e28-f5a5c05c050a",
                    "sense_definition": "Żydowski muzyk ludowy, wykonujący tradycyjną muzykę instrumentalną związaną z kulturą Żydów aszkenazyjskich",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na krakowskim Kazimierzu stary <w>klezmer</w> grał na klarnecie smutną melodię.",
                            "target_lang_translations": {
                                "ru": "В краковском Казимеже старый <w>клезмер</w> играл на кларнете грустную мелодию."
                            }
                        },
                        {
                            "text": "Zespół klezmerów wystąpił na festiwalu kultury żydowskiej, zachwycając publiczność.",
                            "target_lang_translations": {
                                "ru": "Ансамбль <w>клезмеров</w> выступил на фестивале еврейской культуры, восхитив публику."
                            }
                        }
                    ],
                    "common_phrases": [
                        "muzyka klezmerska",
                        "zespół klezmerów"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Jewish culture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Еврейский народный музыкант, исполняющий традиционную инструментальную музыку, связанную с культурой ашкеназских евреев"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клезмер",
                                "target_lang_sense_clarification": "еврейский музыкант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b23199a-f653-3719-a8d8-4a3e4f9b9c4f",
                    "sense_definition": "Muzyk grający zarobkowo w lokalach gastronomicznych, na weselach lub imprezach okolicznościowych",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad jest <w>klezmerem</w> i w każdy weekend gra na weselach.",
                            "target_lang_translations": {
                                "ru": "Мой сосед — <w>лабух</w>, и каждые выходные он играет на свадьбах."
                            }
                        },
                        {
                            "text": "W tej restauracji do kotleta przygrywa miejscowy <w>klezmer</w>.",
                            "target_lang_translations": {
                                "ru": "В этом ресторане фоном играет местный <w>лабух</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "muzykant",
                        "chałturnik"
                    ],
                    "antonyms": [
                        "wirtuoz"
                    ],
                    "common_phrases": [
                        "los klezmera",
                        "grać jak klezmer"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used by musicians"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкант, играющий за деньги в ресторанах, на свадьбах или мероприятиях (часто используется как профессиональный жаргонизм)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лабух",
                                "target_lang_sense_clarification": "ресторанный музыкант (жаргон)"
                            },
                            {
                                "target_lang_word": "халтурщик",
                                "target_lang_sense_clarification": "пренебрежительно"
                            },
                            {
                                "target_lang_word": "ресторанный музыкант",
                                "target_lang_sense_clarification": "нейтрально"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
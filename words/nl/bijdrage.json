                {
                    "sense_id": "f651815a-4846-3986-a996-bb5a0c65b41e",
                    "sense_definition": "Een geldbedrag dat men betaalt om iets te steunen of als lidmaatschapsgeld.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "contribution_financial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De school vraagt een vrijwillige <w>bijdrage</w> van de ouders voor extra activiteiten.",
                            "target_lang_translations": {
                                "en": "The school asks for a voluntary <w>contribution</w> from the parents for extra activities.",
                                "ru": "Школа просит родителей о добровольном <w>взносе</w> на дополнительные мероприятия."
                            }
                        },
                        {
                            "text": "Zonder uw financiële <w>bijdrage</w> kunnen wij ons werk niet voortzetten.",
                            "target_lang_translations": {
                                "en": "Without your financial <w>contribution</w>, we cannot continue our work.",
                                "ru": "Без вашего финансового <w>вклада</w> мы не сможем продолжать нашу работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "contributie"
                    ],
                    "common_phrases": [

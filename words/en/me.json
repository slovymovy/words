{
    "word_family": [
        "myself"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "3bf809d5-94d6-32a9-b64e-aa376b4fdf01",
                    "sense_definition": "Used as the object of a verb or preposition to refer to the person speaking or writing.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "speaker_reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please tell <w>me</w> what time the train arrives at the station.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, скажите <w>мне</w>, во сколько поезд прибывает на станцию."
                            }
                        },
                        {
                            "text": "She gave <w>me</w> a beautiful gift for my birthday last week.",
                            "target_lang_translations": {
                                "ru": "Она подарила <w>мне</w> красивый подарок на мой день рождения на прошлой неделе."
                            }
                        },
                        {
                            "text": "Excuse <w>me</w>, could you help me find the library?",
                            "target_lang_translations": {
                                "ru": "Извините <w>меня</w>, не могли бы вы помочь мне найти библиотеку?"
                            }
                        }
                    ],
                    "synonyms": [
                        "us"
                    ],
                    "antonyms": [
                        "you"
                    ],
                    "common_phrases": [
                        "excuse me",
                        "tell me",
                        "let me",
                        "give me",
                        "help me"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется как дополнение глагола или предлога для обозначения говорящего или пишущего."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "меня",
                                "target_lang_sense_clarification": "винительный или родительный падеж"
                            },
                            {
                                "target_lang_word": "мне",
                                "target_lang_sense_clarification": "дательный падеж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71715a92-8d6c-3e00-8cae-37cde27ef94d",
                    "sense_definition": "Used to refer to the speaker after the verb 'to be', or standing alone as an answer.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "speaker_reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was <w>me</w> who left the door open yesterday morning.",
                            "target_lang_translations": {
                                "ru": "Это был <w>я</w>, кто оставил дверь открытой вчера утром."
                            }
                        },
                        {
                            "text": "Who wants to go for ice cream? — <w>me</w>!",
                            "target_lang_translations": {
                                "ru": "Кто хочет пойти за мороженым? — <w>Я</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "I"
                    ],
                    "common_phrases": [
                        "it is me",
                        "that is me",
                        "just me"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для обозначения говорящего после глагола 'to be' или отдельно в качестве ответа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "я",
                                "target_lang_sense_clarification": "именительный падеж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48bb9d51-a72d-3d3e-b140-25b9a6b3670e",
                    "sense_definition": "Used as the subject of a sentence, often combined with another name or pronoun.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "speaker_reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Me</w> and my brother are going to the cinema tonight.",
                            "target_lang_translations": {
                                "ru": "<w>Я</w> и мой брат идем сегодня в кино."
                            }
                        },
                        {
                            "text": "My friends and <w>me</w> love playing football in the park on weekends.",
                            "target_lang_translations": {
                                "ru": "Мои друзья и <w>я</w> обожаем играть в футбол в парке по выходным."
                            }
                        }
                    ],
                    "synonyms": [
                        "I"
                    ],
                    "common_phrases": [
                        "me and you",
                        "me and my friends"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Standard English requires 'I' in the subject position, but 'me' is very common in casual speech."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется как подлежащее предложения, часто в сочетании с другим именем или местоимением (разговорная речь)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "я",
                                "target_lang_sense_clarification": "в роли подлежащего"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "063a7b5a-8fe9-31dc-a7c4-7580debb2805",
                    "sense_definition": "Used for emphasis to show that the subject is doing something for their own benefit.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "speaker_reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am going to buy <w>me</w> a nice new hat for the party.",
                            "target_lang_translations": {
                                "ru": "Я собираюсь купить <w>себе</w> новую красивую шляпу для вечеринки."
                            }
                        },
                        {
                            "text": "He decided to get <w>me</w> some dinner before heading home.",
                            "target_lang_translations": {
                                "ru": "Он решил взять <w>мне</w> ужин перед тем, как отправиться домой."
                            }
                        }
                    ],
                    "synonyms": [
                        "us"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Often used in US regional dialects."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для усиления, чтобы показать, что действие совершается в интересах самого субъекта или говорящего."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "себе",
                                "target_lang_sense_clarification": "возвратное местоимение"
                            },
                            {
                                "target_lang_word": "мне",
                                "target_lang_sense_clarification": "дательный падеж"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "0a8ca34f-fd09-3940-b099-48350cb54bd0",
                    "sense_definition": "Alternative form of <w>my</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I seem to have lost <w>me</w> keys somewhere in the house.",
                            "target_lang_translations": {
                                "ru": "Кажется, я где-то в доме потерял <w>мои</w> ключи."
                            }
                        },
                        {
                            "text": "Give <w>me</w> bag back right now, or I will tell the teacher!",
                            "target_lang_translations": {
                                "ru": "Верни <w>мою</w> сумку сейчас же, или я расскажу учителю!"
                            }
                        }
                    ],
                    "synonyms": [
                        "my"
                    ],
                    "common_phrases": [
                        "me mum",
                        "me dad"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Common in British, Irish, and Australian English dialects."
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Альтернативная, диалектная форма притяжательного местоимения 'my' (мой)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мой",
                                "target_lang_sense_clarification": "притяжательное местоимение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6a7332ec-a4c4-3443-80a5-bc2357a6f1ef",
                    "sense_definition": "The personality or authentic identity of the speaker.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "identity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old <w>me</w> would have been angry, but now I am calm.",
                            "target_lang_translations": {
                                "ru": "Прежний <w>я</w> разозлился бы, но сейчас я спокоен."
                            }
                        },
                        {
                            "text": "I need some time alone to find the real <w>me</w> again.",
                            "target_lang_translations": {
                                "ru": "Мне нужно побыть одному, чтобы снова найти настоящего <w>себя</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "I"
                    ],
                    "common_phrases": [
                        "the real me",
                        "the old me",
                        "a new me"
                    ],
                    "target_lang_definitions": {
                        "ru": "Личность или подлинная сущность говорящего."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "я",
                                "target_lang_sense_clarification": "сущность, личность"
                            },
                            {
                                "target_lang_word": "себя",
                                "target_lang_sense_clarification": "в значении 'свою личность'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3865782-ad59-3de2-9f43-9500a61268d0",
                    "sense_definition": "Initialism of <w>medical examiner</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abbreviation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>ME</w> arrived at the scene to investigate the cause of death.",
                            "target_lang_translations": {
                                "ru": "<w>Судмедэксперт</w> прибыл на место происшествия, чтобы расследовать причину смерти."
                            }
                        },
                        {
                            "text": "The report from the <w>ME</w> will be ready by tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "Отчет от <w>судмедэксперта</w> будет готов к завтрашнему утру."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от 'medical examiner' (судмедэксперт)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "судмедэксперт",
                                "target_lang_sense_clarification": "профессия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "5eb91775-1ccf-372b-bfcb-0b112774dd2d",
                    "sense_definition": "Abbreviation of <w>Maine</w>: a state of the United States.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geography",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "The package was shipped from Portland, <w>ME</w>, yesterday afternoon.",
                            "target_lang_translations": {
                                "ru": "Посылка была отправлена из Портленда, штат <w>Мэн</w>, вчера днем."
                            }
                        },
                        {
                            "text": "We are planning a road trip to <w>ME</w> next summer to see the coast.",
                            "target_lang_translations": {
                                "ru": "Мы планируем поездку в <w>Мэн</w> следующим летом, чтобы увидеть побережье."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от 'Maine': штат США."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мэн",
                                "target_lang_sense_clarification": "штат США"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
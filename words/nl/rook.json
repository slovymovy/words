            "senses": [
                {
                    "sense_id": "48cd8d5c-6bae-3a4c-b577-7ea2ec4a61f4",
                    "sense_definition": "Een zichtbaar mengsel van gassen, dampen en kleine deeltjes dat vrijkomt wanneer iets verbrandt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "smoke_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er kwam dikke zwarte <w>rook</w> uit de schoorsteen van de fabriek.",
                            "target_lang_translations": {
                                "ru": "Из заводской трубы валил густой черный <w>дым</w>."
                            }
                        },
                        {
                            "text": "Door de dichte <w>rook</w> konden de brandweermannen bijna niets zien.",
                            "target_lang_translations": {
                                "ru": "Из-за густого <w>дыма</w> пожарные почти ничего не видели."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in rook opgaan",
                        "waar rook is",
                        "zwarte rook"
                    ],
                    "target_lang_definitions": {
                        "ru": "Видимая смесь газов, паров и мелких частиц, выделяющаяся при горении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дым",
                                "target_lang_sense_clarification": "продукт горения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a60f6ad-3c23-3d93-9e52-ca5a4492f37c",
                    "sense_definition": "Een kegelvormige stapel hooi of gemaaid gewas die op het land staat te drogen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "haystack",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer bouwde een grote <w>rook</w> om het hooi te beschermen tegen de regen.",
                            "target_lang_translations": {
                                "ru": "Фермер сложил большой <w>стог</w>, чтобы защитить сено от дождя."
                            }
                        },
                        {
                            "text": "Op het veld stond hier en daar nog een eenzame <w>rook</w>.",
                            "target_lang_translations": {
                                "ru": "На поле тут и там стоял одинокий <w>стог</w>."
                            }
                        }
                    ],
                    "synonyms": [

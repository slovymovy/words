    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b9e3e906-d586-3dd7-83d9-cb74a9876c49",
                    "sense_definition": "The oval object laid by a domestic fowl, such as a chicken, used as food.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_egg",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I usually eat a boiled <w>egg</w> for breakfast.",
                            "target_lang_translations": {
                                "ru": "Обычно я ем на завтрак варёное <w>яйцо</w>."
                            }
                        },
                        {
                            "text": "She needs to buy a carton of fresh <w>eggs</w> from the market.",
                            "target_lang_translations": {
                                "ru": "Ей нужно купить на рынке упаковку свежих <w>яиц</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fried egg",
                        "scrambled eggs",
                        "boiled egg",
                        "egg yolk",
                        "egg white"
                    ],
                    "target_lang_definitions": {
                        "ru": "Овальный предмет, откладываемый домашней птицей, например курицей, употребляемый в пищу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "яйцо",
                                "target_lang_sense_clarification": "пища"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5774f65-71a8-3ed8-921b-43c239d9e935",
                    "sense_definition": "An oval body produced by birds, insects, or reptiles that contains a developing embryo.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_egg",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The robin laid a small blue <w>egg</w> in the nest.",
                            "target_lang_translations": {
                                "ru": "Малиновка отложила в гнезде маленькое голубое <w>яйцо</w>."
                            }
                        },
                        {
                            "text": "We watched the snake <w>egg</w> hatch in the warm sand.",
                            "target_lang_translations": {
                                "ru": "Мы наблюдали, как в тёплом песке из змеиного <w>яйца</w> вылупляется детёныш."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lay an egg",
                        "hatch from an egg"
                    ],
                    "target_lang_definitions": {
                        "ru": "Овальное тело, производимое птицами, насекомыми или рептилиями, содержащее развивающийся эмбрион."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "яйцо",
                                "target_lang_sense_clarification": "биология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28001116-8050-37ff-9184-40f46a9de10a",
                    "sense_definition": "An object that has the oval shape of an egg.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_egg",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sculpture was a smooth marble <w>egg</w> resting on a pedestal.",
                            "target_lang_translations": {
                                "ru": "Скульптура представляла собой гладкое мраморное <w>яйцо</w>, покоящееся на пьедестале."
                            }
                        },
                        {
                            "text": "She bought a chocolate <w>egg</w> for the holiday celebration.",
                            "target_lang_translations": {
                                "ru": "Она купила к празднику шоколадное <w>яйцо</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "egg-shaped",
                        "chocolate egg"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет, имеющий овальную форму яйца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "яйцо",
                                "target_lang_sense_clarification": "форма предмета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26d03805-7afc-3745-b188-c986e3c641a3",
                    "sense_definition": "The female reproductive cell in animals and plants; an ovum.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "biological_cell",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sperm must fertilize the <w>egg</w> for reproduction to occur.",
                            "target_lang_translations": {
                                "ru": "Для оплодотворения сперматозоид должен проникнуть в <w>яйцеклетку</w>."
                            }
                        },
                        {
                            "text": "Doctors can now freeze an <w>egg</w> for future fertilization.",
                            "target_lang_translations": {
                                "ru": "Врачи теперь могут заморозить <w>яйцеклетку</w> для последующего оплодотворения."
                            }
                        }
                    ],
                    "synonyms": [
                        "ovum"
                    ],
                    "common_phrases": [
                        "egg cell",
                        "fertilized egg"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "biology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женская половая клетка животных и растений; яйцеклетка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "яйцеклетка",
                                "target_lang_sense_clarification": "биология"
                            },
                            {
                                "target_lang_word": "яйцо",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8133d847-e4a1-3e99-bb7f-dfb7c6ad1c6c",
                    "sense_definition": "A person of a specified kind, usually described as good or bad.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person_idiom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is a good <w>egg</w> who always helps his neighbors when they are in trouble.",
                            "target_lang_translations": {
                                "ru": "Он славный <w>малый</w>, который всегда помогает соседям, когда они в беде."
                            }
                        },
                        {
                            "text": "I would avoid him if I were you because he is known to be a bad <w>egg</w>.",
                            "target_lang_translations": {
                                "ru": "Я бы держался от него подальше, так как известно, что он дрянной <w>человек</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "good egg",
                        "bad egg"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "idiomatic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек с определёнными качествами, обычно описываемый как хороший или плохой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "человек",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "малый",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91a5202b-de37-374a-b12d-a8478a430278",
                    "sense_definition": "A score of zero in a game or sport.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "score",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team ended the inning with a big fat <w>egg</w> on the scoreboard.",
                            "target_lang_translations": {
                                "ru": "Команда завершила иннинг с жирным <w>нулём</w> на табло."
                            }
                        },
                        {
                            "text": "He was disappointed to finish the match with an <w>egg</w> instead of a point.",
                            "target_lang_translations": {
                                "ru": "Он был разочарован тем, что закончил матч с <w>баранкой</w> вместо очка."
                            }
                        }
                    ],
                    "synonyms": [
                        "goose egg",
                        "love"
                    ],
                    "common_phrases": [
                        "duck egg",
                        "goose egg"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "sports"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Счёт ноль в игре или спорте."

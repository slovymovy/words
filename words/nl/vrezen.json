    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9d01c184-7388-35ae-9323-f37cb105112e",
                    "sense_definition": "Bang zijn dat er iets vervelends of ergs gaat gebeuren; angst voelen voor iets of iemand.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>vrees</w> dat het morgen de hele dag gaat regenen.",
                            "target_lang_translations": {
                                "en": "I <w>fear</w> that it will rain all day tomorrow.",
                                "ru": "Я <w>боюсь</w>, что завтра весь день будет идти дождь."
                            }
                        },
                        {
                            "text": "De artsen <w>vrezen</w> het ergste voor de gezondheid van de patiënt.",
                            "target_lang_translations": {
                                "en": "The doctors <w>fear</w> the worst for the patient's health.",
                                "ru": "Врачи <w>опасаются</w> худшего для здоровья пациента."
                            }
                        },
                        {
                            "text": "Je hoeft nergens bang voor te zijn, want je hebt niets te <w>vrezen</w>.",
                            "target_lang_translations": {
                                "en": "You don't have to be afraid of anything, because you have nothing to <w>fear</w>.",
                                "ru": "Тебе не нужно ни о чем беспокоиться, ведь тебе нечего <w>бояться</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "duchten"
                    ],
                    "antonyms": [
                        "hopen",
                        "vertrouwen"
                    ],
                    "common_phrases": [
                        "het ergste vrezen",
                        "niets te vrezen",
                        "te vrezen valt"
                    ],
                    "target_lang_definitions": {
                        "en": "To be afraid that something unpleasant or bad will happen; to feel fear of something or someone.",
                        "ru": "Бояться, что произойдет что-то неприятное или плохое; испытывать страх перед чем-то или кем-то."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fear",
                                "target_lang_sense_clarification": "to be afraid of or expect with alarm"
                            },
                            {
                                "target_lang_word": "dread",
                                "target_lang_sense_clarification": "to anticipate with great apprehension"
                            },
                            {
                                "target_lang_word": "be afraid",
                                "target_lang_sense_clarification": "used to politely introduce bad news or express fear"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "бояться",
                                "target_lang_sense_clarification": "испытывать чувство страха"
                            },
                            {
                                "target_lang_word": "опасаться",
                                "target_lang_sense_clarification": "ждать чего-то плохого, тревожиться о возможных последствиях"
                            },
                            {
                                "target_lang_word": "страшиться",
                                "target_lang_sense_clarification": "испытывать сильный страх (более литературное)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9ffc26cd-3f7f-38dc-83df-c81b81ab91f0",
                    "sense_definition": "Meervoud van <w>vrees</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Al zijn <w>vrezen</w> bleken uiteindelijk ongegrond te zijn.",
                            "target_lang_translations": {
                                "en": "All his <w>fears</w> turned out to be unfounded in the end.",
                                "ru": "Все его <w>страхи</w> в конечном итоге оказались необоснованными."
                            }
                        },
                        {
                            "text": "Zij sprak zelden over haar diepste <w>vrezen</w> en onzekerheden.",
                            "target_lang_translations": {
                                "en": "She rarely spoke about her deepest <w>fears</w> and insecurities.",
                                "ru": "Она редко говорила о своих глубочайших <w>страхах</w> и неуверенности."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of <w>fear</w>.",
                        "ru": "Множественное число от слова <w>страх</w>."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fears",
                                "target_lang_sense_clarification": "plural form; specific instances or objects of fear"
                            }
                        ],

                        "пережива́ние",
                        "восприя́тие",
                        "восприя́тие"
                    ],
                    "common_phrases": [
                        "странное ощущение",
                        "ощущение счастья",
                        "ощущение тревоги",
                        "не покидает ощущение",
                        "ощущение свободы"
                    ],
                    "target_lang_definitions": {

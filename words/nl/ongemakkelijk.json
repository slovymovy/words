{
    "word_family": [
        "ongemak",
        "gemak"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e1c187db-5be1-3ce1-9d36-906aee33bf8e",
                    "sense_definition": "Een gevoel van onbehagen of schaamte veroorzakend of ervarend; niet op het gemak.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "awkward_feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij voelde zich zichtbaar <w>ongemakkelijk</w> toen iedereen naar hem keek.",
                            "target_lang_translations": {
                                "en": "He felt visibly <w>uncomfortable</w> when everyone looked at him.",
                                "ru": "Он чувствовал себя явно <w>неловко</w>, когда все смотрели на него."
                            }
                        },
                        {
                            "text": "Er viel een <w>ongemakkelijke</w> stilte tijdens het gesprek.",
                            "target_lang_translations": {
                                "en": "An <w>awkward</w> silence fell during the conversation.",
                                "ru": "Во время разговора повисла <w>неловкая</w> тишина."
                            }
                        }
                    ],
                    "synonyms": [
                        "pijnlijk",
                        "lastig"
                    ],
                    "antonyms": [
                        "gemakkelijk"
                    ],
                    "common_phrases": [
                        "zich ongemakkelijk voelen",
                        "een ongemakkelijke stilte",
                        "een ongemakkelijke situatie"
                    ],
                    "target_lang_definitions": {
                        "en": "Causing or experiencing a feeling of unease or shame; not at ease.",
                        "ru": "Вызывающий или испытывающий чувство неловкости или стыда; чувствующий себя не в своей тарелке."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "uncomfortable",
                                "target_lang_sense_clarification": "feeling physical or mental unease"
                            },
                            {
                                "target_lang_word": "awkward",
                                "target_lang_sense_clarification": "causing or feeling embarrassment"
                            },
                            {
                                "target_lang_word": "uneasy",
                                "target_lang_sense_clarification": "feeling anxious or unsettled"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "неловкий",
                                "target_lang_sense_clarification": "вызывающий смущение или стыд"
                            },
                            {
                                "target_lang_word": "неудобный",
                                "target_lang_sense_clarification": "вызывающий психологический дискомфорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b63e2bdf-cd72-3009-82d3-4a54d2007013",
                    "sense_definition": "Wat veel inspanning kost of niet goed uitkomt; lastig of bezwaarlijk.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "difficult_inconvenient",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het tijdstip van de vergadering was voor de meeste deelnemers erg <w>ongemakkelijk</w>.",
                            "target_lang_translations": {
                                "en": "The time of the meeting was very <w>inconvenient</w> for most participants.",
                                "ru": "Время встречи было очень <w>неудобным</w> для большинства участников."
                            }
                        },
                        {
                            "text": "Het is een <w>ongemakkelijke</w> taak om dit probleem zonder hulp op te lossen.",
                            "target_lang_translations": {
                                "en": "It is an <w>awkward</w> task to solve this problem without help.",
                                "ru": "Решить эту проблему без помощи — <w>затруднительная</w> задача."
                            }
                        }
                    ],
                    "synonyms": [
                        "moeilijk",
                        "lastig"
                    ],
                    "antonyms": [
                        "gemakkelijk",
                        "simpel"
                    ],
                    "common_phrases": [
                        "een ongemakkelijk moment",
                        "ongemakkelijke vragen"
                    ],
                    "target_lang_definitions": {
                        "en": "Requiring much effort or not convenient; troublesome or objectionable.",
                        "ru": "Требующий больших усилий или неуместный по времени; трудный или обременительный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "inconvenient",
                                "target_lang_sense_clarification": "causing trouble or difficulty; not suitable"
                            },
                            {
                                "target_lang_word": "awkward",
                                "target_lang_sense_clarification": "difficult to use, do, or deal with"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "неудобный",
                                "target_lang_sense_clarification": "не подходящий по времени или условиям"
                            },
                            {
                                "target_lang_word": "затруднительный",
                                "target_lang_sense_clarification": "требующий усилий, сложный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "1427ff93-286d-3732-b18e-fd56f8ee3ed9",
                    "sense_definition": "Op een manier die onprettig, geforceerd of niet op het gemak aanvoelt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "awkward_feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze lachte <w>ongemakkelijk</w> toen ze merkte dat ze een fout had gemaakt.",
                            "target_lang_translations": {
                                "en": "She laughed <w>awkwardly</w> when she noticed she had made a mistake.",
                                "ru": "Она <w>неловко</w> рассмеялась, когда заметила, что допустила ошибку."
                            }
                        },
                        {
                            "text": "Hij schoof <w>ongemakkelijk</w> heen en weer op zijn stoel tijdens het verhoor.",
                            "target_lang_translations": {
                                "en": "He shifted <w>uncomfortably</w> back and forth in his chair during the interrogation.",
                                "ru": "Он <w>беспокойно</w> ерзал на стуле во время допроса."
                            }
                        }
                    ],
                    "synonyms": [
                        "pijnlijk",
                        "moeilijk"
                    ],
                    "antonyms": [
                        "gemakkelijk"
                    ],
                    "common_phrases": [
                        "ongemakkelijk kijken",
                        "ongemakkelijk lachen"
                    ],
                    "target_lang_definitions": {
                        "en": "In a way that feels unpleasant, forced, or not at ease.",
                        "ru": "Неприятным, неестественным образом или с чувством дискомфорта."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "uncomfortably",
                                "target_lang_sense_clarification": "in a way that causes or shows unease"
                            },
                            {
                                "target_lang_word": "awkwardly",
                                "target_lang_sense_clarification": "in a clumsy or embarrassed manner"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "неловко",
                                "target_lang_sense_clarification": "с чувством стеснения"
                            },
                            {
                                "target_lang_word": "смущённо",
                                "target_lang_sense_clarification": "выражая стыд или замешательство"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
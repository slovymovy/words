    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "cc6b6f9e-34ec-3f06-8a58-a97b36776bd8",
                    "sense_definition": "For the most part; almost completely or strictly true.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new house is <w>basically</w> finished, but we still need to paint the walls.",
                            "target_lang_translations": {
                                "ru": "Новый дом <w>практически</w> готов, но нам всё ещё нужно покрасить стены."
                            }
                        },
                        {
                            "text": "It is <w>basically</w> the same recipe, just with less sugar.",
                            "target_lang_translations": {
                                "ru": "Это <w>по сути</w> тот же рецепт, просто с меньшим количеством сахара."
                            }
                        }
                    ],
                    "synonyms": [
                        "mostly",
                        "essentially",
                        "chiefly",
                        "more often than not"
                    ],
                    "common_phrases": [
                        "basically the same",
                        "basically correct",
                        "basically done"
                    ],
                    "target_lang_definitions": {
                        "ru": "В основном; почти полностью или фактически верно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в основно́м",
                                "target_lang_sense_clarification": "большей частью"
                            },
                            {
                                "target_lang_word": "практически",
                                "target_lang_sense_clarification": "почти"
                            },
                            {
                                "target_lang_word": "по сути",
                                "target_lang_sense_clarification": "фактически"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ab4627c-bd1e-33ff-83b4-070a85151c9f",
                    "sense_definition": "Used to introduce the most important point or a simplified explanation of a situation.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "So <w>basically</w>, we missed the train and had to walk home.",
                            "target_lang_translations": {
                                "ru": "Так что, <w>короче говоря</w>, мы опоздали на поезд, и нам пришлось идти домой пешком."
                            }
                        },
                        {
                            "text": "Well, <w>basically</w>, I do not agree with the decision.",
                            "target_lang_translations": {
                                "ru": "Ну, <w>по сути</w>, я не согласен с этим решением."
                            }
                        }
                    ],
                    "synonyms": [
                        "essentially"
                    ],
                    "common_phrases": [
                        "so basically",
                        "well basically"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used as a filler word in speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для введения главного или упрощенного объяснения ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "короче говоря",
                                "target_lang_sense_clarification": "вкратце"
                            },
                            {
                                "target_lang_word": "по сути",
                                "target_lang_sense_clarification": "главное"
                            },
                            {
                                "target_lang_word": "в общем",
                                "target_lang_sense_clarification": "обобщая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da982ea3-3e2b-3efd-8ac3-cbdc05cf2a95",
                    "sense_definition": "In a fundamental, essential, or basic manner.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two political parties are <w>basically</w> different in their approach to the economy.",
                            "target_lang_translations": {
                                "ru": "Эти две политические партии <w>принципиально</w> отличаются в своём подходе к экономике."
                            }
                        },
                        {
                            "text": "Human beings are <w>basically</w> social creatures who need interaction.",
                            "target_lang_translations": {
                                "ru": "Люди — <w>в своей основе</w> социальные существа, которым необходимо общение."
                            }
                        }
                    ],
                    "synonyms": [
                        "fundamentally",
                        "essentially"
                    ],
                    "common_phrases": [
                        "basically different",
                        "basically sound"
                    ],
                    "target_lang_definitions": {
                        "ru": "В своей основе; фундаментально."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в своей основе",
                                "target_lang_sense_clarification": "по природе"
                            },
                            {
                                "target_lang_word": "принципиально",
                                "target_lang_sense_clarification": "коренным образом"
                            },
                            {
                                "target_lang_word": "по существу́",
                                "target_lang_sense_clarification": "в сущности"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                        "leśny",
                        "ogrodowy",
                        "miejski"
                    ],
                    "common_phrases": [
                        "droga polna",
                        "kwiaty polne",
                        "konik polny",
                        "mysz polna",
                        "kamień polny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к полю, находящийся в поле, растущий или обитающий в диком виде на полях и лугах"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полевой",
                                "target_lang_sense_clarification": "относящийся к полю, растущий в поле"

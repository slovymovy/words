            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3294f5c5-0bc0-398e-83bd-a017ef868a5b",
                    "sense_definition": "Соглашение между двумя или несколькими сторонами о совместных действиях, взаимных обязательствах или обмене ценностями.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наша компания заключила выгодную <w>сделку</w> с новыми поставщиками.",
                            "target_lang_translations": {
                                "en": "Our company closed a profitable <w>deal</w> with new suppliers."
                            }
                        },
                        {
                            "text": "Юристы внимательно проверяют документы, чтобы оформить <w>сделку</w> по закону.",
                            "target_lang_translations": {
                                "en": "Lawyers are carefully checking the documents to finalize the <w>deal</w> according to the law."
                            }
                        }
                    ],
                    "synonyms": [
                        "договор",
                        "соглаше́ние",
                        "контра́кт"
                    ],
                    "common_phrases": [
                        "заключить сделку",
                        "выгодная сделка",
                        "расторгнуть сделку",
                        "оформить сделку"
                    ],
                    "target_lang_definitions": {
                        "en": "An agreement between two or more parties regarding joint actions, mutual obligations, or the exchange of values."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deal",
                                "target_lang_sense_clarification": "business agreement or arrangement"
                            },
                            {
                                "target_lang_word": "transaction",
                                "target_lang_sense_clarification": "specific instance of buying or selling"
                            },
                            {
                                "target_lang_word": "bargain",
                                "target_lang_sense_clarification": "agreement or advantageous purchase"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "293f1f73-07a4-327a-bb76-4a577bb743de",
                    "sense_definition": "Неблаговидный, часто тайный сговор, противоречащий моральным нормам или закону.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Честный человек никогда не пойдёт на <w>сделку</w> с совестью ради денег.",
                            "target_lang_translations": {
                                "en": "An honest person would never make a <w>deal</w> with their conscience for money."
                            }
                        },
                        {
                            "text": "Журналисты раскрыли тайную <w>сделку</w> между политиками и бизнесом.",
                            "target_lang_translations": {
                                "en": "Journalists revealed a secret <w>deal</w> between politicians and businesses."
                            }
                        }
                    ],
                    "synonyms": [
                        "сговор"
                    ],
                    "common_phrases": [
                        "сделка с совестью",
                        "закулисная сделка",
                        "пойти на сделку"
                    ],
                    "target_lang_definitions": {
                        "en": "A shady, often secret agreement that contradicts moral norms or the law."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deal",

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "b7bbb66e-f8de-3181-ae6f-aa09cc38d947",
                    "sense_definition": "Op een manier die nauwelijks hoorbaar of zichtbaar is; in stilte of in het geheim.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Iedere avond sloop hij <stilletjes> naar de stal om te gaan slapen.",
                            "target_lang_translations": {
                                "en": "Every evening he crept <quietly> to the stable to go to sleep.",
                                "ru": "Каждый вечер он <тихонько> пробирался в конюшню, чтобы лечь спать."
                            }
                        },
                        {
                            "text": "Ze hoopte <stilletjes> dat niemand haar zou zien vertrekken.",
                            "target_lang_translations": {
                                "en": "She <secretly> hoped that no one would see her leave.",
                                "ru": "Она <втайне> надеялась, что никто не увидит, как она уходит."
                            }
                        },
                        {
                            "text": "Ik herhaalde mijn nieuwe naam een aantal keer <stilletjes> voor mezelf.",
                            "target_lang_translations": {
                                "en": "I repeated my new name <softly> to myself a few times.",
                                "ru": "Я повторил свое новое имя несколько раз <тихо> про себя."
                            }
                        }
                    ],
                    "synonyms": [
                        "zachtjes",
                        "ongemerkt",
                        "heimelijk"
                    ],
                    "antonyms": [
                        "luid",
                        "luidruchtig",
                        "openlijk"
                    ],
                    "common_phrases": [
                        "stilletjes hopen",
                        "stilletjes weggaan",
                        "stilletjes genieten"
                    ],
                    "target_lang_definitions": {
                        "en": "In a way that is barely audible or visible; in silence or in secret.",
                        "ru": "Образом, который едва слышен или заметен; в тишине или в тайне."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quietly",
                                "target_lang_sense_clarification": "without making much noise"
                            },
                            {
                                "target_lang_word": "secretly",
                                "target_lang_sense_clarification": "in one's mind or without others knowing"
                            },
                            {
                                "target_lang_word": "softly",
                                "target_lang_sense_clarification": "with a low volume"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тихо",
                                "target_lang_sense_clarification": "без шума"
                            },
                            {
                                "target_lang_word": "тайком",
                                "target_lang_sense_clarification": "скрытно, незаметно для других"
                            },
                            {
                                "target_lang_word": "втайне",
                                "target_lang_sense_clarification": "про себя, внутренне (о чувствах или мыслях)"
                            },
                            {
                                "target_lang_word": "потихоньку",
                                "target_lang_sense_clarification": "незаметно или медленно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [

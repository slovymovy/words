                            },
                            {
                                "target_lang_word": "pellicle",
                                "target_lang_sense_clarification": "thin skin or film"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac389cfa-8050-31da-b566-dd30f6b3f42d",

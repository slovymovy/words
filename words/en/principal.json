    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a2a3ce60-cf03-3447-a8db-27b1880b95dc",
                    "sense_definition": "Primary; most important; first level in importance.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>principal</w> reason for my visit is to see the manager.",
                            "target_lang_translations": {
                                "ru": "<w>Гла́вной</w> причи́ной моего́ визи́та явля́ется встре́ча с ме́неджером."
                            }
                        },
                        {
                            "text": "Vegetables are the <w>principal</w> ingredient in this stew.",
                            "target_lang_translations": {
                                "ru": "Овощи — <w>основно́й</w> ингредие́нт в э́том рагу́."
                            }
                        },
                        {
                            "text": "Our <w>principal</w> concern is the safety of the children.",
                            "target_lang_translations": {
                                "ru": "На́ша <w>гла́вная</w> забо́та — безопа́сность дете́й."
                            }
                        }
                    ],
                    "synonyms": [
                        "main",
                        "primary",
                        "chief"
                    ],
                    "antonyms": [
                        "secondary",
                        "minor"
                    ],
                    "common_phrases": [
                        "principal reason",
                        "principal cause",
                        "principal objective",
                        "principal source"
                    ],
                    "target_lang_definitions": {
                        "ru": "Главный; наиболее важный; первый по значимости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "главный",
                                "target_lang_sense_clarification": "наиболее важный"
                            },
                            {
                                "target_lang_word": "основной",
                                "target_lang_sense_clarification": "базовый, первостепенный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2cfad753-e40e-3e3c-9f91-1d0ecdfbb86a",
                    "sense_definition": "The chief administrator of a school.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "education_admin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>principal</w> announced the new rules during the morning assembly.",
                            "target_lang_translations": {
                                "ru": "<w>Дире́ктор</w> объяви́л о но́вых пра́вилах во вре́мя у́треннего собра́ния."
                            }
                        },
                        {
                            "text": "Parents must make an appointment if they wish to see the <w>principal</w>.",
                            "target_lang_translations": {
                                "ru": "Роди́тели должны́ записа́ться на приём, е́сли хотя́т уви́деть <w>дире́ктора</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "headteacher",
                        "headmaster",
                        "headmistress"
                    ],
                    "common_phrases": [
                        "school principal",
                        "vice principal",
                        "principal's office"
                    ],
                    "target_lang_definitions": {
                        "ru": "Директор школы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "директор",
                                "target_lang_sense_clarification": "школы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e452063-32bd-3f5f-8f71-b9878797d9f7",
                    "sense_definition": "The money originally invested or loaned, on which basis interest and returns are calculated.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A portion of your monthly payment goes to reduce the <w>principal</w>.",
                            "target_lang_translations": {
                                "ru": "Часть ва́шего ежеме́сячного платежа́ идёт на погаше́ние <w>основно́го до́лга</w>."
                            }
                        },
                        {
                            "text": "She managed to pay off the <w>principal</w> of her loan earlier than expected.",
                            "target_lang_translations": {
                                "ru": "Ей удало́сь вы́платить <w>основну́ю су́мму</w> креди́та ра́ньше ожида́емого."
                            }
                        }
                    ],
                    "antonyms": [
                        "interest"

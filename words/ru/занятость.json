                },
                {
                    "sense_id": "5ff675d8-e4e6-3097-a900-d37ccc050f02",
                    "sense_definition": "Отсутствие свободного времени, большая загруженность делами или работой.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Несмотря на свою <w>занятость</w>, он всегда находил время для семьи.",
                            "target_lang_translations": {
                                "en": "Despite his <w>busyness</w>, he always found time for his family."
                            }
                        },
                        {
                            "text": "Из-за высокой <w>занятости</w> на работе мы редко видимся с друзьями.",
                            "target_lang_translations": {
                                "en": "Due to a heavy <w>workload</w> at work, we rarely see our friends."
                            }
                        }
                    ],
                    "synonyms": [
                        "загруженность"
                    ],
                    "common_phrases": [
                        "вечная занятость",
                        "ссылаться на занятость",
                        "высокая занятость"
                    ],
                    "target_lang_definitions": {
                        "en": "The state of being busy or having a lack of free time due to tasks or work."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "busyness",
                                "target_lang_sense_clarification": "state of being busy"
                            },
                            {
                                "target_lang_word": "workload",
                                "target_lang_sense_clarification": "amount of work to be done"
                            }
                        ]
                    }
                }
            ]

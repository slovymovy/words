    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "95a51db1-301b-38a7-9f1b-67299845dc30",
                    "sense_definition": "An unspecified or unknown thing, object, or event.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "unspecified_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have <w>something</w> important to tell you.",
                            "target_lang_translations": {
                                "ru": "Я должен тебе <w>кое-что</w> важное сказать."
                            }
                        },
                        {
                            "text": "Did you hear <w>something</w> outside the window?",
                            "target_lang_translations": {
                                "ru": "Ты слышал <w>что-то</w> за окном?"
                            }
                        },
                        {
                            "text": "I must have forgotten to pack <w>something</w> for the trip.",
                            "target_lang_translations": {
                                "ru": "Должно быть, я забыл взять <w>что-то</w> в поездку."
                            }
                        }
                    ],
                    "synonyms": [
                        "sth"
                    ],
                    "antonyms": [
                        "nothing",
                        "everything"
                    ],
                    "common_phrases": [
                        "something else",
                        "something new",
                        "something like that",
                        "or something"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неопределенный или неизвестный предмет, объект или событие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "что-то",
                                "target_lang_sense_clarification": "неопределенный предмет"
                            },
                            {
                                "target_lang_word": "кое-что",
                                "target_lang_sense_clarification": "предмет, известный говорящему, но не называемый"
                            },
                            {
                                "target_lang_word": "нечто",
                                "target_lang_sense_clarification": "неопределенный предмет (книжн.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd3b73a5-fe74-3e53-960e-4792026ff29a",
                    "sense_definition": "A person or thing having a quality to a moderate degree; used to describe someone or something as having characteristics of a specific type.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is <w>something</w> of an expert on this subject.",
                            "target_lang_translations": {
                                "ru": "Он <w>в некотором роде</w> эксперт в этом вопросе."
                            }
                        },
                        {
                            "text": "The news came as <w>something</w> of a surprise to everyone.",
                            "target_lang_translations": {
                                "ru": "Эта новость стала для всех <w>своего рода</w> сюрпризом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "something of a"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или вещь, обладающие определенным качеством в умеренной степени; используется для описания наличия характеристик определенного типа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в некотором роде",
                                "target_lang_sense_clarification": "в определенной степени"
                            },
                            {
                                "target_lang_word": "своего рода",
                                "target_lang_sense_clarification": "разновидность чего-либо"
                            },
                            {
                                "target_lang_word": "отчасти",
                                "target_lang_sense_clarification": "частично"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb105cd5-ed8e-3d57-a396-796ceb59b22b",
                    "sense_definition": "A person or thing that is impressive, remarkable, or significant.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "notable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That new movie is really <w>something</w>.",
                            "target_lang_translations": {
                                "ru": "Этот новый фильм — действительно <w>нечто</w>."
                            }
                        },
                        {
                            "text": "She is quite <w>something</w> to have finished the race so quickly.",
                            "target_lang_translations": {
                                "ru": "Она просто <w>нечто</w>, раз закончила гонку так быстро."
                            }
                        }
                    ],
                    "common_phrases": [
                        "really something",
                        "quite something"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to express admiration or shock"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или вещь, которые впечатляют, примечательны или значительны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нечто",
                                "target_lang_sense_clarification": "о чем-то выдающемся"
                            },
                            {
                                "target_lang_word": "что-то с чем-то",
                                "target_lang_sense_clarification": "разг. о чем-то удивительном"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4d33496-0b78-36c1-8363-bc7380222c56",
                    "sense_definition": "An indefinable quality, talent, or characteristic that makes someone or something special.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "indefinable_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has a certain <w>something</w> that makes her very popular.",
                            "target_lang_translations": {
                                "ru": "В ней есть некое <w>что-то</w>, что делает её очень популярной."
                            }
                        },
                        {
                            "text": "There is a special <w>something</w> about this place.",
                            "target_lang_translations": {
                                "ru": "В этом месте есть особое <w>нечто</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "je ne sais quoi"
                    ],
                    "common_phrases": [
                        "a certain something"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неопределимое качество, талант или характеристика, делающие кого-то или что-то особенным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изюминка",
                                "target_lang_sense_clarification": "особое качество"
                            },
                            {
                                "target_lang_word": "нечто",
                                "target_lang_sense_clarification": "неопределимое свойство"
                            },
                            {
                                "target_lang_word": "что-то",
                                "target_lang_sense_clarification": "неуловимая черта"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "5333b95e-f0ea-33c0-8420-a620796a6744",
                    "sense_definition": "Наречие к <w>феерический</w>; так, как бывает в феерии, волшебно, сказочно.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Праздничное шоу закончилось <w>феерически</w>, когда небо озарили тысячи огней.",
                            "target_lang_translations": {
                                "en": "The festive show ended <w>spectacularly</w> when thousands of lights lit up the sky."
                            }
                        },
                        {
                            "text": "В лучах прожекторов декорации на сцене выглядели <w>феерически</w> красиво.",
                            "target_lang_translations": {
                                "en": "In the spotlight beams, the stage scenery looked <w>breathtakingly</w> beautiful."
                            }
                        }
                    ],
                    "synonyms": [
                        "волшебно",
                        "сказочно"
                    ],
                    "antonyms": [
                        "буднично"
                    ],
                    "common_phrases": [
                        "феерически красиво",
                        "выглядеть феерически"
                    ],
                    "target_lang_definitions": {
                        "en": "In a magical, fairytale-like, or spectacular manner."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spectacularly",
                                "target_lang_sense_clarification": "regarding a visual display or event"
                            },
                            {
                                "target_lang_word": "magically",
                                "target_lang_sense_clarification": "resembling a fairy tale"
                            },
                            {
                                "target_lang_word": "breathtakingly",
                                "target_lang_sense_clarification": "used to describe impressive beauty"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e155bab-c74a-3639-a7f3-843ee0b46439",
                    "sense_definition": "Неправдоподобно, поразительно.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нам <w>феерически</w> повезло успеть на последний поезд перед закрытием метро.",
                            "target_lang_translations": {
                                "en": "We were <w>incredibly</w> lucky to catch the last train before the metro closed."
                            }
                        },
                        {
                            "text": "Этот план был <w>феерически</w> глупым и с самого начала обречённым на провал.",
                            "target_lang_translations": {
                                "en": "This plan was <w>spectacularly</w> stupid and doomed to fail from the very beginning."
                            }
                        }
                    ],
                    "synonyms": [
                        "сказочно"
                    ],
                    "common_phrases": [
                        "феерически глупо",
                        "феерически везёт"
                    ],
                    "target_lang_definitions": {
                        "en": "To an unbelievable or striking extent; amazingly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spectacularly",
                                "target_lang_sense_clarification": "intensifier, often used with negative qualities like 'stupid' or 'fail'"
                            },
                            {
                                "target_lang_word": "incredibly",
                                "target_lang_sense_clarification": "general intensifier"
                            },
                            {
                                "target_lang_word": "fabulously",
                                "target_lang_sense_clarification": "intensifier, often used with luck or wealth"
                            }
                        ]
                    }
                }
            ]
        }
    ]

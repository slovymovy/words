    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8d5c8b33-dee3-3fdf-be6a-38a09f2041f5",
                    "sense_definition": "A publication, usually published daily or weekly, containing news, articles, advertisements, and correspondence.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I read the <w>newspaper</w> every morning with my coffee.",
                            "target_lang_translations": {
                                "ru": "Я читаю <w>газету</w> каждое утро за чашкой кофе."
                            }
                        },
                        {
                            "text": "She bought a local <w>newspaper</w> to look for a job.",
                            "target_lang_translations": {
                                "ru": "Она купила местную <w>газету</w>, чтобы найти работу."
                            }
                        },
                        {
                            "text": "The article appeared in a major national <w>newspaper</w>.",
                            "target_lang_translations": {
                                "ru": "Статья появилась в крупной национальной <w>газете</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "paper"
                    ],
                    "common_phrases": [
                        "daily newspaper",
                        "local newspaper",
                        "read the newspaper",
                        "national newspaper"
                    ],
                    "target_lang_definitions": {
                        "ru": "Печатное издание, обычно выходящее ежедневно или еженедельно, содержащее новости, статьи и рекламу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "газета",
                                "target_lang_sense_clarification": "периодическое издание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7d27b98-8079-3101-950b-f24f6152f086",
                    "sense_definition": "The news organization or company that produces and publishes a newspaper.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>newspaper</w> decided to publish the controversial story.",
                            "target_lang_translations": {
                                "ru": "<w>Газета</w> решила опубликовать эту скандальную историю."
                            }
                        },
                        {
                            "text": "He has worked for the same <w>newspaper</w> for twenty years.",
                            "target_lang_translations": {
                                "ru": "Он проработал в одной и той же <w>газете</w> двадцать лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "paper"
                    ],
                    "common_phrases": [
                        "work for a newspaper",
                        "newspaper editor",
                        "newspaper office"
                    ],
                    "target_lang_definitions": {
                        "ru": "Новостная организация или компания, которая выпускает газету."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "газета",
                                "target_lang_sense_clarification": "организация"
                            },
                            {
                                "target_lang_word": "редакция",
                                "target_lang_sense_clarification": "издательство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ce2856c-0daf-3053-9f55-653863983961",
                    "sense_definition": "The cheap, low-quality paper on which newspapers are typically printed.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We wrapped the fragile glasses in old <w>newspaper</w> before packing them.",
                            "target_lang_translations": {
                                "ru": "Мы завернули хрупкие бокалы в старую <w>газету</w> перед упаковкой."
                            }
                        },
                        {
                            "text": "The fish and chips were traditionally served wrapped in <w>newspaper</w>.",
                            "target_lang_translations": {
                                "ru": "Рыбу с картошкой традиционно подавали завернутыми в <w>газету</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "newsprint"
                    ],
                    "common_phrases": [
                        "wrapped in newspaper",
                        "sheets of newspaper",
                        "piece of newspaper"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дешёвая бумага низкого качества, на которой обычно печатаются газеты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "газетная бумага",
                                "target_lang_sense_clarification": "материал"
                            },
                            {
                                "target_lang_word": "газета",
                                "target_lang_sense_clarification": "использованная как обёртка"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7cb53747-86d6-3561-9f9e-84d9c7cdf604",
                    "sense_definition": "To cover an object or surface with sheets of newspaper, often for protection.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>newspapered</w> the floor before she started painting the walls.",
                            "target_lang_translations": {
                                "ru": "Она <w>застелила газетами</w> пол, прежде чем начать красить стены."
                            }
                        },
                        {
                            "text": "The windows were <w>newspapered</w> to keep the sunlight out.",
                            "target_lang_translations": {
                                "ru": "Окна были <w>заклеены газетами</w>, чтобы не пропускать солнечный свет."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать предмет или поверхность листами газеты, часто для защиты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "застилать газетами",
                                "target_lang_sense_clarification": "покрывать поверхность"
                            },
                            {
                                "target_lang_word": "обклеивать газетами",
                                "target_lang_sense_clarification": "наклеивать на поверхность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b51cc87-fd92-3dc6-ac8c-0b4f9b8b022d",
                    "sense_definition": "To work as a journalist or engage in the business of publishing newspapers.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>newspapered</w> his way across the country reporting on sports.",
                            "target_lang_translations": {
                                "ru": "Он <w>работал газетчиком</w>, путешествуя по стране и ведя спортивные репортажи."
                            }
                        },
                        {
                            "text": "After he <w>newspapered</w> for a decade, he wrote a book.",
                            "target_lang_translations": {
                                "ru": "Проработав десять лет <w>газетчиком</w>, он написал книгу."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to describe the act of working in the industry"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Работать журналистом или заниматься газетным делом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "работать в газете",
                                "target_lang_sense_clarification": "быть сотрудником"
                            },
                            {
                                "target_lang_word": "быть газетчиком",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c009a9f-50da-35b8-923f-ae25857545a8",
                    "sense_definition": "To harass or attack someone publicly through negative newspaper articles.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The politician claimed he was being <w>newspapered</w> by his opponents.",
                            "target_lang_translations": {
                                "ru": "Политик заявил, что его оппоненты <w>травили его в газетах</w>."
                            }
                        },
                        {
                            "text": "He got <w>newspapered</w> out of public life after the scandal.",
                            "target_lang_translations": {
                                "ru": "Его <w>выжили</w> из общественной жизни газетной травлей после скандала."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies a coordinated media attack"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Публично нападать на кого-либо или преследовать через негативные газетные статьи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "травить в газетах",
                                "target_lang_sense_clarification": "преследовать через прессу"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
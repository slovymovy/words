{
    "word_family": [
        "close",
        "closure",
        "closeness",
        "closely",
        "enclosed"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a5ce792d-3254-3cd0-9ec4-6e48c846a109",
                    "sense_definition": "Shut, sealed, or covered so that nothing can enter or leave.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please keep the door <w>closed</w> to stop the cold wind coming in.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, держи дверь <w>закрытой</w>, чтобы не впускать холодный ветер."
                            }
                        },
                        {
                            "text": "She sat quietly with her eyes <w>closed</w> and listened to the music.",
                            "target_lang_translations": {
                                "ru": "Она сидела тихо с <w>закрытыми</w> глазами и слушала музыку."
                            }
                        }
                    ],
                    "synonyms": [
                        "shut"
                    ],
                    "antonyms": [
                        "open"
                    ],
                    "common_phrases": [
                        "closed door",
                        "eyes closed",
                        "keep closed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запертый, запечатанный или прикрытый так, что ничто не может войти или выйти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закрытый",
                                "target_lang_sense_clarification": "физически (дверь, окно)"
                            },
                            {
                                "target_lang_word": "сомкнутый",
                                "target_lang_sense_clarification": "о глазах или губах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d44382ad-4f59-3c2c-aa30-d3b8a0b8064c",
                    "sense_definition": "Not open for business, service, or public use.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "availability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum is <w>closed</w> on Mondays, so we should visit tomorrow.",
                            "target_lang_translations": {
                                "ru": "Музей <w>закрыт</w> по понедельникам, поэтому нам стоит пойти завтра."
                            }
                        },
                        {
                            "text": "I tried to call the office, but the phone lines were already <w>closed</w>.",
                            "target_lang_translations": {
                                "ru": "Я пытался позвонить в офис, но телефонные линии были уже <w>закрыты</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "shut"
                    ],
                    "antonyms": [
                        "open"
                    ],
                    "common_phrases": [
                        "closed for business",
                        "officially closed",
                        "temporarily closed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не работающий для посетителей или клиентов в данный момент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закрытый",
                                "target_lang_sense_clarification": "не работающий (магазин, учреждение)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4abb5fb4-f80b-32c4-a763-d792555b8e7c",
                    "sense_definition": "Restricted to a specific group of people; not open to the public.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "restricted_access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee held a <w>closed</w> session to discuss the sensitive issue.",
                            "target_lang_translations": {
                                "ru": "Комитет провел <w>закрытое</w> заседание для обсуждения деликатного вопроса."
                            }
                        },
                        {
                            "text": "This is a <w>closed</w> event, so only registered members can attend.",
                            "target_lang_translations": {
                                "ru": "Это <w>закрытое</w> мероприятие, поэтому присутствовать могут только зарегистрированные участники."
                            }
                        }
                    ],
                    "antonyms": [
                        "open"
                    ],
                    "common_phrases": [
                        "closed session",
                        "closed meeting",
                        "closed group"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ограниченный определенной группой людей; не открытый для широкой публики."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закрытый",
                                "target_lang_sense_clarification": "частный, непубличный"
                            },
                            {
                                "target_lang_word": "ограниченный",
                                "target_lang_sense_clarification": "с ограниченным доступом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd7b9bfa-dec4-3ac9-9507-a31fa7128a82",
                    "sense_definition": "Unwilling to accept or consider new ideas or opinions.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has a very <w>closed</w> mind and refuses to listen to other perspectives.",
                            "target_lang_translations": {
                                "ru": "У него очень <w>закрытый</w> ум, и он отказывается слушать другие точки зрения."
                            }
                        },
                        {
                            "text": "The manager was <w>closed</w> to our suggestions for improving the workflow.",
                            "target_lang_translations": {
                                "ru": "Менеджер был <w>закрыт</w> для наших предложений по улучшению рабочего процесса."
                            }
                        }
                    ],
                    "antonyms": [
                        "open"
                    ],
                    "common_phrases": [
                        "closed mind",
                        "closed to ideas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не желающий принимать или рассматривать новые идеи или мнения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закрытый",
                                "target_lang_sense_clarification": "невосприимчивый к идеям"
                            },
                            {
                                "target_lang_word": "за́мкнутый",
                                "target_lang_sense_clarification": "о характере (скрытный)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "080a4276-46b9-314b-8169-87a0e1e994b6",
                    "sense_definition": "Finally settled, decided, or finished; no longer up for discussion.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police consider the investigation <w>closed</w> after finding the evidence.",
                            "target_lang_translations": {
                                "ru": "Полиция считает расследование <w>закрытым</w> после обнаружения улик."
                            }
                        },
                        {
                            "text": "As far as I am concerned, this subject is <w>closed</w>.",
                            "target_lang_translations": {
                                "ru": "Насколько я могу судить, эта тема <w>закрыта</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "open"
                    ],
                    "common_phrases": [
                        "case closed",
                        "subject is closed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Окончательно урегулированный, решенный или завершенный; не подлежащий дальнейшему обсуждению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закрытый",
                                "target_lang_sense_clarification": "завершенный (дело, вопрос)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25bd78ef-d5d2-3d75-86aa-89aa09ae3875",
                    "sense_definition": "Forming a complete loop that allows electricity to flow.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Electricity will only flow through the wires if there is a <w>closed</w> circuit.",
                            "target_lang_translations": {
                                "ru": "Электричество будет течь по проводам только при наличии <w>замкнутой</w> цепи."
                            }
                        },
                        {
                            "text": "The switch must be in the on position to create a <w>closed</w> loop.",
                            "target_lang_translations": {
                                "ru": "Переключатель должен быть в положении «включено», чтобы создать <w>замкнутый</w> контур."
                            }
                        }
                    ],
                    "antonyms": [
                        "open"
                    ],
                    "common_phrases": [
                        "closed circuit",
                        "closed loop"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Образующий полный контур, позволяющий протекать электричеству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за́мкнутый",
                                "target_lang_sense_clarification": "о цепи или контуре"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0e5822c4-ba3a-3664-bfc0-9e311e5c632a",
                    "sense_definition": "Simple past and past participle of <w>close</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>closed</w> the book and put it back on the shelf.",
                            "target_lang_translations": {
                                "ru": "Она <w>закрыла</w> книгу и поставила её обратно на полку."
                            }
                        },
                        {
                            "text": "The shop <w>closed</w> early yesterday because of the storm.",
                            "target_lang_translations": {
                                "ru": "Магазин <w>закрылся</w> рано вчера из-за шторма."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола 'закрывать'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закрыл",
                                "target_lang_sense_clarification": "прошедшее время (он)"
                            },
                            {
                                "target_lang_word": "закрылся",
                                "target_lang_sense_clarification": "возвратный глагол (магазин закрылся)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
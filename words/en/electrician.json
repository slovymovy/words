    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d6eae411-c411-34b2-af55-1ad77260a614",
                    "sense_definition": "A tradesperson who installs, repairs, and maintains electrical wiring and equipment.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to call an <w>electrician</w> to fix the wiring in the kitchen.",
                            "target_lang_translations": {
                                "ru": "Нам нужно вызвать <w>электрика</w>, чтобы починить проводку на кухне."
                            }
                        },
                        {
                            "text": "The licensed <w>electrician</w> ensured that all the cables were safe.",
                            "target_lang_translations": {
                                "ru": "Лицензированный <w>электрик</w> убедился, что все кабели безопасны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "licensed electrician",
                        "master electrician",
                        "call an electrician",
                        "qualified electrician"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специалист, который занимается установкой, ремонтом и обслуживанием электрической проводки и оборудования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "электрик",
                                "target_lang_sense_clarification": "общее название профессии"
                            },
                            {
                                "target_lang_word": "электромонтёр",
                                "target_lang_sense_clarification": "техническое название, специалист по монтажу и ремонту"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f561c681-9683-3df2-b62c-ad28933b1117",
                    "sense_definition": "A scientist who studies electricity.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the 18th century, a famous <w>electrician</w> conducted experiments with static charge.",
                            "target_lang_translations": {
                                "ru": "В XVIII веке известный <w>исследователь электричества</w> проводил эксперименты со статическим зарядом."
                            }
                        },
                        {
                            "text": "The historical text described the researcher as a skilled <w>electrician</w>.",
                            "target_lang_translations": {
                                "ru": "В историческом тексте этот исследователь описывался как опытный <w>знаток электричества</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Учёный, занимающийся изучением электричества (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исследователь электричества",
                                "target_lang_sense_clarification": "описательный перевод для исторического контекста"
                            },
                            {
                                "target_lang_word": "физик",
                                "target_lang_sense_clarification": "современный эквивалент"
                            }
                        ]
                    }
                }
            ]
        }
    ]

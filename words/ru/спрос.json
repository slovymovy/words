{
    "word_family": [
        "спрашивать",
        "спросить",
        "запрос",
        "опрос",
        "допрос",
        "вопрос",
        "расспрос"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3169b166-4c9e-37ce-a40f-01176f2a13c3",
                    "sense_definition": "Потребность покупателей в товарах или услугах, подкреплённая готовностью и возможностью их оплатить.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "economic_market",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом году <w>спрос</w> на зимнюю спортивную одежду значительно вырос.",
                            "target_lang_translations": {
                                "en": "This year, the <w>demand</w> for winter sportswear has grown significantly."
                            }
                        },
                        {
                            "text": "Основной закон рынка гласит, что <w>спрос</w> рождает предложение.",
                            "target_lang_translations": {
                                "en": "The basic law of the market states that <w>demand</w> creates supply."
                            }
                        }
                    ],
                    "antonyms": [
                        "предложение"
                    ],
                    "common_phrases": [
                        "спрос и предложение",
                        "пользоваться спросом",
                        "удовлетворить спрос",
                        "высокий спрос"
                    ],
                    "target_lang_definitions": {
                        "en": "Consumer need for goods or services, backed by the willingness and ability to pay for them."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "demand",
                                "target_lang_sense_clarification": "economic"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5dbfea68-8723-34b5-ab53-783545151d72",
                    "sense_definition": "Разрешение или позволение на совершение какого-либо действия (чаще всего используется в выражении «без спроса»).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ребёнок взял конфеты со стола без <w>спроса</w> родителей.",
                            "target_lang_translations": {
                                "en": "The child took candy from the table without the parents' <w>permission</w>."
                            }
                        },
                        {
                            "text": "Нельзя входить в чужую комнату без <w>спроса</w>.",
                            "target_lang_translations": {
                                "en": "You cannot enter someone else's room without <w>asking</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "разрешение"
                    ],
                    "common_phrases": [
                        "без спроса",
                        "взять без спроса",
                        "уйти без спроса"
                    ],
                    "target_lang_definitions": {
                        "en": "Authorization or permission to perform an action (most often used in the phrase 'without asking')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "asking",
                                "target_lang_sense_clarification": "in the phrase 'without asking'"
                            },
                            {
                                "target_lang_word": "permission",
                                "target_lang_sense_clarification": "authorization"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83c8e285-570d-38ee-8aca-18bf3f82337a",
                    "sense_definition": "Требование ответственности за чьи-либо действия, поступки или исполнение обязанностей.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "responsibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "С начальника отдела всегда особый <w>спрос</w> за результаты работы всей команды.",
                            "target_lang_translations": {
                                "en": "The department head always faces special <w>accountability</w> for the results of the entire team's work."
                            }
                        },
                        {
                            "text": "Он ещё совсем неопытный сотрудник, какой с него <w>спрос</w>?",
                            "target_lang_translations": {
                                "en": "He is still a completely inexperienced employee, what <w>responsibility</w> can be expected of him?"
                            }
                        }
                    ],
                    "synonyms": [
                        "ответственность"
                    ],
                    "common_phrases": [
                        "какой с него спрос",
                        "строгий спрос"
                    ],
                    "target_lang_definitions": {
                        "en": "The requirement to be answerable for one's actions, deeds, or fulfillment of duties."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "accountability",
                                "target_lang_sense_clarification": "state of being answerable"
                            },
                            {
                                "target_lang_word": "responsibility",
                                "target_lang_sense_clarification": "duty or obligation"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
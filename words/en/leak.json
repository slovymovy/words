                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вытекать",
                                "target_lang_sense_clarification": "литься наружу"
                            },
                            {
                                "target_lang_word": "просачиваться",
                                "target_lang_sense_clarification": "медленно проникать сквозь что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d65bf839-9675-34c9-8c36-f9071a4d4000",
                    "sense_definition": "To disclose secret or confidential information to the public.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "info_leak",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The unhappy employee <w>leaked</w> the documents to a national newspaper.",

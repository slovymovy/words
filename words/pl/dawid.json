                    "sense_id": "177b79b0-01a3-3e9b-baba-1a294058cb83",
                    "sense_definition": "imię męskie pochodzenia biblijnego",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "given_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mój brat <w>Dawid</w> chodzi do drugiej klasy szkoły podstawowej.",
                            "target_lang_translations": {
                                "ru": "Мой брат <w>Давид</w> ходит во второй класс начальной школы."
                            }
                        },
                        {
                            "text": "Cześć, mam na imię <w>Dawid</w> i miło mi cię poznać.",
                            "target_lang_translations": {
                                "ru": "Привет, меня зовут <w>Давид</w>, и я рад с тобой познакомиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "Dawidek"
                    ],
                    "common_phrases": [
                        "mam na imię Dawid",
                        "święty Dawid"
                    ],
                    "target_lang_definitions": {
                        "ru": "мужское имя библейского происхождения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Давид",
                                "target_lang_sense_clarification": "мужское имя"
                            }

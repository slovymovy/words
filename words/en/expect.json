{
    "word_family": [
        "expectation",
        "expectant",
        "expectancy",
        "unexpected"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "de23f440-6cf8-354c-bbf5-ca0cdef21ada",
                    "sense_definition": "To think or believe that something will happen or that someone will arrive.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "prediction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>expect</w> that it will rain later this afternoon."
                        },
                        {
                            "text": "We <w>expect</w> a reply from the company by tomorrow morning."
                        },
                        {
                            "text": "She did not <w>expect</w> to see her friend at the library."
                        }
                    ],
                    "synonyms": [
                        "anticipate"
                    ],
                    "antonyms": [
                        "doubt"
                    ],
                    "common_phrases": [
                        "expect the worst",
                        "expect a miracle",
                        "expect to find"
                    ]
                },
                {
                    "sense_id": "e4f0f6af-bc13-3532-9114-3497d64b95cb",
                    "sense_definition": "To require or demand something because it is a duty, obligation, or standard.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The school <w>expects</w> all students to wear a uniform."
                        },
                        {
                            "text": "You are <w>expected</w> to arrive at the office on time every day."
                        },
                        {
                            "text": "My parents <w>expect</w> too much of me when it comes to grades."
                        }
                    ],
                    "synonyms": [
                        "demand",
                        "call for"
                    ],
                    "common_phrases": [
                        "expect too much",
                        "be expected to",
                        "what to expect"
                    ]
                },
                {
                    "sense_id": "588ca3e2-d436-32c3-bdf1-110ca5d48a77",
                    "sense_definition": "To be pregnant; to wait for the birth of a child.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "pregnancy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My sister is <w>expecting</w> her first child in June."
                        },
                        {
                            "text": "They are <w>expecting</w> twins and need to buy a larger car."
                        }
                    ],
                    "common_phrases": [
                        "expecting a baby",
                        "she is expecting",
                        "expecting a child"
                    ]
                },
                {
                    "sense_id": "14d447e1-fdfa-3ba1-b4fe-0a9d7bff268d",
                    "sense_definition": "To wait for or await someone or something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "waiting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We shall remain here and <w>expect</w> his coming."
                        },
                        {
                            "text": "The knight knelt and <w>expected</w> the king's command."
                        }
                    ],
                    "synonyms": [
                        "await",
                        "wait for"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "This sense is largely obsolete in modern English, found mostly in older literature."
                        }
                    ]
                }
            ]
        }
    ]

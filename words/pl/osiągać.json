                {
                    "sense_id": "e5846a8d-c944-3be2-b748-840687534e17",
                    "sense_definition": "Zdobywać to, co się zamierzyło, lub realizować wyznaczony cel dzięki pracy i staraniom",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "goal_achievement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzięki systematycznej pracy uczeń <w>osiąga</w> coraz lepsze wyniki w nauce.",
                            "target_lang_translations": {
                                "ru": "Благодаря систематической работе ученик <w>добивается</w> всё лучших результатов в учёбе."
                            }
                        },
                        {
                            "text": "Firma od wielu lat <w>osiąga</w> sukcesy na rynku międzynarodowym.",
                            "target_lang_translations": {
                                "ru": "Компания уже много лет <w>достигает</w> успехов на международном рынке."
                            }
                        }
                    ],
                    "synonyms": [
                        "zdobywać",
                        "uzyskiwać"
                    ],
                    "common_phrases": [
                        "osiągać cel",
                        "osiągać sukces",
                        "osiągać wyniki",
                        "osiągać porozumienie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получать желаемое или реализовывать поставленную цель благодаря труду и стараниям"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добиваться",
                                "target_lang_sense_clarification": "прилагать усилия для получения чего-либо"
                            },
                            {
                                "target_lang_word": "достигать",
                                "target_lang_sense_clarification": "получать результат, доходить до цели"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26aa0563-5626-3efd-afff-29b719663929",
                    "sense_definition": "Dochodzić do określonej wartości liczbowej, poziomu lub wielkości",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "value_measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten nowoczesny pociąg bez trudu <w>osiąga</w> prędkość trzystu kilometrów na godzinę.",
                            "target_lang_translations": {
                                "ru": "Этот современный поезд без труда <w>достигает</w> скорости триста километров в час."
                            }
                        },
                        {
                            "text": "Latem temperatura w tym regionie często <w>osiąga</w> czterdzieści stopni.",
                            "target_lang_translations": {
                                "ru": "Летом температура в этом регионе часто <w>достигает</w> сорока градусов."
                            }
                        }
                    ],
                    "synonyms": [
                        "uzyskiwać"
                    ],
                    "common_phrases": [
                        "osiągać prędkość",
                        "osiągać temperaturę",
                        "osiągać poziom",
                        "osiągać wartość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доходить до определённого числового значения, уровня или величины"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "достигать",
                                "target_lang_sense_clarification": "доходить до какого-либо предела, уровня или размера"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ba01632-484c-337b-ae2e-8beab831a4ba",
                    "sense_definition": "Docierać do fizycznego miejsca przeznaczenia lub do określonego etapu w rozwoju",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "place_stage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas naszej długiej wędrówki powoli <w>osiągaliśmy</w> szczyt góry.",
                            "target_lang_translations": {
                                "ru": "Во время нашего долгого похода мы медленно <w>достигали</w> вершины горы."
                            }

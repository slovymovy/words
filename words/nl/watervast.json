            "senses": [
                {
                    "sense_id": "cc19b4d7-e400-36a9-b9d7-e2e39a69ffde",
                    "sense_definition": "Niet oplosbaar in of beschadigd door water; bestand tegen de inwerking van water.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heb je een <w>watervaste</w> stift om de dozen te labelen?",
                            "target_lang_translations": {
                                "en": "Do you have a <w>permanent</w> marker to label the boxes?",
                                "ru": "У тебя есть <w>несмываемый</w> маркер, чтобы подписать коробки?"
                            }
                        },
                        {
                            "text": "Deze mascara is <w>watervast</w> en blijft zitten tijdens het zwemmen.",
                            "target_lang_translations": {
                                "en": "This mascara is <w>waterproof</w> and stays on while swimming.",
                                "ru": "Эта тушь <w>водостойкая</w> и держится во время плавания."
                            }
                        }
                    ],
                    "synonyms": [
                        "waterbestendig"
                    ],
                    "antonyms": [
                        "wateroplosbaar",
                        "waterdoorlatend"
                    ],
                    "common_phrases": [
                        "watervaste stift",
                        "watervaste lijm",
                        "watervast multiplex"
                    ],
                    "target_lang_definitions": {
                        "en": "Not soluble in or damaged by water; resistant to the effects of water.",
                        "ru": "Нерастворимый в воде или не повреждаемый водой; устойчивый к воздействию воды."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "waterproof",
                                "target_lang_sense_clarification": "impervious to water"
                            },
                            {
                                "target_lang_word": "water-resistant",
                                "target_lang_sense_clarification": "able to resist the penetration of water to some degree"
                            },
                            {
                                "target_lang_word": "permanent",
                                "target_lang_sense_clarification": "specifically for markers or ink"
                            },
                            {
                                "target_lang_word": "indelible",
                                "target_lang_sense_clarification": "impossible to remove or wash away"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "водостойкий",

            "senses": [
                {
                    "sense_id": "f3648647-b37e-3460-9e08-bab3d46cae94",
                    "sense_definition": "Обозначение части текста или объекта, чтобы сделать его заметным или отличным от других (цветом, шрифтом, интонацией).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emphasis_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для удобства чтения сделайте <w>выделение</w> заголовков жирным шрифтом."
                        },
                        {
                            "text": "Цветовое <w>выделение</w> помогает быстро найти нужную информацию в таблице."
                        }
                    ],
                    "synonyms": [
                        "подчёркивание",
                        "акцентирование"
                    ],
                    "common_phrases": [
                        "выделение текста",
                        "цветовое выделение",
                        "выделение курсивом",
                        "интонационное выделение"
                    ]
                },
                {
                    "sense_id": "1af0a972-31e7-3e95-a1dd-7ca8c4f58cac",
                    "sense_definition": "Предоставление чего-либо (денег, ресурсов, земли) в чьё-либо распоряжение для определённой цели.",

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "f21cfb60-116d-3919-9b82-2c75df948496",
                    "sense_definition": "В средней степени, не слишком сильно и не слишком слабо; придерживаясь средней линии.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Цены в этом новом ресторане <w>умеренно</w> высокие.",
                            "target_lang_translations": {
                                "en": "The prices in this new restaurant are <w>moderately</w> high."
                            }
                        },
                        {
                            "text": "Врач посоветовал пациенту заниматься спортом <w>умеренно</w>, не перегружая организм.",
                            "target_lang_translations": {
                                "en": "The doctor advised the patient to exercise <w>moderately</w>, without overloading the body."
                            }
                        }
                    ],
                    "synonyms": [
                        "средне",
                        "сносно"
                    ],
                    "antonyms": [
                        "чрезмерно",
                        "слишком",
                        "экстремально"
                    ],
                    "common_phrases": [
                        "умеренно выраженный",
                        "умеренно сложный",
                        "умеренно пить"
                    ],
                    "target_lang_definitions": {
                        "en": "To a medium degree; neither too much nor too little."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "moderately",
                                "target_lang_sense_clarification": "general degree"
                            },
                            {
                                "target_lang_word": "reasonably",
                                "target_lang_sense_clarification": "fairly; often used regarding prices or expectations"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bfa08903-c2b3-3fde-881d-d3cf63388e1e",
                    "sense_definition": "О климате или температуре: будучи средним между жарким и холодным.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этой полосе преобладает <w>умеренно</w> континентальный климат.",
                            "target_lang_translations": {
                                "en": "A <w>moderately</w> continental climate prevails in this zone."
                            }
                        },
                        {
                            "text": "Лето в этом году выдалось <w>умеренно</w> тёплым и дождливым.",
                            "target_lang_translations": {
                                "en": "Summer this year turned out to be <w>moderately</w> warm and rainy."
                            }
                        }
                    ],
                    "synonyms": [
                        "мягко"
                    ],
                    "antonyms": [
                        "резко"
                    ],
                    "common_phrases": [
                        "умеренно континентальный",
                        "умеренно тёплый",
                        "умеренно холодный"
                    ],
                    "target_lang_definitions": {
                        "en": "Regarding climate or temperature: mild, neither very hot nor very cold."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "moderately",
                                "target_lang_sense_clarification": "used to modify adjectives like 'warm' or 'continental'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "525f237d-1011-3db3-8a56-2267803c89fe",
                    "sense_definition": "Сдержанно, без излишеств; скромно в желаниях и потребностях.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старый профессор жил очень <w>умеренно</w> и тратил деньги только на книги.",
                            "target_lang_translations": {
                                "en": "The old professor lived very <w>moderately</w> and spent money only on books."
                            }
                        },
                        {
                            "text": "Они старались питаться <w>умеренно</w>, исключив из рациона всё лишнее.",
                            "target_lang_translations": {
                                "en": "They tried to eat <w>in moderation</w>, excluding everything unnecessary from their diet."
                            }
                        }
                    ],
                    "synonyms": [
                        "сдержанно",
                        "скромно",
                        "аскетично"
                    ],
                    "antonyms": [
                        "неумеренно",
                        "расточительно"
                    ],
                    "common_phrases": [
                        "жить умеренно",
                        "питаться умеренно"
                    ],
                    "target_lang_definitions": {
                        "en": "In a restrained manner; without excess or extravagance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "in moderation",
                                "target_lang_sense_clarification": "idiomatic phrase, especially for eating or drinking"
                            },
                            {
                                "target_lang_word": "sparingly",
                                "target_lang_sense_clarification": "frugally; using very little of something"
                            },
                            {
                                "target_lang_word": "moderately",
                                "target_lang_sense_clarification": "modestly; without excess"
                            }
                        ]
                    }
                }
            ]
        },
        {

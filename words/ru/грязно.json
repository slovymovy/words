        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "f56abc38-4d2d-308e-81fe-e090e230cb23",
                    "sense_definition": "Оценочная характеристика места или пространства, где много грязи, мусора или пятен.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "literal_dirt",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На улице сегодня холодно и <w>грязно</w>.",
                            "target_lang_translations": {
                                "en": "It is cold and <w>muddy</w> outside today."
                            }
                        },
                        {
                            "text": "В этой комнате очень <w>грязно</w>, нужно срочно убраться.",
                            "target_lang_translations": {
                                "en": "It is very <w>dirty</w> in this room, we need to clean up urgently."
                            }
                        }
                    ],
                    "synonyms": [
                        "нечисто"
                    ],
                    "antonyms": [
                        "чисто"
                    ],
                    "common_phrases": [
                        "на улице грязно",
                        "здесь грязно",
                        "дома грязно"
                    ],
                    "target_lang_definitions": {
                        "en": "Describing a place or environment that is unclean, muddy, or messy."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dirty",
                                "target_lang_sense_clarification": "general state of being unclean"
                            },

                            }
                        },
                        {
                            "text": "На улице сегодня <чудесно>, давай погуляем в парке.",
                            "target_lang_translations": {
                                "en": "It's <w>wonderful</w> outside today, let's take a walk in the park."
                            }
                        }
                    ],
                    "synonyms": [
                        "прекрасно",
                        "хорошо",
                        "восхитительно"
                    ],
                    "antonyms": [
                        "ужасно",
                        "плохо"
                    ],
                    "common_phrases": [

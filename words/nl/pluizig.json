    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "34d910e4-7332-32b6-b28a-09c58d2a10f1",
                    "sense_definition": "Zacht en donzig van structuur, zoals de vacht van een jong dier of wol.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "texture_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het kleine konijntje heeft een witte, <w>pluizige</w> staart.",
                            "target_lang_translations": {
                                "en": "The little rabbit has a white, <w>fluffy</w> tail.",
                                "ru": "У маленького кролика белый <w>пушистый</w> хвостик."
                            }
                        },
                        {
                            "text": "Na het douchen wikkelde ze zich in een warme, <w>pluizige</w> handdoek.",
                            "target_lang_translations": {
                                "en": "After showering, she wrapped herself in a warm, <w>fluffy</w> towel.",
                                "ru": "После душа она завернулась в теплое <w>пушистое</w> полотенце."
                            }
                        }
                    ],
                    "synonyms": [
                        "donzig",
                        "wollig",
                        "zacht"
                    ],
                    "antonyms": [
                        "glad",
                        "kaal"
                    ],
                    "common_phrases": [
                        "een pluizig dier",
                        "pluizig haar",
                        "lekker pluizig"
                    ],
                    "target_lang_definitions": {
                        "en": "Soft and downy in structure, like the fur of a young animal or wool.",
                        "ru": "Мягкая и пушистая структура, напоминающая шерсть молодого животного или вату."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fluffy",
                                "target_lang_sense_clarification": "soft, airy, and light"
                            },
                            {
                                "target_lang_word": "fuzzy",
                                "target_lang_sense_clarification": "covered with fine, soft hair or fibers"
                            },
                            {
                                "target_lang_word": "downy",
                                "target_lang_sense_clarification": "resembling soft feathers or fine hair"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пушистый",
                                "target_lang_sense_clarification": "мягкий, покрытый мехом или пухом"
                            },
                            {
                                "target_lang_word": "мягкий",
                                "target_lang_sense_clarification": "приятный на ощупь, нежный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3e51cc3-3a29-396a-9741-4257808feb28",
                    "sense_definition": "Bedekt met losse vezels, bolletjes stof of pluisjes.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "texture_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zaden van de paardenbloem vormen samen een <w>pluizige</w> bol.",
                            "target_lang_translations": {
                                "en": "The seeds of the dandelion form a <w>fluffy</w> ball together.",
                                "ru": "Семена одуванчика образуют <w>пушистый</w> шар."
                            }
                        },
                        {
                            "text": "Mijn oude wollen trui is na het wassen erg <w>pluizig</w> geworden.",
                            "target_lang_translations": {
                                "en": "My old wool sweater has become very <w>fuzzy</w> after washing.",
                                "ru": "Мой старый шерстяной свитер стал очень <w>ворсистым</w> после стирки."
                            }
                        }
                    ],
                    "synonyms": [
                        "pluizerig"
                    ],
                    "antonyms": [
                        "glad"
                    ],
                    "common_phrases": [
                        "een pluizige trui",
                        "pluizige bollen"
                    ],
                    "target_lang_definitions": {
                        "en": "Covered with loose fibers, balls of dust, or lint.",
                        "ru": "Покрытый свободными волокнами, комочками пыли или пухом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fuzzy",
                                "target_lang_sense_clarification": "covered with loose fibers or fraying"
                            },
                            {
                                "target_lang_word": "linty",
                                "target_lang_sense_clarification": "covered with small pieces of fluff or lint"
                            },
                            {
                                "target_lang_word": "fluffy",
                                "target_lang_sense_clarification": "having a loose, light texture"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ворсистый",
                                "target_lang_sense_clarification": "имеющий ворс, шероховатый"
                            },
                            {
                                "target_lang_word": "мохнатый",
                                "target_lang_sense_clarification": "с торчащими волокнами или шерстью"
                            }
                        ]
                    }
                }
            ]
        }
    ]

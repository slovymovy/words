            "pos": "verb",
            "senses": [
                {
                    "sense_id": "25d8da39-e4a5-3293-8e89-7ac655642064",
                    "sense_definition": "Kracht uitoefenen op iets door ertegen te duwen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet op de rode knop <w>drukken</w> om de machine te stoppen.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно <w>нажать</w> на красную кнопку, чтобы остановить машину.",
                                "en": "You have to <w>press</w> the red button to stop the machine."
                            }
                        },
                        {
                            "text": "Hij <w>drukte</w> zijn neus tegen het koude raam om naar buiten te kijken.",
                            "target_lang_translations": {
                                "ru": "Он <w>прижал</w> нос к холодному окну, чтобы выглянуть наружу.",
                                "en": "He <w>pressed</w> his nose against the cold window to look outside."
                            }
                        }
                    ],
                    "synonyms": [
                        "duwen"
                    ],
                    "common_phrases": [
                        "op de knop drukken",
                        "tegen de borst drukken",
                        "de hand drukken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прикладывать силу к чему-либо, толкая это.",
                        "en": "To exert force on something by pushing against it."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нажимать",
                                "target_lang_sense_clarification": "на кнопку или клавишу"
                            },
                            {
                                "target_lang_word": "давить",
                                "target_lang_sense_clarification": "оказывать физическое давление"
                            },
                            {
                                "target_lang_word": "прижимать",
                                "target_lang_sense_clarification": "плотно прислонять к чему-либо"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "press",
                                "target_lang_sense_clarification": "to apply physical force"
                            },
                            {
                                "target_lang_word": "push",
                                "target_lang_sense_clarification": "to apply force to move or activate"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aaa0b542-a5f0-3d51-ac2d-76b228b5aa99",
                    "sense_definition": "Tekst of afbeeldingen op papier zetten met behulp van een machine.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De schrijver wil zijn nieuwe boek in een grote oplage laten <w>drukken</w>.",
                            "target_lang_translations": {
                                "ru": "Писатель хочет <w>напечатать</w> свою новую книгу большим тиражом.",
                                "en": "The writer wants to have his new book <w>printed</w> in a large edition."
                            }
                        },
                        {
                            "text": "Deze krant wordt elke nacht vers <w>gedrukt</w> en bezorgd.",
                            "target_lang_translations": {
                                "ru": "Эту газету <w>печатают</w> свежей каждую ночь и доставляют.",
                                "en": "This newspaper is <w>printed</w> fresh every night and delivered."
                            }
                        }
                    ],
                    "synonyms": [
                        "afdrukken"
                    ],
                    "common_phrases": [
                        "laten drukken",
                        "geld drukken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наносить текст или изображения на бумагу с помощью машины.",
                        "en": "To put text or images on paper using a machine."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "печатать",
                                "target_lang_sense_clarification": "издавать книги, газеты"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "print",
                                "target_lang_sense_clarification": "to produce books, newspapers, etc."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "177fdacb-b6b6-31cb-8819-393faf9ac199",
                    "sense_definition": "Zich ontlasten; zijn behoefte doen op het toilet.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De hond zat te <w>drukken</w> in het gras van de buren.",
                            "target_lang_translations": {
                                "ru": "Собака сидела и <w>тужилась</w> на соседском газоне.",
                                "en": "The dog was sitting and <w>straining</w> in the neighbors' grass."
                            }
                        },
                        {
                            "text": "Hij voelde dat hij moest <w>drukken</w> en zocht snel een wc.",
                            "target_lang_translations": {
                                "ru": "Он почувствовал, что ему нужно <w>тужиться</w>, и быстро искал туалет.",
                                "en": "He felt that he had to <w>poop</w> and quickly looked for a toilet."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontlasten",
                        "poepen"
                    ],
                    "common_phrases": [
                        "moeten drukken"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "informal usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Испражняться; тужиться в туалете.",
                        "en": "To relieve oneself; to do one's business on the toilet."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тужиться",
                                "target_lang_sense_clarification": "напрягаться при испражнении"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "poop",
                                "target_lang_sense_clarification": "informal: to defecate"
                            },
                            {
                                "target_lang_word": "strain",
                                "target_lang_sense_clarification": "to push physically during defecation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec80069e-eddb-33ee-ae36-90988f8f2f5a",
                    "sense_definition": "Zich onttrekken aan een taak of verplichting.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij probeerde <w>zich</w> te <w>drukken</w> toen er zwaar werk gedaan moest worden.",
                            "target_lang_translations": {
                                "ru": "Он пытался <w>увильнуть</w>, когда нужно было выполнять тяжелую работу.",
                                "en": "He tried to <w>shirk</w> when heavy work had to be done."
                            }
                        },
                        {
                            "text": "Je mag <w>je</w> niet <w>drukken</w> voor je verantwoordelijkheden in dit team.",
                            "target_lang_translations": {
                                "ru": "Ты не должен <w>уклоняться</w> от своих обязанностей в этой команде.",
                                "en": "You must not <w>shirk</w> your responsibilities in this team."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zich drukken",
                        "zich voor iets drukken"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used reflexively"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уклоняться от задачи или обязательства.",
                        "en": "To withdraw from a task or obligation."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "увиливать",
                                "target_lang_sense_clarification": "избегать работы"
                            },
                            {
                                "target_lang_word": "отлынивать",
                                "target_lang_sense_clarification": "разг. уклоняться от дела"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "shirk",
                                "target_lang_sense_clarification": "to avoid work or duty"
                            },
                            {
                                "target_lang_word": "duck",
                                "target_lang_sense_clarification": "informal: to avoid a responsibility"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b798b3f6-8851-32ae-83d9-1abbeec476bf",
                    "sense_definition": "Zich plat tegen de grond of een oppervlak maken om niet gezien te worden.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaat <w>drukte</w> <w>zich</w> plat tegen de aarde toen hij de vijand hoorde.",
                            "target_lang_translations": {
                                "ru": "Солдат плоско <w>прижался</w> к земле, когда услышал врага.",
                                "en": "The soldier <w>pressed</w> <w>himself</w> flat against the earth when he heard the enemy."
                            }
                        },
                        {
                            "text": "Het bange dier <w>drukte</w> <w>zich</w> tegen de muur in de hoop onzichtbaar te blijven.",
                            "target_lang_translations": {
                                "ru": "Испуганное животное <w>прижалось</w> к стене в надежде остаться невидимым.",
                                "en": "The frightened animal <w>pressed</w> <w>itself</w> against the wall in the hope of remaining invisible."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zich tegen de muur drukken",
                        "zich plat drukken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прижиматься к земле или поверхности, чтобы остаться незамеченным.",
                        "en": "To press oneself flat against the ground or a surface to avoid being seen."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прижиматься",
                                "target_lang_sense_clarification": "плотно прилегать всем телом"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "press",
                                "target_lang_sense_clarification": "reflexive: to flatten oneself against something"
                            }
                        ]
                    }
                }
            ]
        },

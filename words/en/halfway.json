    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "34ba7709-3541-3b33-ac09-50dfcbc283a7",
                    "sense_definition": "At or to a point equal in distance between two limits; in the middle of a process or journey.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was already <w>halfway</w> home when I realized I had forgotten my keys.",
                            "target_lang_translations": {
                                "ru": "Я был уже <w>на полпути</w> к дому, когда понял, что забыл ключи."
                            }
                        },
                        {
                            "text": "We are <w>halfway</w> through the project and everything is going according to plan.",
                            "target_lang_translations": {
                                "ru": "Мы выполнили проект <w>наполовину</w>, и всё идёт по плану."
                            }
                        },
                        {
                            "text": "The shop is located <w>halfway</w> between the bank and the post office.",
                            "target_lang_translations": {
                                "ru": "Магазин находится <w>посередине</w> между банком и почтой."
                            }
                        }
                    ],
                    "synonyms": [
                        "midway",
                        "partway"
                    ],
                    "common_phrases": [
                        "halfway through",
                        "halfway there",
                        "halfway between",
                        "meet halfway"
                    ],
                    "target_lang_definitions": {
                        "ru": "На середине пути или процесса; на равном расстоянии между двумя точками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на полпути",
                                "target_lang_sense_clarification": "на середине расстояния"
                            },
                            {
                                "target_lang_word": "наполовину",
                                "target_lang_sense_clarification": "о частичном завершении процесса"
                            },
                            {
                                "target_lang_word": "посередине",
                                "target_lang_sense_clarification": "между двумя объектами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "daa79214-4187-3fa3-95b3-7b93278b646c",
                    "sense_definition": "To some extent or degree; reasonably or moderately.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The food at the cafeteria was actually <w>halfway</w> decent today.",
                            "target_lang_translations": {
                                "ru": "Еда в столовой сегодня была <w>более-менее</w> приличной."
                            }
                        },
                        {
                            "text": "If you are <w>halfway</w> serious about this, you need to start practicing every day.",
                            "target_lang_translations": {
                                "ru": "Если ты <w>хоть сколько-нибудь</w> серьёзно относишься к этому, тебе нужно начать тренироваться каждый день."
                            }
                        }
                    ],
                    "common_phrases": [
                        "halfway decent"
                    ],
                    "traits": [
                        {

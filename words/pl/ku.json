            "senses": [
                {
                    "sense_id": "3b75e649-a21b-3016-9758-409aba3b8e88",
                    "sense_definition": "Przyimek wskazujący kierunek ruchu lub zwrócenia się w stronę kogoś lub czegoś.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zmęczony włóczęga powoli skierował się <w>ku</w> miastu.",
                            "target_lang_translations": {
                                "ru": "Уставший бродяга медленно направился <w>к</w> городу."
                            }
                        },
                        {
                            "text": "Statek płynął spokojnie <w>ku</w> odległemu brzegowi.",
                            "target_lang_translations": {
                                "ru": "Корабль спокойно плыл <w>к</w> далекому берегу."
                            }
                        }
                    ],
                    "synonyms": [
                        "k"
                    ],
                    "common_phrases": [
                        "ku górze",
                        "ku dołowi",
                        "ku sobie",
                        "ku słońcu",
                        "mieć się ku sobie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предлог, указывающий направление движения или обращения в сторону кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "направление движения"
                            },
                            {
                                "target_lang_word": "в сторону",
                                "target_lang_sense_clarification": "направление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "904d9e81-d32c-3d3b-b3f8-ba23ae7ab92e",
                    "sense_definition": "Przyimek wprowadzający określenie skutku zdarzenia lub uczucia, jakie ono wywołuje u kogoś.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "result_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uczeń, <w>ku</w> zgorszeniu nauczycielki, używał na lekcji wulgaryzmów.",
                            "target_lang_translations": {
                                "ru": "Ученик, <w>к</w> возмущению учительницы, использовал на уроке ругательства."
                            }
                        },
                        {
                            "text": "Wszystko skończyło się dobrze <w>ku</w> naszej wielkiej radości.",
                            "target_lang_translations": {
                                "ru": "Всё закончилось хорошо <w>к</w> нашей великой радости."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ku zaskoczeniu",
                        "ku przestrodze",
                        "ku pamięci",
                        "ku zadowoleniu",
                        "ku pokrzepieniu serc"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предлог, вводящий описание следствия события или чувства, которое оно вызывает у кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "выражение чувства или следствия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "413786e1-6ac2-363d-846f-d8feb7c47970",
                    "sense_definition": "Przyimek wskazujący na cel działania, przeznaczenie lub osobę, dla której coś jest czynione; używany głównie w kontekstach podniosłych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "purpose",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierze walczyli dzielnie <w>ku</w> chwale swojej ojczyzny.",
                            "target_lang_translations": {
                                "ru": "Солдаты храбро сражались <w>во</w> славу своей родины."
                            }
                        },
                        {
                            "text": "Pomnik ten został postawiony <w>ku</w> czci poległych bohaterów.",
                            "target_lang_translations": {
                                "ru": "Этот памятник был воздвигнут <w>в</w> честь павших героев."
                            }
                        }
                    ],
                    "synonyms": [
                        "dla"
                    ],
                    "common_phrases": [
                        "ku chwale",
                        "ku czci",
                        "ku ozdobie"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary style"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предлог, указывающий на цель действия, предназначение или лицо, для которого что-то делается; используется в основном в возвышенном контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "во",
                                "target_lang_sense_clarification": "в устойчивых выражениях, напр. «во славу»"
                            },
                            {
                                "target_lang_word": "в честь",

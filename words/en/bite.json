{
    "word_family": [
        "biter",
        "biting",
        "bite-sized"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2a81c873-d509-37cd-9652-b85fd4a8483f",
                    "sense_definition": "To cut into something by clamping the teeth.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "teeth_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As soon as you <w>bite</w> that sandwich, you'll know how good it is.",
                            "target_lang_translations": {
                                "ru": "Как только ты <w>откусишь</w> этот сэндвич, ты поймешь, насколько он хорош."
                            }
                        },
                        {
                            "text": "He took a fresh apple and <w>bit</w> into it.",
                            "target_lang_translations": {
                                "ru": "Он взял свежее яблоко и <w>надкусил</w> его."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bite into",
                        "bite off"
                    ],
                    "target_lang_definitions": {
                        "ru": "Откусывать или вгрызаться во что-то зубами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отку́сывать",
                                "target_lang_sense_clarification": "отделять кусок зубами (несов. вид)"
                            },
                            {
                                "target_lang_word": "откуси́ть",
                                "target_lang_sense_clarification": "отделить кусок зубами (сов. вид)"
                            },
                            {
                                "target_lang_word": "куса́ть",
                                "target_lang_sense_clarification": "вонзать зубы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7095f56-d6d3-3e82-801b-307f11b33f47",
                    "sense_definition": "To attack with the teeth.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "teeth_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That dog is about to <w>bite</w>!",
                            "target_lang_translations": {
                                "ru": "Эта собака сейчас <w>укусит</w>!"
                            }
                        },
                        {
                            "text": "Be careful, the dog might <w>bite</w> you if you get too close.",
                            "target_lang_translations": {
                                "ru": "Осторожно, собака может тебя <w>укусить</w>, если подойдешь слишком близко."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dog bites"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нападать, используя зубы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "куса́ть",
                                "target_lang_sense_clarification": "нападать (несов. вид)"
                            },
                            {
                                "target_lang_word": "укуси́ть",
                                "target_lang_sense_clarification": "напасть (сов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a7a9493-8207-3d68-b0a4-db8e56f3d3d5",
                    "sense_definition": "To sting.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "insect_bite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "These mosquitoes are really <w>biting</w> today!",
                            "target_lang_translations": {
                                "ru": "Эти комары сегодня сильно <w>кусаются</w>!"
                            }
                        },
                        {
                            "text": "I was <w>bitten</w> by a spider while gardening.",
                            "target_lang_translations": {
                                "ru": "Меня <w>укусил</w> паук, пока я работал в саду."
                            }
                        }
                    ],
                    "synonyms": [
                        "sting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жалить или кусать (о насекомых)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "куса́ть",
                                "target_lang_sense_clarification": "о комарах, мухах, пауках"
                            },
                            {
                                "target_lang_word": "жа́лить",
                                "target_lang_sense_clarification": "о пчелах, осах, крапиве"
                            },
                            {
                                "target_lang_word": "укуси́ть",
                                "target_lang_sense_clarification": "совершить укус"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7de98705-edf1-331c-96c3-5238c4c0c8ee",
                    "sense_definition": "To bite a baited hook or other lure and thus be caught.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Are the fish <w>biting</w> today?",
                            "target_lang_translations": {
                                "ru": "Рыба сегодня <w>клюёт</w>?"
                            }
                        },
                        {
                            "text": "We waited by the lake for hours, but nothing <w>bit</w>.",
                            "target_lang_translations": {
                                "ru": "Мы прождали у озера несколько часов, но ничего не <w>клюнуло</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Хватать наживку (о рыбе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клева́ть",
                                "target_lang_sense_clarification": "хватать приманку (несов. вид)"
                            },
                            {
                                "target_lang_word": "клю́нуть",
                                "target_lang_sense_clarification": "схватить приманку (сов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2efcee31-65d8-334b-9da8-d1635fde4ebd",
                    "sense_definition": "To have a significant effect, often negative.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_effect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "For homeowners with adjustable rate mortgages, rising interest will really <w>bite</w>.",
                            "target_lang_translations": {
                                "ru": "Для домовладельцев с плавающей ипотечной ставкой рост процентов будет сильно <w>кусаться</w>."
                            }
                        },
                        {
                            "text": "The new economic sanctions began to <w>bite</w> the country's economy.",
                            "target_lang_translations": {
                                "ru": "Новые экономические санкции начали <w>сказываться</w> на экономике страны."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказывать сильное, часто негативное воздействие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "куса́ться",
                                "target_lang_sense_clarification": "о ценах, морозе или затратах"
                            },
                            {
                                "target_lang_word": "ска́зываться",
                                "target_lang_sense_clarification": "иметь последствия"
                            },
                            {
                                "target_lang_word": "бить",
                                "target_lang_sense_clarification": "наносить ущерб (метафорически)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c900490d-6914-3b76-870c-abde26623ecf",
                    "sense_definition": "To lack quality; to be worthy of derision.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This music really <w>bites</w>.",
                            "target_lang_translations": {
                                "ru": "Эта музыка — полный <w>отстой</w>."
                            }
                        },
                        {
                            "text": "It <w>bites</w> that we have to work on the weekend.",
                            "target_lang_translations": {
                                "ru": "<w>Отстойно</w>, что нам приходится работать в выходные."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть плохого качества; вызывать раздражение (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть отсто́ем",
                                "target_lang_sense_clarification": "быть очень плохим (сленг)"
                            },
                            {
                                "target_lang_word": "беси́ть",
                                "target_lang_sense_clarification": "раздражать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e03c87f-a574-38ae-ae42-bf4eb3d2c0f2",
                    "sense_definition": "To take hold; to establish firm contact with.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I needed snow chains to make the tires <w>bite</w>.",
                            "target_lang_translations": {
                                "ru": "Мне понадобились цепи противоскольжения, чтобы шины <w>сцепились</w> с дорогой."
                            }
                        },
                        {
                            "text": "The anchor <w>bites</w> into the seabed to hold the ship.",
                            "target_lang_translations": {
                                "ru": "Якорь <w>вгрызается</w> в морское дно, чтобы удержать корабль."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Зацепляться; устанавливать прочный контакт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вгрыза́ться",
                                "target_lang_sense_clarification": "прочно зацепляться"
                            },
                            {
                                "target_lang_word": "сцепля́ться",
                                "target_lang_sense_clarification": "соединяться, схватываться"
                            },
                            {
                                "target_lang_word": "хвата́ть",
                                "target_lang_sense_clarification": "держать"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "35e643d3-20aa-3af5-9c3e-a8dc7cd16b71",
                    "sense_definition": "The wound left behind after having been bitten.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "teeth_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That snake <w>bite</w> really hurts!",
                            "target_lang_translations": {
                                "ru": "Этот змеиный <w>укус</w> очень болит!"
                            }
                        },
                        {
                            "text": "The doctor examined the dog <w>bite</w> on his leg.",
                            "target_lang_translations": {
                                "ru": "Врач осмотрел собачий <w>укус</w> на его ноге."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Рана, оставленная зубами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уку́с",
                                "target_lang_sense_clarification": "след от зубов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8868a702-874d-3f67-8fdf-b8abbaa0f2b1",
                    "sense_definition": "The swelling of one's skin caused by an insect's mouthparts or sting.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "insect_bite",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After just one night in the jungle I was covered with mosquito <w>bites</w>.",
                            "target_lang_translations": {
                                "ru": "Всего после одной ночи в джунглях я был покрыт комариными <w>укусами</w>."
                            }
                        },
                        {
                            "text": "Don't scratch that insect <w>bite</w> or it might get infected.",
                            "target_lang_translations": {
                                "ru": "Не чеши этот <w>укус</w> насекомого, иначе можно занести инфекцию."
                            }
                        }
                    ],
                    "synonyms": [
                        "sting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Припухлость на коже от укуса насекомого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уку́с",
                                "target_lang_sense_clarification": "след от насекомого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06248528-589a-3cd4-bbd7-d63cba4f2c66",
                    "sense_definition": "A small meal or snack.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "food_portion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Let's grab a <w>bite</w> to eat before the movie.",
                            "target_lang_translations": {
                                "ru": "Давай <w>перекусим</w> перед фильмом."
                            }
                        },
                        {
                            "text": "I'll have a quick <w>bite</w> to quiet my stomach until dinner.",
                            "target_lang_translations": {
                                "ru": "Я быстро <w>перекушу</w>, чтобы успокоить желудок до ужина."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bite to eat",
                        "grab a bite"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой прием пищи или закуска."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переку́с",
                                "target_lang_sense_clarification": "легкая еда"
                            },
                            {
                                "target_lang_word": "еда́",
                                "target_lang_sense_clarification": "пища (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49cf1991-7900-3c8c-af62-8286c274b515",
                    "sense_definition": "A piece of food of a size that would be produced by biting; a mouthful.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "food_portion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There were only a few <w>bites</w> left on the plate.",
                            "target_lang_translations": {
                                "ru": "На тарелке оставалось всего несколько <w>кусков</w>."
                            }
                        },
                        {
                            "text": "She was too sick to eat more than a <w>bite</w> of toast.",
                            "target_lang_translations": {
                                "ru": "Она была слишком больна, чтобы съесть больше одного <w>куска</w> тоста."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Кусок пищи, который можно откусить за один раз."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кусо́к",
                                "target_lang_sense_clarification": "часть пищи"
                            },
                            {
                                "target_lang_word": "глото́к",
                                "target_lang_sense_clarification": "малое количество (перен.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a03bb82b-5cd5-335c-b6f2-ddb8b60bc013",
                    "sense_definition": "The act of biting.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "teeth_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dog gave a warning growl before the <w>bite</w>.",
                            "target_lang_translations": {
                                "ru": "Собака издала предупреждающее рычание перед <w>укусом</w>."
                            }
                        },
                        {
                            "text": "He took a large <w>bite</w> from the sandwich.",
                            "target_lang_translations": {
                                "ru": "Он откусил большой <w>кусок</w> от сэндвича."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие кусания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уку́с",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8c2e25f-e05e-3b4b-b553-dc33541dfe1b",
                    "sense_definition": "Incisiveness, provocativeness, or exactness.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_effect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The article lacks <w>bite</w> and fails to address the real issues.",
                            "target_lang_translations": {
                                "ru": "Статье не хватает <w>остроты</w>, и она не затрагивает реальные проблемы."
                            }
                        },
                        {
                            "text": "Her comments had a sharp <w>bite</w> to them.",
                            "target_lang_translations": {
                                "ru": "В её комментариях была острая <w>язвительность</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Острота, язвительность или резкость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "острота́",
                                "target_lang_sense_clarification": "резкость, язвительность"
                            },
                            {
                                "target_lang_word": "хва́тка",
                                "target_lang_sense_clarification": "сила воздействия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5469117-bc7b-3fb4-89ef-d28a3efb6363",
                    "sense_definition": "A short extract from a recorded interview.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The news channel played a <w>bite</w> from the president's speech.",
                            "target_lang_translations": {
                                "ru": "Новостной канал проиграл <w>отрывок</w> из речи президента."
                            }
                        },
                        {
                            "text": "Politicians often speak in <w>bites</w> suitable for television.",
                            "target_lang_translations": {
                                "ru": "Политики часто говорят короткими <w>фразами</w>, подходящими для телевидения."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for sound bite"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткий отрывок из интервью или речи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отры́вок",
                                "target_lang_sense_clarification": "фрагмент записи"
                            },
                            {
                                "target_lang_word": "фрагме́нт",
                                "target_lang_sense_clarification": "часть выступления"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "ac328cdf-e271-3482-90e6-d274c13a9197",
                    "sense_definition": "A smooth, sweet food made by heating sugar until it melts and turns brown, often used as a sauce or flavoring.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I love the taste of salted <w>caramel</w> ice cream.",
                            "target_lang_translations": {
                                "ru": "Я обожаю вкус мороженого с солёной <w>карамелью</w>."
                            }
                        },
                        {
                            "text": "You need to heat the sugar slowly to make the <w>caramel</w> without burning it.",
                            "target_lang_translations": {
                                "ru": "Нужно медленно нагревать сахар, чтобы приготовить <w>карамель</w>, не пережарив её."
                            }
                        }
                    ],
                    "common_phrases": [
                        "salted caramel",
                        "caramel sauce",
                        "crème caramel",
                        "caramel apple"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гладкая сладкая масса, получаемая нагреванием сахара до коричневого цвета, часто используется как соус или добавка."
                    },
                    "translations": {
                        "ru": [
                            {

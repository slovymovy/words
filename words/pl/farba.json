            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ef37b7e5-4dc7-3640-9c81-99f1cca653cc",
                    "sense_definition": "Mieszanina barwników i substancji wiążących, służąca do malowania powierzchni w celu nadania jej koloru lub ochrony.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "paint_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy kupić białą <w>farbę</w>, żeby odnowić sufit w salonie.",
                            "target_lang_translations": {
                                "ru": "Нам нужно купить белую <w>краску</w>, чтобы обновить потолок в гостиной."
                            }
                        },
                        {
                            "text": "Uważaj na płaszcz, bo na ławce w parku jest świeża <w>farba</w>.",
                            "target_lang_translations": {
                                "ru": "Осторожно с пальто, на скамейке в парке свежая <w>краска</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "farba olejna",
                        "farba akrylowa",
                        "świeża farba",
                        "puszka farby"
                    ],
                    "target_lang_definitions": {
                        "ru": "Смесь пигментов и связующих веществ, используемая для окрашивания поверхности с целью придания ей цвета или защиты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "краска",
                                "target_lang_sense_clarification": "вещество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80b4b144-f315-3845-a3d0-e49df1a064dd",
                    "sense_definition": "Krew zwierzyny łownej (termin używany w łowiectwie).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "hunting_blood",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Myśliwy znalazł na śniegu krople <w>farby</w>, co świadczyło o trafieniu dzika.",
                            "target_lang_translations": {
                                "ru": "Охотник нашел на снегу капли <w>крови</w>, что свидетельствовало о ранении кабана."
                            }
                        },
                        {
                            "text": "Pies myśliwski zgubił trop, gdy ślad <w>farby</w> urwał się przy strumieniu.",
                            "target_lang_translations": {
                                "ru": "Охотничья собака потеряла след, когда след <w>крови</w> оборвался у ручья."
                            }
                        }
                    ],
                    "synonyms": [
                        "posoka"
                    ],
                    "common_phrases": [
                        "ślad farby",
                        "iść po farbie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кровь дичи (термин, используемый в охоте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кровь",
                                "target_lang_sense_clarification": "животного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3584dc3c-9a77-39ee-a319-a89cf38673d1",
                    "sense_definition": "Kosmetyk do makijażu twarzy, dawniej głównie róż lub bielidło.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "makeup_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aktorka zmyła z twarzy grubą warstwę scenicznej <w>farby</w> po zakończonym spektaklu.",
                            "target_lang_translations": {
                                "ru": "Актриса смыла с лица толстый слой сценического <w>грима</w> после окончания спектакля."
                            }
                        },
                        {
                            "text": "Jej policzki były nienaturalnie czerwone od taniej <w>farby</w>.",
                            "target_lang_translations": {
                                "ru": "Её щеки были неестественно красными от дешевой <w>краски</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nałożyć farbę"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "określenie używane dawniej lub w kontekście teatralnym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Косметическое средство для макияжа лица, ранее в основном румяна или белила."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грим",
                                "target_lang_sense_clarification": "театральный"
                            },
                            {
                                "target_lang_word": "краска",
                                "target_lang_sense_clarification": "косметика, разг."
                            }
                        ]
                    }
                }
            ]
        }
    ]

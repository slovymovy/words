    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "53c63f49-5a78-35ee-b088-913a98031a73",
                    "sense_definition": "A coursebook, a formal manual of instruction in a specific subject, especially one for use in schools or colleges.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher asked the students to open their <w>textbook</w> to page fifty.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил учеников открыть <w>учебник</w> на пятидесятой странице."
                            }
                        },
                        {
                            "text": "She bought a used chemistry <w>textbook</w> to save money for the semester.",
                            "target_lang_translations": {
                                "ru": "Она купила подержанный <w>учебник</w> по химии, чтобы сэкономить деньги на семестр."
                            }
                        }
                    ],
                    "synonyms": [
                        "coursebook",
                        "manual",
                        "schoolbook"
                    ],
                    "common_phrases": [
                        "history textbook",
                        "science textbook",
                        "school textbook",
                        "college textbook"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учебная книга, официальное руководство по обучению определенному предмету, особенно для использования в школах или колледжах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "учебник",
                                "target_lang_sense_clarification": "основная книга для обучения предмету"
                            },
                            {
                                "target_lang_word": "учебное пособие",
                                "target_lang_sense_clarification": "книга или руководство для помощи в обучении"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "08c6e51f-c85a-3ae5-aa48-37fd895b405c",
                    "sense_definition": "Having the typical characteristics of a specific class of phenomenon; classic or quintessential.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "adj_standard",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The patient displayed all the <w>textbook</w> symptoms of a severe flu infection.",
                            "target_lang_translations": {
                                "ru": "У пациента проявились все <w>хрестоматийные</w> симптомы тяжелой гриппозной инфекции."
                            }
                        },
                        {
                            "text": "This is a <w>textbook</w> example of how supply and demand affect prices.",
                            "target_lang_translations": {
                                "ru": "Это <w>хрестоматийный</w> пример того, как спрос и предложение влияют на цены."
                            }
                        }
                    ],
                    "synonyms": [
                        "classic",
                        "typical",
                        "quintessential",
                        "standard"
                    ],
                    "antonyms": [
                        "atypical",
                        "unusual"
                    ],
                    "common_phrases": [
                        "textbook example",
                        "textbook case",
                        "textbook symptoms"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий типичными характеристиками определенного класса явлений; классический или наиболее показательный пример."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хрестоматийный",
                                "target_lang_sense_clarification": "типичный, как в учебнике"
                            },
                            {
                                "target_lang_word": "классический",
                                "target_lang_sense_clarification": "традиционный, образцовый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6db8f81b-7d00-38a5-ae63-0d3f77b77e13",
                    "sense_definition": "Done exactly correctly or in an exemplary way, according to standard procedures.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "adj_standard",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pilot executed a <w>textbook</w> landing despite the difficult weather conditions.",
                            "target_lang_translations": {
                                "ru": "Пилот совершил <w>образцовую</w> посадку, несмотря на сложные погодные условия."
                            }
                        },
                        {
                            "text": "Her management of the crisis was absolutely <w>textbook</w> and saved the company.",
                            "target_lang_translations": {
                                "ru": "Её управление кризисом было абсолютно <w>образцовым</w> и спасло компанию."
                            }
                        }
                    ],
                    "synonyms": [
                        "perfect",
                        "flawless",
                        "exemplary",
                        "model"
                    ],
                    "antonyms": [
                        "flawed",
                        "botched"
                    ],
                    "common_phrases": [
                        "textbook landing",
                        "textbook maneuver",
                        "absolutely textbook"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполненный совершенно правильно или образцово, в строгом соответствии со стандартными процедурами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "образцовый",
                                "target_lang_sense_clarification": "выполненный идеально, для подражания"
                            },
                            {
                                "target_lang_word": "эталонный",
                                "target_lang_sense_clarification": "соответствующий высшему стандарту"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7495c22-99ba-38ff-8fb0-bbe27eb72bb6",
                    "sense_definition": "Learned from a book rather than from practical experience; theoretical or academic.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has a <w>textbook</w> knowledge of the law but lacks actual courtroom experience.",
                            "target_lang_translations": {
                                "ru": "У него <w>книжное</w> знание закона, но нет реального опыта работы в суде."
                            }
                        },
                        {
                            "text": "Her approach to the problem was too <w>textbook</w> and failed in the real world.",
                            "target_lang_translations": {
                                "ru": "Её подход к проблеме был слишком <w>теоретическим</w> и провалился в реальной жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "theoretical",
                        "academic",
                        "abstract"
                    ],
                    "antonyms": [
                        "practical",
                        "empirical",
                        "hands-on"
                    ],
                    "common_phrases": [
                        "textbook knowledge",
                        "textbook understanding"
                    ],
                    "target_lang_definitions": {
                        "ru": "Почерпнутый из книг, а не из практического опыта; теоретический или оторванный от реальности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "книжный",
                                "target_lang_sense_clarification": "оторванный от практики"
                            },
                            {
                                "target_lang_word": "теоретический",
                                "target_lang_sense_clarification": "основанный только на теории"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
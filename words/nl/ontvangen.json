                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "received",
                                "target_lang_sense_clarification": "that has been obtained or delivered"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "полученный",

{
    "word_family": [
        "agreement",
        "disagreement",
        "agreeable",
        "disagreeable",
        "disagree"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e41f9f14-122f-3b65-996e-02d60168b70e",
                    "sense_definition": "To have the same opinion or reach a shared decision about something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "opinion_harmony",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I completely <w>agree</w> with your decision to stay home.",
                            "target_lang_translations": {
                                "ru": "Я полностью <w>согласен</w> с твоим решением остаться дома."
                            }
                        },
                        {
                            "text": "We need to <w>agree</w> on a time for the meeting.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>договориться</w> о времени встречи."
                            }
                        }
                    ],
                    "synonyms": [
                        "concur",
                        "harmonize"
                    ],
                    "antonyms": [
                        "disagree"
                    ],
                    "common_phrases": [
                        "agree with someone",
                        "agree on something",
                        "agree about something",
                        "agree to disagree"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь то же мнение или прийти к общему решению по какому-либо вопросу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соглашаться",
                                "target_lang_sense_clarification": "иметь общее мнение"
                            },
                            {
                                "target_lang_word": "договариваться",
                                "target_lang_sense_clarification": "приходить к общему решению"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b3693c0-67c1-391f-a1e7-fa774d5627ae",
                    "sense_definition": "To say yes to a suggestion, plan, or request; to give permission.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "consent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>agreed</w> to help us with the project next week.",
                            "target_lang_translations": {
                                "ru": "Она <w>согласилась</w> помочь нам с проектом на следующей неделе."
                            }
                        },
                        {
                            "text": "The bank has <w>agreed</w> to lend him the money he needs.",
                            "target_lang_translations": {
                                "ru": "Банк <w>согласился</w> одолжить ему необходимые деньги."
                            }
                        }
                    ],
                    "synonyms": [
                        "consent",
                        "assent",
                        "accede"
                    ],
                    "common_phrases": [
                        "agree to do",
                        "agree to terms",
                        "agree to a proposal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ответить согласием на предложение, план или просьбу; дать разрешение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соглашаться",
                                "target_lang_sense_clarification": "принимать предложение или просьбу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3bac19e3-e03f-38e1-ac7f-9e46e13124ac",
                    "sense_definition": "To be suitable or healthy for someone's constitution (usually used in the negative regarding food).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "suitability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I love spicy food, but unfortunately it does not <w>agree</w> with me.",
                            "target_lang_translations": {
                                "ru": "Я люблю острую еду, но, к сожалению, она мне не <w>подходит</w>."
                            }
                        },
                        {
                            "text": "The warm coastal climate really <w>agrees</w> with her health.",
                            "target_lang_translations": {
                                "ru": "Тёплый морской климат очень <w>подходит</w> её здоровью."
                            }
                        }
                    ],
                    "synonyms": [
                        "suit",
                        "fit"
                    ],
                    "common_phrases": [
                        "not agree with me",
                        "agree with someone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть подходящим или полезным для чьего-либо организма (обычно используется в отрицании по отношению к еде)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подходить",
                                "target_lang_sense_clarification": "быть приемлемым для организма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f9e9eb1-00c4-3564-8a29-8bd2c3fe54c6",
                    "sense_definition": "To be consistent with or match another fact, statement, or account.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "correspondence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His version of the story does not <w>agree</w> with the police report.",
                            "target_lang_translations": {
                                "ru": "Его версия событий не <w>соответствует</w> полицейскому отчёту."
                            }
                        },
                        {
                            "text": "The numbers in your spreadsheet must <w>agree</w> with the bank statement.",
                            "target_lang_translations": {
                                "ru": "Цифры в вашей таблице должны <w>сходиться</w> с банковской выпиской."
                            }
                        }
                    ],
                    "synonyms": [
                        "correspond",
                        "tally",
                        "match",
                        "coincide"
                    ],
                    "antonyms": [
                        "clash"
                    ],
                    "common_phrases": [
                        "agree with the facts",
                        "agree with the evidence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соответствовать или совпадать с другим фактом, утверждением или отчетом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соответствовать",
                                "target_lang_sense_clarification": "совпадать с фактами"
                            },
                            {
                                "target_lang_word": "сходиться",
                                "target_lang_sense_clarification": "совпадать (о цифрах или данных)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c60fc915-ec91-3c38-ad76-37cbc5b49ce0",
                    "sense_definition": "To decide on or settle a price, deal, or plan together.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "settlement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They managed to <w>agree</w> a price for the house after hours of negotiation.",
                            "target_lang_translations": {
                                "ru": "Им удалось <w>согласовать</w> цену дома после многочасовых переговоров."
                            }
                        },
                        {
                            "text": "The two companies have <w>agreed</w> a deal to share technology.",
                            "target_lang_translations": {
                                "ru": "Две компании <w>согласовали</w> сделку по обмену технологиями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "agree a price",
                        "agree a deal",
                        "agree a contract"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совместно установить или утвердить цену, сделку или план."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "согласовывать",
                                "target_lang_sense_clarification": "утверждать детали сделки"
                            },
                            {
                                "target_lang_word": "договариваться",
                                "target_lang_sense_clarification": "приходить к соглашению о чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95357a47-3c6c-32a5-b14f-c575eeabf5ca",
                    "sense_definition": "To correspond to another word in grammatical categories such as gender, number, or case.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "correspondence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In English, the verb must always <w>agree</w> with the subject.",
                            "target_lang_translations": {
                                "ru": "В английском языке глагол всегда должен <w>согласовываться</w> с подлежащим."
                            }
                        },
                        {
                            "text": "Adjectives in Spanish <w>agree</w> in gender and number with the nouns they modify.",
                            "target_lang_translations": {
                                "ru": "Прилагательные в испанском языке <w>согласуются</w> в роде и числе с существительными, которые они определяют."
                            }
                        }
                    ],
                    "synonyms": [
                        "correspond"
                    ],
                    "common_phrases": [
                        "agree with the subject",
                        "agree in number",
                        "agree in gender"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "grammar"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Соответствовать другому слову в грамматических категориях, таких как род, число или падеж."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "согласовываться",
                                "target_lang_sense_clarification": "грамматическое соответствие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
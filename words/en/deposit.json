{
    "word_family": [
        "depositor",
        "depository",
        "deposition",
        "redeposit"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f0f7e27b-e78e-3f7b-82f4-5856bc1a77cc",
                    "sense_definition": "Money placed in a bank account, usually to keep it safe or to earn interest.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "banking_finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to go to the bank to make a <w>deposit</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно пойти в банк, чтобы сделать <w>вклад</w>."
                            }
                        },
                        {
                            "text": "The monthly <w>deposit</w> from my employer appeared in my account this morning.",
                            "target_lang_translations": {
                                "ru": "Ежемесячное <w>поступление</w> от моего работодателя появилось на моем счету сегодня утром."
                            }
                        }
                    ],
                    "antonyms": [
                        "withdraw"
                    ],
                    "common_phrases": [
                        "make a deposit",
                        "cash deposit",
                        "direct deposit",
                        "bank deposit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деньги, помещенные на банковский счет, обычно для хранения или получения процентов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вклад",
                                "target_lang_sense_clarification": "банковский"
                            },
                            {
                                "target_lang_word": "депозит",
                                "target_lang_sense_clarification": "финансовый термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af47f212-3dc7-3b5c-a2e1-dc66c0b8ea15",
                    "sense_definition": "A sum of money paid as a first installment to reserve an item or show good faith before the full payment is made.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "commerce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We put down a <w>deposit</w> on a new apartment yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера мы внесли <w>задаток</w> за новую квартиру."
                            }
                        },
                        {
                            "text": "The store requires a small <w>deposit</w> to hold the item for you.",
                            "target_lang_translations": {
                                "ru": "Магазин требует небольшой <w>задаток</w>, чтобы отложить товар для вас."
                            }
                        }
                    ],
                    "synonyms": [
                        "down payment"
                    ],
                    "common_phrases": [
                        "put down a deposit",
                        "pay a deposit",
                        "non-refundable deposit",
                        "refundable deposit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сумма денег, выплачиваемая в качестве первого взноса для бронирования товара или подтверждения намерений до внесения полной оплаты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задаток",
                                "target_lang_sense_clarification": "предоплата для бронирования"
                            },
                            {
                                "target_lang_word": "взнос",
                                "target_lang_sense_clarification": "часть оплаты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0898682-0f3a-3bf4-a4e2-0f3eb52fa835",
                    "sense_definition": "A sum of money given as security for a borrowed item or rental, which is returned when the item is brought back in good condition.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "commerce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You will get your security <w>deposit</w> back when you move out of the house.",
                            "target_lang_translations": {
                                "ru": "Вы получите свой страховой <w>залог</w> обратно, когда будете съезжать из дома."
                            }
                        },
                        {
                            "text": "There is a small <w>deposit</w> on glass bottles that you can claim by returning them.",
                            "target_lang_translations": {
                                "ru": "За стеклянные бутылки взимается небольшой <w>залог</w>, который можно вернуть, сдав их."
                            }
                        }
                    ],
                    "common_phrases": [
                        "security deposit",
                        "bottle deposit",
                        "damage deposit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сумма денег, передаваемая в качестве обеспечения за арендованный предмет или недвижимость, которая возвращается при возврате предмета в хорошем состоянии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "залог",
                                "target_lang_sense_clarification": "обеспечение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6d4202a-1b88-359c-b1b6-ebc6ac857218",
                    "sense_definition": "A layer or mass of accumulated matter, such as sediment, rock, or ore, found naturally in the ground.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_geology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Geologists discovered a rich <w>deposit</w> of gold in the mountains.",
                            "target_lang_translations": {
                                "ru": "Геологи обнаружили богатое <w>месторождение</w> золота в горах."
                            }
                        },
                        {
                            "text": "The river left a thick <w>deposit</w> of mud on its banks after the flood.",
                            "target_lang_translations": {
                                "ru": "После наводнения река оставила на берегах толстый слой <w>отложений</w> ила."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mineral deposit",
                        "oil deposit",
                        "rich deposit",
                        "sedimentary deposit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слой или масса скопившегося вещества, такого как осадочные породы или руда, встречающиеся в природе в земле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "месторождение",
                                "target_lang_sense_clarification": "полезных ископаемых"
                            },
                            {
                                "target_lang_word": "залежь",
                                "target_lang_sense_clarification": "пласт породы"
                            },
                            {
                                "target_lang_word": "отложение",
                                "target_lang_sense_clarification": "осадочные породы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35a3d46a-1cfb-3a54-aa46-acc02d8a674c",
                    "sense_definition": "A substance or layer that has been left on a surface, often gradually over time.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_geology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hard water can leave a white <w>deposit</w> on your faucets.",
                            "target_lang_translations": {
                                "ru": "Жесткая вода может оставлять белый <w>налет</w> на кранах."
                            }
                        },
                        {
                            "text": "The doctor identified a calcium <w>deposit</w> in the patient's shoulder joint.",
                            "target_lang_translations": {
                                "ru": "Врач обнаружил <w>отложение</w> кальция в плечевом суставе пациента."
                            }
                        }
                    ],
                    "common_phrases": [
                        "calcium deposit",
                        "fatty deposit",
                        "scale deposit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещество или слой, оставшийся на поверхности, часто накапливающийся постепенно с течением времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "налет",
                                "target_lang_sense_clarification": "тонкий слой на поверхности"
                            },
                            {
                                "target_lang_word": "отложение",
                                "target_lang_sense_clarification": "накопление вещества"
                            },
                            {
                                "target_lang_word": "осадок",
                                "target_lang_sense_clarification": "остаток вещества"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2696d276-071b-335e-8b98-7e72d686bb32",
                    "sense_definition": "To put money into a bank account.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "banking_finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>deposit</w> this check before the bank closes.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>положить</w> этот чек на счет до закрытия банка."
                            }
                        },
                        {
                            "text": "She <w>deposited</w> half of her salary into a savings account.",
                            "target_lang_translations": {
                                "ru": "Она <w>положила</w> половину своей зарплаты на сберегательный счет."
                            }
                        }
                    ],
                    "antonyms": [
                        "withdraw"
                    ],
                    "common_phrases": [
                        "deposit money",
                        "deposit a check",
                        "deposit funds",
                        "deposit cash"
                    ],
                    "target_lang_definitions": {
                        "ru": "Класть деньги на банковский счет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "класть",
                                "target_lang_sense_clarification": "несов. вид"
                            },
                            {
                                "target_lang_word": "положить",
                                "target_lang_sense_clarification": "сов. вид"
                            },
                            {
                                "target_lang_word": "вносить",
                                "target_lang_sense_clarification": "официально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c2c7c9e-240e-3736-8695-eb1c3f14b4a2",
                    "sense_definition": "To put something down in a specific place, or to leave a layer of substance on a surface.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_geology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The floodwaters <w>deposited</w> a layer of silt over the fields.",
                            "target_lang_translations": {
                                "ru": "Паводковые воды <w>нанесли</w> слой ила на поля."
                            }
                        },
                        {
                            "text": "Sea turtles come to the beach to <w>deposit</w> their eggs in the sand.",
                            "target_lang_translations": {
                                "ru": "Морские черепахи приплывают на пляж, чтобы <w>отложить</w> яйца в песок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "deposit eggs",
                        "deposit sediment",
                        "deposit waste"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставлять что-либо в определенном месте или оставлять слой вещества на поверхности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "откладывать",
                                "target_lang_sense_clarification": "яйца, вещество"
                            },
                            {
                                "target_lang_word": "наносить",
                                "target_lang_sense_clarification": "слой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f94cdbb-64ae-3abc-a821-1aebb3c3a908",
                    "sense_definition": "To place something somewhere for safekeeping or storage.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "general_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Guests are advised to <w>deposit</w> their valuables in the hotel safe.",
                            "target_lang_translations": {
                                "ru": "Гостям рекомендуется <w>сдать</w> свои ценности в сейф отеля."
                            }
                        },
                        {
                            "text": "He <w>deposited</w> his heavy luggage at the station lockers.",
                            "target_lang_translations": {
                                "ru": "Он <w>оставил</w> свой тяжелый багаж в камерах хранения на вокзале."
                            }
                        }
                    ],
                    "common_phrases": [
                        "deposit valuables",
                        "deposit items"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать что-либо куда-либо для сохранности или хранения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдавать",
                                "target_lang_sense_clarification": "на хранение"
                            },
                            {
                                "target_lang_word": "оставлять",
                                "target_lang_sense_clarification": "вещи"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
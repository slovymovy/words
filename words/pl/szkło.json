                {
                    "sense_id": "1c665289-2160-3f46-a461-e18006f3fce3",
                    "sense_definition": "Twardy, kruchy i przezroczysty materiał produkowany z piasku, używany do wyrobu szyb, butelek i naczyń.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "material_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uważaj, na podłodze leży rozbite <w>szkło</w>.",
                            "target_lang_translations": {
                                "ru": "Осторожно, на полу лежит разбитое <w>стекло</w>."
                            }
                        },
                        {
                            "text": "To nowoczesne biuro jest zbudowane głównie ze stali i <w>szkła</w>.",
                            "target_lang_translations": {
                                "ru": "Этот современный офис построен в основном из стали и <w>стекла</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szkło hartowane",
                        "szkło powiększające",
                        "włókno szklane",
                        "dmuchanie szkła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердый, хрупкий и прозрачный материал, изготавливаемый из песка, используемый для производства окон, бутылок и посуды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стекло",
                                "target_lang_sense_clarification": "материал"

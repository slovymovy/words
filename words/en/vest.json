{
    "word_family": [
        "invest",
        "divest",
        "vestment",
        "investment",
        "investor"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c19b0015-ef66-301a-b991-92452460ba79",
                    "sense_definition": "A sleeveless piece of clothing worn on the upper body under other clothes to keep warm.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "clothing_underwear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He put on a warm <w>vest</w> under his shirt because it was freezing outside.",
                            "target_lang_translations": {
                                "ru": "Он надел теплую <w>майку</w> под рубашку, потому что на улице было очень холодно."
                            }
                        },
                        {
                            "text": "She usually wears a cotton <w>vest</w> to sleep in during the summer.",
                            "target_lang_translations": {
                                "ru": "Летом она обычно надевает хлопковую <w>майку</w>, чтобы спать в ней."
                            }
                        }
                    ],
                    "synonyms": [
                        "undershirt",
                        "singlet"
                    ],
                    "common_phrases": [
                        "wear a vest",
                        "cotton vest",
                        "thermal vest"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mainly UK"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет одежды без рукавов, который носят на верхней части тела под другой одеждой для тепла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "майка",
                                "target_lang_sense_clarification": "нижнее белье"
                            },
                            {
                                "target_lang_word": "фуфайка",
                                "target_lang_sense_clarification": "утепленная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f06498d2-891d-322c-becf-829080d8fd81",
                    "sense_definition": "A sleeveless garment with buttons down the front, usually worn over a shirt and under a suit jacket.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "clothing_formal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The groom looked elegant in a black tuxedo and a silver <w>vest</w>.",
                            "target_lang_translations": {
                                "ru": "Жених выглядел элегантно в черном смокинге и серебряном <w>жилете</w>."
                            }
                        },
                        {
                            "text": "He took off his jacket to reveal a matching <w>vest</w> underneath.",
                            "target_lang_translations": {
                                "ru": "Он снял пиджак, чтобы показать подходящий <w>жилет</w> под ним."
                            }
                        }
                    ],
                    "synonyms": [
                        "waistcoat"
                    ],
                    "common_phrases": [
                        "three-piece suit",
                        "wear a vest"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mainly US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет одежды без рукавов с пуговицами спереди, который обычно носят поверх рубашки и под пиджаком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жилет",
                                "target_lang_sense_clarification": "классический костюмный"
                            },
                            {
                                "target_lang_word": "жилетка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2123743-fa5c-36ce-8740-f407da64b0f5",
                    "sense_definition": "A special sleeveless jacket worn to protect the body, carry equipment, or to be easily seen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "clothing_protective",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police officer was wearing a bulletproof <w>vest</w> for protection.",
                            "target_lang_translations": {
                                "ru": "Полицейский был одет в пуленепробиваемый <w>жилет</w> для защиты."
                            }
                        },
                        {
                            "text": "You must wear a bright safety <w>vest</w> when working on the road.",
                            "target_lang_translations": {
                                "ru": "Вы должны носить яркий сигнальный <w>жилет</w> при работе на дороге."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bulletproof vest",
                        "safety vest",
                        "life vest",
                        "reflective vest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специальная куртка без рукавов, которую носят для защиты тела, переноски снаряжения или для заметности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жилет",
                                "target_lang_sense_clarification": "защитный или сигнальный"
                            },
                            {
                                "target_lang_word": "бронежилет",
                                "target_lang_sense_clarification": "защитный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "117e0563-89f5-3fca-9043-7191a617665d",
                    "sense_definition": "A sleeveless top worn by athletes or sports teams, often showing team colors.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "clothing_sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Every runner was given a numbered <w>vest</w> before the marathon began.",
                            "target_lang_translations": {
                                "ru": "Каждому бегуну выдали пронумерованную <w>майку</w> перед началом марафона."
                            }
                        },
                        {
                            "text": "She put on her running <w>vest</w> and headed to the track.",
                            "target_lang_translations": {
                                "ru": "Она надела свою беговую <w>майку</w> и направилась на трек."
                            }
                        }
                    ],
                    "synonyms": [
                        "tank top",
                        "singlet"
                    ],
                    "common_phrases": [
                        "running vest",
                        "training vest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спортивная майка без рукавов, которую носят спортсмены, часто цветов команды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "майка",
                                "target_lang_sense_clarification": "спортивная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11590d78-80dd-3f91-b8b2-170394582dc1",
                    "sense_definition": "A ceremonial robe or gown, especially one worn by clergy.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "clothing_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The priest stood by the altar dressed in a long white <w>vest</w>.",
                            "target_lang_translations": {
                                "ru": "Священник стоял у алтаря, одетый в длинное белое <w>облачение</w>."
                            }
                        },
                        {
                            "text": "Figures in ancient paintings are often depicted in a flowing <w>vest</w>.",
                            "target_lang_translations": {
                                "ru": "Фигуры на древних картинах часто изображены в струящейся <w>ризе</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Церемониальное одеяние или мантия, особенно носимая духовенством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "облачение",
                                "target_lang_sense_clarification": "церковное"
                            },
                            {
                                "target_lang_word": "риза",
                                "target_lang_sense_clarification": "устаревшее"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8df3d7e6-1998-3a1c-9791-a6923efe5650",
                    "sense_definition": "To give someone the legal right, power, or authority to do something or own something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_authority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The constitution <w>vests</w> the power to make laws in the parliament.",
                            "target_lang_translations": {
                                "ru": "Конституция <w>наделяет</w> парламент властью издавать законы."
                            }
                        },
                        {
                            "text": "He has a <w>vested</w> interest in the outcome of this business deal.",
                            "target_lang_translations": {
                                "ru": "У него есть <w>личная</w> заинтересованность в исходе этой сделки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "vested interest",
                        "power vested in",
                        "vested with"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal or legal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наделять кого-либо законным правом, властью или полномочиями делать что-либо или владеть чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наделять",
                                "target_lang_sense_clarification": "правом или властью"
                            },
                            {
                                "target_lang_word": "облекать",
                                "target_lang_sense_clarification": "властью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd0d38df-fc01-3da9-a609-0100e97be30e",
                    "sense_definition": "To become the permanent property of an employee, especially regarding shares or pension benefits.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her stock options will fully <w>vest</w> after four years of employment.",
                            "target_lang_translations": {
                                "ru": "Опционы на ее акции полностью <w>перейдут в собственность</w> через четыре года работы."
                            }
                        },
                        {
                            "text": "If you leave the company early, your retirement benefits may not <w>vest</w>.",
                            "target_lang_translations": {
                                "ru": "Если вы покинете компанию раньше, ваши пенсионные накопления могут не <w>закрепиться</w> за вами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "options vest",
                        "fully vested"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "business or finance"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Переходить в полную собственность сотрудника (об акциях или пенсионных накоплениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переходить в собственность",
                                "target_lang_sense_clarification": "об активах"
                            },
                            {
                                "target_lang_word": "закрепляться",
                                "target_lang_sense_clarification": "о правах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37118780-dcaf-3fdb-bcb3-884ab96a9fbd",
                    "sense_definition": "To dress or clothe someone, often in ceremonial robes.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "clothing_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king was <w>vested</w> in purple robes for the ceremony.",
                            "target_lang_translations": {
                                "ru": "Король был <w>облачен</w> в пурпурные мантии для церемонии."
                            }
                        },
                        {
                            "text": "They <w>vested</w> the statue with garlands of flowers.",
                            "target_lang_translations": {
                                "ru": "Они <w>облачили</w> статую в цветочные гирлянды."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одевать или облачать кого-либо, часто в церемониальные одежды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "облачать",
                                "target_lang_sense_clarification": "торжественно одевать"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "22e4612d-b174-3c79-b96b-9f426698c8a4",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Vest</w> signed the guestbook at the entrance.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Вест</w> расписался в гостевой книге на входе."
                            }
                        },
                        {
                            "text": "We are going to visit the <w>Vest</w> family this weekend.",
                            "target_lang_translations": {
                                "ru": "Мы собираемся навестить семью <w>Вест</w> в эти выходные."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Вест",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
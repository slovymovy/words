    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "26b81275-ace1-3131-87d9-2bcf2e36d404",
                    "sense_definition": "De functie of het ambt van voorzitter; de taak om een vergadering of organisatie te leiden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "functie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij heeft het <w>voorzitterschap</w> van de politieke partij op zich genomen.",
                            "target_lang_translations": {
                                "en": "She has taken on the <w>chairmanship</w> of the political party.",
                                "ru": "Она взяла на себя <w>председательство</w> в политической партии."
                            }
                        },
                        {
                            "text": "De conferentie vond plaats onder <w>voorzitterschap</w> van een ervaren diplomaat.",
                            "target_lang_translations": {
                                "en": "The conference took place under the <w>chairmanship</w> of an experienced diplomat.",
                                "ru": "Конференция проходила под <w>председательством</w> опытного дипломата."
                            }
                        }
                    ],
                    "synonyms": [
                        "leiding",
                        "presidium"
                    ],
                    "common_phrases": [
                        "onder voorzitterschap van",
                        "het voorzitterschap bekleden",
                        "het voorzitterschap overdragen"
                    ],
                    "target_lang_definitions": {
                        "en": "The position or office of a chairperson; the duty of leading a meeting or organization.",
                        "ru": "Должность или обязанности председателя; руководство собранием или организацией."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chairmanship",
                                "target_lang_sense_clarification": "position of leading a group"
                            },
                            {
                                "target_lang_word": "presidency",
                                "target_lang_sense_clarification": "office of a president or high-level chair"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "председательство",
                                "target_lang_sense_clarification": "должность или руководство"
                            },
                            {
                                "target_lang_word": "руководство",
                                "target_lang_sense_clarification": "процесс управления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "765292f8-d406-30a2-aea3-f33caa4e1f55",
                    "sense_definition": "De periode waarin iemand de functie van voorzitter vervult.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "periode",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens zijn <w>voorzitterschap</w> werden er belangrijke hervormingen doorgevoerd.",
                            "target_lang_translations": {
                                "en": "Important reforms were implemented during his <w>chairmanship</w>.",
                                "ru": "Во время его <w>председательства</w> были проведены важные реформы."
                            }
                        },
                        {
                            "text": "Haar <w>voorzitterschap</w> duurde in totaal vier jaar.",
                            "target_lang_translations": {
                                "en": "Her <w>chairmanship</w> lasted a total of four years.",
                                "ru": "Её <w>председательство</w> длилось в общей сложности четыре года."
                            }
                        }
                    ],
                    "synonyms": [
                        "ambtstermijn"
                    ],
                    "common_phrases": [
                        "tijdens zijn voorzitterschap",
                        "tijdens haar voorzitterschap"
                    ],
                    "target_lang_definitions": {
                        "en": "The period during which someone holds the position of chairperson.",
                        "ru": "Период, в течение которого кто-либо исполняет обязанности председателя."
                    },
                    "translations": {
                        "en": [
                            {

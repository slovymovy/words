{
    "word_family": [
        "count",
        "encounter",
        "counteract",
        "counterattack",
        "counterpart"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c6cf34fd-8798-3c7a-805d-f4e0a74b50fc",
                    "sense_definition": "A long, flat, narrow surface or table in a shop, bank, restaurant, etc., at which people are served or business is transacted.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She placed the groceries on the <w>counter</w> to pay for them.",
                            "target_lang_translations": {
                                "ru": "Она положила продукты на <w>прилавок</w>, чтобы заплатить за них."
                            }
                        },
                        {
                            "text": "The pharmacist handed the medicine over the <w>counter</w>.",
                            "target_lang_translations": {
                                "ru": "Фармацевт передал лекарство через <w>прилавок</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "over the counter",
                        "under the counter",
                        "checkout counter",
                        "service counter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная плоская поверхность или стол в магазине, банке, ресторане и т. д., за которым обслуживают людей или совершаются сделки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прилавок",
                                "target_lang_sense_clarification": "в магазине"
                            },
                            {
                                "target_lang_word": "стойка",
                                "target_lang_sense_clarification": "в баре, банке или на ресепшене"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68868164-a820-3962-a01e-6bc2d444080c",
                    "sense_definition": "A flat surface in a kitchen, often on top of cabinets, used for preparing food.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please wipe the kitchen <w>counter</w> after you finish cooking.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, протри кухонную <w>столешницу</w>, когда закончишь готовить."
                            }
                        },
                        {
                            "text": "He left his keys on the granite <w>counter</w>.",
                            "target_lang_translations": {
                                "ru": "Он оставил ключи на гранитной <w>столешнице</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kitchen counter",
                        "wipe the counter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоская поверхность на кухне, часто поверх шкафов, используемая для приготовления пищи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столешница",
                                "target_lang_sense_clarification": "кухонная рабочая поверхность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58327301-ae7e-357a-88f4-92a7e4e58bb1",
                    "sense_definition": "A device, instrument, or software function that keeps a record of the number of times something happens.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The Geiger <w>counter</w> began to click loudly near the rocks.",
                            "target_lang_translations": {
                                "ru": "<w>Счётчик</w> Гейгера начал громко щелкать рядом с камнями."
                            }
                        },
                        {
                            "text": "We installed a visitor <w>counter</w> on the website to track traffic.",
                            "target_lang_translations": {
                                "ru": "Мы установили <w>счётчик</w> посетителей на сайте, чтобы отслеживать трафик."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Geiger counter",
                        "hit counter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство, прибор или программная функция, которая ведет учет количества раз, когда что-то происходит."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "счётчик",
                                "target_lang_sense_clarification": "прибор для подсчета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b72cff4e-86a4-3a4e-be55-e1531bb3d367",
                    "sense_definition": "A small flat disc or object used to mark a player's position or score in board games.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Move your <w>counter</w> three spaces forward on the board.",
                            "target_lang_translations": {
                                "ru": "Передвинь свою <w>фишку</w> на три клетки вперед по доске."
                            }
                        },
                        {
                            "text": "Each player must choose a colored <w>counter</w> before starting.",
                            "target_lang_translations": {
                                "ru": "Каждый игрок должен выбрать цветную <w>фишку</w> перед началом."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленький плоский диск или предмет, используемый для обозначения позиции игрока или счета в настольных играх."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фишка",
                                "target_lang_sense_clarification": "в настольных играх"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "350c0d21-b077-39c7-ac41-70d114dfe3bf",
                    "sense_definition": "An action or move made in response to an attack or criticism, especially in sports or arguments.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "opposition_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boxer delivered a swift <w>counter</w> to his opponent's jaw.",
                            "target_lang_translations": {
                                "ru": "Боксер нанес быстрый <w>встречный удар</w> в челюсть противника."
                            }
                        },
                        {
                            "text": "Our best <w>counter</w> to their marketing strategy is to lower prices.",
                            "target_lang_translations": {
                                "ru": "Наш лучший <w>ответный ход</w> на их маркетинговую стратегию — снизить цены."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или ход, предпринятые в ответ на атаку или критику, особенно в спорте или спорах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встречный удар",
                                "target_lang_sense_clarification": "в спорте/единоборствах"
                            },
                            {
                                "target_lang_word": "противодействие",
                                "target_lang_sense_clarification": "ответная мера"
                            },
                            {
                                "target_lang_word": "ответный ход",
                                "target_lang_sense_clarification": "в споре или стратегии"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "46de752f-7e94-3887-b30b-a60ce9a504ec",
                    "sense_definition": "To say or do something in reaction to something else, often to oppose, contradict, or defend against it.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "opposition_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>countered</w> her argument with fresh evidence from the report.",
                            "target_lang_translations": {
                                "ru": "Он <w>парировал</w> её аргумент новыми доказательствами из отчета."
                            }
                        },
                        {
                            "text": "The army immediately <w>countered</w> the attack with heavy fire.",
                            "target_lang_translations": {
                                "ru": "Армия немедленно <w>ответила</w> на атаку шквальным огнем."
                            }
                        }
                    ],
                    "synonyms": [
                        "opposed",
                        "contrasted"
                    ],
                    "common_phrases": [
                        "counter an argument",
                        "counter a threat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сказать или сделать что-то в ответ на что-то другое, часто чтобы возразить, опровергнуть или защититься."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "противостоять",
                                "target_lang_sense_clarification": "оказывать сопротивление"
                            },
                            {
                                "target_lang_word": "парировать",
                                "target_lang_sense_clarification": "отражать аргумент или удар"
                            },
                            {
                                "target_lang_word": "возражать",
                                "target_lang_sense_clarification": "высказываться против"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "d5802668-0ed8-3499-b73d-880311a7c732",
                    "sense_definition": "In a direction or manner that opposes, conflicts with, or is opposite to something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "opposition_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His recent decisions run <w>counter</w> to the company's stated values.",
                            "target_lang_translations": {
                                "ru": "Его недавние решения идут <w>вразрез</w> с заявленными ценностями компании."
                            }
                        },
                        {
                            "text": "The results ran <w>counter</w> to what the scientists had expected.",
                            "target_lang_translations": {
                                "ru": "Результаты шли <w>вразрез</w> с тем, что ожидали ученые."
                            }
                        }
                    ],
                    "common_phrases": [
                        "run counter to"
                    ],
                    "target_lang_definitions": {
                        "ru": "В направлении или образом, который противоречит, конфликтует или противоположен чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вразрез",
                                "target_lang_sense_clarification": "вопреки (идти вразрез)"
                            },
                            {
                                "target_lang_word": "против",
                                "target_lang_sense_clarification": "наперекор"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "45f436d5-29f3-3a3d-bae3-2c67152b2448",
                    "sense_definition": "Indicating opposition or being contrary to something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "opposition_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They prepared a <w>counter</w> proposal to present to the management.",
                            "target_lang_translations": {
                                "ru": "Они подготовили <w>встречное</w> предложение для представления руководству."
                            }
                        },
                        {
                            "text": "His carrying a weapon was <w>counter</w> to my plan.",
                            "target_lang_translations": {
                                "ru": "То, что он носил оружие, <w>противоречило</w> моему плану."
                            }
                        }
                    ],
                    "synonyms": [
                        "opposite",
                        "opposed",
                        "adverse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывающий на оппозицию или противоречие чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встречный",
                                "target_lang_sense_clarification": "ответный (напр. предложение)"
                            },
                            {
                                "target_lang_word": "противоположный",
                                "target_lang_sense_clarification": "обратный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "734023da-4389-385b-802a-c8741191bb87",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Counter</w> signed the guest book at the hotel.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Каунтер</w> расписался в гостевой книге отеля."
                            }
                        },
                        {
                            "text": "We are looking for a Ms. <w>Counter</w> who works in this department.",
                            "target_lang_translations": {
                                "ru": "Мы ищем мисс <w>Каунтер</w>, которая работает в этом отделе."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Каунтер",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
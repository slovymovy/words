    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e7af38de-e1ab-30c9-b0ea-f54b245096ea",
                    "sense_definition": "A specific place where someone or something is located.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The troops moved into <w>position</w> before the sun came up.",
                            "target_lang_translations": {
                                "ru": "Войска заняли <w>позиции</w> до восхода солнца."
                            }
                        },
                        {
                            "text": "Can you show me the exact <w>position</w> of the ship on this map?",
                            "target_lang_translations": {
                                "ru": "Можешь показать мне точное <w>местоположение</w> корабля на этой карте?"
                            }
                        }
                    ],
                    "synonyms": [
                        "place",
                        "move",
                        "relocate",
                        "station",
                        "transfer"
                    ],
                    "common_phrases": [
                        "in position",
                        "out of position",
                        "current position"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретное место, где кто-то или что-то находится."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "местоположение",
                                "target_lang_sense_clarification": "географическое или пространственное нахождение"
                            },
                            {
                                "target_lang_word": "позиция",

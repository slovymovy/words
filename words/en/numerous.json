                    "sense_definition": "Existing in great numbers; many.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "many_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The library contains <w>numerous</w> books on the subject.",
                            "target_lang_translations": {
                                "ru": "В библиотеке содержатся <w>многочисленные</w> книги по этому предмету."
                            }
                        },
                        {
                            "text": "She has visited the museum on <w>numerous</w> occasions.",
                            "target_lang_translations": {
                                "ru": "Она посещала музей в <w>многочисленных</w> случаях."
                            }
                        }
                    ],
                    "synonyms": [
                        "many",
                        "multiple"
                    ],
                    "antonyms": [
                        "unnumerous"
                    ],
                    "common_phrases": [
                        "numerous occasions",
                        "numerous attempts",
                        "numerous sources"
                    ],
                    "target_lang_definitions": {

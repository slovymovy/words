    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d2f492c2-d90d-32b9-a2bf-1da4908dde3f",
                    "sense_definition": "Een bijzondere, vaak aangeboren aanleg om iets goed te kunnen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "talent_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het goed kunnen onthouden van namen is echt een <w>gave</w>.",
                            "target_lang_translations": {
                                "en": "Being able to remember names well is truly a <w>gift</w>.",
                                "ru": "Способность хорошо запоминать имена — это настоящий <w>дар</w>."
                            }
                        },
                        {
                            "text": "Zij heeft de <w>gave</w> om mensen direct op hun gemak te stellen.",
                            "target_lang_translations": {
                                "en": "She has the <w>gift</w> to put people at ease immediately.",
                                "ru": "У нее есть <w>дар</w> сразу располагать к себе людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "talent"
                    ],
                    "common_phrases": [
                        "de gave des woords",
                        "een bijzondere gave"
                    ],
                    "target_lang_definitions": {
                        "en": "A special, often innate ability to do something well.",
                        "ru": "Особая, часто врожденная способность делать что-то хорошо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gift",
                                "target_lang_sense_clarification": "natural ability"
                            },
                            {
                                "target_lang_word": "talent",
                                "target_lang_sense_clarification": "skill or aptitude"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "дар",
                                "target_lang_sense_clarification": "врожденная способность"
                            },
                            {
                                "target_lang_word": "талант",
                                "target_lang_sense_clarification": "выдающаяся способность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56ee4e5f-056d-3eb3-a677-ae482ee343ab",
                    "sense_definition": "Iets dat men krijgt of geeft, vaak in de context van een donatie of een geschenk van hogere hand.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "gift_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kerk ontving een gulle <w>gave</w> van een anonieme donateur.",
                            "target_lang_translations": {
                                "en": "The church received a generous <w>donation</w> from an anonymous donor.",
                                "ru": "Церковь получила щедрое <w>пожертвование</w> от анонимного дарителя."
                            }
                        },
                        {
                            "text": "Hij beschouwde zijn goede gezondheid als een kostbare <w>gave</w>.",
                            "target_lang_translations": {
                                "en": "He considered his good health a precious <w>gift</w>.",
                                "ru": "Он считал свое крепкое здоровье драгоценным <w>даром</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "geschenk",
                        "gift",
                        "cadeau"
                    ],
                    "common_phrases": [
                        "een milde gave",
                        "een gave Gods"
                    ],
                    "target_lang_definitions": {
                        "en": "Something given or received, often a donation or a divine gift.",
                        "ru": "То, что передается или получается, часто в контексте пожертвования или дара свыше."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gift",
                                "target_lang_sense_clarification": "something given voluntarily"
                            },
                            {
                                "target_lang_word": "donation",
                                "target_lang_sense_clarification": "charitable contribution"
                            },
                            {
                                "target_lang_word": "present",
                                "target_lang_sense_clarification": "gift for an occasion"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пожертвование",
                                "target_lang_sense_clarification": "материальный взнос"
                            },
                            {
                                "target_lang_word": "дар",
                                "target_lang_sense_clarification": "подарок, благословение"
                            },
                            {
                                "target_lang_word": "подношение",
                                "target_lang_sense_clarification": "дар (часто торжественный)"
                            }
                        ]
                    }
                }
            ]
        },

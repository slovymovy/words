{
    "word_family": [
        "tradition",
        "traditional",
        "traditionalist",
        "traditionalism"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "2a7d0ed6-224e-3861-99d1-42ab1347d738",
                    "sense_definition": "In a way that follows tradition or established custom.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dancers were dressed <w>traditionally</w> in bright, colorful costumes.",
                            "target_lang_translations": {
                                "ru": "Танцоры были одеты <w>традиционно</w> — в яркие, красочные костюмы."
                            }
                        },
                        {
                            "text": "We celebrate the holiday <w>traditionally</w> with a large family dinner.",
                            "target_lang_translations": {
                                "ru": "Мы отмечаем этот праздник <w>традиционно</w>, большим семейным ужином."
                            }
                        }
                    ],
                    "synonyms": [
                        "conventionally",
                        "customarily"
                    ],
                    "antonyms": [
                        "nontraditionally",
                        "untraditionally"
                    ],
                    "common_phrases": [
                        "dressed traditionally",
                        "traditionally prepared",
                        "traditionally built"
                    ],
                    "target_lang_definitions": {
                        "ru": "Следуя традициям, в соответствии с установленными обычаями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "традиционно",
                                "target_lang_sense_clarification": "наречие образа действия"
                            },
                            {
                                "target_lang_word": "по традиции",
                                "target_lang_sense_clarification": "в силу обычая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18439a80-29bd-33d3-8199-fdb0df8639e7",
                    "sense_definition": "According to what has been done or believed for a long time; historically.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This region is <w>traditionally</w> known for its excellent wine production.",
                            "target_lang_translations": {
                                "ru": "Этот регион <w>традиционно</w> славится производством превосходного вина."
                            }
                        },
                        {
                            "text": "Teaching has <w>traditionally</w> been viewed as a very noble profession.",
                            "target_lang_translations": {
                                "ru": "Преподавание <w>традиционно</w> рассматривается как очень благородная профессия."
                            }
                        }
                    ],
                    "synonyms": [
                        "historically"
                    ],
                    "common_phrases": [
                        "traditionally associated",
                        "traditionally known",
                        "traditionally used"
                    ],
                    "target_lang_definitions": {
                        "ru": "Как было принято или считалось на протяжении долгого времени; исторически."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "традиционно",
                                "target_lang_sense_clarification": "исторически сложившись"
                            },
                            {
                                "target_lang_word": "исторически",
                                "target_lang_sense_clarification": "с давних пор"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
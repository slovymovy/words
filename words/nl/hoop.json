        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dfd7f710-485f-3f96-a73b-5610bdf48eff",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>hopen</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>hoop</w> dat je snel weer beter wordt na je operatie.",
                            "target_lang_translations": {
                                "en": "I <w>hope</w> that you get better soon after your surgery.",
                                "ru": "Я <w>надеюсь</w>, что ты скоро поправишься после операции."
                            }
                        },
                        {
                            "text": "Natuurlijk <w>hoop</w> ik op een goede afloop van het examen.",
                            "target_lang_translations": {
                                "en": "Of course I <w>hope</w> for a good outcome of the exam.",
                                "ru": "Конечно, я <w>надеюсь</w> на хороший исход экзамена."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ik hoop dat",
                        "ik hoop van wel"
                    ],

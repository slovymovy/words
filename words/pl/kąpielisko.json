    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1e43e1d6-bd9f-326d-8ab3-12ef831fbca5",
                    "sense_definition": "Wyznaczone miejsce nad wodą (np. nad jeziorem, rzeką lub morzem) lub zespół basenów, przeznaczone do bezpiecznej kąpieli i rekreacji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "place_for_swimming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W upalne dni miejskie <w>kąpielisko</w> przyciąga tłumy mieszkańców szukających ochłody.",
                            "target_lang_translations": {
                                "ru": "В жаркие дни городской <w>пляж</w> привлекает толпы жителей, ищущих прохлады."
                            }
                        },
                        {
                            "text": "Pływanie jest bezpieczne tylko tam, gdzie znajduje się strzeżone <w>kąpielisko</w> z ratownikiem.",
                            "target_lang_translations": {
                                "ru": "Плавание безопасно только там, где есть охраняемое <w>место для купания</w> со спасателем."
                            }
                        }
                    ],
                    "synonyms": [
                        "pływalnia",
                        "basen"
                    ],
                    "common_phrases": [
                        "strzeżone kąpielisko",
                        "dzikie kąpielisko",
                        "miejskie kąpielisko",
                        "zakaz kąpieli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специально отведенное место у воды (например, на озере, реке или море) или комплекс бассейнов, предназначенные для безопасного купания и отдыха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "место для купания",
                                "target_lang_sense_clarification": "специально отведенная зона"
                            },
                            {
                                "target_lang_word": "пляж",
                                "target_lang_sense_clarification": "берег, оборудованный для купания"
                            },
                            {
                                "target_lang_word": "купальня",
                                "target_lang_sense_clarification": "сооружение или огороженное место в воде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fcc54c2d-de00-398b-b660-6d5f665bf6f9",
                    "sense_definition": "Miejscowość wypoczynkowa położona nad wodą, posiadająca infrastrukturę dla turystów i kuracjuszy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "resort_town",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sopot to popularne <w>kąpielisko</w>, które tętni życiem przez cały sezon wakacyjny.",
                            "target_lang_translations": {
                                "ru": "Сопот — это популярный <w>курорт</w>, который кипит жизнью весь отпускной сезон."
                            }
                        },
                        {
                            "text": "To małe nadmorskie <w>kąpielisko</w> oferuje spokój i piękne widoki z dala od hałasu.",
                            "target_lang_translations": {
                                "ru": "Этот маленький морской <w>курорт</w> предлагает спокойствие и красивые виды вдали от шума."
                            }
                        }
                    ],
                    "synonyms": [
                        "kurort",
                        "uzdrowisko"
                    ],
                    "common_phrases": [
                        "nadmorskie kąpielisko",
                        "znane kąpielisko",
                        "modne kąpielisko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Курортная местность, расположенная у воды, обладающая инфраструктурой для туристов и отдыхающих."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "курорт",
                                "target_lang_sense_clarification": "место отдыха"
                            },
                            {
                                "target_lang_word": "морской курорт",
                                "target_lang_sense_clarification": "курорт на побережье"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
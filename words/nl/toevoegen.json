{
    "word_family": [
        "toevoeging",
        "toevoegsel"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f8b8a5b2-47fd-3355-b62c-fcea1f4cc1c6",
                    "sense_definition": "Iets erbij doen om het geheel groter, beter of completer te maken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "general_addition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet nog wat extra water aan de soep <w>toevoegen</w>.",
                            "target_lang_translations": {
                                "en": "You need to <w>add</w> some extra water to the soup."
                            }
                        },
                        {
                            "text": "De leraar heeft mijn naam gisteren aan de lijst <w>toegevoegd</w>.",
                            "target_lang_translations": {
                                "en": "The teacher <w>added</w> my name to the list yesterday."
                            }
                        }
                    ],
                    "synonyms": [
                        "erbij doen",
                        "bijvoegen"
                    ],
                    "antonyms": [
                        "weglaten",
                        "verwijderen"
                    ],
                    "common_phrases": [
                        "aan de lijst toevoegen",
                        "water toevoegen",
                        "waarde toevoegen"
                    ],
                    "target_lang_definitions": {
                        "en": "To put something in or with something else to make the whole larger, better, or more complete."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "add",
                                "target_lang_sense_clarification": "to increase or combine"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5c9acd2-707c-3273-93dc-6e90feb8fe81",
                    "sense_definition": "Een opmerking maken als aanvulling op wat al gezegd of geschreven is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "speech_addition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb hier verder helemaal niets aan <w>toe</w> te <w>voegen</w>.",
                            "target_lang_translations": {
                                "en": "I have absolutely nothing to <w>add</w> to this."
                            }
                        },
                        {
                            "text": "De directeur <w>voegde</w> eraan <w>toe</w> dat het bedrijf winstgevend blijft.",
                            "target_lang_translations": {
                                "en": "The director <w>added</w> that the company remains profitable."
                            }
                        }
                    ],
                    "synonyms": [
                        "erbij zeggen",
                        "aanvullen"
                    ],
                    "common_phrases": [
                        "eraan toevoegen",
                        "niets aan toevoegen"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a remark as a supplement to what has already been said or written."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "add",
                                "target_lang_sense_clarification": "to say something extra"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
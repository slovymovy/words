    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f7c219e5-56b3-38de-b762-b4ebf62fad5f",
                    "sense_definition": "Usuwać brud z powierzchni czegoś za pomocą wody, często z dodatkiem środków czyszczących (np. mydła, płynu).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cleaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz dokładnie <w>myć</w> owoce przed jedzeniem.",
                            "target_lang_translations": {
                                "ru": "Ты должен тщательно <w>мыть</w> фрукты перед едой."
                            }
                        },
                        {
                            "text": "Mój brat nie lubi <w>myć</w> naczyń po obiedzie.",
                            "target_lang_translations": {
                                "ru": "Мой брат не любит <w>мыть</w> посуду после обеда."
                            }
                        },
                        {
                            "text": "Zęby należy <w>myć</w> co najmniej dwa razy dziennie.",
                            "target_lang_translations": {
                                "ru": "Зубы следует <w>чистить</w> как минимум два раза в день."
                            }
                        }
                    ],
                    "synonyms": [
                        "czyścić",
                        "zmywać",
                        "czyścić",
                        "wymywać",
                        "prać"
                    ],
                    "antonyms": [
                        "brudzić"
                    ],
                    "common_phrases": [
                        "myć ręce",
                        "myć zęby",
                        "myć naczynia",
                        "myć okna",
                        "myć podłogę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удалять грязь с поверхности чего-либо с помощью воды, часто с добавлением чистящих средств (например, мыла, жидкости)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мыть",
                                "target_lang_sense_clarification": "общая чистка водой (посуда, пол, фрукты)"
                            },
                            {
                                "target_lang_word": "чистить",
                                "target_lang_sense_clarification": "используется в контексте чистки зубов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ac3ef14-e8f9-3973-9a7c-4cf3ba2fa120",
                    "sense_definition": "Czyścić własne ciało wodą i środkami higienicznymi; brać kąpiel lub prysznic (używane z zaimkiem zwrotnym \"się\").",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cleaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdego wieczoru <w>myję</w> się pod prysznicem.",
                            "target_lang_translations": {
                                "ru": "Каждый вечер я <w>моюсь</w> под душем."
                            }
                        },
                        {
                            "text": "Natasza <w>myje</w> się gąbką, którą przywiozła z wakacji.",
                            "target_lang_translations": {
                                "ru": "Наташа <w>моется</w> губкой, которую привезла из отпуска."
                            }
                        },
                        {
                            "text": "Ten kot bardzo często <w>myje</w> się na słońcu.",
                            "target_lang_translations": {
                                "ru": "Этот кот очень часто <w>умывается</w> на солнце."
                            }
                        }
                    ],
                    "synonyms": [
                        "kąpać",
                        "nurzać się"
                    ],
                    "antonyms": [
                        "brudzić"
                    ],
                    "common_phrases": [
                        "myć się codziennie",
                        "myć się dokładnie",
                        "myć się w zimnej wodzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мыть собственное тело водой и гигиеническими средствами; принимать ванну или душ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мыться",
                                "target_lang_sense_clarification": "мыть себя (возвратный глагол)"
                            },
                            {
                                "target_lang_word": "умываться",
                                "target_lang_sense_clarification": "мыть лицо или (о животных) чистить шерсть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ff2611d-9292-381f-ac09-e50b281b3932",
                    "sense_definition": "Surowo kogoś upominać, robić komuś wyrzuty (zwykle w zwrocie \"myć/zmyć komuś głowę\").",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szef wezwał pracownika, aby <w>myć</w> mu głowę za spóźnienia.",
                            "target_lang_translations": {
                                "ru": "Начальник вызвал сотрудника, чтобы <w>намылить</w> ему шею за опоздания."
                            }
                        },
                        {
                            "text": "Nie lubię, gdy ktoś próbuje mi <w>myć</w> głowę przy innych.",
                            "target_lang_translations": {
                                "ru": "Я не люблю, когда кто-то пытается <w>отчитывать</w> меня при других."
                            }
                        }
                    ],
                    "synonyms": [
                        "strofować"
                    ],
                    "common_phrases": [
                        "myć komuś głowę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "idiom"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Строго отчитывать кого-то, делать выговор (обычно в идиоматических выражениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отчитывать",
                                "target_lang_sense_clarification": "ругать, делать выговор"
                            },
                            {
                                "target_lang_word": "намылить",
                                "target_lang_sense_clarification": "в идиоме 'намылить шею'"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
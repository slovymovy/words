                {
                    "sense_id": "316a0174-146d-3628-992e-2934f28ef41f",
                    "sense_definition": "Taki, którego tematem jest przestępstwo i metody jego wykrywania (o literaturze, filmie itp.)",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "genre",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja ulubiona powieść <w>kryminalna</w> trzyma w napięciu do ostatniej strony.",
                            "target_lang_translations": {
                                "ru": "Мой любимый <w>детективный</w> роман держит в напряжении до последней страницы."
                            }
                        },
                        {
                            "text": "Wczoraj wieczorem obejrzeliśmy ciekawy film <w>kryminalny</w>.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером мы посмотрели интересный <w>криминальный</w> фильм."
                            }
                        }
                    ],
                    "synonyms": [
                        "sensacyjny"
                    ],
                    "common_phrases": [
                        "powieść kryminalna",
                        "film kryminalny",
                        "zagadka kryminalna",
                        "literatura kryminalna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повествующий о преступлениях и методах их расследования (о литературе, кино и т. п.)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "детективный",
                                "target_lang_sense_clarification": "о жанре"
                            },
                            {
                                "target_lang_word": "криминальный",
                                "target_lang_sense_clarification": "о сюжете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8dc0a1a-f0aa-396c-9cb3-8601e8a55f1e",
                    "sense_definition": "Odnoszący się do przestępstwa, związany z popełnieniem przestępstwa",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "crime_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>kryminalna</w> przeszłość uniemożliwiła mu znalezienie dobrej pracy.",
                            "target_lang_translations": {
                                "ru": "Его <w>криминальное</w> прошлое помешало ему найти хорошую работу."
                            }
                        },
                        {
                            "text": "Występek i zbrodnia to czyny <w>kryminalne</w>, za które grozi kara.",
                            "target_lang_translations": {
                                "ru": "Проступок и преступление — это <w>уголовные</w> деяния, за которые грозит наказание."
                            }
                        }
                    ],
                    "synonyms": [
                        "przestępczy"
                    ],
                    "common_phrases": [
                        "czyn kryminalny",
                        "przeszłość kryminalna",
                        "odpowiedzialność kryminalna",
                        "świat kryminalny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к преступлению, связанный с совершением преступления"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уголовный",
                                "target_lang_sense_clarification": "правовой термин"

{
    "word_family": [
        "brutoloon",
        "brutowinst",
        "brutogewicht",
        "brutomarge"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9cbe4272-5d92-3fb8-a915-c0d1ee99b6f6",
                    "sense_definition": "Zonder aftrek van kosten, belastingen of het gewicht van de verpakking.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "gross_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>bruto</w> inkomen ligt een stuk hoger dan wat je uiteindelijk op je rekening krijgt.",
                            "target_lang_translations": {
                                "ru": "Доход <w>брутто</w> значительно выше того, что вы в итоге получаете на свой счет."
                            }
                        },
                        {
                            "text": "Op de doos staat het <w>bruto</w> gewicht vermeld, dus inclusief het verpakkingsmateriaal.",
                            "target_lang_translations": {
                                "ru": "На коробке указан вес <w>брутто</w>, то есть включая упаковочный материал."
                            }
                        }
                    ],
                    "antonyms": [
                        "netto"
                    ],
                    "common_phrases": [
                        "bruto inkomen",
                        "bruto gewicht",
                        "bruto nationaal product"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без вычета расходов, налогов или веса упаковки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брутто",
                                "target_lang_sense_clarification": "общий, без вычетов"
                            },
                            {
                                "target_lang_word": "валовой",
                                "target_lang_sense_clarification": "экономический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ded53b3-5424-33e5-a6cd-a024a7bd3a47",
                    "sense_definition": "Vermengd met een ander, vaak minder waardevol metaal.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "gross_metal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Is dit <w>bruto</w> zilver of is het al gezuiverd?",
                            "target_lang_translations": {
                                "ru": "Это <w>неочищенное</w> серебро или оно уже рафинировано?"
                            }
                        },
                        {
                            "text": "De handelaar kocht het goud <w>bruto</w> in om het later te laten smelten.",
                            "target_lang_translations": {
                                "ru": "Торговец купил <w>неочищенное</w> золото, чтобы позже переплавить его."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Смешанный с другим, часто менее ценным металлом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неочищенный",
                                "target_lang_sense_clarification": "с примесями"
                            },
                            {
                                "target_lang_word": "черновой",
                                "target_lang_sense_clarification": "необработанный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "04130852-fe89-3fe8-99ac-9857d4d81996",
                    "sense_definition": "Gerekend vóór de aftrek van belastingen en premies.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "gross_financial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij verdient <w>bruto</w> meer dan een miljoen per jaar met optredens.",
                            "target_lang_translations": {
                                "ru": "Он зарабатывает <w>брутто</w> более миллиона в год на выступлениях."
                            }
                        },
                        {
                            "text": "Als je <w>bruto</w> tweeduizend euro verdient, houd je netto minder over.",
                            "target_lang_translations": {
                                "ru": "Если вы зарабатываете две тысячи евро <w>брутто</w>, чистыми у вас останется меньше."
                            }
                        }
                    ],
                    "antonyms": [
                        "netto"
                    ],
                    "common_phrases": [
                        "bruto verdienen",
                        "bruto uitkeren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рассчитанный до вычета налогов и взносов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брутто",
                                "target_lang_sense_clarification": "до вычетов"
                            },
                            {
                                "target_lang_word": "грязными",
                                "target_lang_sense_clarification": "разговорное, о зарплате"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afff2c21-cebe-36bf-9c70-faa6b947701e",
                    "sense_definition": "Gerekend inclusief het gewicht van de verpakking.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "gross_weight",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de stevige verpakking woog de zending <w>bruto</w> meer dan een kilo.",
                            "target_lang_translations": {
                                "ru": "Из-за прочной упаковки посылка весила <w>брутто</w> более килограмма."
                            }
                        },
                        {
                            "text": "De vrachtwagen woog <w>bruto</w> te veel voor de kleine brug.",
                            "target_lang_translations": {
                                "ru": "Грузовик весил <w>брутто</w> слишком много для маленького моста."
                            }
                        }
                    ],
                    "antonyms": [
                        "netto"
                    ],
                    "common_phrases": [
                        "bruto wegen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рассчитанный с учетом веса упаковки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брутто",
                                "target_lang_sense_clarification": "общий вес"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "efa58f03-ed34-377a-b127-78057b9d9fd1",
                    "sense_definition": "Oddziaływanie fizyczną siłą na powierzchnię czegoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_force",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zalecił stosować zimny <w>ucisk</w> na opuchnięte miejsce.",
                            "target_lang_translations": {
                                "ru": "Врач рекомендовал прикладывать холодный <w>компресс</w> на опухшее место."
                            }
                        },
                        {
                            "text": "Poczułem silny <w>ucisk</w> dłoni mojego przyjaciela na powitanie.",
                            "target_lang_translations": {
                                "ru": "Я почувствовал сильное <w>пожатие</w> руки моего друга в знак приветствия."
                            }
                        }
                    ],
                    "synonyms": [
                        "nacisk",
                        "napór",
                        "przyciskanie",
                        "ściskanie"
                    ],
                    "common_phrases": [
                        "ucisk na ranę",
                        "miejscowy ucisk",
                        "lekki ucisk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Воздействие физической силы на поверхность чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "давление",
                                "target_lang_sense_clarification": "физическое воздействие"
                            },
                            {
                                "target_lang_word": "нажатие",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "сжатие",
                                "target_lang_sense_clarification": "сдавливание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e52edb76-df92-3fc4-bbe1-e19019704a10",
                    "sense_definition": "Nieprzyjemne wrażenie fizyczne polegające na odczuwaniu ciężaru lub ściskania wewnątrz ciała.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pacjent skarżył się na bolesny <w>ucisk</w> w klatce piersiowej.",
                            "target_lang_translations": {
                                "ru": "Пациент жаловался на болезненное <w>давление</w> в грудной клетке."
                            }
                        },
                        {
                            "text": "Stres wywołał u niej dziwny <w>ucisk</w> w żołądku przed egzaminem.",
                            "target_lang_translations": {
                                "ru": "Стресс вызвал у неё странное <w>сжатие</w> в желудке перед экзаменом."
                            }
                        }
                    ],
                    "synonyms": [
                        "gniecenie",
                        "ciężar"
                    ],
                    "common_phrases": [
                        "ucisk w klatce piersiowej",
                        "ucisk w gardle",
                        "ucisk w żołądku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неприятное физическое ощущение тяжести или сжатия внутри тела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдавленность",
                                "target_lang_sense_clarification": "ощущение тесноты"
                            },
                            {
                                "target_lang_word": "давление",
                                "target_lang_sense_clarification": "боль или тяжесть"
                            },
                            {
                                "target_lang_word": "тяжесть",
                                "target_lang_sense_clarification": "дискомфорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6533cde-6751-3451-8fe7-1c317c7e7207",
                    "sense_definition": "Ograniczanie czyjejś wolności, praw lub swobody działania, często przez władzę.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "oppression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ludzie zbuntowali się przeciwko wieloletniemu <w>uciskowi</w> władzy.",
                            "target_lang_translations": {
                                "ru": "Люди взбунтовались против многолетнего <w>гнёта</w> властей."
                            }
                        },
                        {
                            "text": "Pisarz w swoich książkach często opisywał <w>ucisk</w> najbiedniejszych warstw społecznych.",
                            "target_lang_translations": {
                                "ru": "Писатель в своих книгах часто описывал <w>угнетение</w> беднейших слоев общества."
                            }
                        }
                    ],
                    "synonyms": [
                        "uciemiężenie",
                        "ciemiężenie",
                        "gnębienie",
                        "wyzysk"
                    ],
                    "common_phrases": [
                        "ucisk podatkowy",
                        "walka z uciskiem",
                        "wyzwolenie z ucisku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ограничение чьей-либо свободы, прав или свободы действий, часто со стороны власти."
                    },
                    "translations": {

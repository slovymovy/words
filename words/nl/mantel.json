                    ],
                    "common_phrases": [
                        "de mantel van de kabel",
                        "beschermende mantel"
                    ],
                    "target_lang_definitions": {
                        "en": "A protective outer layer or casing surrounding an object, cable, or machine."
                    },
                    "translations": {
                        "en": [
                            {

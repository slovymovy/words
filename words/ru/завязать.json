                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "strike up",
                                "target_lang_sense_clarification": "to initiate a conversation or friendship"
                            },
                            {
                                "target_lang_word": "start",
                                "target_lang_sense_clarification": "general term"
                            },
                            {
                                "target_lang_word": "establish",
                                "target_lang_sense_clarification": "to create a connection or relationship"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97b45593-764a-3ddc-9ac3-dd6fc640c3dd",
                    "sense_definition": "Окончательно прекратить заниматься чем-либо (обычно вредной привычкой).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "quit_habit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой друг пообещал навсегда <завязать> с курением.",
                            "target_lang_translations": {
                                "en": "My friend promised to <quit> smoking forever."
                            }
                        },
                        {
                            "text": "Он решил <завязать> с прошлым и начать новую жизнь.",
                            "target_lang_translations": {
                                "en": "He decided to <be done> with the past and start a new life."
                            }
                        }
                    ],
                    "synonyms": [
                        "прекратить",
                        "закончить"
                    ],
                    "antonyms": [
                        "начать"
                    ],
                    "common_phrases": [
                        "завязать с курением",
                        "завязать пить",
                        "решил завязать"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "прост."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To stop doing something permanently (usually a bad habit)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quit",
                                "target_lang_sense_clarification": "to stop a habit"
                            },
                            {
                                "target_lang_word": "give up",
                                "target_lang_sense_clarification": "phrasal verb for stopping something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b5cb32d-0b88-3544-8362-267136e0c683",
                    "sense_definition": "Образовать завязь (о плодах растений).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "botany_fruit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если весна будет теплой, яблоня сможет <завязать> много плодов.",
                            "target_lang_translations": {
                                "en": "If the spring is warm, the apple tree will be able to <set> a lot of fruit."
                            }
                        },
                        {
                            "text": "Растение должно <завязать> семена до наступления холодов.",
                            "target_lang_translations": {
                                "en": "The plant must <set> seeds before the cold sets in."
                            }

{
    "word_family": [
        "post",
        "postage",
        "postal",
        "posting"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5205a068-76db-31f3-be2f-311d175237cf",
                    "sense_definition": "A large printed picture, often of a celebrity or decorative image, designed to be attached to a wall for decoration.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_sign",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has a <w>poster</w> of her favorite band on the bedroom wall.",
                            "target_lang_translations": {
                                "ru": "У неё на стене спальни висит <w>постер</w> с её любимой группой."
                            }
                        },
                        {
                            "text": "I bought a colorful <w>poster</w> to brighten up the office.",
                            "target_lang_translations": {
                                "ru": "Я купил красочный <w>плакат</w>, чтобы украсить офис."
                            }
                        }
                    ],
                    "synonyms": [
                        "picture",
                        "print"
                    ],
                    "common_phrases": [
                        "movie poster",
                        "wall poster",
                        "put up a poster"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое печатное изображение, часто с фотографией знаменитости или декоративным рисунком, предназначенное для крепления на стену для украшения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постер",
                                "target_lang_sense_clarification": "декоративный"
                            },
                            {
                                "target_lang_word": "плакат",
                                "target_lang_sense_clarification": "настенное изображение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e5a89dc-1769-3b4e-b4dc-d9541955a4ea",
                    "sense_definition": "A printed notice or advertisement displayed in a public place to promote a product, event, or idea.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_sign",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We saw a <w>poster</w> for the new superhero movie at the bus stop.",
                            "target_lang_translations": {
                                "ru": "Мы увидели <w>афишу</w> нового фильма о супергероях на автобусной остановке."
                            }
                        },
                        {
                            "text": "The campaign <w>poster</w> was displayed in the shop window.",
                            "target_lang_translations": {
                                "ru": "Предвыборный <w>плакат</w> был выставлен в витрине магазина."
                            }
                        }
                    ],
                    "synonyms": [
                        "placard",
                        "notice",
                        "bill",
                        "sign",
                        "advertisement"
                    ],
                    "common_phrases": [
                        "wanted poster",
                        "campaign poster",
                        "election poster",
                        "advertising poster"
                    ],
                    "target_lang_definitions": {
                        "ru": "Печатное уведомление или реклама, вывешенные в общественном месте для продвижения товара, события или идеи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "афиша",
                                "target_lang_sense_clarification": "о мероприятиях, концертах, фильмах"
                            },
                            {
                                "target_lang_word": "плакат",
                                "target_lang_sense_clarification": "рекламный или агитационный"
                            },
                            {
                                "target_lang_word": "объявление",
                                "target_lang_sense_clarification": "информационное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e6396a8-fdf3-34df-8411-7466b8e753d9",
                    "sense_definition": "A person who publishes a message or content on an online forum or social media platform.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_agent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The original <w>poster</w> asked for advice on fixing a computer error.",
                            "target_lang_translations": {
                                "ru": "<w>Автор поста</w> попросил совета по исправлению компьютерной ошибки."
                            }
                        },
                        {
                            "text": "Several <w>posters</w> commented that the information was incorrect.",
                            "target_lang_translations": {
                                "ru": "Несколько <w>пользователей</w> прокомментировали, что информация была неверной."
                            }
                        }
                    ],
                    "synonyms": [
                        "user",
                        "contributor",
                        "commenter"
                    ],
                    "antonyms": [
                        "lurker"
                    ],
                    "common_phrases": [
                        "original poster",
                        "frequent poster",
                        "forum poster"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который публикует сообщение или контент на интернет-форуме или в социальной сети."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "автор поста",
                                "target_lang_sense_clarification": "создатель сообщения"
                            },
                            {
                                "target_lang_word": "пользователь",
                                "target_lang_sense_clarification": "участник обсуждения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a7d31ee-46b0-3353-9adb-1c1d0890e975",
                    "sense_definition": "A spectacular slam dunk performed over a defending player.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sports_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crowd went wild after he scored a massive <w>poster</w> on the center.",
                            "target_lang_translations": {
                                "ru": "Толпа ликовала после того, как он забил мощнейший <w>постер-данк</w> через центрового."
                            }
                        },
                        {
                            "text": "That dunk was an absolute <w>poster</w> worthy of the highlight reel.",
                            "target_lang_translations": {
                                "ru": "Этот данк был настоящим <w>постером</w>, достойным нарезки лучших моментов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "put on a poster"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "basketball"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Зрелищный слэм-данк, выполненный через защищающегося игрока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постер-данк",
                                "target_lang_sense_clarification": "бросок сверху через соперника"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96bf9716-6f76-33a1-b6bd-8f9f40080342",
                    "sense_definition": "A shot in sports that hits the goalpost instead of entering the goal.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sports_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The striker hit a <w>poster</w> in the final minute of the game.",
                            "target_lang_translations": {
                                "ru": "Нападающий совершил <w>попадание в штангу</w> на последней минуте матча."
                            }
                        },
                        {
                            "text": "We had three <w>posters</w> and zero goals during the second period.",
                            "target_lang_translations": {
                                "ru": "У нас было три <w>попадания в штангу</w> и ноль голов во втором периоде."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "sports"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Удар в спорте, который попадает в штангу ворот вместо того, чтобы залететь в ворота."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "попадание в штангу",
                                "target_lang_sense_clarification": "удар в каркас ворот"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d23674f-b393-3ab8-bcab-3c33233a2b5e",
                    "sense_definition": "A horse kept at a station to be used by travelers or for carrying mail.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "historical_travel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The carriage was pulled by four fresh <w>posters</w>.",
                            "target_lang_translations": {
                                "ru": "Карету тянули четыре свежие <w>почтовые лошади</w>."
                            }
                        },
                        {
                            "text": "They changed <w>posters</w> at the inn to continue their journey quickly.",
                            "target_lang_translations": {
                                "ru": "Они сменили <w>почтовых лошадей</w> на постоялом дворе, чтобы быстро продолжить путь."
                            }
                        }
                    ],
                    "synonyms": [
                        "posthorse"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лошадь, содержащаяся на станции для использования путешественниками или для перевозки почты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почтовая лошадь",
                                "target_lang_sense_clarification": "сменная лошадь"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "80dc9776-ce13-3792-8a1a-ed437aee7e1e",
                    "sense_definition": "To cover or decorate a surface with posters.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_decorate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Teenagers often <w>poster</w> their walls with images of pop stars.",
                            "target_lang_translations": {
                                "ru": "Подростки часто <w>обклеивают плакатами</w> свои стены изображениями поп-звезд."
                            }
                        },
                        {
                            "text": "The activists went out at night to <w>poster</w> the neighborhood with flyers.",
                            "target_lang_translations": {
                                "ru": "Активисты вышли ночью, чтобы <w>расклеить</w> листовки по району."
                            }
                        }
                    ],
                    "synonyms": [
                        "plaster",
                        "cover"
                    ],
                    "common_phrases": [
                        "poster the walls",
                        "poster the town"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать или украшать поверхность плакатами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обклеивать плакатами",
                                "target_lang_sense_clarification": "декорировать поверхность"
                            },
                            {
                                "target_lang_word": "расклеивать",
                                "target_lang_sense_clarification": "размещать объявления"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "40cec5b6-5cad-382e-826a-04147e6b5346",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Poster</w> arrived late for the meeting.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Постер</w> опоздал на встречу."
                            }
                        },
                        {
                            "text": "I have never met anyone with the last name <w>Poster</w> before.",
                            "target_lang_translations": {
                                "ru": "Я никогда раньше не встречал никого с фамилией <w>Постер</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Постер",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "9be0fc72-1680-3a5a-ab07-dd8cd38c5a35",
                    "sense_definition": "Zich draaiend over een oppervlak voortbewegen",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bal <w>rolt</w> langzaam van de steile heuvel af.",
                            "target_lang_translations": {
                                "en": "The ball <w>rolls</w> slowly down the steep hill.",
                                "ru": "Мяч медленно <w>катится</w> с крутого холма."
                            }
                        },
                        {
                            "text": "Er <w>rolde</w> een dikke traan over haar wang toen ze het nieuws hoorde.",
                            "target_lang_translations": {
                                "en": "A big tear <w>rolled</w> down her cheek when she heard the news.",
                                "ru": "Крупная слеза <w>скатилась</w> по её щеке, когда она услышала новости."
                            }
                        }
                    ],
                    "synonyms": [

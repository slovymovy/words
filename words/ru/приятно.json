                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В саду <w>приятно</w> пахло цветами и травой.",
                            "target_lang_translations": {

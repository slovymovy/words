{
    "word_family": [
        "wonder",
        "middel"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b333a75d-c013-3a94-915f-91aae4bebe26",
                    "sense_definition": "Een oplossing of maatregel die alle problemen of moeilijkheden in één keer zou verhelpen (vaak gebruikt om aan te geven dat zoiets niet bestaat).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "figurative_solution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De nieuwe wet is geen <w>wondermiddel</w> dat alle economische problemen direct oplost.",
                            "target_lang_translations": {
                                "en": "The new law is not a <w>panacea</w> that immediately solves all economic problems."
                            }
                        },
                        {
                            "text": "Het is geen <w>wondermiddel</w>, maar het kan wel helpen om de situatie te verbeteren.",
                            "target_lang_translations": {
                                "en": "It is not a <w>magic bullet</w>, but it can help to improve the situation."
                            }
                        }
                    ],
                    "synonyms": [
                        "panacee",
                        "tovermiddel"
                    ],
                    "common_phrases": [
                        "geen wondermiddel",
                        "het ultieme wondermiddel",
                        "zoeken naar een wondermiddel"
                    ],
                    "target_lang_definitions": {
                        "en": "A solution or measure supposed to solve all problems or difficulties at once."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "panacea",
                                "target_lang_sense_clarification": "universal remedy"
                            },
                            {
                                "target_lang_word": "magic bullet",
                                "target_lang_sense_clarification": "simple solution to a complex problem"
                            },
                            {
                                "target_lang_word": "cure-all",
                                "target_lang_sense_clarification": "solution for everything"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09e73531-28be-38e6-9dd7-babe6dfe68de",
                    "sense_definition": "Een geneesmiddel met een buitengewoon krachtige en snelle werking.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "medical_cure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prednison lijkt soms een <w>wondermiddel</w> te zijn, maar heeft helaas ook bijwerkingen.",
                            "target_lang_translations": {
                                "en": "Prednisone sometimes seems to be a <w>wonder drug</w>, but unfortunately also has side effects."
                            }
                        },
                        {
                            "text": "De arts schreef een nieuw <w>wondermiddel</w> voor dat de infectie snel deed verdwijnen.",
                            "target_lang_translations": {
                                "en": "The doctor prescribed a new <w>miracle drug</w> that made the infection disappear quickly."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een medisch wondermiddel"
                    ],
                    "target_lang_definitions": {
                        "en": "A medicine with an extraordinarily powerful and rapid effect."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wonder drug",
                                "target_lang_sense_clarification": "highly effective medicine"
                            },
                            {
                                "target_lang_word": "miracle drug",
                                "target_lang_sense_clarification": "powerful medication"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a728603-d411-3ea8-b78d-26044a2f7eef",
                    "sense_definition": "Een middel dat beweert alles te genezen, maar in werkelijkheid vaak geen effect heeft.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "quackery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Geloof niet in elk <w>wondermiddel</w> dat op internet wordt aangeboden om snel af te vallen.",
                            "target_lang_translations": {
                                "en": "Do not believe in every <w>miracle cure</w> offered on the internet for quick weight loss."
                            }
                        },
                        {
                            "text": "Hij gaf veel geld uit aan vage <w>wondermiddeltjes</w> die eeuwige jeugd beloofden.",
                            "target_lang_translations": {
                                "en": "He spent a lot of money on vague <w>miracle cures</w> that promised eternal youth."
                            }
                        }
                    ],
                    "synonyms": [
                        "panacee"
                    ],
                    "common_phrases": [
                        "dure wondermiddeltjes"
                    ],
                    "target_lang_definitions": {
                        "en": "A remedy claiming to cure everything, but often having no effect in reality."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "miracle cure",
                                "target_lang_sense_clarification": "dubious remedy"
                            },
                            {
                                "target_lang_word": "cure-all",
                                "target_lang_sense_clarification": "claimed universal remedy"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
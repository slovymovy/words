                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "balance",
                                "target_lang_sense_clarification": "harmonious proportion"
                            },
                            {
                                "target_lang_word": "harmony",
                                "target_lang_sense_clarification": "pleasing combination"
                            }
                        ]

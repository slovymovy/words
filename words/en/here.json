{
    "word_family": [
        "hereabouts",
        "hereafter",
        "hereby",
        "herein",
        "hereof",
        "hereon",
        "hereto",
        "herewith"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "66478137-243e-366a-878a-90b596feb47b",
                    "sense_definition": "In, on, or at this place; the place where the speaker is.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You wait <w>here</w> while I fetch my coat.",
                            "target_lang_translations": {
                                "ru": "Подожди <w>здесь</w>, пока я схожу за пальто."
                            }
                        },
                        {
                            "text": "Ms. Doe is not <w>here</w> at the moment.",
                            "target_lang_translations": {
                                "ru": "Г-жи Доу сейчас нет <w>здесь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "right here"
                    ],
                    "antonyms": [
                        "there"
                    ],
                    "common_phrases": [
                        "over here",
                        "look here",
                        "right here"
                    ],
                    "target_lang_definitions": {
                        "ru": "В этом месте; там, где находится говорящий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здесь",
                                "target_lang_sense_clarification": "наиболее нейтральный и употребительный вариант"
                            },
                            {
                                "target_lang_word": "тут",
                                "target_lang_sense_clarification": "разговорный вариант, синоним слова «здесь»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46b31530-b9df-399f-8895-3d29687a5a63",
                    "sense_definition": "To this place; towards the speaker.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please come <w>here</w> immediately.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, иди <w>сюда</w> немедленно."
                            }
                        },
                        {
                            "text": "How did you get <w>here</w> so fast?",
                            "target_lang_translations": {
                                "ru": "Как ты добрался <w>сюда</w> так быстро?"
                            }
                        }
                    ],
                    "antonyms": [
                        "there"
                    ],
                    "common_phrases": [
                        "come here",
                        "get here"
                    ],
                    "target_lang_definitions": {
                        "ru": "В это место; по направлению к говорящему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сюда",
                                "target_lang_sense_clarification": "указывает на направление движения к говорящему"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2548525d-4160-3e7a-b24a-548f8ed18659",
                    "sense_definition": "Used to indicate that a particular time, event, or season has arrived.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Flu season is <w>here</w>.",
                            "target_lang_translations": {
                                "ru": "Сезон гриппа <w>наступил</w>."
                            }
                        },
                        {
                            "text": "Christmas will soon be <w>here</w>.",
                            "target_lang_translations": {
                                "ru": "Рождество скоро <w>наступит</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "summer is here",
                        "here comes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания на то, что определенное время, событие или сезон наступили."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наступил",
                                "target_lang_sense_clarification": "используется, когда речь идет о приходе времени года или события"
                            },
                            {
                                "target_lang_word": "пришёл",
                                "target_lang_sense_clarification": "синоним «наступил» в контексте времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6430300b-6dfe-3c55-832f-d074d811e51c",
                    "sense_definition": "At this point or stage in a process, story, or activity.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have done enough work, so I am stopping <w>here</w>.",
                            "target_lang_translations": {
                                "ru": "Я достаточно поработал, так что <w>на этом</w> я остановлюсь."
                            }
                        },
                        {
                            "text": "We can end the lesson <w>here</w> for today.",
                            "target_lang_translations": {
                                "ru": "Мы можем закончить урок <w>здесь</w> на сегодня."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stop here"
                    ],
                    "target_lang_definitions": {
                        "ru": "В этот момент или на этом этапе процесса, рассказа или деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на этом",
                                "target_lang_sense_clarification": "указывает на остановку в определенной точке процесса"
                            },
                            {
                                "target_lang_word": "здесь",
                                "target_lang_sense_clarification": "указывает на текущий момент в повествовании или действии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "229a531c-c7e4-32d1-861e-53ab9c730b27",
                    "sense_definition": "In this context, matter, or writing.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The briefest description is all that will be attempted <w>here</w>.",
                            "target_lang_translations": {
                                "ru": "Кратчайшее описание — это всё, что будет предпринято <w>здесь</w>."
                            }
                        },
                        {
                            "text": "You say that all people are equal, but <w>here</w> I must disagree.",
                            "target_lang_translations": {
                                "ru": "Вы говорите, что все люди равны, но <w>здесь</w> я должен не согласиться."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В данном контексте, вопросе или тексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здесь",
                                "target_lang_sense_clarification": "в этом случае, в данных обстоятельствах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "e47bc99d-e30f-302d-a383-3e8096f37245",
                    "sense_definition": "Used when offering or giving something to someone.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "offering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Here</w>, take this money for the bus.",
                            "target_lang_translations": {
                                "ru": "<w>Вот</w>, возьми эти деньги на автобус."
                            }
                        },
                        {
                            "text": "<w>Here</w>, I am giving this book to you.",
                            "target_lang_translations": {
                                "ru": "<w>Вот</w>, я дарю тебе эту книгу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "here you go",
                        "here you are"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется при предложении или передаче чего-либо кому-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вот",
                                "target_lang_sense_clarification": "указательная частица при передаче предмета"
                            },
                            {
                                "target_lang_word": "держи",
                                "target_lang_sense_clarification": "повелительное наклонение, используется при передаче из рук в руки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ed38e72-9a6a-3207-abe8-6237fe3dc1dd",
                    "sense_definition": "Said in response to one's name being called to indicate presence.",
                    "learner_level": "A1",
                    "frequency": "Middle",
                    "semantic_group_id": "presence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "When the teacher called my name, I answered <w>here</w>.",
                            "target_lang_translations": {
                                "ru": "Когда учитель назвал мое имя, я ответил: «<w>здесь</w>»."
                            }
                        },
                        {
                            "text": "Smith? — <w>Here</w>!",
                            "target_lang_translations": {
                                "ru": "Смит? — <w>Здесь</w>!"
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Произносится в ответ на перекличку, чтобы подтвердить присутствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здесь",
                                "target_lang_sense_clarification": "стандартный ответ на перекличке"
                            },
                            {
                                "target_lang_word": "я",
                                "target_lang_sense_clarification": "краткий ответ, подтверждающий присутствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee595aa1-7454-3aac-8503-38e8c8b7f1a2",
                    "sense_definition": "Said to command a person or animal to come to the speaker.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "command",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Fido! <w>Here</w>! Come get the ball!",
                            "target_lang_translations": {
                                "ru": "Фидо! <w>Ко мне</w>! Иди за мячом!"
                            }
                        },
                        {
                            "text": "I called my dog by shouting <w>here</w>.",
                            "target_lang_translations": {
                                "ru": "Я подозвал собаку, крикнув «<w>ко мне</w>»."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Команда человеку или животному подойти к говорящему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ко мне",
                                "target_lang_sense_clarification": "стандартная команда для собак"
                            },
                            {
                                "target_lang_word": "сюда",
                                "target_lang_sense_clarification": "указание подойти"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "686847f9-1e0f-3bf4-83df-07f2f9c6b92f",
                    "sense_definition": "This place or location.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "place_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is not far from <w>here</w> to the station.",
                            "target_lang_translations": {
                                "ru": "От <w>сюда</w> недалеко до станции."
                            }
                        },
                        {
                            "text": "<w>Here</w> is where I met my best friend.",
                            "target_lang_translations": {
                                "ru": "<w>Здесь</w> — то место, где я встретил своего лучшего друга."
                            }
                        }
                    ],
                    "common_phrases": [
                        "from here",
                        "near here",
                        "out of here"
                    ],
                    "target_lang_definitions": {
                        "ru": "Это место или локация."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здесь",
                                "target_lang_sense_clarification": "обозначает текущее местоположение как существительное"
                            },
                            {
                                "target_lang_word": "отсюда",
                                "target_lang_sense_clarification": "используется в значении «из этого места» (from here)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
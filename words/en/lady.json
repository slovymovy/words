{
    "word_family": [
        "ladylike",
        "ladyship"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6e2bb4ab-6c96-3d42-a15f-32ec58873f2b",
                    "sense_definition": "An adult female human.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "woman_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>lady</w> at the store helped me find the milk.",
                            "target_lang_translations": {
                                "ru": "<w>Женщина</w> в магазине помогла мне найти молоко."
                            }
                        },
                        {
                            "text": "Please stand up and let that old <w>lady</w> sit down.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, встаньте и дайте этой пожилой <w>женщине</w> сесть."
                            }
                        }
                    ],
                    "synonyms": [
                        "Goddess"
                    ],
                    "common_phrases": [
                        "old lady",
                        "young lady",
                        "nice lady"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взрослая женщина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дама",
                                "target_lang_sense_clarification": "вежливое или официальное обозначение женщины"
                            },
                            {
                                "target_lang_word": "женщина",
                                "target_lang_sense_clarification": "нейтральное обозначение взрослого человека женского пола"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8982d7bc-bdc3-3df4-a618-4079334b8c94",
                    "sense_definition": "A polite form of address to women, often used in the plural.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "woman_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ladies</w> and gentlemen, welcome to our show.",
                            "target_lang_translations": {
                                "ru": "<w>Дамы</w> и господа, добро пожаловать на наше шоу."
                            }
                        },
                        {
                            "text": "Excuse me, <w>ladies</w>, is this seat taken?",
                            "target_lang_translations": {
                                "ru": "Прошу прощения, <w>дамы</w>, это место занято?"
                            }
                        }
                    ],
                    "antonyms": [
                        "antonym(s) of"
                    ],
                    "common_phrases": [
                        "ladies and gentlemen",
                        "ladies' room"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in public announcements or polite greetings"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вежливая форма обращения к женщинам, часто используется во множественном числе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дама",
                                "target_lang_sense_clarification": "используется в вежливых обращениях, например, «дамы и господа»"
                            },
                            {
                                "target_lang_word": "леди",
                                "target_lang_sense_clarification": "подчеркнуто вежливое или светское обращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff788817-e235-33bb-8202-fcbe2f650678",
                    "sense_definition": "Used before another noun to indicate that the person is a woman.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "woman_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My aunt is a very talented <w>lady</w> golfer.",
                            "target_lang_translations": {
                                "ru": "Моя тетя — очень талантливая <w>женщина</w>-гольфист."
                            }
                        },
                        {
                            "text": "A <w>lady</w> doctor examined the patient at the clinic.",
                            "target_lang_translations": {
                                "ru": "<w>Женщина</w>-врач осмотрела пациента в клинике."
                            }
                        }
                    ],
                    "synonyms": [
                        "Goddess"
                    ],
                    "common_phrases": [
                        "lady friend",
                        "cleaning lady",
                        "dinner lady"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often replaced by 'female' or 'woman' in modern contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется перед другим существительным для указания на то, что человек — женщина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "женщина",
                                "target_lang_sense_clarification": "используется как приложение или первая часть составного слова (напр. женщина-врач)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a02de44-cc8e-3b9e-af43-b64fa01a0ab0",
                    "sense_definition": "A woman of high social rank or title; the female equivalent of a lord.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "nobility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>lady</w> of the manor hosted a grand dinner for her guests.",
                            "target_lang_translations": {
                                "ru": "<w>Хозяйка</w> поместья устроила грандиозный ужин для своих гостей."
                            }
                        },
                        {
                            "text": "He bowed respectfully to the <w>lady</w> as she entered the room.",
                            "target_lang_translations": {
                                "ru": "Он почтительно поклонился <w>леди</w>, когда она вошла в комнату."
                            }
                        }
                    ],
                    "synonyms": [
                        "Goddess"
                    ],
                    "antonyms": [
                        "antonym(s) of"
                    ],
                    "common_phrases": [
                        "my lady",
                        "lord and lady"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "often capitalized when used as a specific title"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина высокого социального ранга или титула; женский эквивалент лорда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "леди",
                                "target_lang_sense_clarification": "аристократический титул в Великобритании"
                            },
                            {
                                "target_lang_word": "госпожа",
                                "target_lang_sense_clarification": "почтительное именование владелицы или знатной особы"
                            },
                            {
                                "target_lang_word": "хозяйка",
                                "target_lang_sense_clarification": "в контексте владения поместьем (lady of the manor)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3509713-4016-3265-a1e4-0b1d16c0a590",
                    "sense_definition": "A five-pound note.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slang_money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you lend me a <w>lady</w> until I get paid next week?",
                            "target_lang_translations": {
                                "ru": "Ты можешь одолжить мне <w>пятёрку</w>, пока я не получу зарплату на следующей неделе?"
                            }
                        },
                        {
                            "text": "That ticket cost me a <w>lady</w>, which was quite expensive.",
                            "target_lang_translations": {
                                "ru": "Этот билет стоил мне <w>пятёрку</w>, что было довольно дорого."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "Cockney rhyming slang: Lady Godiva for fiver"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "British/London"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Банкнота в пять фунтов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пятёрка",
                                "target_lang_sense_clarification": "сленговое название денежной купюры номиналом 5"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "a43aee3c-6822-31fb-9f01-1d2e4ce1b63f",
                    "sense_definition": "The title for the primary female deity in Goddess-centered religions, such as Wicca.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "deity",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Followers of the religion worship both the Lord and the <w>Lady</w>.",
                            "target_lang_translations": {
                                "ru": "Последователи этой религии поклоняются и Богу, и <w>Богине</w>."
                            }
                        },
                        {
                            "text": "She offered a prayer to the <w>Lady</w> during the full moon ritual.",
                            "target_lang_translations": {
                                "ru": "Она вознесла молитву <w>Богине</w> во время ритуала полнолуния."
                            }
                        }
                    ],
                    "synonyms": [
                        "Goddess"
                    ],
                    "antonyms": [
                        "antonym(s) of"
                    ],
                    "common_phrases": [
                        "The Lord and the Lady"
                    ],
                    "target_lang_definitions": {
                        "ru": "Титул главного женского божества в религиях, почитающих Богиню, например, в Викке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Богиня",
                                "target_lang_sense_clarification": "верховное женское божество"
                            },
                            {
                                "target_lang_word": "Владычица",
                                "target_lang_sense_clarification": "почтительный религиозный титул"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3ec800e1-2d4a-334c-9524-8b56e3d8b5ef",
                    "sense_definition": "To address someone using the term 'lady', often excessively or formally.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "address_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please do not <w>lady</w> me; just call me by my name.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не надо <w>называть меня «леди»</w>; просто зовите меня по имени."
                            }
                        },
                        {
                            "text": "He kept <w>ladying</w> the customer until she became annoyed.",
                            "target_lang_translations": {
                                "ru": "Он продолжал <w>называть</w> клиентку <w>«леди»</w>, пока она не рассердилась."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обращаться к кому-либо, используя слово «леди», часто чрезмерно или формально."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "называть «леди»",
                                "target_lang_sense_clarification": "использовать слово «леди» в обращении"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
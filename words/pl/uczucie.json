            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5f0ac9a7-2881-309c-a7c9-6dd45dcf535b",
                    "sense_definition": "Stan psychiczny odzwierciedlający stosunek emocjonalny do osób, rzeczy lub zdarzeń; emocja.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emotion_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Matka zawsze okazywała swoim dzieciom wiele ciepła i <w>uczucia</w>.",
                            "target_lang_translations": {
                                "ru": "Мать всегда проявляла к своим детям много тепла и <w>чувства</w>."
                            }
                        },
                        {
                            "text": "To przykre zdarzenie wywołało u niego <w>uczucie</w> smutku i żalu.",
                            "target_lang_translations": {
                                "ru": "Это неприятное событие вызвало у него <w>чувство</w> грусти и сожаления."
                            }
                        }
                    ],
                    "synonyms": [
                        "emocja",
                        "odczucie",
                        "afekt"
                    ],
                    "common_phrases": [
                        "okazywać uczucia",
                        "tłumić uczucia",
                        "mieszać uczucia",
                        "wyrażać uczucia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Психическое состояние, отражающее эмоциональное отношение к людям, вещам или событиям; эмоция."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чувство",
                                "target_lang_sense_clarification": "эмоция, душевное состояние"
                            },
                            {
                                "target_lang_word": "эмоция",
                                "target_lang_sense_clarification": "душевное переживание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a84d080-6631-3bcb-93e9-52802b7bc0c9",
                    "sense_definition": "Silna więź emocjonalna łącząca ludzi, zazwyczaj miłość lub głęboka przyjaźń.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotion_love",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo upływu lat ich <w>uczucie</w> wciąż było żywe i silne.",
                            "target_lang_translations": {
                                "ru": "Несмотря на прошедшие годы, их <w>чувство</w> всё ещё было живым и сильным."
                            }
                        },
                        {
                            "text": "Bohater powieści postanowił wreszcie wyznać jej swoje <w>uczucie</w>.",
                            "target_lang_translations": {
                                "ru": "Герой романа решил наконец признаться ей в своём <w>чувстве</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "przywiązanie",
                        "sentyment",
                        "skłonność"
                    ],
                    "common_phrases": [
                        "wielkie uczucie",
                        "wyznać uczucie",
                        "odwzajemnić uczucie",
                        "stałość uczuć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильная эмоциональная связь между людьми, обычно любовь или глубокая дружба."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чувство",
                                "target_lang_sense_clarification": "любовь, сердечная привязанность"
                            },
                            {
                                "target_lang_word": "любовь",
                                "target_lang_sense_clarification": "глубокая привязанность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37345f5f-f79b-36cc-8ac4-971b794f11ea",
                    "sense_definition": "Wrażenie zmysłowe odbierane przez organizm, na przykład ból, temperatura lub dotyk.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sensation_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po długim spacerze na mrozie towarzyszyło mi nieprzyjemne <w>uczucie</w> chłodu.",
                            "target_lang_translations": {
                                "ru": "После долгой прогулки на морозе меня преследовало неприятное <w>чувство</w> холода."
                            }
                        },
                        {
                            "text": "Lekarz zapytał pacjenta, czy towarzyszy mu <w>uczucie</w> bólu w klatce piersiowej.",
                            "target_lang_translations": {
                                "ru": "Врач спросил пациента, есть ли у него <w>ощущение</w> боли в груди."
                            }
                        }
                    ],
                    "synonyms": [
                        "doznanie",
                        "czucie",
                        "wrażenie"
                    ],
                    "common_phrases": [
                        "uczucie głodu",
                        "uczucie chłodu",
                        "uczucie bólu",
                        "uczucie ulgi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическое ощущение, воспринимаемое организмом, например, боль, температура или прикосновение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ощущение",
                                "target_lang_sense_clarification": "физическое восприятие"
                            },
                            {
                                "target_lang_word": "чувство",
                                "target_lang_sense_clarification": "физическое состояние (напр. голода, холода)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ac204a4-9e42-3d75-b3cc-0b297ab410e2",
                    "sense_definition": "Przeczucie lub intuicyjne wrażenie dotyczące jakiejś sytuacji, często trudne do racjonalnego wyjaśnienia.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "intuition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miałem dziwne <w>uczucie</w>, że ktoś mnie obserwuje z ukrycia.",
                            "target_lang_translations": {
                                "ru": "У меня было странное <w>ощущение</w>, что кто-то наблюдает за мной из укрытия."
                            }
                        },
                        {
                            "text": "Przez cały dzień nie opuszczało mnie <w>uczucie</w>, że zapomniałem o czymś ważnym.",
                            "target_lang_translations": {
                                "ru": "Весь день меня не покидало <w>чувство</w>, что я забыл о чём-то важном."
                            }
                        }
                    ],
                    "synonyms": [

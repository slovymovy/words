    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "abb3c5dd-cf59-3e47-8c04-61761d9c0eb8",
                    "sense_definition": "Up to the time of; <w>until</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_limit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will wait here <w>til</w> five o'clock.",
                            "target_lang_translations": {
                                "ru": "Я буду ждать здесь <w>до</w> пяти часов."
                            }
                        },
                        {
                            "text": "The shop is open from nine <w>til</w> six every day.",
                            "target_lang_translations": {
                                "ru": "Магазин открыт с девяти <w>до</w> шести каждый день."
                            }
                        }
                    ],
                    "synonyms": [
                        "until",
                        "till"
                    ],
                    "common_phrases": [
                        "wait til",
                        "from dawn til dusk",
                        "up til"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often considered an informal shortening of 'until' or spelling of 'till'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вплоть до определенного момента времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до",
                                "target_lang_sense_clarification": "предлог времени"
                            }

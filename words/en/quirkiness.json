    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e981495c-5497-34bc-b6ae-34a8323f81bb",
                    "sense_definition": "The quality of being unusual, peculiar, or unconventional in an appealing or interesting way.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>quirkiness</w> of the decor gave the small cafe a very welcoming atmosphere.",
                            "target_lang_translations": {
                                "ru": "<w>Причудливость</w> декора придавала этому маленькому кафе очень уютную атмосферу."
                            }
                        },
                        {
                            "text": "She is loved by her friends for her humor and endearing <w>quirkiness</w>.",
                            "target_lang_translations": {
                                "ru": "Друзья любят её за чувство юмора и милую <w>чудаковатость</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "eccentricity",
                        "unconventionality",
                        "oddness",
                        "whimsy",
                        "idiosyncrasy"
                    ],
                    "antonyms": [
                        "conventionality",
                        "normality",
                        "ordinariness"
                    ],
                    "common_phrases": [
                        "endearing quirkiness",
                        "sheer quirkiness",
                        "full of quirkiness"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество быть необычным, своеобразным или нетрадиционным, часто в привлекательном или интересном смысле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "причудливость",
                                "target_lang_sense_clarification": "свойство чего-то необычного и затейливого"
                            },
                            {
                                "target_lang_word": "чудаковатость",
                                "target_lang_sense_clarification": "о странном, но милом поведении человека"
                            },
                            {
                                "target_lang_word": "своеобразие",
                                "target_lang_sense_clarification": "уникальность, непохожесть на других"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5cd22489-fa02-3af5-a456-feb25bd1d823",
                    "sense_definition": "A specific strange habit, feature, or detail of a person or thing.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "instance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old car has some mechanical <w>quirkinesses</w>, but it still runs well enough for daily use.",
                            "target_lang_translations": {
                                "ru": "У этой старой машины есть некоторые механические <w>странности</w>, но она всё ещё достаточно хорошо ездит для повседневного использования."
                            }
                        },
                        {
                            "text": "You will soon learn to ignore the minor <w>quirkinesses</w> of this operating system.",
                            "target_lang_translations": {
                                "ru": "Вы скоро научитесь не обращать внимания на мелкие <w>особенности</w> этой операционной системы."
                            }
                        }
                    ],
                    "synonyms": [
                        "quirk",
                        "idiosyncrasy",
                        "peculiarity",
                        "oddity"
                    ],
                    "common_phrases": [
                        "minor quirkinesses"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретная странная привычка, особенность или деталь человека или предмета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "странность",
                                "target_lang_sense_clarification": "необычная черта или деталь"
                            },
                            {
                                "target_lang_word": "причуда",
                                "target_lang_sense_clarification": "неожиданная особенность или каприз"
                            },
                            {
                                "target_lang_word": "особенность",
                                "target_lang_sense_clarification": "специфическая черта работы системы или механизма"
                            }
                        ]
                    }
                }
            ]
        }
    ]

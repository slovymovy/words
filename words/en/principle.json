                    ],
                    "target_lang_definitions": {
                        "ru": "Химическое вещество, особенно растительного происхождения, создающее определенное качество или эффект."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начало",
                                "target_lang_sense_clarification": "химический термин"
                            },
                            {
                                "target_lang_word": "действующее вещество",
                                "target_lang_sense_clarification": "активный компонент"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "aca139f5-2db4-3288-a069-69e27c147ce2",
                    "sense_definition": "Принадлежащий мне или имеющий прямое отношение ко мне (моим вещам, качествам, действиям или родственникам).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "possession_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это <мой> лучший друг, с которым мы знакомы с детства.",
                            "target_lang_translations": {
                                "en": "This is <my> best friend, whom we have known since childhood."
                            }
                        },
                        {
                            "text": "Где <моя> любимая книга, которую я читал вчера?",
                            "target_lang_translations": {
                                "en": "Where is <my> favorite book that I was reading yesterday?"
                            }
                        },
                        {
                            "text": "На <мой> взгляд, это решение было самым правильным.",
                            "target_lang_translations": {
                                "en": "In <my> opinion, this decision was the most correct one."
                            }
                        }
                    ],
                    "antonyms": [
                        "твой",
                        "ваш",
                        "чужой"
                    ],
                    "common_phrases": [
                        "мой друг",
                        "боже мой",
                        "на мой взгляд",
                        "мой дом"
                    ],
                    "target_lang_definitions": {
                        "en": "Belonging to me or directly related to me (my things, qualities, actions, or relatives)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "my",
                                "target_lang_sense_clarification": "possessive determiner"
                            },
                            {
                                "target_lang_word": "mine",
                                "target_lang_sense_clarification": "possessive pronoun (predicative)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e5935c0-e798-3f79-a85f-591de8365ab6",
                    "sense_definition": "Мои родственники, члены моей семьи или близкие мне люди (употребляется как существительное во множественном числе).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "possession_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Все <мои> сейчас на даче, вернутся только в воскресенье.",
                            "target_lang_translations": {
                                "en": "All <my folks> are at the dacha now; they will return only on Sunday."
                            }
                        },
                        {
                            "text": "Передавай привет всем <моим>, когда увидишь их.",
                            "target_lang_translations": {
                                "en": "Say hello to all <my family> when you see them."
                            }
                        }
                    ],
                    "synonyms": [
                        "родные",
                        "родственники",
                        "семья"
                    ],
                    "common_phrases": [
                        "все мои",
                        "привет моим"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to refer to family or close group"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "My relatives, family members, or people close to me (used as a noun in the plural)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "my folks",
                                "target_lang_sense_clarification": "colloquial, referring to family"
                            },
                            {
                                "target_lang_word": "my family",
                                "target_lang_sense_clarification": "referring to relatives"
                            },
                            {
                                "target_lang_word": "mine",
                                "target_lang_sense_clarification": "substantive use referring to people"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fe02db3-787f-3a2f-a6b3-ed949c5c0ac8",
                    "sense_definition": "Мой муж, моя жена или возлюбленный человек (употребляется как существительное).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "possession_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "А <мой> вчера принёс домой огромный букет цветов.",
                            "target_lang_translations": {
                                "en": "And <my husband> brought home a huge bouquet of flowers yesterday."
                            }
                        },
                        {
                            "text": "Твоя звонила, спрашивала, когда ты вернёшься с работы.",
                            "target_lang_translations": {
                                "en": "Your [wife] called asking when you would return from work."
                            }
                        }
                    ],
                    "synonyms": [
                        "муж",
                        "супруг",
                        "партнёр"
                    ],
                    "common_phrases": [
                        "мой сказал",
                        "а мой"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "familiar reference to a partner"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "My husband, my wife, or romantic partner (used as a noun)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "my husband",
                                "target_lang_sense_clarification": "referring to a male spouse"
                            },
                            {
                                "target_lang_word": "my boyfriend",
                                "target_lang_sense_clarification": "referring to a male partner"
                            },
                            {
                                "target_lang_word": "my partner",
                                "target_lang_sense_clarification": "referring to a significant other"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f65be10c-cefe-3f23-84c0-ca1769a2aa7b",
                    "sense_definition": "Форма повелительного наклонения глагола <w>мыть</w> (очищать что-либо с помощью воды).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "action_clean",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Обязательно <мой> руки с мылом перед едой.",
                            "target_lang_translations": {
                                "en": "Be sure to <wash> your hands with soap before eating."
                            }
                        },
                        {
                            "text": "Пожалуйста, <мой> посуду сразу после ужина, не оставляй её в раковине.",
                            "target_lang_translations": {
                                "en": "Please <wash> the dishes immediately after dinner; don't leave them in the sink."
                            }
                        }
                    ],
                    "synonyms": [
                        "очищай",
                        "отмывай"
                    ],
                    "antonyms": [
                        "пачкай",
                        "грязни"
                    ],
                    "common_phrases": [
                        "мой руки",
                        "мой посуду",
                        "иди мой"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "imperative singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Imperative form of the verb 'to wash' (to clean something with water)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wash",
                                "target_lang_sense_clarification": "imperative command"
                            },
                            {
                                "target_lang_word": "clean",
                                "target_lang_sense_clarification": "imperative command"
                            }
                        ]
                    }
                }
            ]
        }
    ]

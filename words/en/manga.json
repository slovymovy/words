                {
                    "sense_id": "7ea381f8-6d5d-3cfa-a218-ddc9c9f74d8e",
                    "sense_definition": "A comic book or graphic novel originating in Japan.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "japanese_comics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She loves reading <w>manga</w> on the weekends.",
                            "target_lang_translations": {
                                "ru": "Она любит читать <w>мангу</w> по выходным."
                            }
                        },
                        {
                            "text": "Many popular anime series are based on a best-selling <w>manga</w>.",
                            "target_lang_translations": {
                                "ru": "Многие популярные аниме-сериалы основаны на бестселлерах <w>манги</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Japanese comic"
                    ],
                    "common_phrases": [
                        "read manga",
                        "manga series",
                        "manga artist"
                    ],
                    "target_lang_definitions": {
                        "ru": "Японские комиксы или графические романы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ма́нга",
                                "target_lang_sense_clarification": "японские комиксы"
                            },
                            {
                                "target_lang_word": "япо́нские ко́миксы",
                                "target_lang_sense_clarification": "описание жанра"
                            }
                        ]

                    "sense_id": "d6f8b241-e6b9-35dc-9872-6b36c603b6a0",
                    "sense_definition": "Określony etap trwania lub rozwoju jakiegoś procesu, zjawiska lub działania.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "stage_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przygotowania do turnieju wkroczyły w końcową <w>fazę</w>.",
                            "target_lang_translations": {
                                "ru": "Подготовка к турниру вступила в завершающую <w>фазу</w>."
                            }
                        },
                        {
                            "text": "Każda <w>faza</w> rozwoju produktu musi być dokładnie zaplanowana.",
                            "target_lang_translations": {
                                "ru": "Каждая <w>фаза</w> разработки продукта должна быть тщательно спланирована."
                            }
                        }
                    ],
                    "synonyms": [
                        "etap",
                        "stadium"
                    ],
                    "common_phrases": [

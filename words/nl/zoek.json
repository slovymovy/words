                    "target_lang_definitions": {
                        "en": "The act or state of searching for something or someone (especially in the expression 'op zoek')",
                        "ru": "Действие или состояние поиска (особенно в выражении 'op zoek')"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "looking for",
                                "target_lang_sense_clarification": "used in the phrase 'to be looking for' (op zoek zijn naar)"
                            },
                            {
                                "target_lang_word": "search",
                                "target_lang_sense_clarification": "used in the phrase 'in search of' (op zoek naar)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "поиск",
                                "target_lang_sense_clarification": "процесс поиска (в выражениях 'op zoek naar' - в поисках)"
                            },
                            {
                                "target_lang_word": "искать",
                                "target_lang_sense_clarification": "функциональный перевод конструкции 'op zoek zijn' (быть в поиске)"
                            }
                        ]
                    }
                }
            ]
        },

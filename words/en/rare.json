                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "variant of rear"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вставать на задние ноги (о лошади); вздыматься."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вставать на дыбы",
                                "target_lang_sense_clarification": "о лошади"
                            },
                            {
                                "target_lang_word": "рваться",
                                "target_lang_sense_clarification": "сильно хотеть (в идиомах)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

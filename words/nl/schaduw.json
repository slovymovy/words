                    "sense_id": "4857a7b9-498b-3f79-9c7f-20aabe456f95",
                    "sense_definition": "Tweede persoon enkelvoud tegenwoordige tijd van <w>schaduwen</w> (bij inversie).",
                    "learner_level": "B1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Schaduw</w> je hem al lang?",
                            "target_lang_translations": {
                                "en": "Have you been <w>shadowing</w> him for long?",
                                "ru": "Ты давно за ним <w>следишь</w>?"
                            }
                        },
                        {
                            "text": "Waarom <w>schaduw</w> je die persoon?",
                            "target_lang_translations": {
                                "en": "Why are you <w>shadowing</w> that person?",
                                "ru": "Почему ты <w>следишь</w> за этим человеком?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "conjugation"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Second person singular present tense of <w>schaduwen</w> (used in inversion).",
                        "ru": "Второе лицо единственного числа настоящего времени от глагола schaduwen (при инверсии)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shadow",
                                "target_lang_sense_clarification": "to follow secretly"
                            },
                            {
                                "target_lang_word": "tail",
                                "target_lang_sense_clarification": "to follow closely"
                            }
                        ],
                        "ru": [

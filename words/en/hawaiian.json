            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cc7b7294-6d2c-3fc7-a93d-3dc26ff4f6f6",
                    "sense_definition": "A native or resident of the state of Hawaii.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "person_resident",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many <w>Hawaiians</w> enjoy surfing and spending time in the ocean.",
                            "target_lang_translations": {
                                "ru": "Многие <w>гавайцы</w> любят заниматься серфингом и проводить время в океане."
                            }
                        },
                        {
                            "text": "As a <w>Hawaiian</w>, she is proud of her state's unique traditions.",
                            "target_lang_translations": {
                                "ru": "Будучи <w>гавайкой</w>, она гордится уникальными традициями своего штата."
                            }
                        }
                    ],
                    "common_phrases": [
                        "native Hawaiian"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уроженец или житель штата Гавайи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гава́ец",
                                "target_lang_sense_clarification": "мужской род"
                            },
                            {
                                "target_lang_word": "гава́йка",
                                "target_lang_sense_clarification": "женский род"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25e1b698-2e81-3087-b2db-8ccd7cca4e61",
                    "sense_definition": "A pizza topped with ham and pineapple.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I ordered a large <w>Hawaiian</w> and a soda for lunch.",
                            "target_lang_translations": {
                                "ru": "Я заказал большую <w>гавайскую пиццу</w> и газировку на обед."
                            }
                        },
                        {
                            "text": "Some people love a <w>Hawaiian</w>, while others dislike fruit on their pizza.",
                            "target_lang_translations": {
                                "ru": "Некоторые люди обожают <w>гавайскую пиццу</w>, в то время как другие не любят фрукты в пицце."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пицца с ветчиной и ананасами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гава́йская пи́цца",
                                "target_lang_sense_clarification": "блюдо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58767c2a-783c-3023-b2b7-940bcdad7b14",
                    "sense_definition": "A descendant of the peoples inhabiting the Hawaiian Islands prior to European contact.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_indigenous",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum exhibit honors the ancient <w>Hawaiians</w> who navigated the Pacific.",
                            "target_lang_translations": {
                                "ru": "Музейная экспозиция посвящена древним <w>гавайцам</w>, которые покоряли Тихий океан."
                            }
                        },
                        {
                            "text": "He is a native <w>Hawaiian</w> working to preserve his ancestral language.",
                            "target_lang_translations": {
                                "ru": "Он коренной <w>гаваец</w>, работающий над сохранением языка своих предков."
                            }

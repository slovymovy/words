    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "665e58b5-3570-3119-942e-b9d0a73d564d",
                    "sense_definition": "Сокращение от <w>некоммерческая</w> <w>организация</w>; юридическое лицо, не имеющее извлечение прибыли в качестве основной цели своей деятельности.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Многие <w>НКО</w> получают государственные гранты на реализацию важных социальных проектов.",
                            "target_lang_translations": {
                                "en": "Many <w>NPOs</w> receive state grants for the implementation of important social projects."
                            }
                        },
                        {
                            "text": "После принятия поправок закон об <w>НКО</w> стал предметом активного обсуждения юристов.",
                            "target_lang_translations": {
                                "en": "After the adoption of amendments, the law on <w>NPOs</w> became a subject of active discussion among lawyers."
                            }
                        }
                    ],
                    "synonyms": [
                        "некоммерческая организация"
                    ],
                    "antonyms": [
                        "коммерческая организация"
                    ],
                    "common_phrases": [
                        "закон об НКО",
                        "деятельность НКО",
                        "регистрация НКО",
                        "социально ориентированные НКО"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Abbreviation for \"non-profit organization\"; a legal entity whose primary goal is not making a profit."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "NPO",
                                "target_lang_sense_clarification": "abbreviation for non-profit organization"
                            },
                            {
                                "target_lang_word": "non-profit organization",
                                "target_lang_sense_clarification": "legal entity type"
                            },
                            {
                                "target_lang_word": "NGO",
                                "target_lang_sense_clarification": "non-governmental organization (often used interchangeably in this context)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b348334-f9a4-37de-84aa-34a4decc9b56",
                    "sense_definition": "Сокращение от <w>небанковская</w> <w>кредитная</w> <w>организация</w>; финансовая организация, имеющая право выполнять лишь отдельные банковские операции.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Расчетная <w>НКО</w> обеспечивает проведение платежей между юридическими лицами без открытия счетов.",
                            "target_lang_translations": {
                                "en": "A settlement <w>NBCO</w> ensures the processing of payments between legal entities without opening accounts."
                            }
                        },
                        {
                            "text": "Центральный банк отозвал лицензию у крупной <w>НКО</w> за систематические нарушения отчетности.",
                            "target_lang_translations": {
                                "en": "The Central Bank revoked the license of a large <w>NBCO</w> for systematic reporting violations."
                            }
                        }
                    ],
                    "synonyms": [
                        "небанковская кредитная организация"
                    ],
                    "common_phrases": [
                        "расчетная НКО",
                        "платежная НКО",
                        "лицензия НКО"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Abbreviation for \"non-bank credit organization\"; a financial institution authorized to perform specific banking operations but not full banking services."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "NBCO",
                                "target_lang_sense_clarification": "financial abbreviation"
                            },
                            {
                                "target_lang_word": "non-bank credit organization",
                                "target_lang_sense_clarification": "financial institution type"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "да́же",
                                "target_lang_sense_clarification": "усилительная частица"
                            },
                            {
                                "target_lang_word": "вообще́",
                                "target_lang_sense_clarification": "в отрицательных предложениях (not even)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b40779c-a14a-34ca-9a54-1af4fb4c541a",
                    "sense_definition": "Used to emphasize a comparative adjective or adverb, suggesting a higher degree.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This book is <w>even</w> better than the last one.",
                            "target_lang_translations": {
                                "ru": "Эта книга <w>ещё</w> лучше, чем предыдущая."
                            }
                        },
                        {
                            "text": "I was strong before, but now I am <w>even</w> stronger.",
                            "target_lang_translations": {
                                "ru": "Я был силён раньше, но теперь я <w>ещё</w> сильнее."
                            }
                        }
                    ],
                    "synonyms": [
                        "still"
                    ],
                    "common_phrases": [
                        "even more",
                        "even better",
                        "even less"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для усиления сравнительной степени прилагательного или наречия, указывая на более высокую степень."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ещё",
                                "target_lang_sense_clarification": "со сравнительной степенью"
                            },
                            {
                                "target_lang_word": "да́же",
                                "target_lang_sense_clarification": "для усиления сравнения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "075a3f62-e728-35a0-bb67-ca3b4bf7b2b9",
                    "sense_definition": "Exactly; just; fully.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He fulfilled his promise <w>even</w> as he had said he would.",
                            "target_lang_translations": {
                                "ru": "Он выполнил своё обещание <w>точно</w> так, как и говорил."
                            }
                        },
                        {
                            "text": "You are leaving tonight? — <w>Even</w> so.",
                            "target_lang_translations": {
                                "ru": "Вы уезжаете сегодня вечером? — <w>Именно</w> так."
                            }
                        }
                    ],
                    "synonyms": [
                        "exactly",
                        "precisely",
                        "definitely"
                    ],
                    "common_phrases": [
                        "even so",
                        "even as"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal or archaic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Точно; именно; полностью."

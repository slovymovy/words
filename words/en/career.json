                    "common_phrases": [
                        "in full career"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Used primarily in literary contexts."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Скорость или быстрый бег (обычно о лошади)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "карьер",
                                "target_lang_sense_clarification": "самый быстрый аллюр"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a71fda1b-ab5c-30ae-9a34-7bc6260f2a74",

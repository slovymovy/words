                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращенное название торгового центра Alabang Town Center на Филиппинах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Таун",

                            {
                                "target_lang_word": "slow",
                                "target_lang_sense_clarification": "mentally slow"
                            },
                            {
                                "target_lang_word": "lethargic",
                                "target_lang_sense_clarification": "drowsy or dull"
                            }
                        ]
                    }
                }
            ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "832b8bba-2aaa-37b7-b14a-71dfc93a6e58",
                    "sense_definition": "Большое празднество по случаю какого-либо события, торжественная церемония.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На семейное <w>торжество</w> собрались все близкие родственники и друзья.",
                            "target_lang_translations": {
                                "en": "All close relatives and friends gathered for the family <w>celebration</w>."
                            }
                        },
                        {
                            "text": "Виновник <w>торжества</w> с улыбкой принимал подарки и поздравления от гостей.",
                            "target_lang_translations": {
                                "en": "The hero of the <w>occasion</w> accepted gifts and congratulations from the guests with a smile."
                            }
                        }
                    ],
                    "synonyms": [
                        "праздник"
                    ],
                    "antonyms": [
                        "будни",
                        "траур"
                    ],
                    "common_phrases": [
                        "семейное торжество",
                        "виновник торжества",
                        "свадебное торжество",
                        "по случаю торжества"
                    ],
                    "target_lang_definitions": {
                        "en": "A large celebration or formal ceremony marking a special event."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "celebration",
                                "target_lang_sense_clarification": "event"
                            },
                            {
                                "target_lang_word": "festivities",
                                "target_lang_sense_clarification": "joyful events"
                            },
                            {
                                "target_lang_word": "ceremony",
                                "target_lang_sense_clarification": "formal event"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05bf494f-a44a-3e4b-aa34-9666e49d3e54",
                    "sense_definition": "Полная победа, большой успех или преобладание одной идеи над другой.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы верим в конечное <w>торжество</w> добра над злом в этом мире.",
                            "target_lang_translations": {
                                "en": "We believe in the ultimate <w>triumph</w> of good over evil in this world."
                            }
                        },
                        {
                            "text": "Этот закон означает <w>торжество</w> справедливости для всех пострадавших граждан.",
                            "target_lang_translations": {
                                "en": "This law signifies the <w>triumph</w> of justice for all affected citizens."
                            }
                        }
                    ],
                    "synonyms": [
                        "победа"
                    ],
                    "antonyms": [
                        "поражение",
                        "крах"
                    ],
                    "common_phrases": [
                        "торжество справедливости",
                        "торжество разума",
                        "полное торжество",
                        "торжество демократии"
                    ],
                    "target_lang_definitions": {
                        "en": "Complete victory, great success, or the prevalence of a principle or idea."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "triumph",
                                "target_lang_sense_clarification": "victory of an idea or cause"
                            },
                            {
                                "target_lang_word": "victory",
                                "target_lang_sense_clarification": "success"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e8fcb65-937b-3153-801e-87361884c04c",
                    "sense_definition": "Чувство радости, гордости и удовлетворения от осознания своего успеха или превосходства.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В его громком голосе звучало нескрываемое <w>торжество</w> победителя.",
                            "target_lang_translations": {
                                "en": "Undisguised <w>triumph</w> of a winner could be heard in his loud voice."
                            }
                        },
                        {
                            "text": "Она посмотрела на свою соперницу с явным <w>торжеством</w> и насмешкой.",
                            "target_lang_translations": {
                                "en": "She looked at her rival with obvious <w>exultation</w> and mockery."
                            }
                        }
                    ],
                    "synonyms": [
                        "ликование",
                        "радость"
                    ],
                    "antonyms": [
                        "уныние",
                        "грусть",
                        "тоска"
                    ],
                    "common_phrases": [
                        "скрытое торжество",
                        "смотреть с торжеством",
                        "нотки торжества",
                        "злорадное торжество"
                    ],
                    "target_lang_definitions": {
                        "en": "A feeling of intense joy, pride, and satisfaction derived from success or superiority."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "triumph",
                                "target_lang_sense_clarification": "feeling of pride"
                            },
                            {
                                "target_lang_word": "exultation",
                                "target_lang_sense_clarification": "joy of success"
                            },
                            {
                                "target_lang_word": "jubilation",
                                "target_lang_sense_clarification": "great happiness"
                            }
                        ]
                    }
                }
            ]
        }

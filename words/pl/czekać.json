{
    "word_family": [
        "poczekać",
        "zaczekać",
        "odczekać",
        "doczekać",
        "wyczekiwać",
        "oczekiwać",
        "przeczekać",
        "poczekalnia"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4ace7d24-485e-3d8a-a535-04b62e3f38d4",
                    "sense_definition": "Pozostawać w jednym miejscu lub nie podejmować działania do momentu, aż coś się wydarzy lub ktoś przyjdzie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "waiting_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy cierpliwie <czekać> na przystanku, aż przyjedzie nasz autobus.",
                            "target_lang_translations": {
                                "ru": "Мы должны терпеливо <ждать> на остановке, пока приедет наш автобус."
                            }
                        },
                        {
                            "text": "Nie lubię <czekać> w długich kolejkach do kasy w supermarkecie.",
                            "target_lang_translations": {
                                "ru": "Я не люблю <ждать> в длинных очередях в кассу в супермаркете."
                            }
                        }
                    ],
                    "synonyms": [
                        "oczekiwać",
                        "wyczekiwać"
                    ],
                    "antonyms": [
                        "spieszyć się"
                    ],
                    "common_phrases": [
                        "czekać na autobus",
                        "czekać w kolejce",
                        "kazać na siebie czekać",
                        "czekać na telefon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться в одном месте или бездействовать до тех пор, пока что-то не произойдет или кто-то не придет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ждать",
                                "target_lang_sense_clarification": "проводить время в ожидании"
                            },
                            {
                                "target_lang_word": "ожидать",
                                "target_lang_sense_clarification": "более формально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30456d6a-a59e-3ddc-8fbd-72f91e02eaa8",
                    "sense_definition": "Przerywać wykonywanie jakiejś czynności lub wstrzymywać się z nią na chwilę; często używane w trybie rozkazującym, aby kogoś zatrzymać.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "imperative_stop",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Czekaj>, nie możesz wejść do środka bez ważnego biletu!",
                            "target_lang_translations": {
                                "ru": "<Постой>, ты не можешь войти внутрь без действительного билета!"
                            }
                        },
                        {
                            "text": "<Czekajcie>, chyba zapomniałem wziąć kluczyków do samochodu z biurka.",
                            "target_lang_translations": {
                                "ru": "<Подождите>, кажется, я забыл взять ключи от машины со стола."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czekaj chwilę",
                        "czekaj no"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used as an interjection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прерывать действие или останавливаться на время; часто используется в повелительном наклонении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подождать",
                                "target_lang_sense_clarification": "повременить"
                            },
                            {
                                "target_lang_word": "постой",
                                "target_lang_sense_clarification": "остановись (разг.)"
                            },
                            {
                                "target_lang_word": "погоди",
                                "target_lang_sense_clarification": "подожди (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2a4d0ea-2769-3907-8454-2c1ed0b9f635",
                    "sense_definition": "Spodziewać się nadejścia kogoś lub wystąpienia czegoś, często z nadzieją, niepokojem lub tęsknotą.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "waiting_mental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z wielką niecierpliwością <czekamy> na oficjalne wyniki egzaminu końcowego.",
                            "target_lang_translations": {
                                "ru": "Мы с большим нетерпением <ждём> официальных результатов выпускного экзамена."
                            }
                        },
                        {
                            "text": "Cała rodzina <czekała> na powrót ojca z dalekiej podróży służbowej.",
                            "target_lang_translations": {
                                "ru": "Вся семья <ждала> возвращения отца из дальней командировки."
                            }
                        }
                    ],
                    "synonyms": [
                        "oczekiwać",
                        "wyglądać"
                    ],
                    "common_phrases": [
                        "czekać na cud",
                        "czekać na okazję",
                        "czekać z niecierpliwością",
                        "czekać na list"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рассчитывать на приход кого-либо или наступление чего-либо, часто с надеждой или волнением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ждать",
                                "target_lang_sense_clarification": "надеяться на появление"
                            },
                            {
                                "target_lang_word": "ожидать",
                                "target_lang_sense_clarification": "предвкушать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "daa52724-0075-3a77-b91a-e654a1623fdf",
                    "sense_definition": "Być gotowym do użycia, dostępnym lub przygotowanym dla kogoś (o rzeczach).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "waiting_objects",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gorący obiad już <czeka> na stole, więc chodźcie szybko jeść.",
                            "target_lang_translations": {
                                "ru": "Горячий обед уже <ждёт> на столе, так что идите скорее есть."
                            }
                        },
                        {
                            "text": "Na parkingu przed lotniskiem <czekała> na nas zamówiona wcześniej taksówka.",
                            "target_lang_translations": {
                                "ru": "На парковке перед аэропортом нас <ждало> заказанное заранее такси."
                            }
                        }
                    ],
                    "common_phrases": [
                        "obiad czeka",
                        "praca czeka",
                        "taksówka czeka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть готовым к использованию или доступным для кого-либо (о предметах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ждать",
                                "target_lang_sense_clarification": "быть готовым"
                            },
                            {
                                "target_lang_word": "дожидаться",
                                "target_lang_sense_clarification": "оставаться до востребования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1954ea4b-77f3-3308-9404-a412669f843a",
                    "sense_definition": "Być nieuchronnym w przyszłości; być czymś, co kogoś spotka lub jest mu przeznaczone.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "waiting_destiny",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wiemy, co nas <czeka> w przyszłości, ale musimy być dobrej myśli.",
                            "target_lang_translations": {
                                "ru": "Мы не знаем, что нас <ждёт> в будущем, но мы должны надеяться на лучшее."
                            }
                        },
                        {
                            "text": "Jeśli nie odrobisz lekcji, <czeka> cię poważna rozmowa z nauczycielem.",
                            "target_lang_translations": {
                                "ru": "Если ты не сделаешь уроки, тебя <ждёт> серьёзный разговор с учителем."
                            }
                        }
                    ],
                    "common_phrases": [
                        "co nas czeka",
                        "czeka cię niespodzianka",
                        "czeka go kara",
                        "czeka cię nagroda"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть неизбежным в будущем; быть уготованным кому-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ждать",
                                "target_lang_sense_clarification": "быть сужденным"
                            },
                            {
                                "target_lang_word": "предстоять",
                                "target_lang_sense_clarification": "о будущем событии"
                            },
                            {
                                "target_lang_word": "ожидать",
                                "target_lang_sense_clarification": "прогнозироваться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be910865-5a8f-3618-9942-4ee739a85ad0",
                    "sense_definition": "W grze w karty: nie podbijać stawki, ale nie rezygnować z dalszej gry.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "waiting_games",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony gracz postanowił <czekać>, aby zobaczyć, co zrobią jego przeciwnicy.",
                            "target_lang_translations": {
                                "ru": "Опытный игрок решил <пропустить ход>, чтобы посмотреть, что сделают его противники."
                            }
                        },
                        {
                            "text": "Zamiast ryzykować i podbijać stawkę, wolał <czekać> w tej turze.",
                            "target_lang_translations": {
                                "ru": "Вместо того чтобы рисковать и повышать ставку, он предпочел <пропустить ход> в этом раунде."
                            }
                        }
                    ],
                    "synonyms": [
                        "pasować"
                    ],
                    "target_lang_definitions": {
                        "ru": "В карточных играх: не повышать ставку, но не выходить из игры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пропускать ход",
                                "target_lang_sense_clarification": "в игре"
                            },
                            {
                                "target_lang_word": "выжидать",
                                "target_lang_sense_clarification": "стратегически"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
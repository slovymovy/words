{
    "word_family": [
        "krawat"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "198d58b6-da98-3762-8718-990f9d3547da",
                    "sense_definition": "Plama sierści o odmiennej barwie (zwykle białej) znajdująca się pod szyją zwierzęcia, na przykład kota lub psa.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój czarny kot ma charakterystyczną białą <krawatkę>.",
                            "target_lang_translations": {
                                "ru": "У моего черного кота есть характерная белая <манишка>."
                            }
                        },
                        {
                            "text": "Pies z brązową <krawatką> biegał wesoło po podwórku.",
                            "target_lang_translations": {
                                "ru": "Пес с коричневой <манишкой> весело бегал по двору."
                            }
                        }
                    ],
                    "common_phrases": [
                        "biała krawatka",
                        "kot z krawatką"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пятно шерсти другого цвета (обычно белого) под шеей животного, например, кошки или собаки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "манишка",
                                "target_lang_sense_clarification": "пятно на груди животного"
                            },
                            {
                                "target_lang_word": "галстук",
                                "target_lang_sense_clarification": "разговорное название пятна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30dc38f8-3b7a-3683-8c59-f1cc4b657eee",
                    "sense_definition": "Metalowe uszko lub element biżuterii służący do zawieszenia wisiorka na łańcuszku.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Złota <krawatka> pękła i musiałam oddać wisiorek do naprawy.",
                            "target_lang_translations": {
                                "ru": "Золотое <ушко> сломалось, и мне пришлось отдать кулон в ремонт."
                            }
                        },
                        {
                            "text": "Jubiler dobrał ozdobną <krawatkę> pasującą do mojego medalika.",
                            "target_lang_translations": {
                                "ru": "Ювелир подобрал декоративное <ушко>, подходящее к моему медальону."
                            }
                        }
                    ],
                    "common_phrases": [
                        "srebrna krawatka",
                        "krawatka do wisiorka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Металлическое ушко или элемент ювелирного изделия для подвешивания кулона на цепочку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ушко",
                                "target_lang_sense_clarification": "часть кулона"
                            },
                            {
                                "target_lang_word": "бейл",
                                "target_lang_sense_clarification": "держатель кулона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ea21f95-5ff1-338b-95c0-89a1fb5394c9",
                    "sense_definition": "Ozdobna wstążka, kokarda lub chustka wiązana pod szyją, noszona jako element stroju.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "neckwear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do eleganckiej białej bluzki założyła jedwabną <krawatkę>.",
                            "target_lang_translations": {
                                "ru": "К элегантной белой блузке она надела шелковый <бант>."
                            }
                        },
                        {
                            "text": "Staromodna <krawatka> dodawała jej strojowi wyjątkowego uroku.",
                            "target_lang_translations": {
                                "ru": "Старомодный <шейный бант> придавал её наряду особое очарование."
                            }
                        }
                    ],
                    "synonyms": [
                        "halsztuch",
                        "szyjnica"
                    ],
                    "common_phrases": [
                        "jedwabna krawatka",
                        "wiązać krawatkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративная лента, бант или платок, повязываемые под шеей как элемент одежды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шейный бант",
                                "target_lang_sense_clarification": "украшение"
                            },
                            {
                                "target_lang_word": "галстук-бант",
                                "target_lang_sense_clarification": "тип галстука"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1806760-ba8e-351a-ad03-60f611f9678f",
                    "sense_definition": "Papierowa etykieta naklejana na szyjkę butelki, zazwyczaj z piwem lub winem, umieszczona nad etykietą główną.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Data ważności produktu została nadrukowana na <krawatce> butelki.",
                            "target_lang_translations": {
                                "ru": "Срок годности продукта был напечатан на <кольеретке> бутылки."
                            }
                        },
                        {
                            "text": "Kolekcjonerzy często zbierają nie tylko kapsle, ale i <krawatki> z butelek.",
                            "target_lang_translations": {
                                "ru": "Коллекционеры часто собирают не только пробки, но и <кольеретки> с бутылок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "krawatka na butelce",
                        "krawatka piwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бумажная этикетка, наклеиваемая на горлышко бутылки (обычно с пивом или вином) над основной этикеткой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кольеретка",
                                "target_lang_sense_clarification": "этикетка на горлышке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10522684-38c3-3612-ad73-fc153075a4e1",
                    "sense_definition": "Męski krawat (określenie używane regionalnie w Krakowie).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "neckwear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój wujek z Krakowa zawsze zakłada elegancką <krawatkę> do garnituru.",
                            "target_lang_translations": {
                                "ru": "Мой дядя из Кракова всегда надевает элегантный <галстук> к костюму."
                            }
                        },
                        {
                            "text": "Muszę kupić nową <krawatkę>, bo stara już się zużyła.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новый <галстук>, потому что старый уже износился."
                            }
                        }
                    ],
                    "synonyms": [
                        "krawat",
                        "zwis męski"
                    ],
                    "common_phrases": [
                        "elegancka krawatka"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Kraków"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужской галстук (региональное краковское название)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "галстук",
                                "target_lang_sense_clarification": "мужской аксессуар"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
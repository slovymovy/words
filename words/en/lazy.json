            "senses": [
                {
                    "sense_id": "07fca75c-21dc-3af2-8fe2-6fabea774dbe",
                    "sense_definition": "Unwilling to do work or make an effort.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is too <w>lazy</w> to clean his room today.",
                            "target_lang_translations": {
                                "ru": "Он слишком <w>ленив</w>, чтобы убраться в своей комнате сегодня."
                            }
                        },
                        {
                            "text": "Get out of bed and do some work, you <w>lazy</w> bones!",
                            "target_lang_translations": {
                                "ru": "Вставай с кровати и займись делом, ты, <w>лентяй</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "idle",
                        "indolent",
                        "slothful",
                        "bone-idle",
                        "work-shy"
                    ],
                    "antonyms": [
                        "active",
                        "diligent",
                        "industrious"
                    ],
                    "common_phrases": [
                        "lazy bones",
                        "lazy person"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не желающий работать или прилагать усилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ленивый",
                                "target_lang_sense_clarification": "о человеке или животном"
                            }
                        ]

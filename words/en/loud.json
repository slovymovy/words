    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e9bd0492-625b-33c3-ba38-fea238c13101",
                    "sense_definition": "Making a lot of noise; having a high volume or great intensity of sound.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sound_intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please turn the volume down because the music is too <w>loud</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, убавь громкость, потому что музыка слишком <w>громкая</w>."
                            }
                        },
                        {
                            "text": "We heard a <w>loud</w> noise coming from the street.",
                            "target_lang_translations": {
                                "ru": "Мы услышали <w>громкий</w> шум, доносящийся с улицы."
                            }
                        },
                        {
                            "text": "He spoke in a <w>loud</w> voice so everyone could hear him.",
                            "target_lang_translations": {
                                "ru": "Он говорил <w>громким</w> голосом, чтобы все могли его слышать."
                            }
                        }
                    ],
                    "synonyms": [
                        "noisy",
                        "blaring",
                        "stentorian",
                        "strong",
                        "powerful"
                    ],
                    "antonyms": [
                        "quiet",
                        "silent",
                        "soft",
                        "low",
                        "weak"
                    ],
                    "common_phrases": [
                        "loud noise",
                        "loud voice",
                        "loud music",
                        "loud bang"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издающий много шума; имеющий высокую громкость или интенсивность звука."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "громкий",
                                "target_lang_sense_clarification": "о звуке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "968d22a4-ce36-3769-8ebb-bde61a024fac",
                    "sense_definition": "Used to describe a person, event, or thing that produces a lot of noise.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sound_intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>loud</w> party next door kept us awake all night.",
                            "target_lang_translations": {
                                "ru": "<w>Шумная</w> вечеринка по соседству не давала нам спать всю ночь."
                            }
                        },
                        {
                            "text": "They are a very <w>loud</w> family who love to shout and laugh.",
                            "target_lang_translations": {
                                "ru": "Это очень <w>шумная</w> семья, которая любит кричать и смеяться."
                            }
                        }
                    ],
                    "synonyms": [
                        "noisy"
                    ],
                    "antonyms": [
                        "quiet",
                        "calm"
                    ],
                    "common_phrases": [
                        "loud party",
                        "loud crowd",
                        "loud neighbors"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для описания человека, события или предмета, производящего много шума."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шумный",
                                "target_lang_sense_clarification": "производящий шум"
                            },
                            {
                                "target_lang_word": "громкий",
                                "target_lang_sense_clarification": "о голосе или событии"
                            }
                        ]
                    }
                },
                {

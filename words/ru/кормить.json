    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "400ffc38-36f8-3016-8f83-743f1ced5ce9",
                    "sense_definition": "Давать еду человеку или животному, помогать принимать пищу.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "feeding_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама терпеливо <кормит> маленького сына манной кашей с ложки.",
                            "target_lang_translations": {
                                "en": "Mom is patiently <feeding> her little son semolina porridge with a spoon."
                            }
                        },
                        {
                            "text": "По утрам я всегда <кормлю> свою собаку перед прогулкой.",
                            "target_lang_translations": {
                                "en": "In the mornings, I always <feed> my dog before the walk."
                            }
                        }
                    ],
                    "synonyms": [
                        "питать"
                    ],
                    "common_phrases": [
                        "кормить собаку",
                        "кормить с ложки",
                        "кормить ужином",
                        "пора кормить"
                    ],
                    "target_lang_definitions": {
                        "en": "To give food to a person or animal; to help someone eat."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "feed",
                                "target_lang_sense_clarification": "give food to"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47fe56ea-688f-3e87-8473-d41dd0fc55e3",
                    "sense_definition": "Предоставлять пищу, угощать (обычно о заведениях или гостеприимстве).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "feeding_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этой школьной столовой детей <кормят> очень вкусно и сытно.",
                            "target_lang_translations": {
                                "en": "In this school cafeteria, they <feed> the children very tasty and filling meals."
                            }
                        },
                        {
                            "text": "Бабушка любит <кормить> гостей своими фирменными домашними пирогами.",
                            "target_lang_translations": {
                                "en": "Grandma loves to <feed> guests her signature homemade pies."
                            }
                        }
                    ],
                    "synonyms": [
                        "питать"
                    ],
                    "common_phrases": [
                        "вкусно кормить",
                        "кормить обедом",
                        "кормить гостей"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To provide food for or serve meals to (usually regarding hospitality or establishments)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "feed",
                                "target_lang_sense_clarification": "provide meals for"
                            },
                            {
                                "target_lang_word": "serve",
                                "target_lang_sense_clarification": "provide food to"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c36a493-2d4e-3302-a8f7-69dfc364a5de",
                    "sense_definition": "Питать ребёнка или детёныша своим молоком (о матери или самке животного).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "nursing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врачи рекомендуют <кормить> ребёнка грудью как можно дольше.",
                            "target_lang_translations": {
                                "en": "Doctors recommend <breastfeeding> the baby for as long as possible."
                            }
                        },
                        {
                            "text": "Дворовая кошка лежит в углу и <кормит> своих новорождённых котят.",
                            "target_lang_translations": {
                                "en": "A stray cat is lying in the corner and <nursing> her newborn kittens."
                            }
                        }
                    ],
                    "synonyms": [
                        "питать"
                    ],
                    "common_phrases": [
                        "кормить грудью",
                        "кормить щенков",
                        "кормить молоком"
                    ],
                    "target_lang_definitions": {
                        "en": "To nourish a baby or young animal with milk from the breast."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "breastfeed",
                                "target_lang_sense_clarification": "feed a baby"
                            },
                            {
                                "target_lang_word": "nurse",
                                "target_lang_sense_clarification": "suckle"
                            },
                            {
                                "target_lang_word": "suckle",
                                "target_lang_sense_clarification": "feed at the breast"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0320b30-6e22-3cc5-96be-45cbd6034058",
                    "sense_definition": "Обеспечивать средствами к существованию, содержать кого-либо материально.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "financial_support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ему приходится много работать на двух работах, чтобы <кормить> большую семью.",
                            "target_lang_translations": {
                                "en": "He has to work hard at two jobs to <feed> his large family."
                            }
                        },
                        {
                            "text": "Этот маленький огород <кормит> нас овощами всю долгую зиму.",
                            "target_lang_translations": {
                                "en": "This small vegetable garden <feeds> us with vegetables all the long winter."
                            }
                        }
                    ],
                    "synonyms": [
                        "содержать"
                    ],
                    "common_phrases": [
                        "кормить семью",
                        "работа кормит",
                        "земля кормит"
                    ],
                    "target_lang_definitions": {
                        "en": "To provide the means of subsistence; to support financially."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "support",
                                "target_lang_sense_clarification": "financially"
                            },
                            {
                                "target_lang_word": "feed",
                                "target_lang_sense_clarification": "provide for"
                            },
                            {
                                "target_lang_word": "keep",
                                "target_lang_sense_clarification": "maintain"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe18f18a-df29-3c8a-9926-930969c3289c",
                    "sense_definition": "Много обещать, но не выполнять обещанного (обычно в устойчивых выражениях).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "idiomatic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Начальник продолжает <кормить> нас пустыми обещаниями о повышении зарплаты.",
                            "target_lang_translations": {
                                "en": "The boss continues to <feed> us empty promises about a salary raise."
                            }
                        },
                        {
                            "text": "Хватит <кормить> меня завтраками, просто верни долг сегодня!",
                            "target_lang_translations": {
                                "en": "Stop <feeding> me excuses, just return the debt today!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "кормить завтраками",
                        "кормить обещаниями"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in figurative sense"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To make empty promises or delay fulfillment (figurative)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "feed",
                                "target_lang_sense_clarification": "with promises"
                            }
                        ]
                    }
                }
            ]
        }
    ]

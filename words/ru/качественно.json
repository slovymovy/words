    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "9b60cd91-b997-372f-9098-4234f491619e",
                    "sense_definition": "Выполняя что-либо с высоким качеством, добротно, хорошо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "high_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Строители обещали выполнить ремонт быстро и <w>качественно</w>.",
                            "target_lang_translations": {
                                "en": "The builders promised to do the renovation quickly and <w>to a high standard</w>."
                            }
                        },
                        {
                            "text": "Чтобы <w>качественно</w> подготовиться к экзамену, нужно много времени.",
                            "target_lang_translations": {
                                "en": "To prepare <w>properly</w> for the exam, you need a lot of time."
                            }
                        }
                    ],
                    "synonyms": [
                        "добротно",
                        "хорошо",
                        "отлично",
                        "безупречно"
                    ],
                    "antonyms": [
                        "некачественно",
                        "плохо",
                        "халтурно"
                    ],
                    "common_phrases": [
                        "качественно сделать",
                        "качественно выполнить",
                        "быстро и качественно",
                        "качественно работать"
                    ],
                    "target_lang_definitions": {
                        "en": "Performing something with high quality, soundly, or well."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "well",
                                "target_lang_sense_clarification": "general adverb for doing something good"
                            },
                            {
                                "target_lang_word": "to a high standard",
                                "target_lang_sense_clarification": "performing work or tasks with excellence"
                            },
                            {
                                "target_lang_word": "properly",
                                "target_lang_sense_clarification": "thoroughly and correctly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "607f3fd5-cb46-3c9b-bc94-65ff55b78d1f",
                    "sense_definition": "С точки зрения качества, свойств или сущности (обычно в противопоставлении количеству).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "qualitative_aspect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это был <w>качественно</w> новый этап в развитии компании.",
                            "target_lang_translations": {
                                "en": "It was a <w>qualitatively</w> new stage in the company's development."
                            }
                        },
                        {
                            "text": "Ситуация изменилась не количественно, а <w>качественно</w>.",
                            "target_lang_translations": {
                                "en": "The situation changed not quantitatively, but <w>qualitatively</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "количественно"
                    ],
                    "common_phrases": [
                        "качественно новый",
                        "качественно иной",
                        "качественно отличаться",
                        "качественно изменить"
                    ],
                    "target_lang_definitions": {
                        "en": "From the point of view of quality, properties, or essence (usually opposed to quantity)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "qualitatively",
                                "target_lang_sense_clarification": "relating to the nature or standard of something rather than its amount"
                            }
                        ]
                    }
                }
            ]
        },
        {

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цель",
                                "target_lang_sense_clarification": "намерение, назначение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d70cc03a-bf0f-3039-9e59-5368aad93b15",
                    "sense_definition": "Death or destruction.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He met a terrible <w>end</w> in the jungle.",
                            "target_lang_translations": {
                                "ru": "Он встретил ужасный <w>конец</w> в джунглях."
                            }
                        },
                        {
                            "text": "I hope the <w>end</w> comes quickly and painlessly.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, <w>смерть</w> наступит быстро и безболезненно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "meet one's end"
                    ],
                    "target_lang_definitions": {
                        "ru": "Смерть или уничтожение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смерть",
                                "target_lang_sense_clarification": "гибель"
                            },
                            {
                                "target_lang_word": "коне́ц",
                                "target_lang_sense_clarification": "разг. гибель, кончина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e84913b7-1405-32d6-8f9f-8781fa2d4b1a",
                    "sense_definition": "That which is left; a remnant or fragment.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_remnant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We found various odds and <w>ends</w> in the drawer.",
                            "target_lang_translations": {
                                "ru": "Мы нашли в ящике разные мелочи и <w>остатки</w>."
                            }
                        },
                        {
                            "text": "She used the candle <w>ends</w> to make a new light.",
                            "target_lang_translations": {
                                "ru": "Она использовала <w>огарки</w> свечей, чтобы сделать новый светильник."
                            }
                        }
                    ],
                    "common_phrases": [
                        "odds and ends"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что осталось; остаток или фрагмент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остаток",
                                "target_lang_sense_clarification": "оставшаяся часть"
                            },
                            {
                                "target_lang_word": "огарок",
                                "target_lang_sense_clarification": "остаток свечи"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bbb6edb5-6607-380d-8b86-a28887af91bb",
                    "sense_definition": "To come to a stop or finish.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_stop",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lesson will <w>end</w> when the bell rings.",
                            "target_lang_translations": {
                                "ru": "Урок <w>закончится</w>, когда прозвенит звонок."
                            }
                        },
                        {
                            "text": "Is this movie never going to <w>end</w>?",
                            "target_lang_translations": {
                                "ru": "Этот фильм когда-нибудь <w>кончится</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "finish",
                        "stop",
                        "terminate",
                        "conclude"
                    ],
                    "antonyms": [
                        "begin",
                        "start",
                        "continue"
                    ],
                    "common_phrases": [
                        "end up",
                        "end in"
                    ],
                    "target_lang_definitions": {

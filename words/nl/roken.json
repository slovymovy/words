            "senses": [
                {
                    "sense_id": "2c736d94-b43c-313f-b932-6e4e306d2cb5",
                    "sense_definition": "Tabak of andere middelen consumeren door de rook ervan in te ademen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij wil proberen te stoppen met <w>roken</w>.",
                            "target_lang_translations": {
                                "ru": "Он хочет попытаться бросить <w>курить</w>."
                            }
                        },
                        {
                            "text": "Tijdens de pauze <w>roken</w> ze vaak een sigaret buiten.",
                            "target_lang_translations": {
                                "ru": "Во время перерыва они часто <w>курят</w> сигарету на улице."
                            }
                        }
                    ],
                    "synonyms": [
                        "paffen"
                    ],
                    "common_phrases": [
                        "stoppen met roken",
                        "verboden te roken",
                        "als een ketter roken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Употреблять табак или другие вещества, вдыхая их дым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "курить",
                                "target_lang_sense_clarification": "табак"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "167e40c8-fe50-342b-b123-9c2d6106375e",
                    "sense_definition": "*verleden tijd* meervoud van <w>ruiken</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "form_ruiken",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij <w>roken</w> de geur van versgebakken brood.",
                            "target_lang_translations": {
                                "ru": "Мы <w>чувствовали</w> запах свежеиспеченного хлеба."
                            }
                        },
                        {
                            "text": "Toen ze de keuken binnenkwamen, <w>roken</w> ze dat het eten was aangebrand.",
                            "target_lang_translations": {
                                "ru": "Когда они вошли на кухню, они <w>почувствовали</w>, что еда подгорела."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "*Прошедшее время, множественное число* от <w>ruiken</w> (чувствовать запах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чувствовали запах",
                                "target_lang_sense_clarification": "прошедшее время, мн. ч."
                            },
                            {
                                "target_lang_word": "нюхали",
                                "target_lang_sense_clarification": "прошедшее время, мн. ч."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf188b10-47cf-3b84-9842-69582d850d99",
                    "sense_definition": "Rook uitstoten of afgeven.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "emission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude kachel <w>rookte</w> zo erg dat het alarm afging.",
                            "target_lang_translations": {
                                "ru": "Старая печь так сильно <w>дымила</w>, что сработала сигнализация."
                            }
                        },
                        {
                            "text": "Kijk hoe die fabrieksschoorsteen <w>rookt</w>.",
                            "target_lang_translations": {
                                "ru": "Посмотри, как <w>дымит</w> эта заводская труба."
                            }
                        }
                    ],
                    "synonyms": [
                        "paffen"
                    ],
                    "common_phrases": [
                        "de schoorsteen rookt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испускать или выделять дым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дымить",
                                "target_lang_sense_clarification": "испускать дым"
                            },
                            {
                                "target_lang_word": "дымиться",
                                "target_lang_sense_clarification": "выделять дым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39df7486-81f9-35e0-a7fe-7df66c49d5b2",
                    "sense_definition": "Voedsel conserveren en op smaak brengen door het langdurig in rook te hangen.",

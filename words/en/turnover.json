                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отложно́й",
                                "target_lang_sense_clarification": "о воротнике"
                            },
                            {
                                "target_lang_word": "с отворо́том",
                                "target_lang_sense_clarification": "о манжетах"
                            }
                        ]
                    }
                }
            ]
        }
    ]

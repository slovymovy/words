{
    "word_family": [
        "resourceful",
        "resourcefulness",
        "resourceless"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0f935703-a637-33d7-b3e5-d3b90e8b24db",
                    "sense_definition": "Something that can be used to help achieve an aim, especially a book, website, or equipment that provides information.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The library is a useful <w>resource</w> for students working on their projects.",
                            "target_lang_translations": {
                                "ru": "Библиотека — полезный <w>ресурс</w> для студентов, работающих над своими проектами."
                            }
                        },
                        {
                            "text": "This website is a valuable <w>resource</w> for learning English grammar.",
                            "target_lang_translations": {
                                "ru": "Этот веб-сайт — ценный <w>ресурс</w> для изучения английской грамматики."
                            }
                        }
                    ],
                    "synonyms": [
                        "tool",
                        "aid",
                        "guide",
                        "source"
                    ],
                    "common_phrases": [
                        "online resources",
                        "learning resources",
                        "valuable resource",
                        "teaching resources"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то, что можно использовать для достижения цели, особенно книга, веб-сайт или оборудование, предоставляющее информацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ресу́рс",
                                "target_lang_sense_clarification": "источник информации, веб-сайт"
                            },
                            {
                                "target_lang_word": "исто́чник",
                                "target_lang_sense_clarification": "информации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3f0426d-dacf-3f7d-8b87-8d5b8e6a20da",
                    "sense_definition": "Something that one uses to achieve an objective, such as raw materials, money, or personnel.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The country is rich in natural <w>resources</w> like oil and coal.",
                            "target_lang_translations": {
                                "ru": "Страна богата природными <w>ресурсами</w>, такими как нефть и уголь."
                            }
                        },
                        {
                            "text": "We have limited <w>resources</w> available to complete this large project.",
                            "target_lang_translations": {
                                "ru": "У нас ограниченные <w>ресурсы</w> для завершения этого крупного проекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "asset",
                        "supply",
                        "means",
                        "fund",
                        "reserve"
                    ],
                    "common_phrases": [
                        "natural resources",
                        "human resources",
                        "financial resources",
                        "limited resources",
                        "waste resources"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что используется для достижения цели, например сырьё, деньги или персонал."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ресу́рс",
                                "target_lang_sense_clarification": "природный, человеческий, финансовый"
                            },
                            {
                                "target_lang_word": "запа́с",
                                "target_lang_sense_clarification": "резерв"
                            },
                            {
                                "target_lang_word": "сре́дство",
                                "target_lang_sense_clarification": "материальные ценности, деньги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f833a103-02af-3e8a-b8f8-92c5515fa648",
                    "sense_definition": "Hardware or software that is accessible by a computer, network, or another object connected to a computer.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Video games often require a lot of system <w>resources</w> to run smoothly.",
                            "target_lang_translations": {
                                "ru": "Видеоигры часто требуют много системных <w>ресурсов</w> для плавной работы."
                            }
                        },
                        {
                            "text": "You need a password to access the shared network <w>resources</w>.",
                            "target_lang_translations": {
                                "ru": "Вам нужен пароль для доступа к общим сетевым <w>ресурсам</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "system resources",
                        "network resources",
                        "shared resources"
                    ],
                    "target_lang_definitions": {
                        "ru": "Аппаратное или программное обеспечение, доступное компьютеру или сети."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ресу́рс",
                                "target_lang_sense_clarification": "системный, сетевой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "389c6e8c-9c56-3af8-b125-25bbe2b20628",
                    "sense_definition": "A person's capacity to deal with difficulty; the ability to find quick and clever ways to overcome problems.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She had to rely on her inner <w>resources</w> to survive the difficult ordeal.",
                            "target_lang_translations": {
                                "ru": "Ей пришлось полагаться на свои внутренние <w>ресурсы</w>, чтобы пережить это тяжелое испытание."
                            }
                        },
                        {
                            "text": "He is a man of great <w>resource</w> who can solve almost any problem.",
                            "target_lang_translations": {
                                "ru": "Он человек большой <w>находчивости</w>, который может решить почти любую проблему."
                            }
                        }
                    ],
                    "synonyms": [
                        "ingenuity",
                        "initiative",
                        "resourcefulness",
                        "capability"
                    ],
                    "common_phrases": [
                        "inner resources",
                        "man of resource",
                        "woman of resource"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность человека справляться с трудностями; умение находить быстрые и умные способы решения проблем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нахо́дчивость",
                                "target_lang_sense_clarification": "смекалка, умение найти выход"
                            },
                            {
                                "target_lang_word": "изобрета́тельность",
                                "target_lang_sense_clarification": "способность придумывать решения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a6290e82-2d54-3946-ac2b-51a731a7976a",
                    "sense_definition": "To provide a person or organization with the materials, money, staff, or other assets needed to function effectively.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new hospital is well <w>resourced</w> with modern technology and staff.",
                            "target_lang_translations": {
                                "ru": "Новая больница хорошо <w>оснащена</w> современными технологиями и персоналом."
                            }
                        },
                        {
                            "text": "The project failed because it was not adequately <w>resourced</w> by the management.",
                            "target_lang_translations": {
                                "ru": "Проект провалился, потому что не был должным образом <w>обеспечен ресурсами</w> со стороны руководства."
                            }
                        }
                    ],
                    "synonyms": [
                        "equip",
                        "fund",
                        "supply",
                        "support"
                    ],
                    "common_phrases": [
                        "well resourced",
                        "poorly resourced",
                        "fully resourced",
                        "adequately resourced"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предоставлять человеку или организации материалы, деньги, персонал или другие активы, необходимые для эффективной работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обеспе́чивать ресурсами",
                                "target_lang_sense_clarification": "снабжать всем необходимым"
                            },
                            {
                                "target_lang_word": "снабжа́ть",
                                "target_lang_sense_clarification": "оборудовать, предоставлять средства"
                            },
                            {
                                "target_lang_word": "финанси́ровать",
                                "target_lang_sense_clarification": "выделять денежные средства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7eb763a2-4887-3921-8461-792ab2f3d7f0",
                    "sense_definition": "To find or provide a new source for something; to source anew.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company is <w>resourcing</w> its materials from a local supplier to save money.",
                            "target_lang_translations": {
                                "ru": "Компания <w>закупает</w> материалы у местного поставщика, чтобы сэкономить деньги."
                            }
                        },
                        {
                            "text": "They decided to <w>resource</w> the components after the original factory closed.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>найти новый источник</w> компонентов после закрытия оригинальной фабрики."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Находить или предоставлять новый источник чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закупа́ть",
                                "target_lang_sense_clarification": "приобретать у поставщика"
                            },
                            {
                                "target_lang_word": "находи́ть исто́чник",
                                "target_lang_sense_clarification": "поставки или снабжения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
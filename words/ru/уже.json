{
    "word_family": [
        "узкий",
        "узость",
        "сужать",
        "уж"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a72bd248-dfa5-3035-88c9-a752270a5ac1",
                    "sense_definition": "Указывает на то, что действие совершилось или состояние наступило раньше, чем ожидалось, или к данному моменту.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "completion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы <w>уже</w> приехали домой.",
                            "target_lang_translations": {
                                "en": "We have <w>already</w> arrived home."
                            }
                        },
                        {
                            "text": "Я <w>уже</w> сделал это задание.",
                            "target_lang_translations": {
                                "en": "I have <w>already</w> done this task."
                            }
                        },
                        {
                            "text": "Поезд <w>уже</w> ушёл.",
                            "target_lang_translations": {
                                "en": "The train has <w>already</w> left."
                            }
                        }
                    ],
                    "synonyms": [
                        "уж"
                    ],
                    "antonyms": [
                        "ещё не",
                        "пока не"
                    ],
                    "common_phrases": [
                        "уже давно",
                        "уже всё",
                        "уже пора",
                        "уже готов"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates that an action has been completed or a state has begun earlier than expected, or by this moment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "already",
                                "target_lang_sense_clarification": "by this time; previously"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7616447e-860c-334e-9612-d22b17189974",
                    "sense_definition": "Указывает на прекращение действия или состояния (обычно в сочетании с отрицанием).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "change_of_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <w>уже</w> не хочу спать.",
                            "target_lang_translations": {
                                "en": "I <w>no longer</w> want to sleep."
                            }
                        },
                        {
                            "text": "Они здесь <w>уже</w> не живут.",
                            "target_lang_translations": {
                                "en": "They <w>no longer</w> live here."
                            }
                        },
                        {
                            "text": "Это <w>уже</w> не важно.",
                            "target_lang_translations": {
                                "en": "It is <w>no longer</w> important."
                            }
                        }
                    ],
                    "antonyms": [
                        "ещё"
                    ],
                    "common_phrases": [
                        "уже не",
                        "уже нет",
                        "уже никогда"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates the cessation of an action or state (usually in combination with negation)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "no longer",
                                "target_lang_sense_clarification": "not anymore; cessation of a state"
                            },
                            {
                                "target_lang_word": "not ... anymore",
                                "target_lang_sense_clarification": "used with negation to indicate something has stopped"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "530481e4-31dd-3589-ab4b-86b507f8e770",
                    "sense_definition": "Указывает на то, что действие продолжается в течение определенного времени вплоть до настоящего момента.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он наблюдает меня <w>уже</w> несколько лет.",
                            "target_lang_translations": {
                                "en": "He has been observing me <w>for</w> several years <w>already</w>."
                            }
                        },
                        {
                            "text": "Мы ждём тебя <w>уже</w> два часа.",
                            "target_lang_translations": {
                                "en": "We have been waiting for you <w>for</w> two hours <w>already</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "к настоя́щему вре́мени"
                    ],
                    "common_phrases": [
                        "уже год",
                        "уже неделю",
                        "уже час"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates that an action has been continuing for a certain period up to the present moment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "already",
                                "target_lang_sense_clarification": "emphasizing the duration of an ongoing action"
                            },
                            {
                                "target_lang_word": "for ... now",
                                "target_lang_sense_clarification": "indicating duration up to the present"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "fac620d0-d333-3032-90a2-c2f8630db95c",
                    "sense_definition": "Употребляется для подчеркивания значительности количества, меры или времени.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "emphasis_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он не спал <w>уже</w> вторую ночь.",
                            "target_lang_translations": {
                                "en": "He hasn't slept for the second night <w>now</w>."
                            }
                        },
                        {
                            "text": "Гостья выпила <w>уже</w> четвёртую чашку чаю.",
                            "target_lang_translations": {
                                "en": "The guest has <w>already</w> drunk the fourth cup of tea."
                            }
                        }
                    ],
                    "common_phrases": [
                        "уже второй",
                        "уже третий раз"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to emphasize the significance of a quantity, measure, or time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "already",
                                "target_lang_sense_clarification": "emphasizing a significant amount or count"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eefe77a9-ff01-309d-9ea3-1b99d4aba61d",
                    "sense_definition": "Усиливает значение слова или словосочетания, к которому относится.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "emphasis_word",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Уже</w> по звуку двигателя можно определить, что он неисправен.",
                            "target_lang_translations": {
                                "en": "<w>Even</w> by the sound of the engine, one can determine that it is faulty."
                            }
                        },
                        {
                            "text": "<w>Уже</w> завтра мы будем знать результаты.",
                            "target_lang_translations": {
                                "en": "We will know the results <w>as early as</w> tomorrow."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Intensifies the meaning of the word or phrase it refers to."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "even",
                                "target_lang_sense_clarification": "intensifier"
                            },
                            {
                                "target_lang_word": "as early as",
                                "target_lang_sense_clarification": "emphasizing how soon something happens"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5878487d-cc3e-3ce2-a6e6-95fe8232d6ff",
                    "sense_definition": "Сравнительная степень прилагательного <w>узкий</w>.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "comparative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта улица <w>уже</w>, чем та.",
                            "target_lang_translations": {
                                "en": "This street is <w>narrower</w> than that one."
                            }
                        },
                        {
                            "text": "Тропинка становилась всё <w>уже</w> и темнее.",
                            "target_lang_translations": {
                                "en": "The path was becoming <w>narrower</w> and darker."
                            }
                        }
                    ],
                    "antonyms": [
                        "шире"
                    ],
                    "common_phrases": [
                        "становиться уже",
                        "намного уже",
                        "ещё уже"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "comparative degree"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Comparative degree of the adjective 'narrow'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "narrower",
                                "target_lang_sense_clarification": "comparative of narrow"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "07b9af59-8766-3b33-92a0-6837243a0a7e",
                    "sense_definition": "Форма предложного падежа единственного числа существительного <w>уж</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "snake_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом <w>уже</w> нет яда.",
                            "target_lang_translations": {
                                "en": "There is no poison in this <w>grass snake</w>."
                            }
                        },
                        {
                            "text": "Мы говорили об <w>уже</w>, которого видели в лесу.",
                            "target_lang_translations": {
                                "en": "We spoke about the <w>grass snake</w> which we saw in the forest."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "prepositional singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Prepositional singular form of the noun 'grass snake'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grass snake",
                                "target_lang_sense_clarification": "non-venomous snake (form of)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "Węgry",
        "węgierski",
        "węgierka",
        "po węgiersku"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "33ad900a-8f23-3ce3-b01d-fc8e5e3805a5",
                    "sense_definition": "Obywatel Węgier lub osoba narodowości węgierskiej.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "nationality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad jest <Węgrem> i często opowiada o Budapeszcie.",
                            "target_lang_translations": {
                                "ru": "Мой сосед — <венгр>, и он часто рассказывает о Будапеште."
                            }
                        },
                        {
                            "text": "Wielu <Węgrów> przyjeżdża latem nad polskie morze.",
                            "target_lang_translations": {
                                "ru": "Много <венгров> приезжает летом на польское море."
                            }
                        }
                    ],
                    "synonyms": [
                        "Madziar"
                    ],
                    "common_phrases": [
                        "rodowity Węgier",
                        "spotkać Węgra",
                        "znajomy Węgier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гражданин Венгрии или человек венгерской национальности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "венгр",
                                "target_lang_sense_clarification": "национальность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6752c0b5-edcc-3847-99d1-1f6451951ff4",
                    "sense_definition": "Wędrowny handlarz, dawniej sprzedający drobne towary, często pochodzenia węgierskiego.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "profession_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wsiach chodził dawniej <węgier> z nićmi i guzikami.",
                            "target_lang_translations": {
                                "ru": "По деревням раньше ходил <бродячий торговец> с нитками и пуговицами."
                            }
                        },
                        {
                            "text": "Gospodyni kupiła od wędrownego <węgra> kolorową chustę.",
                            "target_lang_translations": {
                                "ru": "Хозяйка купила у бродячего <торговца> цветной платок."
                            }
                        }
                    ],
                    "synonyms": [
                        "domokrążca"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "słowo wyszło z użycia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бродячий торговец, в прошлом продававший мелкие товары, часто венгерского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бродячий торговец",
                                "target_lang_sense_clarification": "устаревшее"
                            },
                            {
                                "target_lang_word": "коробейник",
                                "target_lang_sense_clarification": "устаревшее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5fc7d152-45eb-34a0-b81f-ffa41d6e28a8",
                    "sense_definition": "Dawne określenie wina sprowadzanego z Węgier.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "alcohol_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szlachta chętnie piła starego <węgra> podczas uczt.",
                            "target_lang_translations": {
                                "ru": "Шляхта охотно пила старое <венгерское вино> во время пиров."
                            }
                        },
                        {
                            "text": "W piwnicy znaleziono zakurzoną butelkę wyśmienitego <węgra>.",
                            "target_lang_translations": {
                                "ru": "В подвале нашли запыленную бутылку превосходного <венгерского>."
                            }
                        }
                    ],
                    "synonyms": [
                        "węgrzyn"
                    ],
                    "common_phrases": [
                        "pić węgra",
                        "kielich węgra"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "słowo historyczne lub przestarzałe"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинное название вина, привозимого из Венгрии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "венгерское вино",
                                "target_lang_sense_clarification": "устаревшее"
                            },
                            {
                                "target_lang_word": "венгерское",
                                "target_lang_sense_clarification": "разговорное, субстантивированное прилагательное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "92c81ed6-7e31-34f9-8f40-d9fca57faa42",
                    "sense_definition": "Forma dopełniacza liczby mnogiej od nazwy własnej <w>Węgry</w> (państwo w Europie Środkowej).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_form",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Właśnie wróciłem z <Węgier>, gdzie spędziłem wspaniały urlop.",
                            "target_lang_translations": {
                                "ru": "Я только что вернулся из <Венгрии>, где провел чудесный отпуск."
                            }
                        },
                        {
                            "text": "Budapeszt jest stolicą i największym miastem <Węgier>.",
                            "target_lang_translations": {
                                "ru": "Будапешт — столица и крупнейший город <Венгрии>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stolica Węgier",
                        "mapa Węgier",
                        "wycieczka do Węgier",
                        "historia Węgier"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "dopełniacz liczby mnogiej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа от названия страны Венгрия (государство в Центральной Европе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Венгрии",
                                "target_lang_sense_clarification": "форма названия страны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce34c3bf-eba0-3b2a-9df8-4bc5bca3ca4a",
                    "sense_definition": "Polskie nazwisko używane zarówno przez mężczyzn, jak i kobiety.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "anthroponym",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <Węgier> pracuje w naszym dziale księgowości od lat.",
                            "target_lang_translations": {
                                "ru": "Господин <Венгер> работает в нашем отделе бухгалтерии уже много лет."
                            }
                        },
                        {
                            "text": "Nie ma dzisiaj w szkole Anny <Węgier> z powodu choroby.",
                            "target_lang_translations": {
                                "ru": "Сегодня в школе нет Анны <Венгер> из-за болезни."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская фамилия, используемая как мужчинами, так и женщинами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Венгер",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d6e0e01e-4a19-3965-b557-659f75c55c1f",
                    "sense_definition": "Forma gramatyczna (liczba mnoga lub dopełniacz liczby pojedynczej) rzeczownika <w>lampka</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na stolikach nocnych po obu stronach łóżka stały małe <w>lampki</w>.",
                            "target_lang_translations": {
                                "ru": "На прикроватных тумбочках по обе стороны кровати стояли маленькие <w>лампы</w>."
                            }
                        },
                        {
                            "text": "Do obiadu wypiliśmy po dwie <w>lampki</w> wytrawnego wina.",
                            "target_lang_translations": {
                                "ru": "За обедом мы выпили по два <w>бокала</w> сухого вина."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "noun form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грамматическая форма (множественное число или родительный падеж единственного числа) существительного <w>lampka</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лампы",
                                "target_lang_sense_clarification": "маленькие светильники"
                            },
                            {
                                "target_lang_word": "бокалы",
                                "target_lang_sense_clarification": "для вина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6b46dd2-aae3-33e7-9d8d-4706ccc8db62",
                    "sense_definition": "Ozdobny łańcuch małych punktów świetlnych połączonych przewodem, używany najczęściej do dekoracji świątecznych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kaziu, zrób coś z tymi <w>lampkami</w> na choince, bo znowu przestały świecić.",
                            "target_lang_translations": {
                                "ru": "Казик, сделай что-нибудь с этими <w>огоньками</w> на елке, а то они снова перестали гореть."
                            }
                        },
                        {
                            "text": "W tym roku kupiliśmy energooszczędne <w>lampki</w> LED, aby ozdobić balkon na święta.",
                            "target_lang_translations": {
                                "ru": "В этом году мы купили энергосберегающие светодиодные <w>гирлянды</w>, чтобы украсить балкон к праздникам."
                            }
                        }
                    ],
                    "synonyms": [
                        "światełka"
                    ],
                    "common_phrases": [
                        "lampki choinkowe",
                        "kolorowe lampki",
                        "sznur lampek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративная цепь из маленьких лампочек, соединенных проводом, обычно используемая для праздничного украшения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гирлянда",
                                "target_lang_sense_clarification": "электрическая, елочная"
                            },
                            {
                                "target_lang_word": "огоньки",
                                "target_lang_sense_clarification": "светящиеся элементы декора"
                            }
                        ]
                    }
                }
            ]
        }
    ]

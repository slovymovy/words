{
    "word_family": [
        "reach",
        "reachable",
        "unreachable",
        "reacher"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "96fdcb5b-1ad1-3914-a918-57479c2e8d99",
                    "sense_definition": "The practice of providing help, advice, or services to people who would not otherwise have access to them, often by a charity or organization.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "service_provision",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hospital has a community <w>outreach</w> team that visits elderly patients at home.",
                            "target_lang_translations": {
                                "ru": "В больнице есть бригада <w>выездной помощи</w>, которая посещает пожилых пациентов на дому."
                            }
                        },
                        {
                            "text": "He is involved in social <w>outreach</w> to support families in need.",
                            "target_lang_translations": {
                                "ru": "Он занимается <w>социальной работой</w> для поддержки нуждающихся семей."
                            }
                        }
                    ],
                    "synonyms": [
                        "assistance",
                        "aid",
                        "support"
                    ],
                    "common_phrases": [
                        "community outreach",
                        "outreach program",
                        "educational outreach",
                        "outreach worker"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деятельность по предоставлению помощи, консультаций или услуг людям, которые иначе не имели бы к ним доступа, часто осуществляемая благотворительными организациями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "социальная работа",
                                "target_lang_sense_clarification": "деятельность по оказанию помощи населению"
                            },
                            {
                                "target_lang_word": "выездная работа",
                                "target_lang_sense_clarification": "работа специалистов вне офиса, на местах"
                            },
                            {
                                "target_lang_word": "просветительская деятельность",
                                "target_lang_sense_clarification": "распространение знаний и информации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24e23b89-127f-3b32-84dc-0f4258308693",
                    "sense_definition": "The activity of contacting people or groups to raise awareness, share information, or build relationships.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Our marketing <w>outreach</w> was successful in attracting new clients.",
                            "target_lang_translations": {
                                "ru": "Наша маркетинговая <w>кампания по привлечению</w> успешно привлекла новых клиентов."
                            }
                        },
                        {
                            "text": "The senator's <w>outreach</w> to voters included town hall meetings and social media campaigns.",
                            "target_lang_translations": {
                                "ru": "<w>Взаимодействие</w> сенатора с избирателями включало встречи с общественностью и кампании в социальных сетях."
                            }
                        }
                    ],
                    "synonyms": [
                        "contact",
                        "communication",
                        "engagement"
                    ],
                    "common_phrases": [
                        "public outreach",
                        "customer outreach",
                        "outreach effort"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деятельность по установлению контактов с людьми или группами для повышения осведомленности, обмена информацией или построения отношений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "информационная работа",
                                "target_lang_sense_clarification": "деятельность по информированию аудитории"
                            },
                            {
                                "target_lang_word": "взаимодействие",
                                "target_lang_sense_clarification": "процесс общения и налаживания связей"
                            },
                            {
                                "target_lang_word": "привлечение",
                                "target_lang_sense_clarification": "активные действия по заинтересовыванию аудитории"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56c718aa-b8ab-3ac0-9126-c160be7debea",
                    "sense_definition": "The physical distance or extent that something or someone can reach.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_extent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>outreach</w> of the crane was sufficient to lift the heavy load from the truck.",
                            "target_lang_translations": {
                                "ru": "<w>Вылет стрелы</w> крана был достаточным, чтобы поднять тяжелый груз с грузовика."
                            }
                        },
                        {
                            "text": "We were surprised by the vast <w>outreach</w> of the river during the flood.",
                            "target_lang_translations": {
                                "ru": "Мы были удивлены огромным <w>разливом</w> реки во время наводнения."
                            }
                        }
                    ],
                    "synonyms": [
                        "reach",
                        "span",
                        "range",
                        "extent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическое расстояние или предел, до которого что-то или кто-то может дотянуться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вылет",
                                "target_lang_sense_clarification": "расстояние, на которое выдвигается часть механизма (например, крана)"
                            },
                            {
                                "target_lang_word": "досягаемость",
                                "target_lang_sense_clarification": "предел доступности"
                            },
                            {
                                "target_lang_word": "размах",
                                "target_lang_sense_clarification": "широта охвата или физическая протяженность"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c99d18f4-91fd-3a64-a585-46f2ab17b773",
                    "sense_definition": "To extend beyond or reach further than a specific point or limit.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "exceeding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The demand for emergency supplies far <w>outreached</w> the available stock.",
                            "target_lang_translations": {
                                "ru": "Спрос на предметы первой необходимости намного <w>превысил</w> имеющиеся запасы."
                            }
                        },
                        {
                            "text": "His ambition often <w>outreached</w> his actual ability to deliver results.",
                            "target_lang_translations": {
                                "ru": "Его амбиции часто <w>превосходили</w> его реальную способность добиваться результатов."
                            }
                        }
                    ],
                    "synonyms": [
                        "exceed",
                        "surpass",
                        "outstrip"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выходить за пределы или простираться дальше определенной точки или лимита."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "превосходить",
                                "target_lang_sense_clarification": "быть лучше или больше чего-либо"
                            },
                            {
                                "target_lang_word": "превышать",
                                "target_lang_sense_clarification": "быть больше по количеству или степени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d235df8-3a62-3e81-b582-15d63d146b57",
                    "sense_definition": "To actively provide services or support to a specific group of people, especially those in need.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "service_provision_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The organization <w>outreaches</w> to vulnerable communities across the state.",
                            "target_lang_translations": {
                                "ru": "Организация <w>оказывает помощь</w> уязвимым слоям населения по всему штату."
                            }
                        },
                        {
                            "text": "They aim to <w>outreach</w> to students who might otherwise drop out of school.",
                            "target_lang_translations": {
                                "ru": "Они стремятся <w>наладить контакт</w> со студентами, которые иначе могли бы бросить учебу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "outreach to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Активно предоставлять услуги или поддержку определенной группе людей, особенно нуждающимся."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оказывать помощь",
                                "target_lang_sense_clarification": "предоставлять поддержку нуждающимся"
                            },
                            {
                                "target_lang_word": "работать с населением",
                                "target_lang_sense_clarification": "взаимодействовать с людьми для решения их проблем"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
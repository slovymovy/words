                    "common_phrases": [
                        "mieć raka",
                        "zachorować na raka",
                        "walka z rakiem",
                        "rak płuc",
                        "rak piersi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тяжелое заболевание, злокачественная опухоль, разрушающая ткани организма."

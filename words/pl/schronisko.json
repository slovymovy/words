    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7ce12f62-2b1a-3467-a369-6e1e7dbb08d9",
                    "sense_definition": "Budynek turystyczny, często położony w górach, oferujący turystom proste zakwaterowanie i posiłki.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "tourist_accommodation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zmęczeni turyści dotarli do <schroniska> tuż przed zachodem słońca.",
                            "target_lang_translations": {
                                "ru": "Уставшие туристы добрались до <w>приюта</w> прямо перед закатом."
                            }
                        },
                        {
                            "text": "To popularne <schronisko> górskie słynie z pysznej szarlotki.",
                            "target_lang_translations": {
                                "ru": "Этот популярный горный <w>приют</w> славится вкусной шарлоткой."
                            }
                        }
                    ],
                    "synonyms": [
                        "hostel"
                    ],
                    "common_phrases": [
                        "schronisko górskie",
                        "schronisko młodzieżowe",
                        "nocleg w schronisku",
                        "szlak do schroniska"
                    ],
                    "target_lang_definitions": {
                        "ru": "Туристическое здание, часто расположенное в горах, предлагающее туристам простой ночлег и питание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "турбаза",
                                "target_lang_sense_clarification": "туристическая база"
                            },
                            {
                                "target_lang_word": "приют",
                                "target_lang_sense_clarification": "горный, для туристов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "618368a1-c666-3127-a26f-f307f5c0b524",
                    "sense_definition": "Placówka zapewniająca opiekę i tymczasowy pobyt bezdomnym lub porzuconym zwierzętom.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animal_care",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi rodzice zaadoptowali starszego psa z lokalnego <schroniska>.",
                            "target_lang_translations": {
                                "ru": "Мои родители взяли пожилую собаку из местного <w>приюта</w>."
                            }
                        },
                        {
                            "text": "Każde <schronisko> dla zwierząt potrzebuje karmy i ciepłych koców na zimę.",
                            "target_lang_translations": {
                                "ru": "Каждому <w>приюту</w> для животных требуются корм и теплые одеяла на зиму."
                            }
                        }
                    ],
                    "synonyms": [
                        "azyl"
                    ],
                    "common_phrases": [
                        "schronisko dla zwierząt",
                        "pies ze schroniska",
                        "wolontariat w schronisku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учреждение, обеспечивающее уход и временное проживание бездомным или брошенным животным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приют",
                                "target_lang_sense_clarification": "для животных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a52651ae-af93-31b8-89ce-4ec80ac38de7",
                    "sense_definition": "Instytucja udzielająca pomocy, noclegu i wyżywienia osobom bezdomnym lub znajdującym się w trudnej sytuacji życiowej.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "social_help",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas mrozów miejskie <schronisko> dla bezdomnych jest zawsze przepełnione.",
                            "target_lang_translations": {
                                "ru": "Во время морозов городской <w>приют</w> для бездомных всегда переполнен."
                            }
                        },
                        {
                            "text": "Mężczyzna otrzymał niezbędną pomoc i ciepły posiłek w <schronisku>.",
                            "target_lang_translations": {
                                "ru": "Мужчина получил необходимую помощь и горячую еду в <w>приюте</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "przytułek",
                        "azyl"
                    ],
                    "common_phrases": [
                        "schronisko dla bezdomnych",
                        "miejskie schronisko",
                        "prowadzić schronisko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учреждение, предоставляющее помощь, ночлег и питание бездомным или людям в трудной жизненной ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приют",
                                "target_lang_sense_clarification": "для бездомных"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
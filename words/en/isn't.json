                    "sense_definition": "Contraction of <w>is</w> <w>not</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It <w>isn't</w> raining right now, so we can go for a walk.",
                            "target_lang_translations": {
                                "ru": "Сейчас <w>не</w> идет дождь, так что мы можем пойти на прогулку."
                            }
                        },
                        {
                            "text": "Nice day, <w>isn't</w> it?",
                            "target_lang_translations": {
                                "ru": "Хороший день, <w>не так ли</w>?"
                            }
                        },
                        {
                            "text": "She <w>isn't</w> ready to leave the house yet.",
                            "target_lang_translations": {
                                "ru": "Она еще <w>не</w> готова выйти из дома."
                            }
                        }
                    ],
                    "common_phrases": [
                        "isn't it",
                        "isn't there",
                        "isn't he",
                        "isn't she",
                        "isn't that"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "contraction"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от \"is not\" (не является, нет)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не",
                                "target_lang_sense_clarification": "отрицательная частица перед глаголом или прилагательным"
                            },
                            {
                                "target_lang_word": "нет",
                                "target_lang_sense_clarification": "отсутствие чего-либо"
                            },
                            {
                                "target_lang_word": "не так ли",
                                "target_lang_sense_clarification": "в конце вопросительных предложений"
                            }
                        ]
                    }
                }
            ]
        },
        {

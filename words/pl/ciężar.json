    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "00cf3639-9df8-3729-9f67-789ca5a5445d",
                    "sense_definition": "Siła, z jaką ciało fizyczne naciska na podłoże pod wpływem grawitacji; potocznie masa lub waga przedmiotu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_weight",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Winda nie ruszyła, ponieważ pasażerowie przekroczyli dopuszczalny <w>ciężar</w>.",
                            "target_lang_translations": {
                                "ru": "Лифт не поехал, так как пассажиры превысили допустимый <w>вес</w>."
                            }
                        },
                        {
                            "text": "Nauczyciel fizyki wyjaśnił uczniom różnicę między masą a <w>ciężarem</w>.",
                            "target_lang_translations": {
                                "ru": "Учитель физики объяснил ученикам разницу между массой и <w>весом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "waga",
                        "ciężkość"
                    ],
                    "common_phrases": [
                        "ciężar właściwy",
                        "ciężar ciała",
                        "nadmierny ciężar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сила, с которой тело давит на опору (физика); масса или вес предмета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вес",
                                "target_lang_sense_clarification": "физическая величина; масса тела"
                            },
                            {
                                "target_lang_word": "тяжесть",
                                "target_lang_sense_clarification": "свойство тяжёлого предмета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96088d0c-d00a-33a6-a4cc-8d4def2a033e",
                    "sense_definition": "Dyscyplina sportowa polegająca na podnoszeniu sztangi; także sprzęt używany do ćwiczeń siłowych.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad codziennie chodzi na siłownię, żeby podnosić <w>ciężary</w>.",
                            "target_lang_translations": {
                                "ru": "Мой сосед каждый день ходит в спортзал, чтобы поднимать <w>тяжести</w>."
                            }
                        },
                        {
                            "text": "Trener kazał zawodnikowi dołożyć <w>ciężar</w> na sztangę przed kolejną serią.",
                            "target_lang_translations": {
                                "ru": "Тренер велел спортсмену добавить <w>вес</w> на штангу перед следующим подходом."
                            }
                        }
                    ],
                    "synonyms": [
                        "podnoszenie ciężarów"
                    ],
                    "common_phrases": [
                        "podnosić ciężary",
                        "trenować ciężary",
                        "sekcja ciężarów"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "często w liczbie mnogiej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид спорта (тяжёлая атлетика) или спортивные снаряды для силовых упражнений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тяжести",
                                "target_lang_sense_clarification": "спортивные снаряды; обычно мн.ч."
                            },
                            {
                                "target_lang_word": "вес",
                                "target_lang_sense_clarification": "нагрузка на штанге"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ad65e38-1a85-3d0d-8eac-b60e92e91a5d",
                    "sense_definition": "Przytłaczające zmartwienie, problem lub trudne przeżycie psychiczne, które sprawia ból.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "metaphorical_burden",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy w końcu wyznał prawdę, poczuł, jak wielki <w>ciężar</w> spada mu z serca.",
                            "target_lang_translations": {
                                "ru": "Когда он наконец сказал правду, то почувствовал, как тяжелый <w>груз</w> упал с его души."
                            }
                        },
                        {
                            "text": "Przez wiele lat nosiła w sobie <w>ciężar</w> bolesnych wspomnień z dzieciństwa.",
                            "target_lang_translations": {
                                "ru": "Много лет она носила в себе <w>груз</w> болезненных воспоминаний из детства."
                            }
                        }
                    ],
                    "synonyms": [
                        "brzemię",
                        "troska",
                        "udręka",
                        "trud"
                    ],
                    "common_phrases": [
                        "ciężar na sercu",
                        "zrzucić ciężar",
                        "psychiczny ciężar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гнетущее переживание, душевная боль или проблема."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "груз",
                                "target_lang_sense_clarification": "перен. гнетущие мысли или чувства"
                            },
                            {
                                "target_lang_word": "тяжесть",
                                "target_lang_sense_clarification": "гнетущее ощущение"
                            },
                            {
                                "target_lang_word": "камень",
                                "target_lang_sense_clarification": "в идиомах (камень с души)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a1658d8-2ce8-3f94-a97d-9bb2638ef1d3",
                    "sense_definition": "Trudny obowiązek, odpowiedzialność lub zobowiązanie (finansowe, prawne lub moralne), które trzeba wypełnić.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_burden",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sprawach cywilnych <w>ciężar</w> dowodu leży zazwyczaj po stronie powoda.",
                            "target_lang_translations": {
                                "ru": "В гражданских делах <w>бремя</w> доказывания обычно лежит на истце."
                            }
                        },
                        {
                            "text": "Po śmierci ojca cały <w>ciężar</w> utrzymania rodziny spoczął na jej barkach.",
                            "target_lang_translations": {
                                "ru": "После смерти отца всё <w>бремя</w> содержания семьи легло на её плечи."
                            }
                        }
                    ],
                    "synonyms": [
                        "obowiązek",
                        "powinność",
                        "brzemię"
                    ],
                    "common_phrases": [
                        "ciężar dowodu",
                        "ciężar odpowiedzialności",
                        "ciężar gatunkowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обременительная обязанность или ответственность (финансовая, юридическая)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бремя",
                                "target_lang_sense_clarification": "ответственность, долг"
                            },
                            {
                                "target_lang_word": "груз",
                                "target_lang_sense_clarification": "ответственность"
                            }
                        ]
                    }
                }
            ]
        }

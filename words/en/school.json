    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a5396b7c-e23d-3838-aba7-0fa24f6b08d7",
                    "sense_definition": "An institution or place for educating children or giving instruction.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "education_inst",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Our children attend a public <w>school</w> in our neighborhood.",
                            "target_lang_translations": {
                                "ru": "Наши дети ходят в государственную <w>школу</w> в нашем районе."
                            }
                        },
                        {
                            "text": "She walks to <w>school</w> every morning with her brother.",
                            "target_lang_translations": {
                                "ru": "Она ходит в <w>школу</w> каждое утро со своим братом."
                            }
                        }
                    ],
                    "synonyms": [
                        "academy"
                    ],
                    "common_phrases": [
                        "go to school",
                        "high school",
                        "primary school",
                        "public school",
                        "private school"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учебное заведение или место для обучения детей и получения образования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шко́ла",
                                "target_lang_sense_clarification": "общеобразовательное учреждение"
                            },
                            {
                                "target_lang_word": "учи́лище",
                                "target_lang_sense_clarification": "специальное учебное заведение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72eaa46c-deb7-3c19-b75e-2ae7673a9367",
                    "sense_definition": "The time during the day when students attend classes.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "education_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will see you at the park <w>after school</w>.",
                            "target_lang_translations": {
                                "ru": "Увидимся в парке <w>после школы</w>."
                            }
                        },
                        {
                            "text": "You cannot watch television until <w>school</w> is finished for the day.",
                            "target_lang_translations": {
                                "ru": "Ты не можешь смотреть телевизор, пока не закончатся <w>уроки</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "after school",
                        "before school",
                        "during school"
                    ],
                    "target_lang_definitions": {
                        "ru": "Время в течение дня, когда ученики посещают занятия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заня́тия",
                                "target_lang_sense_clarification": "учебный процесс"
                            },
                            {
                                "target_lang_word": "уро́ки",
                                "target_lang_sense_clarification": "учебные часы"
                            },
                            {
                                "target_lang_word": "шко́ла",
                                "target_lang_sense_clarification": "время пребывания в школе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54f8d305-1806-33d1-80fd-79d85b6e8039",
                    "sense_definition": "An establishment offering specialized instruction in a specific skill.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "education_inst",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is attending a driving <w>school</w> to get his license.",
                            "target_lang_translations": {
                                "ru": "Он посещает <w>школу</w> вождения, чтобы получить права."
                            }
                        },
                        {
                            "text": "My sister goes to a cooking <w>school</w> on weekends.",
                            "target_lang_translations": {
                                "ru": "Моя сестра ходит в кулинарную <w>школу</w> по выходным."
                            }
                        }
                    ],
                    "synonyms": [
                        "academy"
                    ],
                    "common_phrases": [
                        "driving school",
                        "cooking school",
                        "language school",
                        "art school"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учреждение, предлагающее специальное обучение определенному навыку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шко́ла",
                                "target_lang_sense_clarification": "курсы, спец. обучение"
                            },
                            {
                                "target_lang_word": "ку́рсы",
                                "target_lang_sense_clarification": "обучающая программа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63d2a3bd-696b-35eb-8655-b8ebdfd269b9",
                    "sense_definition": "A department or faculty within a university dedicated to a specific subject area.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "education_dept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was accepted into the <w>School</w> of Medicine at a top university.",
                            "target_lang_translations": {
                                "ru": "Ее приняли на медицинский <w>факультет</w> престижного университета."
                            }
                        },
                        {
                            "text": "The <w>School</w> of Engineering has a new laboratory for students.",
                            "target_lang_translations": {
                                "ru": "Инженерный <w>факультет</w> открыл новую лабораторию для студентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "department",
                        "faculty",
                        "institute"
                    ],
                    "common_phrases": [
                        "law school",
                        "medical school",
                        "business school"
                    ],
                    "target_lang_definitions": {
                        "ru": "Факультет или отделение университета, специализирующееся на определенной области знаний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "факульте́т",
                                "target_lang_sense_clarification": "подразделение университета"
                            },
                            {
                                "target_lang_word": "институ́т",
                                "target_lang_sense_clarification": "в составе университета"
                            },
                            {
                                "target_lang_word": "шко́ла",
                                "target_lang_sense_clarification": "в названиях (напр. Высшая школа)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "409d4835-6e33-3c52-a90a-127668ffe426",
                    "sense_definition": "A large group of fish or marine animals swimming together.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "fish_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The divers encountered a huge <w>school</w> of mackerel near the reef.",
                            "target_lang_translations": {
                                "ru": "Дайверы встретили огромный <w>косяк</w> скумбрии возле рифа."
                            }
                        },
                        {
                            "text": "We saw a <w>school</w> of dolphins jumping in the waves.",
                            "target_lang_translations": {
                                "ru": "Мы видели <w>стаю</w> дельфинов, прыгающих в волнах."
                            }
                        }
                    ],
                    "synonyms": [
                        "shoal"
                    ],
                    "common_phrases": [
                        "school of fish"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая группа рыб или морских животных, плавающих вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кося́к",
                                "target_lang_sense_clarification": "стая рыб"
                            },
                            {
                                "target_lang_word": "ста́я",
                                "target_lang_sense_clarification": "группа животных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e3b5284-a3c8-3d03-880d-14de7e8308ac",
                    "sense_definition": "A group of people, especially artists or philosophers, who share similar ideas, methods, or style.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "These economists belong to the monetarist <w>school</w>.",
                            "target_lang_translations": {
                                "ru": "Эти экономисты принадлежат к монетаристской <w>школе</w>."
                            }
                        },
                        {
                            "text": "The painting belongs to the Dutch <w>school</w> of the seventeenth century.",
                            "target_lang_translations": {
                                "ru": "Картина принадлежит к голландской <w>школе</w> семнадцатого века."
                            }
                        }
                    ],
                    "common_phrases": [
                        "school of thought",
                        "old school"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей (особенно художников или философов), объединенных общими идеями, методами или стилем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шко́ла",
                                "target_lang_sense_clarification": "направление в науке/искусстве"
                            },
                            {
                                "target_lang_word": "направле́ние",
                                "target_lang_sense_clarification": "течение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3dcfb582-6006-3edc-9d2a-27e6c249fa08",
                    "sense_definition": "To educate, teach, or train someone.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "education_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many future leaders were <w>schooled</w> at this prestigious academy.",
                            "target_lang_translations": {
                                "ru": "Многие будущие лидеры <w>прошли обучение</w> в этой престижной академии."
                            }
                        },
                        {
                            "text": "He was well <w>schooled</w> in the classics and philosophy.",
                            "target_lang_translations": {
                                "ru": "Он был хорошо <w>обучен</w> классике и философии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "schooled in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обучать, учить или тренировать кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обуча́ть",
                                "target_lang_sense_clarification": "давать образование"
                            },
                            {
                                "target_lang_word": "учи́ть",
                                "target_lang_sense_clarification": "передавать знания"
                            },
                            {
                                "target_lang_word": "воспи́тывать",
                                "target_lang_sense_clarification": "прививать навыки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "647eac7b-545b-35af-bba4-2ed8241aecac",
                    "sense_definition": "To defeat an opponent completely or teach them a harsh lesson.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "defeat_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The experienced player <w>schooled</w> the rookie on the basketball court.",
                            "target_lang_translations": {
                                "ru": "Опытный игрок <w>преподал урок</w> новичку на баскетбольной площадке."
                            }
                        },
                        {
                            "text": "I thought I was good at the game until I got <w>schooled</w> by my younger brother.",
                            "target_lang_translations": {
                                "ru": "Я думал, что хорошо играю, пока мой младший брат меня не <w>уделал</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get schooled"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "Used primarily in sports or competitive contexts."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью победить противника или преподать ему суровый урок (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проучи́ть",
                                "target_lang_sense_clarification": "преподать урок"
                            },
                            {
                                "target_lang_word": "уде́лать",
                                "target_lang_sense_clarification": "сленг: победить"
                            },
                            {
                                "target_lang_word": "разгроми́ть",
                                "target_lang_sense_clarification": "нанести поражение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa76b21f-5558-3c6b-a9a0-33c46d8fe1c9",
                    "sense_definition": "To control or compose one's expression or behavior.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "control_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She took care to <w>school</w> her expression to hide her anger.",
                            "target_lang_translations": {
                                "ru": "Она постаралась <w>сдержать</w> выражение лица, чтобы скрыть гнев."
                            }
                        },
                        {
                            "text": "He <w>schooled</w> himself to remain calm during the emergency.",
                            "target_lang_translations": {
                                "ru": "Он <w>приучил</w> себя сохранять спокойствие в чрезвычайной ситуации."
                            }
                        }
                    ],
                    "common_phrases": [
                        "school one's features"
                    ],
                    "target_lang_definitions": {
                        "ru": "Контролировать выражение лица или поведение; сдерживать себя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приуча́ть",
                                "target_lang_sense_clarification": "дисциплинировать себя"
                            },
                            {
                                "target_lang_word": "сде́рживать",
                                "target_lang_sense_clarification": "контролировать эмоции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c97ae960-ec7d-3c92-9b74-6f01989d8b7a",
                    "sense_definition": "To form into or travel in a group, referring to fish.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fish_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Small fish often <w>school</w> tightly together for protection.",
                            "target_lang_translations": {
                                "ru": "Мелкие рыбы часто <w>сбиваются в</w> плотные <w>косяки</w> для защиты."
                            }
                        },
                        {
                            "text": "We watched the tuna <w>school</w> near the surface of the water.",
                            "target_lang_translations": {
                                "ru": "Мы наблюдали, как тунец <w>сбивается в косяки</w> у поверхности воды."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сбиваться в стаи или косяки (о рыбе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбива́ться в косяки́",
                                "target_lang_sense_clarification": "о рыбе"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
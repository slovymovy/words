{
    "word_family": [
        "dochodzić",
        "dojście",
        "dochodzenie",
        "dochodowy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0f275edb-3a1d-3b4b-b015-05b41bdf17fc",
                    "sense_definition": "Dotrzeć do określonego miejsca, poruszając się pieszo.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy szybko <dojść> na przystanek autobusowy.",
                            "target_lang_translations": {
                                "ru": "Мы должны быстро <дойти> до автобусной остановки."
                            }
                        },
                        {
                            "text": "Czy zdołasz <dojść> do domu o własnych siłach?",
                            "target_lang_translations": {
                                "ru": "Ты сможешь <дойти> до дома своими силами?"
                            }
                        }
                    ],
                    "synonyms": [
                        "dotrzeć",
                        "przyjść",
                        "przybyć"
                    ],
                    "antonyms": [
                        "odejść",
                        "oddalić się",
                        "wyjść"
                    ],
                    "common_phrases": [
                        "dojść do domu",
                        "dojść do celu",
                        "dojść na miejsce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добраться до определенного места пешком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дойти",
                                "target_lang_sense_clarification": "пешком до места"
                            },
                            {
                                "target_lang_word": "добраться",
                                "target_lang_sense_clarification": "прибыть куда-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30f78ed3-4817-33d7-afa1-2dbacead841b",
                    "sense_definition": "Osiągnąć pewien stan, rezultat lub poziom (często po wysiłku lub upływie czasu).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "state_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój ojciec ciężką pracą <doszedł> do wszystkiego, co ma.",
                            "target_lang_translations": {
                                "ru": "Мой отец тяжелым трудом <добился> всего, что имеет."
                            }
                        },
                        {
                            "text": "Po wypadku długo nie mógł <dojść> do siebie.",
                            "target_lang_translations": {
                                "ru": "После аварии он долго не мог <прийти> в себя."
                            }
                        }
                    ],
                    "synonyms": [
                        "osiągnąć",
                        "uzyskać"
                    ],
                    "common_phrases": [
                        "dojść do siebie",
                        "dojść do władzy",
                        "dojść do wprawy",
                        "dojść do głosu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достичь определенного состояния, результата или уровня (часто после усилий или по прошествии времени)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добиться",
                                "target_lang_sense_clarification": "достичь результата"
                            },
                            {
                                "target_lang_word": "прийти",
                                "target_lang_sense_clarification": "в определенное состояние (напр. в себя)"
                            },
                            {
                                "target_lang_word": "дойти",
                                "target_lang_sense_clarification": "достичь состояния"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c302a77-25bf-30d4-8a57-048af4912881",
                    "sense_definition": "Zrozumieć coś, ustalić fakty lub wyrobić sobie zdanie na jakiś temat w wyniku myślenia.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "intellectual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wreszcie <doszedłem>, jak działa ta skomplikowana maszyna.",
                            "target_lang_translations": {
                                "ru": "Наконец я <понял>, как работает эта сложная машина."
                            }
                        },
                        {
                            "text": "Po długiej dyskusji <doszliśmy> do porozumienia.",
                            "target_lang_translations": {
                                "ru": "После долгой дискуссии мы <пришли> к соглашению."
                            }
                        }
                    ],
                    "synonyms": [
                        "zrozumieć",
                        "pojąć",
                        "ustalić"
                    ],
                    "common_phrases": [
                        "dojść do wniosku",
                        "dojść do porozumienia",
                        "dojść do prawdy",
                        "dojść do przekonania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Понять что-либо, установить факты или сформировать мнение в результате размышлений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "додуматься",
                                "target_lang_sense_clarification": "понять размышляя"
                            },
                            {
                                "target_lang_word": "прийти",
                                "target_lang_sense_clarification": "к выводу или соглашению"
                            },
                            {
                                "target_lang_word": "понять",
                                "target_lang_sense_clarification": "осознать смысл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "465644f0-3924-3ac8-9019-82619692fee6",
                    "sense_definition": "Dotrzeć do odbiorcy lub stać się wyczuwalnym (o wiadomościach, dźwiękach, zapachach).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sensory_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta smutna wiadomość jeszcze do nas nie <doszła>.",
                            "target_lang_translations": {
                                "ru": "Эта печальная новость до нас еще не <дошла>."
                            }
                        },
                        {
                            "text": "Z kuchni <doszedł> mnie zapach pieczonego ciasta.",
                            "target_lang_translations": {
                                "ru": "Из кухни до меня <донесся> запах печеного пирога."
                            }
                        }
                    ],
                    "synonyms": [
                        "dotrzeć",
                        "dobiec"
                    ],
                    "common_phrases": [
                        "słuchy doszły",
                        "wiadomość doszła",
                        "list doszedł"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достичь адресата или стать ощутимым (о новостях, звуках, запахах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дойти",
                                "target_lang_sense_clarification": "о новостях или письмах"
                            },
                            {
                                "target_lang_word": "донестись",
                                "target_lang_sense_clarification": "о звуках или запахах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cad0f2a3-5c63-3259-9134-c87a3541e894",
                    "sense_definition": "O potrawach lub owocach: stać się gotowym do spożycia (ugotować się, upiec lub dojrzeć).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "culinary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mięso musi jeszcze chwilę <dojść> w piekarniku.",
                            "target_lang_translations": {
                                "ru": "Мясо должно еще немного <дойти> в духовке."
                            }
                        },
                        {
                            "text": "Te zielone pomidory muszą <dojść> na słońcu.",
                            "target_lang_translations": {
                                "ru": "Эти зеленые помидоры должны <дозреть> на солнце."
                            }
                        }
                    ],
                    "synonyms": [
                        "dojrzeć"
                    ],
                    "common_phrases": [
                        "ciasto musi dojść",
                        "owoce doszły"
                    ],
                    "target_lang_definitions": {
                        "ru": "О блюдах или фруктах: стать готовым к употреблению (свариться, испечься или созреть)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дойти",
                                "target_lang_sense_clarification": "о готовности блюда"
                            },
                            {
                                "target_lang_word": "дозреть",
                                "target_lang_sense_clarification": "о фруктах или овощах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d51ab67-967c-3741-a4bd-186da829bb7a",
                    "sense_definition": "Uzyskać coś należnego na drodze prawnej lub poprzez starania.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Będziemy <dochodzić> swoich praw przed sądem.",
                            "target_lang_translations": {
                                "ru": "Мы будем <отстаивать> свои права в суде."
                            }
                        },
                        {
                            "text": "Udało mu się w końcu <dojść> sprawiedliwości.",
                            "target_lang_translations": {
                                "ru": "Ему наконец удалось <добиться> справедливости."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyegzekwować",
                        "uzyskać"
                    ],
                    "common_phrases": [
                        "dojść swego",
                        "dojść sprawiedliwości",
                        "dojść roszczeń"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получить что-либо причитающееся законным путем или благодаря стараниям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добиться",
                                "target_lang_sense_clarification": "справедливости или прав"
                            },
                            {
                                "target_lang_word": "отстоять",
                                "target_lang_sense_clarification": "права"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd37012b-2e26-31cd-9a4b-1765b517cacf",
                    "sense_definition": "Osiągnąć orgazm.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sexual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Partnerzy starali się <dojść> w tym samym momencie.",
                            "target_lang_translations": {
                                "ru": "Партнеры старались <кончить> одновременно."
                            }
                        },
                        {
                            "text": "Ona <doszła> wyjątkowo szybko.",
                            "target_lang_translations": {
                                "ru": "Она <кончила> исключительно быстро."
                            }
                        }
                    ],
                    "synonyms": [
                        "szczytować"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытать оргазм."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кончить",
                                "target_lang_sense_clarification": "испытать оргазм"
                            }
                        ]
                    }
                }
            ]
        },
        {

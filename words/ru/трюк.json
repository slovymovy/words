    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5c26db53-e4a0-353d-bd1c-c0b8142a0756",
                    "sense_definition": "Ловкий или рискованный приём, который сложно повторить неподготовленному человеку (обычно в цирке, кино или спорте).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каскадёр выполнил опасный <w>трюк</w> на быстром мотоцикле.",
                            "target_lang_translations": {
                                "en": "The stuntman performed a dangerous <w>stunt</w> on a fast motorcycle."
                            }
                        },
                        {
                            "text": "Зрители с восторгом смотрели на каждый новый акробатический <w>трюк</w>.",
                            "target_lang_translations": {
                                "en": "The audience watched every new acrobatic <w>stunt</w> with delight."
                            }
                        }
                    ],
                    "synonyms": [
                        "фо́кус"
                    ],
                    "common_phrases": [
                        "акробатический трюк",
                        "смертельный трюк",
                        "выполнить трюк"
                    ],
                    "target_lang_definitions": {
                        "en": "A skillful or risky move that is difficult for an unprepared person to repeat (usually in a circus, movies, or sports)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stunt",
                                "target_lang_sense_clarification": "dangerous or cinematic feat"
                            },
                            {
                                "target_lang_word": "trick",
                                "target_lang_sense_clarification": "skillful move"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53b869ba-29db-3835-a324-5517e4f8705f",
                    "sense_definition": "Хитрый поступок или уловка для достижения цели, часто с целью обмана.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это был всего лишь рекламный <w>трюк</w>, чтобы привлечь покупателей.",
                            "target_lang_translations": {
                                "en": "It was just a publicity <w>stunt</w> to attract buyers."
                            }
                        },
                        {
                            "text": "Он использовал старый <w>трюк</w>, чтобы избежать заслуженного наказания.",
                            "target_lang_translations": {
                                "en": "He used an old <w>trick</w> to avoid deserved punishment."
                            }
                        }
                    ],
                    "synonyms": [
                        "уловка",
                        "манёвр",
                        "фо́кус"
                    ],
                    "common_phrases": [
                        "рекламный трюк",
                        "дешёвый трюк",
                        "пустить в ход трюк"
                    ],
                    "target_lang_definitions": {
                        "en": "A cunning act or ploy to achieve a goal, often with the aim of deceiving."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trick",
                                "target_lang_sense_clarification": "deceptive act"
                            },

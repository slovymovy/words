        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cd4c5a38-7465-3a02-b440-8c375b8f2e4a",
                    "sense_definition": "Ciemnoszary, często z brunatnym odcieniem; brudnoszary, ponury.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "color_desc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nad miastem wisiały ciężkie, <bure> chmury, zapowiadające deszcz.",
                            "target_lang_translations": {
                                "ru": "Над городом висели тяжёлые <серые> тучи, предвещающие дождь."
                            }
                        },
                        {
                            "text": "Jego płaszcz był stary i <bury>, więc nikt nie zwracał na niego uwagi.",
                            "target_lang_translations": {
                                "ru": "Его плащ был старым и <грязно-серым>, поэтому никто не обращал на него внимания."
                            }
                        }
                    ],
                    "synonyms": [
                        "salceson",
                        "śledź"
                    ],
                    "antonyms": [
                        "jasny",
                        "kolorowy",
                        "barwny"
                    ],
                    "common_phrases": [
                        "bure chmury",
                        "bure niebo",
                        "bury dzień"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тёмно-серый, часто с коричневатым оттенком; грязно-серый, мрачный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грязно-серый",
                                "target_lang_sense_clarification": "цвет"
                            },
                            {
                                "target_lang_word": "тёмно-серый",
                                "target_lang_sense_clarification": "цвет"
                            },
                            {
                                "target_lang_word": "бурый",
                                "target_lang_sense_clarification": "коричневато-серый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fdfc86d4-1bea-3cdc-a4ed-7f3ac37cc2da",
                    "sense_definition": "O sierści: szary, zwykle w ciemniejsze prążki lub cętki (najczęściej o kocie).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "color_desc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz <bury> kot uwielbia spać na kaloryferze.",

    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "520126ba-0e23-3a2b-86ae-8599e65dea89",
                    "sense_definition": "Слово, которое употребляется при расставании с кем-либо на длительное время или навсегда (при обращении на «ты»).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ну всё, <w>прощай</w>, я уезжаю в другой город и вряд ли вернусь.",
                            "target_lang_translations": {
                                "en": "Well, that's it, <w>farewell</w>, I'm leaving for another city and it's unlikely I'll return."
                            }
                        },
                        {
                            "text": "<w>Прощай</w>, мой друг, мы больше никогда не увидимся.",
                            "target_lang_translations": {
                                "en": "<w>Farewell</w>, my friend, we will never see each other again."
                            }
                        }
                    ],
                    "synonyms": [
                        "прощайте"
                    ],
                    "common_phrases": [
                        "прощай навсегда",
                        "прощай навеки"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A word used when parting with someone for a long time or forever (informal address)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "farewell",
                                "target_lang_sense_clarification": "used for final partings"
                            },
                            {
                                "target_lang_word": "goodbye",
                                "target_lang_sense_clarification": "implying a long separation"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f88d359e-5076-3b4a-b8fc-a40deba615ab",
                    "sense_definition": "Форма повелительного наклонения глагола <w>прощать</w> (2-е лицо, единственное число).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Прощай</w> людям их ошибки, и тебе самому станет легче жить.",
                            "target_lang_translations": {
                                "en": "<w>Forgive</w> people their mistakes, and it will become easier for you to live."
                            }
                        },
                        {
                            "text": "Пожалуйста, <w>прощай</w> меня за то, что я не смог прийти вовремя.",
                            "target_lang_translations": {
                                "en": "Please <w>forgive</w> me for the fact that I couldn't come on time."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "imperative"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Imperative form of the verb 'to forgive' (2nd person singular)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "forgive",
                                "target_lang_sense_clarification": "imperative form"
                            }
                        ]
                    }
                }
            ]
        }
    ]

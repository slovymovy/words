            "senses": [
                {
                    "sense_id": "ddb49de3-0ec7-3a39-b14f-cb6815edb6f6",
                    "sense_definition": "A series of lessons or lectures on a particular subject.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I decided to sign up for a photography <w>course</w> at the local college.",
                            "target_lang_translations": {
                                "ru": "Я решил записаться на <w>курс</w> фотографии в местном колледже."
                            }
                        },
                        {
                            "text": "She is taking an online <w>course</w> to improve her business skills.",
                            "target_lang_translations": {
                                "ru": "Она проходит онлайн-<w>курс</w>, чтобы улучшить свои деловые навыки."
                            }
                        }
                    ],
                    "synonyms": [
                        "class"
                    ],
                    "common_phrases": [
                        "take a course",
                        "online course",
                        "training course",
                        "crash course"
                    ],
                    "target_lang_definitions": {

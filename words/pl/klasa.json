                },
                {
                    "sense_id": "fa139208-3658-39a6-85f9-541994258d10",
                    "sense_definition": "Pomieszczenie w szkole, w którym odbywają się lekcje.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "edukacja",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel poprosił uczniów, aby weszli do <klasy> i zajęli miejsca.",

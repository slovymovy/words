    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ca64cef7-7bbc-3bec-bcd9-a896a8b757f2",
                    "sense_definition": "Waar behoefte aan is; wat men moet hebben of doen omdat het niet gemist kan worden.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is niet <w>nodig</w> om zo hard te schreeuwen.",
                            "target_lang_translations": {
                                "en": "It is not <w>necessary</w> to scream so loud.",
                                "ru": "Не <w>нужно</w> так громко кричать."
                            }
                        },
                        {
                            "text": "Hebben we nog speciale spullen <w>nodig</w> voor de vakantie?",
                            "target_lang_translations": {
                                "en": "Do we still <w>need</w> special things for the holiday?",
                                "ru": "Нам еще <w>нужны</w> какие-то особые вещи для отпуска?"
                            }
                        }
                    ],
                    "synonyms": [
                        "noodzakelijk",
                        "vereist",
                        "onmisbaar"
                    ],
                    "antonyms": [
                        "onnodig"
                    ],
                    "common_phrases": [
                        "niet nodig zijn",
                        "hulp nodig hebben",
                        "hard nodig",
                        "nergens voor nodig"
                    ],
                    "target_lang_definitions": {
                        "en": "Required or essential; what one must have or do because it cannot be missed.",
                        "ru": "То, в чем есть потребность; то, что нужно иметь или сделать, так как без этого нельзя обойтись."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "necessary",
                                "target_lang_sense_clarification": "essential or required"
                            },
                            {
                                "target_lang_word": "needed",
                                "target_lang_sense_clarification": "required for a specific purpose"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "нужный",
                                "target_lang_sense_clarification": "требуемый или полезный"
                            },
                            {
                                "target_lang_word": "необходимый",
                                "target_lang_sense_clarification": "обязательный, без которого нельзя обойтись"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86b52c12-65b2-3913-ae30-898f6ae58358",
                    "sense_definition": "Een behoorlijke hoeveelheid; nogal wat (vaak gebruikt in combinatie met 'de').",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het heeft de <w>nodige</w> moeite gekost om het project af te ronden.",
                            "target_lang_translations": {
                                "en": "It took <w>quite some</w> effort to complete the project.",
                                "ru": "Завершение проекта стоило <w>немалых</w> усилий."
                            }
                        },
                        {
                            "text": "Tijdens de vergadering ontstond er de <w>nodige</w> verwarring over de nieuwe regels.",
                            "target_lang_translations": {
                                "en": "During the meeting, there was <w>considerable</w> confusion about the new rules.",
                                "ru": "Во время собрания возникла <w>изрядная</w> путаница по поводу новых правил."
                            }
                        }
                    ],
                    "synonyms": [
                        "flink wat",
                        "behoorlijk wat",
                        "veel"
                    ],
                    "common_phrases": [
                        "de nodige tijd",
                        "de nodige ervaring",
                        "de nodige problemen"
                    ],
                    "target_lang_definitions": {
                        "en": "A considerable amount; quite a lot (often used in combination with 'the').",
                        "ru": "Значительное количество; немало (часто используется в сочетании с определенным артиклем 'de')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "considerable",
                                "target_lang_sense_clarification": "a large amount"
                            },
                            {
                                "target_lang_word": "quite some",
                                "target_lang_sense_clarification": "a significant amount"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "немалый",
                                "target_lang_sense_clarification": "значительный по количеству или размеру"
                            },
                            {
                                "target_lang_word": "изрядный",
                                "target_lang_sense_clarification": "довольно большой"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "3f56cb29-7de0-3373-baf2-232632beb20a",
                    "sense_definition": "Op dringende wijze; in hoge mate.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze kamer moet <w>nodig</w> een keer worden opgeruimd.",
                            "target_lang_translations": {
                                "en": "This room <w>badly</w> needs to be cleaned sometime.",
                                "ru": "В этой комнате <w>срочно</w> нужно убраться."
                            }
                        },
                        {
                            "text": "Ik moet echt heel <w>nodig</w> naar het toilet.",
                            "target_lang_translations": {
                                "en": "I really <w>badly</w> need to go to the toilet.",
                                "ru": "Мне правда очень <w>сильно</w> нужно в туалет."
                            }
                        }
                    ],
                    "synonyms": [
                        "dringend",
                        "hoognodig"
                    ],
                    "common_phrases": [
                        "nodig moeten",
                        "ergens nodig aan toe zijn"
                    ],
                    "target_lang_definitions": {
                        "en": "Urgently; to a high degree.",
                        "ru": "Срочно; в высокой степени."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "badly",
                                "target_lang_sense_clarification": "very much or urgently"
                            },
                            {
                                "target_lang_word": "urgently",
                                "target_lang_sense_clarification": "requiring immediate attention"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "срочно",
                                "target_lang_sense_clarification": "требует немедленного внимания"
                            },
                            {
                                "target_lang_word": "очень",
                                "target_lang_sense_clarification": "в сильной степени (о потребности)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fd495d16-2996-3fd5-90b3-370593c296b5",
                    "sense_definition": "Vervoeging van <w>nodigen</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>nodig</w> je van harte uit voor mijn verjaardagsfeest.",
                            "target_lang_translations": {
                                "en": "I cordially <w>invite</w> you to my birthday party.",
                                "ru": "Я от всей души <w>приглашаю</w> тебя на свой день рождения."
                            }
                        },
                        {
                            "text": "<w>Nodig</w> jij de buren uit voor de barbecue vanavond?",
                            "target_lang_translations": {
                                "en": "Will you <w>invite</w> the neighbors for the barbecue tonight?",
                                "ru": "Ты <w>пригласишь</w> соседей на барбекю сегодня вечером?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Conjugated form of 'nodigen' (to invite).",
                        "ru": "Спрягаемая форма глагола 'nodigen' (приглашать)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "invite",
                                "target_lang_sense_clarification": "to ask someone to come"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "приглашать",
                                "target_lang_sense_clarification": "звать кого-то прийти"
                            }
                        ]
                    }
                }
            ]
        }
    ]

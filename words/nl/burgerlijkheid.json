            "senses": [
                {
                    "sense_id": "e850430d-c2ff-3c28-838f-c081562a9127",
                    "sense_definition": "Het gedrag en de levenswijze die typisch zijn voor gewone mensen, vaak geassocieerd met rust, regelmaat en eenvoud.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze genoten van de rustige <w>burgerlijkheid</w> in hun nieuwe woonwijk.",
                            "target_lang_translations": {
                                "ru": "Они наслаждались спокойной <w>буржуазностью</w> своего нового жилого района."
                            }
                        },
                        {
                            "text": "Ondanks zijn succes bleef hij vasthouden aan een zekere <w>burgerlijkheid</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на свой успех, он продолжал придерживаться определенной <w>буржуазности</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gezellige burgerlijkheid",
                        "saaie burgerlijkheid",
                        "typische burgerlijkheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поведение и образ жизни, типичные для обычных людей, часто ассоциируемые со спокойствием, размеренностью и простотой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "буржуазность",
                                "target_lang_sense_clarification": "образ жизни среднего класса"
                            },
                            {
                                "target_lang_word": "бюргерство",
                                "target_lang_sense_clarification": "размеренный уклад жизни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7bac206f-19c3-336c-8b02-1bb57a17ce21",
                    "sense_definition": "Een bekrompen manier van denken en leven die overdreven hecht aan vastgeroeste gewoonten en fatsoen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De schrijver verzette zich fel tegen de verstikkende <w>burgerlijkheid</w> van zijn tijd.",
                            "target_lang_translations": {
                                "ru": "Писатель яростно выступал против удушающего <w>мещанства</w> своего времени."
                            }
                        },
                        {
                            "text": "Hij wilde ontsnappen aan de <w>burgerlijkheid</w> en de wereld ontdekken.",
                            "target_lang_translations": {
                                "ru": "Он хотел вырваться из <w>обывательщины</w> и открыть для себя мир."
                            }
                        }
                    ],
                    "synonyms": [
                        "bekrompenheid",
                        "kleinburgerlijkheid"
                    ],
                    "common_phrases": [
                        "verstikkende burgerlijkheid",
                        "benauwde burgerlijkheid",
                        "Hollandse burgerlijkheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ограниченный образ мыслей и жизни, придающий чрезмерное значение устоявшимся привычкам и внешним приличиям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мещанство",
                                "target_lang_sense_clarification": "ограниченность взглядов"
                            },
                            {
                                "target_lang_word": "обывательщина",
                                "target_lang_sense_clarification": "узкий кругозор"
                            }
                        ]
                    }
                }
            ]

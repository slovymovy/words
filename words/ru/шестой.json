                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "five",
                                "target_lang_sense_clarification": "in expressions like 'half past five'"
                            },
                            {
                                "target_lang_word": "sixth",
                                "target_lang_sense_clarification": "referring to the specific hour sequence"
                            }
                        ]

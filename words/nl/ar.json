    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "046f395f-b255-367a-b56c-030196469608",
                    "sense_definition": "Een slee die door trekdieren, zoals paarden, over sneeuw of ijs wordt getrokken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op de <w>ar</w> gleden we door de sneeuw.",
                            "target_lang_translations": {
                                "en": "We glided through the snow on the <w>sleigh</w>.",
                                "ru": "Мы скользили по снегу на <w>санях</w>."
                            }
                        },
                        {
                            "text": "De paarden trokken de <w>ar</w> moeiteloos over het bevroren meer.",
                            "target_lang_translations": {
                                "en": "The horses pulled the <w>sleigh</w> effortlessly across the frozen lake.",
                                "ru": "Лошади без усилий тянули <w>сани</w> по замерзшему озеру."
                            }
                        }
                    ],
                    "synonyms": [
                        "arrenslede",
                        "arrenslee"
                    ],
                    "common_phrases": [
                        "met de ar"
                    ],
                    "target_lang_definitions": {
                        "en": "A vehicle on runners, usually drawn by horses, used for traveling over snow or ice.",
                        "ru": "Сани, которые тянут упряжные животные, например лошади, по снегу или льду."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sleigh",
                                "target_lang_sense_clarification": "vehicle pulled by animals over snow"
                            },
                            {
                                "target_lang_word": "sledge",
                                "target_lang_sense_clarification": "vehicle on runners for transporting loads or passengers"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сани",
                                "target_lang_sense_clarification": "повозка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ebd9bc94-d9f7-3957-a921-ce1eeea2ee24",
                    "sense_definition": "Digitale informatie die aan de waarneming van de werkelijkheid wordt toegevoegd; afkorting van augmented reality.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Veel moderne spellen gebruiken <w>AR</w> om de spelwereld met de echte wereld te mengen.",
                            "target_lang_translations": {
                                "en": "Many modern games use <w>AR</w> to mix the game world with the real world.",
                                "ru": "Многие современные игры используют <w>AR</w> (дополненную реальность), чтобы смешивать игровой мир с реальным."
                            }
                        },
                        {
                            "text": "Met een <w>AR</w>-bril kun je extra informatie zien over de gebouwen waar je naar kijkt.",
                            "target_lang_translations": {
                                "en": "With <w>AR</w> glasses, you can see extra information about the buildings you are looking at.",
                                "ru": "В <w>AR</w>-очках можно увидеть дополнительную информацию о зданиях, на которые вы смотрите."
                            }
                        }
                    ],
                    "synonyms": [
                        "augmented reality"
                    ],
                    "target_lang_definitions": {
                        "en": "Abbreviation for augmented reality; technology that superimposes a computer-generated image on a user's view of the real world.",
                        "ru": "Цифровая информация, добавляемая к восприятию реальности; аббревиатура от augmented reality."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "AR",
                                "target_lang_sense_clarification": "abbreviation for augmented reality"
                            },
                            {
                                "target_lang_word": "augmented reality",
                                "target_lang_sense_clarification": "technology overlaying digital info on real world"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "дополненная реальность",
                                "target_lang_sense_clarification": "технология"
                            },
                            {
                                "target_lang_word": "AR",
                                "target_lang_sense_clarification": "аббревиатура"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "750300de-bd3d-3498-a264-73804f8492ff",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>arren</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>ar</w> elke winter met mijn paard door het bos.",

            "senses": [
                {
                    "sense_id": "20c30485-24f4-3d35-90b6-99f1b6b7fabb",
                    "sense_definition": "Popularna potrawa z ziemniaków pokrojonych w słupki i smażonych w głębokim tłuszczu lub pieczonych.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na obiad zamówiliśmy rybę i <w>frytki</w> z surówką.",
                            "target_lang_translations": {
                                "ru": "На обед мы заказали рыбу и <w>картофель фри</w> с салатом."
                            }
                        },
                        {
                            "text": "Dzieci zazwyczaj lubią jeść <w>frytki</w> z ketchupem lub majonezem.",
                            "target_lang_translations": {
                                "ru": "Дети обычно любят есть <w>картофель фри</w> с кетчупом или майонезом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "frytki z ketchupem",
                        "duże frytki",
                        "porcja frytek",
                        "frytki belgijskie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Популярное блюдо из картофеля, нарезанного брусочками и обжаренного во фритюре или запеченного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "картофель фри",
                                "target_lang_sense_clarification": "блюдо"
                            },
                            {
                                "target_lang_word": "фри",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d7e9556-c8c5-36d1-99a3-8b8bb66668ec",
                    "sense_definition": "Forma mianownika, biernika lub wołacza liczby mnogiej od rzeczownika <w>frytka</w> (oznaczająca pojedyncze kawałki).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na talerzu zostały tylko dwie zimne <w>frytki</w>.",
                            "target_lang_translations": {
                                "ru": "На тарелке остались только два холодных <w>ломтика картофеля</w>."
                            }
                        },
                        {
                            "text": "Te <w>frytki</w> są zbyt mocno przypieczone i twarde.",
                            "target_lang_translations": {
                                "ru": "Этот <w>картофель фри</w> слишком сильно зажарен и твердый."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural of frytka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма именительного, винительного или звательного падежа множественного числа от существительного frytka (обозначает отдельные кусочки)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ломтики картофеля фри",
                                "target_lang_sense_clarification": "отдельные кусочки"
                            },
                            {
                                "target_lang_word": "картофель фри",
                                "target_lang_sense_clarification": "собирательное значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98209ccb-c4a5-3d86-88f3-e64df939e302",
                    "sense_definition": "Forma dopełniacza liczby pojedynczej od rzeczownika <w>frytka</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie zjadłem ani jednej <w>frytki</w>, bo nie byłem głodny.",
                            "target_lang_translations": {
                                "ru": "Я не съел ни одного <w>ломтика картофеля</w>, потому что не был голоден."
                            }
                        },
                        {
                            "text": "Szukam tej konkretnej <w>frytki</w>, która spadła na podłogę.",
                            "target_lang_translations": {
                                "ru": "Я ищу тот конкретный <w>ломтик картофеля</w>, который упал на пол."
                            }
                        }
                    ],
                    "traits": [

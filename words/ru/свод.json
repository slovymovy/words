{
    "word_family": [
        "сводить",
        "сводный",
        "сводка"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a07a938e-8194-3aaa-b8b6-d752db25e6c7",
                    "sense_definition": "Систематизированное собрание законов, правил, текстов или данных, объединённых в единое целое.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый новый сотрудник обязан внимательно изучить <w>свод</w> корпоративных правил.",
                            "target_lang_translations": {
                                "en": "Every new employee is required to carefully study the <w>code</w> of corporate rules."
                            }
                        },
                        {
                            "text": "В библиотеке университета хранится полный <w>свод</w> исторических документов этой эпохи.",
                            "target_lang_translations": {
                                "en": "The university library holds a complete <w>collection</w> of historical documents from that era."
                            }
                        }
                    ],
                    "synonyms": [
                        "собрание",
                        "корпус"
                    ],
                    "common_phrases": [
                        "свод законов",
                        "свод правил",
                        "свод обычаев"
                    ],
                    "target_lang_definitions": {
                        "en": "A systematic collection of laws, rules, texts, or data organized into a single body."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "code",
                                "target_lang_sense_clarification": "system of laws or rules"
                            },
                            {
                                "target_lang_word": "collection",
                                "target_lang_sense_clarification": "systematic set of documents"
                            },
                            {
                                "target_lang_word": "corpus",
                                "target_lang_sense_clarification": "body of texts or data"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "893842d7-59bc-3466-ada9-52f864808b22",
                    "sense_definition": "Пространство или поверхность, имеющие форму дуги или купола (часто о небе или кронах деревьев).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vault_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Яркое солнце медленно катилось через голубой небесный <w>свод</w>.",
                            "target_lang_translations": {
                                "en": "The bright sun rolled slowly across the blue <w>vault</w> of heaven."
                            }
                        },
                        {
                            "text": "Мы шли по лесной тропинке под густым зелёным <w>сводом</w> вековых дубов.",
                            "target_lang_translations": {
                                "en": "We walked along the forest path under the thick green <w>canopy</w> of ancient oaks."
                            }
                        }
                    ],
                    "common_phrases": [
                        "небесный свод"
                    ],
                    "target_lang_definitions": {
                        "en": "An arch-like or dome-like space or surface, often referring to the sky or a covering of tree branches."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "vault",
                                "target_lang_sense_clarification": "of the sky or heaven"
                            },
                            {
                                "target_lang_word": "canopy",
                                "target_lang_sense_clarification": "overhead covering, especially of trees"
                            },
                            {
                                "target_lang_word": "firmament",
                                "target_lang_sense_clarification": "literary term for the sky"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef3b93a1-0553-34a6-bb04-abec4e37f26f",
                    "sense_definition": "Архитектурное криволинейное перекрытие, соединяющее стены или опоры какого-либо сооружения.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "vault_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Голоса туристов гулко раздавались под высоким каменным <w>сводом</w> старинного собора.",
                            "target_lang_translations": {
                                "en": "The voices of tourists echoed loudly under the high stone <w>vault</w> of the ancient cathedral."
                            }
                        },
                        {
                            "text": "Строители начали возводить <w>свод</w> над главным залом дворца.",
                            "target_lang_translations": {
                                "en": "The builders began to erect the <w>vault</w> over the palace's main hall."
                            }
                        }
                    ],
                    "common_phrases": [
                        "каменный свод",
                        "стрельчатый свод",
                        "под сводами"
                    ],
                    "target_lang_definitions": {
                        "en": "A curved architectural structure forming a ceiling or roof."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "vault",
                                "target_lang_sense_clarification": "arched ceiling or roof"
                            },
                            {
                                "target_lang_word": "arch",
                                "target_lang_sense_clarification": "curved structural element"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ec732f0-7326-39fa-a03c-50f7de6bffe1",
                    "sense_definition": "Действие по сбору, объединению или соединению чего-либо в одно целое.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для годового отчёта необходим полный <w>свод</w> всех статистических данных.",
                            "target_lang_translations": {
                                "en": "A complete <w>consolidation</w> of all statistical data is necessary for the annual report."
                            }
                        },
                        {
                            "text": "Автоматический <w>свод</w> информации из разных источников значительно ускоряет работу аналитиков.",
                            "target_lang_translations": {
                                "en": "Automatic <w>compilation</w> of information from different sources significantly speeds up the analysts' work."
                            }
                        }
                    ],
                    "synonyms": [
                        "сведение"
                    ],
                    "antonyms": [
                        "разведение",
                        "развод"
                    ],
                    "common_phrases": [
                        "свод данных"
                    ],
                    "target_lang_definitions": {
                        "en": "The act or result of gathering, combining, or summarizing information into a single report."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "consolidation",
                                "target_lang_sense_clarification": "combining data into a whole"
                            },
                            {
                                "target_lang_word": "compilation",
                                "target_lang_sense_clarification": "gathering information"
                            },
                            {
                                "target_lang_word": "summary",
                                "target_lang_sense_clarification": "condensed report"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
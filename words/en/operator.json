    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "46fa5d13-0c30-31b4-88cb-a80ed0766120",
                    "sense_definition": "A person who works for a telephone company and assists in connecting calls.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If you cannot find the number, dial the <w>operator</w> for assistance.",
                            "target_lang_translations": {
                                "ru": "Если вы не можете найти номер, позвоните <w>оператору</w> для получения помощи."
                            }
                        },
                        {
                            "text": "The <w>operator</w> connected me to the international line immediately.",
                            "target_lang_translations": {
                                "ru": "<w>Телефонист</w> немедленно соединил меня с международной линией."
                            }
                        }
                    ],
                    "synonyms": [
                        "telephonist"
                    ],
                    "common_phrases": [
                        "call the operator",
                        "dial the operator",
                        "switchboard operator"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сотрудник телефонной компании, помогающий соединять звонки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "телефонист",
                                "target_lang_sense_clarification": "сотрудник телефонной станции"
                            },
                            {
                                "target_lang_word": "оператор",
                                "target_lang_sense_clarification": "связь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b673a74f-252e-337e-b747-06595cae5d3f",
                    "sense_definition": "A person or company that controls a machine, system, or provides a specific service.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The machine <w>operator</w> stopped the production line when the alarm sounded.",
                            "target_lang_translations": {
                                "ru": "<w>Оператор</w> станка остановил производственную линию, когда прозвучал сигнал тревоги."
                            }
                        },
                        {
                            "text": "We booked our holiday through a well-known tour <w>operator</w>.",
                            "target_lang_translations": {
                                "ru": "Мы забронировали отпуск через известного тур<w>оператора</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "provider",
                        "technician",
                        "driver"
                    ],
                    "common_phrases": [
                        "tour operator",
                        "machine operator",
                        "bus operator",
                        "camera operator",
                        "system operator"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или компания, управляющие машиной, системой или предоставляющие определенную услугу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оператор",
                                "target_lang_sense_clarification": "управляющий устройством или поставщик услуг"
                            },
                            {
                                "target_lang_word": "машинист",
                                "target_lang_sense_clarification": "рабочий на станке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a7960bd-9a7d-39f8-8385-d5dc3e174ff6",
                    "sense_definition": "A symbol in computer programming that performs a specific action on data or variables.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "math_logic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must use the correct <w>operator</w> to compare the two values in your code.",
                            "target_lang_translations": {
                                "ru": "Вы должны использовать правильный <w>оператор</w> для сравнения двух значений в вашем коде."
                            }
                        },
                        {
                            "text": "The boolean <w>operator</w> returns true only if both conditions are met.",
                            "target_lang_translations": {
                                "ru": "Логический <w>оператор</w> возвращает «истину» только в том случае, если выполняются оба условия."
                            }
                        }
                    ],
                    "antonyms": [
                        "operand"
                    ],
                    "common_phrases": [
                        "logical operator",
                        "boolean operator",
                        "arithmetic operator"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Символ в программировании, выполняющий определенное действие над данными или переменными."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оператор",
                                "target_lang_sense_clarification": "программирование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35aa7858-cdac-3bff-9a13-632afea2f679",
                    "sense_definition": "A person who is skilled at making deals or getting what they want, often using clever or slightly dishonest methods.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is a smooth <w>operator</w> who always manages to close the deal regardless of the obstacles.",
                            "target_lang_translations": {
                                "ru": "Он большой <w>ловкач</w>, которому всегда удается заключить сделку, невзирая на препятствия."
                            }
                        },
                        {
                            "text": "Be careful when negotiating with him; he is known as a shrewd <w>operator</w> in the industry.",
                            "target_lang_translations": {
                                "ru": "Будьте осторожны при переговорах с ним; в этой индустрии он известен как проницательный <w>делец</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "manipulator",
                        "maneuverer"
                    ],
                    "common_phrases": [
                        "smooth operator",
                        "shrewd operator",
                        "political operator"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, умеющий заключать сделки или добиваться своего, часто используя хитрые методы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делец",
                                "target_lang_sense_clarification": "предприимчивый человек"
                            },
                            {
                                "target_lang_word": "ловкач",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "махинатор",
                                "target_lang_sense_clarification": "нечестный человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3cad28ed-dd23-3301-bef3-e9f76031ea02",
                    "sense_definition": "A mathematical function that changes one value or function into another.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "math_logic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The differential <w>operator</w> is a fundamental concept in calculus.",
                            "target_lang_translations": {
                                "ru": "Дифференциальный <w>оператор</w> является фундаментальным понятием в исчислении."
                            }
                        },
                        {
                            "text": "Students learned how to apply the linear <w>operator</w> to the equation.",
                            "target_lang_translations": {
                                "ru": "Студенты учились применять линейный <w>оператор</w> к уравнению."
                            }
                        }
                    ],
                    "synonyms": [
                        "function",
                        "mapping"
                    ],
                    "common_phrases": [
                        "linear operator",
                        "differential operator"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Mathematics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Математическая функция, преобразующая одно значение или функцию в другую."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оператор",
                                "target_lang_sense_clarification": "математика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7114d3a2-d57a-3930-a5b6-69b5c0f71367",
                    "sense_definition": "A member of a military special operations unit.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The special forces <w>operator</w> moved silently through the hostile territory.",
                            "target_lang_translations": {
                                "ru": "<w>Боец</w> спецназа бесшумно продвигался по вражеской территории."
                            }
                        },
                        {
                            "text": "Every <w>operator</w> in the team is highly trained in combat and survival skills.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>спецназовец</w> в команде отлично обучен навыкам боя и выживания."
                            }
                        }
                    ],
                    "synonyms": [
                        "commando"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Military"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Член подразделения специальных операций вооруженных сил."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спецназовец",
                                "target_lang_sense_clarification": "боец спецназа"
                            },
                            {
                                "target_lang_word": "оперативник",
                                "target_lang_sense_clarification": "сотрудник спецслужб"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "f0437e59-2243-37eb-bdf2-5defc09f4757",
                    "sense_definition": "Iets weer op zijn oorspronkelijke plek zetten.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_placement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet de boeken na gebruik netjes in de kast <w>terugplaatsen</w>.",
                            "target_lang_translations": {
                                "en": "You must neatly <w>put back</w> the books in the bookcase after use.",
                                "ru": "После использования нужно аккуратно <w>вернуть</w> книги в шкаф."
                            }
                        },
                        {
                            "text": "De monteur zal het gerepareerde onderdeel voorzichtig <w>terugplaatsen</w>.",
                            "target_lang_translations": {
                                "en": "The mechanic will carefully <w>put back</w> the repaired part.",
                                "ru": "Механик осторожно <w>установит обратно</w> отремонтированную деталь."
                            }
                        }
                    ],
                    "synonyms": [
                        "terugzetten",
                        "herplaatsen",
                        "terugleggen"
                    ],
                    "common_phrases": [
                        "op zijn plek terugplaatsen",
                        "voorzichtig terugplaatsen"
                    ],
                    "target_lang_definitions": {
                        "en": "To put something back in its original place.",
                        "ru": "Поставить или вернуть что-либо на прежнее место."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put back",
                                "target_lang_sense_clarification": "general action of returning an object"
                            },
                            {
                                "target_lang_word": "replace",
                                "target_lang_sense_clarification": "to place something back where it belongs"
                            },
                            {
                                "target_lang_word": "return",
                                "target_lang_sense_clarification": "formal; to restore to a former place"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вернуть",
                                "target_lang_sense_clarification": "положить или поставить обратно"
                            },
                            {
                                "target_lang_word": "поставить на место",
                                "target_lang_sense_clarification": "разместить там, где было"
                            },
                            {
                                "target_lang_word": "установить обратно",
                                "target_lang_sense_clarification": "для деталей или механизмов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8bbede63-27d5-3ff1-a3d4-1b81e637c1da",
                    "sense_definition": "Iemand of iets een lagere rang of positie geven.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "rank_demotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vanwege zijn slechte prestaties wilden ze hem in rang <w>terugplaatsen</w>.",
                            "target_lang_translations": {
                                "en": "Because of his poor performance, they wanted to <w>demote</w> him in rank.",
                                "ru": "Из-за плохих результатов его хотели <w>понизить</w> в звании."
                            }
                        },
                        {
                            "text": "De trainer besloot de speler naar het tweede team <w>terugplaatsen</w>.",
                            "target_lang_translations": {
                                "en": "The coach decided to <w>demote</w> the player to the second team.",
                                "ru": "Тренер решил <w>перевести</w> игрока <w>обратно</w> во вторую команду."
                            }
                        }
                    ],
                    "synonyms": [
                        "degraderen",
                        "terugzetten",
                        "achteruitzetten"
                    ],
                    "common_phrases": [
                        "in rang terugplaatsen",
                        "naar een lager team terugplaatsen"
                    ],
                    "target_lang_definitions": {
                        "en": "To assign someone or something to a lower rank or position.",
                        "ru": "Понизить кого-либо в должности, звании или вернуть на более низкую позицию."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "demote",
                                "target_lang_sense_clarification": "reduce in rank or status"
                            },
                            {
                                "target_lang_word": "relegate",
                                "target_lang_sense_clarification": "move to a lower division or position"
                            },
                            {
                                "target_lang_word": "move back",
                                "target_lang_sense_clarification": "return to a previous position"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "понизить",
                                "target_lang_sense_clarification": "в звании или должности"
                            },
                            {
                                "target_lang_word": "перевести обратно",
                                "target_lang_sense_clarification": "вернуть в предыдущую команду или класс"
                            }
                        ]
                    }
                }
            ]
        }

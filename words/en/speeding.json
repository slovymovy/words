                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "под кайфом",
                                "target_lang_sense_clarification": "сленг, состояние наркотического опьянения"
                            },
                            {
                                "target_lang_word": "под спидами",
                                "target_lang_sense_clarification": "сленг, под воздействием амфетаминов"

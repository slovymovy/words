    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1a3340e9-0915-3555-bd90-d24be62b661c",
                    "sense_definition": "Iemand die erg aardig, zachtaardig en behulpzaam is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "affectionate_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je bent echt een <w>lieverd</w> omdat je me hebt geholpen met verhuizen.",
                            "target_lang_translations": {
                                "ru": "Ты просто <w>прелесть</w>, что помог мне с переездом."
                            }
                        },
                        {
                            "text": "Mijn oma is een grote <w>lieverd</w> die altijd koekjes voor ons bakt.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка — настоящая <w>душка</w>, она всегда печет нам печенье."
                            }
                        }
                    ],
                    "synonyms": [
                        "schat",
                        "lieveling"
                    ],
                    "common_phrases": [
                        "een echte lieverd",
                        "wat een lieverd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кто-то очень милый, добрый и отзывчивый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "душка",
                                "target_lang_sense_clarification": "очень приятный, милый человек"
                            },
                            {
                                "target_lang_word": "прелесть",
                                "target_lang_sense_clarification": "о человеке, вызывающем восхищение своей добротой"
                            },
                            {
                                "target_lang_word": "золото",
                                "target_lang_sense_clarification": "перен. о человеке с прекрасным характером"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6cf8337-06be-34b5-a4c9-ccf52d195d74",
                    "sense_definition": "Een koosnaam om iemand aan te spreken van wie men houdt, zoals een partner of kind.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "affectionate_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Slaap lekker, <w>lieverd</w>, ik zie je morgenochtend weer.",
                            "target_lang_translations": {
                                "ru": "Спи сладко, <w>солнышко</w>, увидимся завтра утром."
                            }
                        },
                        {
                            "text": "Sorry <w>lieverd</w>, ik ben wat later thuis vandaag.",
                            "target_lang_translations": {
                                "ru": "Извини, <w>милый</w>, я сегодня буду дома попозже."
                            }
                        }
                    ],
                    "synonyms": [
                        "schat",
                        "lieveling",
                        "schattebout"
                    ],
                    "common_phrases": [
                        "dag lieverd",
                        "hallo lieverd"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used as a term of endearment"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ласковое обращение к любимому человеку, например, к партнеру или ребенку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "милый / милая",
                                "target_lang_sense_clarification": "универсальное ласковое обращение"
                            },
                            {
                                "target_lang_word": "солнышко",
                                "target_lang_sense_clarification": "ласковое прозвище"
                            },
                            {
                                "target_lang_word": "дорогой / дорогая",
                                "target_lang_sense_clarification": "обращение к близкому человеку"
                            },
                            {
                                "target_lang_word": "зайка",
                                "target_lang_sense_clarification": "распространенное ласковое прозвище"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
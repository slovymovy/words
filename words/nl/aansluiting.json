            "pos": "noun",
            "senses": [
                {
                    "sense_id": "165ebfab-983e-3a2b-91da-78724b6cbe93",
                    "sense_definition": "De mogelijkheid om tijdens een reis over te stappen van het ene openbaar vervoermiddel op het andere.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport_link",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de vertraging van de trein misten veel reizigers hun <w>aansluiting</w>.",
                            "target_lang_translations": {
                                "en": "Due to the train's delay, many travelers missed their <w>connection</w>."
                            }
                        },
                        {
                            "text": "Ik heb de <w>aansluiting</w> met de bus naar het centrum net niet gehaald.",
                            "target_lang_translations": {
                                "en": "I just missed the <w>connection</w> to the bus to the city center."
                            }
                        }
                    ],
                    "synonyms": [
                        "overstap"
                    ],
                    "common_phrases": [
                        "de aansluiting missen",
                        "een goede aansluiting",
                        "aansluiting hebben"
                    ],
                    "target_lang_definitions": {
                        "en": "The opportunity to transfer from one means of public transport to another during a journey."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "connection",
                                "target_lang_sense_clarification": "transport transfer"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abe4b3a9-eb12-315b-a7ca-7c67878ee25c",
                    "sense_definition": "Een punt waar kabels of leidingen verbonden zijn met een netwerk, bijvoorbeeld voor elektriciteit, water of internet.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "technical_link",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In ons nieuwe huis hebben we direct een <w>aansluiting</w> op het glasvezelnetwerk aangevraagd.",
                            "target_lang_translations": {
                                "en": "In our new house, we immediately applied for a <w>connection</w> to the fiber optic network."
                            }
                        },
                        {
                            "text": "De monteur controleerde de <w>aansluiting</w> van de wasmachine op de waterleiding.",
                            "target_lang_translations": {
                                "en": "The technician checked the washing machine's <w>connection</w> to the water pipe."
                            }
                        }
                    ],
                    "synonyms": [
                        "verbinding",
                        "connectie"
                    ],
                    "antonyms": [
                        "afsluiting"
                    ],
                    "common_phrases": [
                        "aansluiting op het net",
                        "elektrische aansluiting",
                        "vaste aansluiting"
                    ],
                    "target_lang_definitions": {
                        "en": "A point where cables or pipes are linked to a network, such as for electricity, water, or internet."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "connection",
                                "target_lang_sense_clarification": "network or utility link"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4bccd888-f2d6-3b32-9348-94c76f3b71ed",
                    "sense_definition": "Het gevoel van contact, begrip of verbondenheid met een groep mensen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_link",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze vond op haar nieuwe school helaas weinig <w>aansluiting</w> bij de andere leerlingen.",
                            "target_lang_translations": {
                                "en": "Unfortunately, she found little <w>connection</w> with the other students at her new school."
                            }
                        },
                        {
                            "text": "Het verlegen jongetje zocht tijdens het spelen <w>aansluiting</w> bij zijn klasgenootjes.",
                            "target_lang_translations": {
                                "en": "The shy little boy sought a <w>connection</w> with his classmates while playing."
                            }
                        }
                    ],
                    "synonyms": [
                        "contact",
                        "band"
                    ],
                    "common_phrases": [
                        "aansluiting vinden bij",
                        "aansluiting zoeken"
                    ],
                    "target_lang_definitions": {
                        "en": "The feeling of contact, understanding, or belonging with a group of people."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "connection",
                                "target_lang_sense_clarification": "social bond or belonging"
                            },
                            {
                                "target_lang_word": "rapport",
                                "target_lang_sense_clarification": "harmonious understanding"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f6596fa-3f41-31ea-862d-1621dc8db695",
                    "sense_definition": "Iets wat logisch volgt op of past bij iets dat eerder is gebeurd of gezegd.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_link",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In <w>aansluiting</w> op de vergadering van vanochtend stuur ik jullie hierbij de notulen.",
                            "target_lang_translations": {
                                "en": "In <w>continuation</w> of this morning's meeting, I am sending you the minutes."
                            }
                        },
                        {
                            "text": "Dit nieuwe beleid is gemaakt in <w>aansluiting</w> op de internationale regels.",
                            "target_lang_translations": {
                                "en": "This new policy was made in <w>alignment</w> with international rules."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in aansluiting op",
                        "in aansluiting bij"
                    ],
                    "target_lang_definitions": {
                        "en": "Something that logically follows or fits with something that happened or was said earlier."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "continuation",
                                "target_lang_sense_clarification": "logical follow-up"
                            },
                            {
                                "target_lang_word": "alignment",
                                "target_lang_sense_clarification": "fitting with rules or context"
                            }
                        ]
                    }
                }
            ]
        }
    ]

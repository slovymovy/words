    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "52a6eb8c-2057-380a-89b3-a7fd3d6aa910",
                    "sense_definition": "To convey information, thoughts, or feelings by speech; to say or communicate something to someone.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>tell</w> me your name so I can introduce you."
                        },
                        {
                            "text": "He <w>told</w> us the truth about what happened yesterday."
                        }
                    ],
                    "synonyms": [
                        "make known",
                        "disclose"
                    ],
                    "common_phrases": [
                        "tell the truth",
                        "tell a lie",
                        "tell me",
                        "tell someone something"
                    ]
                },
                {
                    "sense_id": "d15dc865-99fc-332a-9dc8-98dd576966a6",
                    "sense_definition": "To narrate or give an account of a story or event.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandmother likes to <w>tell</w> stories about her childhood."
                        },
                        {
                            "text": "Can you <w>tell</w> us a funny joke to cheer everyone up?"

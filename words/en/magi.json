    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c76575f3-1dd7-3969-985c-a8cb216094c0",
                    "sense_definition": "The wise men from the East who visited and gave gifts to the baby Jesus, traditionally celebrated at Epiphany.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "biblical_figures",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "The <w>Magi</w> followed the bright star across the desert to find the newborn king.",
                            "target_lang_translations": {
                                "ru": "<w>Волхвы</w> следовали за яркой звездой через пустыню, чтобы найти новорожденного царя."
                            }
                        },
                        {
                            "text": "In the nativity play, three children dressed up as the <w>Magi</w> bearing gold, frankincense, and myrrh.",
                            "target_lang_translations": {
                                "ru": "В рождественском спектакле трое детей оделись как <w>волхвы</w>, несущие золото, ладан и смирну."
                            }
                        }
                    ],
                    "synonyms": [
                        "Three Wise Men",
                        "Three Kings",
                        "Wise Men"
                    ],
                    "common_phrases": [
                        "gifts of the Magi",
                        "adoration of the Magi",
                        "journey of the Magi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мудрецы с Востока, которые посетили младенца Иисуса и принесли ему дары; традиционно чествуются в праздник Богоявления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волхвы",
                                "target_lang_sense_clarification": "библейский термин"
                            },
                            {
                                "target_lang_word": "мудрецы",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0df99b3f-800f-37f0-81a3-c4d77256e726",
                    "sense_definition": "The three bright stars that form the belt of the constellation Orion.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "astronomy_names",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "On a clear winter night, you can easily identify the <w>Magi</w> in the center of the hunter constellation.",
                            "target_lang_translations": {
                                "ru": "Ясной зимней ночью можно легко найти <w>Трех волхвов</w> в центре созвездия Охотника."
                            }
                        },
                        {
                            "text": "Ancient sailors often navigated using the position of the <w>Magi</w> in the sky.",
                            "target_lang_translations": {
                                "ru": "Древние мореплаватели часто ориентировались по положению <w>Трех волхвов</w> на небе."
                            }
                        }
                    ],
                    "synonyms": [
                        "Three Kings",
                        "Three Sisters"
                    ],
                    "target_lang_definitions": {
                        "ru": "Три яркие звезды, образующие пояс созвездия Орион."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Три волхва",
                                "target_lang_sense_clarification": "народное название астеризма"
                            },
                            {
                                "target_lang_word": "Пояс Ориона",
                                "target_lang_sense_clarification": "астрономическое название"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2fcb9e9a-8372-3d89-a896-6c08be37588f",
                    "sense_definition": "Plural of <w>magus</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "plural_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient Persian <w>magi</w> were renowned for their wisdom and knowledge of the stars.",
                            "target_lang_translations": {
                                "ru": "Древние персидские <w>маги</w> славились своей мудростью и знанием звезд."
                            }
                        },
                        {
                            "text": "In the fantasy novel, a council of powerful <w>magi</w> gathered to protect the realm.",
                            "target_lang_translations": {
                                "ru": "В фэнтезийном романе совет могущественных <w>магов</w> собрался, чтобы защитить королевство."
                            }
                        }
                    ],
                    "synonyms": [
                        "maguses"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова magus (древнеперсидский жрец)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маги",
                                "target_lang_sense_clarification": "зороастрийские жрецы"
                            },
                            {
                                "target_lang_word": "жрецы",
                                "target_lang_sense_clarification": "служители культа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9eb05913-e020-3064-bbcf-9b23ab095354",
                    "sense_definition": "Plural of <w>mage</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "plural_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Several <w>magi</w> combined their magical energy to cast a protective shield.",
                            "target_lang_translations": {
                                "ru": "Несколько <w>магов</w> объединили свою магическую энергию, чтобы создать защитный щит."
                            }
                        },
                        {
                            "text": "The guild of <w>magi</w> required years of study before an apprentice could join.",
                            "target_lang_translations": {
                                "ru": "Гильдия <w>магов</w> требовала годы обучения, прежде чем ученик мог вступить в неё."
                            }
                        }
                    ],
                    "synonyms": [
                        "mages"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова mage (маг, волшебник)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маги",
                                "target_lang_sense_clarification": "волшебники"
                            },
                            {
                                "target_lang_word": "чародеи",
                                "target_lang_sense_clarification": "колдуны"
                            },
                            {
                                "target_lang_word": "кудесники",
                                "target_lang_sense_clarification": "устаревшее/поэтическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b6d68cb-03fa-3459-a458-793f7572bc5a",
                    "sense_definition": "An acronym for Modified Adjusted Gross Income, a financial metric used in tax calculations.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "financial_terms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Your eligibility for certain tax deductions depends on whether your <w>MAGI</w> falls below the threshold.",
                            "target_lang_translations": {
                                "ru": "Ваше право на определенные налоговые вычеты зависит от того, падает ли ваш <w>MAGI</w> ниже порогового значения."
                            }
                        },
                        {
                            "text": "The accountant asked for additional documents to accurately calculate the client's <w>MAGI</w>.",
                            "target_lang_translations": {
                                "ru": "Бухгалтер попросил дополнительные документы, чтобы точно рассчитать <w>MAGI</w> клиента."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура для Modified Adjusted Gross Income (модифицированный скорректированный валовой доход), финансовый показатель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "MAGI",
                                "target_lang_sense_clarification": "аббревиатура (используется как есть)"
                            },
                            {
                                "target_lang_word": "модифицированный скорректированный валовой доход",
                                "target_lang_sense_clarification": "полный перевод термина"
                            }
                        ]
                    }
                }
            ]
        }
    ]

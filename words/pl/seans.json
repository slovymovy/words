                        "seans kinowy",
                        "wieczorny seans",
                        "bilet na seans"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мероприятие, заключающееся в показе фильма в кинотеатре для зрителей"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сеанс",

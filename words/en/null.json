        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "580c0cff-b6c5-313c-ab7a-db45920a76a7",
                    "sense_definition": "Having no legal force, validity, or binding effect.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "legal_invalidity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The contract was declared <w>null</w> and void because it was never signed.",
                            "target_lang_translations": {
                                "ru": "Контракт был объявлен <w>недействительным</w>, так как он так и не был подписан."
                            }
                        },
                        {
                            "text": "If you break the rules, your warranty will be rendered <w>null</w>.",
                            "target_lang_translations": {
                                "ru": "Если вы нарушите правила, ваша гарантия станет <w>недействительной</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "invalid",
                        "void",
                        "ineffective"
                    ],
                    "antonyms": [
                        "non-null",
                        "antinull"
                    ],
                    "common_phrases": [
                        "null and void"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in the fixed phrase 'null and void'"
                        },
                        {
                            "trait_type": "form",
                            "comment": "legal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Не имеющий юридической силы, недействительный."

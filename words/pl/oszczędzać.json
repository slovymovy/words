                    "target_lang_definitions": {
                        "ru": "Проявлять милосердие, сохранять жизнь или оставлять что-то нетронутым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щадить",
                                "target_lang_sense_clarification": "проявлять милосердие"
                            },
                            {
                                "target_lang_word": "жалеть",
                                "target_lang_sense_clarification": "не причинять вреда из жалости"
                            }
                        ]
                    }
                }
            ]
        }

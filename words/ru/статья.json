                    "common_phrases": [
                        "статья расходов",
                        "статья доходов",
                        "статья бюджета",
                        "защищённые статьи"
                    ],
                    "target_lang_definitions": {
                        "en": "A section in a budget or financial document indicating a type of income or expense."
                    },
                    "translations": {

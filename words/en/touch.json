{
    "word_family": [
        "touchable",
        "untouchable",
        "touching",
        "touchy",
        "retouch",
        "touchdown",
        "touchscreen"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9e4be111-2f3c-3895-abeb-8fd771270ab3",
                    "sense_definition": "To put one's hand, finger, or another part of the body onto someone or something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_contact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please do not <w>touch</w> the artwork in the museum.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>трогайте</w> произведения искусства в музее."
                            }
                        },
                        {
                            "text": "She gently <w>touched</w> his arm to get his attention.",
                            "target_lang_translations": {
                                "ru": "Она нежно <w>коснулась</w> его руки, чтобы привлечь его внимание."
                            }
                        }
                    ],
                    "synonyms": [
                        "feel",
                        "contact",
                        "handle"
                    ],
                    "common_phrases": [
                        "touch the ground",
                        "touch your toes",
                        "do not touch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прикасаться рукой, пальцем или другой частью тела к кому-либо или чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трогать",
                                "target_lang_sense_clarification": "наиболее общее слово; касаться руками"
                            },
                            {
                                "target_lang_word": "прикасаться",
                                "target_lang_sense_clarification": "слегка дотрагиваться; более формально или осторожно"
                            },
                            {
                                "target_lang_word": "касаться",
                                "target_lang_sense_clarification": "легкий контакт, часто случайный или мимолетный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7172090d-07c0-3e6b-a48a-fda18e2b5d21",
                    "sense_definition": "To affect someone emotionally, especially by making them feel grateful, sympathetic, or sad.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotional_affect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was deeply <w>touched</w> by the kind letter you sent me.",
                            "target_lang_translations": {
                                "ru": "Я был глубоко <w>тронут</w> добрым письмом, которое вы мне прислали."
                            }
                        },
                        {
                            "text": "The sad ending of the movie really <w>touched</w> my heart.",
                            "target_lang_translations": {
                                "ru": "Грустный конец фильма действительно <w>тронул</w> мое сердце."
                            }
                        }
                    ],
                    "synonyms": [
                        "move",
                        "affect",
                        "stir"
                    ],
                    "common_phrases": [
                        "deeply touched",
                        "touched by"
                    ],
                    "target_lang_definitions": {
                        "ru": "Эмоционально воздействовать на кого-либо, вызывая чувство благодарности, сочувствия или грусти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трогать",
                                "target_lang_sense_clarification": "вызывать эмоциональный отклик"
                            },
                            {
                                "target_lang_word": "растрогать",
                                "target_lang_sense_clarification": "вызвать сильные чувства, умиление или слезы"
                            },
                            {
                                "target_lang_word": "волновать",
                                "target_lang_sense_clarification": "беспокоить или вызывать душевное волнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea05ec5d-311e-3404-9668-f36d22e6da8b",
                    "sense_definition": "To harm, disturb, or interfere with someone or something.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "harm_interfere",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If you <w>touch</w> my computer again, I will be very angry.",
                            "target_lang_translations": {
                                "ru": "Если ты снова <w>тронешь</w> мой компьютер, я очень разозлюсь."
                            }
                        },
                        {
                            "text": "The thief did not <w>touch</w> the jewelry, but he took the cash.",
                            "target_lang_translations": {
                                "ru": "Вор не <w>тронул</w> драгоценности, но забрал наличные."
                            }
                        }
                    ],
                    "synonyms": [
                        "harm",
                        "disturb",
                        "meddle with"
                    ],
                    "common_phrases": [
                        "dare not touch",
                        "never touched him"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причинять вред, беспокоить или вмешиваться во что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трогать",
                                "target_lang_sense_clarification": "беспокоить, мешать или брать без спроса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b35b8016-6e27-3a07-9b3e-330899dc2a04",
                    "sense_definition": "To eat or drink something; often used in negative sentences to show that someone has not eaten.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "consume",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was so sick that he hardly <w>touched</w> his dinner.",
                            "target_lang_translations": {
                                "ru": "Он был так болен, что едва <w>притронулся</w> к ужину."
                            }
                        },
                        {
                            "text": "I never <w>touch</w> coffee because it keeps me awake at night.",
                            "target_lang_translations": {
                                "ru": "Я никогда не <w>пью</w> кофе, потому что из-за него не могу уснуть ночью."
                            }
                        }
                    ],
                    "synonyms": [
                        "consume",
                        "eat",
                        "drink"
                    ],
                    "common_phrases": [
                        "hardly touched",
                        "never touch alcohol"
                    ],
                    "target_lang_definitions": {
                        "ru": "Есть или пить что-либо; часто используется в отрицательных предложениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "притрагиваться",
                                "target_lang_sense_clarification": "начать есть или пить (обычно с отрицанием)"
                            },
                            {
                                "target_lang_word": "трогать",
                                "target_lang_sense_clarification": "разг. употреблять в пищу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90794408-fe18-377d-9857-d6981e98785c",
                    "sense_definition": "To mention a subject briefly when speaking or writing.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The professor only <w>touched</w> on the complex history of the region.",
                            "target_lang_translations": {
                                "ru": "Профессор лишь <w>коснулся</w> сложной истории региона."
                            }
                        },
                        {
                            "text": "Our meeting <w>touched</w> upon several important issues regarding the budget.",
                            "target_lang_translations": {
                                "ru": "Наша встреча <w>затронула</w> несколько важных вопросов, касающихся бюджета."
                            }
                        }
                    ],
                    "synonyms": [
                        "mention",
                        "refer to",
                        "bring up"
                    ],
                    "common_phrases": [
                        "touch on",
                        "touch upon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кратко упоминать тему при разговоре или на письме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "касаться",
                                "target_lang_sense_clarification": "упоминать вскользь"
                            },
                            {
                                "target_lang_word": "затрагивать",
                                "target_lang_sense_clarification": "поднимать вопрос или тему для обсуждения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82dfdeb0-b7c0-317e-ae74-4589e95d4ad3",
                    "sense_definition": "To reach a particular level, amount, or state.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "reach_level",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The temperature is expected to <w>touch</w> thirty degrees tomorrow.",
                            "target_lang_translations": {
                                "ru": "Ожидается, что температура завтра <w>достигнет</w> тридцати градусов."
                            }
                        },
                        {
                            "text": "The car's speed <w>touched</w> one hundred miles per hour on the track.",
                            "target_lang_translations": {
                                "ru": "Скорость машины на трассе <w>достигла</w> ста миль в час."
                            }
                        }
                    ],
                    "synonyms": [
                        "reach",
                        "hit"
                    ],
                    "common_phrases": [
                        "touch bottom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достигать определенного уровня, количества или состояния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "достигать",
                                "target_lang_sense_clarification": "доходить до определенного предела"
                            },
                            {
                                "target_lang_word": "доходить до",
                                "target_lang_sense_clarification": "приближаться к уровню"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09fc7325-b793-3db9-b580-40c069116340",
                    "sense_definition": "To be as good as someone or something else; to rival in quality.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "compare",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "No other Italian restaurant in the city can <w>touch</w> this one for quality.",
                            "target_lang_translations": {
                                "ru": "Ни один другой итальянский ресторан в городе не может <w>сравниться</w> с этим по качеству."
                            }
                        },
                        {
                            "text": "When it comes to playing tennis, nobody can <w>touch</w> him.",
                            "target_lang_translations": {
                                "ru": "Когда дело доходит до тенниса, никто не может <w>сравниться</w> с ним."
                            }
                        }
                    ],
                    "synonyms": [
                        "rival",
                        "match",
                        "equal"
                    ],
                    "common_phrases": [
                        "nothing can touch",
                        "no one can touch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть таким же хорошим, как кто-то или что-то другое; соперничать по качеству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сравниться",
                                "target_lang_sense_clarification": "быть равным по качеству или силе"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "10adf664-6e15-36ab-849c-84f624b2ac22",
                    "sense_definition": "An act of putting a hand or finger on something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_contact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I felt a light <w>touch</w> on my shoulder and turned around.",
                            "target_lang_translations": {
                                "ru": "Я почувствовал легкое <w>прикосновение</w> к плечу и обернулся."
                            }
                        },
                        {
                            "text": "The screen responds to the slightest <w>touch</w> of a finger.",
                            "target_lang_translations": {
                                "ru": "Экран реагирует на малейшее <w>касание</w> пальца."
                            }
                        }
                    ],
                    "synonyms": [
                        "tap",
                        "pat",
                        "contact"
                    ],
                    "common_phrases": [
                        "gentle touch",
                        "light touch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, при котором кладут руку или палец на что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прикосновение",
                                "target_lang_sense_clarification": "акт касания"
                            },
                            {
                                "target_lang_word": "касание",
                                "target_lang_sense_clarification": "легкий контакт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef6e9ae9-202c-3f99-a3d7-b2d30c9603ce",
                    "sense_definition": "A state of communication or connection between people.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We promised to keep in <w>touch</w> after we graduated from college.",
                            "target_lang_translations": {
                                "ru": "Мы обещали поддерживать <w>связь</w> после окончания колледжа."
                            }
                        },
                        {
                            "text": "I have lost <w>touch</w> with many of my old friends.",
                            "target_lang_translations": {
                                "ru": "Я потерял <w>связь</w> со многими старыми друзьями."
                            }
                        }
                    ],
                    "synonyms": [
                        "contact",
                        "connection"
                    ],
                    "common_phrases": [
                        "keep in touch",
                        "get in touch",
                        "lose touch",
                        "out of touch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние общения или связи между людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связь",
                                "target_lang_sense_clarification": "коммуникация, поддержание отношений"
                            },
                            {
                                "target_lang_word": "контакт",
                                "target_lang_sense_clarification": "взаимодействие"
                            },
                            {
                                "target_lang_word": "общение",
                                "target_lang_sense_clarification": "процесс обмена информацией"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "197ee044-40f4-3459-95a5-06da0b8012ab",
                    "sense_definition": "The physical sense by which one perceives the texture, temperature, or shape of things.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_contact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The material was very soft and smooth to the <w>touch</w>.",
                            "target_lang_translations": {
                                "ru": "Материал был очень мягким и гладким на <w>ощупь</w>."
                            }
                        },
                        {
                            "text": "Blind people often rely on their sense of <w>touch</w> to read Braille.",
                            "target_lang_translations": {
                                "ru": "Слепые люди часто полагаются на <w>осязание</w>, чтобы читать шрифт Брайля."
                            }
                        }
                    ],
                    "synonyms": [
                        "feeling",
                        "tactility"
                    ],
                    "common_phrases": [
                        "sense of touch",
                        "to the touch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическое чувство, с помощью которого воспринимается текстура, температура или форма предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осязание",
                                "target_lang_sense_clarification": "одно из пяти чувств"
                            },
                            {
                                "target_lang_word": "ощупь",
                                "target_lang_sense_clarification": "восприятие через прикосновение (на ощупь)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "550447f0-9756-35d9-b64b-43df149f092d",
                    "sense_definition": "A very small amount or a slight trace of something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chef added just a <w>touch</w> of garlic to the sauce.",
                            "target_lang_translations": {
                                "ru": "Шеф-повар добавил в соус всего лишь <w>чуточку</w> чеснока."
                            }
                        },
                        {
                            "text": "I think I have a <w>touch</w> of the flu coming on.",
                            "target_lang_translations": {
                                "ru": "Думаю, у меня легкий <w>приступ</w> гриппа."
                            }
                        }
                    ],
                    "synonyms": [
                        "bit",
                        "trace",
                        "hint",
                        "dash",
                        "smidgen"
                    ],
                    "antonyms": [
                        "lot"
                    ],
                    "common_phrases": [
                        "a touch of",
                        "nice touch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень небольшое количество или легкий след чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чуточка",
                                "target_lang_sense_clarification": "очень малое количество"
                            },
                            {
                                "target_lang_word": "оттенок",
                                "target_lang_sense_clarification": "незначительная примесь или нюанс"
                            },
                            {
                                "target_lang_word": "налёт",
                                "target_lang_sense_clarification": "слабый след чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6293482d-4755-364f-99a5-83ffa6b2f83d",
                    "sense_definition": "A special skill, ability, or style in doing something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "skill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He used to be a great player, but he seems to have lost his <w>touch</w>.",
                            "target_lang_translations": {
                                "ru": "Раньше он был отличным игроком, но, похоже, потерял <w>хватку</w>."
                            }
                        },
                        {
                            "text": "The decorations added a personal <w>touch</w> to the party.",
                            "target_lang_translations": {
                                "ru": "Украшения добавили вечеринке личный <w>штрих</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "skill",
                        "knack",
                        "flair",
                        "technique"
                    ],
                    "common_phrases": [
                        "lose one's touch",
                        "personal touch",
                        "finishing touch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Особое умение, способность или стиль в выполнении чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мастерство",
                                "target_lang_sense_clarification": "умение делать что-то хорошо"
                            },
                            {
                                "target_lang_word": "штрих",
                                "target_lang_sense_clarification": "характерная деталь или особенность"
                            },
                            {
                                "target_lang_word": "хватка",
                                "target_lang_sense_clarification": "способность или навык (потерять хватку)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b9d2e74-ddb9-3892-915f-120a356882f0",
                    "sense_definition": "The area outside the side boundaries of the field in sports like rugby and soccer.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The player kicked the ball into <w>touch</w> to stop the game.",
                            "target_lang_translations": {
                                "ru": "Игрок выбил мяч в <w>аут</w>, чтобы остановить игру."
                            }
                        },
                        {
                            "text": "The referee signaled that the ball had gone into <w>touch</w>.",
                            "target_lang_translations": {
                                "ru": "Судья просигнализировал, что мяч ушел в <w>аут</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kick into touch",
                        "find touch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зона за боковыми границами поля в таких видах спорта, как регби и футбол."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аут",
                                "target_lang_sense_clarification": "положение вне игры или зона за пределами поля"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f341f7b3-c896-3307-b4e6-c33370223637",
                    "sense_definition": "To prepare food for eating by heating it, often combining it with other ingredients.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_prep",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will <w>cook</w> dinner for the whole family tonight.",
                            "target_lang_translations": {
                                "ru": "Я <w>приготовлю</w> ужин для всей семьи сегодня вечером."
                            }
                        },
                        {
                            "text": "She loves to <w>cook</w> with fresh vegetables from her garden.",
                            "target_lang_translations": {
                                "ru": "Она любит <w>готовить</w> из свежих овощей со своего огорода."
                            }
                        }
                    ],
                    "synonyms": [
                        "prepare",
                        "make",
                        "roast",
                        "bake",
                        "fry"
                    ],
                    "common_phrases": [
                        "cook dinner",
                        "cook a meal",
                        "home-cooked"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовить пищу к употреблению путем нагревания, часто смешивая с другими ингредиентами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "готовить",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "варить",
                                "target_lang_sense_clarification": "готовить посредством кипячения"
                            },
                            {
                                "target_lang_word": "жарить",
                                "target_lang_sense_clarification": "готовить на огне или масле"
                            },
                            {
                                "target_lang_word": "стряпать",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6016bc9-73f0-3586-a88b-2cf800deac1f",
                    "sense_definition": "To be in the process of being prepared by heat.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "food_prep",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soup is <w>cooking</w> slowly on the stove.",
                            "target_lang_translations": {
                                "ru": "Суп медленно <w>варится</w> на плите."
                            }
                        },
                        {
                            "text": "Leave the rice to <w>cook</w> for about twenty minutes.",
                            "target_lang_translations": {
                                "ru": "Оставьте рис <w>вариться</w> примерно на двадцать минут."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться в процессе приготовления под воздействием тепла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "готовиться",
                                "target_lang_sense_clarification": "быть в процессе приготовления"
                            },
                            {
                                "target_lang_word": "вариться",
                                "target_lang_sense_clarification": "кипеть, готовиться в жидкости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae8338fc-7d32-30df-acf4-4c3f626550d4",
                    "sense_definition": "To feel uncomfortably hot.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "temperature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am absolutely <w>cooking</w> in this thick winter jacket.",
                            "target_lang_translations": {
                                "ru": "Я просто <w>варюсь</w> в этой толстой зимней куртке."
                            }
                        },
                        {
                            "text": "It must be <w>cooking</w> inside that car without air conditioning.",
                            "target_lang_translations": {
                                "ru": "В той машине без кондиционера, должно быть, всё <w>жарится</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "boil",
                        "bake",
                        "roast"
                    ],
                    "antonyms": [
                        "freeze"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствовать себя некомфортно жарко, изнывать от жары."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жариться",
                                "target_lang_sense_clarification": "разговорное, страдать от жары"
                            },
                            {
                                "target_lang_word": "вариться",
                                "target_lang_sense_clarification": "разговорное, перегреваться (как в кипятке)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa5f2dc2-29ce-3337-803b-5d865b037989",
                    "sense_definition": "To perform an activity with great skill, energy, or success.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team is really <w>cooking</w> now that they have a new strategy.",
                            "target_lang_translations": {
                                "ru": "Команда сейчас реально <w>жарит</w>, когда у них появилась новая стратегия."
                            }
                        },
                        {
                            "text": "Let him <w>cook</w>; he knows exactly what he is doing.",
                            "target_lang_translations": {
                                "ru": "Пусть <w>творит</w>; он точно знает, что делает."
                            }
                        }
                    ],
                    "common_phrases": [
                        "let him cook"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "modern internet and youth slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять действие с большим мастерством, энергией или успехом (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жарить",
                                "target_lang_sense_clarification": "молодежный сленг, делать что-то энергично и круто"
                            },
                            {
                                "target_lang_word": "жжечь",
                                "target_lang_sense_clarification": "сленг, выступать или действовать ярко"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66f3c2f3-21ef-3d4c-9dc5-e665b766e021",
                    "sense_definition": "To falsify or alter records dishonestly.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "dishonesty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The accountant tried to <w>cook</w> the books to hide the company's losses.",
                            "target_lang_translations": {
                                "ru": "Бухгалтер пытался <w>подделать</w> отчетность, чтобы скрыть убытки компании."
                            }
                        },
                        {
                            "text": "They were caught <w>cooking</w> the financial reports.",
                            "target_lang_translations": {
                                "ru": "Их поймали на том, что они <w>фальсифицировали</w> финансовые отчеты."
                            }
                        }
                    ],
                    "synonyms": [
                        "falsify",
                        "manipulate",
                        "alter"
                    ],
                    "common_phrases": [
                        "cook the books"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "idiomatic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нечестно изменять записи или отчетность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подделывать",
                                "target_lang_sense_clarification": "фальсифицировать документы"
                            },
                            {
                                "target_lang_word": "химичить",
                                "target_lang_sense_clarification": "разговорное, махинировать с данными"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d3d6cfad-36aa-3f41-b51d-caccd301cade",
                    "sense_definition": "A person who prepares food for eating.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My father is a very good <w>cook</w> and makes dinner every Sunday.",
                            "target_lang_translations": {
                                "ru": "Мой отец — очень хороший <w>повар</w> и готовит ужин каждое воскресенье."
                            }
                        },
                        {
                            "text": "The restaurant hired a new <w>cook</w> to help in the kitchen.",
                            "target_lang_translations": {
                                "ru": "Ресторан нанял нового <w>повара</w> в помощь на кухне."
                            }
                        }
                    ],
                    "synonyms": [
                        "chef"
                    ],
                    "common_phrases": [
                        "head cook",
                        "short-order cook"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который готовит еду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повар",
                                "target_lang_sense_clarification": "профессионал или тот, кто готовит"
                            },
                            {
                                "target_lang_word": "кухарка",
                                "target_lang_sense_clarification": "женщина, готовящая пищу (часто в домашнем хозяйстве)"
                            },
                            {

{
    "word_family": [
        "leaver",
        "leaving",
        "leftover"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0775f96b-4716-3fd9-938a-b268fca8ea4b",
                    "sense_definition": "To go away from a place or person.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_depart",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I usually <w>leave</w> for work at eight o'clock in the morning.",
                            "target_lang_translations": {
                                "ru": "Я обычно <w>ухожу</w> на работу в восемь часов утра."
                            }
                        },
                        {
                            "text": "She <w>left</w> the party early because she was tired.",
                            "target_lang_translations": {
                                "ru": "Она рано <w>ушла</w> с вечеринки, потому что устала."
                            }
                        }
                    ],
                    "synonyms": [
                        "depart",
                        "go",
                        "go away",
                        "exit",
                        "set off",
                        "take off"
                    ],
                    "antonyms": [
                        "remain",
                        "tarry"
                    ],
                    "common_phrases": [
                        "leave home",
                        "leave the room",
                        "time to leave"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уходить или уезжать из какого-либо места или от кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уходи́ть",
                                "target_lang_sense_clarification": "общее значение: удаляться пешком или вообще"
                            },
                            {
                                "target_lang_word": "уезжа́ть",
                                "target_lang_sense_clarification": "удаляться, используя транспорт"
                            },
                            {
                                "target_lang_word": "покида́ть",
                                "target_lang_sense_clarification": "оставлять место или человека (более формально)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d850e511-758c-370f-8fbd-4dee3a46f218",
                    "sense_definition": "To cause or allow something to remain in a specific place or condition; to not take something with you.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_remain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>leave</w> the door open when you go out.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>оставь</w> дверь открытой, когда будешь уходить."
                            }
                        },
                        {
                            "text": "I <w>left</w> my keys on the kitchen table.",
                            "target_lang_translations": {
                                "ru": "Я <w>оставил</w> свои ключи на кухонном столе."
                            }
                        }
                    ],
                    "synonyms": [
                        "leave behind"
                    ],
                    "common_phrases": [
                        "leave alone",
                        "leave it",
                        "leave open"
                    ],
                    "target_lang_definitions": {
                        "ru": "Позволять чему-либо оставаться в определенном месте или состоянии; не брать что-либо с собой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставля́ть",
                                "target_lang_sense_clarification": "не брать с собой; сохранять в прежнем положении"
                            },
                            {
                                "target_lang_word": "забыва́ть",
                                "target_lang_sense_clarification": "случайно оставить что-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3585e896-0045-3607-8442-e56b9f5e6107",
                    "sense_definition": "To permanently end one's connection, employment, or relationship with something or someone.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_depart",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He decided to <w>leave</w> his job after finding a better opportunity.",
                            "target_lang_translations": {
                                "ru": "Он решил <w>уйти</w> с работы, найдя лучшую возможность."
                            }
                        },
                        {
                            "text": "She <w>left</w> her husband and moved to another city.",
                            "target_lang_translations": {
                                "ru": "Она <w>ушла</w> от мужа и переехала в другой город."
                            }
                        }
                    ],
                    "synonyms": [
                        "quit",
                        "part",
                        "split"
                    ],
                    "antonyms": [
                        "remain"
                    ],
                    "common_phrases": [
                        "leave a job",
                        "leave school",
                        "leave the band"
                    ],
                    "target_lang_definitions": {
                        "ru": "Окончательно прекращать связь, работу или отношения с чем-либо или кем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уходи́ть",
                                "target_lang_sense_clarification": "увольняться с работы или расставаться с кем-то"
                            },
                            {
                                "target_lang_word": "броса́ть",
                                "target_lang_sense_clarification": "резко прекращать отношения или привычку (разг.)"
                            },
                            {
                                "target_lang_word": "оставля́ть",
                                "target_lang_sense_clarification": "покидать семью, работу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6619cdbc-b7d2-3514-90ae-da670f36eb70",
                    "sense_definition": "To entrust a task, decision, or responsibility to someone else.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_entrust",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will <w>leave</w> the final decision to you.",
                            "target_lang_translations": {
                                "ru": "Я <w>предоставлю</w> окончательное решение вам."
                            }
                        },
                        {
                            "text": "You can <w>leave</w> the details to our expert team.",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>доверить</w> детали нашей команде экспертов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "leave it to someone",
                        "leave a message"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доверять задачу, решение или ответственность кому-либо другому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предоставля́ть",
                                "target_lang_sense_clarification": "давать право или возможность решить"
                            },
                            {
                                "target_lang_word": "оставля́ть",
                                "target_lang_sense_clarification": "передавать на усмотрение другого"
                            },
                            {
                                "target_lang_word": "поруча́ть",
                                "target_lang_sense_clarification": "вверять заботу или выполнение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36c5d347-48a6-32bf-b7e2-b02130b6b2df",
                    "sense_definition": "To result in a specific state, feeling, or condition.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The shocking news <w>left</w> him speechless for a moment.",
                            "target_lang_translations": {
                                "ru": "Шокирующая новость на мгновение <w>оставила</w> его без дара речи."
                            }
                        },
                        {
                            "text": "The heavy rain <w>left</w> the football field muddy and wet.",
                            "target_lang_translations": {
                                "ru": "Сильный дождь <w>оставил</w> футбольное поле грязным и мокрым."
                            }
                        }
                    ],
                    "common_phrases": [
                        "leave someone cold",
                        "leave much to be desired"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить к определенному состоянию, чувству или условию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставля́ть",
                                "target_lang_sense_clarification": "вызывать последствия или состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15e5a78f-ac15-3166-8a22-cd54107219e5",
                    "sense_definition": "To give property or money to someone after death (usually via a will).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_bequeath",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His grandmother <w>left</w> him a valuable painting in her will.",
                            "target_lang_translations": {
                                "ru": "Бабушка <w>оставила</w> ему ценную картину в своем завещании."
                            }
                        },
                        {
                            "text": "She decided to <w>leave</w> all her money to charity.",
                            "target_lang_translations": {
                                "ru": "Она решила <w>оставить</w> все свои деньги на благотворительность."
                            }
                        }
                    ],
                    "common_phrases": [
                        "leave a fortune",
                        "leave to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать имущество или деньги кому-либо после смерти (обычно через завещание)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставля́ть",
                                "target_lang_sense_clarification": "передавать в наследство"
                            },
                            {
                                "target_lang_word": "завеща́ть",
                                "target_lang_sense_clarification": "официально передавать по завещанию"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "40cd3372-4761-3802-8da0-ac35effbb23a",
                    "sense_definition": "Time allowed away from one's work or duty.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_absence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is currently on maternity <w>leave</w> for six months.",
                            "target_lang_translations": {
                                "ru": "В настоящее время она находится в декретном <w>отпуске</w> на шесть месяцев."
                            }
                        },
                        {
                            "text": "I have three days of annual <w>leave</w> remaining this year.",
                            "target_lang_translations": {
                                "ru": "У меня осталось три дня ежегодного <w>отпуска</w> в этом году."
                            }
                        }
                    ],
                    "synonyms": [
                        "time off",
                        "annual leave",
                        "holiday",
                        "vacation",
                        "break",
                        "leave of absence"
                    ],
                    "common_phrases": [
                        "annual leave",
                        "sick leave",
                        "maternity leave",
                        "on leave",
                        "paid leave"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрешенное время отсутствия на работе или службе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "о́тпуск",
                                "target_lang_sense_clarification": "ежегодный отдых или декрет"
                            },
                            {
                                "target_lang_word": "отгу́л",
                                "target_lang_sense_clarification": "кратковременное отсутствие в счет отработки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57e75934-2ec9-336f-ae5d-2809ecf4e2bb",
                    "sense_definition": "Formal permission to do something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers were granted <w>leave</w> to enter the city.",
                            "target_lang_translations": {
                                "ru": "Солдатам было дано <w>разрешение</w> войти в город."
                            }
                        },
                        {
                            "text": "He asked for <w>leave</w> to speak during the meeting.",
                            "target_lang_translations": {
                                "ru": "Он попросил <w>разрешения</w> говорить во время собрания."
                            }
                        }
                    ],
                    "synonyms": [
                        "permission",
                        "consent",
                        "authorisation"
                    ],
                    "common_phrases": [
                        "grant leave",
                        "beg leave",
                        "by your leave"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное разрешение сделать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разреше́ние",
                                "target_lang_sense_clarification": "право на совершение действия"
                            },
                            {
                                "target_lang_word": "позволе́ние",
                                "target_lang_sense_clarification": "согласие (устар./форм.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1e6079d-0618-3c77-ac42-48ece927c0d9",
                    "sense_definition": "The act of saying goodbye or departing.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "noun_departure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The guests took their <w>leave</w> after a wonderful dinner.",
                            "target_lang_translations": {
                                "ru": "Гости <w>простились</w> после чудесного ужина."
                            }
                        },
                        {
                            "text": "He took his <w>leave</w> of the king and exited the hall.",
                            "target_lang_translations": {
                                "ru": "Он <w>простился</w> с королем и вышел из зала."
                            }
                        }
                    ],
                    "synonyms": [
                        "departure"
                    ],
                    "common_phrases": [
                        "take leave",
                        "take one's leave"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal or literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт прощания или ухода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проща́ние",
                                "target_lang_sense_clarification": "действие при расставании"
                            },
                            {
                                "target_lang_word": "ухо́д",
                                "target_lang_sense_clarification": "удаление откуда-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a79179bc-d073-3cf8-994d-b268e6c2fdfd",
                    "sense_definition": "Niewielki oddział żołnierzy, policjantów lub innych funkcjonariuszy wyznaczony do pilnowania bezpieczeństwa, rozpoznania terenu lub kontroli.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na ulicy pojawił się policyjny <w>patrol</w>, więc wszyscy przechodnie poczuli się bezpieczniej.",
                            "target_lang_translations": {
                                "ru": "На улице появился полицейский <w>патруль</w>, поэтому все прохожие почувствовали себя безопаснее."
                            }
                        },
                        {
                            "text": "Niemiecki <w>patrol</w> zatrzymał oddział partyzancki do rutynowej kontroli dokumentów.",
                            "target_lang_translations": {
                                "ru": "Немецкий <w>патруль</w> остановил партизанский отряд для плановой проверки документов."
                            }
                        }
                    ],
                    "synonyms": [
                        "warta",
                        "straż"
                    ],
                    "common_phrases": [
                        "patrol policyjny",
                        "patrol wojskowy",
                        "pieszy patrol",
                        "patrol drogowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой отряд солдат, полицейских или других служащих, назначенный для охраны безопасности, разведки местности или контроля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "патруль",
                                "target_lang_sense_clarification": "группа людей"
                            },
                            {
                                "target_lang_word": "дозор",
                                "target_lang_sense_clarification": "военный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12de692f-e8d8-30eb-9608-011808fe6556",
                    "sense_definition": "Czynność polegająca na obchodzeniu lub objeżdżaniu określonego terenu w celu jego pilnowania i sprawdzania bezpieczeństwa.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zaraz strażnicy wrócą z <w>patrolu</w> i złożą raport dowódcy.",
                            "target_lang_translations": {
                                "ru": "Скоро охранники вернутся из <w>патруля</w> и доложат командиру."
                            }
                        },
                        {
                            "text": "Żołnierze wyruszyli na nocny <w>patrol</w> wzdłuż granicy państwa.",
                            "target_lang_translations": {
                                "ru": "Солдаты отправились в ночной <w>патруль</w> вдоль государственной границы."
                            }
                        }
                    ],
                    "synonyms": [
                        "obchód",
                        "zwiad",
                        "rekonesans"
                    ],
                    "common_phrases": [
                        "wyjść na patrol",
                        "wrócić z patrolu",
                        "rutynowy patrol",
                        "nocny patrol"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, заключающееся в обходе или объезде определенной территории с целью её охраны и проверки безопасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "патрулирование",
                                "target_lang_sense_clarification": "процесс/действие"
                            },
                            {
                                "target_lang_word": "патруль",
                                "target_lang_sense_clarification": "служебное задание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b962af06-5619-39be-b823-7076092a0473",
                    "sense_definition": "W harcerstwie: kilkuosobowy zespół harcerzy utworzony doraźnie do wykonania konkretnego zadania.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz <w>patrol</w> wygrał zawody w biegu na orientację podczas letniego obozu.",
                            "target_lang_translations": {
                                "ru": "Наш <w>патруль</w> выиграл соревнования по спортивному ориентированию во время летнего лагеря."
                            }
                        },
                        {
                            "text": "Zastępowy podzielił drużynę na dwa mniejsze <w>patrole</w>, aby szybciej wykonać zadanie.",
                            "target_lang_translations": {
                                "ru": "Командир звена разделил команду на два меньших <w>патруля</w>, чтобы быстрее выполнить задание."
                            }
                        }
                    ],
                    "common_phrases": [
                        "patrol harcerski",
                        "patrol medyczny"
                    ],
                    "target_lang_definitions": {
                        "ru": "В скаутском движении: группа из нескольких скаутов, созданная временно для выполнения конкретной задачи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "патруль",
                                "target_lang_sense_clarification": "группа скаутов"
                            },
                            {
                                "target_lang_word": "звено",
                                "target_lang_sense_clarification": "организационная единица"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
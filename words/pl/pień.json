    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8614d7f2-a0c2-3032-b6e4-c8eba65d64d1",
                    "sense_definition": "Zdrewniała łodyga drzewa, nadziemna część drzewa bez gałęzi.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "tree_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Drzewo to wieloletnia roślina składająca się z korzeni, <w>pnia</w> i korony.",
                            "target_lang_translations": {
                                "ru": "Дерево — это многолетнее растение, состоящее из корней, <w>ствола</w> и кроны."
                            }
                        },
                        {
                            "text": "Oparł się plecami o szorstki <w>pień</w> starego dębu.",
                            "target_lang_translations": {
                                "ru": "Он прислонился спиной к шершавому <w>стволу</w> старого дуба."
                            }
                        }
                    ],
                    "common_phrases": [
                        "głuchy jak pień",
                        "spać jak pień",
                        "zdrowy jak pień"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одеревеневший стебель дерева, надземная часть дерева без ветвей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ствол",
                                "target_lang_sense_clarification": "часть дерева"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8bc696eb-4d7b-3075-8e46-7910622c84c6",
                    "sense_definition": "Klocek drewna służący jako podstawa do rąbania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "wood_block",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Postawił kawałek drewna na <w>pniu</w> i zamachnął się siekierą.",
                            "target_lang_translations": {
                                "ru": "Он поставил кусок дерева на <w>пень</w> и замахнулся топором."
                            }
                        },
                        {
                            "text": "W szopie stał stary <w>pień</w>, na którym rąbaliśmy drewno do kominka.",
                            "target_lang_translations": {
                                "ru": "В сарае стоял старый <w>пень</w>, на котором мы рубили дрова для камина."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyciąć w pień"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кусок дерева, служащий основанием для рубки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пень",
                                "target_lang_sense_clarification": "основание для рубки"
                            },
                            {
                                "target_lang_word": "колода",
                                "target_lang_sense_clarification": "толстое бревно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c3a6f25-83bf-371d-9dd4-929222fbda5a",
                    "sense_definition": "Zasiedlona rodzina pszczela, ul wraz ze znajdującymi się w nim pszczołami.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "bees",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pszczelarz posiadał w swojej pasiece dwadzieścia <w>pni</w>.",
                            "target_lang_translations": {
                                "ru": "У пчеловода на пасеке было двадцать <w>ульев</w>."
                            }
                        },
                        {
                            "text": "Zimą musimy szczególnie zadbać o każdy pszczeli <w>pień</w>.",
                            "target_lang_translations": {
                                "ru": "Зимой мы должны особенно заботиться о каждой пчелиной <w>семье</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rodzina pszczela"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пчелиная семья, улей вместе с находящимися в нем пчелами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пчелиная семья",
                                "target_lang_sense_clarification": "сообщество пчел"
                            },
                            {
                                "target_lang_word": "улей",
                                "target_lang_sense_clarification": "домик с пчелами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8b77688-c3cd-37d6-bc10-9c99e107f3a2",
                    "sense_definition": "Dopełniacz liczby mnogiej rzeczownika <w>pienie</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "form_pienie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W opustoszałym kościele nie było słychać żadnych nabożnych <w>pień</w>.",
                            "target_lang_translations": {
                                "ru": "В опустевшем костеле не было слышно никаких набожных <w>песнопений</w>."
                            }
                        },
                        {
                            "text": "Zbiór dawnych <w>pień</w> leżał zakurzony na półce.",
                            "target_lang_translations": {
                                "ru": "Сборник старинных <w>песнопений</w> лежал запыленный на полке."
                            }
                        }
                    ],
                    "synonyms": [
                        "pieni"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Родительный падеж множественного числа существительного <w>pienie</w> (пение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "песнопение",
                                "target_lang_sense_clarification": "церковное пение"
                            },
                            {
                                "target_lang_word": "пение",
                                "target_lang_sense_clarification": "вокальное исполнение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f37d0bd3-b00f-3ed4-9909-ae3f23fd4648",
                    "sense_definition": "Całkowity, zupełny, obejmujący wszystko; nieograniczony żadnymi wyjątkami.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "completeness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W bibliotece panowała <w>absolutna</w> cisza, która pozwalała na skupienie.",
                            "target_lang_translations": {
                                "ru": "В библиотеке царила <w>абсолютная</w> тишина, которая позволяла сосредоточиться."
                            }
                        },
                        {
                            "text": "To, co on mówi, to <w>absolutna</w> bzdura i nie warto tego słuchać.",
                            "target_lang_translations": {
                                "ru": "То, что он говорит, — это <w>абсолютная</w> чушь, и не стоит этого слушать."
                            }
                        }
                    ],
                    "synonyms": [
                        "całkowity",
                        "zupełny",
                        "kompletny",
                        "totalny",
                        "pełny",
                        "bezwyjątkowy"
                    ],
                    "antonyms": [
                        "niezupełny",
                        "niepełny",
                        "niekompletny",
                        "niecałkowity",
                        "względny"
                    ],
                    "common_phrases": [
                        "absolutna cisza",
                        "absolutna większość",
                        "absolutny spokój",
                        "absolutny brak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полный, всеобъемлющий, совершенный; не ограниченный никакими исключениями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "абсолютный",
                                "target_lang_sense_clarification": "полный, безусловный"
                            },
                            {
                                "target_lang_word": "полный",
                                "target_lang_sense_clarification": "целостный, исчерпывающий"
                            },
                            {
                                "target_lang_word": "совершенный",
                                "target_lang_sense_clarification": "достигший высшей степени (часто о качествах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0c61cd6-d4ee-3dc9-ac78-c11683c9d716",
                    "sense_definition": "Niepodlegający dyskusji, pewny i niewątpliwy; taki, którego nie można zakwestionować.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "completeness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój przyjaciel ma <w>absolutną</w> rację w tej sprawie.",
                            "target_lang_translations": {
                                "ru": "Мой друг <w>абсолютно</w> прав в этом вопросе."
                            }
                        },
                        {
                            "text": "Muszę mieć <w>absolutną</w> pewność, że zamknąłem drzwi na klucz.",
                            "target_lang_translations": {
                                "ru": "Я должен иметь <w>абсолютную</w> уверенность, что запер дверь на ключ."
                            }
                        }
                    ],
                    "synonyms": [
                        "niewątpliwy",
                        "niepodważalny",
                        "bezdyskusyjny",
                        "pełny"
                    ],
                    "antonyms": [
                        "względny"
                    ],
                    "common_phrases": [
                        "absolutna racja",
                        "absolutna pewność",
                        "absolutny priorytet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не подлежащий обсуждению, несомненный и верный; такой, который нельзя оспорить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "абсолютный",
                                "target_lang_sense_clarification": "несомненный, бесспорный"
                            },
                            {
                                "target_lang_word": "безусловный",
                                "target_lang_sense_clarification": "неоспоримый, стопроцентный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e1d7562-c776-32b2-9fc1-14e8232e2742",
                    "sense_definition": "Dotyczący formy rządów, w której władca posiada nieograniczoną władzę nad państwem i obywatelami.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "political",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ludwik XIV był najsłynniejszym władcą <w>absolutnym</w> we Francji.",
                            "target_lang_translations": {
                                "ru": "Людовик XIV был самым известным <w>абсолютным</w> правителем во Франции."
                            }
                        },
                        {
                            "text": "Wiele państw odeszło od monarchii <w>absolutnej</w> na rzecz demokracji.",
                            "target_lang_translations": {
                                "ru": "Многие государства отошли от <w>абсолютной</w> монархии в пользу демократии."
                            }
                        }
                    ],
                    "synonyms": [
                        "despotyczny",
                        "autokratyczny",
                        "samowładny",
                        "nieograniczony",
                        "wszechwładny",
                        "dyktatorski"
                    ],
                    "antonyms": [
                        "demokratyczny",
                        "konstytucyjny",
                        "parlamentarny"
                    ],
                    "common_phrases": [
                        "władca absolutny",
                        "monarchia absolutna",
                        "rządy absolutne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к форме правления, при которой правитель обладает неограниченной властью над государством и гражданами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "абсолютный",
                                "target_lang_sense_clarification": "неограниченный, самодержавный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea401df5-1efb-3a12-9fbd-95e95800c663",
                    "sense_definition": "Istniejący samoistnie i obiektywnie, niezależnie od jakichkolwiek warunków, relacji czy punktu odniesienia.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "objective",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Filozofowie od wieków spierają się o istnienie prawdy <w>absolutnej</w>.",
                            "target_lang_translations": {
                                "ru": "Философы веками спорят о существовании <w>абсолютной</w> истины."
                            }
                        },
                        {
                            "text": "W świecie fizyki czas nie jest wartością <w>absolutną</w>, lecz względną.",
                            "target_lang_translations": {
                                "ru": "В мире физики время является не <w>абсолютной</w> величиной, а относительной."
                            }
                        }
                    ],
                    "synonyms": [
                        "bezwzględny"
                    ],
                    "antonyms": [
                        "względny",
                        "relatywny",
                        "uwarunkowany"
                    ],
                    "common_phrases": [
                        "prawda absolutna",
                        "wartość absolutna",
                        "słuch absolutny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Существующий сам по себе и объективно, независимо от каких-либо условий, отношений или точки отсчета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "абсолютный",
                                "target_lang_sense_clarification": "безотносительный, объективный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "540aadf3-d87a-34d7-81ec-1a512e545023",
                    "sense_definition": "W fizyce: odnoszący się do skali temperatury liczonej od zera bezwzględnego (skali Kelvina).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "objective",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zero <w>absolutne</w> to teoretycznie najniższa możliwa temperatura we wszechświecie.",
                            "target_lang_translations": {
                                "ru": "<w>Абсолютный</w> нуль — это теоретически самая низкая возможная температура во вселенной."
                            }
                        },
                        {
                            "text": "W laboratoriach naukowcy próbują zbliżyć się do zera <w>absolutnego</w>.",
                            "target_lang_translations": {
                                "ru": "В лабораториях ученые пытаются приблизиться к <w>абсолютному</w> нулю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zero absolutne",
                        "temperatura absolutna"
                    ],
                    "target_lang_definitions": {
                        "ru": "В физике: относящийся к температурной шкале, отсчитываемой от абсолютного нуля (шкале Кельвина)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "абсолютный",
                                "target_lang_sense_clarification": "физический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
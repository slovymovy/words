                {
                    "sense_id": "b63828c1-cb31-36b5-9dc8-b3cf5deb6654",
                    "sense_definition": "Met een hek, schutting of haag omsluiten.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "enclosure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer besloot het weiland met een stevig hek te <w>omtuinen</w>.",
                            "target_lang_translations": {
                                "en": "The farmer decided to <w>fence in</w> the pasture with a sturdy fence.",
                                "ru": "Фермер решил <w>огородить</w> пастбище прочным забором."
                            }
                        },
                        {
                            "text": "Vroeger moest men een moestuin goed <w>omtuinen</w> om dieren buiten te houden.",
                            "target_lang_translations": {
                                "en": "In the past, one had to <w>enclose</w> a vegetable garden well to keep animals out.",
                                "ru": "Раньше приходилось тщательно <w>огораживать</w> огород, чтобы не допускать туда животных."
                            }
                        }
                    ],
                    "synonyms": [
                        "omheinen",
                        "omrasteren",
                        "omsluiten"
                    ],
                    "antonyms": [
                        "openlaten"
                    ],
                    "common_phrases": [
                        "een stuk land omtuinen",
                        "de tuin omtuinen"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "In modern Dutch 'omheinen' is the standard term."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To enclose with a fence, screen, or hedge.",
                        "ru": "Окружать забором, изгородью или живой изгородью."
                    },
                    "translations": {
                        "en": [
                            {

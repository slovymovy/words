    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e90c3a34-a750-35b9-939e-cc90b7cc28e8",
                    "sense_definition": "Onbeschreven, niet ingevuld of leeg.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "empty_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij pakte een <w>blanco</w> vel papier om een nieuwe tekening te maken.",
                            "target_lang_translations": {
                                "en": "He grabbed a <w>blank</w> sheet of paper to make a new drawing.",
                                "ru": "Он взял <w>чистый</w> лист бумаги, чтобы сделать новый рисунок."
                            }
                        },
                        {
                            "text": "De cheque was nog <w>blanco</w>, dus hij kon elk gewenst bedrag invullen.",
                            "target_lang_translations": {
                                "en": "The cheque was still <w>blank</w>, so he could fill in any desired amount.",
                                "ru": "Чек был еще <w>незаполненным</w>, поэтому он мог вписать любую сумму."
                            }
                        }
                    ],
                    "synonyms": [
                        "leeg",
                        "onbeschreven"
                    ],
                    "antonyms": [
                        "ingevuld",
                        "beschreven"
                    ],
                    "common_phrases": [
                        "blanco papier",
                        "blanco cheque",
                        "blanco strafblad"
                    ],
                    "target_lang_definitions": {
                        "en": "Not written on, unfilled, or empty.",
                        "ru": "Неисписанный, незаполненный или пустой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blank",
                                "target_lang_sense_clarification": "regarding paper or documents"
                            },
                            {
                                "target_lang_word": "empty",
                                "target_lang_sense_clarification": "containing nothing"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "чистый",
                                "target_lang_sense_clarification": "о бумаге или поверхности"
                            },
                            {
                                "target_lang_word": "незаполненный",
                                "target_lang_sense_clarification": "о документах или чеках"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb49794d-bc3c-3d94-ad16-90c93cef03be",
                    "sense_definition": "Zonder een keuze te maken voor een specifieke kandidaat of optie bij een stemming.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "voting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Veel kiezers besloten uit protest <w>blanco</w> te stemmen tijdens de verkiezingen.",
                            "target_lang_translations": {
                                "en": "Many voters decided to vote <w>blank</w> out of protest during the elections.",
                                "ru": "Многие избиратели решили в знак протеста проголосовать <w>пустым</w> бюллетенем во время выборов."
                            }
                        },
                        {
                            "text": "Een <w>blanco</w> stem telt wel mee voor de opkomst, maar niet voor de zetelverdeling.",
                            "target_lang_translations": {
                                "en": "A <w>blank</w> vote counts towards the turnout, but not for the seat distribution.",
                                "ru": "<w>Пустой</w> бюллетень учитывается при подсчете явки, но не при распределении мест."
                            }
                        }
                    ],
                    "common_phrases": [
                        "blanco stemmen",
                        "blanco stem"
                    ],
                    "target_lang_definitions": {
                        "en": "Casting a vote without selecting a specific candidate or option.",
                        "ru": "Голосование без выбора конкретного кандидата или варианта."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blank",
                                "target_lang_sense_clarification": "in the context of voting"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пустой",
                                "target_lang_sense_clarification": "о бюллетене при голосовании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd6d7ab6-7e4a-3b7f-ab84-78aed3cc43bb",
                    "sense_definition": "Zonder vooroordelen, voorkennis of eerdere invloeden; helemaal opnieuw beginnend.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De nieuwe manager wil er volledig <w>blanco</w> instaan en zijn eigen oordeel vormen.",
                            "target_lang_translations": {
                                "en": "The new manager wants to go in completely <w>open-minded</w> and form his own judgment.",
                                "ru": "Новый менеджер хочет подойти к делу совершенно <w>непредвзято</w> и составить собственное мнение."
                            }
                        },
                        {
                            "text": "Na de mislukte onderhandelingen moeten we het overleg weer <w>blanco</w> beginnen.",
                            "target_lang_translations": {
                                "en": "After the failed negotiations, we have to start the consultation <w>fresh</w> again.",
                                "ru": "После неудачных переговоров мы должны начать обсуждение снова <w>с чистого листа</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "onbevooroordeeld",
                        "neutraal",
                        "onbevangen"
                    ],
                    "antonyms": [
                        "bevooroordeeld"
                    ],
                    "common_phrases": [
                        "er blanco instaan",
                        "blanco beginnen"
                    ],
                    "target_lang_definitions": {
                        "en": "Without prejudice, prior knowledge, or previous influences; starting fresh.",
                        "ru": "Без предубеждений, предварительных знаний или влияния прошлого; начинающий с чистого листа."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "open-minded",
                                "target_lang_sense_clarification": "without preconceptions"
                            },
                            {
                                "target_lang_word": "fresh",
                                "target_lang_sense_clarification": "starting anew without baggage"
                            },
                            {
                                "target_lang_word": "unbiased",
                                "target_lang_sense_clarification": "neutral attitude"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "непредвзятый",
                                "target_lang_sense_clarification": "об отношении или мнении"
                            },
                            {
                                "target_lang_word": "с чистого листа",
                                "target_lang_sense_clarification": "начать заново"
                            }
                        ]
                    }
                }
            ]
        }
    ]

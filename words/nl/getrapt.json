                    "target_lang_definitions": {
                        "en": "Provided with steps or having a shape resembling a staircase.",
                        "ru": "Имеющий форму лестницы или ступеней."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stepped",
                                "target_lang_sense_clarification": "having a stair-like shape"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ступенчатый",
                                "target_lang_sense_clarification": "напоминающий ступени"
                            }
                        ]
                    }
                }
            ]
        }
    ]

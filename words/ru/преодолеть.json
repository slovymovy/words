            "senses": [
                {
                    "sense_id": "200c9bef-929d-36cb-a6b3-dc66df793382",
                    "sense_definition": "Справиться с чем-либо, осилить какое-либо препятствие, трудность или сильное чувство.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студент смог <преодолеть> лень и успешно сдать все экзамены.",
                            "target_lang_translations": {
                                "en": "The student was able to <overcome> laziness and successfully pass all the exams."
                            }
                        },
                        {
                            "text": "Вам нужно <преодолеть> страх перед публичными выступлениями, чтобы стать лидером.",
                            "target_lang_translations": {
                                "en": "You need to <conquer> your fear of public speaking to become a leader."
                            }
                        }
                    ],
                    "synonyms": [
                        "пересилить",
                        "справиться",
                        "побороть",
                        "осилить"
                    ],
                    "antonyms": [
                        "сдаться",
                        "уступить",
                        "спасовать"
                    ],
                    "common_phrases": [

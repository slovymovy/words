{
    "word_family": [
        "опера",
        "оперетта"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fcc16c5b-9837-3943-8fdb-dde71111bf0c",
                    "sense_definition": "Относящийся к опере — музыкально-драматическому произведению или театру, в котором ставятся такие представления.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы купили билеты в <w>оперный</w> театр на премьеру нового спектакля.",
                            "target_lang_translations": {
                                "en": "We bought tickets to the <w>opera</w> house for the premiere of a new performance."
                            }
                        },
                        {
                            "text": "Известный <w>оперный</w> певец исполнил свою самую знаменитую арию.",
                            "target_lang_translations": {
                                "en": "A famous <w>opera</w> singer performed his most famous aria."
                            }
                        }
                    ],
                    "common_phrases": [
                        "оперный театр",
                        "оперный певец",
                        "оперная музыка",
                        "оперная ария",
                        "оперное искусство"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to opera — a musical-dramatic work or the theater where such performances are staged."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "opera",
                                "target_lang_sense_clarification": "used attributively (e.g., opera house, opera singer)"
                            },
                            {
                                "target_lang_word": "operatic",
                                "target_lang_sense_clarification": "pertaining to the art form or style"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b711b078-65c4-3fe7-8f43-f542cb596d49",
                    "sense_definition": "Напоминающий оперу своей неестественностью, напыщенностью или преувеличенным драматизмом.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его <w>оперный</w> жест вызвал у присутствующих лишь ироничную улыбку.",
                            "target_lang_translations": {
                                "en": "His <w>operatic</w> gesture elicited only an ironic smile from those present."
                            }
                        },
                        {
                            "text": "В её голосе звучал какой-то <w>оперный</w> трагизм, совершенно неуместный в этой простой ситуации.",
                            "target_lang_translations": {
                                "en": "There was a certain <w>operatic</w> tragedy in her voice, completely inappropriate for this simple situation."
                            }
                        }
                    ],
                    "synonyms": [
                        "театральный",
                        "напыщенный",
                        "картинный"
                    ],
                    "antonyms": [
                        "естественный",
                        "простой"
                    ],
                    "common_phrases": [
                        "оперные страсти",
                        "оперный жест",
                        "оперная поза"
                    ],
                    "target_lang_definitions": {
                        "en": "Resembling opera in its unnaturalness, pomposity, or exaggerated drama."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "operatic",
                                "target_lang_sense_clarification": "exaggeratedly dramatic or grand"
                            },
                            {
                                "target_lang_word": "theatrical",
                                "target_lang_sense_clarification": "artificial or pompous"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    ],
                    "target_lang_definitions": {
                        "ru": "Метод нанесения жидкости для электронных сигарет непосредственно на нагревательный элемент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дриппинг",
                                "target_lang_sense_clarification": "вейпинг"
                            },
                            {
                                "target_lang_word": "закапывание",
                                "target_lang_sense_clarification": "процесс"
                            }
                        ]
                    }
                }
            ]
        }
    ]

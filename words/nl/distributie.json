{
    "word_family": [
        "distribueren",
        "distributeur",
        "distributief"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5042abcf-f31c-35e6-b8d6-49752a2f5eb9",
                    "sense_definition": "Het proces van het verdelen en leveren van goederen aan winkels, klanten of andere afnemers.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "logistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf heeft de <w>distributie</w> van hun producten uitbesteed aan een logistieke partner.",
                            "target_lang_translations": {
                                "ru": "Компания передала <w>дистрибуцию</w> своей продукции логистическому партнеру."
                            }
                        },
                        {
                            "text": "Door een staking in het transportcentrum liep de <w>distributie</w> ernstige vertraging op.",
                            "target_lang_translations": {
                                "ru": "Из-за забастовки в транспортном центре <w>дистрибуция</w> серьезно задержалась."
                            }
                        }
                    ],
                    "synonyms": [
                        "verspreiding",
                        "verdeling"
                    ],
                    "common_phrases": [
                        "logistieke distributie",
                        "distributie van goederen",
                        "wereldwijde distributie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс распределения и доставки товаров в магазины, клиентам или другим потребителям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дистрибуция",
                                "target_lang_sense_clarification": "логистика и бизнес"
                            },
                            {
                                "target_lang_word": "распределение",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "сбыт",
                                "target_lang_sense_clarification": "торговля"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "187507ef-6688-3f42-857f-30f2e836a8ee",
                    "sense_definition": "Het systeem in een verbrandingsmotor dat zorgt voor de aansturing van de kleppen (vaak via een riem of ketting).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "automotive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij deze auto moet de <w>distributie</w> na honderdduizend kilometer worden vervangen.",
                            "target_lang_translations": {
                                "ru": "В этом автомобиле <w>ГРМ</w> необходимо менять через сто тысяч километров."
                            }
                        },
                        {
                            "text": "Als de <w>distributie</w> breekt, kan dit leiden tot zware schade aan de motor.",
                            "target_lang_translations": {
                                "ru": "Если <w>ГРМ</w> оборвется, это может привести к серьезному повреждению двигателя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "distributie vervangen",
                        "riem van de distributie"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Система в двигателе внутреннего сгорания, управляющая работой клапанов (газораспределительный механизм)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "газораспределительный механизм",
                                "target_lang_sense_clarification": "технический термин"
                            },
                            {
                                "target_lang_word": "ГРМ",
                                "target_lang_sense_clarification": "аббревиатура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e2928ff-8f39-38e0-8e87-a5573cb2496b",
                    "sense_definition": "De door de overheid gecontroleerde toewijzing van schaarse goederen, vaak met behulp van bonnen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "rationing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de oorlog waren suiker en koffie alleen verkrijgbaar via de <w>distributie</w>.",
                            "target_lang_translations": {
                                "ru": "Во время войны сахар и кофе можно было получить только через <w>распределение</w>."
                            }
                        },
                        {
                            "text": "Mijn grootouders vertelden vaak verhalen over de schaarste en de <w>distributie</w> in de jaren veertig.",
                            "target_lang_translations": {
                                "ru": "Мои бабушка и дедушка часто рассказывали истории о дефиците и <w>распределении</w> в сороковые годы."
                            }
                        }
                    ],
                    "synonyms": [
                        "rantsoenering"
                    ],
                    "common_phrases": [
                        "op de distributie",
                        "via de distributie"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Контролируемое государством распределение дефицитных товаров, часто с использованием талонов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нормированное распределение",
                                "target_lang_sense_clarification": "исторический контекст"
                            },
                            {
                                "target_lang_word": "карточная система",
                                "target_lang_sense_clarification": "система талонов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "baf673db-55ff-3654-a2bf-efb5182e050f",
                    "sense_definition": "De overheidsdienst of instantie die verantwoordelijk is voor de rantsoenering van goederen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "rationing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij werkte tijdens de bezetting als ambtenaar bij de <w>distributie</w>.",
                            "target_lang_translations": {
                                "ru": "Во время оккупации он работал чиновником в <w>службе распределения</w>."
                            }
                        },
                        {
                            "text": "De <w>distributie</w> gaf nieuwe stamkaarten uit aan de bevolking.",
                            "target_lang_translations": {
                                "ru": "<w>Служба распределения</w> выдала населению новые учетные карточки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Государственная служба или учреждение, ответственное за нормированное распределение товаров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "служба распределения",
                                "target_lang_sense_clarification": "учреждение"
                            },
                            {
                                "target_lang_word": "пункт распределения",
                                "target_lang_sense_clarification": "место выдачи"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1fdeed55-e3be-3828-95e5-1e3c6317f11e",
                    "sense_definition": "Относящийся к спортивной или рабочей команде; выполняемый сообща, коллективно.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Футбол — это популярный <w>командный</w> вид спорта.",
                            "target_lang_translations": {
                                "en": "Football is a popular <w>team</w> sport."
                            }
                        },
                        {
                            "text": "Для успеха нашего проекта очень важен <w>командный</w> дух.",
                            "target_lang_translations": {
                                "en": "<w>Team</w> spirit is very important for the success of our project."
                            }
                        }
                    ],
                    "antonyms": [
                        "индивидуальный",
                        "личный"
                    ],
                    "common_phrases": [
                        "командный дух",
                        "командная игра",
                        "командная работа",
                        "командный спорт"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to a sports or work team; performed jointly, collectively."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "team",
                                "target_lang_sense_clarification": "relating to a group working together"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c29c7617-315c-3f91-a4b8-0d32d9260efd",
                    "sense_definition": "Относящийся к командованию, командирам или осуществлению власти; выражающий властность.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Офицер отдал распоряжение резким <w>командным</w> голосом.",
                            "target_lang_translations": {
                                "en": "The officer gave the order in a sharp <w>commanding</w> voice."
                            }
                        },
                        {
                            "text": "Весь <w>командный</w> состав собрался в штабе для обсуждения плана.",
                            "target_lang_translations": {
                                "en": "The entire <w>command</w> staff gathered at headquarters to discuss the plan."
                            }
                        }
                    ],
                    "synonyms": [
                        "командирский",
                        "приказной"
                    ],
                    "common_phrases": [
                        "командный голос",
                        "командный пункт",
                        "командный состав",
                        "командный тон"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to command, commanders, or the exercise of power; expressing authority."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "commanding",
                                "target_lang_sense_clarification": "expressing authority"
                            },
                            {
                                "target_lang_word": "command",
                                "target_lang_sense_clarification": "relating to military leadership"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a435cb2-e335-3e28-a797-06cb167f378e",
                    "sense_definition": "Относящийся к набору инструкций для управления компьютером или программой.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Программист ввёл необходимый код в <w>командную</w> строку.",
                            "target_lang_translations": {
                                "en": "The programmer entered the necessary code into the <w>command</w> line."
                            }
                        },
                        {
                            "text": "Этот <w>командный</w> файл автоматически запускает обновление системы.",
                            "target_lang_translations": {
                                "en": "This <w>command</w> file automatically starts the system update."
                            }
                        }
                    ],
                    "common_phrases": [
                        "командная строка",
                        "командный файл",
                        "командная оболочка",
                        "командный интерфейс"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to a set of instructions for controlling a computer or program."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "command",
                                "target_lang_sense_clarification": "computing context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70a69423-ddca-3d37-9e04-c11d714d0b8d",
                    "sense_definition": "Господствующий над окружающей местностью, дающий преимущество в обзоре (обычно о высоте).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наши войска успешно заняли важную <w>командную</w> высоту.",
                            "target_lang_translations": {
                                "en": "Our troops successfully occupied an important <w>commanding</w> height."
                            }
                        },
                        {
                            "text": "С этой <w>командной</w> позиции отлично просматривается вся долина.",
                            "target_lang_translations": {
                                "en": "From this <w>commanding</w> position, the entire valley is clearly visible."
                            }
                        }
                    ],
                    "synonyms": [
                        "господствующий",
                        "первенствующий"
                    ],
                    "antonyms": [
                        "второстепенный"
                    ],
                    "common_phrases": [
                        "командная высота",
                        "командная позиция"
                    ],
                    "target_lang_definitions": {
                        "en": "Dominating the surrounding terrain, providing an advantage in observation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "commanding",
                                "target_lang_sense_clarification": "dominating a landscape"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
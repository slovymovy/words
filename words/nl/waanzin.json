    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "928de971-b9f3-3f17-aa92-f8f0722ea237",
                    "sense_definition": "Iets dat volkomen onlogisch, onverstandig of absurd is.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is pure <w>waanzin</w> om met dit slechte weer te gaan varen.",
                            "target_lang_translations": {
                                "ru": "Отправляться в плавание в такую плохую погоду — это чистое <w>безумие</w>.",
                                "en": "It is pure <w>madness</w> to go sailing in this bad weather."
                            }
                        },
                        {
                            "text": "De Mount Everest beklimmen zonder enige voorbereiding is <w>waanzin</w>.",
                            "target_lang_translations": {
                                "ru": "Восхождение на Эверест без какой-либо подготовки — это <w>сумасшествие</w>.",
                                "en": "Climbing Mount Everest without any preparation is <w>insanity</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gekkenwerk",
                        "dwaasheid",
                        "onzin"
                    ],
                    "antonyms": [
                        "redelijkheid",
                        "wijsheid"
                    ],
                    "common_phrases": [
                        "pure waanzin",
                        "je reinste waanzin",
                        "complete waanzin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то совершенно нелогичное, неразумное или абсурдное.",
                        "en": "Something that is completely illogical, unwise, or absurd."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безумие",
                                "target_lang_sense_clarification": "абсурдный поступок или ситуация"
                            },
                            {
                                "target_lang_word": "сумасшествие",
                                "target_lang_sense_clarification": "крайняя степень неразумности"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "madness",
                                "target_lang_sense_clarification": "extreme folly or foolishness"
                            },
                            {
                                "target_lang_word": "insanity",
                                "target_lang_sense_clarification": "complete senselessness"
                            },
                            {
                                "target_lang_word": "lunacy",
                                "target_lang_sense_clarification": "wild absurdity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e64ff7de-3960-3876-bb12-75db5464111d",
                    "sense_definition": "Een toestand van ernstige geestelijke stoornis of krankzinnigheid.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de jarenlange eenzaamheid werd hij langzaam tot <w>waanzin</w> gedreven.",
                            "target_lang_translations": {
                                "ru": "Многолетнее одиночество медленно доводило его до <w>безумия</w>.",
                                "en": "He was slowly driven to <w>madness</w> by years of loneliness."
                            }
                        },
                        {
                            "text": "In zijn <w>waanzin</w> dacht de man dat hij kon vliegen.",
                            "target_lang_translations": {
                                "ru": "В своем <w>помешательстве</w> мужчина думал, что может летать.",
                                "en": "In his <w>madness</w>, the man thought he could fly."
                            }
                        }
                    ],
                    "synonyms": [
                        "krankzinnigheid",
                        "gekte"
                    ],
                    "antonyms": [
                        "verstand"
                    ],
                    "common_phrases": [
                        "tot waanzin drijven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние серьезного психического расстройства или помешательства.",
                        "en": "A state of severe mental disorder or insanity."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безумие",
                                "target_lang_sense_clarification": "психическое расстройство"
                            },
                            {
                                "target_lang_word": "помешательство",
                                "target_lang_sense_clarification": "потеря рассудка"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "insanity",
                                "target_lang_sense_clarification": "severe mental illness"
                            },
                            {
                                "target_lang_word": "madness",
                                "target_lang_sense_clarification": "state of mental instability"
                            }
                        ]
                    }
                }

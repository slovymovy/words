            "pos": "verb",
            "senses": [
                {
                    "sense_id": "06323c1c-cddd-39f6-9d55-36f2549150b5",
                    "sense_definition": "Стать членом или участником общественной организации, группы или сообщества.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой друг решил <вступить> в шахматный клуб.",
                            "target_lang_translations": {
                                "en": "My friend decided to <w>join</w> the chess club."
                            }
                        },
                        {
                            "text": "Чтобы <вступить> в эту организацию, нужно заполнить анкету.",
                            "target_lang_translations": {
                                "en": "To <w>join</w> this organization, you need to fill out a form."
                            }
                        }
                    ],
                    "synonyms": [

                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "носиться",
                                "target_lang_sense_clarification": "быстро бегать или перемещаться (разг.)"
                            },
                            {
                                "target_lang_word": "гонять",
                                "target_lang_sense_clarification": "быстро ездить или бегать (разг.)"
                            },
                            {
                                "target_lang_word": "мчаться",
                                "target_lang_sense_clarification": "стремительно ехать или бежать"
                            }
                        ]
                    }
                }
            ]
        },
        {

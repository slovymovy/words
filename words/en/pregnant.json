        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ec16d19a-ce97-3293-bc9a-8bc61f9370b1",
                    "sense_definition": "Carrying a developing baby or offspring within the body.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "biological_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She stopped drinking alcohol as soon as she found out she was <w>pregnant</w>.",
                            "target_lang_translations": {
                                "ru": "Она перестала употреблять алкоголь, как только узнала, что <w>беременна</w>."
                            }
                        },
                        {
                            "text": "The doctor advised the <w>pregnant</w> woman to rest as much as possible.",
                            "target_lang_translations": {
                                "ru": "Врач посоветовал <w>беременной</w> женщине как можно больше отдыхать."
                            }
                        }
                    ],
                    "synonyms": [
                        "expecting",
                        "expectant"
                    ],
                    "antonyms": [
                        "nonpregnant"
                    ],
                    "common_phrases": [
                        "get pregnant",
                        "heavily pregnant",
                        "become pregnant",
                        "pregnant woman"
                    ],
                    "target_lang_definitions": {

                {
                    "sense_id": "f9e062b7-b64c-354f-ba02-3371b57f195d",
                    "sense_definition": "Een vervoersbewijs dat geldig is voor zowel de heenreis als de terugreis.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "ticket",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mag ik twee <w>retourtjes</w> naar Utrecht?",

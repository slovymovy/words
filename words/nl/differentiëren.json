    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "af3196b4-ba45-3092-bfb5-491984cb1fde",
                    "sense_definition": "Onderscheid maken of aanbrengen; zorgen dat zaken of personen van elkaar verschillen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "distinction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf probeert zich te <w>differentiëren</w> door uitstekende service te bieden.",
                            "target_lang_translations": {
                                "en": "The company tries to <w>differentiate</w> itself by offering excellent service.",
                                "ru": "Компания пытается <w>дифференцироваться</w> (выделиться), предлагая отличный сервис."
                            }
                        },
                        {
                            "text": "Een goede leraar kan <w>differentiëren</w> tussen de niveaus van de leerlingen.",
                            "target_lang_translations": {
                                "en": "A good teacher can <w>differentiate</w> between the students' levels.",
                                "ru": "Хороший учитель умеет <w>различать</w> уровни учеников."
                            }
                        }
                    ],
                    "synonyms": [
                        "onderscheiden"
                    ],
                    "antonyms": [
                        "gelijkschakelen",
                        "uniformeren"
                    ],
                    "common_phrases": [
                        "zich differentiëren",
                        "prijzen differentiëren"
                    ],
                    "target_lang_definitions": {
                        "en": "To make or introduce a distinction; to ensure that things or people differ from each other.",
                        "ru": "Делать различие или вносить разнообразие; обеспечивать отличие вещей или людей друг от друга."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "differentiate",
                                "target_lang_sense_clarification": "to make distinct or treat differently"
                            },
                            {
                                "target_lang_word": "distinguish",
                                "target_lang_sense_clarification": "to recognize or treat as different"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "дифференцировать",
                                "target_lang_sense_clarification": "проводить различие, классифицировать"
                            },
                            {
                                "target_lang_word": "различать",
                                "target_lang_sense_clarification": "видеть разницу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79a15b39-0aed-3b04-912a-4f01ad745c55",
                    "sense_definition": "De afgeleide van een wiskundige functie berekenen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mathematics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens het tentamen moesten de studenten een ingewikkelde functie <w>differentiëren</w>.",
                            "target_lang_translations": {
                                "en": "During the exam, the students had to <w>differentiate</w> a complex function.",
                                "ru": "Во время экзамена студенты должны были <w>дифференцировать</w> сложную функцию."
                            }
                        },
                        {
                            "text": "Als je leert <w>differentiëren</w>, kun je de helling van de grafiek bepalen.",
                            "target_lang_translations": {
                                "en": "If you learn to <w>differentiate</w>, you can determine the slope of the graph.",
                                "ru": "Если научишься <w>дифференцировать</w>, сможешь определить наклон графика."
                            }
                        }
                    ],
                    "synonyms": [
                        "afleiden"
                    ],
                    "antonyms": [
                        "integreren"
                    ],
                    "common_phrases": [
                        "een functie differentiëren"
                    ],
                    "target_lang_definitions": {
                        "en": "To calculate the derivative of a mathematical function.",
                        "ru": "Вычислять производную математической функции."
                    },
                    "translations": {

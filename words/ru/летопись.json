    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c062e823-f0da-3fdf-a359-14126695dc7b",
                    "sense_definition": "Запись исторических событий древнего времени по годам, производимая современником; книга с такими записями",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В старом монастыре была найдена древняя <летопись>.",
                            "target_lang_translations": {
                                "en": "An ancient <w>chronicle</w> was found in the old monastery."
                            }
                        },
                        {
                            "text": "Монах старательно переписывал <летопись> при свете свечи.",
                            "target_lang_translations": {
                                "en": "The monk was diligently copying the <w>chronicle</w> by candlelight."
                            }
                        }
                    ],
                    "synonyms": [
                        "хро́ника",
                        "анна́лы"
                    ],
                    "common_phrases": [
                        "древняя летопись",
                        "вести летопись",
                        "страницы летописи"
                    ],
                    "target_lang_definitions": {
                        "en": "A record of historical events of ancient times arranged by year, written by a contemporary; a book containing such records"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chronicle",
                                "target_lang_sense_clarification": "historical account"
                            },
                            {
                                "target_lang_word": "annals",
                                "target_lang_sense_clarification": "yearly records"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea524735-419a-3588-8d96-6364ba24b7d2",
                    "sense_definition": "Регулярная запись каких-либо событий; история развития чего-либо",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот роман представляет собой <летопись> жизни целого поколения.",
                            "target_lang_translations": {
                                "en": "This novel represents a <w>chronicle</w> of the life of a whole generation."
                            }
                        },
                        {
                            "text": "Газеты ведут ежедневную <летопись> городских происшествий.",
                            "target_lang_translations": {
                                "en": "Newspapers keep a daily <w>chronicle</w> of city incidents."
                            }
                        }
                    ],
                    "synonyms": [

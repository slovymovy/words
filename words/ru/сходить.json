    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7e6e97d1-553c-3c84-9082-fa9327fe3203",
                    "sense_definition": "Пойти куда-либо, побыть там некоторое время и вернуться обратно (совершенный вид).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "round_trip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама попросила меня <w>сходить</w> в магазин за свежим хлебом.",
                            "target_lang_translations": {
                                "en": "Mom asked me to <w>go</w> to the store for fresh bread."
                            }
                        },
                        {
                            "text": "Мы решили <w>сходить</w> в кино в эти выходные.",
                            "target_lang_translations": {
                                "en": "We decided to <w>go</w> to the cinema this weekend."
                            }
                        }
                    ],
                    "antonyms": [
                        "появляться"
                    ],
                    "common_phrases": [
                        "сходить в магазин",
                        "сходить за хлебом",
                        "сходить к врачу",
                        "сходить в туалет"
                    ],
                    "target_lang_definitions": {
                        "en": "To go somewhere for a short time and return."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go",
                                "target_lang_sense_clarification": "somewhere and return"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13bfb3af-203d-3aa2-9369-e665569026c0",
                    "sense_definition": "Покидать транспортное средство на остановке или станции (несовершенный вид).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport_exit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вы <w>сходите</w> на следующей остановке или едете дальше?",
                            "target_lang_translations": {
                                "en": "Are you <w>getting off</w> at the next stop or going further?"
                            }
                        },
                        {
                            "text": "Пассажиры начали медленно <w>сходить</w> с поезда на перрон.",
                            "target_lang_translations": {
                                "en": "The passengers began to slowly <w>get off</w> the train onto the platform."
                            }
                        }
                    ],
                    "synonyms": [
                        "выходить",
                        "вылезать"
                    ],
                    "antonyms": [
                        "садиться",
                        "входить"
                    ],
                    "common_phrases": [
                        "сходить на остановке",
                        "сходить с поезда",
                        "сходить на берег"
                    ],
                    "target_lang_definitions": {
                        "en": "To leave a vehicle or mode of transport."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get off",
                                "target_lang_sense_clarification": "transport"
                            },
                            {
                                "target_lang_word": "alight",
                                "target_lang_sense_clarification": "formal"
                            },
                            {
                                "target_lang_word": "disembark",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68613e4f-972a-3ec8-b665-b1943c74fb1a",
                    "sense_definition": "Перемещаться сверху вниз, спускаться (несовершенный вид).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "descend",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристы осторожно <w>сходили</w> с горы, чтобы не упасть.",
                            "target_lang_translations": {
                                "en": "The tourists were carefully <w>descending</w> the mountain so as not to fall."
                            }
                        },
                        {
                            "text": "Он каждый день <w>сходил</w> вниз по этой крутой лестнице.",
                            "target_lang_translations": {
                                "en": "Every day he <w>went down</w> these steep stairs."
                            }
                        }
                    ],
                    "synonyms": [
                        "спускаться",
                        "слезать",
                        "идти"
                    ],
                    "antonyms": [
                        "подниматься",
                        "входить"
                    ],
                    "common_phrases": [
                        "сходить вниз",
                        "сходить по лестнице",
                        "сходить с крыльца"
                    ],
                    "target_lang_definitions": {
                        "en": "To move from a higher place to a lower one."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "descend",
                                "target_lang_sense_clarification": "move downwards"
                            },
                            {
                                "target_lang_word": "go down",
                                "target_lang_sense_clarification": "move downwards"
                            },
                            {
                                "target_lang_word": "come down",
                                "target_lang_sense_clarification": "move downwards"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "692e5c3b-2a5e-3b05-a873-17850caa1469",
                    "sense_definition": "Исчезать, удаляться с поверхности, переставать быть видимым (о пятнах, краске, загаре).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "disappear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После стирки пятно на рубашке не хотело <w>сходить</w>.",
                            "target_lang_translations": {
                                "en": "After washing, the stain on the shirt did not want to <w>come off</w>."
                            }
                        },
                        {
                            "text": "Летний загар уже начал <w>сходить</w> с кожи, и она стала бледной.",
                            "target_lang_translations": {
                                "en": "The summer tan had already started to <w>fade</w> from the skin, and it became pale."
                            }
                        }
                    ],
                    "synonyms": [
                        "исчезать",
                        "пропадать",
                        "спускаться"
                    ],
                    "antonyms": [
                        "появляться"
                    ],
                    "common_phrases": [
                        "сходить с лица",
                        "пятно не сходит",
                        "краска сходит"
                    ],
                    "target_lang_definitions": {
                        "en": "To disappear or be removed from a surface (of stains, paint, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come off",
                                "target_lang_sense_clarification": "stains, paint"
                            },
                            {
                                "target_lang_word": "fade",
                                "target_lang_sense_clarification": "tan, color"
                            },
                            {
                                "target_lang_word": "disappear",
                                "target_lang_sense_clarification": "vanish"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee6bc1b0-e53b-3e6a-8525-dc65fd4a8f2b",
                    "sense_definition": "Отклоняться от правильного пути, направления или нормы.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "deviate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В густом лесу очень опасно <w>сходить</w> с намеченной тропы.",
                            "target_lang_translations": {
                                "en": "In a dense forest, it is very dangerous to <w>stray</w> from the marked path."
                            }
                        },
                        {
                            "text": "От такого количества работы можно просто <w>сходить</w> с ума.",
                            "target_lang_translations": {
                                "en": "From such an amount of work, one could simply <w>go</w> crazy."
                            }
                        }
                    ],
                    "synonyms": [
                        "отклониться"
                    ],
                    "common_phrases": [
                        "сходить с ума",
                        "сходить с тропы",
                        "сходить с дистанции"
                    ],
                    "target_lang_definitions": {
                        "en": "To deviate from a path, norm, or state."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go",
                                "target_lang_sense_clarification": "in idioms like 'go crazy'"
                            },
                            {
                                "target_lang_word": "stray",
                                "target_lang_sense_clarification": "from a path"
                            },
                            {
                                "target_lang_word": "deviate",
                                "target_lang_sense_clarification": "from a norm"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98271fe6-2ba5-3309-99b6-2c71e3ac21c4",
                    "sense_definition": "Соскакивать с рельсов, смещаться при движении (о поездах, трамваях).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "derail",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за старых путей трамвай мог <w>сходить</w> с рельсов на повороте.",
                            "target_lang_translations": {
                                "en": "Due to the old tracks, the tram could <w>derail</w> at the turn."
                            }
                        },
                        {
                            "text": "Поезд начал <w>сходить</w> с пути, и машинист применил экстренное торможение.",
                            "target_lang_translations": {
                                "en": "The train began to <w>go off</w> the track, and the driver applied emergency braking."
                            }
                        }
                    ],
                    "synonyms": [
                        "соскакивать"
                    ],
                    "antonyms": [
                        "удерживаться"
                    ],
                    "common_phrases": [
                        "сходить с рельсов",
                        "сходить с пути"
                    ],
                    "target_lang_definitions": {
                        "en": "To leave the rails or track (of trains)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "derail",
                                "target_lang_sense_clarification": "trains"
                            },
                            {
                                "target_lang_word": "come off",
                                "target_lang_sense_clarification": "the rails"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
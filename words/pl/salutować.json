{
    "word_family": [
        "salut",
        "odsalutować"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "db87bd66-6232-3d78-b224-44777714fb81",
                    "sense_definition": "Pozdrawiać lub oddawać honory zgodnie z regulaminem wojskowym, zwykle poprzez przyłożenie dłoni do nakrycia głowy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "military_honors",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody żołnierz sprężyście <w>zasalutował</w> przechodzącemu generałowi.",
                            "target_lang_translations": {
                                "ru": "Молодой солдат четко <w>отдал честь</w> проходящему генералу."
                            }
                        },
                        {
                            "text": "Policjant <w>zasalutował</w> kierowcy, zanim poprosił go o dokumenty.",
                            "target_lang_translations": {
                                "ru": "Полицейский <w>отдал честь</w> водителю, прежде чем попросить у него документы."
                            }
                        }
                    ],
                    "synonyms": [
                        "oddać honory"
                    ],
                    "common_phrases": [
                        "salutować dowódcy",
                        "salutować do daszka",
                        "salutować sztandarowi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приветствовать или отдавать воинские почести в соответствии с уставом, обычно прикладывая руку к головному убору."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отдавать честь",
                                "target_lang_sense_clarification": "воинское приветствие рукой"
                            },
                            {
                                "target_lang_word": "салютовать",
                                "target_lang_sense_clarification": "приветствовать жестом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1365b514-247a-345e-bc5c-ec5c1838e724",
                    "sense_definition": "Oddawać strzały z broni, zazwyczaj z dział, jako uroczysty gest powitania lub uczczenia kogoś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_honors",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Okręt wojenny <w>salutował</w> wystrzałami armatnimi, wpływając do sojuszniczego portu.",
                            "target_lang_translations": {
                                "ru": "Военный корабль <w>салютовал</w> пушечными выстрелами, входя в союзный порт."
                            }
                        },
                        {
                            "text": "Artyleria <w>salutowała</w> na cześć nowej głowy państwa.",
                            "target_lang_translations": {
                                "ru": "Артиллерия <w>салютовала</w> в честь нового главы государства."
                            }
                        }
                    ],
                    "common_phrases": [
                        "salutować z dział",
                        "salutować wystrzałami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Производить выстрелы из оружия, обычно из орудий, в качестве торжественного жеста приветствия или чествования кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "салютовать",
                                "target_lang_sense_clarification": "производить салют выстрелами"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
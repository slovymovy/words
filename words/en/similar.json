            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3e74a715-61df-3492-91fc-38a1a4155ce4",
                    "sense_definition": "Having traits or characteristics in common; alike, allied, comparable.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "general_likeness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My new car is quite <w>similar</w> to my old one, but it has more space.",
                            "target_lang_translations": {
                                "ru": "Моя новая машина довольно <w>похожа</w> на старую, но в ней больше места."
                            }
                        },
                        {
                            "text": "The two sisters look very <w>similar</w> to each other.",
                            "target_lang_translations": {
                                "ru": "Две сестры очень <w>похожи</w> друг на друга."
                            }
                        }
                    ],
                    "synonyms": [
                        "alike",
                        "comparable",
                        "akin"
                    ],
                    "antonyms": [
                        "different",
                        "dissimilar",
                        "unlike",
                        "unique"
                    ],
                    "common_phrases": [
                        "similar to",
                        "very similar",
                        "look similar",
                        "similar situation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий общие черты или характеристики; похожий, сравнимый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "похожий",
                                "target_lang_sense_clarification": "имеющий общие черты, напоминающий кого-либо или что-либо"
                            },
                            {
                                "target_lang_word": "подобный",
                                "target_lang_sense_clarification": "такой же, как кто-либо или что-либо; схожий"
                            },
                            {
                                "target_lang_word": "схожий",
                                "target_lang_sense_clarification": "обладающий сходством"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52970ca7-a704-35b4-9865-26dfd8cec10c",
                    "sense_definition": "Having the same shape, in particular, having corresponding angles equal and corresponding line segments proportional.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "geometry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In geometry, two triangles are <w>similar</w> if their angles are identical.",
                            "target_lang_translations": {
                                "ru": "В геометрии два треугольника <w>подобны</w>, если их углы одинаковы."
                            }
                        },
                        {
                            "text": "We learned how to calculate the sides of <w>similar</w> figures in math class.",
                            "target_lang_translations": {
                                "ru": "На уроке математики мы учились вычислять стороны <w>подобных</w> фигур."
                            }
                        }
                    ],
                    "common_phrases": [
                        "similar triangles",
                        "similar figures"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий одинаковую форму; (о геометрических фигурах) с равными углами и пропорциональными сторонами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подобный",
                                "target_lang_sense_clarification": "геометрический термин"

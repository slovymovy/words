    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4eda5d52-487c-30ba-aaa2-6beae9bc951d",
                    "sense_definition": "Een les of voordracht aan een universiteit of hogeschool.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Morgenochtend heb ik al om negen uur mijn eerste <w>college</w>.",
                            "target_lang_translations": {
                                "ru": "Завтра утром у меня уже в девять часов первая <w>лекция</w>."
                            }
                        },
                        {
                            "text": "De professor geeft elke dinsdag een <w>college</w> over moderne geschiedenis.",
                            "target_lang_translations": {
                                "ru": "Профессор каждый вторник читает <w>лекцию</w> по современной истории."
                            }
                        }
                    ],
                    "synonyms": [
                        "les",
                        "voordracht"
                    ],
                    "common_phrases": [
                        "college geven",
                        "college volgen",
                        "hoorcollege"
                    ],
                    "target_lang_definitions": {
                        "ru": "Урок или лекция в университете или высшем учебном заведении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лекция",
                                "target_lang_sense_clarification": "учебное занятие"
                            },
                            {
                                "target_lang_word": "пара",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0e6e700-1d8c-34fc-a078-c1c61aae7df7",
                    "sense_definition": "Een bestuurlijk lichaam of groep personen met een gezamenlijke functie, vaak binnen de overheid.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>college</w> van burgemeester en wethouders heeft een nieuw besluit genomen.",
                            "target_lang_translations": {
                                "ru": "<w>Коллегия</w> мэра и олдерменов приняла новое решение."
                            }
                        },
                        {
                            "text": "Zij is benoemd tot lid van het <w>college</w> van bestuur van de universiteit.",
                            "target_lang_translations": {
                                "ru": "Она была назначена членом <w>совета</w> правления университета."
                            }
                        }
                    ],
                    "synonyms": [
                        "raad",
                        "lichaam",
                        "gremium"
                    ],
                    "common_phrases": [
                        "college van B en W",
                        "college van bestuur"
                    ],
                    "target_lang_definitions": {
                        "ru": "Административный орган или группа лиц, выполняющих общую функцию, часто в правительстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коллегия",
                                "target_lang_sense_clarification": "группа должностных лиц"
                            },
                            {
                                "target_lang_word": "совет",
                                "target_lang_sense_clarification": "орган управления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "850743c1-6000-30a1-8f83-0629a3987a98",
                    "sense_definition": "Een middelbare school, specifiek in België vaak voor katholiek onderwijs.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn neefje gaat elke dag met de fiets naar het <w>college</w> in de stad.",
                            "target_lang_translations": {
                                "ru": "Мой племянник каждый день ездит на велосипеде в <w>колледж</w> в городе."
                            }
                        },
                        {
                            "text": "Het oude gebouw van het <w>college</w> wordt gerenoveerd.",
                            "target_lang_translations": {
                                "ru": "Старое здание <w>колледжа</w> ремонтируется."
                            }
                        }
                    ],
                    "synonyms": [
                        "school",
                        "onderwijsinstituut"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "België"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Средняя школа, в Бельгии часто католическая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колледж",
                                "target_lang_sense_clarification": "среднее учебное заведение"
                            },
                            {
                                "target_lang_word": "школа",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

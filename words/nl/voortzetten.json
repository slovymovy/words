{
    "word_family": [
        "voortzetting"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "10b7fc14-645d-3330-8a00-5124a954312e",
                    "sense_definition": "Verdergaan met een activiteit, handeling of situatie zonder te stoppen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "continue_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De regering wil het huidige economische beleid <w>voortzetten</w>.",
                            "target_lang_translations": {
                                "ru": "Правительство хочет <w>продолжать</w> текущую экономическую политику.",
                                "en": "The government wants to <w>continue</w> the current economic policy."
                            }
                        },
                        {
                            "text": "Ondanks de tegenslagen zullen wij ons werk <w>voortzetten</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на неудачи, мы <w>продолжим</w> нашу работу.",
                                "en": "Despite the setbacks, we will <w>continue</w> our work."
                            }
                        }
                    ],
                    "synonyms": [
                        "doorgaan",
                        "verderzetten",
                        "voortgaan",
                        "verder gaan met"
                    ],
                    "antonyms": [
                        "stoppen",
                        "beëindigen",
                        "ophouden"
                    ],
                    "common_phrases": [
                        "het beleid voortzetten",
                        "een traditie voortzetten",
                        "de strijd voortzetten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продолжать деятельность, действие или ситуацию без остановки.",
                        "en": "To continue an activity, action, or situation without stopping."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "продолжать",
                                "target_lang_sense_clarification": "делать дальше"
                            },
                            {
                                "target_lang_word": "продолжить",
                                "target_lang_sense_clarification": "совершить действие продолжения"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "continue",
                                "target_lang_sense_clarification": "to keep doing something"
                            },
                            {
                                "target_lang_word": "proceed with",
                                "target_lang_sense_clarification": "to move forward with"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16dcf970-44b7-3f5b-9635-9c9eb883a977",
                    "sense_definition": "Opnieuw beginnen met een handeling na een tijdelijke onderbreking.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "continue_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de lunchpauze zullen we de vergadering <w>voortzetten</w>.",
                            "target_lang_translations": {
                                "ru": "После обеденного перерыва мы <w>продолжим</w> собрание.",
                                "en": "After the lunch break, we will <w>resume</w> the meeting."
                            }
                        },
                        {
                            "text": "Toen het stopte met regenen, konden de spelers de wedstrijd <w>voortzetten</w>.",
                            "target_lang_translations": {
                                "ru": "Когда дождь прекратился, игроки смогли <w>продолжить</w> матч.",
                                "en": "When it stopped raining, the players could <w>resume</w> the match."
                            }
                        }
                    ],
                    "synonyms": [
                        "vervolgen",
                        "doorgaan"
                    ],
                    "antonyms": [
                        "onderbreken",
                        "staken"
                    ],
                    "common_phrases": [
                        "de vergadering voortzetten",
                        "het gesprek voortzetten",
                        "de reis voortzetten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возобновить действие после временного перерыва.",
                        "en": "To start an action again after a temporary interruption."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возобновить",
                                "target_lang_sense_clarification": "начать снова"
                            },
                            {
                                "target_lang_word": "продолжить",
                                "target_lang_sense_clarification": "после паузы"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "resume",
                                "target_lang_sense_clarification": "to start again after a pause"
                            },
                            {
                                "target_lang_word": "continue",
                                "target_lang_sense_clarification": "to pick up where left off"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
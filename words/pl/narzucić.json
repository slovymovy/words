{
    "word_family": [
        "narzucać",
        "narzut",
        "narzutka"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "48310beb-1f75-380d-bb9b-8c1a170bc1b2",
                    "sense_definition": "Szybko włożyć na siebie ubranie lub okryć nim ramiona.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zrobiło się chłodno, więc musiałem <narzucić> sweter przed wyjściem.",
                            "target_lang_translations": {
                                "ru": "Стало прохладно, поэтому мне пришлось <накинуть> свитер перед выходом."
                            }
                        },
                        {
                            "text": "Staruszka <narzuciła> na ramiona chustę i wyszła na ganek.",
                            "target_lang_translations": {
                                "ru": "Старушка <набросила> на плечи шаль и вышла на крыльцо."
                            }
                        }
                    ],
                    "synonyms": [
                        "zarzucić",
                        "włożyć"
                    ],
                    "antonyms": [
                        "zdjąć"
                    ],
                    "common_phrases": [
                        "narzucić coś na siebie",
                        "narzucić płaszcz",
                        "narzucić kurtkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро надеть на себя одежду или набросить её на плечи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "накинуть",
                                "target_lang_sense_clarification": "наспех надеть"
                            },
                            {
                                "target_lang_word": "набросить",
                                "target_lang_sense_clarification": "покрыть плечи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b5fa801-5834-3f45-ac44-ffd5e4bd2879",
                    "sense_definition": "Zmusić kogoś do przyjęcia czegoś, na przykład woli, poglądów lub zasad.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "imposition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trener chciał <narzucić> zawodnikom nowy styl gry, ale oni woleli stary.",
                            "target_lang_translations": {
                                "ru": "Тренер хотел <навязать> игрокам новый стиль игры, но они предпочитали старый."
                            }
                        },
                        {
                            "text": "Nie pozwól nikomu <narzucić> sobie zdania w tej ważnej sprawie.",
                            "target_lang_translations": {
                                "ru": "Не позволяй никому <навязать> себе мнение в этом важном вопросе."
                            }
                        }
                    ],
                    "synonyms": [
                        "wymusić",
                        "podyktować",
                        "nakazać"
                    ],
                    "common_phrases": [
                        "narzucić swoją wolę",
                        "narzucić tempo",
                        "narzucić zdanie",
                        "narzucić dyscyplinę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-либо принять что-либо (например, волю, взгляды или правила) против желания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "навязать",
                                "target_lang_sense_clarification": "заставить принять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b3df444-16e7-3dd5-8d40-8355f745d725",
                    "sense_definition": "Rzucić dużą ilość czegoś na wierzch, przykrywając to.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy <narzucić> więcej ziemi na te korzenie, żeby nie zmarzły.",
                            "target_lang_translations": {
                                "ru": "Мы должны <набросать> больше земли на эти корни, чтобы они не замёрзли."
                            }
                        },
                        {
                            "text": "Robotnicy musieli <narzucić> piach na oblodzoną drogę.",
                            "target_lang_translations": {
                                "ru": "Рабочие должны были <накидать> песка на обледенелую дорогу."
                            }
                        }
                    ],
                    "synonyms": [
                        "zarzucić",
                        "przykryć"
                    ],
                    "antonyms": [
                        "zrzucić"
                    ],
                    "common_phrases": [
                        "narzucić ziemi",
                        "narzucić koc"
                    ],
                    "target_lang_definitions": {
                        "ru": "Накидать большое количество чего-либо сверху, чтобы прикрыть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "набросать",
                                "target_lang_sense_clarification": "землю, песок"
                            },
                            {
                                "target_lang_word": "накидать",
                                "target_lang_sense_clarification": "разг."
                            },
                            {
                                "target_lang_word": "набросить",
                                "target_lang_sense_clarification": "покрывало, ткань"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51b538b3-dcd7-32ab-97f1-0f776a599a8b",
                    "sense_definition": "Sprawić, że ktoś musi znosić czyjąś obecność lub towarzystwo.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "imposition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie chcę ci <narzucić> mojego towarzystwa, jeśli jesteś bardzo zajęty.",
                            "target_lang_translations": {
                                "ru": "Я не хочу <навязать> тебе своё общество, если ты очень занят."
                            }
                        },
                        {
                            "text": "Próbował <narzucić> swoją obecność kolegom z pracy, ale go unikali.",
                            "target_lang_translations": {
                                "ru": "Он пытался <навязать> своё присутствие коллегам по работе, но они его избегали."
                            }
                        }
                    ],
                    "common_phrases": [
                        "narzucić swoje towarzystwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-либо терпеть чьё-либо общество или присутствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "навязать",
                                "target_lang_sense_clarification": "своё общество"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "718517e6-c0b0-322e-8452-5d42297db976",
                    "sense_definition": "Het heelal en alles wat daarin leeft, vaak gezien als het werk van God.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "universe",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten goed zorgen voor de <w>schepping</w> en de natuur.",
                            "target_lang_translations": {
                                "ru": "Мы должны хорошо заботиться о <w>творении</w> и природе."
                            }
                        },
                        {
                            "text": "In het oude verhaal wordt verteld over de <w>schepping</w> van de aarde.",
                            "target_lang_translations": {
                                "ru": "В древнем предании рассказывается о <w>сотворении</w> Земли."
                            }
                        }
                    ],
                    "synonyms": [
                        "creatie"
                    ],
                    "common_phrases": [
                        "de hele schepping",
                        "Gods schepping"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вселенная и всё живое в ней, часто рассматриваемое как дело рук Бога."
                    },
                    "translations": {
                        "ru": [
                            {

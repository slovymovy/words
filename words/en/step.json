    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "12662eaf-2304-3686-8b8b-e1d56ae41549",
                    "sense_definition": "An act of lifting the foot and setting it down in a new position, as in walking or running.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She took a <w>step</w> forward to get a better look at the painting.",
                            "target_lang_translations": {
                                "ru": "Она сделала <w>шаг</w> вперед, чтобы лучше рассмотреть картину."
                            }
                        },
                        {
                            "text": "The baby took his first uncertain <w>steps</w> across the living room floor.",
                            "target_lang_translations": {
                                "ru": "Ребенок сделал свои первые неуверенные <w>шаги</w> по полу гостиной."
                            }
                        }
                    ],
                    "synonyms": [
                        "stride"
                    ],
                    "common_phrases": [
                        "take a step",
                        "step forward",
                        "one step"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, заключающееся в поднятии ноги и опускании её на новое место, как при ходьбе или беге."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шаг",
                                "target_lang_sense_clarification": "движение ногой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bad8532f-ba3b-339a-bf4f-2d7c9831c225",
                    "sense_definition": "A flat surface, such as on a stair or ladder, on which to place the foot when moving up or down.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "structure_stair",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please be careful, the bottom <w>step</w> is a bit loose.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, будьте осторожны, нижняя <w>ступенька</w> немного шатается."
                            }
                        },
                        {
                            "text": "He sat on the front <w>steps</w> waiting for the mail to arrive.",
                            "target_lang_translations": {
                                "ru": "Он сидел на <w>ступеньках</w> крыльца, ожидая почту."
                            }
                        }
                    ],
                    "common_phrases": [
                        "front steps",
                        "mind the step",
                        "flight of steps"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоская поверхность, например, на лестнице, на которую ставят ногу при подъеме или спуске."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ступенька",
                                "target_lang_sense_clarification": "часть лестницы (разг.)"
                            },
                            {
                                "target_lang_word": "ступень",
                                "target_lang_sense_clarification": "часть лестницы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95718a84-dd92-3311-9fc4-f26b7064a45f",
                    "sense_definition": "A distinct stage or point in a process or series of events.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Finding a job is the first <w>step</w> to becoming independent.",
                            "target_lang_translations": {
                                "ru": "Поиск работы — это первый <w>шаг</w> к независимости."
                            }
                        },
                        {
                            "text": "We need to follow the instructions <w>step</w> by <w>step</w> to build the shelf.",
                            "target_lang_translations": {
                                "ru": "Нам нужно следовать инструкциям <w>шаг</w> за <w>шагом</w>, чтобы собрать полку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "step by step",
                        "next step",
                        "first step",
                        "big step"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельная стадия или момент в процессе или последовательности событий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шаг",
                                "target_lang_sense_clarification": "стадия процесса"
                            },
                            {
                                "target_lang_word": "этап",
                                "target_lang_sense_clarification": "стадия развития"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de948f48-bfdb-3f01-ab41-2da9e7d5205c",
                    "sense_definition": "A measure or action taken to achieve a particular goal.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government must take <w>steps</w> to reduce pollution in the city.",
                            "target_lang_translations": {
                                "ru": "Правительство должно принять <w>меры</w> для снижения загрязнения в городе."
                            }
                        },
                        {
                            "text": "We are taking immediate <w>steps</w> to correct the error in the report.",
                            "target_lang_translations": {
                                "ru": "Мы принимаем немедленные <w>меры</w> для исправления ошибки в отчете."
                            }
                        }
                    ],
                    "common_phrases": [
                        "take steps",
                        "necessary steps"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мера или действие, предпринятое для достижения определенной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мера",
                                "target_lang_sense_clarification": "действие для достижения цели"

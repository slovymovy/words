            "senses": [
                {
                    "sense_id": "8e152b4b-7978-3a11-b45a-b5ea925773d4",
                    "sense_definition": "To form a border around something.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "decoration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tall palm trees <w>fringe</w> the white sandy beach.",
                            "target_lang_translations": {
                                "ru": "Высокие пальмы <w>окаймляют</w> белый песчаный пляж."
                            }
                        },
                        {
                            "text": "The dark clouds were <w>fringed</w> with golden light from the setting sun.",
                            "target_lang_translations": {
                                "ru": "Тёмные облака были <w>окаймлены</w> золотым светом заходящего солнца."
                            }

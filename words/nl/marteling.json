{
    "word_family": [
        "martelen",
        "martelaar"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "12463db0-cf56-3540-a8b2-75856ff4565b",
                    "sense_definition": "Het opzettelijk toebrengen van ernstige pijn en letsel aan iemand die zich niet kan verdedigen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "literal_torture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gevangene vertelde de rechters over de vreselijke <w>marteling</w> in de cel.",
                            "target_lang_translations": {
                                "ru": "Заключенный рассказал судьям об ужасной <w>пытке</w> в камере."
                            }
                        },
                        {
                            "text": "In veel landen is elke vorm van <w>marteling</w> streng verboden.",
                            "target_lang_translations": {
                                "ru": "Во многих странах любые формы <w>пыток</w> строго запрещены."
                            }
                        }
                    ],
                    "synonyms": [
                        "foltering",
                        "pijniging"
                    ],
                    "common_phrases": [
                        "wrede marteling",
                        "fysieke marteling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Умышленное причинение сильной боли и травм тому, кто не может защитить себя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пытка",
                                "target_lang_sense_clarification": "физическое насилие"
                            },
                            {
                                "target_lang_word": "истязание",
                                "target_lang_sense_clarification": "жестокое обращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec443aaa-35cd-30c0-9e74-938c2ddf8ae4",
                    "sense_definition": "Een zeer vervelende, pijnlijke of saaie ervaring die lang lijkt te duren.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_torture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het wachten op de uitslag van het examen was een ware <w>marteling</w>.",
                            "target_lang_translations": {
                                "ru": "Ожидание результатов экзамена было настоящей <w>пыткой</w>."
                            }
                        },
                        {
                            "text": "Luisteren naar die saaie toespraak voelde als een eindeloze <w>marteling</w>.",
                            "target_lang_translations": {
                                "ru": "Прослушивание этой скучной речи казалось бесконечным <w>мучением</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "foltering"
                    ],
                    "common_phrases": [
                        "een ware marteling",
                        "geestelijke marteling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень неприятное, болезненное или скучное переживание, которое кажется долгим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мучение",
                                "target_lang_sense_clarification": "страдание, неприятное переживание"
                            },
                            {
                                "target_lang_word": "пытка",
                                "target_lang_sense_clarification": "перен. невыносимая ситуация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
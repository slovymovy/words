                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соразмерять",
                                "target_lang_sense_clarification": "приводить в соответствие"
                            },
                            {
                                "target_lang_word": "соотносить",
                                "target_lang_sense_clarification": "сравнивать или связывать"
                            }
                        ]
                    }
                }
            ]
        }

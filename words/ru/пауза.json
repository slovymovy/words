                        "выдержать паузу",
                        "четвертная пауза"
                    ],
                    "target_lang_definitions": {
                        "en": "A symbol in musical notation indicating a break in sound, as well as the break itself."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rest",
                                "target_lang_sense_clarification": "musical symbol or silence"
                            },
                            {
                                "target_lang_word": "pause",
                                "target_lang_sense_clarification": "musical hold"
                            }
                        ]
                    }
                }
            ]

                        "крутой"
                    ],
                    "antonyms": [
                        "луговой",
                        "низменный",
                        "пологий"
                    ],
                    "common_phrases": [
                        "нагорный берег",
                        "нагорная сторона"
                    ],
                    "target_lang_definitions": {
                        "en": "High, steep (usually referring to the primary bank of a river, opposite the low-lying meadow bank)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "upland",

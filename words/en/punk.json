{
    "word_family": [
        "punky",
        "punkish",
        "punker"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0ef099ff-d8a9-3440-a2e9-72a63c3dbde2",
                    "sense_definition": "A genre of loud, fast rock music with aggressive lyrics, or the subculture associated with it.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "music_culture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The band became famous for playing loud, energetic <punk> in small clubs.",
                            "target_lang_translations": {
                                "ru": "Группа прославилась тем, что играла громкий, энергичный <панк> в небольших клубах."
                            }
                        },
                        {
                            "text": "Many teenagers in the 1970s expressed their rebellion through <punk>.",
                            "target_lang_translations": {
                                "ru": "Многие подростки в 1970-х выражали свой бунт через <панк>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "punk rock",
                        "punk band",
                        "pop punk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жанр громкой, быстрой рок-музыки с агрессивными текстами или связанная с ним субкультура."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "панк",
                                "target_lang_sense_clarification": "музыкальный жанр или субкультура"
                            },
                            {
                                "target_lang_word": "панк-рок",
                                "target_lang_sense_clarification": "музыкальный стиль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f787ecec-5c34-37df-b76a-298b950154ee",
                    "sense_definition": "A person who is a fan of punk rock music or a member of the punk subculture, often characterized by unconventional clothing and hairstyles.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "music_culture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The young <punk> wore a leather jacket covered in metal studs.",
                            "target_lang_translations": {
                                "ru": "Молодой <панк> носил кожаную куртку, покрытую металлическими заклепками."
                            }
                        },
                        {
                            "text": "She dyed her hair pink and started dressing like a <punk>.",
                            "target_lang_translations": {
                                "ru": "Она покрасила волосы в розовый цвет и начала одеваться как <панк>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поклонник панк-рока или представитель панк-субкультуры, часто отличающийся необычной одеждой и прической."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "панк",
                                "target_lang_sense_clarification": "человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1411e4bb-5885-3564-b9d9-804e076c4aef",
                    "sense_definition": "A young person who behaves in a rude, aggressive, or criminal way.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "person_negative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Some street <punks> vandalized the bus stop last night.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером какие-то уличные <хулиганы> разгромили автобусную остановку."
                            }
                        },
                        {
                            "text": "The detective chased the young <punk> down the alleyway.",
                            "target_lang_translations": {
                                "ru": "Детектив преследовал молодого <хулигана> в переулке."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoodlum",
                        "hooligan",
                        "trouble-maker"
                    ],
                    "common_phrases": [
                        "street punk"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Молодой человек, ведущий себя грубо, агрессивно или преступно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хулиган",
                                "target_lang_sense_clarification": "нарушитель порядка"
                            },
                            {
                                "target_lang_word": "шпана",
                                "target_lang_sense_clarification": "собирательное, уличные хулиганы"
                            },
                            {
                                "target_lang_word": "отморозок",
                                "target_lang_sense_clarification": "сленг, агрессивный человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bca8c6d3-a634-30f5-84e1-b1c0c7b3767d",
                    "sense_definition": "A person considered weak, cowardly, or inferior.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "person_negative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't be such a <punk>; just go and talk to her.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <слабаком>, просто пойди и поговори с ней."
                            }
                        },
                        {
                            "text": "He acted tough, but everyone knew he was just a <punk>.",
                            "target_lang_translations": {
                                "ru": "Он строил из себя крутого, но все знали, что он просто <ничтожество>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sissy"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "derogatory"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, которого считают слабым, трусливым или неполноценным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слабак",
                                "target_lang_sense_clarification": "трусливый или слабый человек"
                            },
                            {
                                "target_lang_word": "ничтожество",
                                "target_lang_sense_clarification": "никчемный человек"
                            },
                            {
                                "target_lang_word": "сопляк",
                                "target_lang_sense_clarification": "пренебрежительно о молодом или неопытном"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c591406-96e7-3f85-b41a-58a132bbaa4f",
                    "sense_definition": "A slow-burning stick used to light fireworks.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "fire_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We used a <punk> to light the fuses so we wouldn't burn our fingers.",
                            "target_lang_translations": {
                                "ru": "Мы использовали <тлеющую палочку>, чтобы поджечь фитили и не обжечь пальцы."
                            }
                        },
                        {
                            "text": "The child held the glowing <punk> carefully while waiting for the next firework.",
                            "target_lang_translations": {
                                "ru": "Ребенок осторожно держал тлеющий <фитиль>, ожидая следующего фейерверка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "light a punk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тлеющая палочка, используемая для поджигания фейерверков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фитиль",
                                "target_lang_sense_clarification": "для поджигания"
                            },
                            {
                                "target_lang_word": "тлеющая палочка",
                                "target_lang_sense_clarification": "описание предмета"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "37737834-2a8f-3b86-b97b-e55bc95d618a",
                    "sense_definition": "Relating to or characteristic of punk rock music or its subculture.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "music_culture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has a very <punk> hairstyle with bright colors and spikes.",
                            "target_lang_translations": {
                                "ru": "У него очень <панковская> прическа с яркими цветами и шипами."
                            }
                        },
                        {
                            "text": "The designer released a new collection inspired by <punk> fashion.",
                            "target_lang_translations": {
                                "ru": "Дизайнер выпустил новую коллекцию, вдохновленную модой <в стиле панк>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "punk rock",
                        "punk style"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к панк-року или характерный для его субкультуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "панковский",
                                "target_lang_sense_clarification": "прилагательное"
                            },
                            {
                                "target_lang_word": "в стиле панк",
                                "target_lang_sense_clarification": "о стиле или моде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3fd89ff-a8eb-3c95-9ab3-e0cbfb7dceb2",
                    "sense_definition": "Of very poor quality; worthless.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "quality_negative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I felt <punk> all day after eating that bad seafood.",
                            "target_lang_translations": {
                                "ru": "Я чувствовал себя <паршиво> весь день после того, как съел те плохие морепродукты."
                            }
                        },
                        {
                            "text": "The service at this restaurant is absolutely <punk>.",
                            "target_lang_translations": {
                                "ru": "Обслуживание в этом ресторане абсолютно <дрянное>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "dated"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень низкого качества; никчемный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паршивый",
                                "target_lang_sense_clarification": "разг., плохого качества"
                            },
                            {
                                "target_lang_word": "дрянной",
                                "target_lang_sense_clarification": "разг., плохой"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dd0b069e-46a1-3aa1-a050-e99a09787a76",
                    "sense_definition": "To play a trick or prank on someone.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "action_trick",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He thought he was being <punked> when he saw the hidden camera.",
                            "target_lang_translations": {
                                "ru": "Он подумал, что его <разыгрывают>, когда увидел скрытую камеру."
                            }
                        },
                        {
                            "text": "My friends <punked> me by filling my office with balloons.",
                            "target_lang_translations": {
                                "ru": "Друзья <разыграли> меня, заполнив мой офис воздушными шарами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get punked"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разыграть кого-либо, устроить розыгрыш."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разыграть",
                                "target_lang_sense_clarification": "подшутить"
                            },
                            {
                                "target_lang_word": "приколоться",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27f78de8-df4f-3f9b-b8a7-7923aab04c4f",
                    "sense_definition": "To withdraw from something due to fear or cowardice.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "action_quit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was supposed to dive off the cliff, but he <punked> out at the last second.",
                            "target_lang_translations": {
                                "ru": "Он должен был прыгнуть со скалы, но <сдрейфил> в последнюю секунду."
                            }
                        },
                        {
                            "text": "Don't <punk> out now; we are almost at the finish line.",
                            "target_lang_translations": {
                                "ru": "Не <трусь> сейчас, мы почти у финиша."
                            }
                        }
                    ],
                    "common_phrases": [
                        "punk out"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отказаться от чего-либо из-за страха или трусости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдрейфить",
                                "target_lang_sense_clarification": "разг., испугаться"
                            },
                            {
                                "target_lang_word": "струсить",
                                "target_lang_sense_clarification": "проявить трусость"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
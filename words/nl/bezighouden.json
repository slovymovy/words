{
    "word_family": [
        "bezigheid",
        "bezig"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4806492d-fcc5-3905-8b3e-41654b395845",
                    "sense_definition": "Tijd en aandacht besteden aan een bepaalde activiteit of onderwerp (meestal wederkerend: zich bezighouden met).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij <w>hielden</w> zich <w>bezig</w> met het ontwerp van nieuwe zonnecellen.",
                            "target_lang_translations": {
                                "en": "They <w>occupied</w> themselves with the design of new solar cells.",
                                "ru": "Они <w>занимались</w> разработкой новых солнечных батарей."
                            }
                        },
                        {
                            "text": "Hij <w>houdt</w> zich in zijn vrije tijd graag <w>bezig</w> met fotografie en kunst.",
                            "target_lang_translations": {
                                "en": "In his free time, he likes to <w>occupy</w> himself with photography and art.",
                                "ru": "В свободное время он любит <w>заниматься</w> фотографией и искусством."
                            }
                        }
                    ],
                    "synonyms": [
                        "zich wijden aan",
                        "zich toeleggen op",
                        "besteden aan"
                    ],
                    "antonyms": [
                        "verwaarlozen",
                        "negeren"
                    ],
                    "common_phrases": [
                        "zich bezighouden met",
                        "zich ergens mee bezighouden"
                    ],
                    "target_lang_definitions": {
                        "en": "To spend time and attention on a specific activity or subject (usually reflexive).",
                        "ru": "Уделять время и внимание определенной деятельности или теме (обычно возвратный глагол: заниматься чем-либо)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "occupy oneself with",
                                "target_lang_sense_clarification": "reflexive, formal"
                            },
                            {
                                "target_lang_word": "busy oneself with",
                                "target_lang_sense_clarification": "reflexive, descriptive"
                            },
                            {
                                "target_lang_word": "engage in",
                                "target_lang_sense_clarification": "formal activity"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "заниматься",
                                "target_lang_sense_clarification": "посвящать время какому-либо делу"
                            },
                            {
                                "target_lang_word": "увлекаться",
                                "target_lang_sense_clarification": "иметь хобби или интерес"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d90f47b-cb30-3890-9d15-8410050662ba",
                    "sense_definition": "Iemand iets te doen geven of vermaken, zodat diegene niet verveeld raakt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "occupy_someone",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>hield</w> de kinderen even <w>bezig</w> met een leuk spelletje.",
                            "target_lang_translations": {
                                "en": "He <w>kept</w> the children <w>busy</w> for a while with a fun game.",
                                "ru": "Он ненадолго <w>занял</w> детей веселой игрой."
                            }
                        },
                        {
                            "text": "Met alle examens voor de boeg werden de studenten flink <w>beziggehouden</w>.",
                            "target_lang_translations": {
                                "en": "With all the exams ahead, the students were <w>kept</w> very <w>busy</w>.",
                                "ru": "С предстоящими экзаменами студенты были сильно <w>заняты</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "vermaken",
                        "afleiden",
                        "aan het werk houden"
                    ],
                    "antonyms": [
                        "vervelen"
                    ],
                    "common_phrases": [
                        "kinderen bezighouden",
                        "iemand even bezighouden"
                    ],
                    "target_lang_definitions": {
                        "en": "To give someone something to do or to entertain them so they do not get bored.",
                        "ru": "Давать кому-то занятие или развлекать, чтобы человек не скучал."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "keep busy",
                                "target_lang_sense_clarification": "common, general"
                            },
                            {
                                "target_lang_word": "occupy",
                                "target_lang_sense_clarification": "to hold attention"
                            },
                            {
                                "target_lang_word": "entertain",
                                "target_lang_sense_clarification": "to amuse"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "занимать",
                                "target_lang_sense_clarification": "давать дело, отвлекать внимание"
                            },
                            {
                                "target_lang_word": "развлекать",
                                "target_lang_sense_clarification": "веселить, не давать скучать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d12f5d4-5c63-31a0-afbe-850f9e6e818b",
                    "sense_definition": "De gedachten of aandacht van iemand in beslag nemen; iemand mentaal niet loslaten.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vraag of ik de juiste keuze had gemaakt, bleef mij dagenlang <w>bezighouden</w>.",
                            "target_lang_translations": {
                                "en": "The question of whether I had made the right choice continued to <w>preoccupy</w> me for days.",
                                "ru": "Вопрос о том, сделал ли я правильный выбор, продолжал <w>беспокоить</w> меня днями."
                            }
                        },
                        {
                            "text": "Dit ingewikkelde probleem <w>houdt</w> de wetenschappers al jaren <w>bezig</w>.",
                            "target_lang_translations": {
                                "en": "This complicated problem has <w>occupied</w> the scientists for years.",
                                "ru": "Эта сложная проблема <w>занимает</w> ученых уже много лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "in beslag nemen",
                        "preoccupëren"
                    ],
                    "antonyms": [
                        "loslaten"
                    ],
                    "common_phrases": [
                        "blijven bezighouden",
                        "de gemoederen bezighouden"
                    ],
                    "target_lang_definitions": {
                        "en": "To take up someone's thoughts or attention; to weigh on one's mind.",
                        "ru": "Завладевать мыслями или вниманием кого-либо; не отпускать ментально."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "preoccupy",
                                "target_lang_sense_clarification": "mental state"
                            },
                            {
                                "target_lang_word": "occupy",
                                "target_lang_sense_clarification": "to take up thoughts/time"
                            },
                            {
                                "target_lang_word": "concern",
                                "target_lang_sense_clarification": "to cause worry or interest"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "волновать",
                                "target_lang_sense_clarification": "тревожить, вызывать беспокойство"
                            },
                            {
                                "target_lang_word": "занимать",
                                "target_lang_sense_clarification": "поглощать мысли или внимание"
                            },
                            {
                                "target_lang_word": "беспокоить",
                                "target_lang_sense_clarification": "не давать покоя"
                            }
                        ]
                    }
                }
            ]
        }

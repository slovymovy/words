    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0e24caa4-8c6a-39dc-8935-3403848e47f1",
                    "sense_definition": "A spoken or written remark expressing an opinion or reaction.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "remark_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have no <w>comment</w> on that.",
                            "target_lang_translations": {
                                "ru": "У меня нет <w>комментариев</w> по этому поводу."
                            }
                        },
                        {
                            "text": "Please leave a <w>comment</w> below if you enjoyed the video.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, оставьте <w>комментарий</w> ниже, если вам понравилось видео."
                            }
                        }
                    ],
                    "synonyms": [
                        "rheme",
                        "rheme"
                    ],
                    "common_phrases": [
                        "no comment",
                        "make a comment",
                        "leave a comment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высказывание или письменная заметка, выражающая мнение или реакцию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комментарий",
                                "target_lang_sense_clarification": "мнение или пояснение"
                            },
                            {
                                "target_lang_word": "замечание",
                                "target_lang_sense_clarification": "краткое суждение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "707cb2b7-34a2-31b7-bbe6-b06ddf7e1d11",
                    "sense_definition": "A text note in computer code that is ignored by the program but helps humans understand it.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "computing_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The programmer added a <w>comment</w> to explain the complex function.",
                            "target_lang_translations": {
                                "ru": "Программист добавил <w>комментарий</w>, чтобы объяснить сложную функцию."
                            }
                        },
                        {
                            "text": "This <w>comment</w> helps other developers understand the code logic.",
                            "target_lang_translations": {
                                "ru": "Этот <w>комментарий</w> помогает другим разработчикам понять логику кода."
                            }
                        }
                    ],
                    "common_phrases": [
                        "code comment",
                        "add a comment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Текстовая заметка в компьютерном коде, которая игнорируется программой, но помогает людям понять его."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комментарий",
                                "target_lang_sense_clarification": "в программировании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "867cdd27-b0f5-3ae4-b614-e4c7bc934e23",
                    "sense_definition": "The action of expressing an opinion or explanation.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "act_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The controversial decision passed without <w>comment</w> from the board.",

                    "sense_id": "bff4fbd9-99aa-37e5-859f-803615cdbd18",
                    "sense_definition": "Op zo'n manier dat iets of iemand zijn evenwicht verliest en valt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De harde wind blies de oude schutting <w>omver</w>.",
                            "target_lang_translations": {
                                "en": "The strong wind blew the old fence <w>over</w>.",
                                "ru": "Сильный ветер <w>повалил</w> старый забор."
                            }
                        },
                        {
                            "text": "Hij liep in zijn haast bijna een fietser <w>omver</w>.",
                            "target_lang_translations": {
                                "en": "In his haste, he almost knocked a cyclist <w>over</w>.",
                                "ru": "В спешке он чуть не <w>сбил</w> велосипедиста."
                            }
                        }
                    ],
                    "synonyms": [
                        "ondersteboven",
                        "ondersteboven"
                    ],
                    "common_phrases": [

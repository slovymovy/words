                {
                    "sense_id": "16f9d23a-9576-31cc-9196-6925b84dbdc5",
                    "sense_definition": "Een driedimensionaal kunstwerk dat een persoon, dier of object voorstelt, vaak gemaakt van steen, brons of hout.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_art",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het midden van het plein staat een groot bronzen <w>beeld</w>.",
                            "target_lang_translations": {
                                "en": "In the middle of the square stands a large bronze <w>statue</w>.",
                                "ru": "В центре площади стоит большая бронзовая <w>статуя</w>."
                            }
                        },
                        {
                            "text": "De kunstenaar heeft maanden gewerkt aan dit prachtige <w>beeld</w>.",
                            "target_lang_translations": {
                                "en": "The artist has worked for months on this beautiful <w>sculpture</w>.",
                                "ru": "Художник месяцами работал над этой прекрасной <w>скульптурой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "standbeeld",
                        "sculptuur",
                        "beeldhouwwerk"
                    ],
                    "common_phrases": [
                        "een bronzen beeld",
                        "een beeld maken"
                    ],
                    "target_lang_definitions": {
                        "en": "A three-dimensional work of art representing a person, animal, or object, often made of stone, bronze, or wood.",
                        "ru": "Трёхмерное произведение искусства, изображающее человека, животное или объект, часто сделанное из камня, бронзы или дерева."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "statue",
                                "target_lang_sense_clarification": "representation of a figure"
                            },
                            {
                                "target_lang_word": "sculpture",
                                "target_lang_sense_clarification": "artistic work"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "статуя",
                                "target_lang_sense_clarification": "изображение фигуры"
                            },
                            {
                                "target_lang_word": "скульптура",
                                "target_lang_sense_clarification": "художественное произведение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c3bdd9b-5093-35d6-957b-30f1099bf6a6",
                    "sense_definition": "Een zichtbare weergave op een vlak, zoals op een televisie, computerscherm, foto of schilderij.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "visual_representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>beeld</w> van de televisie is plotseling zwart geworden.",
                            "target_lang_translations": {
                                "en": "The television <w>picture</w> has suddenly turned black.",
                                "ru": "<w>Изображение</w> на телевизоре внезапно стало черным."
                            }
                        },
                        {
                            "text": "Deze nieuwe camera zorgt voor een heel scherp <w>beeld</w>.",
                            "target_lang_translations": {
                                "en": "This new camera provides a very sharp <w>image</w>.",
                                "ru": "Эта новая камера дает очень четкое <w>изображение</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "visualisatie",
                        "voorstelling"
                    ],
                    "common_phrases": [
                        "scherp beeld",
                        "beeld en geluid"
                    ],
                    "target_lang_definitions": {

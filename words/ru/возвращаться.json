    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1682f296-c39c-3727-997e-ed22b36a75be",
                    "sense_definition": "Приходить или приезжать обратно в то место, откуда ушёл или уехал.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый вечер папа <возвращается> с работы домой в шесть часов."
                        },
                        {
                            "text": "Мы планируем <возвращаться> из отпуска в следующее воскресенье."
                        }
                    ],
                    "synonyms": [
                        "приходить назад",
                        "приезжать назад"
                    ],
                    "antonyms": [
                        "уходить",
                        "уезжать",
                        "покидать"
                    ],
                    "common_phrases": [
                        "возвращаться домой",
                        "возвращаться назад",
                        "возвращаться поздно",
                        "возвращаться с работы"
                    ]
                },
                {
                    "sense_id": "09bc211e-689c-382a-94dd-a1e35ca15d8a",
                    "sense_definition": "Снова начинать обсуждать, обдумывать или рассматривать какую-либо тему или вопрос.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_return",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Давайте не будем <возвращаться> к этому неприятному разговору."
                        },
                        {
                            "text": "Учитель постоянно <возвращается> к правилам грамматики, чтобы мы их запомнили."
                        }
                    ],
                    "synonyms": [
                        "возобновлять"
                    ],
                    "common_phrases": [
                        "возвращаться к теме",
                        "возвращаться к вопросу",
                        "возвращаться к разговору"
                    ]
                },
                {
                    "sense_id": "63c85168-29a6-376c-8e56-9152064cc6ca",
                    "sense_definition": "Снова оказываться в прежнем состоянии, ситуации или приступать к прежним занятиям.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_return",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После долгих праздников всегда трудно <возвращаться> к работе."
                        },
                        {
                            "text": "Город постепенно <возвращается> к нормальной жизни после сильного урагана."
                        }
                    ],
                    "common_phrases": [
                        "возвращаться к жизни",
                        "возвращаться к работе",
                        "возвращаться в строй",
                        "возвращаться к реальности"
                    ]
                },
                {
                    "sense_id": "297b002e-ff89-36e4-be26-be3d2ec1461e",
                    "sense_definition": "Передаваться обратно прежнему владельцу или отправителю (о вещах, письмах, деньгах).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если адрес указан неверно, заказное письмо <возвращается> отправителю."
                        },
                        {
                            "text": "Потерянные в аэропорту вещи редко <возвращаются> к своим хозяевам."
                        }
                    ],
                    "common_phrases": [
                        "письмо возвращается",
                        "деньги возвращаются",
                        "всё возвращается"
                    ]
                }
            ]
        }
    ]
}
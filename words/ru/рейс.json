                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A journey back and forth for the purpose of transporting items or fetching something."
                    },
                    "translations": {

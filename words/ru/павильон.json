            "pos": "noun",
            "senses": [
                {
                    "sense_id": "439b9b94-e952-36e4-949f-e30fad09f166",
                    "sense_definition": "Небольшая отдельная постройка или изолированное помещение в торговом центре, оборудованные для торговли.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я купил свежие овощи в <w>павильоне</w> рядом с домом.",
                            "target_lang_translations": {
                                "en": "I bought fresh vegetables at a <w>stall</w> near my house."
                            }
                        },
                        {
                            "text": "Торговый <w>павильон</w> закрывается на перерыв в два часа дня.",
                            "target_lang_translations": {
                                "en": "The shopping <w>pavilion</w> closes for a break at two in the afternoon."
                            }
                        }
                    ],
                    "synonyms": [
                        "киоск",
                        "ларёк",
                        "магазин"
                    ],
                    "common_phrases": [
                        "торговый павильон",
                        "цветочный павильон",
                        "павильон на рынке"
                    ],
                    "target_lang_definitions": {
                        "en": "A small separate building or an isolated section in a shopping center, equipped for retail trade."
                    },
                    "translations": {

                {
                    "sense_id": "c254c64e-efd5-3efe-a7ec-d87680a8ca59",
                    "sense_definition": "Iets zichtbaar maken zodat anderen het kunnen zien.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "show_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <toonde> zijn nieuwe fiets trots aan zijn vrienden.",
                            "target_lang_translations": {
                                "en": "He proudly <showed> his new bike to his friends.",
                                "ru": "Он с гордостью <показал> свой новый велосипед друзьям."
                            }
                        },
                        {
                            "text": "Kunt u mij uw legitimatiebewijs even <tonen>?",
                            "target_lang_translations": {
                                "en": "Could you <show> me your ID for a moment?",
                                "ru": "Не могли бы вы <предъявить> мне ваше удостоверение личности?"

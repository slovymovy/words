    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "785337b9-d1f2-3b0b-9a2f-ce5347f89733",
                    "sense_definition": "Мелкие капли воды, оседающие из влажного воздуха на землю и растения при охлаждении, обычно утром или вечером.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Утром на зелёной траве блестела холодная <w>роса</w>.",
                            "target_lang_translations": {
                                "en": "In the morning, cold <w>dew</w> glistened on the green grass."
                            }
                        },
                        {
                            "text": "Летом мы часто ходили босиком по утренней <w>росе</w>.",
                            "target_lang_translations": {
                                "en": "In summer, we often walked barefoot on the morning <w>dew</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "утренняя роса",
                        "выпала роса",
                        "капли росы",
                        "ходить по росе"
                    ],
                    "target_lang_definitions": {
                        "en": "Small drops of water settling from moist air onto the ground and plants upon cooling, usually in the morning or evening."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dew",
                                "target_lang_sense_clarification": "condensed moisture"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72c5e2bb-6ec0-3e76-a33f-a1ce4dbe0495",
                    "sense_definition": "Болезнь растений, при которой листья и стебли покрываются белым налётом.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мучнистая <w>роса</w> может быстро уничтожить весь урожай огурцов.",
                            "target_lang_translations": {
                                "en": "Powdery <w>mildew</w> can quickly destroy the entire cucumber harvest."
                            }
                        },
                        {
                            "text": "Садовник заметил, что на листьях роз появилась мучнистая <w>роса</w>.",
                            "target_lang_translations": {
                                "en": "The gardener noticed that powdery <w>mildew</w> had appeared on the rose leaves."
                            }
                        }
                    ],
                    "common_phrases": [
                        "мучнистая роса",
                        "медвяная роса",
                        "ложная мучнистая роса"
                    ],
                    "target_lang_definitions": {
                        "en": "A plant disease in which leaves and stems become covered with a white coating."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mildew",
                                "target_lang_sense_clarification": "plant disease"
                            }
                        ]
                    }
                }
            ]
        }
    ]

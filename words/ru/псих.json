                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fit of rage",
                                "target_lang_sense_clarification": "a sudden outburst of anger"
                            },
                            {
                                "target_lang_word": "tantrum",
                                "target_lang_sense_clarification": "an uncontrolled outburst of anger and frustration"
                            }

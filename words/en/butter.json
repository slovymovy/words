            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e081bd7e-11db-3944-8a5b-644ae215a5f2",
                    "sense_definition": "A soft, fatty foodstuff made by churning the cream of milk.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_dairy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I like to spread fresh <w>butter</w> on my morning toast.",
                            "target_lang_translations": {
                                "ru": "Я люблю намазывать свежее <w>сливочное масло</w> на утренний тост."
                            }
                        },
                        {
                            "text": "You will need a sharp knife to cut the cold <w>butter</w>.",
                            "target_lang_translations": {
                                "ru": "Вам понадобится острый нож, чтобы отрезать холодное <w>масло</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bread and butter",
                        "melt like butter",
                        "butter knife"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мягкий жирный продукт питания, получаемый путем сбивания сливок из молока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сли́вочное ма́сло",
                                "target_lang_sense_clarification": "молочный продукт"
                            },
                            {
                                "target_lang_word": "ма́сло",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ea33772-165b-3554-8668-f298bf65e932",
                    "sense_definition": "A food paste made from ground nuts, seeds, or fruits, similar in consistency to dairy butter.",

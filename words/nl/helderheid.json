        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "370c67e8-e75b-3ef8-892f-c13a09e08963",
                    "sense_definition": "De intensiteit of felheid van licht, bijvoorbeeld van een lamp of beeldscherm.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "light_intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt de <w>helderheid</w> van je telefoon aanpassen om batterij te besparen.",
                            "target_lang_translations": {
                                "ru": "Вы можете настроить <w>яркость</w> своего телефона, чтобы сэкономить заряд батареи.",
                                "en": "You can adjust the <w>brightness</w> of your phone to save battery."
                            }
                        },
                        {
                            "text": "De <w>helderheid</w> van deze ster is aan periodieke veranderingen onderhevig.",
                            "target_lang_translations": {
                                "ru": "<w>Яркость</w> этой звезды подвержена периодическим изменениям.",
                                "en": "The <w>brightness</w> of this star is subject to periodic changes."
                            }
                        }
                    ],
                    "synonyms": [
                        "lichtsterkte",
                        "felheid",
                        "luminantie"
                    ],
                    "antonyms": [
                        "duisternis"
                    ],
                    "common_phrases": [
                        "helderheid van het beeldscherm",
                        "automatische helderheid",
                        "grote helderheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Интенсивность или яркость света, например, от лампы или экрана.",
                        "en": "The intensity or brilliance of light, for example from a lamp or screen."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "яркость",
                                "target_lang_sense_clarification": "света или экрана"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "brightness",
                                "target_lang_sense_clarification": "of light or screen"
                            },
                            {
                                "target_lang_word": "luminosity",
                                "target_lang_sense_clarification": "technical/astronomical"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cb307a0-01b9-3902-b8f3-18e01dc8cb0b",
                    "sense_definition": "De mate waarin iets duidelijk, begrijpelijk of logisch is; het ontbreken van verwarring.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_clarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De directeur gaf helaas geen <w>helderheid</w> over de toekomst van het bedrijf.",
                            "target_lang_translations": {
                                "ru": "К сожалению, директор не внес <w>ясности</w> относительно будущего компании.",
                                "en": "Unfortunately, the director provided no <w>clarity</w> regarding the future of the company."
                            }
                        },
                        {
                            "text": "Dankzij de <w>helderheid</w> van haar presentatie begreep iedereen het plan direct.",
                            "target_lang_translations": {
                                "ru": "Благодаря <w>ясности</w> её презентации все сразу поняли план.",
                                "en": "Thanks to the <w>clarity</w> of her presentation, everyone understood the plan immediately."
                            }
                        }
                    ],
                    "synonyms": [
                        "duidelijkheid",
                        "inzichtelijkheid"
                    ],
                    "antonyms": [
                        "onduidelijkheid",
                        "vaagheid",
                        "verwarring"
                    ],
                    "common_phrases": [
                        "helderheid verschaffen",
                        "helderheid scheppen",
                        "voor helderheid zorgen",
                        "helderheid van geest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Степень понятности, логичности или отсутствия путаницы в чем-либо.",
                        "en": "The degree to which something is clear, understandable, or logical; the absence of confusion."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ясность",
                                "target_lang_sense_clarification": "понимания или ситуации"
                            },
                            {
                                "target_lang_word": "чёткость",
                                "target_lang_sense_clarification": "изложения или мыслей"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "clarity",
                                "target_lang_sense_clarification": "of understanding or situation"
                            },
                            {
                                "target_lang_word": "lucidity",
                                "target_lang_sense_clarification": "of thought or expression"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83af4885-fd2a-394a-af06-64a9078be016",
                    "sense_definition": "De eigenschap van doorzichtig zijn of een scherp zicht bieden, bijvoorbeeld van de lucht of water.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "visual_transparency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>helderheid</w> was op deze dag bijzonder groot en daardoor vormden de besneeuwde bergen een prachtig schouwspel.",
                            "target_lang_translations": {
                                "ru": "<w>Прозрачность</w> воздуха в этот день была особенно высокой, и поэтому заснеженные горы представляли собой прекрасное зрелище.",
                                "en": "The <w>clarity</w> was particularly high on this day, and as a result, the snowy mountains formed a beautiful spectacle."
                            }
                        },
                        {
                            "text": "Duikers waarderen de uitzonderlijke <w>helderheid</w> van de zee rond het tropische eiland.",
                            "target_lang_translations": {
                                "ru": "Дайверы ценят исключительную <w>прозрачность</w> моря вокруг тропического острова.",
                                "en": "Divers appreciate the exceptional <w>clarity</w> of the sea around the tropical island."
                            }
                        }
                    ],
                    "synonyms": [
                        "doorzichtigheid",
                        "transparantie",
                        "klaarheid"
                    ],
                    "antonyms": [
                        "troebelheid"
                    ],
                    "common_phrases": [
                        "helderheid van het water",
                        "helderheid van de lucht"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство прозрачности или обеспечения хорошей видимости, например, воздуха или воды.",
                        "en": "The quality of being transparent or offering a sharp view, for example of the sky or water."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прозрачность",
                                "target_lang_sense_clarification": "воды или воздуха"
                            },
                            {
                                "target_lang_word": "чистота",
                                "target_lang_sense_clarification": "неба или воды"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "clarity",
                                "target_lang_sense_clarification": "of water or air"
                            },
                            {
                                "target_lang_word": "clearness",
                                "target_lang_sense_clarification": "visual transparency"
                            },
                            {
                                "target_lang_word": "transparency",
                                "target_lang_sense_clarification": "physical property"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5bc45b34-c061-3eff-ae73-08a08e2597d0",
                    "sense_definition": "De zuiverheid van een klank of geluid waardoor deze goed hoorbaar en verstaanbaar is.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zangeres staat internationaal bekend om de <w>helderheid</w> van haar stem.",
                            "target_lang_translations": {
                                "ru": "Певица известна во всем мире <w>чистотой</w> своего голоса.",
                                "en": "The singer is internationally known for the <w>clarity</w> of her voice."
                            }
                        },
                        {
                            "text": "Bij het kopen van dure luidsprekers is de <w>helderheid</w> van het geluid essentieel.",
                            "target_lang_translations": {
                                "ru": "При покупке дорогих колонок <w>чистота</w> звука имеет существенное значение.",
                                "en": "When buying expensive speakers, the <w>clarity</w> of the sound is essential."
                            }
                        }
                    ],
                    "synonyms": [
                        "zuiverheid"
                    ],
                    "common_phrases": [
                        "helderheid van klank",
                        "helderheid van stem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чистота звука или тона, делающая его хорошо слышимым и разборчивым.",
                        "en": "The purity of a sound or tone, making it clearly audible and intelligible."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чистота",
                                "target_lang_sense_clarification": "звука или голоса"
                            },
                            {
                                "target_lang_word": "звонкость",
                                "target_lang_sense_clarification": "голоса"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "clarity",
                                "target_lang_sense_clarification": "of sound or voice"
                            },
                            {
                                "target_lang_word": "purity",
                                "target_lang_sense_clarification": "of tone"
                            }
                        ]
                    }
                }
            ]
        }

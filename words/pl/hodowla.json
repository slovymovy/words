    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fabd961f-7c2e-3c93-aba2-4b6ff222ad24",
                    "sense_definition": "Planowa opieka nad zwierzętami lub roślinami w celu ich rozmnażania, wzrostu i ulepszania cech dziedzicznych.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego rodzina od wielu pokoleń zajmuje się <w>hodowlą</w> koni wyścigowych.",
                            "target_lang_translations": {
                                "ru": "Его семья уже много поколений занимается <w>разведением</w> скаковых лошадей."
                            }
                        },
                        {
                            "text": "Ekologiczna <w>hodowla</w> zwierząt wymaga spełnienia wielu surowych norm.",
                            "target_lang_translations": {
                                "ru": "Экологическое <w>разведение</w> животных требует соблюдения множества строгих норм."
                            }
                        }
                    ],
                    "synonyms": [
                        "chów"
                    ],
                    "common_phrases": [
                        "hodowla zwierząt",
                        "hodowla roślin",
                        "hodowla bydła",
                        "domowa hodowla"
                    ],
                    "target_lang_definitions": {
                        "ru": "Планомерный уход за животными или растениями с целью их размножения, роста и улучшения наследственных качеств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разведение",
                                "target_lang_sense_clarification": "размножение животных"
                            },
                            {
                                "target_lang_word": "выращивание",
                                "target_lang_sense_clarification": "уход за растениями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ea3cb8b-7cf7-3e5f-97f0-dc7d2b729c01",
                    "sense_definition": "Miejsce, gospodarstwo lub przedsiębiorstwo, w którym trzyma się i rozmnaża zwierzęta lub rośliny.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Odwiedziliśmy wczoraj dużą <w>hodowlę</w> psów rasowych pod Warszawą.",
                            "target_lang_translations": {
                                "ru": "Вчера мы посетили большой <w>питомник</w> породистых собак под Варшавой."
                            }
                        },
                        {
                            "text": "Ta <w>hodowla</w> jest znana w całym kraju ze zdrowych i silnych zwierząt.",
                            "target_lang_translations": {
                                "ru": "Этот <w>питомник</w> известен по всей стране своими здоровыми и сильными животными."
                            }
                        }
                    ],
                    "common_phrases": [
                        "założyć hodowlę",
                        "właściciel hodowli",
                        "zarejestrowana hodowla",
                        "hodowla psów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, хозяйство или предприятие, где содержат и разводят животных или растения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "питомник",
                                "target_lang_sense_clarification": "место разведения собак, кошек или растений"
                            },
                            {
                                "target_lang_word": "ферма",
                                "target_lang_sense_clarification": "хозяйство для разведения скота"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a401a8c1-e11e-3c82-af8a-f07e1589f6b6",
                    "sense_definition": "Laboratoryjne namnażanie bakterii, grzybów, komórek lub tkanek na specjalnej pożywce.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Laborant przygotował pożywkę niezbędną pod <w>hodowlę</w> bakterii.",
                            "target_lang_translations": {
                                "ru": "Лаборант подготовил питательную среду, необходимую для <w>выращивания</w> бактерий."
                            }
                        },
                        {
                            "text": "Do badań nad nowym lekiem wykorzystywana jest <w>hodowla</w> tkankowa.",
                            "target_lang_translations": {
                                "ru": "Для исследований нового лекарства используется тканевая <w>культура</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kultura"
                    ],
                    "common_phrases": [
                        "hodowla bakterii",
                        "hodowla komórkowa",
                        "hodowla in vitro",
                        "pożywka dla hodowli"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "biological/medical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лабораторное размножение бактерий, грибов, клеток или тканей в специальной питательной среде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "культура",
                                "target_lang_sense_clarification": "биологический термин"
                            },
                            {
                                "target_lang_word": "выращивание",
                                "target_lang_sense_clarification": "лабораторный процесс"
                            }
                        ]
                    }
                }
            ]
        }
    ]

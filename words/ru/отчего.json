    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "84374705-f1f0-3add-a7f2-41da6fefcb41",
                    "sense_definition": "По какой причине, почему",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cause_question",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я не понимаю, <w>отчего</w> ты так расстроен.",
                            "target_lang_translations": {
                                "en": "I don't understand <w>why</w> you are so upset."
                            }
                        },
                        {
                            "text": "<w>Отчего</w> люди не летают так, как птицы?",
                            "target_lang_translations": {
                                "en": "<w>Why</w> don't people fly like birds?"
                            }
                        }
                    ],
                    "synonyms": [
                        "почему"
                    ],
                    "common_phrases": [
                        "отчего же"
                    ],
                    "target_lang_definitions": {
                        "en": "For what reason, why"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "why",
                                "target_lang_sense_clarification": "interrogative adverb asking for a cause or reason"
                            },
                            {
                                "target_lang_word": "for what reason",
                                "target_lang_sense_clarification": "formal interrogative phrase"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e52f562c-1931-30d6-8cfe-0c36699803be",
                    "sense_definition": "Употребляется как союзное слово, присоединяющее придаточные предложения со значением следствия; вследствие чего, поэтому",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "consequence_link",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "По утрам на траве лежал иней, <w>отчего</w> зелень казалась сизой.",
                            "target_lang_translations": {
                                "en": "In the mornings frost lay on the grass, <w>which is why</w> the greenery looked bluish-grey."
                            }
                        },
                        {
                            "text": "Дверь была открыта, <w>отчего</w> в комнате стало очень холодно.",
                            "target_lang_translations": {
                                "en": "The door was open, <w>which is why</w> it got very cold in the room."
                            }
                        }
                    ],
                    "synonyms": [
                        "вследствие чего",
                        "поэтому"
                    ],
                    "target_lang_definitions": {
                        "en": "Used as a conjunctive word connecting subordinate clauses of consequence; as a result of which, therefore"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "which is why",
                                "target_lang_sense_clarification": "phrase linking a cause to a result"
                            },
                            {
                                "target_lang_word": "so",
                                "target_lang_sense_clarification": "conjunction indicating consequence"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9a1f5728-0a93-3b72-b8d1-a647e8e0936e",
                    "sense_definition": "Форма родительного падежа мужского или среднего рода единственного числа прилагательного <w>отчий</w>",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он уехал далеко от <w>отчего</w> дома и редко писал письма.",
                            "target_lang_translations": {
                                "en": "He went far away from his <w>father's</w> house and rarely wrote letters."
                            }
                        },
                        {
                            "text": "Свет <w>отчего</w> края всегда остаётся в памяти человека.",
                            "target_lang_translations": {
                                "en": "The light of one's <w>native</w> land always remains in a person's memory."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "родительный падеж"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Genitive singular masculine or neuter form of the adjective 'otchi' (paternal)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "paternal",
                                "target_lang_sense_clarification": "adjective referring to a father"
                            },
                            {
                                "target_lang_word": "father's",
                                "target_lang_sense_clarification": "possessive form referring to a father"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
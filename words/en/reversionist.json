            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fd5c0333-4efd-35b1-a823-64e3786103de",
                    "sense_definition": "A person who prefers previous patterns of behavior or thought and rejects social or cultural change.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "social_view",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>reversionist</w> argued that society should return to the simpler values of the past."
                        },
                        {
                            "text": "As a staunch <w>reversionist</w>, he refused to adopt the new digital system in his office."
                        }
                    ],
                    "synonyms": [
                        "reactionary",
                        "traditionalist"
                    ],
                    "antonyms": [
                        "progressive",
                        "modernist"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "94d7879e-c7c0-358f-9325-5eb332d7cb78",
                    "sense_definition": "Mający kolor jasnofioletowy lub różowoniebieski, taki jak kwiaty lilaka.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "color_purple",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wiosnę kupiłam sobie nową, zwiewną sukienkę w kolorze <w>liliowym</w>.",
                            "target_lang_translations": {
                                "ru": "Весной я купила себе новое воздушное платье <w>лилового</w> цвета."
                            }
                        },
                        {
                            "text": "Ściany w sypialni mają delikatny, <w>liliowy</w> odcień, który uspokaja.",
                            "target_lang_translations": {
                                "ru": "Стены в спальне имеют нежный <w>лиловый</w> оттенок, который успокаивает."
                            }
                        }
                    ],
                    "synonyms": [
                        "jasnofioletowy",
                        "lila",
                        "lilowy"
                    ],
                    "common_phrases": [
                        "kolor liliowy",
                        "liliowa sukienka",
                        "liliowy odcień"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий светло-фиолетовый или розово-голубой цвет, как цветы сирени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лиловый",
                                "target_lang_sense_clarification": "цвет"
                            },
                            {
                                "target_lang_word": "сиреневый",
                                "target_lang_sense_clarification": "оттенок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ebb4b348-9d3e-3fbe-92f3-6f1b851618d4",
                    "sense_definition": "Dotyczący lilii (rośliny), należący do niej lub z niej zrobiony.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Naturalny olejek <w>liliowy</w> jest cenionym składnikiem wielu luksusowych perfum.",
                            "target_lang_translations": {
                                "ru": "Натуральное <w>лилейное</w> масло является ценным компонентом многих роскошных духов."
                            }
                        },
                        {
                            "text": "Ten intensywny, <w>liliowy</w> zapach unosił się w całym ogrodzie botanicznym.",
                            "target_lang_translations": {
                                "ru": "Этот насыщенный <w>лилейный</w> аромат витал во всём ботаническом саду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "olejek liliowy",
                        "zapach liliowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к лилии (растению), принадлежащий ей или сделанный из неё."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лилейный",
                                "target_lang_sense_clarification": "относящийся к лилии"
                            }
                        ]
                    }
                }
            ]
        }
    ]

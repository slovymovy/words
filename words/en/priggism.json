    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ae0739cd-4a94-31e5-b003-ceea2e098213",
                    "sense_definition": "The quality or state of being priggish; exaggerated propriety and fussiness about trivialities.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His <w>priggism</w> made him unpopular with the more relaxed members of the club.",
                            "target_lang_translations": {
                                "ru": "Его <w>чопорность</w> сделала его непопулярным среди более расслабленных членов клуба."
                            }
                        },
                        {
                            "text": "She could not tolerate the <w>priggism</w> of her neighbors who judged everyone's clothing.",
                            "target_lang_translations": {
                                "ru": "Она не могла терпеть <w>ханжество</w> своих соседей, которые осуждали одежду каждого."
                            }
                        }
                    ],
                    "synonyms": [
                        "priggery",
                        "priggishness"
                    ],
                    "common_phrases": [
                        "moral priggism",
                        "intellectual priggism"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство личности, характеризующееся самодовольным педантизмом, чопорностью и чрезмерным вниманием к соблюдению внешних приличий и правил."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чопорность",
                                "target_lang_sense_clarification": "чрезмерная строгость в поведении и соблюдении приличий"
                            },
                            {
                                "target_lang_word": "педантизм",
                                "target_lang_sense_clarification": "излишняя требовательность к мелочам и формальностям"
                            },
                            {
                                "target_lang_word": "ханжество",
                                "target_lang_sense_clarification": "лицемерная добродетельность (в контексте морального превосходства)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c356809-d735-3d93-8833-26b19ea4a32f",
                    "sense_definition": "Roguery or thievery.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "crime",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The historical novel described the <w>priggism</w> rampant in the city's slums.",
                            "target_lang_translations": {
                                "ru": "В историческом романе описывалось <w>воровство</w>, процветавшее в городских трущобах."
                            }
                        },
                        {
                            "text": "He decided to leave his life of <w>priggism</w> behind and find honest work.",
                            "target_lang_translations": {
                                "ru": "Он решил оставить свою жизнь, полную <w>жульничества</w>, позади и найти честную работу."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "referring to thievery"
                        },
                        {
                            "trait_type": "slang",
                            "comment": "thieves' cant"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Воровство, мошенничество или плутовство (устаревшее значение, сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воровство",
                                "target_lang_sense_clarification": "хищение чужого имущества"
                            },
                            {
                                "target_lang_word": "жульничество",
                                "target_lang_sense_clarification": "плутовство, мошенничество"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "8fb3de98-570e-3e36-9065-d93d52db740c",
                    "sense_definition": "De benadrukte vorm van het persoonlijk voornaamwoord voor de tweede persoon enkelvoud (jij), gebruikt als lijdend of meewerkend voorwerp en na voorzetsels.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "pronoun_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb dit cadeau speciaal voor <w>jou</w> gekocht.",
                            "target_lang_translations": {
                                "en": "I bought this gift especially for <w>you</w>.",
                                "ru": "Я купил этот подарок специально для <w>тебя</w>."
                            }
                        },
                        {
                            "text": "Wil jij dat ik met <w>jou</w> meega naar de dokter?",
                            "target_lang_translations": {
                                "en": "Do you want me to go with <w>you</w> to the doctor?",
                                "ru": "Ты хочешь, чтобы я пошел с <w>тобой</w> к врачу?"
                            }
                        }
                    ],
                    "synonyms": [
                        "je"
                    ],
                    "antonyms": [
                        "mij"
                    ],
                    "common_phrases": [
                        "speciaal voor jou",
                        "ik hou van jou",
                        "met jou",
                        "aan jou"
                    ],
                    "target_lang_definitions": {
                        "en": "The emphasized form of the personal pronoun for the second person singular (you), used as a direct or indirect object and after prepositions.",
                        "ru": "Ударная форма личного местоимения второго лица единственного числа (ты), используемая в качестве дополнения или после предлогов."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "you",
                                "target_lang_sense_clarification": "object pronoun"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "тебя",
                                "target_lang_sense_clarification": "винительный или родительный падеж"
                            },
                            {
                                "target_lang_word": "тебе",
                                "target_lang_sense_clarification": "дательный падеж"
                            },
                            {
                                "target_lang_word": "тобой",
                                "target_lang_sense_clarification": "творительный падеж"
                            }
                        ]
                    }
                }
            ]
        },
        {

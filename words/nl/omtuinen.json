                },
                {
                    "sense_id": "a043ee31-ec41-3ce6-b0aa-48a0cd7b0d8d",
                    "sense_definition": "Afschermen of beschermen tegen invloeden van buitenaf.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "enclosure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bezorgde ouders wilden hun kinderen <w>omtuinen</w> tegen de gevaren van de stad.",
                            "target_lang_translations": {
                                "en": "The worried parents wanted to <w>shield</w> their children from the dangers of the city.",
                                "ru": "Обеспокоенные родители хотели <w>оградить</w> своих детей от опасностей города."
                            }
                        },
                        {
                            "text": "De gemeenschap probeerde zich te <w>omtuinen</w> van de moderne wereld.",
                            "target_lang_translations": {
                                "en": "The community tried to <w>insulate</w> itself from the modern world.",
                                "ru": "Община пыталась <w>отгородиться</w> от современного мира."
                            }
                        }
                    ],
                    "synonyms": [
                        "afschermen",
                        "beschermen",
                        "beveiligen"
                    ],
                    "antonyms": [
                        "blootstellen"
                    ],
                    "common_phrases": [
                        "zich laten omtuinen"
                    ],
                    "target_lang_definitions": {
                        "en": "To screen off or protect against outside influences.",
                        "ru": "Ограждать или защищать от внешних воздействий."
                    },
                    "translations": {
                        "en": [
                            {

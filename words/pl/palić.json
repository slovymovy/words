    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9e1d7e16-bc49-326e-98b5-ec2543670830",
                    "sense_definition": "Wdychać i wydychać dym z wyrobów tytoniowych (np. papierosów, cygar, fajki), zazwyczaj jako nałóg.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "smoking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek <pali> fajkę każdego wieczoru na tarasie.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка <курит> трубку каждый вечер на террасе."
                            }
                        },
                        {
                            "text": "Niestety <palę> papierosy od czasów liceum i trudno mi rzucić.",
                            "target_lang_translations": {
                                "ru": "К сожалению, я <курю> сигареты со времен лицея, и мне трудно бросить."
                            }
                        }
                    ],
                    "synonyms": [
                        "kurzyć",
                        "jarać",
                        "kopcić",
                        "ćmić",
                        "fajczyć"
                    ],
                    "common_phrases": [
                        "palić papierosy",
                        "zakaz palenia",
                        "rzucić palenie",
                        "palić jak smok"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вдыхать и выдыхать дым табачных изделий (например, сигарет, сигар, трубки), обычно как вредная привычка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "курить",
                                "target_lang_sense_clarification": "вдыхать дым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e68cac7e-11dc-30e1-ae11-0fdaa6dfca26",
                    "sense_definition": "Rozniecać i utrzymywać ogień (np. w piecu, w kominku) w celu ogrzania pomieszczenia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "heating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą codziennie <palimy> w kominku, żeby było przytulnie.",
                            "target_lang_translations": {
                                "ru": "Зимой мы каждый день <топим> камин, чтобы было уютно."
                            }
                        },
                        {
                            "text": "Moi sąsiedzi <palą> węglem, co powoduje dużo dymu.",
                            "target_lang_translations": {
                                "ru": "Мои соседи <топят> углем, из-за чего много дыма."
                            }
                        }
                    ],
                    "antonyms": [
                        "gasić"
                    ],
                    "common_phrases": [
                        "palić w piecu",
                        "palić w kominku",
                        "palić drewnem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разводить и поддерживать огонь (например, в печи, в камине) для обогрева помещения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "топить",
                                "target_lang_sense_clarification": "печь или камин"
                            },
                            {
                                "target_lang_word": "жечь",
                                "target_lang_sense_clarification": "дрова или уголь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9adc7168-d9fe-3f70-8cad-cdd6e168b178",
                    "sense_definition": "Zużywać paliwo (o pojazdach i silnikach spalinowych).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fuel_consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary samochód <pali> bardzo dużo benzyny w mieście.",
                            "target_lang_translations": {
                                "ru": "Эта старая машина <расходует> очень много бензина в городе."
                            }
                        },
                        {
                            "text": "Ile <pali> twoje nowe auto na autostradzie?",
                            "target_lang_translations": {
                                "ru": "Сколько <потребляет> твоя новая машина на трассе?"
                            }
                        }
                    ],
                    "synonyms": [
                        "spalać"
                    ],
                    "common_phrases": [
                        "dużo palić",
                        "mało palić",
                        "ile pali"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расходовать топливо (о транспортных средствах и двигателях внутреннего сгорания)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расходовать",
                                "target_lang_sense_clarification": "тратить топливо"
                            },
                            {
                                "target_lang_word": "потреблять",
                                "target_lang_sense_clarification": "использовать ресурс"
                            },
                            {
                                "target_lang_word": "есть",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d3d4647-06cf-3bb7-ab1c-999820d18ec1",
                    "sense_definition": "Być objętym ogniem, płonąć (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "burning_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary drewniany dom <palił> się przez całą noc.",
                            "target_lang_translations": {
                                "ru": "Старый деревянный дом <горел> всю ночь."
                            }
                        },
                        {
                            "text": "Uważaj w kuchni, bo ścierka <pali> się od kuchenki!",
                            "target_lang_translations": {
                                "ru": "Осторожно на кухне, тряпка <загорелась> от плиты!"
                            }
                        }
                    ],
                    "synonyms": [
                        "płonąć",
                        "goreć",
                        "jarać się"
                    ],
                    "common_phrases": [
                        "palić się",
                        "dom się pali",
                        "las się pali"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть охваченным огнем, пылать (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гореть",
                                "target_lang_sense_clarification": "быть в огне"
                            },
                            {
                                "target_lang_word": "пылать",
                                "target_lang_sense_clarification": "сильно гореть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a566870e-46d2-30cd-9917-2e38c975afe6",
                    "sense_definition": "Powodować uczucie gorąca lub ból przypominający oparzenie (np. przez słońce, pikantne jedzenie, pokrzywę).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słońce pustyni <paliło> nas niemiłosiernie podczas wycieczki.",
                            "target_lang_translations": {
                                "ru": "Солнце пустыни немилосердно <палило> нас во время экскурсии."
                            }
                        },
                        {
                            "text": "Ten ostry sos meksykański strasznie <pali> w język.",
                            "target_lang_translations": {
                                "ru": "Этот острый мексиканский соус ужасно <жжёт> язык."
                            }
                        }
                    ],
                    "synonyms": [
                        "piec",
                        "przypiekać",
                        "prażyć"
                    ],
                    "common_phrases": [
                        "palić w gardle",
                        "słońce pali",
                        "wstyd pali"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать ощущение жара или боль, напоминающую ожог (например, от солнца, острой еды, крапивы)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жечь",
                                "target_lang_sense_clarification": "вызывать жжение"
                            },
                            {
                                "target_lang_word": "печь",
                                "target_lang_sense_clarification": "припекать"
                            },
                            {
                                "target_lang_word": "палить",
                                "target_lang_sense_clarification": "о солнце"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e4adb25-d8fc-39e7-bfb0-99108e379bc8",
                    "sense_definition": "Używać źródła światła; mieć włączone światło.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "light",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziadek jeszcze nie śpi, widać że w jego pokoju <pali> się światło.",
                            "target_lang_translations": {
                                "ru": "Дедушка еще не спит, видно, что в его комнате <горит> свет."
                            }
                        },
                        {
                            "text": "Nie <pal> niepotrzebnie lampy, jeśli wychodzisz z pokoju.",
                            "target_lang_translations": {
                                "ru": "Не <жги> зря лампу, если выходишь из комнаты."
                            }
                        }
                    ],
                    "synonyms": [
                        "świecić się"
                    ],
                    "antonyms": [
                        "gasić"
                    ],
                    "common_phrases": [
                        "palić światło",
                        "palić lampę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often replaced by 'świecić' or 'włączać' in formal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать источник света; держать свет включенным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жечь",
                                "target_lang_sense_clarification": "тратить электричество"
                            },
                            {
                                "target_lang_word": "гореть",
                                "target_lang_sense_clarification": "быть включенным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f64adf4-1941-3fdb-8823-0ec73908240a",
                    "sense_definition": "Niszczyć coś całkowicie za pomocą ognia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnicy nie powinni <palić> suchych traw na łąkach wiosną.",
                            "target_lang_translations": {
                                "ru": "Фермеры не должны <жечь> сухую траву на лугах весной."
                            }
                        },
                        {
                            "text": "Wrogie wojska <paliły> i niszczyły wszystkie wioski na swojej drodze.",
                            "target_lang_translations": {
                                "ru": "Вражеские войска <жгли> и уничтожали все деревни на своем пути."
                            }
                        }
                    ],
                    "synonyms": [
                        "spalać"
                    ],
                    "common_phrases": [
                        "palić mosty",
                        "palić śmieci"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью уничтожать что-либо с помощью огня."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сжигать",
                                "target_lang_sense_clarification": "уничтожать огнем"
                            },
                            {
                                "target_lang_word": "жечь",
                                "target_lang_sense_clarification": "предавать огню"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef5a853d-0365-3422-b70d-d1c3a83ad470",
                    "sense_definition": "Odczuwać bardzo silne emocje, np. niecierpliwość, chęć zrobienia czegoś lub wstyd (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowi pracownicy <palili> się do pracy i chcieli pokazać swoje umiejętności.",
                            "target_lang_translations": {
                                "ru": "Новые сотрудники <горели> желанием работать и хотели показать свои навыки."
                            }
                        },
                        {
                            "text": "Chłopak <palił> się ze wstydu, gdy nauczyciel głośno go upomniał.",
                            "target_lang_translations": {
                                "ru": "Мальчик <сгорал> со стыда, когда учитель громко сделал ему замечание."
                            }
                        }
                    ],
                    "synonyms": [
                        "rumienić się"
                    ],
                    "common_phrases": [
                        "palić się do roboty",
                        "palić się ze wstydu",
                        "robota pali się w rękach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать очень сильные эмоции, например, нетерпение, желание сделать что-то или стыд."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гореть",
                                "target_lang_sense_clarification": "от нетерпения или желания"
                            },
                            {
                                "target_lang_word": "сгорать",
                                "target_lang_sense_clarification": "от стыда"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
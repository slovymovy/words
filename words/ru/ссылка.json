    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "eca819b5-371a-337b-8e83-7f2f42ce69f8",
                    "sense_definition": "Адрес веб-страницы или элемента в интернете, позволяющий перейти к нему при нажатии; гиперссылка.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Друг прислал мне <ссылку> на интересное видео в интернете.",
                            "target_lang_translations": {
                                "en": "A friend sent me a <link> to an interesting video on the internet."
                            }
                        },
                        {
                            "text": "Если нажать на эту <ссылку>, откроется страница с фотографиями.",
                            "target_lang_translations": {
                                "en": "If you click on this <link>, a page with photos will open."
                            }
                        }
                    ],
                    "common_phrases": [
                        "перейти по ссылке",
                        "отправить ссылку",
                        "активная ссылка",
                        "битая ссылка"
                    ],
                    "target_lang_definitions": {
                        "en": "The address of a web page or item on the internet that allows you to navigate to it when clicked; a hyperlink."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "link",
                                "target_lang_sense_clarification": "to a webpage"
                            },
                            {
                                "target_lang_word": "hyperlink",
                                "target_lang_sense_clarification": "computing term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec8d7849-f145-3f49-a0a2-45ce7b94c84e",
                    "sense_definition": "Указание на источник информации, книгу или документ, подтверждающий сказанное или написанное.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В своей курсовой работе студент сделал <ссылку> на известного учёного.",
                            "target_lang_translations": {
                                "en": "In his term paper, the student made a <reference> to a famous scientist."
                            }
                        },
                        {
                            "text": "Внизу страницы есть <ссылка> на словарь, где объясняется это слово.",
                            "target_lang_translations": {
                                "en": "At the bottom of the page, there is a <reference> to a dictionary where this word is explained."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ссылка на источник",
                        "список ссылок",
                        "делать ссылку",
                        "прямая ссылка"
                    ],
                    "target_lang_definitions": {
                        "en": "An indication of a source of information, book, or document confirming what is said or written."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reference",
                                "target_lang_sense_clarification": "to a source"
                            },
                            {
                                "target_lang_word": "citation",
                                "target_lang_sense_clarification": "academic/formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17ae94c7-aa28-34ee-b1e5-c722895a1520",
                    "sense_definition": "Вид уголовного или административного наказания, заключающийся в принудительном переселении человека в отдалённую местность.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Писателя отправили в <ссылку> за его революционные взгляды.",
                            "target_lang_translations": {
                                "en": "The writer was sent into <exile> for his revolutionary views."
                            }
                        },
                        {
                            "text": "После десяти лет жизни в <ссылке> он наконец смог вернуться домой.",
                            "target_lang_translations": {
                                "en": "After ten years of life in <exile>, he was finally able to return home."
                            }
                        }
                    ],
                    "synonyms": [
                        "высылка",
                        "изгнание"
                    ],
                    "common_phrases": [
                        "отправить в ссылку",
                        "вернуться из ссылки",
                        "пожизненная ссылка",
                        "место ссылки"
                    ],
                    "target_lang_definitions": {
                        "en": "A form of criminal or administrative punishment consisting of the forced relocation of a person to a remote area."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exile",
                                "target_lang_sense_clarification": "punishment/banishment"
                            },
                            {
                                "target_lang_word": "banishment",
                                "target_lang_sense_clarification": "act of expelling"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa8ead68-c963-3ada-b2a5-a5cf1d0c8e4c",
                    "sense_definition": "Объект в программировании, который указывает на данные, хранящиеся в другом месте памяти, и позволяет обращаться к ним.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Функция возвращает <ссылку> на созданный в памяти объект.",
                            "target_lang_translations": {
                                "en": "The function returns a <reference> to an object created in memory."
                            }
                        },
                        {
                            "text": "Программист использовал <ссылку> для передачи большого массива данных.",
                            "target_lang_translations": {
                                "en": "The programmer used a <reference> to pass a large array of data."
                            }
                        }
                    ],
                    "common_phrases": [
                        "передача по ссылке",
                        "пустая ссылка",
                        "значение ссылки"
                    ],
                    "target_lang_definitions": {
                        "en": "An object in programming that points to data stored elsewhere in memory and allows access to it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reference",
                                "target_lang_sense_clarification": "programming"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "offensive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Оскорбительное прозвище человека ирландского происхождения."
                    },
                    "translations": {

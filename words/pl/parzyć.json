    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "66071824-f3c2-3dc5-93fe-8fcb82f6a7a3",
                    "sense_definition": "Przygotowywać gorący napój (np. herbatę, kawę, zioła) zalewając suszone liście lub ziarna wrzątkiem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "brewing_drink",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano <parzę> świeżą kawę dla całej rodziny.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я <завариваю> свежий кофе для всей семьи."
                            }
                        },
                        {
                            "text": "Moja babcia zawsze <parzyła> miętę na ból brzucha.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка всегда <заваривала> мяту от боли в животе."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaparzać"
                    ],
                    "common_phrases": [
                        "parzyć herbatę",
                        "parzyć kawę",
                        "świeżo parzona kawa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовить горячий напиток (например, чай, кофе, травы), заливая сушеные листья или зерна кипятком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заваривать",
                                "target_lang_sense_clarification": "чай, кофе, травы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a9df0b0-098e-3098-9ceb-8a71d244055f",
                    "sense_definition": "Sprawiać ból fizyczny lub uszkadzać skórę poprzez działanie wysokiej temperatury.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "heat_pain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uważaj na ten garnek, bo metalowa pokrywka mocno <parzy>.",
                            "target_lang_translations": {
                                "ru": "Осторожно с этой кастрюлей, металлическая крышка сильно <обжигает>."
                            }
                        },
                        {
                            "text": "Gorący piasek na plaży <parzył> nas w stopy, gdy biegliśmy do wody.",
                            "target_lang_translations": {
                                "ru": "Горячий песок на пляже <обжигал> нам ступни, когда мы бежали к воде."
                            }
                        }
                    ],
                    "synonyms": [
                        "oparzać",
                        "zaparzać"
                    ],
                    "antonyms": [
                        "chłodzić",
                        "studzić"
                    ],
                    "common_phrases": [
                        "słońce parzy",
                        "ogień parzy",
                        "parzyć w ręce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причинять физическую боль или повреждать кожу воздействием высокой температуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обжигать",
                                "target_lang_sense_clarification": "причинять ожог"
                            },
                            {
                                "target_lang_word": "жечь",
                                "target_lang_sense_clarification": "вызывать чувство жжения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01d22c23-65f1-39e3-9ba8-4fd0e8b9a179",
                    "sense_definition": "Doznawać oparzenia własnego ciała przez kontakt z czymś gorącym (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "heat_pain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jestem niezdarą, bo często <parzę> się żelazkiem podczas prasowania.",
                            "target_lang_translations": {
                                "ru": "Я неуклюжий, потому что часто <обжигаюсь> утюгом во время глажки."
                            }
                        },
                        {
                            "text": "Kucharz przypadkowo <parzył> się gorącym olejem w pracy.",
                            "target_lang_translations": {
                                "ru": "Повар случайно <обжигался> горячим маслом на работе."
                            }
                        }
                    ],
                    "synonyms": [
                        "oparzać",
                        "zaparzać"
                    ],
                    "common_phrases": [
                        "parzyć się wrzątkiem",
                        "parzyć się pokrzywą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получать ожог тела при контакте с чем-то горячим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обжигаться",
                                "target_lang_sense_clarification": "получать ожог"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "760e7cf7-5dd2-3c06-9ecf-74bc7fb585b8",
                    "sense_definition": "Wywoływać piekący ból przy dotknięciu (o roślinach lub zwierzętach wydzielających drażniące substancje).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "heat_pain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wchodź w te wysokie krzaki, bo rosną tam pokrzywy, które <parzą>.",
                            "target_lang_translations": {
                                "ru": "Не ходи в эти высокие кусты, там растёт крапива, которая <жжётся>."
                            }
                        },
                        {
                            "text": "W tym morzu żyją meduzy, które boleśnie <parzą> pływaków.",
                            "target_lang_translations": {
                                "ru": "В этом море живут медузы, которые больно <жалят> пловцов."
                            }
                        }
                    ],
                    "synonyms": [
                        "naparzać"
                    ],
                    "common_phrases": [
                        "pokrzywa parzy",
                        "meduza parzy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать жгучую боль при прикосновении (о растениях или животных, выделяющих раздражающие вещества)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жалить",
                                "target_lang_sense_clarification": "о медузах, насекомых"
                            },
                            {
                                "target_lang_word": "жечься",
                                "target_lang_sense_clarification": "о крапиве"
                            },
                            {
                                "target_lang_word": "обжигать",
                                "target_lang_sense_clarification": "оставлять ожог"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0991f74c-681d-308c-99fe-1946fbe37763",
                    "sense_definition": "Naciągać smakiem i aromatem pod wpływem gorącej wody (o napoju; używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "brewing_drink",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Herbata musi <parzyć> się przez pięć minut pod przykryciem, żeby była mocna.",
                            "target_lang_translations": {
                                "ru": "Чай должен <завариваться> пять минут под крышкой, чтобы быть крепким."
                            }
                        },
                        {
                            "text": "Zioła <parzyły> się w dzbanku, wydzielając piękny zapach w całej kuchni.",
                            "target_lang_translations": {
                                "ru": "Травы <настаивались> в кувшине, источая прекрасный аромат на всю кухню."
                            }
                        }
                    ],
                    "synonyms": [
                        "naparzać"
                    ],
                    "common_phrases": [
                        "herbata się parzy",
                        "zioła się parzą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Настаиваться, приобретая вкус и аромат под воздействием горячей воды (о напитке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завариваться",
                                "target_lang_sense_clarification": "о чае"
                            },
                            {
                                "target_lang_word": "настаиваться",
                                "target_lang_sense_clarification": "о травах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5427277a-498b-3212-8f82-f80b577a0c92",
                    "sense_definition": "Poddawać żywność krótkotrwałemu działaniu wrzątku lub gorącej wody w celu obróbki.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "culinary_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed obraniem ze skórki pomidory należy krótko <parzyć> we wrzątku.",
                            "target_lang_translations": {
                                "ru": "Перед снятием кожицы помидоры нужно недолго <ошпаривать> в кипятке."
                            }
                        },
                        {
                            "text": "Według tradycyjnego przepisu szynkę trzeba <parzyć> w określonej temperaturze.",
                            "target_lang_translations": {
                                "ru": "По традиционному рецепту ветчину нужно <варить> при определённой температуре."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaparzać",
                        "naparzać"
                    ],
                    "common_phrases": [
                        "parzyć pomidory",
                        "parzyć wędlinę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвергать продукты кратковременному воздействию кипятка или горячей воды для обработки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ошпаривать",
                                "target_lang_sense_clarification": "обдавать кипятком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af4e8527-d5d2-3fe5-a4a5-236e0408b0b1",
                    "sense_definition": "Łączyć się w pary w celu odbycia stosunku płciowego i rozmnażania (o zwierzętach; używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biology_mating",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiele gatunków ptaków <parzy> się tylko raz w roku, zazwyczaj wiosną.",
                            "target_lang_translations": {
                                "ru": "Многие виды птиц <спариваются> только раз в год, обычно весной."
                            }
                        },
                        {
                            "text": "W tym rezerwacie zwierzęta bezpiecznie <parzą> się i wychowują swoje młode.",
                            "target_lang_translations": {
                                "ru": "В этом заповеднике животные безопасно <спариваются> и растят своё потомство."
                            }
                        }
                    ],
                    "common_phrases": [
                        "parzyć się wiosną"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупляться с целью размножения (о животных)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спариваться",
                                "target_lang_sense_clarification": "о животных"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
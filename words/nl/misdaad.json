    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2e202b73-8c8f-3ba9-a7d8-d7a43dcd74a7",
                    "sense_definition": "Een ernstig strafbaar feit dat volgens de wet bestraft wordt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "legal_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moord is een zware <w>misdaad</w>.",
                            "target_lang_translations": {
                                "en": "Murder is a serious <w>crime</w>.",
                                "ru": "Убийство — это тяжкое <w>преступление</w>."
                            }
                        },
                        {
                            "text": "Hij werd gearresteerd voor het plegen van een <w>misdaad</w>.",
                            "target_lang_translations": {
                                "en": "He was arrested for committing a <w>crime</w>.",
                                "ru": "Его арестовали за совершение <w>преступления</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "misdrijf",
                        "delict"
                    ],
                    "common_phrases": [
                        "een misdaad plegen",
                        "zware misdaad"
                    ],
                    "target_lang_definitions": {
                        "en": "A serious punishable act that is penalized according to the law.",
                        "ru": "Серьезное противоправное деяние, наказуемое по закону."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crime",
                                "target_lang_sense_clarification": "an illegal act"
                            },
                            {
                                "target_lang_word": "felony",
                                "target_lang_sense_clarification": "serious crime"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "преступление",
                                "target_lang_sense_clarification": "конкретное нарушение закона"

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9e00ebab-1905-332f-8cdc-506d4a73aa1a",
                    "sense_definition": "Sprawić za pomocą argumentów lub dowodów, że ktoś uwierzy w coś, zmieni zdanie lub zgodzi się coś zrobić.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <w>przekonać</w> rodziców, żeby pozwolili ci jechać na wycieczkę.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>убедить</w> родителей, чтобы они разрешили тебе поехать на экскурсию."
                            }
                        },
                        {
                            "text": "Jego argumenty były silne, ale nie zdołały mnie <w>przekonać</w>.",
                            "target_lang_translations": {
                                "ru": "Его аргументы были сильными, но не смогли меня <w>убедить</w>."
                            }
                        },
                        {
                            "text": "Próbowałem <w>przekonać</w> go do zmiany decyzji, ale był uparty.",
                            "target_lang_translations": {
                                "ru": "Я пытался <w>убедить</w> его изменить решение, но он был упрям."
                            }
                        }
                    ],
                    "synonyms": [
                        "namówić",
                        "skłonić",
                        "udowodnić",
                        "zwerbować"
                    ],
                    "antonyms": [
                        "zniechęcić",
                        "odwieść"
                    ],
                    "common_phrases": [
                        "przekonać kogoś do pomysłu",
                        "przekonać o swojej racji",
                        "dać się przekonać",
                        "trudno go przekonać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить кого-либо поверить во что-то, изменить мнение или согласиться сделать что-то с помощью аргументов или доказательств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убедить",
                                "target_lang_sense_clarification": "заставить поверить или согласиться"
                            },
                            {
                                "target_lang_word": "уговорить",
                                "target_lang_sense_clarification": "склонить к действию уговорами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f336c254-3942-3a4e-a4ba-369c100be5fb",
                    "sense_definition": "Sprawdzić coś osobiście i zyskać pewność co do prawdziwości czegoś lub stanu faktycznego (używane z zaimkiem „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cognition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz tam pojechać i <w>przekonać się</w>, jak piękne jest to miejsce.",
                            "target_lang_translations": {
                                "ru": "Ты должен поехать туда и <w>убедиться</w>, насколько красиво это место."
                            }
                        },
                        {
                            "text": "Chciałem <w>przekonać się</w> na własne oczy, czy mówili prawdę.",
                            "target_lang_translations": {
                                "ru": "Я хотел <w>убедиться</w> своими глазами, говорили ли они правду."
                            }
                        },
                        {
                            "text": "Wkrótce <w>przekonasz się</w>, że miałem rację w tej sprawie.",
                            "target_lang_translations": {
                                "ru": "Скоро ты <w>убедишься</w>, что я был прав в этом деле."
                            }
                        }
                    ],
                    "synonyms": [
                        "upewnić się",
                        "sprawdzić",
                        "zobaczyć",
                        "doświadczyć"
                    ],
                    "antonyms": [
                        "zwątpić"
                    ],
                    "common_phrases": [
                        "przekonać się na własne oczy",
                        "przekonać się na własnej skórze",
                        "warto się przekonać",
                        "sam się przekonaj"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лично проверить что-либо и удостовериться в истинности факта или положения дел (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убедиться",
                                "target_lang_sense_clarification": "удостовериться лично"
                            }
                        ]
                    }
                }
            ]
        }
    ]

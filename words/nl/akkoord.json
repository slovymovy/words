{
    "word_family": [
        "accorderen",
        "slotakkoord",
        "wanakkoord",
        "akkoordverklaring"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cd901b0e-3ba7-301b-aef0-f6dd4810b016",
                    "sense_definition": "Een officiële afspraak of overeenkomst tussen twee of meer partijen waarover men het eens is geworden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na lang onderhandelen was er eindelijk een <w>akkoord</w> bereikt.",
                            "target_lang_translations": {
                                "en": "After long negotiations, an <w>agreement</w> was finally reached.",
                                "ru": "После долгих переговоров наконец было достигнуто <w>соглашение</w>."
                            }
                        },
                        {
                            "text": "De twee landen hebben een <w>akkoord</w> gesloten over de nieuwe handelsregels.",
                            "target_lang_translations": {
                                "en": "The two countries have concluded an <w>agreement</w> on the new trade rules.",
                                "ru": "Две страны заключили <w>соглашение</w> о новых торговых правилах."
                            }
                        }
                    ],
                    "synonyms": [
                        "overeenkomst",
                        "deal"
                    ],
                    "common_phrases": [
                        "een akkoord bereiken",
                        "een akkoord sluiten"
                    ],
                    "target_lang_definitions": {
                        "en": "An official arrangement or contract between two or more parties that has been agreed upon.",
                        "ru": "Официальная договоренность или соглашение между двумя или более сторонами."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "agreement",
                                "target_lang_sense_clarification": "mutual understanding or contract"
                            },
                            {
                                "target_lang_word": "deal",
                                "target_lang_sense_clarification": "business or informal arrangement"
                            },
                            {
                                "target_lang_word": "accord",
                                "target_lang_sense_clarification": "formal or political agreement"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "соглашение",
                                "target_lang_sense_clarification": "официальный договор"
                            },
                            {
                                "target_lang_word": "договорённость",
                                "target_lang_sense_clarification": "взаимное согласие"
                            },
                            {
                                "target_lang_word": "сделка",
                                "target_lang_sense_clarification": "коммерческое соглашение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21c7ef11-2046-37b6-8d6c-9894d37ffc2d",
                    "sense_definition": "Een samenklank van drie of meer verschillende muzikale tonen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "group_music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De muzikant sloeg een <w>akkoord</w> aan op de piano.",
                            "target_lang_translations": {
                                "en": "The musician struck a <w>chord</w> on the piano.",
                                "ru": "Музыкант взял <w>аккорд</w> на пианино."
                            }
                        },
                        {
                            "text": "Voor dit liedje moet je een lastig <w>akkoord</w> op de gitaar spelen.",
                            "target_lang_translations": {
                                "en": "For this song, you have to play a difficult <w>chord</w> on the guitar.",
                                "ru": "Для этой песни нужно сыграть сложный <w>аккорд</w> на гитаре."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een akkoord aanslaan"
                    ],
                    "target_lang_definitions": {
                        "en": "A combination of three or more different musical notes played simultaneously.",
                        "ru": "Сочетание трех или более различных музыкальных нот, звучащих одновременно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chord",
                                "target_lang_sense_clarification": "musical set of notes"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "аккорд",
                                "target_lang_sense_clarification": "музыкальное созвучие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "a00959e0-abcb-3405-874e-bbb4950d3350",
                    "sense_definition": "Een uitroep om aan te geven dat men het ergens mee eens is of toestemming geeft.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ga je mee naar de film? <w>Akkoord</w>, dat lijkt me leuk.",
                            "target_lang_translations": {
                                "en": "Are you coming to the movie? <w>Agreed</w>, that sounds fun.",
                                "ru": "Пойдешь в кино? <w>Договорились</w>, звучит здорово."
                            }
                        },
                        {
                            "text": "Ik bied tien euro voor het boek. <w>Akkoord</w>, het is van jou!",
                            "target_lang_translations": {
                                "en": "I offer ten euros for the book. <w>Deal</w>, it's yours!",
                                "ru": "Предлагаю десять евро за книгу. <w>Идёт</w>, она твоя!"
                            }
                        }
                    ],
                    "synonyms": [
                        "ja"
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation to indicate agreement, consent, or permission.",
                        "ru": "Возглас, выражающий согласие или разрешение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "agreed",
                                "target_lang_sense_clarification": "expression of consent"
                            },
                            {
                                "target_lang_word": "deal",
                                "target_lang_sense_clarification": "confirmation of a transaction or plan"
                            },
                            {
                                "target_lang_word": "okay",
                                "target_lang_sense_clarification": "informal expression of assent"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "договорились",
                                "target_lang_sense_clarification": "подтверждение плана"
                            },
                            {
                                "target_lang_word": "согласен",
                                "target_lang_sense_clarification": "выражение согласия"
                            },
                            {
                                "target_lang_word": "идёт",
                                "target_lang_sense_clarification": "разговорное выражение согласия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "eece1096-e3ad-333e-b867-b4853d020ee9",
                    "sense_definition": "In een toestand van overeenstemming verkeren; het eens zijn met iets.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben <w>akkoord</w> met de voorwaarden die in het contract staan.",
                            "target_lang_translations": {
                                "en": "I <w>agree</w> with the conditions stated in the contract.",
                                "ru": "Я <w>согласен</w> с условиями, указанными в контракте."
                            }
                        },
                        {
                            "text": "De directeur moet nog <w>akkoord</w> gaan met het nieuwe plan.",
                            "target_lang_translations": {
                                "en": "The director still has to <w>agree</w> to the new plan.",
                                "ru": "Директор еще должен дать <w>согласие</w> на новый план."
                            }
                        }
                    ],
                    "common_phrases": [
                        "akkoord gaan",
                        "akkoord zijn"
                    ],
                    "target_lang_definitions": {
                        "en": "To be in a state of agreement; to share the same opinion or consent to something.",
                        "ru": "Находиться в состоянии согласия; быть согласным с чем-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "agreed",
                                "target_lang_sense_clarification": "united in opinion"
                            },
                            {
                                "target_lang_word": "in agreement",
                                "target_lang_sense_clarification": "state of consensus"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "согласен",
                                "target_lang_sense_clarification": "единый во мнении"
                            },
                            {
                                "target_lang_word": "согласие",
                                "target_lang_sense_clarification": "одобрение или разрешение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
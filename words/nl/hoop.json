    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "10dea6de-62cf-3268-bf64-52ca82f01fe4",
                    "sense_definition": "Het verlangen en de verwachting dat er iets goeds of wenselijks zal gebeuren.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "expectation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb goede <w>hoop</w> dat het morgen droog blijft tijdens de wandeling.",
                            "target_lang_translations": {
                                "en": "I have high <w>hope</w> that it will stay dry during the walk tomorrow.",
                                "ru": "У меня большая <w>надежда</w>, что завтра во время прогулки будет сухо."
                            }
                        },
                        {
                            "text": "Zij gaf de <w>hoop</w> niet op ondanks de vele tegenslagen die ze had.",
                            "target_lang_translations": {
                                "en": "She did not give up <w>hope</w> despite the many setbacks she had.",
                                "ru": "Она не теряла <w>надежды</w>, несмотря на множество неудач, которые у нее были."
                            }
                        }
                    ],
                    "antonyms": [
                        "wanhoop"
                    ],
                    "common_phrases": [
                        "hoop doet leven",
                        "in de hoop dat",
                        "hoop en vrees",
                        "valse hoop"
                    ],
                    "target_lang_definitions": {
                        "en": "The feeling of expectation and desire for a certain thing to happen.",
                        "ru": "Чувство ожидания и желания того, чтобы что-то произошло."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hope",
                                "target_lang_sense_clarification": "feeling of expectation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "надежда",
                                "target_lang_sense_clarification": "чувство ожидания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9cc5b747-0199-3f0d-89ff-a104940ff66c",
                    "sense_definition": "Een grote hoeveelheid van iets.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er is nog een hele <w>hoop</w> werk te doen voordat we naar huis kunnen.",
                            "target_lang_translations": {
                                "en": "There is still a whole <w>heap</w> of work to do before we can go home.",
                                "ru": "Предстоит еще целая <w>куча</w> работы, прежде чем мы сможем пойти домой."
                            }
                        },
                        {
                            "text": "Dat nieuwe huis heeft hem echt een <w>hoop</w> geld gekost.",
                            "target_lang_translations": {
                                "en": "That new house really cost him a <w>heap</w> of money.",
                                "ru": "Этот новый дом действительно стоил ему <w>кучу</w> денег."
                            }

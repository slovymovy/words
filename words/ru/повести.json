    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5b1bb703-f3be-3409-9387-44347290bf65",
                    "sense_definition": "Начать вести кого-либо или что-либо в определённом направлении, указывая путь или сопровождая.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "motion_guide",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец взял сына за руку и <повёл> его домой.",
                            "target_lang_translations": {
                                "en": "The father took his son by the hand and <led> him home."
                            }
                        },
                        {
                            "text": "Экскурсовод <повёл> группу туристов к старому замку.",
                            "target_lang_translations": {
                                "en": "The tour guide <led> the group of tourists to the old castle."
                            }
                        }
                    ],
                    "synonyms": [
                        "зати́хнуть"
                    ],
                    "common_phrases": [
                        "повести за руку",
                        "повести в школу",
                        "повести домой"
                    ],
                    "target_lang_definitions": {
                        "en": "To start leading someone or something in a certain direction, indicating the way or accompanying."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lead",
                                "target_lang_sense_clarification": "to guide or conduct someone"
                            },
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "to escort or guide someone to a place"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14da02b3-d80c-304f-9af5-0b70f8991d30",
                    "sense_definition": "Начать управлять транспортным средством, заставляя его двигаться.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "motion_guide",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Водитель уверенно <повёл> автобус по горной дороге.",
                            "target_lang_translations": {
                                "en": "The driver confidently <drove> the bus along the mountain road."
                            }
                        },
                        {
                            "text": "Капитан <повёл> корабль в открытое море.",
                            "target_lang_translations": {
                                "en": "The captain <steered> the ship into the open sea."
                            }
                        }
                    ],
                    "synonyms": [
                        "угомони́ться"
                    ],
                    "common_phrases": [
                        "повести машину",
                        "повести корабль"
                    ],
                    "target_lang_definitions": {
                        "en": "To start operating a vehicle, making it move."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drive",
                                "target_lang_sense_clarification": "to operate a vehicle"
                            },
                            {
                                "target_lang_word": "steer",
                                "target_lang_sense_clarification": "to guide a vessel or vehicle"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4885ea22-72ee-30a7-95b5-64e92193c1c3",
                    "sense_definition": "Выйти вперёд в счёте во время спортивного состязания или игры.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sports_lead",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Уже на десятой минуте матча наша команда <повела> в счёте.",
                            "target_lang_translations": {
                                "en": "Already in the tenth minute of the match, our team <took the lead> in the score."
                            }
                        },
                        {
                            "text": "После точного броска баскетболисты <повели> с разницей в два очка.",
                            "target_lang_translations": {
                                "en": "After an accurate shot, the basketball players <took the lead> by two points."
                            }
                        }
                    ],
                    "synonyms": [
                        "присмире́ть"
                    ],
                    "antonyms": [
                        "отстать"
                    ],
                    "common_phrases": [
                        "повести в счёте",
                        "команда повела"
                    ],
                    "target_lang_definitions": {
                        "en": "To go ahead in the score during a sports competition or game."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take the lead",
                                "target_lang_sense_clarification": "to go ahead in a game"
                            },
                            {
                                "target_lang_word": "lead",
                                "target_lang_sense_clarification": "to be ahead in score"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b014919-6893-3323-bb39-233e86b0b4b2",
                    "sense_definition": "Стать причиной чего-либо, иметь что-либо своим следствием.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это необдуманное решение может <повести> к серьёзным проблемам.",
                            "target_lang_translations": {
                                "en": "This rash decision could <lead> to serious problems."
                            }
                        },
                        {
                            "text": "Его слова <повели> за собой долгую дискуссию.",
                            "target_lang_translations": {
                                "en": "His words <led> to a long discussion."
                            }
                        }
                    ],
                    "synonyms": [
                        "приути́хнуть",
                        "ути́хнуть"
                    ],
                    "common_phrases": [
                        "повести к последствиям",
                        "повести за собой"
                    ],
                    "target_lang_definitions": {
                        "en": "To become the cause of something, to have something as its consequence."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lead to",
                                "target_lang_sense_clarification": "to result in something"
                            },
                            {
                                "target_lang_word": "result in",
                                "target_lang_sense_clarification": "to cause an outcome"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7df020f-4158-3fad-b4bf-4158ecc4f407",
                    "sense_definition": "Сделать плавное движение частью тела (обычно бровями, плечами, глазами).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "body_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На мой вопрос он лишь удивлённо <повёл> плечами.",
                            "target_lang_translations": {
                                "en": "To my question, he only <shrugged> his shoulders in surprise."
                            }
                        },
                        {
                            "text": "Он даже ухом не <повёл>, когда услышал эту новость.",
                            "target_lang_translations": {
                                "en": "He didn't even <bat an eyelid> when he heard this news."
                            }
                        }
                    ],
                    "synonyms": [
                        "уле́чься"
                    ],
                    "common_phrases": [
                        "повести бровью",
                        "повести плечами",
                        "ухом не повести"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a smooth movement with a body part (usually eyebrows, shoulders, eyes)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shrug",
                                "target_lang_sense_clarification": "of shoulders"
                            },
                            {
                                "target_lang_word": "move",
                                "target_lang_sense_clarification": "of body parts"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cf45920-9e20-37ea-8fc7-696b664d2b50",
                    "sense_definition": "О непроизвольном отклонении в сторону при движении (обычно о транспортном средстве или человеке).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "deviation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На скользкой дороге автомобиль внезапно <повело> влево.",
                            "target_lang_translations": {
                                "en": "On the slippery road, the car suddenly <skidded> to the left."
                            }
                        },
                        {
                            "text": "От сильной усталости и головокружения его <повело> в сторону.",
                            "target_lang_translations": {
                                "en": "From severe fatigue and dizziness, he <staggered> to the side."
                            }
                        }
                    ],
                    "synonyms": [
                        "ослабе́ть"
                    ],
                    "common_phrases": [
                        "машину повело",
                        "меня повело"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "безличный глагол"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Regarding an involuntary deviation to the side during movement (usually of a vehicle or person)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "skid",
                                "target_lang_sense_clarification": "of a vehicle sliding sideways"
                            },
                            {
                                "target_lang_word": "stagger",
                                "target_lang_sense_clarification": "of a person walking unsteadily"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f373a00-23ce-35ba-b060-96d6c4be1c08",
                    "sense_definition": "Искривиться, деформироваться под воздействием влаги, температуры или времени (о материалах).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "deformation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "От сырости деревянную раму окна сильно <повело>.",
                            "target_lang_translations": {
                                "en": "The wooden window frame <warped> badly from the dampness."
                            }
                        },
                        {
                            "text": "Если сушить доски неправильно, их может <повести>.",
                            "target_lang_translations": {
                                "en": "If boards are dried incorrectly, they might <warp>."
                            }
                        }
                    ],
                    "synonyms": [
                        "смо́лкнуть"
                    ],
                    "common_phrases": [
                        "дверь повело",
                        "доски повело"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "безличный глагол"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To warp or deform under the influence of moisture, temperature, or time (about materials)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "warp",
                                "target_lang_sense_clarification": "to become bent or twisted out of shape"
                            }
                        ]
                    }
                }
            ]
        },

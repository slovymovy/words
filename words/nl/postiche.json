            "senses": [
                {
                    "sense_id": "7403d8db-b3d5-33d1-aa1f-948c617dd726",
                    "sense_definition": "Een kunstmatig haarstukje, zoals een valse baard, snor, vlecht of toupet, dat wordt gedragen om het uiterlijk te veranderen of kaalheid te verhullen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De acteur moest voor zijn rol als piraat elke avond een zware <w>postiche</w> op zijn kin plakken.",
                            "target_lang_translations": {
                                "ru": "Для роли пирата актеру приходилось каждый вечер приклеивать на подбородок тяжелую <w>накладную бороду</w>."
                            }
                        },
                        {
                            "text": "Zij gebruikt soms een <w>postiche</w> in de vorm van een paardenstaart om haar kapsel meer volume te geven.",
                            "target_lang_translations": {
                                "ru": "Она иногда использует <w>шиньон</w> в форме конского хвоста, чтобы придать прическе больше объема."
                            }
                        }
                    ],
                    "synonyms": [
                        "fake",
                        "onecht",
                        "nep",
                        "vervalst"
                    ],
                    "common_phrases": [
                        "een postiche dragen",
                        "een postiche opplakken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусственное изделие из волос, такое как накладная борода, усы, коса или парик, используемое для изменения внешности или маскировки лысины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шиньон",
                                "target_lang_sense_clarification": "накладной хвост или пучок"
                            },
                            {
                                "target_lang_word": "накладка",
                                "target_lang_sense_clarification": "изделие из волос"
                            },
                            {
                                "target_lang_word": "постиж",
                                "target_lang_sense_clarification": "профессиональный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d75bdb8-b894-36b7-8594-f4a4237fb3f1",
                    "sense_definition": "Een nabootsing of imitatie van een voorwerp of materiaal, vaak bedoeld om de schijn van echtheid of waarde te wekken zonder dat daadwerkelijk te zijn.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De marmeren zuilen in de hal bleken bij nader inzien van gips en dus pure <w>postiche</w>.",
                            "target_lang_translations": {
                                "ru": "Мраморные колонны в холле при ближайшем рассмотрении оказались гипсовыми, то есть чистой <w>бутафорией</w>."
                            }
                        },
                        {
                            "text": "De criticus noemde de emoties in het toneelstuk louter <w>postiche</w> en zonder enige diepgang.",
                            "target_lang_translations": {
                                "ru": "Критик назвал эмоции в пьесе сплошной <w>фальшью</w> и лишенными какой-либо глубины."
                            }
                        }
                    ],

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "139ba811-e55d-3338-bed3-2427227ad124",
                    "sense_definition": "Wydawać głośne, chrapliwe dźwięki podczas oddychania w czasie snu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój mąż zaczyna <w>chrapać</w>, gdy tylko położy głowę na poduszce.",
                            "target_lang_translations": {
                                "ru": "Мой муж начинает <w>храпеть</w>, как только кладёт голову на подушку."
                            }
                        },
                        {
                            "text": "Musiałem go obudzić, bo tak głośno <w>chrapał</w>, że nie mogłem zasnąć.",
                            "target_lang_translations": {
                                "ru": "Мне пришлось его разбудить, потому что он так громко <w>храпел</w>, что я не мог уснуть."
                            }
                        }
                    ],
                    "common_phrases": [
                        "głośno chrapać",
                        "przestać chrapać",
                        "chrapać w nocy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать громкие, хриплые звуки при дыхании во сне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "храпеть",
                                "target_lang_sense_clarification": "во сне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4047b3ee-9b27-3b21-ac37-cf8a3e7065a4",
                    "sense_definition": "Spać.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po ciężkim dniu pracy on już dawno <w>chrapie</w> w swoim łóżku.",
                            "target_lang_translations": {
                                "ru": "После тяжёлого рабочего дня он уже давно <w>храпит</w> в своей кровати."
                            }
                        },
                        {
                            "text": "Zamiast się uczyć, ty wolisz <w>chrapać</w> do późna.",
                            "target_lang_translations": {
                                "ru": "Вместо того чтобы учиться, ты предпочитаешь <w>дрыхнуть</w> допоздна."
                            }
                        }
                    ],
                    "synonyms": [
                        "spać",
                        "kimać",
                        "lulać",
                        "drzemać"
                    ],
                    "common_phrases": [
                        "smacznie sobie chrapać",
                        "chrapać do południa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спать",
                                "target_lang_sense_clarification": "нейтральное"
                            },
                            {
                                "target_lang_word": "дрыхнуть",
                                "target_lang_sense_clarification": "разг., пренебр."
                            },
                            {
                                "target_lang_word": "храпеть",
                                "target_lang_sense_clarification": "перен., разг."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13ef20ab-8df0-3a62-836f-f85dfdb2c56d",
                    "sense_definition": "O zwierzętach (zwłaszcza koniach): wydawać głośny dźwięk przez nozdrza.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spłoszony koń stanął dęba i głośno <w>chrapał</w>.",
                            "target_lang_translations": {
                                "ru": "Испуганный конь встал на дыбы и громко <w>храпел</w>."
                            }
                        },
                        {
                            "text": "Zwierzęta <w>chrapały</w> nerwowo, wyczuwając zbliżające się niebezpieczeństwo.",
                            "target_lang_translations": {
                                "ru": "Животные нервно <w>храпели</w>, чувствуя приближающуюся опасность."
                            }
                        }
                    ],
                    "synonyms": [
                        "parskać"
                    ],
                    "common_phrases": [
                        "koń chrapie"
                    ],
                    "target_lang_definitions": {
                        "ru": "О животных (особенно лошадях): издавать громкий звук через ноздри."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "храпеть",
                                "target_lang_sense_clarification": "о животных"
                            },
                            {
                                "target_lang_word": "фыркать",
                                "target_lang_sense_clarification": "резко выдыхать воздух"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b1c3a31-38cb-3c7b-84e7-fd66dafb9f19",
                    "sense_definition": "Wydawać niezdrowe, chrapliwe odgłosy przy oddychaniu (zwykle o chorych lub umierających).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chory leżał na łóżku i ciężko <w>chrapał</w> przy każdym oddechu.",
                            "target_lang_translations": {
                                "ru": "Больной лежал на кровати и тяжело <w>хрипел</w> при каждом вздохе."
                            }
                        },
                        {
                            "text": "Z jego piersi wydobywał się świst, jakby <w>chrapał</w> na jawie.",
                            "target_lang_translations": {
                                "ru": "Из его груди вырывался свист, как будто он <w>храпел</w> наяву."
                            }
                        }
                    ],
                    "synonyms": [
                        "rzęzić",
                        "charczeć"
                    ],
                    "common_phrases": [
                        "ciężko chrapać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать нездоровые, хриплые звуки при дыхании (обычно о больных или умирающих)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хрипеть",
                                "target_lang_sense_clarification": "издавать хрипы"
                            },
                            {
                                "target_lang_word": "сипеть",
                                "target_lang_sense_clarification": "дышать с сипом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95f9cb5a-dbbf-3cc1-bc76-68bdde77babf",
                    "sense_definition": "O ptakach (np. słonkach): wydawać specyficzny głos podczas lotu godowego.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Myśliwy nasłuchiwał, czy słonka zacznie <w>chrapać</w> nad lasem.",
                            "target_lang_translations": {
                                "ru": "Охотник прислушивался, начнёт ли вальдшнеп <w>хоркать</w> над лесом."
                            }
                        },
                        {
                            "text": "Wieczorem usłyszeliśmy, jak ptaki <w>chrapią</w> w trakcie toków.",
                            "target_lang_translations": {
                                "ru": "Вечером мы услышали, как птицы <w>хоркают</w> во время токования."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О птицах (например, вальдшнепах): издавать специфический звук во время брачного полета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хоркать",
                                "target_lang_sense_clarification": "о вальдшнепах"
                            }
                        ]
                    }
                }
            ]
        }
    ]

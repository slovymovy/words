            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4aea71f6-8f74-3c30-a666-9aadeb16011f",
                    "sense_definition": "Een massale, vaak gewelddadige poging om het heersende gezag omver te werpen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bevolking kwam massaal in <w>opstand</w> tegen de dictator.",
                            "target_lang_translations": {
                                "en": "The population rose up in mass <w>revolt</w> against the dictator.",
                                "ru": "Население массово подняло <w>восстание</w> против диктатора."
                            }
                        },
                        {
                            "text": "Het leger wist de gewapende <w>opstand</w> na drie dagen neer te slaan.",
                            "target_lang_translations": {
                                "en": "The army managed to crush the armed <w>uprising</w> after three days.",
                                "ru": "Армии удалось подавить вооруженное <w>восстание</w> спустя три дня."
                            }
                        }
                    ],
                    "synonyms": [
                        "rebellie",
                        "oproer"
                    ],
                    "common_phrases": [
                        "in opstand komen",
                        "een opstand onderdrukken",
                        "gewapende opstand"
                    ],
                    "target_lang_definitions": {
                        "en": "A massive, often violent attempt to overthrow the ruling authority.",
                        "ru": "Массовая, часто насильственная попытка свергнуть правящую власть."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rebellion",
                                "target_lang_sense_clarification": "organized resistance to authority"
                            },
                            {
                                "target_lang_word": "uprising",
                                "target_lang_sense_clarification": "popular revolt"
                            },
                            {
                                "target_lang_word": "revolt",
                                "target_lang_sense_clarification": "act of rising up"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "восстание",
                                "target_lang_sense_clarification": "организованное сопротивление власти"
                            },
                            {
                                "target_lang_word": "мятеж",
                                "target_lang_sense_clarification": "вооруженное выступление против власти"
                            },
                            {
                                "target_lang_word": "бунт",
                                "target_lang_sense_clarification": "стихийное массовое неповиновение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53084108-2a6b-344e-8198-2b4c569203cc",
                    "sense_definition": "Een groep staande bomen of struiken, ter onderscheiding van de aangrenzende begroeiing.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boswachter inspecteerde de jonge <w>opstand</w> van eikenbomen.",
                            "target_lang_translations": {
                                "en": "The ranger inspected the young <w>stand</w> of oak trees.",
                                "ru": "Лесник осмотрел молодое <w>насаждение</w> дубов."
                            }
                        },
                        {
                            "text": "Door de zware storm is een groot deel van de <w>opstand</w> beschadigd geraakt.",
                            "target_lang_translations": {
                                "en": "A large part of the <w>stand</w> was damaged by the severe storm.",
                                "ru": "Из-за сильного шторма большая часть <w>древостоя</w> была повреждена."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A group of standing trees or shrubs, distinct from the surrounding vegetation.",
                        "ru": "Группа стоящих деревьев или кустарников, отличающаяся от окружающей растительности."
                    },
                    "translations": {

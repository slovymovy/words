    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "77768a80-1424-316a-8fbc-527eae43d476",
                    "sense_definition": "Czasopismo, zazwyczaj ukazujące się codziennie lub kilka razy w tygodniu, zawierające aktualne wiadomości, artykuły i ogłoszenia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "media_publication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano kupuję świeżą <w>gazetę</w> w kiosku za rogiem.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я покупаю свежую <w>газету</w> в киоске за углом."
                            }
                        },
                        {
                            "text": "W dzisiejszej <w>gazecie</w> napisano, że od jutra zdrożeje cukier.",
                            "target_lang_translations": {
                                "ru": "В сегодняшней <w>газете</w> написано, что с завтрашнего дня подорожает сахар."
                            }
                        },
                        {
                            "text": "Mój dziadek lubi czytać <w>gazetę</w>, siedząc w wygodnym fotelu.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка любит читать <w>газету</w>, сидя в удобном кресле."
                            }
                        }
                    ],
                    "synonyms": [
                        "gazetówka"
                    ],
                    "common_phrases": [
                        "czytać gazetę",
                        "gazeta codzienna",
                        "artykuł w gazecie",
                        "pierwsza strona gazety"
                    ],
                    "target_lang_definitions": {
                        "ru": "Периодическое издание, обычно выходящее ежедневно или несколько раз в неделю, содержащее текущие новости, статьи и объявления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "газета",
                                "target_lang_sense_clarification": "периодическое издание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a655dbb5-0943-34c0-ae03-d6c5d6a0c154",
                    "sense_definition": "Zespół redakcyjny, instytucja lub wydawnictwo tworzące dane czasopismo.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "media_organization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cała <w>gazeta</w> pracuje teraz nad specjalnym wydaniem świątecznym.",
                            "target_lang_translations": {
                                "ru": "Вся <w>редакция</w> сейчас работает над специальным праздничным выпуском."
                            }
                        },
                        {
                            "text": "Ta <w>gazeta</w> ma swoją siedzibę w centrum Warszawy.",
                            "target_lang_translations": {
                                "ru": "Эта <w>газета</w> базируется в центре Варшавы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "redakcja gazety",
                        "pracować w gazecie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Редакционный коллектив, учреждение или издательство, выпускающее данное периодическое издание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "редакция",
                                "target_lang_sense_clarification": "коллектив сотрудников"
                            },
                            {
                                "target_lang_word": "газета",
                                "target_lang_sense_clarification": "организация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9aec26f0-715a-3054-833b-c0d0f0a836de",
                    "sense_definition": "Papier gorszej jakości, na którym zazwyczaj drukowane są dzienniki.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tanie ulotki zostały wydrukowane na szarej <w>gazecie</w>.",
                            "target_lang_translations": {
                                "ru": "Дешёвые листовки были напечатаны на серой <w>газетной бумаге</w>."
                            }
                        },
                        {
                            "text": "Ten rodzaj <w>gazety</w> bardzo szybko żółknie pod wpływem słońca.",
                            "target_lang_translations": {
                                "ru": "Этот вид <w>газетной бумаги</w> очень быстро желтеет на солнце."
                            }
                        }
                    ],
                    "synonyms": [
                        "gazetówka"
                    ],
                    "common_phrases": [
                        "papier gazetowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бумага низкого качества, на которой обычно печатаются ежедневные издания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "газетная бумага",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "842b182f-776d-344e-a51d-23f8fe78fe81",
                    "sense_definition": "Fragment bielizny lub koszuli wystający nieestetycznie spod ubrania wierzchniego.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Popraw koszulę, bo <w>gazeta</w> ci wystaje ze spodni.",
                            "target_lang_translations": {
                                "ru": "Поправь рубашку, а то у тебя <w>край</w> торчит из брюк."
                            }
                        },
                        {
                            "text": "Wszyscy się śmiali, bo prezesowi wystawały <w>gazety</w>.",
                            "target_lang_translations": {
                                "ru": "Все смеялись, потому что у председателя торчала <w>рубашка</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gazeta wystaje"
                    ],
                    "traits": [
                        {

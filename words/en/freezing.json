    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9cc38907-03f1-3deb-a071-ed0d006865b8",
                    "sense_definition": "Extremely cold in temperature or feeling.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "coldness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Put on a warm coat because it is absolutely <w>freezing</w> outside.",
                            "target_lang_translations": {
                                "ru": "Надень тёплое пальто, потому что на улице ужасно <w>холодно</w>."
                            }
                        },
                        {
                            "text": "I need to turn on the heater because I am <w>freezing</w> in this room.",
                            "target_lang_translations": {
                                "ru": "Мне нужно включить обогреватель, потому что я <w>замерзаю</w> в этой комнате."
                            }
                        }
                    ],
                    "synonyms": [
                        "ice-cold",
                        "icy",
                        "frosty"
                    ],
                    "antonyms": [
                        "boiling",
                        "hot",
                        "scorching"
                    ],
                    "common_phrases": [
                        "freezing cold",
                        "absolutely freezing",
                        "freezing wind"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень холодный по температуре или ощущениям; пронизывающий холод."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ледяной",
                                "target_lang_sense_clarification": "очень холодный, как лёд"
                            },
                            {
                                "target_lang_word": "морозный",
                                "target_lang_sense_clarification": "связанный с морозом, очень холодный"
                            },
                            {
                                "target_lang_word": "очень холодный",
                                "target_lang_sense_clarification": "общая характеристика низкой температуры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca2067ac-acfc-3049-a653-98c82741d819",
                    "sense_definition": "At or below the temperature of zero degrees Celsius, where water turns into ice.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "temperature_point",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The temperature is expected to drop below <w>freezing</w> tonight.",
                            "target_lang_translations": {
                                "ru": "Ожидается, что сегодня ночью температура опустится ниже <w>нуля</w>."
                            }
                        },
                        {
                            "text": "Be careful on the roads as the rain is turning into <w>freezing</w> ice.",
                            "target_lang_translations": {
                                "ru": "Будьте осторожны на дорогах, так как дождь превращается в <w>ледяную</w> корку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "below freezing",
                        "above freezing",
                        "freezing point"
                    ],
                    "target_lang_definitions": {
                        "ru": "Температура ноль градусов Цельсия или ниже; точка замерзания воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "точка замерзания",
                                "target_lang_sense_clarification": "температура, при которой жидкость замерзает"
                            },
                            {
                                "target_lang_word": "ноль",
                                "target_lang_sense_clarification": "в контексте температуры ниже нуля"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "abb80f9f-5b55-3898-b39f-1f85f905d9bb",
                    "sense_definition": "The present participle and gerund of <w>freeze</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is <w>freezing</w> the leftover soup to eat next week.",
                            "target_lang_translations": {
                                "ru": "Она <w>замораживает</w> остатки супа, чтобы съесть их на следующей неделе."
                            }
                        },
                        {
                            "text": "The lake is slowly <w>freezing</w> over as winter approaches.",
                            "target_lang_translations": {
                                "ru": "Озеро медленно <w>замерзает</w> с приближением зимы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "freezing to death"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола freeze (замораживать/замерзать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замораживающий",
                                "target_lang_sense_clarification": "причастие (активное действие)"
                            },
                            {
                                "target_lang_word": "замерзающий",
                                "target_lang_sense_clarification": "причастие (процесс состояния)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e7d008ff-3559-3a7f-8ec4-e397d9adc088",
                    "sense_definition": "The process of a substance changing from a liquid to a solid state due to cold.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rapid <w>freezing</w> helps to preserve the vitamins in vegetables.",
                            "target_lang_translations": {
                                "ru": "Быстрое <w>замораживание</w> помогает сохранить витамины в овощах."
                            }
                        },
                        {
                            "text": "The constant <w>freezing</w> and thawing caused cracks in the pavement.",
                            "target_lang_translations": {
                                "ru": "Постоянное <w>замерзание</w> и оттаивание привело к трещинам в асфальте."
                            }
                        }
                    ],
                    "antonyms": [
                        "melting",
                        "thawing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс перехода вещества из жидкого состояния в твердое под воздействием холода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замораживание",
                                "target_lang_sense_clarification": "процесс охлаждения до твердого состояния"
                            },
                            {
                                "target_lang_word": "замерзание",
                                "target_lang_sense_clarification": "переход в твердое состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83d3f626-b5c5-3ec1-882c-1f5c68a82153",
                    "sense_definition": "The act of numbing a part of the body with an anesthetic.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dentist applied <w>freezing</w> to my gum before starting the procedure.",
                            "target_lang_translations": {
                                "ru": "Стоматолог сделал <w>заморозку</w> десны перед началом процедуры."
                            }
                        },
                        {
                            "text": "It took a few hours for the <w>freezing</w> to wear off after the surgery.",
                            "target_lang_translations": {
                                "ru": "Прошло несколько часов, прежде чем <w>заморозка</w> отошла после операции."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in medical or dental contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обезболивание части тела с помощью анестетика (разговорное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заморозка",
                                "target_lang_sense_clarification": "разговорное название местной анестезии"
                            }
                        ]
                    }
                }
            ]
        }
    ]

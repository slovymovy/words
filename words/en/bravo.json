                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возглас «браво»",
                                "target_lang_sense_clarification": "существительное"
                            },
                            {
                                "target_lang_word": "крик «браво»",
                                "target_lang_sense_clarification": "существительное"
                            }

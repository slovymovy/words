    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "779e945a-b9cb-329f-a14b-bb4745515baa",
                    "sense_definition": "Unable to leave a particular place, often due to illness, disability, or imprisonment.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "restriction_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mia was <w>confined</w> to her bedroom after suffering a broken leg.",
                            "target_lang_translations": {
                                "ru": "Мия была <w>прикована</w> к своей спальне после того, как сломала ногу."
                            }
                        },
                        {
                            "text": "The prisoner remained <w>confined</w> in his cell for twenty-three hours a day.",
                            "target_lang_translations": {
                                "ru": "Заключенный оставался <w>заточённым</w> в своей камере двадцать три часа в сутки."
                            }
                        }
                    ],
                    "synonyms": [
                        "restricted",
                        "captive"
                    ],
                    "antonyms": [
                        "free",
                        "liberated"
                    ],
                    "common_phrases": [
                        "confined to bed",
                        "confined to a wheelchair",
                        "confined to barracks"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неспособный покинуть определенное место, часто из-за болезни, инвалидности или тюремного заключения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прикованный",
                                "target_lang_sense_clarification": "к месту или постели из-за болезни"
                            },
                            {
                                "target_lang_word": "заточённый",
                                "target_lang_sense_clarification": "находящийся в заключении"
                            },
                            {
                                "target_lang_word": "запертый",
                                "target_lang_sense_clarification": "не имеющий возможности выйти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "323b5f69-6bea-3b16-a56d-1882c135e905",
                    "sense_definition": "Very small, narrow, or enclosed, offering little space to move.",

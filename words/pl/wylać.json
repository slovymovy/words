                {
                    "sense_id": "1f931f89-526a-3439-aa6a-14edf77c2e06",
                    "sense_definition": "Opróżnić naczynie z płynu lub pozbyć się cieczy, przechylając pojemnik.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "liquid_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <w>wylać</w> brudną wodę z wiadra po myciu podłogi.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>вылить</w> грязную воду из ведра после мытья пола."
                            }
                        },
                        {
                            "text": "Kucharz kazał <w>wylać</w> stary olej i nalać świeżego.",
                            "target_lang_translations": {
                                "ru": "Повар велел <w>слить</w> старое масло и налить свежее."
                            }
                        }
                    ],
                    "synonyms": [
                        "opróżnić",
                        "chlusnąć"
                    ],
                    "antonyms": [
                        "nalać",
                        "wlać",
                        "napełnić"
                    ],
                    "common_phrases": [
                        "wylać wodę",
                        "wylać pomyje",
                        "wylać za burtę",
                        "wylać dziecko z kąpielą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Опорожнить емкость от жидкости или избавиться от жидкости, наклонив контейнер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вылить",
                                "target_lang_sense_clarification": "опорожнить"
                            },
                            {
                                "target_lang_word": "слить",
                                "target_lang_sense_clarification": "удалить жидкость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5cc73d27-59d3-3d3e-afdc-7fb7334133e0",
                    "sense_definition": "Sprawić przypadkowo lub w sposób niekontrolowany, że płyn wydostanie się z naczynia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "liquid_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez nieuwagę <w>wylałem</w> gorącą kawę na biały obrus.",
                            "target_lang_translations": {
                                "ru": "По неосторожности я <w>пролил</w> горячий кофе на белую скатерть."
                            }
                        },
                        {
                            "text": "Dziecko potrąciło kubek i <w>wylało</w> sok na podłogę.",
                            "target_lang_translations": {
                                "ru": "Ребенок задел чашку и <w>разлил</w> сок на пол."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozlać",
                        "wychlapać"
                    ],
                    "common_phrases": [
                        "wylać kawę",
                        "wylać na podłogę",
                        "wylać na siebie"
                    ],
                    "target_lang_definitions": {

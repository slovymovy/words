    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fb56521d-767e-3798-8bb9-c403e8f898d9",
                    "sense_definition": "Być ostrożnym, zachowywać czujność w celu uniknięcia niebezpieczeństwa, błędu lub nieszczęścia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "caution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz bardzo <w>uważać</w>, kiedy przechodzisz przez ruchliwą ulicę.",
                            "target_lang_translations": {
                                "ru": "Ты должен быть очень <w>осторожным</w>, когда переходишь оживленную улицу."
                            }
                        },
                        {
                            "text": "Proszę <w>uważać</w> na siebie podczas tej dalekiej podróży.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>берегите</w> себя во время этого дальнего путешествия."
                            }
                        }
                    ],
                    "synonyms": [
                        "baczyć"
                    ],
                    "common_phrases": [
                        "uważać na siebie",
                        "uważać na słowa",
                        "uważać na drodze",
                        "proszę uważać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть осторожным, сохранять бдительность, чтобы избежать опасности или ошибки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть осторожным",
                                "target_lang_sense_clarification": "проявлять осторожность"
                            },
                            {
                                "target_lang_word": "остерегаться",
                                "target_lang_sense_clarification": "избегать опасности"
                            },
                            {
                                "target_lang_word": "беречься",
                                "target_lang_sense_clarification": "беречь себя (обычно в контексте здоровья или безопасности)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6cefe98-5261-3491-9a48-1fcc46cd59f0",
                    "sense_definition": "Skupiać uwagę na czymś, słuchać lub obserwować coś z zainteresowaniem i koncentracją.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uczniowie powinni <w>uważać</w> na lekcji, aby zrozumieć nowy temat.",
                            "target_lang_translations": {
                                "ru": "Ученики должны <w>быть внимательными</w> на уроке, чтобы понять новую тему."
                            }
                        },
                        {
                            "text": "Jeśli będziesz pilnie <w>uważać</w>, na pewno nie popełnisz błędu.",
                            "target_lang_translations": {
                                "ru": "Если ты будешь предельно <w>внимателен</w>, ты точно не совершишь ошибку."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwracać uwagę",
                        "baczyć"
                    ],
                    "common_phrases": [
                        "uważać na lekcji",
                        "pilnie uważać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сосредоточивать внимание на чем-либо, слушать или наблюдать с интересом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть внимательным",
                                "target_lang_sense_clarification": "проявлять внимание"
                            },
                            {
                                "target_lang_word": "слушать",
                                "target_lang_sense_clarification": "воспринимать на слух с вниманием"
                            },
                            {
                                "target_lang_word": "следить",
                                "target_lang_sense_clarification": "наблюдать за ходом чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e959761-3316-3099-b07a-26acc781a462",
                    "sense_definition": "Mieć określone zdanie na jakiś temat, być o czymś przekonanym; sądzić.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiele osób <w>uważa</w>, że zdrowa dieta jest kluczem do sukcesu.",
                            "target_lang_translations": {
                                "ru": "Многие люди <w>считают</w>, что здоровая диета — это ключ к успеху."
                            }
                        },
                        {
                            "text": "Ja <w>uważam</w> inaczej i mam na to mocne argumenty.",
                            "target_lang_translations": {
                                "ru": "Я <w>считаю</w> иначе, и у меня есть на это веские аргументы."
                            }
                        }
                    ],
                    "synonyms": [
                        "sądzić",
                        "myśleć",
                        "być zdania",
                        "twierdzić"
                    ],
                    "common_phrases": [
                        "uważać inaczej",
                        "powszechnie uważać",
                        "szczerze uważać",
                        "błędnie uważać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь определенное мнение по какому-либо вопросу, быть убежденным в чем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "считать",
                                "target_lang_sense_clarification": "иметь мнение"
                            },
                            {
                                "target_lang_word": "полагать",
                                "target_lang_sense_clarification": "думать, предполагать (более формально)"
                            },
                            {
                                "target_lang_word": "думать",
                                "target_lang_sense_clarification": "мыслить, иметь суждение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae857dc4-5271-357f-a5af-4d68da946b46",
                    "sense_definition": "Traktować kogoś lub coś w określony sposób, przypisywać komuś lub czemuś pewne cechy (zwykle z przyimkiem „za”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel <w>uważał</w> to zadanie za zbyt trudne dla początkujących uczniów.",
                            "target_lang_translations": {
                                "ru": "Учитель <w>считал</w> это задание слишком трудным для начинающих учеников."
                            }
                        },
                        {
                            "text": "Zawsze <w>uważałem</w> go za mojego najlepszego i najbardziej lojalnego przyjaciela.",
                            "target_lang_translations": {
                                "ru": "Я всегда <w>считал</w> его своим лучшим и самым преданным другом."
                            }
                        }
                    ],
                    "synonyms": [
                        "uznawać"
                    ],
                    "common_phrases": [
                        "uważać za konieczne",
                        "uważać za stosowne",
                        "uważać za błąd",
                        "uważać za sukces"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относиться к кому-либо или чему-либо определенным образом, приписывать качества (обычно с предлогом «за» в польском, Тв. п. в русском)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "считать",
                                "target_lang_sense_clarification": "воспринимать кем-либо/чем-либо"
                            },
                            {
                                "target_lang_word": "расценивать",
                                "target_lang_sense_clarification": "оценивать как что-либо"
                            },
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "ошибочно или условно считать кем-то (принимать за...)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fba3acf-6783-37c3-b466-e0dffcb49b34",
                    "sense_definition": "Mieć o sobie samym określone mniemanie; oceniać siebie w pewien sposób.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "On <w>uważa się</w> za eksperta w tej dziedzinie, choć ma małe doświadczenie.",
                            "target_lang_translations": {
                                "ru": "Он <w>считает себя</w> экспертом в этой области, хотя у него мало опыта."
                            }
                        },
                        {
                            "text": "Czy <w>uważasz się</w> za osobę szczęśliwą i spełnioną w życiu?",
                            "target_lang_translations": {
                                "ru": "Ты <w>считаешь себя</w> счастливым и реализовавшимся в жизни человеком?"
                            }
                        }
                    ],
                    "synonyms": [
                        "czuć się",
                        "poczytywać się"
                    ],
                    "common_phrases": [
                        "uważać się za",
                        "uważać się za lepszego"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь о себе определенное мнение; оценивать себя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "считать себя",
                                "target_lang_sense_clarification": "иметь мнение о собственной персоне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35cd3c86-c9e1-3c0b-b2fa-06017ac07c48",
                    "sense_definition": "Darzyć kogoś szacunkiem, liczyć się z kimś.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "respect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych czasach dzieci bardziej <w>uważały</w> zdanie swoich rodziców.",
                            "target_lang_translations": {
                                "ru": "В былые времена дети больше <w>уважали</w> мнение своих родителей."
                            }
                        },
                        {
                            "text": "Szlachta nie zawsze <w>uważała</w> króla, co prowadziło do wielu konfliktów.",
                            "target_lang_translations": {
                                "ru": "Шляхта не всегда <w>почитала</w> короля, что приводило ко многим конфликтам."
                            }
                        }
                    ],
                    "synonyms": [
                        "szanować"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "współcześnie częściej używa się słowa 'szanować' lub 'poważać'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Проявлять уважение к кому-либо, считаться с кем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уважать",
                                "target_lang_sense_clarification": "относиться с уважением"
                            },
                            {
                                "target_lang_word": "почитать",
                                "target_lang_sense_clarification": "относиться с почтением (устар./книжн.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
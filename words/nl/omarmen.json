                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het hele team besloot de nieuwe strategie direct te <w>omarmen</w>.",
                            "target_lang_translations": {
                                "en": "The entire team decided to immediately <w>embrace</w> the new strategy.",
                                "ru": "Вся команда решила сразу же <w>принять</w> новую стратегию."
                            }
                        },
                        {
                            "text": "We moeten technologische vooruitgang <w>omarmen</w> om relevant te blijven.",
                            "target_lang_translations": {

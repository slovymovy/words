    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7c17f422-e8d6-3b03-aa0c-82f6760b67d7",
                    "sense_definition": "Делать кого-либо или что-либо безобразным, лишать красоты или привлекательности.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Глубокий шрам на щеке сильно <w>безобразил</w> его лицо.",
                            "target_lang_translations": {
                                "en": "A deep scar on his cheek severely <w>disfigured</w> his face."
                            }
                        },
                        {
                            "text": "Неудачно подобранная одежда лишь <w>безобразила</w> её фигуру.",
                            "target_lang_translations": {
                                "en": "The poorly chosen clothes only <w>spoiled</w> her figure."
                            }
                        }
                    ],
                    "synonyms": [
                        "уродовать",
                        "искажать",
                        "портить"
                    ],
                    "antonyms": [
                        "украшать"
                    ],
                    "common_phrases": [
                        "безобразить лицо",
                        "безобразить вид"
                    ],
                    "target_lang_definitions": {
                        "en": "To make someone or something ugly, to deprive of beauty or attractiveness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "disfigure",
                                "target_lang_sense_clarification": "to spoil the appearance of something or someone"
                            },
                            {
                                "target_lang_word": "mar",
                                "target_lang_sense_clarification": "to impair the appearance of; disfigure"
                            },
                            {
                                "target_lang_word": "make ugly",
                                "target_lang_sense_clarification": "to render unattractive"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7525752d-db5b-3a94-b245-da382146aa5a",
                    "sense_definition": "Вести себя возмутительно, нарушать порядок и приличия; то же, что безобразничать.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель строго сказал ученикам, чтобы они прекратили <w>безобразить</w>.",
                            "target_lang_translations": {
                                "en": "The teacher strictly told the students to stop <w>misbehaving</w>."
                            }
                        },
                        {
                            "text": "Пьяные гости начали <w>безобразить</w> и разбили посуду.",
                            "target_lang_translations": {
                                "en": "The drunken guests started <w>acting outrageously</w> and broke the dishes."
                            }
                        }
                    ],
                    "synonyms": [
                        "безобразничать",
                        "хулиганить",
                        "буянить"
                    ],
                    "antonyms": [
                        "вести себя прилично"
                    ],
                    "common_phrases": [
                        "хватит безобразить",
                        "перестань безобразить"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To behave outrageously, to violate order and decency."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "misbehave",
                                "target_lang_sense_clarification": "to conduct oneself badly"
                            },
                            {
                                "target_lang_word": "act outrageously",
                                "target_lang_sense_clarification": "to behave in a shocking or disgraceful manner"
                            },
                            {
                                "target_lang_word": "act up",
                                "target_lang_sense_clarification": "to behave badly or uncontrollably (colloquial)"
                            }
                        ]
                    }

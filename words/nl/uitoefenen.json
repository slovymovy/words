                {
                    "sense_id": "9f4dace7-2f9b-3a55-9553-e376e6e2f85e",
                    "sense_definition": "Invloed, macht of druk op iemand of iets toepassen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ouders <w>oefenen</w> veel druk <w>uit</w> op de schoolleiding.",
                            "target_lang_translations": {
                                "en": "The parents <w>exert</w> a lot of pressure on the school management.",
                                "ru": "Родители <w>оказывают</w> сильное давление на руководство школы."
                            }
                        },
                        {
                            "text": "Hij probeert invloed <w>uit</w> te <w>oefenen</w> op de uiteindelijke beslissing.",
                            "target_lang_translations": {
                                "en": "He is trying to <w>exert</w> influence on the final decision.",
                                "ru": "Он пытается <w>оказать</w> влияние на окончательное решение."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanwenden"
                    ],
                    "common_phrases": [
                        "druk uitoefenen",
                        "invloed uitoefenen",
                        "macht uitoefenen",
                        "gezag uitoefenen"
                    ],
                    "target_lang_definitions": {
                        "en": "To apply influence, power, or pressure on someone or something.",
                        "ru": "Оказывать влияние, давление или применять власть по отношению к кому-либо или чему-либо."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "550fc06c-1935-30b7-9ec5-9d6f4478bdd7",
                    "sense_definition": "Having horns or a horn-like shape.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The biologist examined the <w>cornute</w> head of the strange beetle.",
                            "target_lang_translations": {
                                "ru": "Биолог изучил <w>рогатую</w> голову странного жука."
                            }
                        },
                        {
                            "text": "Some plants produce <w>cornute</w> seed pods that look like tiny hooks.",
                            "target_lang_translations": {
                                "ru": "Некоторые растения образуют <w>роговидные</w> семенные коробочки, похожие на крошечные крючки."
                            }
                        }
                    ],
                    "synonyms": [
                        "horned",
                        "horn-shaped"
                    ],
                    "antonyms": [
                        "hornless"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий рога или форму, напоминающую рог."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рогатый",
                                "target_lang_sense_clarification": "имеющий рога"
                            },
                            {
                                "target_lang_word": "роговидный",
                                "target_lang_sense_clarification": "имеющий форму рога"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "277deb00-03ec-3257-84f4-06b689fa1fa3",
                    "sense_definition": "To make a cuckold of a man; to be unfaithful to a husband.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the old play, the villain plots to <w>cornute</w> the king.",
                            "target_lang_translations": {
                                "ru": "В старой пьесе злодей замышляет <w>наставить рога</w> королю."
                            }
                        },
                        {
                            "text": "He feared that his long absence would cause his wife to <w>cornute</w> him.",
                            "target_lang_translations": {
                                "ru": "Он боялся, что из-за его долгого отсутствия жена <w>наставит ему рога</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cuckold"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наставить рога мужу; изменить супругу (устаревшее или книжное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наставить рога",
                                "target_lang_sense_clarification": "идиома, означающая супружескую измену"
                            },
                            {
                                "target_lang_word": "сделать рогоносцем",
                                "target_lang_sense_clarification": "опозорить мужа изменой"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b5d1e050-ea6f-369e-976b-a93d8872e29d",
                    "sense_definition": "A member of the extinct order Cornuta, a group of marine animals.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fossil record shows that the <w>cornute</w> had an asymmetrical body.",
                            "target_lang_translations": {
                                "ru": "Палеонтологическая летопись показывает, что <w>корнута</w> имела асимметричное тело."
                            }
                        },
                        {
                            "text": "Researchers debated how the <w>cornute</w> moved across the sea floor.",
                            "target_lang_translations": {
                                "ru": "Исследователи спорили о том, как <w>корнута</w> передвигалась по морскому дну."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Представитель вымершего отряда морских животных Cornuta (иглокожие)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корнута",
                                "target_lang_sense_clarification": "палеонтологический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

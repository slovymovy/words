                    "sense_id": "a6455878-13f0-3a11-a88e-14bbd393a07d",
                    "sense_definition": "To the full or entire extent.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is <w>entirely</w> up to you to decide where we go for dinner.",
                            "target_lang_translations": {
                                "ru": "Решение, куда мы пойдем ужинать, зависит <w>полностью</w> от тебя."
                            }
                        },
                        {
                            "text": "I am not <w>entirely</w> sure if the shop is open today.",
                            "target_lang_translations": {
                                "ru": "Я не <w>совсем</w> уверен, открыт ли сегодня магазин."
                            }
                        }
                    ],
                    "synonyms": [
                        "completely",
                        "wholly"
                    ],
                    "antonyms": [
                        "partially"
                    ],
                    "common_phrases": [
                        "entirely different",
                        "not entirely sure",
                        "entirely possible"
                    ],
                    "target_lang_definitions": {
                        "ru": "В полной мере или степени; совершенно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полностью",
                                "target_lang_sense_clarification": "в полном объеме, без остатка"
                            },
                            {
                                "target_lang_word": "совершенно",
                                "target_lang_sense_clarification": "абсолютно, употребляется для усиления"
                            },

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c18e9340-8b24-3b9b-bfe3-0ff936322ef7",
                    "sense_definition": "Разрезанное место, отверстие или проём в чём-либо, образовавшиеся в результате разрезания.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cut_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На её новом платье был длинный <разрез> сбоку.",
                            "target_lang_translations": {
                                "en": "Her new dress had a long <slit> on the side."
                            }
                        },
                        {
                            "text": "Врач внимательно осмотрел глубокий <разрез> на руке пациента.",
                            "target_lang_translations": {
                                "en": "The doctor carefully examined the deep <cut> on the patient's arm."
                            }
                        }
                    ],
                    "common_phrases": [
                        "глубокий разрез",
                        "разрез на юбке",
                        "боковой разрез"
                    ],
                    "target_lang_definitions": {
                        "en": "A cut place, opening, or gap in something formed as a result of cutting."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slit",
                                "target_lang_sense_clarification": "in clothing or fabric"
                            },
                            {
                                "target_lang_word": "cut",
                                "target_lang_sense_clarification": "wound or opening"
                            },
                            {
                                "target_lang_word": "incision",
                                "target_lang_sense_clarification": "surgical or precise cut"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af741de2-ffbe-3f74-a1b9-4a927fb57847",
                    "sense_definition": "Изображение внутренней структуры предмета, мысленно рассечённого плоскостью (на чертеже или схеме).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cut_technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На уроке биологии мы рассматривали <разрез> клетки под микроскопом.",
                            "target_lang_translations": {
                                "en": "In biology class, we examined a <cross-section> of a cell under a microscope."
                            }
                        },
                        {
                            "text": "Архитектор показал нам продольный <разрез> будущего здания.",
                            "target_lang_translations": {
                                "en": "The architect showed us a longitudinal <section> of the future building."
                            }
                        }
                    ],
                    "common_phrases": [
                        "поперечный разрез",
                        "продольный разрез",
                        "в разрезе"
                    ],
                    "target_lang_definitions": {
                        "en": "A representation of the internal structure of an object as if sliced by a plane (in a drawing or diagram)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "section",
                                "target_lang_sense_clarification": "technical drawing view"
                            },
                            {
                                "target_lang_word": "cross-section",
                                "target_lang_sense_clarification": "transverse view"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd1ae0b7-4eb5-3d71-9eeb-1550df8c836e",
                    "sense_definition": "Действие по разделению чего-либо на части с помощью режущего инструмента.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cut_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хирург сделал аккуратный <разрез> скальпелем.",
                            "target_lang_translations": {
                                "en": "The surgeon made a neat <incision> with a scalpel."
                            }
                        },
                        {
                            "text": "Торжественный <разрез> красной ленточки ознаменовал открытие выставки.",
                            "target_lang_translations": {
                                "en": "The ceremonial <cutting> of the red ribbon marked the opening of the exhibition."
                            }
                        }
                    ],
                    "synonyms": [
                        "разрезание"
                    ],
                    "common_phrases": [
                        "сделать разрез"
                    ],
                    "target_lang_definitions": {
                        "en": "The action of separating something into parts using a cutting tool."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cutting",
                                "target_lang_sense_clarification": "process or action"
                            },
                            {
                                "target_lang_word": "incision",
                                "target_lang_sense_clarification": "act of cutting surgically"
                            },
                            {
                                "target_lang_word": "cut",
                                "target_lang_sense_clarification": "single act of cutting"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7120afd0-d0f7-3649-b654-60fa18be82fa",
                    "sense_definition": "Место открытой добычи полезных ископаемых; карьер.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой отец много лет работает на угольном <разрезе>.",
                            "target_lang_translations": {
                                "en": "My father has been working at a coal <open-pit mine> for many years."
                            }
                        },
                        {
                            "text": "Этот старый <разрез> был закрыт из-за истощения запасов руды.",
                            "target_lang_translations": {
                                "en": "This old <open-pit mine> was closed due to the depletion of ore reserves."
                            }
                        }
                    ],
                    "common_phrases": [
                        "угольный разрез"
                    ],
                    "target_lang_definitions": {
                        "en": "A site for open-cast mining of minerals; a quarry."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "open-pit mine",
                                "target_lang_sense_clarification": "mining excavation"
                            },
                            {
                                "target_lang_word": "strip mine",
                                "target_lang_sense_clarification": "surface mining"
                            }
                        ]
                    }
                }
            ]
        }
    ]

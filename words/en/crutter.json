    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7cf8fd33-0d24-38c7-acb4-8742266e07a6",
                    "sense_definition": "A worker who drills blasting holes, prepares the explosive charge, and removes blasted rock.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>crutter</w> descended into the dark shaft to prepare the rock face for the next blast.",
                            "target_lang_translations": {
                                "ru": "<w>Проходчик</w> спустился в темную шахту, чтобы подготовить забой к следующему взрыву."
                            }
                        },
                        {
                            "text": "Experienced <w>crutters</w> play a vital role in extending the mine tunnels safely.",
                            "target_lang_translations": {
                                "ru": "Опытные <w>проходчики</w> играют жизненно важную роль в безопасном расширении шахтных туннелей."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Рабочий, который бурит взрывные скважины, подготавливает заряд взрывчатого вещества и убирает взорванную породу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проходчик",
                                "target_lang_sense_clarification": "горный рабочий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1df77985-4a4e-3d92-be22-07d271a721e4",
                    "sense_definition": "An alternative form of <w>creetur</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The poor little <w>crutter</w> looked lost and hungry in the storm.",
                            "target_lang_translations": {
                                "ru": "Бедное маленькое <w>создание</w> выглядело потерянным и голодным во время бури."
                            }
                        },
                        {
                            "text": "He called the dog a miserable <w>crutter</w> but fed it anyway.",
                            "target_lang_translations": {
                                "ru": "Он назвал собаку жалкой <w>тварью</w>, но все равно покормил ее."
                            }
                        }
                    ],
                    "synonyms": [
                        "creature"
                    ],

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cc30fe7a-08d8-37a9-9df3-6df04b260e5d",
                    "sense_definition": "Разговорное сокращение от слова «бисексуал»; человек, испытывающий романтическое или сексуальное влечение и к мужчинам, и к женщинам.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В современной культуре многие открыто заявляют, что они <w>би</w>.",
                            "target_lang_translations": {
                                "en": "In modern culture, many openly state that they are <w>bi</w>."
                            }
                        },
                        {
                            "text": "Вы гетеро? — Я <w>би</w>!",
                            "target_lang_translations": {
                                "en": "Are you straight? — I'm <w>bi</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "бип"
                    ],
                    "common_phrases": [
                        "я би",
                        "он би",
                        "она би"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Colloquial abbreviation for the word \"bisexual\"; a person who experiences romantic or sexual attraction to both men and women."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bi",
                                "target_lang_sense_clarification": "sexual orientation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6919b68-f8da-338c-9e50-5344b7b07bbe",
                    "sense_definition": "Русское фонетическое название второй буквы латинского алфавита (B, b).",
                    "learner_level": "A2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В английском слове «debt» буква <w>би</w> не произносится.",
                            "target_lang_translations": {
                                "en": "In the English word \"debt\", the letter <w>b</w> is not pronounced."
                            }
                        },
                        {
                            "text": "На схеме этот сектор обозначен как <w>би</w>.",
                            "target_lang_translations": {
                                "en": "On the diagram, this sector is marked as <w>B</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "буква би",
                        "план би"
                    ],
                    "target_lang_definitions": {
                        "en": "The Russian phonetic name for the second letter of the Latin alphabet (B, b)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bee",
                                "target_lang_sense_clarification": "name of the letter B"
                            },
                            {
                                "target_lang_word": "B",
                                "target_lang_sense_clarification": "letter"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "1f2895ed-7be9-37cc-897c-722b772cea30",
                    "sense_definition": "Женское личное имя, часто используемое в англоязычных странах (обычно как сокращение от Беатрис или Элизабет).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Мою лучшую подругу зовут <w>Би</w>.",
                            "target_lang_translations": {
                                "en": "My best friend's name is <w>Bea</w>."
                            }
                        },
                        {
                            "text": "Тётушка <w>Би</w> всегда присылает нам подарки на Рождество.",
                            "target_lang_translations": {
                                "en": "Aunt <w>Bee</w> always sends us gifts for Christmas."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A female personal name, often used in English-speaking countries (usually as a short form of Beatrice or Elizabeth)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Bea",
                                "target_lang_sense_clarification": "name"
                            },
                            {
                                "target_lang_word": "Bee",
                                "target_lang_sense_clarification": "name"
                            }
                        ]
                    }
                }
            ]
        },
        {

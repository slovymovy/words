            "senses": [
                {
                    "sense_id": "ac609d96-9129-3613-9bd1-7f2becd6cb17",
                    "sense_definition": "Съесть некоторое количество чего-либо; принять пищу, утолить голод.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я очень хочу <поесть> после долгой прогулки.",
                            "target_lang_translations": {
                                "en": "I really want to <w>eat</w> after a long walk."
                            }
                        },
                        {
                            "text": "В этом ресторане можно вкусно <поесть> за небольшие деньги.",
                            "target_lang_translations": {
                                "en": "In this restaurant, you can <w>eat</w> a delicious meal for a small amount of money."
                            }
                        },
                        {
                            "text": "В старину люди любили хорошенько <поесть> и повеселиться.",
                            "target_lang_translations": {
                                "en": "In the old days, people loved to <w>eat</w> well and have fun."
                            }
                        }
                    ],
                    "synonyms": [
                        "покушать"
                    ],
                    "common_phrases": [
                        "вкусно поесть",
                        "люблю поесть",
                        "хочу поесть",
                        "дать поесть",
                        "сесть поесть"
                    ],
                    "target_lang_definitions": {
                        "en": "To eat a certain amount of food; to have a meal or satisfy hunger."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "eat",
                                "target_lang_sense_clarification": "general action of consuming food"
                            },
                            {
                                "target_lang_word": "have a meal",
                                "target_lang_sense_clarification": "to engage in the act of eating"
                            },
                            {
                                "target_lang_word": "have something to eat",
                                "target_lang_sense_clarification": "to eat a bit or satisfy hunger"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e4a2051-1de3-3c00-b9cd-4d4a8874516a",
                    "sense_definition": "Съесть полностью, без остатка (часто о множестве объектов или запасах).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Птицы <поели> все ягоды вишни в нашем саду.",
                            "target_lang_translations": {
                                "en": "The birds <w>ate</w> all the cherries in our garden."
                            }
                        },
                        {
                            "text": "Туристы быстро <поели> свои припасы и отправились дальше.",
                            "target_lang_translations": {
                                "en": "The tourists quickly <w>ate</w> their supplies and went on."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To eat completely or consume supplies without remainder."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "eat up",
                                "target_lang_sense_clarification": "to finish eating everything"
                            },
                            {
                                "target_lang_word": "consume",
                                "target_lang_sense_clarification": "to use up supplies or food"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45c277ed-7418-3c80-a815-0eab395affde",
                    "sense_definition": "Испортить или повредить поверхность, грызя или разъедая (о насекомых, ржавчине, кислоте).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Моль <поела> старый шерстяной свитер в шкафу.",
                            "target_lang_translations": {
                                "en": "Moths <w>ate</w> the old wool sweater in the closet."
                            }
                        },
                        {
                            "text": "Ржавчина сильно <поела> железные ворота гаража.",
                            "target_lang_translations": {
                                "en": "Rust has severely <w>eaten</w> the iron garage gates."
                            }
                        }
                    ],
                    "common_phrases": [
                        "моль поела",
                        "ржавчина поела"
                    ],
                    "target_lang_definitions": {
                        "en": "To damage or destroy a surface by gnawing or corroding (of insects, rust, or acid)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "eat",
                                "target_lang_sense_clarification": "to damage by gnawing (e.g., moths)"
                            },
                            {
                                "target_lang_word": "eat away",
                                "target_lang_sense_clarification": "to erode or corrode gradually"
                            },
                            {
                                "target_lang_word": "corrode",
                                "target_lang_sense_clarification": "chemical destruction (e.g., rust)"
                            }
                        ]
                    }
                }
            ]
        }

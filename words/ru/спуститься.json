    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5271a567-3328-3f6a-8baa-c35be0c86dcf",
                    "sense_definition": "Переместиться откуда-либо сверху вниз (по лестнице, с горы, на лифте и т.п.).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_down",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <спуститься> по лестнице, так как лифт в доме не работал.",
                            "target_lang_translations": {
                                "en": "We decided to <go down> the stairs since the elevator in the building wasn't working."
                            }
                        },
                        {
                            "text": "Туристы успели <спуститься> с высокой горы до наступления темноты.",
                            "target_lang_translations": {
                                "en": "The tourists managed to <descend> the high mountain before darkness fell."
                            }
                        }
                    ],
                    "synonyms": [
                        "опуститься",
                        "снизиться"
                    ],
                    "antonyms": [
                        "подняться"
                    ],
                    "common_phrases": [
                        "спуститься по лестнице",
                        "спуститься в метро",
                        "спуститься на землю",
                        "спуститься с горы"
                    ],
                    "target_lang_definitions": {
                        "en": "To move from a higher place to a lower one (down stairs, from a mountain, in an elevator, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go down",
                                "target_lang_sense_clarification": "general movement to a lower place"
                            },
                            {
                                "target_lang_word": "come down",
                                "target_lang_sense_clarification": "movement towards the speaker or ground"
                            },
                            {
                                "target_lang_word": "descend",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "feaf4183-ca3f-3480-b1a6-73293a6bf91a",
                    "sense_definition": "Наступить, прийти, покрыть собой пространство (о времени суток, темноте, тумане или тишине).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_arrival",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На город неожиданно <спустилась> ночь, и на улицах зажглись яркие фонари.",
                            "target_lang_translations": {
                                "en": "Night unexpectedly <fell> upon the city, and bright streetlights lit up."
                            }
                        },
                        {
                            "text": "Густой туман <спустился> на долину, скрыв из виду соседние дома и деревья.",
                            "target_lang_translations": {
                                "en": "A thick fog <descended> on the valley, hiding neighboring houses and trees from view."
                            }
                        }
                    ],
                    "synonyms": [
                        "опуститься"
                    ],
                    "common_phrases": [
                        "спустилась ночь",
                        "спустился туман",
                        "спустились сумерки",
                        "спустилась тьма"
                    ],
                    "target_lang_definitions": {
                        "en": "To arrive, appear, or cover a space (referring to time of day, darkness, fog, or silence)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "descend",
                                "target_lang_sense_clarification": "literary, for darkness, fog, or silence"
                            },
                            {
                                "target_lang_word": "fall",
                                "target_lang_sense_clarification": "specifically for night, dusk, or darkness"
                            },
                            {
                                "target_lang_word": "settle",
                                "target_lang_sense_clarification": "for fog, mist, or silence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce5f7ea0-0a45-34c6-83c3-8c2c301f63dc",
                    "sense_definition": "Проплыть по течению реки в сторону устья.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "river_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Группа путешественников планировала <спуститься> по реке на лодках за три дня.",
                            "target_lang_translations": {
                                "en": "The group of travelers planned to <go down> the river on boats in three days."
                            }
                        },
                        {
                            "text": "Нам нужно <спуститься> вниз по течению, чтобы найти удобное место для лагеря.",
                            "target_lang_translations": {
                                "en": "We need to <go down> along the current to find a suitable place for camp."
                            }
                        }
                    ],
                    "antonyms": [
                        "подняться"
                    ],
                    "common_phrases": [
                        "спуститься по реке",
                        "спуститься вниз по течению",
                        "спуститься к морю"
                    ],
                    "target_lang_definitions": {
                        "en": "To travel along the current of a river towards its mouth."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go down",
                                "target_lang_sense_clarification": "travel downstream"
                            },
                            {
                                "target_lang_word": "descend",
                                "target_lang_sense_clarification": "formal, travel down a river"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4df18c95-36a9-33a6-acc2-b32f8601fde1",
                    "sense_definition": "Расплестись, утратить целостность (о петлях вязаного изделия).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "knitting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она заметила, что на её новом чулке <спустилась> петля, и очень расстроилась.",
                            "target_lang_translations": {
                                "en": "She noticed that a stitch had <dropped> in her new stocking, and was very upset."
                            }
                        },
                        {
                            "text": "Из-за маленькой зацепки на свитере <спустилась> нитка, и образовалась заметная дырка.",
                            "target_lang_translations": {
                                "en": "Because of a small snag on the sweater, a thread <unraveled>, and a noticeable hole formed."
                            }
                        }
                    ],
                    "common_phrases": [
                        "петля спустилась",
                        "чулок спустился"
                    ],
                    "target_lang_definitions": {
                        "en": "To come undone or lose integrity (referring to stitches in knitted fabric or hosiery)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drop",
                                "target_lang_sense_clarification": "of a single stitch"
                            },
                            {
                                "target_lang_word": "run",
                                "target_lang_sense_clarification": "of stockings or tights"
                            },
                            {
                                "target_lang_word": "unravel",
                                "target_lang_sense_clarification": "of thread or fabric"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "455af3bb-1f7a-37ff-bd5a-645d3fa70000",
                    "sense_definition": "Морской термин: отойти от линии ветра, увеличивая угол между направлением ветра и курсом корабля.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Капитан отдал приказ <спуститься> под ветер, чтобы избежать столкновения со скалами.",
                            "target_lang_translations": {
                                "en": "The captain gave the order to <bear away> to avoid a collision with the rocks."
                            }
                        },
                        {
                            "text": "Судно начало медленно <спуститься>, меняя курс в соответствии с погодными условиями.",
                            "target_lang_translations": {
                                "en": "The vessel began to slowly <fall off>, changing course according to the weather conditions."
                            }
                        }
                    ],
                    "common_phrases": [
                        "спуститься под ветер"
                    ],
                    "target_lang_definitions": {
                        "en": "Nautical term: to move away from the direction of the wind."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bear away",
                                "target_lang_sense_clarification": "nautical"
                            },
                            {
                                "target_lang_word": "fall off",
                                "target_lang_sense_clarification": "nautical"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
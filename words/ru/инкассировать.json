    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4c42860b-b4f0-35f2-8e0e-2b683da2ba1e",
                    "sense_definition": "Осуществлять сбор наличных денежных средств или ценностей для их транспортировки и сдачи в банк.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "financial_operations",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Специальная служба приезжает каждый вечер, чтобы <инкассировать> дневную выручку магазина.",
                            "target_lang_translations": {
                                "en": "A special service comes every evening to <collect> the shop's daily revenue."
                            }
                        },
                        {
                            "text": "Банк обязан своевременно <инкассировать> денежные средства своих клиентов.",
                            "target_lang_translations": {
                                "en": "The bank is obliged to <collect> its clients' funds in a timely manner."
                            }
                        }
                    ],
                    "common_phrases": [
                        "инкассировать выручку",
                        "инкассировать деньги",
                        "инкассировать наличность"
                    ],
                    "target_lang_definitions": {
                        "en": "To collect cash or valuables for transport and deposit in a bank."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "collect",
                                "target_lang_sense_clarification": "money or revenue for deposit"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d109750-60f0-3d04-a760-f6e482fe1fc7",
                    "sense_definition": "Получать наличные деньги в банке при предъявлении платёжного документа (чека, векселя).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "financial_operations",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Предприниматель решил <инкассировать> вексель в центральном отделении банка.",
                            "target_lang_translations": {
                                "en": "The entrepreneur decided to <encash> the bill of exchange at the central bank branch."
                            }
                        },
                        {
                            "text": "Нам удалось быстро <инкассировать> чек и получить необходимые наличные.",
                            "target_lang_translations": {
                                "en": "We managed to quickly <cash> the check and get the necessary cash."
                            }
                        }
                    ],
                    "synonyms": [
                        "обналичивать"
                    ],
                    "common_phrases": [
                        "инкассировать вексель",
                        "инкассировать чек"
                    ],
                    "target_lang_definitions": {
                        "en": "To receive cash at a bank by presenting a payment document (check, bill of exchange)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cash",
                                "target_lang_sense_clarification": "a check or financial instrument"
                            },
                            {
                                "target_lang_word": "encash",
                                "target_lang_sense_clarification": "formal; convert into cash"
                            }
                        ]
                    }
                }
            ]
        }
    ]

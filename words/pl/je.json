            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "5239f6a6-f32a-36f0-b9c5-d12a20b18066",
                    "sense_definition": "Forma biernika liczby pojedynczej zaimka <w>ono</w> (rodzaj nijaki) lub biernika liczby mnogiej zaimka <w>one</w> (rodzaj niemęskoosobowy).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest małe dziecko, trzeba <w>je</w> nakarmić.",
                            "target_lang_translations": {
                                "ru": "Это маленький ребенок, нужно <w>его</w> покормить."
                            }
                        },
                        {
                            "text": "Kupiłam świeże jabłka i zaraz <w>je</w> umyję.",
                            "target_lang_translations": {
                                "ru": "Я купила свежие яблоки и сейчас <w>их</w> помою."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма винительного падежа единственного числа местоимения «ono» (оно) или винительного падежа множественного числа местоимения «one» (они, для неодушевленных предметов и животных)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "его",
                                "target_lang_sense_clarification": "винительный падеж, средний род"
                            },
                            {
                                "target_lang_word": "их",
                                "target_lang_sense_clarification": "винительный падеж, множественное число"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ba51226a-13a3-3e98-99e1-2fb0ed20537c",
                    "sense_definition": "Trzecia osoba liczby pojedynczej czasu teraźniejszego czasownika <w>jeść</w>; oznacza czynność spożywania pokarmu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "eating_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat codziennie <w>je</w> owsiankę na śniadanie.",
                            "target_lang_translations": {
                                "ru": "Мой брат каждый день <w>ест</w> овсянку на завтрак."
                            }
                        },
                        {
                            "text": "Kto nie pracuje, ten nie <w>je</w>.",
                            "target_lang_translations": {
                                "ru": "Кто не работает, тот не <w>ест</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "spożywa",
                        "konsumuje",
                        "wcina",
                        "pałaszuje"
                    ],
                    "antonyms": [
                        "pości",
                        "głoduje"
                    ],
                    "common_phrases": [
                        "je oczami",
                        "je z ręki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Третье лицо единственного числа настоящего времени глагола «jeść» (есть); обозначает процесс приема пищи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ест",
                                "target_lang_sense_clarification": "основное значение"
                            },
                            {
                                "target_lang_word": "кушает",
                                "target_lang_sense_clarification": "разговорное, вежливое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7fbfb05-6094-3d12-820d-b948a42478b4",
                    "sense_definition": "Regionalna forma czasownika <w>być</w> w trzeciej osobie liczby pojedynczej (odpowiednik ogólnopolskiego „jest”).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "being_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To <w>je</w> bardzo dobry człowiek, każdemu pomoże.",
                            "target_lang_translations": {
                                "ru": "Это <w>есть</w> очень хороший человек, каждому поможет."
                            }
                        },
                        {
                            "text": "Kaj on teraz <w>je</w>, że go nie ma w domu?",
                            "target_lang_translations": {
                                "ru": "Где он сейчас <w>есть</w>, что его нет дома?"
                            }

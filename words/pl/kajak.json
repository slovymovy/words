                {
                    "sense_id": "e647546d-3e81-38ea-814a-d6b197a70854",
                    "sense_definition": "Niewielka łódź zakryta z wierzchu, z otworem dla załogi, poruszana wiosłami o dwóch piórach",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W przyszłym roku wybiorę się z synem na <kajaki>.",
                            "target_lang_translations": {
                                "ru": "В следующем году я выберусь с сыном на <байдарки>."
                            }
                        },
                        {
                            "text": "Załoga <kajaka> siedzi twarzą do kierunku płynięcia.",
                            "target_lang_translations": {
                                "ru": "Экипаж <байдарки> сидит лицом по направлению движения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "spływ kajakowy",
                        "płynąć kajakiem",
                        "kajak jednoosobowy",
                        "wypożyczalnia kajaków"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая лодка, закрытая сверху, с отверстием для экипажа, приводимая в движение двухлопастным веслом"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "байдарка",
                                "target_lang_sense_clarification": "спортивная или туристическая лодка"
                            },
                            {
                                "target_lang_word": "каяк",
                                "target_lang_sense_clarification": "тип гребной лодки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "359d0c68-4ff5-303e-9bef-c7fc29373ffb",
                    "sense_definition": "Bardzo duży but lub nienaturalnie duża stopa",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zdejmij te brudne <kajaki> przed wejściem do salonu.",
                            "target_lang_translations": {
                                "ru": "Сними эти грязные <лыжи> перед входом в гостиную."
                            }
                        },
                        {
                            "text": "Masz takie wielkie <kajaki>, że trudno ci kupić wygodne obuwie.",
                            "target_lang_translations": {
                                "ru": "У тебя такие огромные <ласты>, что тебе трудно купить удобную обувь."
                            }
                        }
                    ],

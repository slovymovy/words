{
    "word_family": [
        "exchanger",
        "exchangeable",
        "interchange"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e8b51ca9-4e9e-3fae-92b2-f55557a531ac",
                    "sense_definition": "An act of giving one thing and receiving another in return.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "trade_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I gave him my apple in <w>exchange</w> for his orange.",
                            "target_lang_translations": {
                                "ru": "Я дал ему свое яблоко в <w>обмен</w> на его апельсин."
                            }
                        },
                        {
                            "text": "The <w>exchange</w> of prisoners took place at the border.",
                            "target_lang_translations": {
                                "ru": "<w>Обмен</w> пленными состоялся на границе."
                            }
                        }
                    ],
                    "synonyms": [
                        "trade",
                        "swap",
                        "barter"
                    ],
                    "common_phrases": [
                        "in exchange",
                        "fair exchange"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по передаче одного предмета и получению другого взамен."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обмен",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "бартер",
                                "target_lang_sense_clarification": "товарообмен"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e9272ed-3a7e-348d-84e7-1b92d8d8d0a5",
                    "sense_definition": "A building or institution where trading, especially of stocks or commodities, takes place.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "trade_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Traders shouted orders across the floor of the stock <w>exchange</w>.",
                            "target_lang_translations": {
                                "ru": "Трейдеры выкрикивали приказы в зале фондовой <w>биржи</w>."
                            }
                        },
                        {
                            "text": "The old corn <w>exchange</w> has been converted into a music venue.",
                            "target_lang_translations": {
                                "ru": "Старая хлебная <w>биржа</w> была переоборудована в музыкальную площадку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stock exchange",
                        "currency exchange"
                    ],
                    "target_lang_definitions": {
                        "ru": "Здание или учреждение, где ведутся торги, особенно акциями или товарами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "биржа",
                                "target_lang_sense_clarification": "учреждение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "08e05a44-0151-37ce-b715-a3d010135432",
                    "sense_definition": "A short conversation or argument between people.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After a sharp <w>exchange</w> of words, they stopped speaking to each other.",
                            "target_lang_translations": {
                                "ru": "После резкого <w>обмена</w> словами они перестали разговаривать друг с другом."
                            }
                        },
                        {
                            "text": "I overheard an interesting <w>exchange</w> between the two managers.",
                            "target_lang_translations": {
                                "ru": "Я подслушал интересный <w>обмен</w> мнениями между двумя менеджерами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "heated exchange",
                        "brief exchange"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткий разговор или спор между людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обмен",
                                "target_lang_sense_clarification": "словами, мнениями"
                            },
                            {
                                "target_lang_word": "перепалка",
                                "target_lang_sense_clarification": "спор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da645011-58ab-3c33-b436-539e7b97bfe7",
                    "sense_definition": "The transfer of substances or elements between systems, often in a biological or chemical context.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "scientific_transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gas <w>exchange</w> occurs in the lungs during respiration.",
                            "target_lang_translations": {
                                "ru": "<w>Обмен</w> газов происходит в легких во время дыхания."
                            }
                        },
                        {
                            "text": "The experiment observed the ion <w>exchange</w> across the membrane.",
                            "target_lang_translations": {
                                "ru": "В ходе эксперимента наблюдался ионный <w>обмен</w> через мембрану."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gas exchange",
                        "ion exchange"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перенос веществ или элементов между системами, часто в биологическом или химическом контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обмен",
                                "target_lang_sense_clarification": "веществ, газов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "372b14a2-b047-3c4a-a213-cd77d348dcee",
                    "sense_definition": "The central office of a telephone system, or the specific digits in a phone number identifying it.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "telecom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The local telephone <w>exchange</w> handles all calls for this town.",
                            "target_lang_translations": {
                                "ru": "Местная телефонная <w>станция</w> обрабатывает все звонки для этого города."
                            }
                        },
                        {
                            "text": "In the old days, you could tell where someone lived by their <w>exchange</w>.",
                            "target_lang_translations": {
                                "ru": "В прежние времена можно было определить, где живет человек, по его <w>АТС</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "telephone exchange"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "referring to the specific number prefix or physical office"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Центральный узел телефонной сети или цифры в номере, идентифицирующие его."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "станция",
                                "target_lang_sense_clarification": "телефонная"
                            },
                            {
                                "target_lang_word": "АТС",
                                "target_lang_sense_clarification": "автоматическая телефонная станция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed44c176-bbb6-30ee-84ed-140cc0e675aa",
                    "sense_definition": "In chess, the capture of a more valuable piece in return for a less valuable one.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "chess",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "White won the <w>exchange</w> by taking the rook for a knight.",
                            "target_lang_translations": {
                                "ru": "Белые выиграли <w>качество</w>, взяв ладью за коня."
                            }
                        },
                        {
                            "text": "Losing the <w>exchange</w> early in the game put him at a disadvantage.",
                            "target_lang_translations": {
                                "ru": "Потеря <w>качества</w> в начале партии поставила его в невыгодное положение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "win the exchange"
                    ],
                    "target_lang_definitions": {
                        "ru": "В шахматах: выигрыш более ценной фигуры в обмен на менее ценную."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "качество",
                                "target_lang_sense_clarification": "шахматный термин"
                            },
                            {
                                "target_lang_word": "размен",
                                "target_lang_sense_clarification": "фигур"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "860d60cc-0d4f-3a0d-a34c-aaddab59ffb8",
                    "sense_definition": "To give something to someone and receive something else from them.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "trade_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>exchanged</w> addresses before parting ways at the station.",
                            "target_lang_translations": {
                                "ru": "Они <w>обменялись</w> адресами перед тем, как расстаться на вокзале."
                            }
                        },
                        {
                            "text": "We usually <w>exchange</w> gifts with our neighbors on Christmas Eve.",
                            "target_lang_translations": {
                                "ru": "Мы обычно <w>обмениваемся</w> подарками с нашими соседями в канун Рождества."
                            }
                        }
                    ],
                    "synonyms": [
                        "trade",
                        "swap",
                        "barter"
                    ],
                    "common_phrases": [
                        "exchange gifts",
                        "exchange ideas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать что-то кому-то и получать что-то другое от него."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обмениваться",
                                "target_lang_sense_clarification": "чем-либо с кем-либо"
                            },
                            {
                                "target_lang_word": "меняться",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e138665-f820-387d-8bcf-fd0b57d94254",
                    "sense_definition": "To return a purchased item to a store and get a different one in its place.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "replacement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I would like to <w>exchange</w> this dress for a smaller size.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <w>обменять</w> это платье на размер меньше."
                            }
                        },
                        {
                            "text": "The store allows you to <w>exchange</w> items within thirty days of purchase.",
                            "target_lang_translations": {
                                "ru": "Магазин разрешает <w>обменивать</w> товары в течение тридцати дней с момента покупки."
                            }
                        }
                    ],
                    "synonyms": [
                        "swap",
                        "interchange"
                    ],
                    "common_phrases": [
                        "exchange for"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вернуть купленный товар в магазин и получить другой взамен."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обменять",
                                "target_lang_sense_clarification": "товар"
                            },
                            {
                                "target_lang_word": "поменять",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d058d30a-25aa-36b1-ba3e-7c3158b3e92e",
                    "sense_definition": "To direct words, looks, or actions at each other at the same time.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two armies <w>exchanged</w> fire for several hours across the valley.",
                            "target_lang_translations": {
                                "ru": "Две армии <w>обменивались</w> огнем в течение нескольких часов через долину."
                            }
                        },
                        {
                            "text": "They <w>exchanged</w> knowing looks across the dinner table.",
                            "target_lang_translations": {
                                "ru": "Они <w>обменялись</w> понимающими взглядами за обеденным столом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "exchange fire",
                        "exchange glances",
                        "exchange words"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направлять слова, взгляды или действия друг на друга одновременно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обмениваться",
                                "target_lang_sense_clarification": "взглядами, репликами"
                            },
                            {
                                "target_lang_word": "перебрасываться",
                                "target_lang_sense_clarification": "словами"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
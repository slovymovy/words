                {
                    "sense_id": "9962ca7e-cde4-3a54-8551-d6f22d3e86fc",
                    "sense_definition": "Posiadający dużą wiedzę, umiejętności i praktykę w danej dziedzinie; będący na wyższym poziomie nauczania.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "skill_level",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten podręcznik jest przeznaczony dla uczniów na poziomie <w>zaawansowanym</w>.",
                            "target_lang_translations": {
                                "ru": "Этот учебник предназначен для учеников на <w>продвинутом</w> уровне."
                            }
                        },
                        {
                            "text": "Mój brat jest bardzo <w>zaawansowanym</w> graczem szachowym i rzadko przegrywa.",
                            "target_lang_translations": {
                                "ru": "Мой брат — очень <w>продвинутый</w> шахматист, и он редко проигрывает."
                            }
                        }
                    ],
                    "synonyms": [
                        "doświadczony"
                    ],
                    "antonyms": [
                        "początkujący"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dd8d5b75-439d-3bcf-9771-8dadcac566c9",
                    "sense_definition": "Приставать, прочно соединяться с поверхностью чего-либо (обычно с помощью клея или липкого вещества).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_sticking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мокрый снег мгновенно <прилепляется> к лобовому стеклу автомобиля.",
                            "target_lang_translations": {
                                "en": "Wet snow instantly <w>sticks</w> to the car's windshield."
                            }
                        },
                        {
                            "text": "Эти дешёвые наклейки плохо <прилепляются> к пластику.",
                            "target_lang_translations": {
                                "en": "These cheap stickers do not <w>stick</w> well to plastic."
                            }
                        }
                    ],
                    "synonyms": [
                        "прилипать",
                        "клеиться",
                        "приставать"
                    ],
                    "antonyms": [
                        "отлепляться",
                        "отпадать"
                    ],
                    "common_phrases": [
                        "прилепляться к стене",
                        "прилепляться намертво",
                        "плохо прилепляться"
                    ],
                    "target_lang_definitions": {
                        "en": "To adhere or stick firmly to a surface."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stick",
                                "target_lang_sense_clarification": "general term"
                            },
                            {
                                "target_lang_word": "adhere",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c316fd6-1e7e-3b07-a343-ed4477bd8afe",
                    "sense_definition": "Страдательный залог к глаголу <w>прилеплять</w>; быть прикрепляемым кем-то.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "passive_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Объявление <прилепляется> на доску с помощью скотча.",
                            "target_lang_translations": {
                                "en": "The announcement <w>is attached</w> to the board with tape."
                            }
                        },
                        {
                            "text": "Почтовая марка аккуратно <прилепляется> в правый верхний угол конверта.",
                            "target_lang_translations": {
                                "en": "The postage stamp <w>is pasted</w> carefully in the upper right corner of the envelope."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "passive voice"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be attached or pasted onto something (passive form)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be attached",
                                "target_lang_sense_clarification": "general passive"
                            },
                            {
                                "target_lang_word": "be stuck",
                                "target_lang_sense_clarification": "common usage"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80e6ca96-d505-3e87-908d-23e54e618107",
                    "sense_definition": "Вплотную прислоняться, прижиматься или располагаться очень близко к кому-либо или чему-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_proximity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Испуганный котёнок <прилепляется> к тёплой батарее.",
                            "target_lang_translations": {
                                "en": "The frightened kitten <w>clings</w> to the warm radiator."
                            }
                        },
                        {
                            "text": "В переполненном автобусе пассажиры невольно <прилепляются> друг к другу.",
                            "target_lang_translations": {
                                "en": "In the crowded bus, passengers involuntarily <w>press</w> against each other."
                            }
                        }
                    ],
                    "synonyms": [
                        "прижиматься",
                        "льнуть"
                    ],
                    "antonyms": [
                        "отодвигаться",
                        "отстраняться"
                    ],
                    "common_phrases": [
                        "прилепляться к окну",
                        "прилепляться к плечу"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To press closely against or cling to someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cling",
                                "target_lang_sense_clarification": "hold on tightly"
                            },
                            {
                                "target_lang_word": "press against",
                                "target_lang_sense_clarification": "lean or touch closely"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df602000-28e4-360e-8bb8-1546fa9e15ed",
                    "sense_definition": "Испытывать сильную душевную привязанность, любовь или преданность к кому-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_attachment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ребёнок быстро <прилепляется> к новым воспитателям.",
                            "target_lang_translations": {
                                "en": "The child quickly <w>gets attached</w> to new caregivers."
                            }
                        },
                        {
                            "text": "Человек со временем <прилепляется> душой к родным местам.",
                            "target_lang_translations": {
                                "en": "Over time, a person <w>becomes attached</w> with their soul to their native places."
                            }
                        }
                    ],
                    "synonyms": [
                        "привязываться"
                    ],
                    "antonyms": [
                        "отвыкать",
                        "охладевать"
                    ],
                    "common_phrases": [
                        "прилепляться душой",
                        "прилепляться сердцем"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To become emotionally attached or devoted to someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get attached",
                                "target_lang_sense_clarification": "emotional connection"
                            },
                            {
                                "target_lang_word": "become attached",
                                "target_lang_sense_clarification": "emotional connection"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8a6db67-00df-331e-82dc-18b600e088f0",
                    "sense_definition": "Становиться постоянным, закрепляться за кем-либо (о прозвище, кличке или названии).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_name",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Обидное прозвище <прилепляется> к нему ещё в школьные годы.",
                            "target_lang_translations": {
                                "en": "The offensive nickname <w>sticks</w> to him back in his school years."
                            }
                        },
                        {
                            "text": "Иногда случайная кличка <прилепляется> к человеку на всю жизнь.",
                            "target_lang_translations": {
                                "en": "Sometimes a random nickname <w>sticks</w> to a person for life."
                            }
                        }
                    ],
                    "synonyms": [
                        "приставать",
                        "закрепляться"
                    ],
                    "common_phrases": [
                        "кличка прилепляется",
                        "прозвище прилепляется"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To become permanently associated with someone (regarding a nickname or label)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stick",
                                "target_lang_sense_clarification": "of a name or label"
                            }
                        ]
                    }
                }
            ]
        }
    ]

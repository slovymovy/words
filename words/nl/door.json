                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "through",
                                "target_lang_sense_clarification": "mixed into or among"
                            },
                            {
                                "target_lang_word": "mixed with",
                                "target_lang_sense_clarification": "combined with"
                            },
                            {
                                "target_lang_word": "in",
                                "target_lang_sense_clarification": "contained within a mixture"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "10673c40-18a8-33f9-bfd4-7183b259079b",
                    "sense_definition": "Verder; geeft aan dat een handeling of situatie onverminderd voortgaat.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "continuation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ondanks de regen ging het feest buiten gewoon <w>door</w>.",
                            "target_lang_translations": {
                                "en": "Despite the rain, the party outside just went <w>on</w>."
                            }
                        },
                        {
                            "text": "Als je moe bent, is het soms lastig om <w>door</w> te gaan.",
                            "target_lang_translations": {
                                "en": "When you are tired, it is sometimes difficult to go <w>on</w>."
                            }

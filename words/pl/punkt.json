                {
                    "sense_id": "526eb1b6-17ee-3ec9-9dcc-e1fc2f9c9e74",
                    "sense_definition": "Jednostka służąca do oceny wyniku w sporcie, grach lub systemach klasyfikacji.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "competition_scoring",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza drużyna zdobyła decydujący <punkt> w ostatniej minucie meczu.",

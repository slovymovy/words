    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "de56d9ba-cf8b-3654-9b10-4abd5d1ae75c",
                    "sense_definition": "Zespół muzyczny złożony z dwóch osób.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "music_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na firmową imprezę szef zaprosił znany <w>duet</w> smyczkowy.",
                            "target_lang_translations": {
                                "ru": "На корпоративную вечеринку начальник пригласил известный струнный <w>дуэт</w>."
                            }
                        },
                        {
                            "text": "Mój brat i jego kolega założyli hip-hopowy <w>duet</w> i nagrywają piosenki.",
                            "target_lang_translations": {
                                "ru": "Мой брат и его друг создали хип-хоп <w>дуэт</w> и записывают песни."
                            }
                        }
                    ],
                    "synonyms": [
                        "duo"
                    ],
                    "antonyms": [
                        "solista"
                    ],
                    "common_phrases": [
                        "zgrany duet",
                        "duet fortepianowy",
                        "występować w duecie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкальный коллектив, состоящий из двух человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дуэт",
                                "target_lang_sense_clarification": "музыкальная группа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c680eb2c-9dc0-36e0-9864-fe103546a7c2",
                    "sense_definition": "Dwie osoby współpracujące ze sobą, występujące razem lub tworzące parę w określonym kontekście.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "people_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako napastnicy tworzą na boisku niezwykle skuteczny <w>duet</w>.",
                            "target_lang_translations": {
                                "ru": "Как нападающие, они образуют на поле невероятно эффективный <w>дуэт</w>."
                            }
                        },
                        {
                            "text": "W pracy biurowej stanowimy z Markiem bardzo zorganizowany <w>duet</w>.",
                            "target_lang_translations": {
                                "ru": "В офисной работе мы с Марком составляем очень организованный <w>дуэт</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "para"
                    ],
                    "common_phrases": [
                        "tworzyć duet",
                        "świetny duet",
                        "egzotyczny duet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Два человека, работающие вместе, выступающие сообща или образующие пару в определенном контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дуэт",
                                "target_lang_sense_clarification": "пара людей"
                            },
                            {
                                "target_lang_word": "тандем",
                                "target_lang_sense_clarification": "сотрудничество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7c29963-36d2-3bea-80b9-c14a182f69ff",
                    "sense_definition": "Utwór muzyczny przeznaczony dla dwóch instrumentów lub na dwa głosy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "music_piece",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <w>duet</w> na skrzypce i wiolonczelę jest bardzo trudny do zagrania.",
                            "target_lang_translations": {
                                "ru": "Этот <w>дуэт</w> для скрипки и виолончели очень сложен для исполнения."
                            }
                        },
                        {
                            "text": "Podczas konkursu wokalnego uczestnicy musieli wykonać klasyczny <w>duet</w> operowy.",
                            "target_lang_translations": {
                                "ru": "Во время вокального конкурса участники должны были исполнить классический оперный <w>дуэт</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "duo"
                    ],
                    "antonyms": [
                        "solo"
                    ],
                    "common_phrases": [
                        "zaśpiewać w duecie",
                        "duet operowy",
                        "duet miłosny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкальное произведение, предназначенное для двух инструментов или двух голосов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дуэт",
                                "target_lang_sense_clarification": "музыкальное произведение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d949a4d7-0467-3949-bbec-80a0e311ef0f",
                    "sense_definition": "Ciepłe nakrycie głowy noszone w XVII i XVIII wieku.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "clothing_history",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na starym portrecie szlachcic ma na głowie kosztowny <w>duet</w>.",
                            "target_lang_translations": {
                                "ru": "На старинном портрете у дворянина на голове дорогой <w>дуэт</w>."
                            }
                        },
                        {
                            "text": "W muzeum historycznym można zobaczyć, jak wyglądał dawny <w>duet</w>.",
                            "target_lang_translations": {
                                "ru": "В историческом музее можно увидеть, как выглядел старинный <w>дуэт</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "używane w odniesieniu do mody XVII–XVIII w."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Теплый головной убор, который носили в XVII и XVIII веках."
                    },

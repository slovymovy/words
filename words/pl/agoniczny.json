                },
                {
                    "sense_id": "faedcb35-74b6-340c-b474-9087e76acd51",
                    "sense_definition": "Dotyczący linii na mapie łączącej punkty o zerowej deklinacji magnetycznej.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {

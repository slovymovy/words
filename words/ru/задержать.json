    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fd9a9d63-1ff8-3c97-b63d-8ad19b47c36a",
                    "sense_definition": "Остановить кого-либо или что-либо, не дать уйти или двигаться дальше; заставить остаться где-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "stop_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Простите, я не хочу вас <w>задерживать</w>, если вы спешите."
                        },
                        {
                            "text": "Сильный дождь <w>задержал</w> туристов в горах на два дня."
                        }
                    ],
                    "synonyms": [
                        "остановить"
                    ],
                    "antonyms": [
                        "пропустить",
                        "отпустить"
                    ],
                    "common_phrases": [
                        "задержать взгляд",
                        "задержать на работе",
                        "задержать в дверях"
                    ]
                },
                {
                    "sense_id": "896dab48-ea18-37e5-9742-6384ebc6da10",
                    "sense_definition": "Замедлить выполнение чего-либо, отложить на более позднее время или временно приостановить.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "delay_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за плохой погоды диспетчеры решили <w>задержать</w> вылет самолёта."
                        },
                        {
                            "text": "Он глубоко вдохнул и <w>задержал</w> дыхание под водой."
                        }
                    ],
                    "synonyms": [
                        "отложить",
                        "замедлить",
                        "приостановить"
                    ],
                    "antonyms": [
                        "ускорить"
                    ],
                    "common_phrases": [
                        "задержать дыхание",
                        "задержать рейс",
                        "задержать отправку",
                        "задержать ответ"
                    ]
                },
                {
                    "sense_id": "de7e2e00-268b-329a-b8b6-3ff2942014f9",
                    "sense_definition": "Взять под стражу, арестовать, лишить свободы (обычно о действиях правоохранительных органов).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "arrest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полиция смогла <w>задержать</w> преступника по горячим следам."
                        },
                        {
                            "text": "Подозреваемого <w>задержали</w> на сорок восемь часов для выяснения обстоятельств."
                        }
                    ],
                    "synonyms": [
                        "арестовать"
                    ],
                    "antonyms": [
                        "освободить",
                        "выпустить"
                    ],
                    "common_phrases": [
                        "задержать преступника",
                        "задержать подозреваемого",
                        "задержать нарушителя"
                    ]
                },
                {
                    "sense_id": "02c33fc7-699d-3934-a5c8-c56cca49b852",
                    "sense_definition": "Не выдать, не вернуть или не выплатить что-либо в установленный срок.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "withhold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В связи с кризисом компания была вынуждена <w>задержать</w> зарплату сотрудникам."
                        },
                        {
                            "text": "Библиотекарь попросил читателей не <w>задерживать</w> редкие книги."
                        }
                    ],
                    "synonyms": [
                        "придержать"
                    ],
                    "antonyms": [
                        "выдать",
                        "выплатить"
                    ],
                    "common_phrases": [
                        "задержать зарплату",
                        "задержать выплату",
                        "задержать документы"
                    ]
                }
            ]
        }
    ]

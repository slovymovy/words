            "senses": [
                {
                    "sense_id": "05c60615-b25d-32e6-a90e-e1ecda939f73",
                    "sense_definition": "Pospolity płaz bezogonowy, który żyje w wodzie i na lądzie oraz porusza się skokami.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mała, zielona <w>żaba</w> siedzi na dużym liściu lilii wodnej.",
                            "target_lang_translations": {
                                "ru": "Маленькая зеленая <w>лягушка</w> сидит на большом листе кувшинки."
                            }
                        },
                        {
                            "text": "Wczoraj nad stawem widziałem, jak <w>żaba</w> wskoczyła do wody.",
                            "target_lang_translations": {
                                "ru": "Вчера у пруда я видел, как <w>лягушка</w> прыгнула в воду."
                            }
                        }
                    ],
                    "synonyms": [
                        "żabka"
                    ],
                    "common_phrases": [
                        "zielona żaba",
                        "rechot żab",
                        "skakać jak żaba"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обычное бесхвостое земноводное, которое живет в воде и на суше и передвигается прыжками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лягушка",
                                "target_lang_sense_clarification": "земноводное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7698540d-e31c-3b96-bbcf-105d14fb7679",
                    "sense_definition": "Styl pływacki, w którym ruchy rąk i nóg przypominają sposób pływania żaby.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat najlepiej lubi pływać <w>żabą</w>, bo mniej się męczy.",
                            "target_lang_translations": {
                                "ru": "Мой брат больше всего любит плавать <w>брассом</w>, потому что так меньше устает."
                            }
                        },
                        {
                            "text": "Instruktor pokazał nam, jak poprawnie technicznie pływać <w>żabą</w>.",
                            "target_lang_translations": {
                                "ru": "Инструктор показал нам, как технически правильно плавать <w>брассом</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pływać żabą",
                        "styl żaba"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стиль плавания, при котором движения рук и ног напоминают плавание лягушки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брасс",
                                "target_lang_sense_clarification": "стиль плавания"
                            },
                            {
                                "target_lang_word": "по-лягушачьи",
                                "target_lang_sense_clarification": "разговорное описание стиля"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b64386eb-2b06-3cbf-8202-8ff82aa37d86",
                    "sense_definition": "Rodzaj regulowanego klucza hydraulicznego służącego do chwytania rur.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tools",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hydraulik musiał użyć dużej <w>żaby</w>, aby odkręcić zardzewiałą rurę.",
                            "target_lang_translations": {
                                "ru": "Сантехнику пришлось использовать большой <w>газовый ключ</w>, чтобы открутить ржавую трубу."
                            }
                        },
                        {
                            "text": "Podaj mi z walizki czerwoną <w>żabę</w>, bo ten klucz nie pasuje.",
                            "target_lang_translations": {
                                "ru": "Подай мне из чемодана красный <w>газовый ключ</w>, а то этот не подходит."
                            }
                        }
                    ],
                    "common_phrases": [
                        "klucz żaba",
                        "nastawna żaba"

{
    "word_family": [
        "rijf"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0bc78fc2-d415-3892-ba2a-30f177be7075",
                    "sense_definition": "Met een rijf (hark) bijeenhalen of de grond ermee effenen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "gardening_agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de herfst moeten we elk weekend de gevallen bladeren <rijven>.",
                            "target_lang_translations": {
                                "en": "In autumn, we have to <rake> the fallen leaves every weekend."
                            }
                        },
                        {
                            "text": "De tuinman is het gemaaide gras aan het <rijven>.",
                            "target_lang_translations": {
                                "en": "The gardener is <raking> the mown grass."
                            }
                        }
                    ],
                    "synonyms": [
                        "harken"
                    ],
                    "common_phrases": [
                        "bladeren rijven",
                        "hooi rijven",
                        "gras rijven"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral gebruikelijk in België (Vlaanderen); in Nederland is 'harken' standaard"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To gather together with a rake or to level the ground with it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rake",
                                "target_lang_sense_clarification": "gardening action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c88eb621-e1a1-3f5e-94c3-10136fcc9c2f",
                    "sense_definition": "Iets fijnmaken of in kleine stukjes verdelen door het over een ruw oppervlak te wrijven.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "culinary_processing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor dit gerecht moet je wat oude kaas <rijven>.",
                            "target_lang_translations": {
                                "en": "For this dish, you need to <grate> some aged cheese."
                            }
                        },
                        {
                            "text": "Zij gebruikt een grove rasp om de wortels te <rijven>.",
                            "target_lang_translations": {
                                "en": "She uses a coarse grater to <grate> the carrots."
                            }
                        }
                    ],
                    "synonyms": [
                        "raspen"
                    ],
                    "common_phrases": [
                        "kaas rijven",
                        "groenten rijven"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in Zuid-Nederland en België"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To make fine or divide into small pieces by rubbing it over a rough surface."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grate",
                                "target_lang_sense_clarification": "culinary processing"
                            },
                            {
                                "target_lang_word": "shred",
                                "target_lang_sense_clarification": "food preparation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ca82232-bd92-3f18-bbd4-3b4b2581eee2",
                    "sense_definition": "Het oppervlak van de zeilen verkleinen door een deel ervan op te binden, meestal bij harde wind.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Toen de storm opstak, gaf de kapitein het bevel om de zeilen te <rijven>.",
                            "target_lang_translations": {
                                "en": "When the storm picked up, the captain gave the order to <reef> the sails."
                            }
                        },
                        {
                            "text": "Het is veiliger om tijdig te <rijven> als de wind plotseling toeneemt.",
                            "target_lang_translations": {
                                "en": "It is safer to <reef> in time if the wind suddenly increases."
                            }
                        }
                    ],
                    "synonyms": [
                        "reven"
                    ],
                    "common_phrases": [
                        "de zeilen rijven"
                    ],
                    "target_lang_definitions": {
                        "en": "To reduce the surface area of sails by tying up a part of them, usually during strong winds."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reef",
                                "target_lang_sense_clarification": "nautical action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1302079-d31e-3a9d-9b97-469f0f27177b",
                    "sense_definition": "Zich verbergen of ergens onder kruipen uit angst of voor bescherming.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "movement_hiding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bange hond probeerde onder de tafel te <rijven>.",
                            "target_lang_translations": {
                                "en": "The scared dog tried to <take shelter> under the table."
                            }
                        },
                        {
                            "text": "Tijdens de zware regenbui gingen we <rijven> in het schuurtje.",
                            "target_lang_translations": {
                                "en": "During the heavy rain shower, we went to <take shelter> in the shed."
                            }
                        }
                    ],
                    "synonyms": [
                        "schuilen"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To hide oneself or crawl under something out of fear or for protection."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take shelter",
                                "target_lang_sense_clarification": "seek protection"
                            },
                            {
                                "target_lang_word": "hide",
                                "target_lang_sense_clarification": "conceal oneself"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7f5bb923-c214-3d2d-99f4-bf72a1b959f8",
                    "sense_definition": "Meervoudsvorm van <w>rijf</w> (een stuk gereedschap met tanden om materiaal bijeen te harken).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "tools",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boer heeft twee nieuwe <rijven> gekocht voor het hooiseizoen.",
                            "target_lang_translations": {
                                "en": "The farmer bought two new <rakes> for the hay season."
                            }
                        },
                        {
                            "text": "Zet de houten <rijven> na gebruik terug in de schuur.",
                            "target_lang_translations": {
                                "en": "Put the wooden <rakes> back in the shed after use."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural form of 'rijf' (a tool with teeth for gathering material)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rakes",
                                "target_lang_sense_clarification": "plural of the tool"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
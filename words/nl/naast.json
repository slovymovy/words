            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b04c4043-2ab0-3a0d-9b51-e5fe68caa720",
                    "sense_definition": "Vervoeging van <w>naasten</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gemeente <w>naast</w> het stuk grond voor de aanleg van een weg.",
                            "target_lang_translations": {
                                "ru": "Муниципалитет <w>отчуждает</w> участок земли для строительства дороги.",
                                "en": "The municipality <w>expropriates</w> the piece of land for the construction of a road."
                            }
                        },
                        {
                            "text": "<w>Naast</w> dat huis en maak het je eigen, sprak de rechter.",
                            "target_lang_translations": {
                                "ru": "<w>Присвой</w> этот дом и сделай его своим, — сказал судья.",
                                "en": "<w>Appropriate</w> that house and make it your own, spoke the judge."
                            }
                        }

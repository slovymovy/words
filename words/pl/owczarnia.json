            "pos": "noun",
            "senses": [
                {
                    "sense_id": "145eab4b-769e-3151-9285-bb32ca2446da",
                    "sense_definition": "Budynek gospodarski lub ogrodzone miejsce przeznaczone do trzymania i hodowli owiec.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "building",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tradycyjnie <w>owczarnia</w> służyła jako miejsce całodobowego pobytu owiec tylko w zimie.",
                            "target_lang_translations": {
                                "ru": "Традиционно <w>овчарня</w> служила местом круглосуточного пребывания овец только зимой."
                            }
                        },
                        {
                            "text": "Rolnik postanowił wyremontować dach, ponieważ stara <w>owczarnia</w> zaczęła przeciekać podczas deszczu.",
                            "target_lang_translations": {
                                "ru": "Фермер решил отремонтировать крышу, так как старая <w>овчарня</w> начала протекать во время дождя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "budowa owczarni",
                        "stara owczarnia",
                        "murowana owczarnia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хозяйственная постройка или огороженное место, предназначенное для содержания и разведения овец."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "овчарня",
                                "target_lang_sense_clarification": "здание"
                            },
                            {
                                "target_lang_word": "кошара",
                                "target_lang_sense_clarification": "загон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78134c71-ed95-3b72-94c1-e2952b2fbdc5",
                    "sense_definition": "Stado owiec należące do jednego właściciela lub gospodarstwa.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "flock",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>owczarnia</w> liczyła ponad trzysta sztuk i była największa w całej okolicy.",
                            "target_lang_translations": {
                                "ru": "Его <w>отара</w> насчитывала более трехсот голов и была самой большой во всей округе."
                            }
                        },
                        {
                            "text": "Pasterz wyprowadził całą swoją <w>owczarnię</w> na górskie pastwiska wczesnym rankiem.",
                            "target_lang_translations": {
                                "ru": "Пастух вывел всю свою <w>отару</w> на горные пастбища ранним утром."
                            }
                        }
                    ],
                    "synonyms": [
                        "stado"
                    ],
                    "common_phrases": [
                        "liczna owczarnia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стадо овец, принадлежащее одному владельцу или хозяйству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отара",
                                "target_lang_sense_clarification": "стадо овец"
                            },
                            {
                                "target_lang_word": "стадо",
                                "target_lang_sense_clarification": "группа животных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3cd10c3e-17d5-396a-a2a4-44b0e4bf3f6d",
                    "sense_definition": "Działalność gospodarcza lub przedsiębiorstwo zajmujące się hodowlą owiec.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "business",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prowadzenie nowoczesnej <w>owczarni</w> wymaga dużej wiedzy zootechnicznej i nakładów finansowych.",
                            "target_lang_translations": {
                                "ru": "Управление современной <w>овцефермой</w> требует больших зоотехнических знаний и финансовых вложений."
                            }
                        },
                        {
                            "text": "Ich rodzinna <w>owczarnia</w> przynosi zyski głównie ze sprzedaży wysokiej jakości wełny.",
                            "target_lang_translations": {
                                "ru": "Их семейная <w>овцеферма</w> приносит прибыль в основном за счет продажи высококачественной шерсти."
                            }
                        }
                    ],
                    "synonyms": [
                        "hodowla"
                    ],
                    "common_phrases": [
                        "prowadzenie owczarni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хозяйственная деятельность или предприятие, занимающееся разведением овец."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "овцеферма",
                                "target_lang_sense_clarification": "предприятие"
                            },
                            {
                                "target_lang_word": "овцеводческое хозяйство",
                                "target_lang_sense_clarification": "бизнес"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a927e9b0-fd36-36f8-9196-fa2b39dcdd1a",
                    "sense_definition": "Wspólnota wiernych w Kościele (znaczenie biblijne i przenośne).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "religious",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I będą słuchać głosu mego, i nastanie jedna <w>owczarnia</w>, jeden pasterz.",
                            "target_lang_translations": {
                                "ru": "И услышат голос Мой, и будет одно <w>стадо</w> и один Пастырь."
                            }
                        },
                        {
                            "text": "Kaznodzieja podkreślał w homilii, że cała parafia powinna tworzyć zgodną <w>owczarnię</w>.",
                            "target_lang_translations": {
                                "ru": "Проповедник подчеркивал в проповеди, что весь приход должен составлять единую <w>паству</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wspólnota"
                    ],
                    "common_phrases": [

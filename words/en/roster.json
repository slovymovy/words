    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "59fea9de-ecb5-378b-9ede-bed489f30aeb",
                    "sense_definition": "A list of names of people belonging to a specific group, team, or organization.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "lists",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The coach announced the starting <w>roster</w> for the championship game.",
                            "target_lang_translations": {
                                "ru": "Тренер объявил стартовый <w>состав</w> на финальную игру."
                            }
                        },
                        {
                            "text": "Please check if your name is correctly spelled on the class <w>roster</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, проверьте, правильно ли написано ваше имя в <w>списке</w> класса."
                            }
                        }
                    ],
                    "synonyms": [
                        "list",
                        "register",
                        "roll",
                        "lineup"
                    ],
                    "common_phrases": [
                        "team roster",
                        "on the roster",
                        "class roster"
                    ],
                    "target_lang_definitions": {
                        "ru": "Список имен людей, принадлежащих к определенной группе, команде или организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "список",
                                "target_lang_sense_clarification": "перечень имен"
                            },
                            {
                                "target_lang_word": "состав",
                                "target_lang_sense_clarification": "команды или группы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d31f9a27-d6df-3cf6-8205-6c94a4c4f2d9",
                    "sense_definition": "A schedule or plan showing when employees are required to work or perform duties.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "schedules",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to check the weekly <w>roster</w> to see when my next shift begins.",
                            "target_lang_translations": {
                                "ru": "Мне нужно проверить недельный <w>график</w>, чтобы узнать, когда начинается моя следующая смена."
                            }
                        },
                        {
                            "text": "The manager posted a new cleaning <w>roster</w> for all staff members.",
                            "target_lang_translations": {
                                "ru": "Менеджер вывесил новый <w>график</w> уборки для всех сотрудников."
                            }
                        }
                    ],
                    "synonyms": [
                        "schedule",
                        "rota",
                        "timetable"
                    ],
                    "common_phrases": [
                        "work roster",
                        "duty roster",
                        "shift roster"
                    ],
                    "target_lang_definitions": {
                        "ru": "График или план, показывающий, когда сотрудники должны работать или выполнять обязанности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "график",
                                "target_lang_sense_clarification": "дежурств или смен"
                            },
                            {
                                "target_lang_word": "расписание",
                                "target_lang_sense_clarification": "работы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17ce40b7-1ae4-3b2a-b0c4-773911281a39",
                    "sense_definition": "A list that displays the elements of a mathematical set within brackets.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "lists",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In this problem, you must represent the set using the <w>roster</w> method.",
                            "target_lang_translations": {
                                "ru": "В этой задаче вы должны представить множество, используя метод <w>перечисления</w>."
                            }
                        },
                        {
                            "text": "The student wrote the elements of the set in <w>roster</w> notation.",
                            "target_lang_translations": {
                                "ru": "Студент записал элементы множества в виде <w>перечисления</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "roster method",
                        "roster notation"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Список, отображающий элементы математического множества в скобках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перечисление",
                                "target_lang_sense_clarification": "элементов множества"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b658d904-3466-3797-9ff2-65031ee21e2a",
                    "sense_definition": "To place a person's name on a list or assign them to a specific duty or time slot.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "scheduling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The supervisor <w>rostered</w> me for the early morning shift next week.",
                            "target_lang_translations": {
                                "ru": "Начальник <w>поставил</w> меня в график на раннюю утреннюю смену на следующей неделе."
                            }
                        },
                        {
                            "text": "She was <w>rostered</w> to work on the holiday, so she could not attend the party.",
                            "target_lang_translations": {
                                "ru": "Её <w>поставили</w> работать в праздник, поэтому она не смогла прийти на вечеринку."
                            }
                        }
                    ],
                    "synonyms": [
                        "schedule",
                        "assign",
                        "list"
                    ],
                    "common_phrases": [
                        "rostered on",
                        "rostered for",
                        "rostered off"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вносить чье-либо имя в список или назначать на определенную обязанность или время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вносить в график",
                                "target_lang_sense_clarification": "работы"
                            },
                            {
                                "target_lang_word": "назначать",
                                "target_lang_sense_clarification": "на смену"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2bb93da-ef53-3007-98fd-d853547defab",
                    "sense_definition": "To show the elements of a set by listing them inside brackets.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "listing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can <w>roster</w> the set of vowels as {a, e, i, o, u}.",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>перечислить</w> множество гласных как {a, e, i, o, u}."
                            }
                        },
                        {
                            "text": "The textbook explains how to <w>roster</w> elements for finite sets.",
                            "target_lang_translations": {
                                "ru": "В учебнике объясняется, как <w>перечислять</w> элементы конечных множеств."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Показывать элементы множества, перечисляя их внутри скобок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перечислять",
                                "target_lang_sense_clarification": "элементы множества"
                            }
                        ]
                    }
                }
            ]
        }

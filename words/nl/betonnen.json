                        "beboeien"
                    ],
                    "common_phrases": [
                        "een geul betonnen",
                        "het vaarwater betonnen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначать фарватер или канал, размещая в нем буи (бочки)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обставлять буями",

{
    "word_family": [
        "rozlec",
        "rozległy",
        "rozległość"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b3097190-e66a-3da0-856a-bf34f97e055c",
                    "sense_definition": "O dźwięku, głosie lub hałasie: rozchodzić się głośno w jakiejś przestrzeni, być wyraźnie słyszanym.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sound_resound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Głośny dzwonek na przerwę codziennie <w>rozlega</w> się w szkolnych korytarzach.",
                            "target_lang_translations": {
                                "ru": "Громкий звонок на перемену ежедневно <w>раздается</w> в школьных коридорах."
                            }
                        },
                        {
                            "text": "W oddali <w>rozlegało</w> się głuche bicie kościelnych dzwonów.",
                            "target_lang_translations": {
                                "ru": "Вдали <w>раздавался</w> глухой звон церковных колоколов."
                            }
                        },
                        {
                            "text": "Kiedy wchodzisz do jaskini, każde twoje słowo <w>rozlega</w> się wielokrotnym echem.",
                            "target_lang_translations": {
                                "ru": "Когда входишь в пещеру, каждое твое слово <w>раздается</w> многократным эхом."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozbrzmiewać"
                    ],
                    "common_phrases": [
                        "rozlegać się głośno",
                        "rozlegać się echem",
                        "nagle się rozlegać",
                        "rozlegać się w ciszy"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "zwykle z zaimkiem „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О звуке, голосе или шуме: громко распространяться в пространстве, быть отчетливо слышным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раздаваться",
                                "target_lang_sense_clarification": "внезапно или громко звучать"
                            },
                            {
                                "target_lang_word": "звучать",
                                "target_lang_sense_clarification": "быть слышным"
                            },
                            {
                                "target_lang_word": "разноситься",
                                "target_lang_sense_clarification": "распространяться на большое расстояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d5e6c71-4da2-3932-8b73-aa35dc09b61c",
                    "sense_definition": "O krajobrazie, widoku lub terenie: zajmować duży obszar, roztaczać się szeroko przed patrzącym.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "space_sprawl",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze szczytu góry <w>rozlega</w> się przepiękna panorama na całą okolicę.",
                            "target_lang_translations": {
                                "ru": "С вершины горы <w>открывается</w> прекрасная панорама на всю округу."
                            }
                        },
                        {
                            "text": "Przed podróżnikami <w>rozlegała</w> się nieskończona, piaszczysta pustynia.",
                            "target_lang_translations": {
                                "ru": "Перед путешественниками <w>простиралась</w> бесконечная песчаная пустыня."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozbrzmiewać"
                    ],
                    "common_phrases": [
                        "rozlegać się szeroko",
                        "rozlegać się po horyzont",
                        "rozlegać się wokół"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "zwykle z zaimkiem „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О пейзаже, виде или местности: занимать большую площадь, широко открываться взору."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "простираться",
                                "target_lang_sense_clarification": "занимать пространство вдаль"
                            },
                            {
                                "target_lang_word": "раскидываться",
                                "target_lang_sense_clarification": "широко располагаться"
                            },
                            {
                                "target_lang_word": "открываться",
                                "target_lang_sense_clarification": "становиться видимым (о панораме)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67eefbf6-d7f2-3ca2-bb6b-2ae664d79ca7",
                    "sense_definition": "Napełniać jakieś miejsce dźwiękiem, gwarem lub hałasem (użycie bez zaimka zwrotnego).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sound_fill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego potężny i donośny głos <w>rozlegał</w> całą salę koncertową.",
                            "target_lang_translations": {
                                "ru": "Его мощный и зычный голос <w>оглашал</w> весь концертный зал."
                            }
                        },
                        {
                            "text": "Radosny śpiew ptaków <w>rozlegał</w> leśną polanę o poranku.",
                            "target_lang_translations": {
                                "ru": "Радостное пение птиц <w>оглашало</w> лесную поляну по утрам."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наполнять какое-либо место звуком, гулом или шумом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оглашать",
                                "target_lang_sense_clarification": "наполнять громким звуком"
                            },
                            {
                                "target_lang_word": "наполнять",
                                "target_lang_sense_clarification": "заполнять пространство (звуком)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
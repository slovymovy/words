    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "68a948d7-cb8a-3e8b-86fa-b5e10d1baf5c",
                    "sense_definition": "A product or substance that serves as a substitute for something else.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "substitute_product",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many bakers use a powdered egg <w>replacer</w> to make their cakes vegan.",
                            "target_lang_translations": {
                                "ru": "Многие пекари используют порошковый <w>заменитель</w> яиц, чтобы сделать свои торты веганскими."
                            }
                        },
                        {
                            "text": "This shake is designed as a meal <w>replacer</w> for busy people.",
                            "target_lang_translations": {
                                "ru": "Этот коктейль разработан как <w>заменитель</w> пищи для занятых людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "substitute",
                        "alternative",
                        "replacement"
                    ],
                    "common_phrases": [
                        "egg replacer",
                        "meal replacer",
                        "sugar replacer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продукт или вещество, используемое в качестве замены чего-либо другого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заменитель",
                                "target_lang_sense_clarification": "продукт-аналог"
                            },
                            {
                                "target_lang_word": "суррогат",
                                "target_lang_sense_clarification": "неполноценная замена"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6b45a33-7f07-3603-a409-e0b745d35d21",
                    "sense_definition": "A person, tool, or software that performs the action of replacing something.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "agent_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The software includes a bulk text <w>replacer</w> to speed up editing.",
                            "target_lang_translations": {
                                "ru": "Программа включает в себя <w>инструмент</w> массовой <w>замены</w> текста для ускорения редактирования."
                            }
                        },
                        {
                            "text": "He acted as a temporary <w>replacer</w> while the main technician was away.",
                            "target_lang_translations": {
                                "ru": "Он действовал как временный <w>заместитель</w>, пока главный техник отсутствовал."
                            }
                        }
                    ],
                    "synonyms": [
                        "replacement"
                    ],
                    "common_phrases": [
                        "text replacer",
                        "background replacer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, инструмент или программа, выполняющие функцию замены."
                    },
                    "translations": {
                        "ru": [
                            {

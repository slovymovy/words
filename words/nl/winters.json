                    ],
                    "antonyms": [
                        "zomers"
                    ],
                    "common_phrases": [
                        "winters weer",
                        "een winters landschap",
                        "winterse neerslag",
                        "winterse omstandigheden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характерный для зимы или относящийся к ней; холодный и суровый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зимний",
                                "target_lang_sense_clarification": "прилагательное"

            "senses": [
                {
                    "sense_id": "8e2f89a0-a549-3364-984f-e54ea4e13725",
                    "sense_definition": "Несходство, наличие отличительных черт между кем-либо или чем-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "distinctness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я не вижу никакой <w>разницы</w> между этими двумя телефонами.",
                            "target_lang_translations": {
                                "en": "I don't see any <w>difference</w> between these two phones."
                            }
                        },
                        {
                            "text": "У них слишком большая <w>разница</w> в возрасте для брака.",
                            "target_lang_translations": {
                                "en": "They have too big an age <w>difference</w> for marriage."
                            }
                        }
                    ],
                    "synonyms": [
                        "различие",
                        "отличие",
                        "несходство",
                        "непохожесть"
                    ],
                    "antonyms": [
                        "сходство",
                        "схожесть",
                        "общность"
                    ],
                    "common_phrases": [
                        "какая разница",
                        "большая разница",
                        "разница в возрасте",
                        "разница во времени",
                        "в чём разница"
                    ],
                    "target_lang_definitions": {
                        "en": "Dissimilarity, the presence of distinctive features between someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "difference",
                                "target_lang_sense_clarification": "the state of being unlike"
                            },
                            {
                                "target_lang_word": "distinction",
                                "target_lang_sense_clarification": "a feature that differentiates"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cdebdeb3-14e1-35d8-9c0b-5babf1eac7e0",
                    "sense_definition": "Числовая величина, на которую одно число или количество отличается от другого.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Покупатель должен доплатить <w>разницу</w> в цене товара.",
                            "target_lang_translations": {
                                "en": "The buyer must pay the <w>difference</w> in the price of the item."
                            }
                        },
                        {
                            "text": "<w>Разница</w> забитых и пропущенных мячей определила победителя.",
                            "target_lang_translations": {
                                "en": "The goal <w>difference</w> determined the winner."
                            }
                        }
                    ],
                    "synonyms": [
                        "расхождение"
                    ],
                    "antonyms": [
                        "сумма"
                    ],
                    "common_phrases": [
                        "разница в цене",
                        "разница в счёте",
                        "разница температур"
                    ],
                    "target_lang_definitions": {
                        "en": "A numerical value by which one number or quantity differs from another."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "difference",

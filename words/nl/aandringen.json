    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "acba74fc-ded3-365e-b128-f2a8f0d45ac8",
                    "sense_definition": "Met klem vragen of eisen dat iets gebeurt; niet accepteren dat een verzoek wordt geweigerd.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vakbond <w>dringt aan</w> op hogere lonen voor alle medewerkers.",
                            "target_lang_translations": {
                                "ru": "Профсоюз <w>настаивает</w> на повышении зарплат для всех сотрудников."
                            }
                        },
                        {
                            "text": "Omdat hij bleef <w>aandringen</w>, besloot ik uiteindelijk toch mee te gaan.",
                            "target_lang_translations": {
                                "ru": "Так как он продолжал <w>настаивать</w>, я в конце концов решил пойти с ним."
                            }
                        },
                        {
                            "text": "Op <w>aandringen</w> van de dokter is hij gestopt met roken.",
                            "target_lang_translations": {
                                "ru": "По <w>настоянию</w> врача он бросил курить."
                            }
                        }
                    ],
                    "synonyms": [
                        "volharden",
                        "eisen"
                    ],
                    "antonyms": [
                        "opgeven",
                        "toegeven"
                    ],
                    "common_phrases": [
                        "aandringen op",
                        "op aandringen van",
                        "blijven aandringen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Настойчиво просить или требовать выполнения чего-либо; не принимать отказа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "настаивать",
                                "target_lang_sense_clarification": "упорно требовать или утверждать что-либо"
                            },
                            {
                                "target_lang_word": "требовать",
                                "target_lang_sense_clarification": "решительно добиваться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56028e97-0b8b-3fa0-bb28-4d6e0cb873c5",
                    "sense_definition": "Fysiek naar voren duwen of dichterbij komen, vaak op een krachtige of dreigende manier.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De menigte <w>drong aan</w> tegen de hekken van het stadion.",
                            "target_lang_translations": {
                                "ru": "Толпа <w>напирала</w> на ограждения стадиона."
                            }
                        },
                        {
                            "text": "Het leger bleef <w>aandringen</w> om de vijandelijke linies te doorbreken.",
                            "target_lang_translations": {
                                "ru": "Армия продолжала <w>напирать</w>, чтобы прорвать вражеские линии."
                            }
                        }
                    ],
                    "synonyms": [
                        "oprukken",
                        "opdringen"
                    ],
                    "antonyms": [
                        "terugtrekken",
                        "wijken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физически продвигаться вперед или приближаться, часто с силой или угрожающе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "напирать",
                                "target_lang_sense_clarification": "давить, наваливаться массой"
                            },
                            {
                                "target_lang_word": "теснить",
                                "target_lang_sense_clarification": "оттеснять, заставлять отступать давлением"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "b9f53d88-3123-36c7-bd62-cdc5f8d56a68",
                    "sense_definition": "Wynik rozumowania, logiczne następstwo przesłanek lub rezultat przemyśleń.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "logic_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po długiej analizie danych badacz wyciągnął ostateczny <w>wniosek</w>.",
                            "target_lang_translations": {
                                "ru": "После долгого анализа данных исследователь сделал окончательный <w>вывод</w>."
                            }
                        },
                        {
                            "text": "Doszedłem do <w>wniosku</w>, że musimy zmienić nasze plany wakacyjne.",
                            "target_lang_translations": {
                                "ru": "Я пришёл к <w>выводу</w>, что мы должны изменить наши планы на отпуск."
                            }
                        }
                    ],
                    "synonyms": [
                        "konkluzja"
                    ],
                    "common_phrases": [
                        "dojść do wniosku",
                        "wyciągnąć wniosek",
                        "logiczny wniosek",
                        "wniosek z obserwacji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Логический итог рассуждения, результат размышлений или анализа фактов."

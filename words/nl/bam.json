            "senses": [
                {
                    "sense_id": "cbd9a43e-26e6-3fa8-b683-0e78e752240c",
                    "sense_definition": "Een vrouw die er bewust voor kiest om zonder partner een of meer kinderen groot te brengen; afkorting van bewust alleenstaande moeder",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als <w>bam</w> moet je de opvoeding en het inkomen in je eentje regelen.",

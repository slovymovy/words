    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "05f789f6-3e94-3666-bafa-54681f6fab46",
                    "sense_definition": "A device for transmitting conversations and other sounds in real time across distances, now often a small portable unit also capable of running software.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is talking on the <w>phone</w> with her mother right now.",
                            "target_lang_translations": {
                                "ru": "Она сейчас говорит по <w>телефону</w> с мамой."
                            }
                        },
                        {
                            "text": "I need to charge my <w>phone</w> because the battery is dead.",
                            "target_lang_translations": {
                                "ru": "Мне нужно зарядить <w>телефон</w>, потому что села батарейка."
                            }
                        }
                    ],
                    "synonyms": [
                        "telephone",
                        "blower",
                        "dog and bone",
                        "horn"
                    ],
                    "common_phrases": [
                        "on the phone",
                        "mobile phone",
                        "cell phone",
                        "answer the phone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство для передачи разговоров и других звуков в реальном времени на расстоянии; в настоящее время часто представляет собой небольшое портативное устройство, способное также запускать программное обеспечение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "телефон",
                                "target_lang_sense_clarification": "устройство"
                            },
                            {
                                "target_lang_word": "мобильный",
                                "target_lang_sense_clarification": "разг. мобильный телефон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e39cdaa-5446-3bbf-9663-a39665d53a7d",
                    "sense_definition": "A person's telephone number.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "number",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He asked for her <w>phone</w> so he could text her later.",
                            "target_lang_translations": {
                                "ru": "Он попросил её <w>телефон</w>, чтобы написать ей позже."
                            }
                        },
                        {
                            "text": "I tried to get his <w>phone</w>, but he refused to give it to me.",
                            "target_lang_translations": {
                                "ru": "Я пытался узнать его <w>телефон</w>, но он отказался мне его дать."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "Refers to the digits rather than the device."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Номер телефона человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "номер телефона",
                                "target_lang_sense_clarification": "цифровая комбинация"
                            },
                            {
                                "target_lang_word": "телефон",
                                "target_lang_sense_clarification": "разг. номер"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d02e5199-4002-30b1-a74a-58cc4518d58f",
                    "sense_definition": "A speech segment that possesses distinct physical or perceptual properties, considered as a physical event.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "linguistics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The linguist analyzed every <w>phone</w> in the recording to understand the dialect.",
                            "target_lang_translations": {
                                "ru": "Лингвист проанализировал каждый <w>фон</w> в записи, чтобы понять диалект."
                            }
                        },
                        {
                            "text": "A <w>phone</w> is the smallest discrete segment of sound in a stream of speech.",
                            "target_lang_translations": {
                                "ru": "<w>Фон</w> — это наименьший дискретный отрезок звука в потоке речи."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Звуковой сегмент, обладающий отчетливыми физическими или перцептивными свойствами; звук речи как физическое явление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фон",
                                "target_lang_sense_clarification": "лингв. звук речи"
                            },
                            {
                                "target_lang_word": "звук",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9d811c22-83a8-3553-9209-d0e4efc1fb85",
                    "sense_definition": "To call someone using a telephone.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>phone</w> me as soon as you arrive at the station.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>позвони</w> мне, как только приедешь на вокзал."
                            }
                        },
                        {
                            "text": "He <w>phoned</w> his parents to tell them the good news.",
                            "target_lang_translations": {
                                "ru": "Он <w>позвонил</w> родителям, чтобы сообщить им хорошую новость."
                            }
                        }
                    ],
                    "synonyms": [
                        "call",
                        "ring",
                        "telephone",
                        "phone up"
                    ],
                    "common_phrases": [
                        "phone home",
                        "phone a friend"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звонить кому-либо по телефону."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звонить",
                                "target_lang_sense_clarification": "несов. вид"
                            },
                            {
                                "target_lang_word": "позвонить",
                                "target_lang_sense_clarification": "сов. вид"
                            },
                            {
                                "target_lang_word": "телефонировать",
                                "target_lang_sense_clarification": "офиц."
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
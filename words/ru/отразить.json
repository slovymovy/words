                        "возразить"
                    ],
                    "common_phrases": [
                        "отразить доводы",
                        "отразить критику",
                        "отразить обвинения"
                    ],
                    "target_lang_definitions": {
                        "en": "To provide arguments against something, to successfully object to an opponent."
                    },
                    "translations": {

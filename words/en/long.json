                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "finance"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Инвестор или трейдер, который держит позицию в ожидании роста стоимости актива."
                    },

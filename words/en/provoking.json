            "senses": [
                {
                    "sense_id": "846100a1-f969-351a-9b5c-99fee90c0392",
                    "sense_definition": "Causing annoyance or irritation.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Would you stop interrupting me? It is a most <w>provoking</w> habit.",
                            "target_lang_translations": {
                                "ru": "Перестань меня перебивать. Это крайне <w>раздражающая</w> привычка."
                            }
                        },
                        {
                            "text": "His <w>provoking</w> smile made me want to argue with him even more.",
                            "target_lang_translations": {
                                "ru": "Его <w>вызывающая</w> улыбка заставила меня хотеть спорить с ним еще больше."
                            }
                        }
                    ],
                    "synonyms": [
                        "annoying",
                        "irritating",
                        "exasperating",
                        "galling"
                    ],
                    "antonyms": [
                        "soothing",
                        "calming",
                        "pleasing"
                    ],
                    "common_phrases": [
                        "most provoking",
                        "provoking habit",
                        "provoking behavior"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий раздражение, злость или досаду."
                    },
                    "translations": {
                        "ru": [
                            {

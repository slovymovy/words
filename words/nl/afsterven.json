{
    "word_family": [
        "afsterving",
        "sterven",
        "sterfte"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e58b6a76-7c66-32e3-a389-8e9c7cd1ddcf",
                    "sense_definition": "Het proces waarbij levend weefsel, organen of delen van planten hun vitaliteit verliezen en doodgaan.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "biological_death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de strenge vorst zullen de bladeren van de plant snel <w>afsterven</w>.",
                            "target_lang_translations": {
                                "en": "Due to the severe frost, the leaves of the plant will quickly <w>die off</w>.",
                                "ru": "Из-за сильного мороза листья растения быстро <w>отомрут</w>."
                            }
                        },
                        {
                            "text": "Als de bloedtoevoer wordt afgesneden, kan het weefsel in de vinger <w>afsterven</w>.",
                            "target_lang_translations": {
                                "en": "If the blood supply is cut off, the tissue in the finger can <w>die</w>.",
                                "ru": "Если кровоснабжение будет перекрыто, ткани пальца могут <w>омертветь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "necrotiseren",
                        "verwelken"
                    ],
                    "antonyms": [
                        "groeien",
                        "herstellen",
                        "genezen"
                    ],
                    "common_phrases": [
                        "weefsel sterft af",
                        "zenuwcellen sterven af",
                        "langzaam afsterven"
                    ],
                    "target_lang_definitions": {
                        "en": "To lose vitality and die, specifically regarding living tissue, organs, or parts of plants.",
                        "ru": "Процесс, при котором живые ткани, органы или части растений теряют жизнеспособность и отмирают."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "die off",
                                "target_lang_sense_clarification": "common for plants or parts of a whole"
                            },
                            {
                                "target_lang_word": "necrotize",
                                "target_lang_sense_clarification": "medical context regarding tissue"
                            },
                            {
                                "target_lang_word": "wither",
                                "target_lang_sense_clarification": "specifically for plants drying out"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отмирать",
                                "target_lang_sense_clarification": "о частях растений или организма"
                            },
                            {
                                "target_lang_word": "омертвевать",
                                "target_lang_sense_clarification": "медицинский термин, некроз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5be3d577-37c1-3d76-b64c-2397dc4bc208",
                    "sense_definition": "Geleidelijk zwakker worden en uiteindelijk verdwijnen, vaak gebruikt voor geluid, licht of abstracte zaken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_fading",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het geluid van de motor zal in de verte langzaam <w>afsterven</w>.",
                            "target_lang_translations": {
                                "en": "The sound of the engine will slowly <w>die away</w> in the distance.",
                                "ru": "Звук двигателя вдалеке медленно <w>затихнет</w>."
                            }
                        },
                        {
                            "text": "Zonder nieuwe leden zal de vereniging op den duur <w>afsterven</w>.",
                            "target_lang_translations": {
                                "en": "Without new members, the association will eventually <w>die out</w>.",
                                "ru": "Без новых членов ассоциация со временем <w>угаснет</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wegsterven",
                        "uitdoven",
                        "verdwijnen"
                    ],
                    "antonyms": [
                        "aanzwellen",
                        "opkomen"
                    ],
                    "common_phrases": [
                        "geluid sterft af",
                        "interesse sterft af"
                    ],
                    "target_lang_definitions": {
                        "en": "To gradually diminish in strength and disappear, often used for sounds, lights, or abstract entities.",
                        "ru": "Постепенное ослабление и исчезновение (звука, света или абстрактных явлений)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "die away",
                                "target_lang_sense_clarification": "fading of sound or light"
                            },
                            {
                                "target_lang_word": "die out",
                                "target_lang_sense_clarification": "disappearance of groups, customs, or organizations"
                            },
                            {
                                "target_lang_word": "fade away",
                                "target_lang_sense_clarification": "general gradual disappearance"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "замирать",
                                "target_lang_sense_clarification": "о звуке или движении"
                            },
                            {
                                "target_lang_word": "угасать",
                                "target_lang_sense_clarification": "переносное значение (об организации, чувствах)"
                            },
                            {
                                "target_lang_word": "затихать",
                                "target_lang_sense_clarification": "о звуке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f39d61db-5a44-3eb7-ad82-0ee0ef982e1f",
                    "sense_definition": "Het overlijden van een persoon.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "human_death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn geliefde vrouw is vorig jaar na een lang ziekbed <w>afgestorven</w>.",
                            "target_lang_translations": {
                                "en": "His beloved wife <w>passed away</w> last year after a long illness.",
                                "ru": "Его любимая жена <w>скончалась</w> в прошлом году после продолжительной болезни."
                            }
                        },
                        {
                            "text": "Wij herdenken degenen die ons zijn <w>afgestorven</w>.",
                            "target_lang_translations": {
                                "en": "We commemorate those who have <w>departed</w> from us.",
                                "ru": "Мы поминаем тех, кто <w>умер</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "overlijden",
                        "heengaan",
                        "ontslapen"
                    ],
                    "antonyms": [
                        "geboren worden"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly found in older texts or very formal religious contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To die or pass away (formal, archaic, or religious context).",
                        "ru": "Смерть человека (часто в устаревшем или формальном контексте)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pass away",
                                "target_lang_sense_clarification": "polite or formal euphemism for death"
                            },
                            {
                                "target_lang_word": "depart",
                                "target_lang_sense_clarification": "formal/archaic, often 'depart this life'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "скончаться",
                                "target_lang_sense_clarification": "официально"
                            },
                            {
                                "target_lang_word": "умереть",
                                "target_lang_sense_clarification": "нейтрально"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
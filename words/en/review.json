{
    "word_family": [
        "reviewer"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "801032e7-47ff-3adf-8356-46a32e22520f",
                    "sense_definition": "An account intended as a critical evaluation of a text or a piece of work.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "critique_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The newspaper <w>review</w> was full of praise for the play.",
                            "target_lang_translations": {
                                "ru": "Газетная <w>рецензия</w> была полна похвал в адрес пьесы."
                            }
                        },
                        {
                            "text": "She decided to write a negative <w>review</w> of the restaurant after the poor service.",
                            "target_lang_translations": {
                                "ru": "Она решила написать негативный <w>отзыв</w> о ресторане после плохого обслуживания."
                            }
                        }
                    ],
                    "common_phrases": [
                        "book review",
                        "film review",
                        "rave review",
                        "mixed reviews",
                        "write a review"
                    ],
                    "target_lang_definitions": {
                        "ru": "Статья или мнение, содержащие критическую оценку произведения, продукта или услуги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рецензия",
                                "target_lang_sense_clarification": "профессиональный разбор произведения (книги, фильма)"
                            },
                            {
                                "target_lang_word": "отзыв",
                                "target_lang_sense_clarification": "мнение потребителя или зрителя"
                            },
                            {
                                "target_lang_word": "критика",
                                "target_lang_sense_clarification": "критический разбор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37428c33-3683-37e4-b35d-f35f37738038",
                    "sense_definition": "A formal assessment or re-examination of a case, event, or situation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "assessment_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The victims demanded a full judicial <w>review</w> of the case.",
                            "target_lang_translations": {
                                "ru": "Потерпевшие потребовали полного судебного <w>пересмотра</w> дела."
                            }
                        },
                        {
                            "text": "The company's spending policy is currently under <w>review</w>.",
                            "target_lang_translations": {
                                "ru": "Политика расходов компании в данный момент находится на <w>рассмотрении</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "under review",
                        "judicial review",
                        "performance review",
                        "salary review"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальная оценка или повторное рассмотрение дела, события или ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пересмотр",
                                "target_lang_sense_clarification": "повторное рассмотрение (судебное, официальное)"
                            },
                            {
                                "target_lang_word": "рассмотрение",
                                "target_lang_sense_clarification": "процесс оценки или анализа"
                            },
                            {
                                "target_lang_word": "ревизия",
                                "target_lang_sense_clarification": "проверка деятельности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77bec1bb-8520-371a-9017-78c8114feca8",
                    "sense_definition": "A periodical which makes a survey of the arts or some other field.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The Times Literary <w>Review</w> is published in London.",
                            "target_lang_translations": {
                                "ru": "«Литературное <w>обозрение</w> Таймс» издается в Лондоне."
                            }
                        },
                        {
                            "text": "He subscribed to a monthly <w>review</w> covering scientific advancements.",
                            "target_lang_translations": {
                                "ru": "Он подписался на ежемесячное <w>обозрение</w>, освещающее научные достижения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "literary review",
                        "quarterly review"
                    ],
                    "target_lang_definitions": {
                        "ru": "Периодическое издание, содержащее обзор событий в области искусства или науки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обозрение",
                                "target_lang_sense_clarification": "тип периодического издания"
                            },
                            {
                                "target_lang_word": "обзор",
                                "target_lang_sense_clarification": "сводка новостей или материалов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6aaaae73-4662-3dd1-a741-af629a2fa21a",
                    "sense_definition": "A military inspection or display for the benefit of superiors or VIPs.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The troops assembled for a <w>review</w> by the Queen.",
                            "target_lang_translations": {
                                "ru": "Войска собрались на <w>смотр</w> королевой."
                            }
                        },
                        {
                            "text": "The general conducted a formal <w>review</w> of the soldiers at the base.",
                            "target_lang_translations": {
                                "ru": "Генерал провел официальный <w>смотр</w> солдат на базе."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Торжественный осмотр войск командованием или высокопоставленными лицами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смотр",
                                "target_lang_sense_clarification": "военный парад или инспекция"
                            },
                            {
                                "target_lang_word": "проверка",
                                "target_lang_sense_clarification": "инспекция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e12b2f5d-34c4-3bd4-ac81-bbfe92803280",
                    "sense_definition": "A stage show made up of topical sketches etc.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The Cambridge Footlights <w>Review</w> launched many Monty Python faces.",
                            "target_lang_translations": {
                                "ru": "«Кембридж Футлайтс <w>Ревю</w>» дало старт многим участникам Монти Пайтон."
                            }
                        },
                        {
                            "text": "They attended a musical <w>review</w> featuring songs from the 1920s.",
                            "target_lang_translations": {
                                "ru": "Они посетили музыкальное <w>ревю</w> с песнями 1920-х годов."
                            }
                        }
                    ],
                    "synonyms": [
                        "revue"
                    ],
                    "target_lang_definitions": {
                        "ru": "Эстрадное или театральное представление, состоящее из отдельных сцен и номеров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ревю",
                                "target_lang_sense_clarification": "тип эстрадного представления"
                            },
                            {
                                "target_lang_word": "обозрение",
                                "target_lang_sense_clarification": "театральный жанр"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "48ca95d0-fe44-3ec9-a989-d47e4dd29c04",
                    "sense_definition": "To look over again (something previously written or learned), especially in preparation for an examination.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "study_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>review</w> my notes before the final exam.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>повторить</w> свои записи перед финальным экзаменом."
                            }
                        },
                        {
                            "text": "The teacher asked the students to <w>review</w> the chapter for homework.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил учеников <w>повторить</w> главу в качестве домашнего задания."
                            }
                        }
                    ],
                    "common_phrases": [
                        "review for a test",
                        "review notes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Повторять или просматривать изученный материал, особенно перед экзаменом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повторять",
                                "target_lang_sense_clarification": "освежать знания в памяти"
                            },
                            {
                                "target_lang_word": "просматривать",
                                "target_lang_sense_clarification": "бегло читать заново"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b44c8032-1f7a-3e1d-8d40-848276893a8b",
                    "sense_definition": "To write a critical evaluation of a new art work etc.; to write a review.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "critique_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The critic <w>reviews</w> every new play in London.",
                            "target_lang_translations": {
                                "ru": "Критик <w>рецензирует</w> каждую новую пьесу в Лондоне."
                            }
                        },
                        {
                            "text": "He was asked to <w>review</w> the latest novel for the magazine.",
                            "target_lang_translations": {
                                "ru": "Его попросили <w>написать рецензию</w> на последний роман для журнала."
                            }
                        }
                    ],
                    "common_phrases": [
                        "review a book",
                        "review a film",
                        "review a product"
                    ],
                    "target_lang_definitions": {
                        "ru": "Писать критическую статью или отзыв о новом произведении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рецензировать",
                                "target_lang_sense_clarification": "писать профессиональный разбор"
                            },
                            {
                                "target_lang_word": "писать отзыв",
                                "target_lang_sense_clarification": "выражать мнение о произведении"
                            },
                            {
                                "target_lang_word": "делать обзор",
                                "target_lang_sense_clarification": "составлять описание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6fc77096-5e0d-3b61-af2b-d4995bdb7021",
                    "sense_definition": "To survey; to look broadly over or examine a situation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "assessment_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Before I tackle the question directly, I must briefly <w>review</w> historical approaches to the problem.",
                            "target_lang_translations": {
                                "ru": "Прежде чем перейти непосредственно к вопросу, я должен кратко <w>рассмотреть</w> исторические подходы к проблеме."
                            }
                        },
                        {
                            "text": "The manager <w>reviewed</w> the team's progress at the end of the month.",
                            "target_lang_translations": {
                                "ru": "Менеджер <w>проанализировал</w> прогресс команды в конце месяца."
                            }
                        }
                    ],
                    "common_phrases": [
                        "review the situation",
                        "review the evidence",
                        "review progress"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оценивать или анализировать ситуацию в целом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассматривать",
                                "target_lang_sense_clarification": "изучать ситуацию"
                            },
                            {
                                "target_lang_word": "анализировать",
                                "target_lang_sense_clarification": "проводить разбор"
                            },
                            {
                                "target_lang_word": "обозревать",
                                "target_lang_sense_clarification": "окидывать взглядом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8022bf55-cc21-3944-b6fb-c20c3412d2d5",
                    "sense_definition": "To look back over in order to correct or edit.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "assessment_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The author is <w>reviewing</w> the manuscript to fix any errors.",
                            "target_lang_translations": {
                                "ru": "Автор <w>просматривает</w> рукопись, чтобы исправить ошибки."
                            }
                        },
                        {
                            "text": "Please <w>review</w> the document and make any necessary changes.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>проверьте</w> документ и внесите необходимые изменения."
                            }
                        }
                    ],
                    "synonyms": [
                        "revise"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проверять документ или текст с целью исправления ошибок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проверять",
                                "target_lang_sense_clarification": "искать ошибки"
                            },
                            {
                                "target_lang_word": "просматривать",
                                "target_lang_sense_clarification": "читать с целью проверки"
                            },
                            {
                                "target_lang_word": "редактировать",
                                "target_lang_sense_clarification": "вносить правки"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
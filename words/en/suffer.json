{
    "word_family": [
        "sufferer",
        "suffering",
        "sufferance",
        "insufferable",
        "sufferable"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "66c9a474-b38b-3c39-8282-9769a518d487",
                    "sense_definition": "To feel physical or mental pain or distress.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "experience_pain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The injured animal did not <w>suffer</w> for long thanks to the vet.",
                            "target_lang_translations": {
                                "ru": "Раненное животное недолго <w>мучилось</w> благодаря ветеринару."
                            }
                        },
                        {
                            "text": "Many people <w>suffer</w> from allergies during the spring season.",
                            "target_lang_translations": {
                                "ru": "Многие люди <w>страдают</w> от аллергии в весенний сезон."
                            }
                        }
                    ],
                    "synonyms": [
                        "hurt",
                        "ache",
                        "agonize",
                        "anguish",
                        "pain"
                    ],
                    "antonyms": [
                        "be pleased",
                        "relax"
                    ],
                    "common_phrases": [
                        "suffer from",
                        "suffer in silence",
                        "suffer greatly",
                        "suffer pain"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать физическую или душевную боль, страдание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страдать",
                                "target_lang_sense_clarification": "испытывать боль или болезнь"
                            },
                            {
                                "target_lang_word": "мучиться",
                                "target_lang_sense_clarification": "испытывать сильные мучения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4fc047b-6960-3ffa-b473-08ea1b516599",
                    "sense_definition": "To experience or undergo something unpleasant, such as an injury, defeat, or loss.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "experience_pain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company will <w>suffer</w> a significant loss if sales do not improve.",
                            "target_lang_translations": {
                                "ru": "Компания <w>понесёт</w> значительные убытки, если продажи не улучшатся."
                            }
                        },
                        {
                            "text": "He had to <w>suffer</w> the consequences of his poor decisions.",
                            "target_lang_translations": {
                                "ru": "Ему пришлось <w>вынести</w> последствия своих плохих решений."
                            }
                        }
                    ],
                    "synonyms": [
                        "undergo",
                        "endure",
                        "bear",
                        "tolerate",
                        "stand",
                        "stomach",
                        "put up with"
                    ],
                    "antonyms": [
                        "luxuriate"
                    ],
                    "common_phrases": [
                        "suffer a loss",
                        "suffer damage",
                        "suffer the consequences",
                        "suffer a heart attack"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переживать или испытывать что-либо неприятное (убытки, поражение, последствия)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потерпеть",
                                "target_lang_sense_clarification": "пережить (поражение, крах, аварию)"
                            },
                            {
                                "target_lang_word": "выносить",
                                "target_lang_sense_clarification": "стойко переносить испытания"
                            },
                            {
                                "target_lang_word": "нести",
                                "target_lang_sense_clarification": "убытки или наказание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34f9b451-4841-3a76-b27e-3c2b249d2ef3",
                    "sense_definition": "To become worse in quality or condition.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Your grades will <w>suffer</w> if you do not study enough for the exams.",
                            "target_lang_translations": {
                                "ru": "Твои оценки <w>пострадают</w>, если ты не будешь достаточно учиться перед экзаменами."
                            }
                        },
                        {
                            "text": "The quality of the project began to <w>suffer</w> due to a lack of funding.",
                            "target_lang_translations": {
                                "ru": "Качество проекта начало <w>страдать</w> из-за недостатка финансирования."
                            }
                        }
                    ],
                    "synonyms": [
                        "deteriorate",
                        "worsen",
                        "decline",
                        "degenerate",
                        "go downhill"
                    ],
                    "common_phrases": [
                        "work will suffer",
                        "quality suffers",
                        "reputation suffers"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ухудшаться по качеству или состоянию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страдать",
                                "target_lang_sense_clarification": "быть недостаточно хорошим (о качестве)"
                            },
                            {
                                "target_lang_word": "пострадать",
                                "target_lang_sense_clarification": "испортиться, понести ущерб"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90cb37c3-4c78-353f-a18c-9f2cdbbd216b",
                    "sense_definition": "To allow or permit something to happen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is a strict manager who does not <w>suffer</w> fools gladly.",
                            "target_lang_translations": {
                                "ru": "Она строгий менеджер, который не <w>терпит</w> дураков."
                            }
                        },
                        {
                            "text": "The king would not <w>suffer</w> anyone to question his absolute authority.",
                            "target_lang_translations": {
                                "ru": "Король не <w>потерпел</w> бы, чтобы кто-то ставил под сомнение его абсолютную власть."
                            }
                        }
                    ],
                    "synonyms": [
                        "permit",
                        "allow",
                        "brook",
                        "abide",
                        "tolerate"
                    ],
                    "common_phrases": [
                        "suffer fools gladly"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "This sense is largely archaic but survives in specific idioms like 'suffer fools gladly'."
                        },
                        {
                            "trait_type": "dated",
                            "comment": "Used in literary or biblical contexts."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Допускать или позволять чему-либо происходить (устар. или книжн.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "терпеть",
                                "target_lang_sense_clarification": "мириться с наличием кого-либо/чего-либо"
                            },
                            {
                                "target_lang_word": "выносить",
                                "target_lang_sense_clarification": "переносить (обычно с отрицанием)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
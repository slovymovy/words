                {
                    "sense_id": "3f3cbd23-8270-39ef-bb40-bc97572b82fc",
                    "sense_definition": "Een lijst met beschikbare gerechten en dranken waaruit men in een restaurant kan kiezen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "restaurant_list",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mag ik het <w>menu</w> even zien om iets te bestellen?",
                            "target_lang_translations": {
                                "en": "May I see the <w>menu</w> for a moment to order something?",
                                "ru": "Можно мне посмотреть <w>меню</w>, чтобы что-нибудь заказать?"
                            }
                        },
                        {
                            "text": "Er staan vandaag heerlijke visgerechten op het <w>menu</w>.",
                            "target_lang_translations": {
                                "en": "There are delicious fish dishes on the <w>menu</w> today.",
                                "ru": "Сегодня в <w>меню</w> есть вкусные рыбные блюда."
                            }
                        }
                    ],
                    "synonyms": [
                        "menukaart",
                        "kaart",
                        "spijskaart"
                    ],
                    "common_phrases": [
                        "op het menu",
                        "het menu bekijken",
                        "naar het menu vragen"
                    ],
                    "target_lang_definitions": {
                        "en": "A list of available dishes and drinks to choose from in a restaurant.",
                        "ru": "Список доступных блюд и напитков, из которых можно выбирать в ресторане."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "menu",
                                "target_lang_sense_clarification": "list of food options"
                            }
                        ],
                        "ru": [

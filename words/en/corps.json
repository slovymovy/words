            "pos": "noun",
            "senses": [
                {
                    "sense_id": "607d1c07-9f05-39da-b355-5d37c5cda660",
                    "sense_definition": "An organized group of people united by a common purpose or profession.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_org",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The diplomatic <w>corps</w> gathered to discuss the new international treaty.",
                            "target_lang_translations": {
                                "ru": "Дипломатический <w>корпус</w> собрался, чтобы обсудить новый международный договор."
                            }
                        },
                        {
                            "text": "She decided to join the peace <w>corps</w> to help communities in developing countries.",
                            "target_lang_translations": {
                                "ru": "Она решила вступить в <w>Корпус</w> мира, чтобы помогать общинам в развивающихся странах."
                            }
                        }
                    ],
                    "synonyms": [
                        "body",
                        "team",
                        "group",
                        "squad"
                    ],
                    "common_phrases": [
                        "diplomatic corps",
                        "press corps",
                        "peace corps"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организованная группа людей, объединенных общей целью или профессией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корпус",
                                "target_lang_sense_clarification": "группа людей (дипломатический, мира)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10fc57f6-d280-3236-9cbf-45cef90c6fbe",
                    "sense_definition": "A large military unit composed of two or more divisions.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_mil",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The general ordered the entire army <w>corps</w> to advance toward the river.",
                            "target_lang_translations": {
                                "ru": "Генерал приказал всему армейскому <w>корпусу</w> выдвигаться к реке."
                            }
                        },
                        {
                            "text": "He served with distinction in the marine <w>corps</w> for over a decade.",
                            "target_lang_translations": {
                                "ru": "Он с отличием прослужил в <w>корпусе</w> морской пехоты более десяти лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "unit",
                        "detachment"
                    ],
                    "common_phrases": [
                        "army corps",
                        "marine corps"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупное воинское формирование, состоящее из двух или более дивизий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корпус",
                                "target_lang_sense_clarification": "военное соединение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f58aad4a-cb3f-3d47-90d4-45c9537c602c",
                    "sense_definition": "The group of dancers in a ballet company who perform together, distinct from the soloists.",

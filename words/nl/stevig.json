                    "synonyms": [
                        "sterk",
                        "solide",
                        "degelijk",
                        "vast"
                    ],
                    "antonyms": [
                        "slap",
                        "wankel",
                        "breekbaar",
                        "fragiel"
                    ],
                    "common_phrases": [
                        "stevige schoenen",
                        "stevig in je schoenen staan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прочный и надежный; устойчивый к давлению, ударам или износу.",
                        "en": "Strong and solid; resistant to pressure, impact, or heavy use."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прочный",
                                "target_lang_sense_clarification": "о материалах и конструкциях"

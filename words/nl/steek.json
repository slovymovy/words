                            {
                                "target_lang_word": "lurch",
                                "target_lang_sense_clarification": "in the phrase 'leave in the lurch'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "бросать",
                                "target_lang_sense_clarification": "в выражении 'in de steek laten'"
                            },
                            {
                                "target_lang_word": "подводить",
                                "target_lang_sense_clarification": "о технике или людях"
                            }
                        ]
                    }

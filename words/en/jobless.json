    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cbb1e29a-1d60-3da1-a722-f9f74ebf4ddb",
                    "sense_definition": "Lacking employment; not having a paid job.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many people became <w>jobless</w> after the local factory closed down.",
                            "target_lang_translations": {
                                "ru": "Многие люди стали <w>безработными</w> после закрытия местной фабрики."
                            }
                        },
                        {
                            "text": "The government is trying to lower the <w>jobless</w> rate by creating new programs.",
                            "target_lang_translations": {
                                "ru": "Правительство пытается снизить уровень <w>безработицы</w> путем создания новых программ."
                            }
                        }
                    ],
                    "synonyms": [
                        "unemployed"
                    ],
                    "antonyms": [
                        "employed"
                    ],
                    "common_phrases": [
                        "jobless rate",
                        "jobless claims",
                        "jobless growth",
                        "jobless recovery"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не имеющий оплачиваемой работы; находящийся без работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безработный",
                                "target_lang_sense_clarification": "человек, не имеющий работы"
                            },
                            {
                                "target_lang_word": "без работы",
                                "target_lang_sense_clarification": "состояние отсутствия занятости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "072e18b6-e677-39be-8a7a-9b29f1d4fbef",
                    "sense_definition": "Having too much free time or nothing productive to do; often used to describe someone engaging in petty, useless, or annoying behavior.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is honestly so <w>jobless</w> to scroll through someone's old posts just to leave mean comments.",
                            "target_lang_translations": {
                                "ru": "Честно говоря, листать чьи-то старые посты только ради того, чтобы оставить злобные комментарии — это удел <w>бездельников</w>."
                            }
                        },
                        {
                            "text": "Stop starting drama over nothing; you are acting totally <w>jobless</w>.",
                            "target_lang_translations": {
                                "ru": "Хватит раздувать драму на пустом месте, тебе явно <w>нечем заняться</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "often used by youth or on the internet"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий слишком много свободного времени и занимающийся ерундой; часто используется для описания мелочного или раздражающего поведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бездельник",
                                "target_lang_sense_clarification": "тот, кто тратит время впустую"
                            },
                            {
                                "target_lang_word": "нечем заняться",
                                "target_lang_sense_clarification": "о поведении от скуки"
                            }
                        ]
                    }
                }
            ]
        }
    ]

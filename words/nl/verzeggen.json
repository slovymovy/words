    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "97dd3d1a-40c7-388d-9943-635a012cc809",
                    "sense_definition": "Plechtig beloven iets te doen of te geven.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft zijn volledige steun aan het nieuwe project <w>verzegd</w>.",
                            "target_lang_translations": {
                                "en": "He has <w>pledged</w> his full support to the new project.",
                                "ru": "Он <w>обещал</w> свою полную поддержку новому проекту."
                            }
                        },
                        {
                            "text": "Zij heeft hem haar hand <w>verzegd</w> in het huwelijk.",
                            "target_lang_translations": {
                                "en": "She <w>pledged</w> her hand to him in marriage.",
                                "ru": "Она <w>обещала</w> ему свою руку в замужестве."
                            }
                        }
                    ],
                    "synonyms": [
                        "beloven",
                        "toezeggen"
                    ],
                    "target_lang_definitions": {
                        "en": "To solemnly promise to do or give something.",
                        "ru": "Торжественно обещать сделать или дать что-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pledge",
                                "target_lang_sense_clarification": "solemn promise"
                            },
                            {
                                "target_lang_word": "promise",
                                "target_lang_sense_clarification": "commit to do something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "обещать",
                                "target_lang_sense_clarification": "брать на себя обязательство"
                            },
                            {
                                "target_lang_word": "давать слово",
                                "target_lang_sense_clarification": "торжественное обещание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d6e2964-2607-3722-a1f7-b54285e6c833",
                    "sense_definition": "Niet meer beschikbaar zijn omdat het al aan iemand anders beloofd is.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het spijt me, maar die mooie antieke kast is helaas al <w>verzegd</w>.",
                            "target_lang_translations": {
                                "en": "I am sorry, but that beautiful antique cupboard is unfortunately already <w>spoken for</w>.",
                                "ru": "Мне жаль, но этот красивый антикварный шкаф, к сожалению, уже <w>обещан</w>."
                            }
                        },
                        {
                            "text": "Ik wilde die kaartjes kopen, maar ze waren al aan een ander <w>verzegd</w>.",
                            "target_lang_translations": {
                                "en": "I wanted to buy those tickets, but they were already <w>promised</w> to someone else.",
                                "ru": "Я хотел купить эти билеты, но они уже были <w>обещаны</w> другому."
                            }
                        }
                    ],
                    "common_phrases": [
                        "al-verzegd-zijn"
                    ],
                    "target_lang_definitions": {
                        "en": "To be no longer available because it has already been promised to someone else.",
                        "ru": "Быть недоступным, так как уже обещано кому-то другому."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be spoken for",
                                "target_lang_sense_clarification": "reserved or claimed by someone"
                            },
                            {
                                "target_lang_word": "be promised",
                                "target_lang_sense_clarification": "committed to someone else"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "быть обещанным",
                                "target_lang_sense_clarification": "зарезервировано за кем-то"
                            },
                            {
                                "target_lang_word": "обещать",
                                "target_lang_sense_clarification": "отдать кому-то другому"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79bf3b0a-c36f-326e-9262-d4ce84e2836b",
                    "sense_definition": "Een gedane belofte verbreken of ervan afzien.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In oude verhalen leest men soms hoe een ridder zijn eed moest <w>verzeggen</w>.",
                            "target_lang_translations": {
                                "en": "In old stories one sometimes reads how a knight had to <w>renounce</w> his oath.",
                                "ru": "В старых легендах иногда читаешь, как рыцарь должен был <w>отречься</w> от своей клятвы."
                            }
                        },
                        {
                            "text": "Het werd als zeer onfatsoenlijk beschouwd om een huwelijksbelofte te <w>verzeggen</w>.",
                            "target_lang_translations": {
                                "en": "It was considered very indecent to <w>break</w> a marriage vow.",
                                "ru": "Считалось крайне неприличным <w>нарушать</w> брачное обещание."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Wordt voornamelijk aangetroffen in oudere teksten of historische contexten."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To break a promise made or to renounce it.",
                        "ru": "Нарушить данное обещание или отказаться от него."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "renounce",
                                "target_lang_sense_clarification": "formally abandon a claim or oath"
                            },
                            {
                                "target_lang_word": "retract",
                                "target_lang_sense_clarification": "withdraw a promise"
                            },
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "fail to keep a promise"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отрекаться",
                                "target_lang_sense_clarification": "формально отказываться от клятвы"
                            },
                            {
                                "target_lang_word": "нарушать",
                                "target_lang_sense_clarification": "не сдержать слово"
                            }
                        ]
                    }
                }
            ]
        }
    ]

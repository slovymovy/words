    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6e8fd3d1-814b-38f8-ab95-9526b6c928f4",
                    "sense_definition": "Ciemna, oleista ciecz wydobywana z ziemi, będąca ważnym surowcem energetycznym i chemicznym.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fuel_energy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ceny <w>ropy</w> na rynkach światowych gwałtownie wzrosły w ostatnim tygodniu.",
                            "target_lang_translations": {
                                "ru": "Цены на <w>нефть</w> на мировых рынках резко выросли на прошлой неделе."
                            }
                        },
                        {
                            "text": "Wyciek <w>ropy</w> z uszkodzonego tankowca spowodował katastrofę ekologiczną.",
                            "target_lang_translations": {
                                "ru": "Разлив <w>нефти</w> из поврежденного танкера вызвал экологическую катастрофу."
                            }
                        }
                    ],
                    "synonyms": [
                        "ropa naftowa"
                    ],
                    "common_phrases": [
                        "wydobycie ropy",
                        "ceny ropy",
                        "złoża ropy",
                        "baryłka ropy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тёмная маслянистая жидкость, добываемая из земли, являющаяся важным энергетическим и химическим сырьем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нефть",
                                "target_lang_sense_clarification": "полезное ископаемое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f2328f8-939c-335a-bac3-2b859342a4ee",
                    "sense_definition": "Paliwo płynne stosowane do napędu silników wysokoprężnych (Diesla); olej napędowy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fuel_energy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój samochód jeździ na <w>ropę</w>, co jest bardziej ekonomiczne w długich trasach.",
                            "target_lang_translations": {
                                "ru": "Моя машина ездит на <w>солярке</w>, что экономичнее в дальних поездках."
                            }
                        },
                        {
                            "text": "Muszę zjechać na stację benzynową, żeby zatankować <w>ropę</w> do pełna.",
                            "target_lang_translations": {
                                "ru": "Мне нужно заехать на заправку, чтобы залить полный бак <w>солярки</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "olej napędowy"
                    ],
                    "common_phrases": [
                        "tankować ropę",
                        "silnik na ropę",
                        "samochód na ropę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common spoken term for diesel fuel"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Жидкое топливо, используемое для дизельных двигателей; дизельное топливо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "солярка",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "дизельное топливо",
                                "target_lang_sense_clarification": "вид топлива"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b23dd9ef-3e79-3f60-96cd-e3739a060509",
                    "sense_definition": "Gęsta, żółtawa wydzielina powstająca w organizmie w miejscu zakażenia bakteryjnego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "body_secretion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z zainfekowanej rany zaczęła sączyć się gęsta <w>ropa</w>.",
                            "target_lang_translations": {
                                "ru": "Из инфицированной раны начал сочиться густой <w>гной</w>."
                            }
                        },
                        {
                            "text": "Lekarz musiał naciąć ropień, aby usunąć nagromadzoną w nim <w>ropę</w>.",
                            "target_lang_translations": {
                                "ru": "Врачу пришлось вскрыть нарыв, чтобы удалить скопившийся в нем <w>гной</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ropa z rany",
                        "zbiera się ropa",
                        "podchodzić ropą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Густая желтоватая жидкость, образующаяся в организме в месте бактериальной инфекции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гной",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d48cab0-5de0-309a-be0a-c5a8ff4c20f5",
                    "sense_definition": "Zaschnięta wydzielina gromadząca się w kącikach oczu, zazwyczaj podczas snu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "body_secretion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po przebudzeniu dziecko miało dużo <w>ropy</w> w kącikach oczu.",
                            "target_lang_translations": {
                                "ru": "После пробуждения у ребенка было много <w>гноя</w> в уголках глаз."
                            }
                        },
                        {
                            "text": "Przemyj oczy solą fizjologiczną, aby delikatnie usunąć zaschniętą <w>ropę</w>.",
                            "target_lang_translations": {
                                "ru": "Промой глаза физраствором, чтобы аккуратно удалить засохший <w>гной</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ropa w oczach",
                        "mieć ropę w oczach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Засохшие выделения, скапливающиеся в уголках глаз, обычно во время сна."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гной",
                                "target_lang_sense_clarification": "выделения из глаз"
                            },
                            {
                                "target_lang_word": "кисляки",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "110ad5fb-7365-32cc-976b-f3373b97a739",
                    "sense_definition": "Pospolite i obraźliwe określenie na kobietę, która jest uważana za nieatrakcyjną fizycznie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Używanie słowa <w>ropa</w> w stosunku do kobiety jest bardzo niekulturalne.",
                            "target_lang_translations": {
                                "ru": "Использование слова <w>уродина</w> по отношению к женщине очень некультурно."
                            }
                        },
                        {
                            "text": "To określenie jest wulgarne i nie powinieneś nazywać nikogo <w>ropą</w>.",
                            "target_lang_translations": {
                                "ru": "Это выражение вульгарно, и ты не должен называть никого <w>уродиной</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "offensive"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грубое и оскорбительное название женщины, которую считают физически непривлекательной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уродина",
                                "target_lang_sense_clarification": "оскорбление"
                            },
                            {
                                "target_lang_word": "страшила",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }
            ]
        }
    ]

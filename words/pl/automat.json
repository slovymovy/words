    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7d99f3ee-9281-30a8-b355-8710f37923fa",
                    "sense_definition": "Maszyna, która wydaje towary (np. napoje, bilety, przekąski) po wrzuceniu monet, żetonów lub użyciu karty płatniczej.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem batonika i wodę w <w>automacie</w> na korytarzu.",
                            "target_lang_translations": {
                                "ru": "Я купил батончик и воду в <w>автомате</w> в коридоре."
                            }
                        },
                        {
                            "text": "Ten <w>automat</w> biletowy jest zepsuty i nie przyjmuje monet.",
                            "target_lang_translations": {
                                "ru": "Этот билетный <w>автомат</w> сломан и не принимает монеты."
                            }
                        }
                    ],
                    "common_phrases": [
                        "automat z kawą",
                        "automat biletowy",
                        "kupić w automacie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Машина, выдающая товары (например, напитки, билеты, закуски) после опускания монет, жетонов или использования платежной карты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "автомат",
                                "target_lang_sense_clarification": "торговый или билетный"
                            },
                            {
                                "target_lang_word": "торговый автомат",
                                "target_lang_sense_clarification": "устройство продажи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5bf197e-1a2d-3e35-b26b-c7de3e101325",
                    "sense_definition": "Samochód wyposażony w automatyczną skrzynię biegów, który nie wymaga ręcznej zmiany biegów przez kierowcę.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój nowy samochód to <w>automat</w>, więc jazda w korkach jest znacznie wygodniejsza.",
                            "target_lang_translations": {
                                "ru": "Моя новая машина — <w>автомат</w>, поэтому езда в пробках гораздо удобнее."
                            }
                        },
                        {
                            "text": "Nigdy wcześniej nie prowadziłem <w>automatu</w>, zawsze jeździłem na manualu.",
                            "target_lang_translations": {
                                "ru": "Я никогда раньше не водил <w>автомат</w>, всегда ездил на механике."
                            }
                        }
                    ],
                    "common_phrases": [
                        "skrzynia automat",
                        "prawo jazdy na automat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Автомобиль, оснащенный автоматической коробкой передач, не требующий ручного переключения скоростей водителем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "автомат",
                                "target_lang_sense_clarification": "разг. об автомобиле или коробке передач"
                            },
                            {
                                "target_lang_word": "автоматическая коробка передач",
                                "target_lang_sense_clarification": "механизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3b8c121-3493-374f-bf08-978d12a83142",
                    "sense_definition": "Potoczne określenie pralki automatycznej, która wykonuje cały cykl prania bez udziału człowieka.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę wstawić pranie, ale nasz <w>automat</w> dziwnie hałasuje.",
                            "target_lang_translations": {
                                "ru": "Мне нужно поставить стирку, но наша <w>стиральная машина</w> странно шумит."
                            }
                        },
                        {
                            "text": "W łazience mamy mało miejsca, więc kupiliśmy wąski <w>automat</w>.",
                            "target_lang_translations": {
                                "ru": "В ванной у нас мало места, поэтому мы купили узкую <w>стиральную машину-автомат</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "prać w automacie",
                        "podłączyć automat"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название автоматической стиральной машины, выполняющей весь цикл стирки без участия человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стиральная машина-автомат",
                                "target_lang_sense_clarification": "бытовая техника"
                            },
                            {
                                "target_lang_word": "стиральная машина",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "babbb1b0-a31d-37df-8792-54fa17211618",
                    "sense_definition": "Urządzenie lub mechanizm wykonujący pracę samodzielnie, bez ciągłego udziału człowieka.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Brama wjazdowa działa jako <w>automat</w> i otwiera się na pilota.",
                            "target_lang_translations": {
                                "ru": "Въездные ворота работают как <w>автомат</w> и открываются с пульта."
                            }
                        },
                        {
                            "text": "Na klatce schodowej światło włącza <w>automat</w> reagujący na ruch.",
                            "target_lang_translations": {
                                "ru": "В подъезде свет включает <w>автомат</w>, реагирующий на движение."
                            }
                        }
                    ],
                    "synonyms": [
                        "maszyna",
                        "robot"
                    ],
                    "common_phrases": [
                        "pełny automat",
                        "automat schodowy"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "26cc6b83-1169-3ecb-81b7-cab7d573cf03",
                    "sense_definition": "Een document waarop staat hoeveel er betaald moet worden voor geleverde goederen of diensten.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "bill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heb je de <w>nota</w> voor het diner al betaald?",
                            "target_lang_translations": {
                                "en": "Have you paid the <w>bill</w> for dinner yet?",
                                "ru": "Ты уже оплатил <w>счёт</w> за ужин?"
                            }
                        },
                        {
                            "text": "De aannemer stuurde ons gisteren de <w>nota</w> voor de verbouwing.",
                            "target_lang_translations": {
                                "en": "The contractor sent us the <w>invoice</w> for the renovation yesterday.",
                                "ru": "Вчера подрядчик прислал нам <w>счёт</w> за ремонт."
                            }
                        }
                    ],
                    "synonyms": [
                        "rekening",
                        "factuur"
                    ],
                    "common_phrases": [
                        "de nota betalen",
                        "een gepeperde nota"
                    ],
                    "target_lang_definitions": {
                        "en": "A document stating the amount to be paid for provided goods or services.",
                        "ru": "Документ, в котором указана сумма к оплате за предоставленные товары или услуги."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bill",
                                "target_lang_sense_clarification": "general request for payment"
                            },
                            {
                                "target_lang_word": "invoice",
                                "target_lang_sense_clarification": "commercial document requesting payment"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "счёт",
                                "target_lang_sense_clarification": "документ на оплату"
                            },
                            {
                                "target_lang_word": "фактура",
                                "target_lang_sense_clarification": "коммерческий документ с перечнем товаров"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d277276-a435-34e7-a011-1fe14961c149",
                    "sense_definition": "Een korte tekst die is opgeschreven om iets niet te vergeten of om informatie door te geven.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "note",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb een korte <w>nota</w> gemaakt van de belangrijkste punten uit de vergadering.",
                            "target_lang_translations": {
                                "en": "I made a short <w>note</w> of the key points from the meeting.",
                                "ru": "Я сделал краткую <w>заметку</w> о ключевых моментах собрания."
                            }
                        },
                        {
                            "text": "Neem goed <w>nota</w> van de veiligheidsinstructies voordat je begint.",
                            "target_lang_translations": {
                                "en": "Take good <w>note</w> of the safety instructions before you begin.",
                                "ru": "Прими к <w>сведению</w> инструкции по технике безопасности, прежде чем начать."
                            }
                        }
                    ],
                    "synonyms": [
                        "notitie",
                        "aantekening",
                        "memo"
                    ],
                    "common_phrases": [
                        "nota nemen van",
                        "een nota maken"
                    ],
                    "target_lang_definitions": {
                        "en": "A short text written to remember something or convey information.",
                        "ru": "Короткий текст, записанный, чтобы что-то не забыть или передать информацию."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "note",
                                "target_lang_sense_clarification": "brief record or message"
                            },
                            {
                                "target_lang_word": "memo",
                                "target_lang_sense_clarification": "short official message"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "заметка",
                                "target_lang_sense_clarification": "краткая запись для памяти"
                            },
                            {
                                "target_lang_word": "записка",
                                "target_lang_sense_clarification": "короткое сообщение на бумаге"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02df3d26-3266-36e7-b744-21b794496408",
                    "sense_definition": "Een formeel geschrift waarin een overheid of organisatie een standpunt, beleid of mededeling uiteenzet.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "official_document",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De minister presenteerde een nieuwe <w>nota</w> over het onderwijsbeleid aan de Tweede Kamer.",
                            "target_lang_translations": {
                                "en": "The minister presented a new <w>memorandum</w> on education policy to the House of Representatives.",
                                "ru": "Министр представил Палате представителей новый <w>меморандум</w> об образовательной политике."
                            }
                        },
                        {
                            "text": "In de diplomatieke <w>nota</w> werd bezwaar gemaakt tegen de grenssluiting.",
                            "target_lang_translations": {
                                "en": "In the diplomatic <w>note</w>, an objection was raised against the border closure.",
                                "ru": "В дипломатической <w>ноте</w> было выражено возражение против закрытия границ."
                            }
                        }
                    ],
                    "synonyms": [
                        "beleidsstuk",
                        "memorandum"
                    ],
                    "common_phrases": [
                        "nota van toelichting",
                        "diplomatieke nota"
                    ],
                    "target_lang_definitions": {
                        "en": "A formal document in which a government or organization outlines a position, policy, or statement.",
                        "ru": "Официальный документ, в котором правительство или организация излагает позицию, политику или заявление."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "memorandum",
                                "target_lang_sense_clarification": "formal report or policy document"
                            },
                            {
                                "target_lang_word": "policy document",
                                "target_lang_sense_clarification": "government paper outlining policy"
                            },
                            {
                                "target_lang_word": "note",
                                "target_lang_sense_clarification": "diplomatic communication"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "меморандум",
                                "target_lang_sense_clarification": "докладная записка или политический документ"
                            },
                            {
                                "target_lang_word": "нота",
                                "target_lang_sense_clarification": "дипломатическое обращение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

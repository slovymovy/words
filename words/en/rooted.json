    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1c611036-6f02-306d-86e6-513bd565f696",
                    "sense_definition": "Fixed in one position; immobile; unable to move.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "fixed_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She stood <w>rooted</w> to the spot with fear when she saw the spider.",
                            "target_lang_translations": {
                                "ru": "Она стояла <w>как вкопанная</w> от страха, когда увидела паука."
                            }
                        },
                        {
                            "text": "The heavy monument remained <w>rooted</w> in place despite the strong winds.",
                            "target_lang_translations": {
                                "ru": "Тяжелый памятник оставался <w>прикованным</w> к месту, несмотря на сильный ветер."
                            }
                        }
                    ],
                    "synonyms": [
                        "transfixed",
                        "frozen",
                        "immobile"
                    ],
                    "antonyms": [
                        "mobile"
                    ],
                    "common_phrases": [
                        "rooted to the spot",
                        "rooted in place"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зафиксированный в одном положении; неподвижный; неспособный двигаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "как вкопанный",
                                "target_lang_sense_clarification": "идиома: застывший на месте"
                            },
                            {
                                "target_lang_word": "приросший",
                                "target_lang_sense_clarification": "к месту"
                            },
                            {
                                "target_lang_word": "неподвижный",
                                "target_lang_sense_clarification": "зафиксированный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2eea3ae-3b2d-388e-b419-48739cef9bf6",
                    "sense_definition": "Having a basic or fundamental connection to a source or origin.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "established_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His political beliefs are deeply <w>rooted</w> in his family's history.",
                            "target_lang_translations": {
                                "ru": "Его политические убеждения глубоко <w>уходят корнями</w> в историю его семьи."
                            }
                        },
                        {
                            "text": "The author wrote a story <w>rooted</w> in reality but filled with magical elements.",
                            "target_lang_translations": {
                                "ru": "Автор написал историю, <w>основанную</w> на реальности, но наполненную магическими элементами."
                            }
                        }
                    ],
                    "synonyms": [
                        "based",
                        "grounded",
                        "founded"
                    ],
                    "antonyms": [
                        "baseless"
                    ],
                    "common_phrases": [
                        "rooted in reality",
                        "rooted in tradition",
                        "rooted in history"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий фундаментальную связь с источником или происхождением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уходящий корнями",
                                "target_lang_sense_clarification": "происходящий из"
                            },
                            {
                                "target_lang_word": "основанный",
                                "target_lang_sense_clarification": "базирующийся на"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14ea7a97-e14c-3896-89b7-eee79f5782c4",
                    "sense_definition": "Firmly established, ingrained, or habitual.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "established_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has a <w>rooted</w> dislike for waking up early in the morning.",
                            "target_lang_translations": {
                                "ru": "У него <w>укоренившаяся</w> неприязнь к раннему подъему по утрам."
                            }
                        },
                        {
                            "text": "The conflict was caused by deep <w>rooted</w> prejudices between the two groups.",
                            "target_lang_translations": {
                                "ru": "Конфликт был вызван глубоко <w>укоренившимися</w> предрассудками между двумя группами."
                            }
                        }
                    ],
                    "synonyms": [
                        "deep-seated",
                        "entrenched",
                        "ingrained"
                    ],
                    "common_phrases": [
                        "deep rooted"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прочно устоявшийся, укоренившийся или привычный."

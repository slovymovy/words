{
    "word_family": [
        "aureolka"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b54c12c7-3d53-37cc-b657-338983a0f75f",
                    "sense_definition": "Świetlisty krąg lub otok malowany wokół głów świętych w sztuce religijnej, symbolizujący ich świętość.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "visual_halo",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na średniowiecznym obrazie każda postać ma złotą <w>aureolę</w>.",
                            "target_lang_translations": {
                                "ru": "На средневековой картине у каждой фигуры есть золотой <w>нимб</w>."
                            }
                        },
                        {
                            "text": "Artysta namalował delikatną <w>aureolę</w> nad głową anioła.",
                            "target_lang_translations": {
                                "ru": "Художник нарисовал нежный <w>нимб</w> над головой ангела."
                            }
                        }
                    ],
                    "synonyms": [
                        "nimb",
                        "gloria"
                    ],
                    "common_phrases": [
                        "złota aureola",
                        "aureola świętego",
                        "aureola nad głową"
                    ],
                    "target_lang_definitions": {
                        "ru": "Светящийся круг или сияние вокруг головы святых в религиозном искусстве, символизирующее их святость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нимб",
                                "target_lang_sense_clarification": "над головой святого"
                            },
                            {
                                "target_lang_word": "ореол",
                                "target_lang_sense_clarification": "сияние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68c78611-c9ff-3a60-9300-8d377d2266c3",
                    "sense_definition": "Niezwykła atmosfera chwały, szacunku, podziwu lub tajemniczości otaczająca kogoś w opinii innych ludzi.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_halo",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po tym bohaterskim czynie otoczyła go <w>aureola</w> sławy.",
                            "target_lang_translations": {
                                "ru": "После этого героического поступка его окружил <w>ореол</w> славы."
                            }
                        },
                        {
                            "text": "Z czasem jego <w>aureola</w> nieskazitelności zaczęła blednąć.",
                            "target_lang_translations": {
                                "ru": "Со временем его <w>ореол</w> безупречности начал блекнуть."
                            }
                        }
                    ],
                    "synonyms": [
                        "nimb",
                        "sława",
                        "chwała",
                        "splendor"
                    ],
                    "common_phrases": [
                        "aureola męczennika",
                        "aureola sławy",
                        "aureola tajemnicy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Атмосфера славы, уважения или таинственности, окружающая кого-то в глазах других людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ореол",
                                "target_lang_sense_clarification": "славы или таинственности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e30e33d-68eb-3529-beda-8d6aa0bca508",
                    "sense_definition": "Świetlista obwódka widoczna wokół źródła światła lub jasny kontur otaczający jakiś przedmiot.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "visual_halo",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wokół księżyca w pełni pojawiła się jasna <w>aureola</w>.",
                            "target_lang_translations": {
                                "ru": "Вокруг полной луны появился яркий <w>ореол</w>."
                            }
                        },
                        {
                            "text": "Latarnie we mgle otoczone były mleczną <w>aureolą</w>.",
                            "target_lang_translations": {
                                "ru": "Фонари в тумане были окружены молочным <w>ореолом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "halo",
                        "nimb"
                    ],
                    "common_phrases": [
                        "aureola światła",
                        "aureola wokół słońca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Светящийся круг, видимый вокруг источника света, или яркий контур вокруг предмета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ореол",
                                "target_lang_sense_clarification": "вокруг света"
                            },
                            {
                                "target_lang_word": "гало",
                                "target_lang_sense_clarification": "оптическое явление"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
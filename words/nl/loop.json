                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иду",
                                "target_lang_sense_clarification": "двигаться пешком (в данный момент)"
                            },
                            {
                                "target_lang_word": "хожу",
                                "target_lang_sense_clarification": "двигаться пешком (регулярно)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2263f24b-624c-326f-825e-fb762520ece6",
                    "sense_definition": "Gebiedende wijs van <w>lopen</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Loop</w> voorzichtig, want de vloer is nat.",
                            "target_lang_translations": {
                                "ru": "<w>Иди</w> осторожно, потому что пол мокрый."
                            }
                        },
                        {
                            "text": "<w>Loop</w> niet zo te schreeuwen in de gang.",
                            "target_lang_translations": {
                                "ru": "Не <w>ходи</w> и не кричи в коридоре."
                            }

{
    "word_family": [
        "позволить",
        "позволение",
        "позволительный",
        "непозволительный"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5df5b1f3-a252-30f0-b671-78682aab4991",
                    "sense_definition": "Разрешать, давать согласие на совершение какого-либо действия.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Родители не <w>позволяют</w> детям смотреть телевизор поздно вечером.",
                            "target_lang_translations": {
                                "en": "Parents do not <w>allow</w> their children to watch TV late in the evening."
                            }
                        },
                        {
                            "text": "Не стоило <w>позволять</w> ему брать твою машину без спроса.",
                            "target_lang_translations": {
                                "en": "You shouldn't have <w>allowed</w> him to take your car without asking."
                            }
                        }
                    ],
                    "synonyms": [
                        "разрешать",
                        "дозволять"
                    ],
                    "antonyms": [
                        "запрещать"
                    ],
                    "common_phrases": [
                        "не позволять",
                        "позволять детям"
                    ],
                    "target_lang_definitions": {
                        "en": "To permit or give consent to perform an action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "allow",
                                "target_lang_sense_clarification": "to give permission"
                            },
                            {
                                "target_lang_word": "permit",
                                "target_lang_sense_clarification": "formal permission"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c50e62c1-fcbd-3670-bfa7-ec8f11d4edf2",
                    "sense_definition": "Давать возможность сделать что-либо, создавать для этого условия.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "possibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хорошая погода <w>позволяет</w> нам сегодня погулять в парке.",
                            "target_lang_translations": {
                                "en": "The good weather <w>allows</w> us to take a walk in the park today."
                            }
                        },
                        {
                            "text": "Новая программа <w>позволяет</w> редактировать фотографии намного быстрее.",
                            "target_lang_translations": {
                                "en": "The new program <w>allows</w> editing photos much faster."
                            }
                        }
                    ],
                    "synonyms": [
                        "давать возможность"
                    ],
                    "antonyms": [
                        "препятствовать"
                    ],
                    "common_phrases": [
                        "время позволяет",
                        "здоровье позволяет",
                        "средства позволяют",
                        "ситуация позволяет"
                    ],
                    "target_lang_definitions": {
                        "en": "To provide the opportunity or create conditions to do something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "enable",
                                "target_lang_sense_clarification": "to give the means or ability"
                            },
                            {
                                "target_lang_word": "allow",
                                "target_lang_sense_clarification": "to make possible"
                            },
                            {
                                "target_lang_word": "permit",
                                "target_lang_sense_clarification": "to make possible (often formal)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
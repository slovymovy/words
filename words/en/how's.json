                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "способы",
                                "target_lang_sense_clarification": "существительное"
                            },
                            {
                                "target_lang_word": "методы",
                                "target_lang_sense_clarification": "существительное"
                            },
                            {
                                "target_lang_word": "подробности",
                                "target_lang_sense_clarification": "существительное"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "acce07c8-cfe1-36f2-9761-e31441709fbb",
                    "sense_definition": "A large country in East Asia, officially known as the People's Republic of China.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "country_location",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "I want to visit <w>China</w> to see the Great Wall.",
                            "target_lang_translations": {
                                "ru": "Я хочу посетить <w>Китай</w>, чтобы увидеть Великую Китайскую стену."
                            }
                        },
                        {
                            "text": "The population of <w>China</w> is over one billion people.",
                            "target_lang_translations": {
                                "ru": "Население <w>Китая</w> составляет более одного миллиарда человек."
                            }
                        }
                    ],
                    "synonyms": [
                        "PRC",
                        "Middle Kingdom",
                        "People's Republic of China"
                    ],
                    "common_phrases": [
                        "Made in China",
                        "mainland China",
                        "Great Wall of China"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая страна в Восточной Азии, официально известная как Китайская Народная Республика."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Китай",
                                "target_lang_sense_clarification": "страна"
                            },
                            {
                                "target_lang_word": "Поднебесная",
                                "target_lang_sense_clarification": "поэтическое или историческое название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dafb4967-4980-3c0f-bbe0-1fc82b564906",
                    "sense_definition": "A female given name.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My friend <w>China</w> is coming to the party tonight.",
                            "target_lang_translations": {
                                "ru": "Моя подруга <w>Чайна</w> придет сегодня вечером на вечеринку."
                            }
                        },
                        {
                            "text": "She decided to name her daughter <w>China</w> because she loved the sound of it.",
                            "target_lang_translations": {
                                "ru": "Она решила назвать свою дочь <w>Чайна</w>, потому что ей нравилось, как это звучит."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Чайна",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2cbb4e94-c7bb-339c-88d5-a9c293e2dad8",
                    "sense_definition": "A hard, white ceramic material of high quality; dishes or ornaments made from this material.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "material_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Be careful with that plate because it is made of fine <w>china</w>.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен с этой тарелкой, потому что она сделана из тонкого <w>фарфора</w>."
                            }
                        },
                        {
                            "text": "We only use the expensive <w>china</w> for special holiday dinners.",
                            "target_lang_translations": {
                                "ru": "Мы используем дорогой <w>фарфор</w> только для особых праздничных ужинов."
                            }
                        }
                    ],
                    "synonyms": [
                        "porcelain",
                        "chinaware"
                    ],
                    "common_phrases": [
                        "fine china",
                        "bone china",
                        "set of china"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердый белый керамический материал высокого качества; посуда или украшения из этого материала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фарфор",
                                "target_lang_sense_clarification": "материал"
                            },
                            {
                                "target_lang_word": "фарфоровая посуда",
                                "target_lang_sense_clarification": "изделия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "340b1143-2275-3fae-8cc7-c6deddb1ff2b",
                    "sense_definition": "A friend or companion (derived from Cockney rhyming slang 'china plate' for 'mate').",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "friendship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hello my old <w>china</w>, how have you been lately?",
                            "target_lang_translations": {
                                "ru": "Привет, старина <w>приятель</w>, как ты поживаешь в последнее время?"
                            }
                        },
                        {
                            "text": "He has been my <w>china</w> since we were in school together.",
                            "target_lang_translations": {
                                "ru": "Он был моим <w>другом</w> с тех пор, как мы вместе учились в школе."
                            }
                        }
                    ],
                    "synonyms": [
                        "mate",
                        "friend",
                        "chum"
                    ],
                    "common_phrases": [
                        "old china"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "Cockney rhyming slang"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "British English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Друг или приятель (происходит от рифмованного сленга кокни 'china plate' — 'mate')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приятель",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "друг",
                                "target_lang_sense_clarification": "значение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "5f103146-693c-32cb-a75c-d78447053eaf",
                    "sense_definition": "To feel bitterness or indignation at a circumstance, action, or person, usually because it is perceived as unfair or insulting.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "emotion_bitterness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She began to <w>resent</w> the constant interference in her private life.",
                            "target_lang_translations": {
                                "ru": "Она начала <w>негодовать</w> из-за постоянного вмешательства в её личную жизнь."
                            }
                        },
                        {
                            "text": "He <w>resents</w> being treated like a child by his older siblings.",
                            "target_lang_translations": {
                                "ru": "Он <w>возмущается</w> тем, что старшие братья и сестры обращаются с ним как с ребенком."
                            }
                        },
                        {
                            "text": "The employees deeply <w>resent</w> the sudden changes to their work schedule.",
                            "target_lang_translations": {
                                "ru": "Сотрудники глубоко <w>негодуют</w> по поводу внезапных изменений в их рабочем графике."
                            }
                        }
                    ],
                    "synonyms": [
                        "begrudge",
                        "grudge"
                    ],
                    "antonyms": [
                        "welcome",
                        "appreciate"
                    ],
                    "common_phrases": [
                        "bitterly resent",
                        "deeply resent",
                        "resent the fact",
                        "resent the implication"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать горечь или негодование по поводу ситуации, действия или человека, обычно потому, что это воспринимается как несправедливость или оскорбление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "негодовать",
                                "target_lang_sense_clarification": "испытывать возмущение"

                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De burgemeester was <w>tegenwoordig</w> bij de opening van het nieuwe gebouw.",
                            "target_lang_translations": {
                                "en": "The mayor was <w>present</w> at the opening of the new building.",
                                "ru": "Мэр <w>присутствовал</w> на открытии нового здания."
                            }
                        },
                        {
                            "text": "Alle leden van het bestuur moeten bij de vergadering <w>tegenwoordig</w> zijn.",
                            "target_lang_translations": {

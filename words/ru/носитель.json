                    "common_phrases": [
                        "носитель языка",
                        "носитель культуры",
                        "носитель информации",
                        "носитель суверенитета"
                    ]
                },
                {
                    "sense_id": "d19d270f-096d-32b3-8c4e-f1a909439526",
                    "sense_definition": "Материальный объект или устройство, предназначенное для записи, хранения и передачи информации (данных).",

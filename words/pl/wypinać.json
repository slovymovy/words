            "pos": "verb",
            "senses": [
                {
                    "sense_id": "701cab0c-9372-3ab4-a979-c608540b7f06",
                    "sense_definition": "Odłączać przedmiot, który był przypięty, wpięty lub włożony w gniazdo.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz ostrożnie <w>wypinać</w> ten delikatny kabel, żeby nie uszkodzić gniazda.",
                            "target_lang_translations": {
                                "ru": "Ты должен осторожно <w>отсоединять</w> этот хрупкий кабель, чтобы не повредить гнездо."
                            }
                        },
                        {
                            "text": "Zawsze pamiętaj, aby <w>wypinać</w> wtyczkę z kontaktu przed naprawą urządzenia.",
                            "target_lang_translations": {
                                "ru": "Всегда помни, что нужно <w>вынимать</w> вилку из розетки перед ремонтом устройства."
                            }
                        }
                    ],
                    "antonyms": [
                        "wpinać",
                        "zapinać"
                    ],
                    "common_phrases": [
                        "wypinać wtyczkę",
                        "wypinać kabel",
                        "wypinać narty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отсоединять предмет, который был пристёгнут, воткнут или вставлен в гнездо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отсоединять",
                                "target_lang_sense_clarification": "отключать кабель или устройство"
                            },
                            {
                                "target_lang_word": "вынимать",
                                "target_lang_sense_clarification": "доставать вилку из розетки"
                            },
                            {
                                "target_lang_word": "отстёгивать",
                                "target_lang_sense_clarification": "откреплять что-то пристёгнутое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "734ee3f0-bc29-3f1b-bbc1-7a62fe5f59aa",
                    "sense_definition": "Wysuwać nienaturalnie część ciała (zwykle klatkę piersiową lub pośladki) do przodu lub do tyłu, napinając mięśnie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierz stojący na warcie musi stać prosto i dumnie <w>wypinać</w> pierś.",
                            "target_lang_translations": {
                                "ru": "Солдат, стоящий в карауле, должен стоять прямо и гордо <w>выпячивать</w> грудь."
                            }
                        },
                        {
                            "text": "Modelka musiała nienaturalnie <w>wypinać</w> biodra do tego artystycznego zdjęcia.",
                            "target_lang_translations": {
                                "ru": "Модели пришлось неестественно <w>отклячивать</w> бедра для этой художественной фотографии."
                            }
                        }
                    ],
                    "antonyms": [
                        "prostować"
                    ],
                    "common_phrases": [
                        "wypinać pierś",
                        "wypinać brzuch",
                        "wypinać pośladki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выставлять неестественно часть тела (обычно грудь или ягодицы) вперёд или назад, напрягая мышцы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выпячивать",
                                "target_lang_sense_clarification": "выставлять вперёд (грудь, живот)"
                            },
                            {
                                "target_lang_word": "отклячивать",
                                "target_lang_sense_clarification": "выставлять назад (ягодицы)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71a89e53-13a4-3a92-af81-5817a5333dd9",
                    "sense_definition": "Uwalniać się z uwięzi, pasów, wiązań lub zaczepów (używane z zaimkiem zwrotnym).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W razie zagrożenia trzeba umieć szybko <w>wypinać</w> się z uprzęży wspinaczkowej.",
                            "target_lang_translations": {
                                "ru": "В случае опасности нужно уметь быстро <w>отстёгиваться</w> от альпинистской обвязки."
                            }
                        },
                        {
                            "text": "Zacząłem powoli <w>wypinać</w> się z nart, gdy tylko dotarłem na dół stoku.",
                            "target_lang_translations": {
                                "ru": "Я начал медленно <w>отстёгиваться</w> от лыж, как только добрался до низа склона."
                            }
                        }
                    ],
                    "antonyms": [
                        "wpinać się",
                        "zapinać się"
                    ],
                    "common_phrases": [
                        "wypinać się z nart",
                        "wypinać się z uprzęży",
                        "wypinać się z pasów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Освобождаться от привязи, ремней, креплений или зацепов (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отстёгиваться",
                                "target_lang_sense_clarification": "освобождаться от креплений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b38bab5a-57c0-3a6c-a9a9-c82a521ce366",
                    "sense_definition": "Okazywać lekceważenie, ignorować kogoś lub odmawiać udziału w czymś (używane z zaimkiem zwrotnym).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie możesz ciągle <w>wypinać</w> się na swoich przyjaciół, gdy potrzebują twojej pomocy.",
                            "target_lang_translations": {
                                "ru": "Ты не можешь постоянно <w>поворачиваться спиной</w> к своим друзьям, когда им нужна твоя помощь."
                            }
                        },
                        {
                            "text": "On ma irytujący zwyczaj <w>wypinać</w> się na wszelkie ustalone odgórnie zasady.",
                            "target_lang_translations": {
                                "ru": "У него есть раздражающая привычка <w>плевать</w> на любые установленные сверху правила."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wypinać się na kogoś",
                        "wypinać się na zasady",
                        "wypinać się na system"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Проявлять пренебрежение, игнорировать кого-то или отказываться от участия в чём-то (разговорное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плевать",
                                "target_lang_sense_clarification": "проявлять полное безразличие"
                            },
                            {
                                "target_lang_word": "поворачиваться спиной",
                                "target_lang_sense_clarification": "игнорировать или предавать"
                            },
                            {
                                "target_lang_word": "забивать",
                                "target_lang_sense_clarification": "сленг: игнорировать обязанности"
                            }
                        ]
                    }
                }
            ]
        }
    ]

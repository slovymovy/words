{
    "word_family": [
        "pobojowisko",
        "bój",
        "bojowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "05a2a330-aa70-303a-bd79-d816ee6b253d",
                    "sense_definition": "Wydzielony teren lub plac, odpowiednio przygotowany i wyposażony, przeznaczony do rozgrywania zawodów sportowych, gier zespołowych lub treningów.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sport_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj graliśmy w siatkówkę na <w>boisku</w> między blokami.",
                            "target_lang_translations": {
                                "ru": "Вчера мы играли в волейбол на <w>площадке</w> между домами."
                            }
                        },
                        {
                            "text": "W ciągu minuty znika las naturalny o powierzchni równej 36 <w>boiskom</w> do piłki nożnej.",
                            "target_lang_translations": {
                                "ru": "За одну минуту исчезает естественный лес площадью, равной 36 футбольных <w>полей</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "boisko szkolne",
                        "boisko piłkarskie",
                        "wejść na boisko",
                        "zejść z boiska",
                        "połowa boiska"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специально оборудованная территория или площадка, предназначенная для спортивных соревнований, командных игр или тренировок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "площадка",
                                "target_lang_sense_clarification": "спортивная"
                            },
                            {
                                "target_lang_word": "поле",
                                "target_lang_sense_clarification": "футбольное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb9160f1-4519-33a7-9336-f6d0548446b9",
                    "sense_definition": "Teren, na którym toczy się walka zbrojna; w przenośni także miejsce rywalizacji lub sporu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "battle_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierze wykazali się niezwykłym bohaterstwem na <w>boisku</w> walki.",
                            "target_lang_translations": {
                                "ru": "Солдаты проявили необычайный героизм на <w>поле</w> боя."
                            }
                        },
                        {
                            "text": "Debata telewizyjna stała się głównym <w>boiskiem</w> dla kandydatów na prezydenta.",
                            "target_lang_translations": {
                                "ru": "Телевизионные дебаты стали главной <w>ареной</w> для кандидатов в президенты."
                            }
                        }
                    ],
                    "common_phrases": [
                        "boisko walki"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "literary or metaphorical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Территория, на которой ведется вооруженная борьба; в переносном смысле — место соперничества или спора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поле",
                                "target_lang_sense_clarification": "боя, битвы"
                            },
                            {
                                "target_lang_word": "арена",
                                "target_lang_sense_clarification": "перен. место борьбы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f39f259d-01f2-312f-a7f2-b9b3b16c0070",
                    "sense_definition": "Środkowa część stodoły, zwykle z utwardzoną podłogą, służąca dawniej do młócenia zboża lub wjazdu wozem.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "barn_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz zrzucił snopy zboża na <w>boisko</w>, przygotowując się do młócki.",
                            "target_lang_translations": {
                                "ru": "Хозяин сбросил снопы зерна на <w>ток</w>, готовясь к молотьбе."
                            }
                        },
                        {
                            "text": "W starej, drewnianej stodole zachowało się jeszcze gliniane <w>boisko</w>.",
                            "target_lang_translations": {
                                "ru": "В старом деревянном сарае ещё сохранился глиняный <w>ток</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "klepisko",
                        "gumno"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "agricultural/historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Средняя часть овина или сарая, обычно с утрамбованным полом, служившая ранее для молотьбы зерна или въезда телег."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ток",
                                "target_lang_sense_clarification": "место для молотьбы"
                            },
                            {
                                "target_lang_word": "гумно",
                                "target_lang_sense_clarification": "помещение для сжатого хлеба"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
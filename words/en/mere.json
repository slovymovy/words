                {
                    "sense_id": "3d8f9485-995c-3108-8279-6193827a369f",
                    "sense_definition": "Used to emphasize that something is small, insignificant, or nothing more than what is specified.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>mere</w> thought of eating insects makes me feel sick.",
                            "target_lang_translations": {
                                "ru": "От <w>одной лишь</w> мысли о поедании насекомых меня тошнит."
                            }
                        },
                        {
                            "text": "He is a <w>mere</w> child and does not understand the complexity of the situation.",
                            "target_lang_translations": {
                                "ru": "Он <w>всего лишь</w> ребёнок и не понимает сложности ситуации."
                            }
                        },
                        {
                            "text": "It was a <w>mere</w> coincidence that we arrived at the same time.",
                            "target_lang_translations": {
                                "ru": "То, что мы приехали одновременно, было <w>чистой</w> случайностью."
                            }
                        }
                    ],
                    "synonyms": [
                        "only",
                        "simple",
                        "bare"
                    ],
                    "common_phrases": [
                        "a mere child",
                        "the mere thought",
                        "a mere fraction",
                        "a mere formality",
                        "a mere coincidence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для подчеркивания того, что что-то является малым, незначительным или не более чем тем, что указано."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "всего лишь",
                                "target_lang_sense_clarification": "в значении «только», «не более чем»"
                            },
                            {
                                "target_lang_word": "простой",
                                "target_lang_sense_clarification": "обычный, ничем не примечательный"
                            },
                            {
                                "target_lang_word": "один лишь",
                                "target_lang_sense_clarification": "используется для усиления, например, «одна лишь мысль»"
                            },
                            {
                                "target_lang_word": "чистый",
                                "target_lang_sense_clarification": "полный, абсолютный (например, «чистая случайность»)"
                            }
                        ]
                    }
                }

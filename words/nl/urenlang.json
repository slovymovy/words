                    "sense_id": "35aa0cbb-347a-3465-b721-29afdb3c44ac",
                    "sense_definition": "Gedurende een periode van vele uren achter elkaar.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "duration_adv",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben <w>urenlang</w> in de file gestaan voordat we thuis waren.",
                            "target_lang_translations": {
                                "ru": "Мы <w>часами</w> стояли в пробке, прежде чем добрались домой.",
                                "en": "We were stuck in traffic <w>for hours</w> before we got home."
                            }
                        },
                        {
                            "text": "Hij heeft <w>urenlang</w> naar de televisie gekeken zonder iets te zeggen.",
                            "target_lang_translations": {
                                "ru": "Он <w>часами</w> смотрел телевизор, не говоря ни слова.",
                                "en": "He watched television <w>for hours</w> without saying anything."
                            }
                        }
                    ],
                    "synonyms": [
                        "tijdenlang",
                        "langdurig"
                    ],
                    "antonyms": [
                        "kort",
                        "even"
                    ],
                    "common_phrases": [
                        "urenlang wachten",
                        "urenlang praten",
                        "urenlang zoeken"
                    ],
                    "target_lang_definitions": {
                        "ru": "В течение многих часов подряд.",
                        "en": "For a period of many hours consecutively."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "часами",
                                "target_lang_sense_clarification": "наречие, указывающее на длительность действия"
                            },
                            {
                                "target_lang_word": "много часов подряд",
                                "target_lang_sense_clarification": "фраза, подчеркивающая непрерывность"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "for hours",
                                "target_lang_sense_clarification": "indicating a long duration"
                            },
                            {
                                "target_lang_word": "for hours on end",
                                "target_lang_sense_clarification": "emphasizing the continuous nature"
                            }
                        ]
                    }
                }
            ]
        },
        {

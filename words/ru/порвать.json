    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ae45fbfe-8f16-32fc-99e6-89234f1f8656",
                    "sense_definition": "Нарушить целостность предмета, с силой потянув его части в разные стороны или зацепив за что-то острое.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_damage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я случайно <w>порвал</w> свою любимую куртку, когда перелезал через забор.",
                            "target_lang_translations": {
                                "en": "I accidentally <w>tore</w> my favorite jacket when I was climbing over the fence."
                            }
                        },
                        {
                            "text": "Неисправный принтер зажевал и <w>порвал</w> бумагу, поэтому мне пришлось печатать всё заново.",
                            "target_lang_translations": {
                                "en": "The faulty printer chewed up and <w>ripped</w> the paper, so I had to print everything all over again."
                            }
                        }
                    ],
                    "synonyms": [
                        "разорвать"
                    ],
                    "common_phrases": [
                        "порвать одежду",
                        "порвать бумагу",
                        "порвать струну",
                        "порвать на куски"
                    ],
                    "target_lang_definitions": {
                        "en": "To damage an object by pulling it apart or snagging it on something sharp."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tear",
                                "target_lang_sense_clarification": "general term for pulling apart"
                            },
                            {
                                "target_lang_word": "rip",
                                "target_lang_sense_clarification": "often implies a quick or forceful action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed7fedf4-ca94-3e11-bab5-8f0bf061faca",
                    "sense_definition": "Резко и окончательно прекратить отношения, общение или связи с кем-либо или чем-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relationships",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После той крупной ссоры он решил окончательно <w>порвать</w> со своими старыми знакомыми.",
                            "target_lang_translations": {
                                "en": "After that big argument, he decided to finally <w>break</w> with his old acquaintances."
                            }
                        },
                        {
                            "text": "Ей было очень тяжело <w>порвать</w> с прошлым и начать новую жизнь в другом городе.",
                            "target_lang_translations": {
                                "en": "It was very hard for her to <w>break</w> with the past and start a new life in another city."
                            }
                        }
                    ],
                    "synonyms": [
                        "расстаться"
                    ],
                    "common_phrases": [
                        "порвать с прошлым",
                        "порвать отношения",
                        "порвать с другом",
                        "порвать связи"
                    ],
                    "target_lang_definitions": {
                        "en": "To abruptly and permanently end a relationship, communication, or connection."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break off",
                                "target_lang_sense_clarification": "relations or engagement"
                            },
                            {
                                "target_lang_word": "break up",
                                "target_lang_sense_clarification": "with a person"
                            },
                            {
                                "target_lang_word": "sever",
                                "target_lang_sense_clarification": "ties or connections (formal)"
                            },
                            {
                                "target_lang_word": "break",
                                "target_lang_sense_clarification": "with the past or habits"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33064c99-ccb2-368e-8731-28f186fd0872",
                    "sense_definition": "Одержать убедительную победу над соперником или произвести очень сильное впечатление на публику.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_victory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наша футбольная команда пообещала <w>порвать</w> соперников в финальном матче сезона.",
                            "target_lang_translations": {
                                "en": "Our football team promised to <w>crush</w> their opponents in the final match of the season."
                            }
                        },
                        {
                            "text": "Этот молодой комик просто <w>порвал</w> зал своими неожиданными и смешными шутками.",
                            "target_lang_translations": {
                                "en": "This young comedian just <w>brought down</w> the house with his unexpected and funny jokes."
                            }
                        }
                    ],
                    "synonyms": [
                        "уделать"
                    ],
                    "common_phrases": [
                        "порвать зал",
                        "порвать всех",
                        "порвать танцпол"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To defeat an opponent convincingly or to entertain an audience successfully (slang)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crush",
                                "target_lang_sense_clarification": "defeat an opponent completely"
                            },
                            {
                                "target_lang_word": "destroy",
                                "target_lang_sense_clarification": "defeat overwhelmingly"
                            },
                            {
                                "target_lang_word": "bring down the house",
                                "target_lang_sense_clarification": "impress an audience greatly"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6726babe-91ad-317b-83a7-752e8b1ea0da",
                    "sense_definition": "The public system used for collecting, transporting, and delivering letters and packages.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "postal_communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I decided to send the contract by <w>mail</w> because it is safer.",
                            "target_lang_translations": {
                                "ru": "Я решил отправить договор по <w>почте</w>, потому что так безопаснее."
                            }
                        },
                        {
                            "text": "The <w>mail</w> is usually slower during the holiday season.",
                            "target_lang_translations": {
                                "ru": "<w>Почта</w> обычно работает медленнее в праздничный сезон."
                            }
                        }
                    ],
                    "synonyms": [
                        "post"
                    ],
                    "common_phrases": [
                        "by mail",
                        "in the mail",
                        "regular mail"
                    ],
                    "target_lang_definitions": {
                        "ru": "Государственная система сбора, транспортировки и доставки писем и посылок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почта",
                                "target_lang_sense_clarification": "система доставки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4efb4d8f-f0cb-3541-8078-c61c001c40ae",
                    "sense_definition": "The letters, packages, and other items delivered by the postal service.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "postal_communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There was a lot of <w>mail</w> in the box when we returned home.",
                            "target_lang_translations": {
                                "ru": "В ящике было много <w>почты</w>, когда мы вернулись домой."
                            }
                        },
                        {
                            "text": "My father likes to open the <w>mail</w> while he drinks his coffee.",
                            "target_lang_translations": {
                                "ru": "Мой отец любит открывать <w>почту</w>, пока пьет кофе."
                            }
                        }
                    ],
                    "synonyms": [
                        "post",
                        "snail mail"
                    ],
                    "antonyms": [
                        "email"
                    ],
                    "common_phrases": [
                        "junk mail",
                        "get the mail",
                        "fan mail"
                    ],
                    "target_lang_definitions": {
                        "ru": "Письма, посылки и другие отправления, доставляемые почтовой службой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почта",
                                "target_lang_sense_clarification": "письма и посылки"
                            },
                            {
                                "target_lang_word": "корреспонденция",
                                "target_lang_sense_clarification": "официальная переписка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27b31d62-1e3a-33b5-9202-5878ca664005",
                    "sense_definition": "Messages sent or received electronically over a computer network; email.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "electronic_communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to check my <w>mail</w> before I go to the meeting.",
                            "target_lang_translations": {
                                "ru": "Мне нужно проверить <w>почту</w> перед тем, как идти на встречу."
                            }
                        },
                        {
                            "text": "You have new <w>mail</w> in your inbox.",
                            "target_lang_translations": {
                                "ru": "У вас новая <w>почта</w> во входящих."
                            }
                        }
                    ],
                    "synonyms": [
                        "email"
                    ],
                    "common_phrases": [
                        "voice mail",
                        "check mail",
                        "read mail"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сообщения, отправляемые или получаемые в электронном виде через компьютерную сеть; электронная почта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почта",
                                "target_lang_sense_clarification": "электронные сообщения"
                            },
                            {
                                "target_lang_word": "электронная почта",
                                "target_lang_sense_clarification": "полное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07427cb5-8ae7-3f0d-81a0-82d57a586416",
                    "sense_definition": "Flexible armour made of small metal rings linked together, used historically for protection in battle.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "armour",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The knight wore a heavy coat of <w>mail</w> under his tunic.",
                            "target_lang_translations": {
                                "ru": "Рыцарь носил тяжелую <w>кольчугу</w> под туникой."
                            }
                        },
                        {
                            "text": "Ancient warriors often used <w>mail</w> to protect themselves from sword strikes.",
                            "target_lang_translations": {
                                "ru": "Древние воины часто использовали <w>кольчугу</w> для защиты от ударов мечом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "coat of mail",
                        "chain mail"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Гибкий доспех из скрепленных между собой металлических колец, использовавшийся исторически для защиты в бою."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кольчуга",
                                "target_lang_sense_clarification": "доспех"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3cce7c79-28ff-331f-b72e-fdceff23a6b1",
                    "sense_definition": "To send a letter, package, or document using the postal system.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "postal_communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>mail</w> this letter to your grandmother as soon as possible.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>отправь</w> это письмо бабушке как можно скорее."
                            }
                        },
                        {
                            "text": "I will <w>mail</w> the package tomorrow morning on my way to work.",
                            "target_lang_translations": {
                                "ru": "Я <w>отправлю</w> посылку завтра утром по дороге на работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "post"
                    ],
                    "common_phrases": [
                        "mail a letter",
                        "mail a package"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправлять письмо, посылку или документ, используя почтовую систему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отправлять по почте",
                                "target_lang_sense_clarification": "посылать письма или посылки"
                            },
                            {
                                "target_lang_word": "посылать",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e2b9e64-1563-3267-8f14-87877c404abf",
                    "sense_definition": "To send a message or file to someone electronically.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "electronic_communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you <w>mail</w> me the spreadsheet by the end of the day?",
                            "target_lang_translations": {
                                "ru": "Можешь <w>прислать</w> мне таблицу до конца дня?"
                            }
                        },
                        {
                            "text": "He decided to <w>mail</w> the photos to all his friends.",
                            "target_lang_translations": {
                                "ru": "Он решил <w>отправить</w> фотографии всем своим друзьям."
                            }
                        }
                    ],
                    "synonyms": [
                        "email"
                    ],
                    "common_phrases": [
                        "mail a file",
                        "mail a copy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправлять сообщение или файл кому-либо в электронном виде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отправлять",
                                "target_lang_sense_clarification": "по электронной почте"
                            },
                            {
                                "target_lang_word": "посылать",
                                "target_lang_sense_clarification": "электронное письмо"
                            }
                        ]
                    }
                }
            ]
        }
    ]

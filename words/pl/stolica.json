    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "95c4250f-27a8-3c43-a2c2-efebbbf46a6c",
                    "sense_definition": "Główne miasto państwa, w którym zazwyczaj znajdują się siedziby najwyższych władz centralnych",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "capital_city",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Warszawa to obecna <w>stolica</w> Polski, położona nad Wisłą.",
                            "target_lang_translations": {
                                "ru": "Варшава — это нынешняя <w>столица</w> Польши, расположенная на Висле."
                            }
                        },
                        {
                            "text": "W przyszłym tygodniu jadę do <w>stolicy</w>, aby załatwić ważne sprawy w urzędzie.",
                            "target_lang_translations": {
                                "ru": "На следующей неделе я еду в <w>столицу</w>, чтобы уладить важные дела в ведомстве."
                            }
                        }
                    ],
                    "synonyms": [
                        "miasto stołeczne"
                    ],
                    "antonyms": [
                        "prowincja"
                    ],
                    "common_phrases": [
                        "stolica Polski",
                        "europejska stolica",
                        "zwiedzać stolicę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Главный город государства, в котором обычно находятся высшие органы центральной власти"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столица",
                                "target_lang_sense_clarification": "государства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4558d184-0cb1-318d-a375-df883b2ac3c5",
                    "sense_definition": "Główne miasto regionu, województwa lub innej jednostki administracyjnej",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "capital_city",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Poznań jest <w>stolicą</w> województwa wielkopolskiego i ważnym ośrodkiem handlowym.",
                            "target_lang_translations": {
                                "ru": "Познань является <w>столицей</w> Великопольского воеводства и важным торговым центром."
                            }
                        },
                        {
                            "text": "Mieszkańcy małych miasteczek często dojeżdżają do pracy w <w>stolicy</w> regionu.",
                            "target_lang_translations": {
                                "ru": "Жители маленьких городков часто ездят на работу в <w>столицу</w> региона."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stolica województwa",
                        "stolica regionu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Главный город региона, воеводства или другой административной единицы"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столица",
                                "target_lang_sense_clarification": "региона"
                            },
                            {
                                "target_lang_word": "административный центр",
                                "target_lang_sense_clarification": "области или края"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3cdbf037-96b6-39c0-9a83-d9906ca0eb3a",
                    "sense_definition": "Miasto lub miejsce uznawane za najważniejsze w określonej dziedzinie życia społecznego lub kulturalnego",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "capital_city",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Paryż jest powszechnie uważany za światową <w>stolicę</w> mody.",
                            "target_lang_translations": {
                                "ru": "Париж общепризнанно считается мировой <w>столицей</w> моды."
                            }
                        },
                        {
                            "text": "Zakopane bywa nazywane zimową <w>stolicą</w> Polski ze względu na popularność wśród narciarzy.",
                            "target_lang_translations": {
                                "ru": "Закопане иногда называют зимней <w>столицей</w> Польши из-за популярности среди лыжников."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stolica mody",
                        "stolica kultury",
                        "zimowa stolica"
                    ],
                    "target_lang_definitions": {
                        "ru": "Город или место, признанное важнейшим в определенной сфере общественной или культурной жизни"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столица",
                                "target_lang_sense_clarification": "центр деятельности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a55d02d-5fcf-391a-849c-e1ab7646ca91",
                    "sense_definition": "Ozdobne siedzenie dla władcy, symbolizujące jego godność i władzę",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "seat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary król w milczeniu zasiadł na swej złotej <w>stolicy</w>.",
                            "target_lang_translations": {
                                "ru": "Старый король молча воссел на своем золотом <w>престоле</w>."
                            }
                        },
                        {
                            "text": "Przed oblicze władcy siedzącego na <w>stolicy</w> wprowadzono zagranicznych posłów.",
                            "target_lang_translations": {
                                "ru": "Пред лицо правителя, сидящего на <w>троне</w>, ввели иностранных послов."
                            }
                        }
                    ],
                    "synonyms": [
                        "tron"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Украшенное сиденье для правителя, символизирующее его достоинство и власть"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "престол",
                                "target_lang_sense_clarification": "трон"
                            },
                            {
                                "target_lang_word": "трон",
                                "target_lang_sense_clarification": "сиденье монарха"

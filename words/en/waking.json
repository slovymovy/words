                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бодрствующий",
                                "target_lang_sense_clarification": "находящийся в сознании, не спящий"
                            },
                            {
                                "target_lang_word": "наяву",
                                "target_lang_sense_clarification": "в реальности, не во сне (о кошмарах, снах)"

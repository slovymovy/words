    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4234d846-19e6-301e-b194-01eaf3542703",
                    "sense_definition": "Przestać być właścicielem czegoś, nie móc czegoś znaleźć lub zostać pozbawionym jakiejś cechy, np. nadziei lub przytomności.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj w zatłoczonym autobusie niestety <w>straciłem</w> portfel z dokumentami.",
                            "target_lang_translations": {
                                "ru": "Вчера в переполненном автобусе я, к сожалению, <w>потерял</w> кошелек с документами."
                            }
                        },
                        {
                            "text": "Po uderzeniu w głowę zawodnik na chwilę <w>stracił</w> przytomność.",
                            "target_lang_translations": {
                                "ru": "После удара по голове спортсмен на мгновение <w>потерял</w> сознание."
                            }
                        }
                    ],
                    "synonyms": [
                        "zgubić"
                    ],
                    "antonyms": [
                        "zyskać"
                    ],
                    "common_phrases": [
                        "stracić głowę",
                        "stracić pracę",
                        "stracić nadzieję",
                        "stracić pieniądze",
                        "stracić panowanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перестать владеть чем-либо, не мочь найти что-либо или лишиться какого-либо качества, например, надежды или сознания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потерять",
                                "target_lang_sense_clarification": "перестать иметь или не знать местонахождение"
                            },
                            {
                                "target_lang_word": "лишиться",
                                "target_lang_sense_clarification": "утратить что-либо (часто абстрактное)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3dc5bfa-caa3-3b02-96c6-d424ca717ec7",
                    "sense_definition": "Zmarnować zasoby, takie jak czas czy pieniądze, lub nie wykorzystać nadarzającej się okazji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie możemy <w>stracić</w> tej niepowtarzalnej okazji na ciekawy wyjazd.",
                            "target_lang_translations": {
                                "ru": "Мы не можем <w>упустить</w> эту уникальную возможность интересной поездки."
                            }
                        },
                        {
                            "text": "<w>Straciłem</w> mnóstwo czasu na szukanie wolnego miejsca parkingowego w centrum.",
                            "target_lang_translations": {
                                "ru": "Я <w>потерял</w> кучу времени на поиск свободного парковочного места в центре."
                            }
                        }
                    ],
                    "antonyms": [
                        "skorzystać",
                        "zyskać"
                    ],
                    "common_phrases": [
                        "stracić czas",
                        "stracić okazję",
                        "stracić szansę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Потратить впустую ресурсы, такие как время или деньги, или не воспользоваться возможностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потерять",
                                "target_lang_sense_clarification": "время или деньги"
                            },
                            {
                                "target_lang_word": "упустить",
                                "target_lang_sense_clarification": "шанс или возможность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "acd30c48-330a-3707-9e2f-2303e3261349",
                    "sense_definition": "Zmniejszyć masę ciała lub pozbyć się fizycznej części organizmu, na przykład zębów lub włosów.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzięki diecie i regularnym ćwiczeniom <w>straciłem</w> pięć kilogramów.",
                            "target_lang_translations": {
                                "ru": "Благодаря диете и регулярным упражнениям я <w>сбросил</w> пять килограммов."
                            }
                        },
                        {
                            "text": "Podczas bójki napastnik <w>stracił</w> dwa przednie zęby.",
                            "target_lang_translations": {
                                "ru": "Во время драки нападавший <w>потерял</w> два передних зуба."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stracić na wadze",
                        "stracić wzrok",
                        "stracić głos",
                        "stracić zęby"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снизить вес или лишиться части тела, например, зубов или волос."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потерять",
                                "target_lang_sense_clarification": "зубы, волосы, зрение"
                            },
                            {
                                "target_lang_word": "сбросить",
                                "target_lang_sense_clarification": "вес"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc1efa84-69b8-3948-807a-c5ac66b0db1c",
                    "sense_definition": "Przestać mieć z kimś kontakt, zazwyczaj w wyniku śmierci tej osoby.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W wypadku samochodowym on niestety <w>stracił</w> całą swoją rodzinę.",
                            "target_lang_translations": {
                                "ru": "В автокатастрофе он, к сожалению, <w>потерял</w> всю свою семью."
                            }
                        },
                        {
                            "text": "Bardzo młodo <w>straciła</w> męża i musiała sama wychowywać dzieci.",
                            "target_lang_translations": {
                                "ru": "Она очень рано <w>потеряла</w> мужа и была вынуждена одна воспитывать детей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stracić przyjaciela",
                        "stracić bliskich",
                        "stracić rodziców"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишиться кого-либо (обычно родственника или друга) в результате смерти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потерять",
                                "target_lang_sense_clarification": "близкого человека"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89a05936-a903-3992-96c6-b19dc7398f31",
                    "sense_definition": "Pozbawić kogoś życia w wyniku wykonania wyroku śmierci.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zdrajca został osądzony przez trybunał i publicznie <w>stracony</w> na rynku.",
                            "target_lang_translations": {
                                "ru": "Предатель был осужден трибуналом и публично <w>казнен</w> на рыночной площади."
                            }
                        },
                        {
                            "text": "Władca kazał <w>stracić</w> wszystkich spiskowców bez okazywania litości.",
                            "target_lang_translations": {
                                "ru": "Правитель приказал <w>казнить</w> всех заговорщиков без жалости."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zostać straconym",
                        "kazać stracić"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "used mainly in historical contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишить кого-либо жизни во исполнение смертного приговора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "казнить",
                                "target_lang_sense_clarification": "привести в исполнение смертный приговор"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
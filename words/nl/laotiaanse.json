            "senses": [
                {
                    "sense_id": "5f10fe10-df35-32cf-be95-a6785b1e2176",
                    "sense_definition": "Verbogen vorm van de stellende trap van <w>Laotiaans</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>Laotiaanse</w> keuken staat bekend om het gebruik van verse kruiden en kleefrijst.",
                            "target_lang_translations": {
                                "ru": "<w>Лаосская</w> кухня известна использованием свежих трав и клейкого риса."
                            }
                        },
                        {
                            "text": "Tijdens zijn reis door Azië leerde hij de <w>Laotiaanse</w> cultuur goed kennen.",
                            "target_lang_translations": {
                                "ru": "Во время своего путешествия по Азии он хорошо узнал <w>лаосскую</w> культуру."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de Laotiaanse keuken",
                        "de Laotiaanse bevolking",
                        "de Laotiaanse taal"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "inflected form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к Лаосу, его народу или культуре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лаосский",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9bcc02fa-3cbd-3843-b80a-e6b453d939b6",
                    "sense_definition": "Een vrouwelijke inwoner van Laos, of een vrouw afkomstig uit Laos.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn buurvrouw is een <w>Laotiaanse</w> die al tien jaar in Nederland woont.",
                            "target_lang_translations": {
                                "ru": "Моя соседка — <w>лаосска</w>, которая живет в Нидерландах уже десять лет."
                            }
                        },
                        {
                            "text": "De <w>Laotiaanse</w> vertelde ons enthousiast over de tradities van haar geboorteland.",
                            "target_lang_translations": {
                                "ru": "<w>Лаосска</w> с энтузиазмом рассказала нам о традициях своей родины."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een jonge Laotiaanse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жительница или уроженка Лаоса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лаосска",
                                "target_lang_sense_clarification": "жительница Лаоса"
                            }
                        ]
                    }
                }
            ]
        }
    ]

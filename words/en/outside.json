                    ],
                    "synonyms": [
                        "the lower 48"
                    ],
                    "antonyms": [
                        "inside",
                        "inside"
                    ],
                    "common_phrases": [
                        "outside the scope",
                        "outside the law"
                    ],
                    "target_lang_definitions": {

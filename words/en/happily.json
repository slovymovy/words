{
    "word_family": [
        "happy",
        "happiness",
        "unhappy",
        "unhappiness"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "de95d93e-c960-3e47-a541-aaa00b2e88ea",
                    "sense_definition": "In a happy or cheerful manner; with feelings of joy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children played <w>happily</w> in the garden all afternoon.",
                            "target_lang_translations": {
                                "ru": "Дети <w>весело</w> играли в саду весь день."
                            }
                        },
                        {
                            "text": "They lived <w>happily</w> ever after in the small cottage.",
                            "target_lang_translations": {
                                "ru": "Они жили долго и <w>счастливо</w> в маленьком домике."
                            }
                        }
                    ],
                    "synonyms": [
                        "cheerfully",
                        "joyfully",
                        "merrily",
                        "joyously",
                        "gaily"
                    ],
                    "antonyms": [
                        "sadly"
                    ],
                    "common_phrases": [
                        "happily married",
                        "lived happily ever after",
                        "smiling happily"
                    ],
                    "target_lang_definitions": {
                        "ru": "Весело, с радостью; испытывая чувство счастья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сча́стливо",
                                "target_lang_sense_clarification": "о жизни или состоянии"
                            },
                            {
                                "target_lang_word": "ра́достно",
                                "target_lang_sense_clarification": "весело, с оживлением"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ad5cb87-5a0a-3781-a61e-31639014727b",
                    "sense_definition": "In a willing way; with good will or enthusiasm.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "willingness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will <w>happily</w> help you move the furniture tomorrow.",
                            "target_lang_translations": {
                                "ru": "Я <w>с радостью</w> помогу тебе переставить мебель завтра."
                            }
                        },
                        {
                            "text": "She <w>happily</w> accepted the invitation to the wedding.",
                            "target_lang_translations": {
                                "ru": "Она <w>с удовольствием</w> приняла приглашение на свадьбу."
                            }
                        }
                    ],
                    "synonyms": [
                        "gladly"
                    ],
                    "common_phrases": [
                        "happily accept",
                        "happily agree",
                        "would happily"
                    ],
                    "target_lang_definitions": {
                        "ru": "Охотно; с желанием или энтузиазмом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с удово́льствием",
                                "target_lang_sense_clarification": "с желанием"
                            },
                            {
                                "target_lang_word": "с ра́достью",
                                "target_lang_sense_clarification": "охотно"
                            },
                            {
                                "target_lang_word": "охо́тно",
                                "target_lang_sense_clarification": "без принуждения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81054a6c-9a4e-3d16-8f60-660610a36c31",
                    "sense_definition": "By good luck or a fortunate chance.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fortune",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Happily</w>, the rain stopped just before the picnic started.",
                            "target_lang_translations": {
                                "ru": "<w>К счастью</w>, дождь прекратился как раз перед началом пикника."
                            }
                        },
                        {
                            "text": "The missing documents were <w>happily</w> found in a drawer.",
                            "target_lang_translations": {
                                "ru": "Пропавшие документы, <w>к счастью</w>, нашлись в ящике стола."
                            }
                        }
                    ],
                    "synonyms": [
                        "blessedly"
                    ],
                    "antonyms": [
                        "sadly"
                    ],
                    "common_phrases": [
                        "quite happily"
                    ],
                    "target_lang_definitions": {
                        "ru": "По счастливой случайности или удаче."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "к сча́стью",
                                "target_lang_sense_clarification": "удачно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5306604-7dff-3488-9c86-f35980b59732",
                    "sense_definition": "By chance or accident; perhaps.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "chance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If <w>happily</w> we meet again, we shall share our stories.",
                            "target_lang_translations": {
                                "ru": "Если <w>волею судеб</w> мы встретимся снова, мы поделимся нашими историями."
                            }
                        },
                        {
                            "text": "He wondered if <w>happily</w> she might arrive before noon.",
                            "target_lang_translations": {
                                "ru": "Он гадал, не приедет ли она <w>случайно</w> до полудня."
                            }
                        }
                    ],
                    "synonyms": [
                        "haply"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Modern usage typically employs 'haply' or 'perhaps' for this meaning."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Случайно или, возможно (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "во́лею су́деб",
                                "target_lang_sense_clarification": "поэтическое/устаревшее"
                            },
                            {
                                "target_lang_word": "случа́йно",
                                "target_lang_sense_clarification": "по совпадению"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
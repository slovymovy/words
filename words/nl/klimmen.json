            "pos": "verb",
            "senses": [
                {
                    "sense_id": "72b1b823-c452-3752-8f6a-e571eda1f824",
                    "sense_definition": "Zich omhoog of over een obstakel bewegen, vaak met gebruik van handen en voeten.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_upwards",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kat <w>klom</w> snel in de hoge boom.",
                            "target_lang_translations": {
                                "ru": "Кот быстро <w>залез</w> на высокое дерево.",
                                "en": "The cat quickly <w>climbed</w> the tall tree."
                            }
                        },
                        {
                            "text": "Wij <w>klommen</w> over het hek om de bal te pakken.",
                            "target_lang_translations": {
                                "ru": "Мы <w>перелезли</w> через забор, чтобы достать мяч.",
                                "en": "We <w>climbed</w> over the fence to get the ball."
                            }
                        },
                        {
                            "text": "Hij kan heel goed in touwen <w>klimmen</w>.",
                            "target_lang_translations": {
                                "ru": "Он умеет очень хорошо <w>лазить</w> по канатам.",
                                "en": "He is very good at <w>climbing</w> ropes."
                            }
                        }
                    ],
                    "synonyms": [
                        "stijgen",
                        "groeien"
                    ],
                    "antonyms": [
                        "dalen",
                        "afdalen"
                    ],
                    "common_phrases": [
                        "in een boom klimmen",
                        "op een ladder klimmen",
                        "over een hek klimmen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаться вверх или через препятствие, часто используя руки и ноги.",
                        "en": "To move upwards or over an obstacle, often using hands and feet."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лезть",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "взбираться",
                                "target_lang_sense_clarification": "подниматься наверх"
                            },
                            {
                                "target_lang_word": "карабкаться",
                                "target_lang_sense_clarification": "лезть с трудом"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "climb",
                                "target_lang_sense_clarification": "general movement upwards"
                            },
                            {
                                "target_lang_word": "scale",
                                "target_lang_sense_clarification": "to climb over something high like a fence or wall"
                            },
                            {
                                "target_lang_word": "clamber",
                                "target_lang_sense_clarification": "to climb with difficulty using hands and feet"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "775ff92e-1c9a-349b-a48f-8d994c48d7a7",
                    "sense_definition": "In hoogte of niveau toenemen; omhooggaan.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_upwards",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het vliegtuig begon steil te <w>klimmen</w> na het opstijgen.",

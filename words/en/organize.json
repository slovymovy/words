{
    "word_family": [
        "organization",
        "organizer",
        "reorganization",
        "reorganize",
        "disorganize",
        "organic",
        "organism"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cefa6e29-378c-395a-ae8e-bceeff502f75",
                    "sense_definition": "To arrange objects, information, or tasks in a logical order or system so they are easy to find or use.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "arrange_plan",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>organize</w> my desk because I cannot find my pens.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>привести в порядок</w> свой стол, так как я не могу найти свои ручки."
                            }
                        },
                        {
                            "text": "She helped <w>organize</w> the books on the shelf alphabetically.",
                            "target_lang_translations": {
                                "ru": "Она помогла <w>расставить</w> книги на полке в алфавитном порядке."
                            }
                        }
                    ],
                    "synonyms": [
                        "arrange",
                        "order",
                        "sort",
                        "classify",
                        "tidy",
                        "systematize",
                        "categorize"
                    ],
                    "antonyms": [
                        "disarrange",
                        "disorganize",
                        "scatter",
                        "mix",
                        "confuse"
                    ],
                    "common_phrases": [
                        "organize files",
                        "organize data",
                        "organize things",
                        "well organized"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить объекты, информацию или задачи в логический порядок или систему для удобства использования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "упорядочивать",
                                "target_lang_sense_clarification": "вносить порядок в вещи или дела"
                            },
                            {
                                "target_lang_word": "систематизировать",
                                "target_lang_sense_clarification": "классифицировать данные или информацию"
                            },
                            {
                                "target_lang_word": "организовывать",
                                "target_lang_sense_clarification": "налаживать пространство или рабочий процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b732a84c-15a3-3537-b4ce-da7963d74fe9",
                    "sense_definition": "To make the necessary arrangements and preparations for an event, activity, or group.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "arrange_plan",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>organize</w> a meeting to discuss the new project.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>организовать</w> встречу, чтобы обсудить новый проект."
                            }
                        },
                        {
                            "text": "They hired a professional to <w>organize</w> their wedding ceremony.",
                            "target_lang_translations": {
                                "ru": "Они наняли профессионала, чтобы <w>организовать</w> их свадебную церемонию."
                            }
                        }
                    ],
                    "synonyms": [
                        "coordinate",
                        "plan",
                        "arrange",
                        "manage"
                    ],
                    "antonyms": [
                        "disorganize"
                    ],
                    "common_phrases": [
                        "organize a meeting",
                        "organize an event",
                        "organize a party",
                        "organize a trip"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать необходимые приготовления и планировать проведение мероприятия или деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "организовывать",
                                "target_lang_sense_clarification": "подготавливать и проводить мероприятие"
                            },
                            {
                                "target_lang_word": "устраивать",
                                "target_lang_sense_clarification": "проводить праздник, встречу или вечеринку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25030a35-a5f7-38ad-94f7-06d680df7e74",
                    "sense_definition": "To form a group, such as a trade union, to act together for shared interests or protection.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The factory workers decided to <w>organize</w> to demand better wages.",
                            "target_lang_translations": {
                                "ru": "Рабочие фабрики решили <w>объединиться</w>, чтобы потребовать повышения зарплаты."
                            }
                        },
                        {
                            "text": "It can be difficult to <w>organize</w> a union in this industry.",
                            "target_lang_translations": {
                                "ru": "В этой отрасли бывает трудно <w>создать</w> профсоюз."
                            }
                        }
                    ],
                    "synonyms": [
                        "group",
                        "form"
                    ],
                    "antonyms": [
                        "disperse"
                    ],
                    "common_phrases": [
                        "organize a union",
                        "workers organize",
                        "right to organize"
                    ],
                    "target_lang_definitions": {
                        "ru": "Формировать группу (например, профсоюз) или объединяться для защиты общих интересов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объединяться",
                                "target_lang_sense_clarification": "сплачиваться в группу для совместных действий"
                            },
                            {
                                "target_lang_word": "создавать",
                                "target_lang_sense_clarification": "основывать организацию или союз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cb2ba42-b934-34b1-b558-a89716f703c4",
                    "sense_definition": "To give a living structure or organic form to something, often used in biology.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the early stages of growth, cells <w>organize</w> into specific tissues.",
                            "target_lang_translations": {
                                "ru": "На ранних стадиях роста клетки <w>организуются</w> в специфические ткани."
                            }
                        },
                        {
                            "text": "Scientists study how matter is <w>organized</w> within living organisms.",
                            "target_lang_translations": {
                                "ru": "Ученые изучают, как материя <w>организована</w> внутри живых организмов."
                            }
                        }
                    ],
                    "synonyms": [
                        "form"
                    ],
                    "common_phrases": [
                        "organized matter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Придавать живую структуру или органическую форму чему-либо (часто в биологии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "организовываться",
                                "target_lang_sense_clarification": "выстраиваться в определенную структуру"
                            },
                            {
                                "target_lang_word": "формироваться",
                                "target_lang_sense_clarification": "образовывать ткани или органы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
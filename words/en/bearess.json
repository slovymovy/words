    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "96a845cb-8630-3c0e-a33c-827492aa3905",
                    "sense_definition": "A female bear.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hunter observed the <w>bearess</w> guiding her cubs through the forest.",
                            "target_lang_translations": {
                                "ru": "Охотник наблюдал, как <w>медведица</w> вела своих медвежат через лес."
                            }
                        },
                        {
                            "text": "In the old fable, the <w>bearess</w> was described as fierce and protective.",
                            "target_lang_translations": {
                                "ru": "В старой басне <w>медведица</w> описывалась как свирепая и заботливая."
                            }
                        }
                    ],
                    "synonyms": [
                        "she-bear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самка медведя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "медведица",
                                "target_lang_sense_clarification": "самка животного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d518d1dd-42b3-3ef1-aafe-2825d5fd0ee7",
                    "sense_definition": "A female person who carries or holds something, particularly in a ceremonial role.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His young niece served as the ring <w>bearess</w> at the wedding ceremony.",
                            "target_lang_translations": {
                                "ru": "Его юная племянница выступила в роли <w>подательницы</w> колец на свадебной церемонии."
                            }
                        },
                        {
                            "text": "She was the only <w>bearess</w> among the group of porters carrying supplies.",
                            "target_lang_translations": {
                                "ru": "Она была единственной <w>носильщицей</w> в группе грузчиков, переносящих припасы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ring bearess"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, которая несет или держит что-либо, часто в церемониальной роли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "носильщица",
                                "target_lang_sense_clarification": "женщина, переносящая груз"
                            },
                            {
                                "target_lang_word": "подательница",
                                "target_lang_sense_clarification": "устар., церемониальная роль"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    ],
                    "synonyms": [
                        "накидать",
                        "набросать",
                        "побросать"
                    ],
                    "common_phrases": [
                        "покидать вещи",
                        "покидать в кучу",
                        "покидать в сумку"
                    ],
                    "traits": [
                        {

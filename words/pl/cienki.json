    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "972c8c69-9a63-3b33-af8d-1b05b1f02d88",
                    "sense_definition": "Mający małą odległość między przeciwległymi powierzchniami lub małą średnicę; przeciwieństwo grubego.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "wymiary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta ściana jest tak <w>cienka</w>, że słyszę każdą rozmowę sąsiadów.",
                            "target_lang_translations": {
                                "ru": "Эта стена такая <w>тонкая</w>, что я слышу каждый разговор соседей."
                            }
                        },
                        {
                            "text": "Proszę pokroić ser na bardzo <w>cienkie</w> plasterki.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, порежьте сыр на очень <w>тонкие</w> ломтики."
                            }
                        }
                    ],
                    "synonyms": [
                        "wąski"
                    ],
                    "antonyms": [
                        "gruby"
                    ],
                    "common_phrases": [
                        "cienki plaster",
                        "cienka linia",
                        "cienki lód",
                        "cienki papier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий небольшое расстояние между противоположными поверхностями или малый диаметр; противоположность толстому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тонкий",
                                "target_lang_sense_clarification": "о предметах, слоях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f74a090b-1563-3c0b-82a7-2767dc227a31",
                    "sense_definition": "O ubraniu lub materiale: lekki, przewiewny, niezapewniający dużo ciepła.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "odzież",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na zewnątrz jest zimno, a ty założyłeś tylko <w>cienki</w> płaszcz.",
                            "target_lang_translations": {
                                "ru": "На улице холодно, а ты надел только <w>тонкое</w> пальто."
                            }
                        },
                        {
                            "text": "Latem lubię nosić <w>cienkie</w> i przewiewne koszule.",
                            "target_lang_translations": {
                                "ru": "Летом я люблю носить <w>тонкие</w> и дышащие рубашки."
                            }
                        }
                    ],
                    "synonyms": [
                        "lekki"
                    ],
                    "antonyms": [
                        "gruby"
                    ],
                    "common_phrases": [
                        "cienki materiał",
                        "cienka kurtka",
                        "cienki sweter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Об одежде или материале: легкий, не плотный, не обеспечивающий много тепла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тонкий",
                                "target_lang_sense_clarification": "об одежде"
                            },
                            {
                                "target_lang_word": "лёгкий",
                                "target_lang_sense_clarification": "не тёплый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd13cdf5-91dd-3273-811f-bfa14d839784",
                    "sense_definition": "O napojach lub potrawach: wodnisty, mało esencjonalny, słaby w smaku.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "konsystencja",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta zupa jest strasznie <w>cienka</w>, smakuje jak sama woda.",
                            "target_lang_translations": {
                                "ru": "Этот суп ужасно <w>жидкий</w>, на вкус как одна вода."
                            }
                        },
                        {
                            "text": "Nie lubię pić <w>cienkiej</w> kawy, wolę mocne espresso.",
                            "target_lang_translations": {
                                "ru": "Я не люблю пить <w>слабый</w> кофе, предпочитаю крепкий эспрессо."
                            }
                        }
                    ],
                    "synonyms": [
                        "słaby",
                        "płaski"
                    ],
                    "antonyms": [
                        "esencjonalny",
                        "gruby"
                    ],
                    "common_phrases": [
                        "cienka herbata",
                        "cienki sos",
                        "cienki barszcz"
                    ],
                    "target_lang_definitions": {
                        "ru": "О напитках или блюдах: водянистый, ненасыщенный, слабый на вкус."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жидкий",
                                "target_lang_sense_clarification": "о супе, соусе"
                            },
                            {
                                "target_lang_word": "слабый",
                                "target_lang_sense_clarification": "о чае, кофе"
                            },
                            {
                                "target_lang_word": "ненасыщенный",
                                "target_lang_sense_clarification": "о вкусе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f657a63-ae9d-3274-a706-dd06bba91796",
                    "sense_definition": "O głosie: wysoki, piskliwy i mało przyjemny dla ucha.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "dźwięk",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko odezwało się cichym i <w>cienkim</w> głosikiem.",
                            "target_lang_translations": {
                                "ru": "Ребёнок ответил тихим и <w>тонким</w> голоском."
                            }
                        },
                        {
                            "text": "Mężczyzna o potężnej budowie zaskoczył nas swoim <w>cienkim</w> głosem.",
                            "target_lang_translations": {
                                "ru": "Мужчина мощного телосложения удивил нас своим <w>тонким</w> голосом."
                            }
                        }
                    ],
                    "synonyms": [
                        "płaski"
                    ],
                    "antonyms": [
                        "gruby"
                    ],
                    "common_phrases": [
                        "cienki głosik",
                        "cienki pisk",
                        "śpiewać cienko"
                    ],
                    "target_lang_definitions": {
                        "ru": "О голосе: высокий, писклявый и малоприятный для слуха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тонкий",
                                "target_lang_sense_clarification": "высокий голос"
                            },
                            {
                                "target_lang_word": "писклявый",
                                "target_lang_sense_clarification": "неприятный для слуха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16149246-286a-363f-a060-eb97d95e182b",
                    "sense_definition": "Słaby, nieudolny, niemający talentu lub niskiej jakości.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "jakość",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten nowy film akcji był naprawdę <w>cienki</w>, szkoda czasu.",
                            "target_lang_translations": {
                                "ru": "Этот новый боевик был действительно <w>слабый</w>, жаль времени."
                            }
                        },
                        {
                            "text": "Nie bierz go do swojej drużyny, on jest <w>cienki</w> w piłkę nożną.",
                            "target_lang_translations": {
                                "ru": "Не бери его в свою команду, он <w>слаб</w> в футболе."
                            }
                        }
                    ],
                    "synonyms": [
                        "słaby"
                    ],
                    "common_phrases": [
                        "cienki zawodnik",
                        "cienki film",
                        "cienki bolek"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слабый, неумелый, бесталанный или низкого качества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слабый",
                                "target_lang_sense_clarification": "о навыках или качестве"
                            },
                            {
                                "target_lang_word": "плохой",
                                "target_lang_sense_clarification": "низкого качества"
                            },
                            {
                                "target_lang_word": "никудышный",
                                "target_lang_sense_clarification": "разг. - ни на что не годный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
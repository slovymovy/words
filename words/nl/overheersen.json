            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b7348832-8593-38d5-847c-d73b03a9f7f6",
                    "sense_definition": "De sterkste invloed hebben, het meest aanwezig zijn of de boventoon voeren.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De geur van verse koffie <w>overheerste</w> in de kleine keuken.",
                            "target_lang_translations": {
                                "en": "The smell of fresh coffee <w>dominated</w> the small kitchen.",
                                "ru": "На маленькой кухне <w>царил</w> запах свежего кофе."
                            }
                        },
                        {
                            "text": "In het debat <w>overheersten</w> de emoties in plaats van de feiten.",
                            "target_lang_translations": {
                                "en": "In the debate, emotions <w>dominated</w> instead of facts.",
                                "ru": "В дебатах <w>преобладали</w> эмоции, а не факты."
                            }
                        }
                    ],
                    "synonyms": [
                        "domineren",
                        "de boventoon voeren",
                        "prevaleren"
                    ],
                    "common_phrases": [
                        "het beeld overheersen",
                        "de angst overheerst"
                    ],
                    "target_lang_definitions": {
                        "en": "To have the strongest influence, be most present, or have the upper hand.",
                        "ru": "Иметь наибольшее влияние, присутствовать в наибольшей степени или задавать тон."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dominate",
                                "target_lang_sense_clarification": "to exert an overwhelming influence"
                            },
                            {
                                "target_lang_word": "predominate",
                                "target_lang_sense_clarification": "to be the most frequent or common element"
                            },
                            {
                                "target_lang_word": "prevail",
                                "target_lang_sense_clarification": "to prove more powerful or superior"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "преобладать",
                                "target_lang_sense_clarification": "быть в большинстве, иметь перевес"
                            },
                            {
                                "target_lang_word": "доминировать",
                                "target_lang_sense_clarification": "занимать главенствующее положение"
                            },
                            {
                                "target_lang_word": "царить",
                                "target_lang_sense_clarification": "о запахах, атмосфере или обстановке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97087927-ad4a-3f69-a3de-9b0cc7a3fbda",
                    "sense_definition": "De macht uitoefenen over een ander volk, gebied of persoon; onderwerpen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spanje is zeven eeuwen <w>overheerst</w> door de Moren.",
                            "target_lang_translations": {
                                "en": "Spain was <w>dominated</w> by the Moors for seven centuries.",
                                "ru": "Испания семь веков <w>находилась под властью</w> мавров."
                            }
                        },
                        {
                            "text": "De dictator wilde de hele regio met zijn leger <w>overheersen</w>.",
                            "target_lang_translations": {
                                "en": "The dictator wanted to <w>dominate</w> the entire region with his army.",
                                "ru": "Диктатор хотел <w>подчинить</w> весь регион своей армией."
                            }
                        }
                    ],
                    "synonyms": [
                        "onderdrukken",
                        "bezetten",
                        "domineren"
                    ],
                    "antonyms": [
                        "bevrijden"
                    ],
                    "common_phrases": [
                        "een land overheersen",
                        "een volk overheersen"
                    ],
                    "target_lang_definitions": {
                        "en": "To exercise power over another people, territory, or person; to subjugate.",
                        "ru": "Осуществлять власть над другим народом, территорией или человеком; подчинять."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dominate",
                                "target_lang_sense_clarification": "to control by superior authority or power"
                            },
                            {
                                "target_lang_word": "rule",
                                "target_lang_sense_clarification": "to govern or exercise authority over"
                            },
                            {
                                "target_lang_word": "subjugate",
                                "target_lang_sense_clarification": "to bring under complete control"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "господствовать",
                                "target_lang_sense_clarification": "иметь верховную власть, владычествовать"
                            },
                            {
                                "target_lang_word": "подчинять",
                                "target_lang_sense_clarification": "ставить в зависимость, заставлять повиноваться"
                            },
                            {
                                "target_lang_word": "властвовать",
                                "target_lang_sense_clarification": "управлять, распоряжаться"
                            }
                        ]
                    }
                }
            ]

            "senses": [
                {
                    "sense_id": "f9e42f49-ef3a-3f52-975f-a994e159c0f5",
                    "sense_definition": "Een opening in de muur van een gebouw om licht door te laten, meestal afgesloten met glas.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij keek de hele middag uit het <w>raam</w> naar de regen.",
                            "target_lang_translations": {
                                "en": "He looked out the <w>window</w> at the rain all afternoon.",
                                "ru": "Он весь день смотрел из <w>окна</w> на дождь."
                            }
                        },
                        {
                            "text": "Kun je het <w>raam</w> even openzetten voor wat frisse lucht?",
                            "target_lang_translations": {
                                "en": "Can you open the <w>window</w> for some fresh air?",
                                "ru": "Ты можешь открыть <w>окно</w>, чтобы впустить свежий воздух?"
                            }
                        }
                    ],
                    "synonyms": [
                        "venster"
                    ],
                    "common_phrases": [
                        "uit het raam kijken",
                        "de ramen lappen",
                        "een open raam"
                    ],
                    "target_lang_definitions": {
                        "en": "An opening in the wall of a building to let light through, usually closed with glass.",
                        "ru": "Отверстие в стене здания для пропускания света, обычно закрытое стеклом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "window",
                                "target_lang_sense_clarification": "opening in a wall"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "окно",
                                "target_lang_sense_clarification": "отверстие в стене"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00301ad8-a8c6-3cdb-8bf3-f7c8565dcaab",

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ed0b0deb-6976-30c5-97d7-634ca9fc61e8",
                    "sense_definition": "Having a wide width or breadth, especially referring to the proportions of a ship.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>beamy</w> vessel provided excellent stability in the rough seas.",
                            "target_lang_translations": {
                                "ru": "Это <w>широкое</w> судно обеспечивало отличную устойчивость в бурном море."
                            }
                        },
                        {
                            "text": "Sailors often prefer a <w>beamy</w> boat because it offers more cargo space.",
                            "target_lang_translations": {
                                "ru": "Моряки часто предпочитают <w>широкие</w> лодки, так как в них больше места для груза."
                            }
                        }
                    ],
                    "synonyms": [
                        "broad",
                        "wide"
                    ],
                    "antonyms": [
                        "narrow"
                    ],
                    "common_phrases": [
                        "beamy ship",
                        "beamy vessel",
                        "beamy hull"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий большую ширину (особенно о пропорциях судна)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "широкий",
                                "target_lang_sense_clarification": "о судне"
                            },
                            {
                                "target_lang_word": "широкобокий",
                                "target_lang_sense_clarification": "имеющий широкие бока"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bac5e0c4-1d54-3d29-9f10-7415ffcf408b",
                    "sense_definition": "Emitting or reflecting bright rays of light; shining and radiant.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "light_radiance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>beamy</w> sun rose slowly over the distant hills.",
                            "target_lang_translations": {
                                "ru": "<w>Лучезарное</w> солнце медленно вставало над далекими холмами."
                            }
                        },
                        {
                            "text": "A <w>beamy</w> star guided the travelers through the night.",
                            "target_lang_translations": {
                                "ru": "<w>Сияющая</w> звезда вела путников сквозь ночь."
                            }
                        }
                    ],
                    "synonyms": [
                        "radiant",
                        "shining",
                        "bright",
                        "luminous"
                    ],
                    "antonyms": [
                        "dark",
                        "dim"
                    ],
                    "target_lang_definitions": {
                        "ru": "Излучающий или отражающий яркие лучи света; сияющий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лучезарный",
                                "target_lang_sense_clarification": "испускающий лучи света"
                            },
                            {
                                "target_lang_word": "сияющий",
                                "target_lang_sense_clarification": "ярко светящийся"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9a32769-2a1c-32d0-a2fd-908d6de0fb58",
                    "sense_definition": "Full of happiness, joy, or cheerfulness; radiant in expression.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "light_radiance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She welcomed the guests with a <w>beamy</w> smile on her face.",
                            "target_lang_translations": {
                                "ru": "Она приветствовала гостей с <w>лучезарной</w> улыбкой на лице."
                            }
                        },
                        {
                            "text": "His <w>beamy</w> optimism lifted the spirits of everyone in the room.",
                            "target_lang_translations": {
                                "ru": "Его <w>сияющий</w> оптимизм поднял настроение всем в комнате."
                            }
                        }
                    ],
                    "synonyms": [
                        "joyous",
                        "cheerful",
                        "gladsome"
                    ],
                    "antonyms": [
                        "gloomy",
                        "sullen"
                    ],
                    "common_phrases": [
                        "beamy face",
                        "beamy smile"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полный счастья, радости или веселья; сияющий (о выражении лица)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лучезарный",
                                "target_lang_sense_clarification": "о радостной улыбке или взгляде"
                            },
                            {
                                "target_lang_word": "сияющий",
                                "target_lang_sense_clarification": "светящийся от счастья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e12c24d5-0d4c-3a50-8540-9c1173e0a3f7",
                    "sense_definition": "Resembling a heavy wooden beam in size and weight; massive.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The warrior wielded a <w>beamy</w> spear that few others could lift.",
                            "target_lang_translations": {
                                "ru": "Воин орудовал <w>массивным</w> копьем, которое мало кто мог поднять."
                            }
                        },
                        {
                            "text": "Huge, <w>beamy</w> timbers supported the roof of the ancient hall.",
                            "target_lang_translations": {
                                "ru": "Огромные, <w>тяжелые</w> брусья поддерживали крышу древнего зала."
                            }
                        }
                    ],
                    "synonyms": [
                        "massive",
                        "heavy",
                        "massy"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Напоминающий тяжелую деревянную балку по размеру и весу; массивный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "массивный",
                                "target_lang_sense_clarification": "большой и тяжелый"
                            },
                            {
                                "target_lang_word": "тяжелый",
                                "target_lang_sense_clarification": "имеющий большой вес"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa4ee59e-97e0-388f-8c0b-0e6f90391e17",
                    "sense_definition": "Possessing large horns or antlers.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <w>beamy</w> stag stood proudly at the edge of the forest clearing.",
                            "target_lang_translations": {
                                "ru": "<w>Широкорогий</w> олень гордо стоял на краю лесной поляны."
                            }
                        },
                        {
                            "text": "The hunters tracked the <w>beamy</w> deer through the dense woods.",
                            "target_lang_translations": {
                                "ru": "Охотники выслеживали <w>ветвистого</w> оленя в густом лесу."
                            }
                        }
                    ],
                    "synonyms": [
                        "antlered"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий большие рога (обычно об оленях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "широкорогий",
                                "target_lang_sense_clarification": "с широким размахом рогов"
                            },
                            {
                                "target_lang_word": "ветвистый",
                                "target_lang_sense_clarification": "о рогах с множеством отростков"
                            }
                        ]
                    }
                }
            ]
        }

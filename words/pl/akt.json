                },
                {
                    "sense_id": "5bff469d-54e9-3beb-a6bd-5559de734adc",
                    "sense_definition": "Krótka modlitwa lub formuła religijna wyrażająca określone uczucia duchowe.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "religion_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wierni zgromadzeni w kościele wspólnie odmówili <w>akt</w> skruchy.",
                            "target_lang_translations": {
                                "ru": "Верующие, собравшиеся в костеле, вместе прочитали <w>акт</w> покаяния."
                            }
                        },
                        {
                            "text": "Codzienny <w>akt</w> wiary jest ważnym elementem jego duchowości.",
                            "target_lang_translations": {
                                "ru": "Ежедневный <w>акт</w> веры является важным элементом его духовности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "akt żalu",
                        "akt wiary",
                        "akt strzelisty",
                        "akt miłości"
                    ],
                    "target_lang_definitions": {
                        "ru": "Краткая молитва или религиозная формула, выражающая определенные духовные чувства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "акт",
                                "target_lang_sense_clarification": "молитвенная формула"

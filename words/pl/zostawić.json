                        {
                            "text": "Znany pisarz <w>zostawił</w> po sobie wiele niepublikowanych wcześniej rękopisów.",
                            "target_lang_translations": {
                                "ru": "Известный писатель <w>оставил</w> после себя много ранее не опубликованных рукописей."
                            }
                        }
                    ],
                    "synonyms": [
                        "pozostawić"
                    ],
                    "common_phrases": [
                        "zostawić majątek",
                        "zostawić spadek",
                        "zostawić dom dzieciom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передать имущество или предметы кому-то в наследство после своей смерти"
                    },
                    "translations": {

                {
                    "sense_id": "e9ed3941-1ed0-378e-8e0a-f7190afbf741",
                    "sense_definition": "Niewielkie, naturalne wzniesienie terenu, mniejsze od góry.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "geo_elevation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dom moich dziadków stoi na malowniczym, zielonym <w>pagórku</w> za wsią.",
                            "target_lang_translations": {
                                "ru": "Дом моих дедушки и бабушки стоит на живописном зеленом <w>холме</w> за деревней."
                            }
                        },
                        {
                            "text": "Zimą wszystkie dzieci z okolicy biegają na ten <w>pagórek</w> z sankami.",
                            "target_lang_translations": {
                                "ru": "Зимой все местные дети бегают на эту <w>горку</w> с санками."
                            }
                        }
                    ],
                    "synonyms": [
                        "wzgórek",
                        "górka",
                        "pagór",
                        "wzgórze"
                    ],
                    "common_phrases": [
                        "zielony pagórek",
                        "stromy pagórek",
                        "wejść na pagórek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая естественная возвышенность местности, меньше горы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "холм",
                                "target_lang_sense_clarification": "возвышенность"
                            },
                            {
                                "target_lang_word": "пригорок",
                                "target_lang_sense_clarification": "небольшой холм"
                            },
                            {
                                "target_lang_word": "горка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "baaf72a9-a5ff-356c-9b67-52a6105b69e4",
                    "sense_definition": "Sterta przedmiotów lub sypkiej substancji ułożona w kształt małego wzniesienia.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "pile_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na środku placu budowy robotnicy usypali spory <w>pagórek</w> piasku.",
                            "target_lang_translations": {
                                "ru": "Посреди стройплощадки рабочие насыпали большую <w>горку</w> песка."
                            }
                        },
                        {
                            "text": "W jego pokoju na podłodze leżał <w>pagórek</w> brudnych ubrań do prania.",
                            "target_lang_translations": {
                                "ru": "В его комнате на полу лежала <w>горка</w> грязной одежды для стирки."
                            }
                        }
                    ],
                    "synonyms": [
                        "sterta",
                        "stos",
                        "kopiec"
                    ],
                    "common_phrases": [
                        "pagórek piasku",
                        "pagórek ziemi"
                    ],
                    "target_lang_definitions": {

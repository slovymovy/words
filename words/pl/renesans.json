                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "period_style",
                    "name_type": "ideology_or_movement_name",
                    "examples": [
                        {
                            "text": "Za kolebkę <w>Renesansu</w> historycy sztuki uznają Toskanię.",
                            "target_lang_translations": {
                                "ru": "Искусствоведы считают Тоскану колыбелью <w>Ренессанса</w>."
                            }
                        },
                        {
                            "text": "Włochy są powszechnie uważane za ojczyznę <w>Renesansu</w>.",
                            "target_lang_translations": {
                                "ru": "Италию принято считать родиной <w>Ренессанса</w>."
                            }
                        }
                    ],

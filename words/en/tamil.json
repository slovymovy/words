                        ]
                    }
                },
                {
                    "sense_id": "5f0bf017-a144-3dcd-926b-a78d8a529d26",
                    "sense_definition": "Of or relating to the Tamil script.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scholar studied the <w>Tamil</w> inscriptions on the temple wall.",
                            "target_lang_translations": {
                                "ru": "Ученый изучал <w>тамильские</w> надписи на стене храма."
                            }
                        },
                        {
                            "text": "He bought a new <w>Tamil</w> keyboard for his computer.",
                            "target_lang_translations": {
                                "ru": "Он купил новую <w>тамильскую</w> клавиатуру для своего компьютера."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к тамильской письменности или шрифту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тами́льский",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "80926781-e83d-3ecb-bdb2-7aac14f46081",
                    "sense_definition": "A person who originates from Tamil Nadu, India or Sri Lanka, or has Tamil origins.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>Tamils</w> have a rich history of art, architecture, and literature.",
                            "target_lang_translations": {
                                "ru": "У <w>тамилов</w> богатая история искусства, архитектуры и литературы."
                            }
                        },
                        {
                            "text": "He is a <w>Tamil</w> who has lived in London for twenty years.",
                            "target_lang_translations": {
                                "ru": "Он — <w>тамил</w>, который живет в Лондоне уже двадцать лет."
                            }
                        }
                    ],
                    "synonyms": [

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "885884cd-131d-3e68-8e95-5a9f60714a8e",
                    "sense_definition": "В значительной степени, довольно (используется для указания на умеренно высокую степень качества).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот новый фильм <достаточно> интересный и весёлый.",
                            "target_lang_translations": {
                                "en": "This new movie is <quite> interesting and funny."
                            }
                        },
                        {
                            "text": "Мы живём <достаточно> далеко от центра города.",
                            "target_lang_translations": {
                                "en": "We live <quite> far from the city center."
                            }
                        }
                    ],
                    "synonyms": [
                        "довольно",
                        "весьма"
                    ],
                    "antonyms": [
                        "мало",
                        "чуть-чуть"
                    ],
                    "common_phrases": [
                        "достаточно хорошо",
                        "достаточно часто",
                        "достаточно большой"
                    ],
                    "target_lang_definitions": {
                        "en": "To a significant extent or degree; fairly or moderately."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quite",
                                "target_lang_sense_clarification": "adverb of degree"
                            },
                            {
                                "target_lang_word": "rather",
                                "target_lang_sense_clarification": "to a certain extent"
                            },
                            {
                                "target_lang_word": "fairly",
                                "target_lang_sense_clarification": "moderately"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db66363a-d072-3006-a249-d9343b39ec76",
                    "sense_definition": "Имеется в нужном количестве; хватает (выступает в роли сказуемого).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sufficiency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У нас <достаточно> времени, чтобы всё успеть.",
                            "target_lang_translations": {
                                "en": "We have <enough> time to get everything done."
                            }
                        },
                        {
                            "text": "Для счастья человеку нужно <достаточно> мало.",
                            "target_lang_translations": {
                                "en": "For happiness, a person needs <quite> little."
                            }
                        }
                    ],
                    "synonyms": [
                        "хватит",
                        "хватает"
                    ],
                    "antonyms": [
                        "мало",
                        "недостаточно"
                    ],
                    "common_phrases": [
                        "этого достаточно",
                        "вполне достаточно"
                    ],
                    "target_lang_definitions": {
                        "en": "Existing in the necessary quantity; there is a sufficient amount."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "enough",
                                "target_lang_sense_clarification": "sufficient quantity"
                            },
                            {
                                "target_lang_word": "plenty",
                                "target_lang_sense_clarification": "more than enough"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74a0e16a-2b78-3126-854d-e584635d7717",
                    "sense_definition": "Столько, сколько нужно; в необходимой мере (наречие меры и степени).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sufficiency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он <достаточно> взрослый, чтобы принимать такие решения.",
                            "target_lang_translations": {
                                "en": "He is old <enough> to make such decisions."
                            }
                        },
                        {
                            "text": "Ты <достаточно> хорошо подготовился к этому экзамену?",
                            "target_lang_translations": {
                                "en": "Did you prepare well <enough> for this exam?"
                            }
                        }
                    ],
                    "antonyms": [
                        "недостаточно"
                    ],
                    "common_phrases": [
                        "достаточно сильный",
                        "достаточно быстро"
                    ],
                    "target_lang_definitions": {
                        "en": "To the necessary degree; as much as is needed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "enough",
                                "target_lang_sense_clarification": "used after adjectives or adverbs"
                            },
                            {
                                "target_lang_word": "sufficiently",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05f183cb-01bf-3b34-b14c-644e469bedd6",
                    "sense_definition": "Возглас, выражающий требование прекратить какое-либо действие; хватит.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "interjection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Всё, <достаточно>, я больше не хочу это обсуждать!",
                            "target_lang_translations": {
                                "en": "That's it, <enough>, I don't want to discuss this anymore!"
                            }
                        },
                        {
                            "text": "Прекратите шуметь, <достаточно>!",
                            "target_lang_translations": {
                                "en": "Stop making noise, <that's enough>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "хватит",
                        "довольно",
                        "стоп"
                    ],
                    "common_phrases": [
                        "всё достаточно"
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation used to demand that an action stop."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "enough",
                                "target_lang_sense_clarification": "stop command"
                            },
                            {
                                "target_lang_word": "that's enough",
                                "target_lang_sense_clarification": "phrase"
                            }
                        ]
                    }
                }
            ]
        },

            "senses": [
                {
                    "sense_id": "fe7bb0b2-33d4-3967-9863-72c8aa666ccd",
                    "sense_definition": "Специальный ящик, сосуд или вместилище для хранения и переноски различных предметов, продуктов или отходов.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама положила мне обед в удобный пластиковый <w>контейнер</w>.",
                            "target_lang_translations": {
                                "en": "Mom put my lunch in a convenient plastic <w>container</w>."
                            }
                        },
                        {
                            "text": "Утром приехала машина и опустошила мусорный <w>контейнер</w> во дворе.",
                            "target_lang_translations": {
                                "en": "In the morning, a truck arrived and emptied the trash <w>container</w> in the yard."
                            }
                        }
                    ],
                    "synonyms": [
                        "ёмкость"
                    ],
                    "common_phrases": [
                        "пластиковый контейнер",
                        "контейнер для еды",
                        "мусорный контейнер",
                        "герметичный контейнер"
                    ],
                    "target_lang_definitions": {
                        "en": "A special box, vessel, or receptacle for storing and carrying various items, food, or waste."
                    },
                    "translations": {

{
    "word_family": [
        "lufa",
        "lufowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bc072621-0cfd-3e08-855c-08290601a114",
                    "sense_definition": "Cienka, zazwyczaj szklana rurka służąca do palenia papierosów bez ustnika.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "smoking_accessories",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pułkownik włożył papierosa do szklanej <w>lufki</w> i powoli go zapalił.",
                            "target_lang_translations": {
                                "ru": "Полковник вставил сигарету в стеклянный <w>мундштук</w> и медленно закурил."
                            }
                        },
                        {
                            "text": "W kiosku można kupić prostą <w>lufkę</w> za zaledwie kilka złotych.",
                            "target_lang_translations": {
                                "ru": "В киоске можно купить простой <w>мундштук</w> всего за несколько злотых."
                            }
                        }
                    ],
                    "synonyms": [
                        "fifka",
                        "cygarniczka"
                    ],
                    "common_phrases": [
                        "szklana lufka",
                        "palić przez lufkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тонкая, обычно стеклянная трубка, используемая для курения сигарет без фильтра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мундштук",
                                "target_lang_sense_clarification": "трубка для курения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7273fd1e-6fad-3bf7-9d41-8563848897d0",
                    "sense_definition": "Mała porcja mocnego alkoholu, zazwyczaj wódki, lub kieliszek o małej pojemności.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "alcohol_measurements",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz zaproponował gościom po jednej <w>lufce</w> na rozgrzewkę.",
                            "target_lang_translations": {
                                "ru": "Хозяин предложил гостям по одной <w>стопке</w> для согрева."
                            }
                        },
                        {
                            "text": "Szybko wypił <w>lufkę</w> wódki i wrócił do rozmowy z przyjaciółmi.",
                            "target_lang_translations": {
                                "ru": "Он быстро выпил <w>стопку</w> водки и вернулся к разговору с друзьями."
                            }
                        }
                    ],
                    "synonyms": [
                        "kieliszek"
                    ],
                    "common_phrases": [
                        "wypić lufkę",
                        "strzelić lufkę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal contexts regarding drinking alcohol"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшая порция крепкого алкоголя или рюмка маленькой емкости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стопка",
                                "target_lang_sense_clarification": "рюмка"
                            },
                            {
                                "target_lang_word": "рюмка",
                                "target_lang_sense_clarification": "сосуд для питья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5ffa37b-808b-3e14-8c0d-3531d276cedd",
                    "sense_definition": "Zdrobnienie od słowa <w>lufa</w> (część broni palnej).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zabawkowy pistolet miał małą, czerwoną <w>lufkę</w>, która świeciła w ciemności.",
                            "target_lang_translations": {
                                "ru": "У игрушечного пистолета был маленький красный <w>стволик</w>, который светился в темноте."
                            }
                        },
                        {
                            "text": "Z <w>lufki</w> miniaturowej armaty wydobył się kłąb szarego dymu.",
                            "target_lang_translations": {
                                "ru": "Из <w>стволика</w> миниатюрной пушки вырвался клуб серого дыма."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от слова «ствол» (часть огнестрельного оружия)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стволик",
                                "target_lang_sense_clarification": "часть оружия (уменьш.)"
                            },
                            {
                                "target_lang_word": "дуло",
                                "target_lang_sense_clarification": "выходное отверстие ствола"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
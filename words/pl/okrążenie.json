    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "32607080-40c8-37e1-9061-2ee18eea1db2",
                    "sense_definition": "Jednokrotne przebycie zamkniętej trasy dookoła czegoś, na przykład stadionu lub toru wyścigowego.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sport_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zawodnik rozpoczął ostatnie <w>okrążenie</w> biegu.",
                            "target_lang_translations": {
                                "ru": "Спортсмен начал последний <w>круг</w> забега."
                            }
                        },
                        {
                            "text": "To <w>okrążenie</w> było najszybsze w całym wyścigu.",
                            "target_lang_translations": {
                                "ru": "Этот <w>круг</w> был самым быстрым во всей гонке."
                            }
                        }
                    ],
                    "synonyms": [
                        "runda"
                    ],
                    "common_phrases": [
                        "ostatnie okrążenie",
                        "jedno okrążenie",
                        "kolejne okrążenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Однократное прохождение замкнутого маршрута вокруг чего-либо, например стадиона или гоночной трассы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "круг",
                                "target_lang_sense_clarification": "в спорте (бег, гонки)"
                            },
                            {
                                "target_lang_word": "виток",
                                "target_lang_sense_clarification": "один оборот по трассе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32d94d7f-9e91-35d6-90e0-4fbba292797a",
                    "sense_definition": "Rzeczownik odczasownikowy od <w>okrążyć</w>: czynność polegająca na obejściu lub objechaniu czegoś dookoła.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pełne <w>okrążenie</w> jeziora zajęło nam dwie godziny.",
                            "target_lang_translations": {
                                "ru": "Полный <w>обход</w> озера занял у нас два часа."
                            }
                        },
                        {
                            "text": "<w>Okrążenie</w> Ziemi przez satelitę trwało kilkadziesiąt minut.",
                            "target_lang_translations": {
                                "ru": "<w>Оборот</w> спутника вокруг Земли длился несколько десятков минут."
                            }
                        }
                    ],
                    "common_phrases": [
                        "okrążenie Ziemi",
                        "okrążenie terenu"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verbal noun"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по глаголу «окружить»: перемещение вокруг объекта (пешком, на транспорте или в полете)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обход",
                                "target_lang_sense_clarification": "действие (пешком)"
                            },
                            {
                                "target_lang_word": "объезд",
                                "target_lang_sense_clarification": "действие (на транспорте)"
                            },
                            {
                                "target_lang_word": "оборот",
                                "target_lang_sense_clarification": "полный цикл вращения (астрономия)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "647af70d-186b-3e48-9ed9-e67e27c3c50b",
                    "sense_definition": "Sytuacja, w której wojska lub inne siły zostały otoczone ze wszystkich stron przez przeciwnika.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierze próbowali wydostać się z <w>okrążenia</w> pod osłoną nocy.",
                            "target_lang_translations": {
                                "ru": "Солдаты пытались выбраться из <w>окружения</w> под покровом ночи."
                            }
                        },
                        {
                            "text": "Armia wroga zamknęła szczelne <w>okrążenie</w> wokół miasta.",
                            "target_lang_translations": {
                                "ru": "Вражеская армия замкнула плотное <w>окружение</w> вокруг города."
                            }
                        }
                    ],
                    "synonyms": [
                        "otoczenie",
                        "osaczenie",
                        "oblężenie"
                    ],
                    "common_phrases": [
                        "wyjść z okrążenia",
                        "pierścień okrążenia",
                        "zamknąć okrążenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация, когда войска или другие силы блокированы противником со всех сторон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окружение",
                                "target_lang_sense_clarification": "военная блокада"
                            },
                            {
                                "target_lang_word": "кольцо",
                                "target_lang_sense_clarification": "оцепление"
                            }
                        ]
                    }
                }
            ]
        }

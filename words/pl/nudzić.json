    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5689356e-e0c3-36fe-987b-ba3d122f710f",
                    "sense_definition": "Odczuwać brak zajęcia, monotonii lub brak zainteresowania czymś (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "boredom_feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj wieczorem bardzo <w>nudziłem</w> się w domu, bo nie było prądu.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером я очень <w>скучал</w> дома, потому что не было электричества."
                            }
                        },
                        {
                            "text": "Dzieci szybko <w>nudzą</w> się podczas długiej podróży samochodem.",
                            "target_lang_translations": {
                                "ru": "Дети быстро начинают <w>скучать</w> во время долгой поездки на машине."
                            }
                        }
                    ],
                    "synonyms": [
                        "przykrzyć się"
                    ],
                    "common_phrases": [
                        "nudzić się jak mops",
                        "śmiertelnie się nudzić",
                        "nudzić się w domu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать скуку, томиться от безделья или отсутствия интереса (используется с возвратным местоимением «się»)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скучать",
                                "target_lang_sense_clarification": "испытывать скуку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35e4ad91-8e95-32ff-83d3-bc50676c2915",
                    "sense_definition": "Sprawiać, że ktoś traci zainteresowanie, czuje znużenie lub monotonnię.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "boredom_causing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten film był tak długi, że w połowie zaczął mnie <w>nudzić</w>.",
                            "target_lang_translations": {
                                "ru": "Этот фильм был таким длинным, что на середине начал мне <w>надоедать</w>."
                            }
                        },
                        {
                            "text": "Nie chcę cię <w>nudzić</w> szczegółami mojej pracy, więc opowiem krótko.",
                            "target_lang_translations": {
                                "ru": "Не хочу <w>утомлять</w> тебя подробностями моей работы, поэтому расскажу кратко."
                            }
                        }
                    ],
                    "synonyms": [
                        "zanudzać",
                        "nużyć"
                    ],
                    "common_phrases": [
                        "strasznie nudzić",
                        "nudzić opowieściami",
                        "nudzić słuchaczy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывать у кого-либо скуку, потерю интереса или утомление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "утомлять",
                                "target_lang_sense_clarification": "вызывать скуку, быть неинтересным"
                            },
                            {
                                "target_lang_word": "надоедать",
                                "target_lang_sense_clarification": "становиться скучным"
                            },
                            {
                                "target_lang_word": "наводить скуку",
                                "target_lang_sense_clarification": "заставлять скучать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56490495-9ea3-3cab-825d-758409bd224f",
                    "sense_definition": "Uporczywie niepokoić kogoś, przeszkadzać lub zawracać głowę ciągłymi sprawami.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "bothering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąsiad przyszedł <w>nudzić</w> mojego ojca o pożyczenie kosiarki.",
                            "target_lang_translations": {
                                "ru": "Сосед пришел <w>докучать</w> моему отцу просьбой одолжить газонокосилку."
                            }
                        },
                        {
                            "text": "Przestań mnie wreszcie <w>nudzić</w> tymi bezsensownymi pytaniami.",
                            "target_lang_translations": {
                                "ru": "Перестань наконец <w>надоедать</w> мне этими бессмысленными вопросами."
                            }
                        }
                    ],
                    "synonyms": [
                        "męczyć"
                    ],
                    "common_phrases": [
                        "nudzić o pieniądze",
                        "nudzić prośbami"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "prowincjonalne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Назойливо беспокоить кого-то, приставать с просьбами или отвлекать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надоедать",
                                "target_lang_sense_clarification": "назойливо беспокоить"
                            },
                            {
                                "target_lang_word": "докучать",
                                "target_lang_sense_clarification": "приставать, мешать"
                            },
                            {
                                "target_lang_word": "донимать",
                                "target_lang_sense_clarification": "мучить просьбами"
                            }
                        ]
                    }
                }
            ]
        }
    ]

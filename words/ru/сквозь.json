    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "a2975b61-b241-3516-8eec-cecae14d42ae",
                    "sense_definition": "Через что-нибудь, проникая внутрь или на другую сторону.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Яркое солнце светило <сквозь> густые облака.",
                            "target_lang_translations": {
                                "en": "The bright sun shone <through> the thick clouds."
                            }
                        },
                        {
                            "text": "Мы с трудом пробирались <сквозь> плотную толпу людей.",
                            "target_lang_translations": {
                                "en": "We struggled <through> the dense crowd of people."
                            }
                        }
                    ],
                    "synonyms": [
                        "через"
                    ],
                    "common_phrases": [
                        "смотреть сквозь пальцы",
                        "сквозь туман",
                        "сквозь землю"
                    ],
                    "target_lang_definitions": {
                        "en": "Through something, penetrating inside or to the other side."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "through",
                                "target_lang_sense_clarification": "movement from one side to the other"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f4f5f94-b66b-3e37-a6fd-76c7ed1c0659",
                    "sense_definition": "Обозначает действие, происходящее несмотря на помехи или сопровождающееся другим состоянием.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Девушка попыталась улыбнуться <сквозь> слёзы.",
                            "target_lang_translations": {
                                "en": "The girl tried to smile <through> her tears."
                            }
                        },
                        {
                            "text": "Он ответил мне что-то невнятное <сквозь> сон.",
                            "target_lang_translations": {
                                "en": "He answered me something indistinctly <through> his sleep."
                            }
                        }
                    ],
                    "synonyms": [
                        "через"
                    ],
                    "common_phrases": [
                        "смех сквозь слёзы",
                        "сквозь зубы",
                        "сквозь сон"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates an action occurring despite obstacles or accompanied by another state."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "through",
                                "target_lang_sense_clarification": "despite obstacles or mixed with a state"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a26853ab-7043-3b5d-9cb5-1629cfc7e6f9",
                    "sense_definition": "То же, что насквозь; полностью, до самого конца.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сильный дождь промочил мою одежду <сквозь>.",
                            "target_lang_translations": {
                                "en": "The heavy rain soaked my clothes <through>."
                            }
                        },
                        {
                            "text": "Пуля прошла <сквозь> и застряла в стене.",
                            "target_lang_translations": {
                                "en": "The bullet went <through> and stuck in the wall."
                            }
                        }
                    ],
                    "synonyms": [

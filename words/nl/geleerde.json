                    "sense_id": "d3169305-64ee-3b87-9d98-8e4f101fc038",
                    "sense_definition": "Iemand die veel gestudeerd heeft en veel kennis bezit, vooral op wetenschappelijk gebied.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "expert_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>geleerde</w> heeft een nieuwe ontdekking gedaan in het laboratorium.",
                            "target_lang_translations": {
                                "en": "The <w>scholar</w> made a new discovery in the laboratory.",
                                "ru": "<w>Учёный</w> сделал новое открытие в лаборатории."
                            }
                        },
                        {
                            "text": "Veel <w>geleerden</w> kwamen samen om over het klimaat te praten.",
                            "target_lang_translations": {
                                "en": "Many <w>scholars</w> gathered to talk about the climate.",
                                "ru": "Многие <w>учёные</w> собрались, чтобы поговорить о климате."
                            }
                        }
                    ],
                    "synonyms": [
                        "wetenschapper"
                    ],
                    "common_phrases": [
                        "een eminente geleerde",
                        "de grote geleerde"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who has studied a lot and possesses much knowledge, especially in a scientific field.",
                        "ru": "Человек, который много учился и обладает большими знаниями, особенно в научной области."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scholar",
                                "target_lang_sense_clarification": "a learned person, especially in the humanities or sciences"

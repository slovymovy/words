    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bde091b0-6187-3b3f-8ed9-fffb093e4db6",
                    "sense_definition": "Бумажный пакет специальной формы для отправки писем или хранения документов.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "paper_container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я аккуратно положил письмо в <w>конверт</w> и написал адрес.",
                            "target_lang_translations": {
                                "en": "I carefully put the letter in the <w>envelope</w> and wrote the address."
                            }
                        },
                        {
                            "text": "На столе лежал белый <w>конверт</w> с важными документами.",
                            "target_lang_translations": {
                                "en": "A white <w>envelope</w> with important documents lay on the table."
                            }
                        }
                    ],
                    "common_phrases": [
                        "почтовый конверт",
                        "заклеить конверт",
                        "вскрыть конверт",
                        "положить в конверт"
                    ],
                    "target_lang_definitions": {
                        "en": "A flat paper container with a sealable flap, used for enclosing letters or documents."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "envelope",
                                "target_lang_sense_clarification": "for mail"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0867aea-ca71-3d04-b201-52f8d2638fec",
                    "sense_definition": "Одеяло или верхняя одежда для младенцев, сшитая в виде мешка.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "baby_clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для зимних прогулок родители купили тёплый меховой <w>конверт</w>.",
                            "target_lang_translations": {
                                "en": "For winter walks, the parents bought a warm fur <w>bunting bag</w>."
                            }
                        },
                        {
                            "text": "Малыш сладко спал в уютном <w>конверте</w> в коляске.",
                            "target_lang_translations": {
                                "en": "The baby slept sweetly in a cozy <w>footmuff</w> in the stroller."
                            }
                        }
                    ],
                    "common_phrases": [
                        "конверт для новорождённого",
                        "зимний конверт",
                        "конверт на выписку"
                    ],
                    "target_lang_definitions": {
                        "en": "A warm, bag-like outer garment or blanket for infants, often used in strollers."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bunting bag",
                                "target_lang_sense_clarification": "baby clothing"
                            },
                            {
                                "target_lang_word": "footmuff",
                                "target_lang_sense_clarification": "stroller accessory"
                            }
                        ]
                    }
                }
            ]
        }
    ]

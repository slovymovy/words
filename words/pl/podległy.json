    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9e7ab15e-64a4-39ec-9289-841df0e96392",
                    "sense_definition": "Zależny od kogoś, znajdujący się pod czyjąś władzą, kontrolą lub zwierzchnictwem.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "subordination",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Księstwo Warszawskie było <w>podległe</w> Francji.",
                            "target_lang_translations": {
                                "ru": "Варшавское герцогство было <w>подвластно</w> Франции."
                            }
                        },
                        {
                            "text": "Wszyscy pracownicy działu są bezpośrednio <w>podlegli</w> dyrektorowi finansowemu.",
                            "target_lang_translations": {
                                "ru": "Все сотрудники отдела непосредственно <w>подчинены</w> финансовому директору."
                            }
                        }
                    ],
                    "synonyms": [
                        "podporządkowany",
                        "zależny",
                        "podrzędny",
                        "poddany"
                    ],
                    "antonyms": [
                        "niepodległy",
                        "niepodporządkowany",
                        "dominujący",
                        "władający"
                    ],
                    "common_phrases": [
                        "personel podległy",
                        "teren podległy",
                        "organ podległy",
                        "być podległym"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в подчинении, под властью или контролем кого-либо; зависимый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подчинённый",
                                "target_lang_sense_clarification": "находящийся в служебной или иерархической зависимости"
                            },
                            {
                                "target_lang_word": "подвластный",
                                "target_lang_sense_clarification": "находящийся под чьей-либо властью или господством"
                            },
                            {
                                "target_lang_word": "зависимый",
                                "target_lang_sense_clarification": "несамостоятельный, находящийся под контролем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d64927f-e18c-3929-b9da-b8e0c72325aa",
                    "sense_definition": "Taki, który musi stosować się do określonych reguł, praw lub może ulec jakiemuś działaniu.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "liability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy obywatel przebywający na terytorium kraju jest <w>podległy</w> jego prawu.",
                            "target_lang_translations": {
                                "ru": "Каждый гражданин, находящийся на территории страны, <w>подчинён</w> её законам."
                            }
                        },
                        {
                            "text": "Ten rodzaj dochodu jest <w>podległy</w> opodatkowaniu według nowych stawek.",
                            "target_lang_translations": {
                                "ru": "Этот вид дохода <w>подлежит</w> налогообложению по новым ставкам."
                            }
                        }
                    ],
                    "synonyms": [
                        "podporządkowany",
                        "zależny",
                        "podrzędny",
                        "podporządkowany"
                    ],
                    "antonyms": [
                        "niepodległy",
                        "niepodporządkowany",
                        "dominujący",
                        "władający"
                    ],
                    "common_phrases": [
                        "podległy karze",
                        "podległy cłu",
                        "podległy zwrotowi",
                        "podległy ocenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обязанный соблюдать определённые правила, законы или подлежащий какому-либо воздействию (напр. налогу, наказанию)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подлежащий",
                                "target_lang_sense_clarification": "подпадающий под действие чего-либо (налога, призыва, возврата)"
                            },
                            {
                                "target_lang_word": "подчинённый",
                                "target_lang_sense_clarification": "регулируемый правилами или законами"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "91484d64-b3a5-351c-a35e-312133a81c66",
                    "sense_definition": "Trzecia osoba liczby mnogiej, rodzaj niemęskoosobowy, czas przeszły od czasownika <w>podlec</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystkie te sporne kwestie <w>podległy</w> w końcu przedawnieniu.",
                            "target_lang_translations": {
                                "ru": "Все эти спорные вопросы в конце концов <w>подпали</w> под действие срока давности."
                            }
                        },
                        {
                            "text": "Zabytkowe budynki <w>podległy</w> zniszczeniu w trakcie ostatnich walk.",
                            "target_lang_translations": {
                                "ru": "Исторические здания <w>подверглись</w> разрушению в ходе последних боёв."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма прошедшего времени глагола 'podlec' (подвергнуться, поддаться) для множественного числа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подверглись",
                                "target_lang_sense_clarification": "испытали на себе какое-либо действие или состояние"
                            },
                            {
                                "target_lang_word": "подпали",
                                "target_lang_sense_clarification": "оказались в сфере действия чего-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "52eb7e03-fdfe-3b06-959c-2b4aa441b269",
                    "sense_definition": "Udać się w jakieś miejsce, używając środka transportu (np. samochodu, pociągu, roweru) lub jadąc na zwierzęciu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_travel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W przyszłym tygodniu <w>pojadę</w> z rodzicami nad morze, żeby trochę odpocząć.",
                            "target_lang_translations": {
                                "ru": "На следующей неделе я <w>поеду</w> с родителями на море, чтобы немного отдохнуть."
                            }
                        },
                        {
                            "text": "Musimy szybko <w>pojechać</w> na stację benzynową, bo w samochodzie kończy nam się paliwo.",
                            "target_lang_translations": {
                                "ru": "Нам нужно быстро <w>поехать</w> на заправку, потому что в машине заканчивается топливо."
                            }
                        },
                        {
                            "text": "Kiedy wreszcie <w>pojedziemy</w> na te wymarzone wakacje do Włoch?",
                            "target_lang_translations": {
                                "ru": "Когда мы наконец <w>поедем</w> в этот отпуск мечты в Италию?"
                            }
                        }
                    ],
                    "synonyms": [
                        "chlapnąć",
                        "palnąć",
                        "odjechać"
                    ],
                    "antonyms": [
                        "przyjść",
                        "zostać"
                    ],
                    "common_phrases": [
                        "pojechać na wakacje",
                        "pojechać do domu",
                        "pojechać pociągiem",
                        "pojechać samochodem",
                        "pojechać w góry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправиться в какое-либо место, используя транспортное средство (например, машину, поезд, велосипед) или верхом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поехать",
                                "target_lang_sense_clarification": "начать движение или отправиться куда-то на транспорте"
                            },
                            {
                                "target_lang_word": "отправиться",
                                "target_lang_sense_clarification": "начать путешествие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65e360cd-1296-350d-acc3-27d66fded8ba",
                    "sense_definition": "O środku transportu: ruszyć z miejsca, rozpocząć podróż lub przemieścić się w określonym kierunku.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twój autobus właśnie <w>pojechał</w>, więc musisz niestety poczekać na następny.",
                            "target_lang_translations": {
                                "ru": "Твой автобус только что <w>уехал</w>, так что тебе, к сожалению, придётся ждать следующего."
                            }
                        },
                        {
                            "text": "Tramwaj numer pięć <w>pojedzie</w> dzisiaj zmienioną trasą z powodu remontu torowiska.",
                            "target_lang_translations": {
                                "ru": "Трамвай номер пять сегодня <w>пойдёт</w> по изменённому маршруту из-за ремонта путей."
                            }
                        }
                    ],
                    "synonyms": [
                        "odjechać"
                    ],
                    "antonyms": [
                        "przyjechać",
                        "zatrzymać się"
                    ],
                    "common_phrases": [
                        "autobus już pojechał",
                        "pociąg zaraz pojedzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "О транспортном средстве: тронуться с места, начать рейс или двигаться в определенном направлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уехать",
                                "target_lang_sense_clarification": "покинуть место (об автобусе, поезде)"
                            },
                            {
                                "target_lang_word": "пойти",
                                "target_lang_sense_clarification": "начать движение по маршруту (о городском транспорте)"
                            },
                            {
                                "target_lang_word": "поехать",
                                "target_lang_sense_clarification": "начать движение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f343ca5b-4268-3b4f-9484-7edffa0470f0",
                    "sense_definition": "Skrytykować kogoś bardzo ostro, obrazić lub wyśmiać w sposób agresywny.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel ostro <w>pojechał</w> uczniowi, który przyszedł na lekcję kompletnie nieprzygotowany.",
                            "target_lang_translations": {
                                "ru": "Учитель жестко <w>наехал</w> на ученика, который пришел на урок совершенно неподготовленным."
                            }
                        },
                        {
                            "text": "Nie powinieneś tak <w>pojechać</w> koledze przy wszystkich, to było bardzo niemiłe.",
                            "target_lang_translations": {
                                "ru": "Тебе не стоило так <w>наезжать</w> на приятеля при всех, это было очень некрасиво."
                            }
                        }
                    ],
                    "antonyms": [
                        "pochwalić"
                    ],
                    "common_phrases": [
                        "równo kogoś pojechać",
                        "pojechać komuś po ambicji"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "used mostly by young people"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Резко раскритиковать, оскорбить или агрессивно высмеять кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наехать",
                                "target_lang_sense_clarification": "агрессивно предъявить претензии (слэнг)"
                            },
                            {
                                "target_lang_word": "проехаться",
                                "target_lang_sense_clarification": "зло высмеять или раскритиковать (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b283f815-db1d-3ab1-8fa7-8469a3ee2e97",
                    "sense_definition": "Powiedzieć coś niespodziewanie i bez zastanowienia lub zrobić coś w sposób brawurowy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "colloquial_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ale <w>pojechałeś</w> z tym żartem, wszyscy przy stole poczuli się trochę niezręcznie.",
                            "target_lang_translations": {
                                "ru": "Ну ты и <w>выдал</w> с этой шуткой, все за столом почувствовали себя немного неловко."
                            }
                        },
                        {
                            "text": "Marek postanowił <w>pojechać</w> po bandzie i rzucił pracę z dnia na dzień.",
                            "target_lang_translations": {
                                "ru": "Марек решил <w>пойти</w> ва-банк и бросил работу одним днём."
                            }
                        }
                    ],
                    "synonyms": [
                        "chlapnąć",
                        "palnąć"
                    ],
                    "common_phrases": [
                        "pojechać po bandzie",
                        "pojechać z tematem"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сказать что-то неожиданное, неуместное или совершить рискованный, безрассудный поступок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выдать",
                                "target_lang_sense_clarification": "сделать или сказать что-то неожиданное (разг.)"
                            },
                            {
                                "target_lang_word": "загнуть",
                                "target_lang_sense_clarification": "сказать глупость или преувеличение (разг.)"
                            },
                            {
                                "target_lang_word": "отмочить",
                                "target_lang_sense_clarification": "совершить странный поступок (разг.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

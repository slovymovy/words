                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одержимый",
                                "target_lang_sense_clarification": "злыми духами"
                            },
                            {
                                "target_lang_word": "бесноватый",
                                "target_lang_sense_clarification": "сумасшедший, одержимый бесом"

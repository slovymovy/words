            "senses": [
                {
                    "sense_id": "b2d88d76-619f-37ed-bde7-bba103694afc",
                    "sense_definition": "Simple past and past participle of <w>project</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The finance department <w>projected</w> a significant increase in revenue for the next quarter.",
                            "target_lang_translations": {
                                "ru": "Финансовый отдел <w>спрогнозировал</w> значительный рост выручки в следующем квартале."
                            }
                        },
                        {
                            "text": "She <w>projected</w> her voice clearly so that everyone in the back of the hall could hear her.",
                            "target_lang_translations": {
                                "ru": "Она <w>направляла</w> свой голос так четко, чтобы все в конце зала могли её слышать."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола project."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прогнозировать",
                                "target_lang_sense_clarification": "планировать или оценивать будущие показатели"
                            },
                            {
                                "target_lang_word": "проецировать",
                                "target_lang_sense_clarification": "отображать на экране или направлять (голос, свет)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f675440f-36f2-370f-82f2-12769ea82963",
                    "sense_definition": "Displayed or cast onto a surface, such as a screen or wall.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>projected</w> image was slightly blurry because the lens was out of focus.",
                            "target_lang_translations": {
                                "ru": "<w>Спроецированное</w> изображение было слегка размытым, потому что объектив был не в фокусе."
                            }
                        },
                        {
                            "text": "We sat in the dark room watching the movie <w>projected</w> on the large white canvas.",
                            "target_lang_translations": {
                                "ru": "Мы сидели в темной комнате и смотрели фильм, <w>проецируемый</w> на большое белое полотно."
                            }
                        }
                    ],
                    "synonyms": [
                        "shown",
                        "cast"
                    ],
                    "antonyms": [
                        "hidden",
                        "concealed"
                    ],
                    "common_phrases": [
                        "projected image",
                        "projected light",
                        "projected on the screen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отображаемый или отбрасываемый на поверхность, например, на экран или стену."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спроецированный",
                                "target_lang_sense_clarification": "отображенный на поверхности"
                            },
                            {
                                "target_lang_word": "проецируемый",
                                "target_lang_sense_clarification": "находящийся в процессе проекции"
                            }
                        ]
                    }
                }
            ]
        }
    ]

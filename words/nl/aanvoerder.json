{
    "word_family": [
        "aanvoeren",
        "aanvoer",
        "aanvoerster"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cbfd41c6-9788-3781-93d1-dc0aad7cc98b",
                    "sense_definition": "De speler die de leiding heeft over een sportteam en dit vertegenwoordigt tijdens een wedstrijd.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "leader_sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De scheidsrechter riep de <w>aanvoerder</w> bij zich om de regels uit te leggen.",
                            "target_lang_translations": {
                                "ru": "Судья подозвал <w>капитана</w> к себе, чтобы объяснить правила."
                            }
                        },
                        {
                            "text": "Als <w>aanvoerder</w> van het elftal moet je het goede voorbeeld geven aan je teamgenoten.",
                            "target_lang_translations": {
                                "ru": "Как <w>капитан</w> команды, ты должен подавать хороший пример своим товарищам."
                            }
                        }
                    ],
                    "synonyms": [
                        "captain"
                    ],
                    "common_phrases": [
                        "aanvoerder van het elftal",
                        "band van de aanvoerder",
                        "tweede aanvoerder"
                    ],
                    "target_lang_definitions": {
                        "ru": "Игрок, который возглавляет спортивную команду и представляет её во время матча."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капитан",
                                "target_lang_sense_clarification": "спортивной команды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5325f7a-6e4a-3142-9d44-14c1823a13b5",
                    "sense_definition": "Iemand die het bevel voert over een groep mensen, zoals militairen, demonstranten of een bende.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "leader_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De soldaten wachtten op een teken van hun <w>aanvoerder</w> om de aanval te beginnen.",
                            "target_lang_translations": {
                                "ru": "Солдаты ждали знака от своего <w>командира</w>, чтобы начать атаку."
                            }
                        },
                        {
                            "text": "De politie heeft gisteren de vermoedelijke <w>aanvoerder</w> van de criminele bende gearresteerd.",
                            "target_lang_translations": {
                                "ru": "Вчера полиция арестовала предполагаемого <w>главаря</w> преступной банды."
                            }
                        }
                    ],
                    "synonyms": [
                        "leider",
                        "bevelhebber",
                        "commandant"
                    ],
                    "antonyms": [
                        "volger"
                    ],
                    "common_phrases": [
                        "aanvoerder van de troepen",
                        "aanvoerder van de opstand"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, командующий группой людей, например, военными, демонстрантами или бандой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предводитель",
                                "target_lang_sense_clarification": "группы людей или движения"
                            },
                            {
                                "target_lang_word": "командир",
                                "target_lang_sense_clarification": "военный"
                            },
                            {
                                "target_lang_word": "главарь",
                                "target_lang_sense_clarification": "банды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d45e319f-c0df-3a2a-a749-9b4dc8acee5d",
                    "sense_definition": "De deelnemer of ploeg die op de eerste plaats staat in een competitie of ranglijst.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "leader_ranking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de winst van gisteren is de club nu de trotse <w>aanvoerder</w> van de competitie.",
                            "target_lang_translations": {
                                "ru": "После вчерашней победы клуб теперь является гордым <w>лидером</w> чемпионата."
                            }
                        },
                        {
                            "text": "Het is moeilijk om de positie als <w>aanvoerder</w> van de ranglijst het hele seizoen vast te houden.",
                            "target_lang_translations": {
                                "ru": "Трудно удерживать позицию <w>лидера</w> турнирной таблицы на протяжении всего сезона."
                            }
                        }
                    ],
                    "synonyms": [
                        "koploper",
                        "lijstaanvoerder"
                    ],
                    "antonyms": [
                        "hekkensluiter"
                    ],
                    "common_phrases": [
                        "aanvoerder van de ranglijst",
                        "aanvoerder van de competitie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участник или команда, занимающая первое место в соревновании или рейтинге."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лидер",
                                "target_lang_sense_clarification": "соревнования или рейтинга"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
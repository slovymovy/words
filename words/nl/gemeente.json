                },
                {
                    "sense_id": "b45c6c36-14ce-3860-88ef-78fbd1578a10",
                    "sense_definition": "De groep mensen die samen lid zijn van een bepaalde kerk.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dominee heette de nieuwe leden van de <w>gemeente</w> hartelijk welkom.",
                            "target_lang_translations": {
                                "en": "The minister warmly welcomed the new members of the <w>congregation</w>."
                            }
                        },
                        {
                            "text": "Na de dienst dronk de hele <w>gemeente</w> samen koffie in de zaal.",
                            "target_lang_translations": {
                                "en": "After the service, the whole <w>congregation</w> drank coffee together in the hall."
                            }
                        }

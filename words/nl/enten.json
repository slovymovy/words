    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b7a2285e-acd8-3dad-bfd3-7db7cbb3a3d5",
                    "sense_definition": "Een deel van een plant (de ent) op een andere plant (de onderstam) vastzetten zodat ze samen één plant vormen en verder groeien",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "grafting_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kweker besloot de appelbomen in het vroege voorjaar te <w>enten</w>.",
                            "target_lang_translations": {
                                "en": "The grower decided to <w>graft</w> the apple trees in early spring.",
                                "ru": "Садовод решил <w>привить</w> яблони ранней весной."
                            }
                        },
                        {
                            "text": "Bij het <w>enten</w> wordt een takje van de ene plant op de andere geplaatst.",
                            "target_lang_translations": {
                                "en": "During <w>grafting</w>, a twig from one plant is placed onto another.",
                                "ru": "При <w>прививке</w> веточка одного растения помещается на другое."
                            }
                        }
                    ],
                    "synonyms": [
                        "griffelen"
                    ],
                    "common_phrases": [
                        "bomen enten",
                        "rozen enten",
                        "op een onderstam enten"
                    ],
                    "target_lang_definitions": {
                        "en": "To attach a part of a plant (the scion) onto another plant (the rootstock) so that they form one plant and grow together",
                        "ru": "Прикрепление части одного растения (привоя) к другому (подвою) так, чтобы они срослись и продолжали расти как единое целое"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "graft",
                                "target_lang_sense_clarification": "horticulture"
                            },
                            {
                                "target_lang_word": "inoculate",
                                "target_lang_sense_clarification": "by budding"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "прививать",
                                "target_lang_sense_clarification": "растения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc675af0-dfd9-3864-b8de-f393a772b5b9",
                    "sense_definition": "Een entstof of ziektekiem inbrengen in een organisme om weerstand op te wekken of een cultuur te starten",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "inoculation_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De laborant moest de bacteriën op een nieuwe voedingsbodem <w>enten</w>.",
                            "target_lang_translations": {
                                "en": "The lab technician had to <w>inoculate</w> the bacteria onto a new culture medium.",
                                "ru": "Лаборант должен был <w>высеять</w> бактерии на новую питательную среду."
                            }
                        },
                        {
                            "text": "Men kan het virus in een ei <w>enten</w> om het verder te laten ontwikkelen.",
                            "target_lang_translations": {
                                "en": "One can <w>inoculate</w> the virus into an egg to let it develop further.",
                                "ru": "Вирус можно <w>инокулировать</w> в яйцо, чтобы дать ему развиваться дальше."
                            }
                        }
                    ],
                    "synonyms": [
                        "inenten",
                        "vaccineren"
                    ],
                    "target_lang_definitions": {
                        "en": "To introduce a vaccine or germ into an organism or culture medium to induce resistance or start growth",
                        "ru": "Введение микроорганизмов, вакцины или биологического материала в организм или питательную среду"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "inoculate",
                                "target_lang_sense_clarification": "microbiology/medicine"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "инокулировать",
                                "target_lang_sense_clarification": "в биологии и медицине"
                            },
                            {
                                "target_lang_word": "высевать",
                                "target_lang_sense_clarification": "бактерии на среду"
                            }
                        ]
                    }
                }
            ]
        },

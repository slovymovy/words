            "senses": [
                {
                    "sense_id": "03241186-c61c-3269-9ef7-a2c29d7e3519",
                    "sense_definition": "Een grote ontvangsthal of wachtruimte bij de ingang van een hotel, theater of ander openbaar gebouw.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben afgesproken om elkaar in de <w>lobby</w> van het hotel te ontmoeten.",
                            "target_lang_translations": {
                                "en": "We agreed to meet each other in the hotel <w>lobby</w>.",
                                "ru": "Мы договорились встретиться в <w>лобби</w> отеля."
                            }
                        },
                        {
                            "text": "De gasten zaten in de <w>lobby</w> te wachten tot hun kamers klaar waren.",
                            "target_lang_translations": {
                                "en": "The guests were waiting in the <w>lobby</w> until their rooms were ready.",
                                "ru": "Гости сидели в <w>лобби</w> и ждали, пока их номера будут готовы."
                            }
                        }
                    ],
                    "synonyms": [
                        "hal",
                        "foyer",
                        "ontvangsthal"
                    ],
                    "common_phrases": [
                        "in de lobby",
                        "de lobby van het hotel"
                    ],
                    "target_lang_definitions": {
                        "en": "A large reception hall or waiting area at the entrance of a hotel, theater, or other public building.",
                        "ru": "Большой приемный зал или зона ожидания у входа в отель, театр или другое общественное здание."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lobby",
                                "target_lang_sense_clarification": "entrance hall"
                            },
                            {
                                "target_lang_word": "foyer",
                                "target_lang_sense_clarification": "reception area"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "лобби",
                                "target_lang_sense_clarification": "в отеле"
                            },
                            {
                                "target_lang_word": "вестибюль",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "фойе",
                                "target_lang_sense_clarification": "в театре"
                            }
                        ]

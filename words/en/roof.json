    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3977c5a0-404c-330b-9b7c-ae48a9a4f367",
                    "sense_definition": "The external covering at the top of a building.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "building_top",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy snow damaged the old <w>roof</w> of the cottage.",
                            "target_lang_translations": {
                                "ru": "Сильный снегопад повредил старую <w>крышу</w> коттеджа."
                            }
                        },
                        {
                            "text": "We all live under one <w>roof</w> as a big family.",
                            "target_lang_translations": {
                                "ru": "Мы все живем под одной <w>крышей</w> как одна большая семья."
                            }
                        }
                    ],
                    "synonyms": [
                        "rooftop"
                    ],
                    "common_phrases": [
                        "under one roof",
                        "red roof",
                        "tiled roof",
                        "leaking roof"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внешнее покрытие верхней части здания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крыша",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "кровля",
                                "target_lang_sense_clarification": "материал или конструкция покрытия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4be1d47-6504-371c-883a-008d540a20eb",
                    "sense_definition": "The top external level of a building.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "building_top",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We went up to the <w>roof</w> to watch the fireworks.",
                            "target_lang_translations": {
                                "ru": "Мы поднялись на <w>крышу</w>, чтобы посмотреть фейерверк."
                            }
                        },
                        {
                            "text": "The cat climbed onto the <w>roof</w> and could not get down.",
                            "target_lang_translations": {
                                "ru": "Кот забрался на <w>крышу</w> и не мог спуститься."
                            }
                        }
                    ],
                    "synonyms": [
                        "rooftop"
                    ],
                    "common_phrases": [
                        "on the roof",
                        "climb to the roof",
                        "roof garden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхний внешний уровень здания (площадка)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крыша",
                                "target_lang_sense_clarification": "место, куда можно подняться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb1f8d18-e06d-370c-bd18-2c8adf15987f",
                    "sense_definition": "The upper part of a cavity.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hot pizza burned the <w>roof</w> of my mouth.",
                            "target_lang_translations": {
                                "ru": "Горячая пицца обожгла мне <w>нёбо</w>."
                            }
                        },
                        {
                            "text": "Stalactites hung from the <w>roof</w> of the dark cave.",
                            "target_lang_translations": {
                                "ru": "Сталактиты свисали со <w>свода</w> темной пещеры."
                            }
                        }
                    ],
                    "synonyms": [
                        "ceiling"
                    ],
                    "common_phrases": [
                        "roof of the mouth",
                        "roof of the cave"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхняя часть полости или пещеры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нёбо",
                                "target_lang_sense_clarification": "верхняя стенка ротовой полости"
                            },
                            {
                                "target_lang_word": "свод",
                                "target_lang_sense_clarification": "потолок пещеры или грота"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a243d75a-aab4-31a8-922f-29baa954d2be",
                    "sense_definition": "The surface or bed of rock immediately overlying a bed of coal or a flat vein.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "geology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miners checked the stability of the <w>roof</w> before proceeding.",
                            "target_lang_translations": {
                                "ru": "Шахтеры проверили устойчивость <w>кровли</w>, прежде чем продолжить."
                            }
                        },
                        {
                            "text": "The coal seam was difficult to access due to the hard <w>roof</w> above it.",
                            "target_lang_translations": {
                                "ru": "Угольный пласт был труднодоступен из-за твердой <w>кровли</w> над ним."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mining context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слой породы, лежащий непосредственно над пластом угля или рудной жилой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кровля",
                                "target_lang_sense_clarification": "геологический/горный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b579aaad-9bf5-38cf-b9c5-997c965ca8af",
                    "sense_definition": "An overhanging rock wall.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "geology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The climber struggled to navigate the steep <w>roof</w> of the cliff.",
                            "target_lang_translations": {
                                "ru": "Альпинист с трудом преодолевал крутой <w>карниз</w> скалы."
                            }
                        },
                        {
                            "text": "We found shelter from the rain under a natural rock <w>roof</w>.",
                            "target_lang_translations": {
                                "ru": "Мы нашли укрытие от дождя под естественным каменным <w>навесом</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нависающая скальная стена."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "карниз",
                                "target_lang_sense_clarification": "выступ скалы"
                            },
                            {
                                "target_lang_word": "навес",
                                "target_lang_sense_clarification": "нависающая часть скалы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "418437aa-55a3-30ed-a7aa-98e316f9d332",
                    "sense_definition": "A hat.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He placed his old <w>roof</w> on his head and walked out the door.",
                            "target_lang_translations": {
                                "ru": "Он надел свою старую <w>шляпу</w> на голову и вышел за дверь."
                            }
                        },
                        {
                            "text": "Don't forget your <w>roof</w> when you go out in the sun.",
                            "target_lang_translations": {
                                "ru": "Не забудь свой <w>головной убор</w>, когда выходишь на солнце."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        },
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Шляпа или головной убор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шляпа",
                                "target_lang_sense_clarification": "устаревший сленг"
                            },
                            {
                                "target_lang_word": "головной убор",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fd2091be-f73d-3c12-94ab-241b73604fac",
                    "sense_definition": "To cover or furnish with a roof.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cover_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The builders <w>roofed</w> the new house before the winter snow arrived.",
                            "target_lang_translations": {
                                "ru": "Строители <w>покрыли крышей</w> новый дом до того, как выпал зимний снег."
                            }
                        },
                        {
                            "text": "They decided to <w>roof</w> the patio to provide shade in the summer.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>сделать крышу</w> над патио, чтобы обеспечить тень летом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "roof a house",
                        "roof over"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать крышей или настилать кровлю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крыть",
                                "target_lang_sense_clarification": "покрывать крышей"
                            },
                            {
                                "target_lang_word": "настилать кровлю",
                                "target_lang_sense_clarification": "строительный процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "513d5e50-5ed5-36e5-9dcd-7c8a0356d7a9",
                    "sense_definition": "To shelter as if under a roof.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cover_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The large oak tree <w>roofed</w> us from the sudden downpour.",
                            "target_lang_translations": {
                                "ru": "Большой дуб <w>укрыл</w> нас от внезапного ливня."
                            }
                        },
                        {
                            "text": "Thick clouds <w>roofed</w> the valley, making the day seem dark.",
                            "target_lang_translations": {
                                "ru": "Густые облака <w>накрыли</w> долину, отчего день казался темным."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Укрывать или защищать, словно крышей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укрывать",
                                "target_lang_sense_clarification": "защищать сверху"
                            },
                            {
                                "target_lang_word": "накрывать",
                                "target_lang_sense_clarification": "опускаться сверху (об облаках, тени)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd6076b7-b187-3ecd-8225-7dd2d837720a",
                    "sense_definition": "To traverse buildings by walking or climbing across their roofs.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The thieves <w>roofed</w> across the city to escape the police.",
                            "target_lang_translations": {
                                "ru": "Воры <w>уходили по крышам</w> через город, чтобы скрыться от полиции."
                            }
                        },
                        {
                            "text": "He enjoys parkour and often <w>roofs</w> through the neighborhood.",
                            "target_lang_translations": {
                                "ru": "Он увлекается паркуром и часто <w>лазает по крышам</w> в округе."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаться по зданиям, переходя или перелезая с крыши на крышу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бегать по крышам",
                                "target_lang_sense_clarification": "паркур или преследование"
                            },
                            {
                                "target_lang_word": "лазить по крышам",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8d2b3b7-e120-3f67-8ddf-fd7da63cfbcf",
                    "sense_definition": "To put into prison.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The judge <w>roofed</w> the criminal for ten years.",
                            "target_lang_translations": {
                                "ru": "Судья <w>посадил</w> преступника на десять лет."
                            }
                        },
                        {
                            "text": "He got <w>roofed</w> after getting caught stealing cars.",
                            "target_lang_translations": {
                                "ru": "Его <w>закрыли</w> после того, как поймали на краже машин."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сажать в тюрьму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сажать",
                                "target_lang_sense_clarification": "отправлять в заключение"
                            },
                            {
                                "target_lang_word": "закрывать",
                                "target_lang_sense_clarification": "тюремный сленг"
                            }
                        ]
                    }
                }
            ]
        },

            "senses": [
                {
                    "sense_id": "31b3713e-a028-3b80-b345-b20bd25f6df0",
                    "sense_definition": "Met moeite of inspanning iets in bezit krijgen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>bemachtigde</w> op het nippertje nog een kaartje voor de belangrijke wedstrijd.",
                            "target_lang_translations": {
                                "en": "He managed to <w>secure</w> a ticket for the important match just in the nick of time."
                            }
                        },
                        {
                            "text": "Het is tegenwoordig erg lastig om een betaalbare woning in het centrum te <w>bemachtigen</w>.",
                            "target_lang_translations": {
                                "en": "Nowadays it is very difficult to <w>get hold of</w> an affordable home in the city center."
                            }
                        }
                    ],
                    "synonyms": [

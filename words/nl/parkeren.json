            "senses": [
                {
                    "sense_id": "276534ce-8e74-3a52-a9aa-3ebf7d57d3b2",
                    "sense_definition": "Een voertuig op een daarvoor bestemde plaats neerzetten en tijdelijk achterlaten.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je mag hier niet op de stoep <w>parkeren</w>.",
                            "target_lang_translations": {
                                "en": "You are not allowed to <w>park</w> on the sidewalk here.",
                                "ru": "Здесь нельзя <w>парковаться</w> на тротуаре."
                            }
                        },
                        {
                            "text": "Hij <w>parkeerde</w> zijn auto in de garage onder het gebouw.",
                            "target_lang_translations": {
                                "en": "He <w>parked</w> his car in the garage under the building.",
                                "ru": "Он <w>припарковал</w> свою машину в гараже под зданием."
                            }
                        }
                    ],
                    "synonyms": [
                        "neerzetten",
                        "stallen"
                    ],
                    "antonyms": [
                        "wegrijden",
                        "vertrekken"
                    ],
                    "common_phrases": [
                        "de auto parkeren",
                        "betaald parkeren",
                        "fout parkeren",
                        "achteruit parkeren"
                    ],
                    "target_lang_definitions": {
                        "en": "To put a vehicle in a designated place and leave it temporarily.",
                        "ru": "Ставить транспортное средство в предназначенном для этого месте и временно оставлять его."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "park",
                                "target_lang_sense_clarification": "a vehicle"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "парковаться",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "ставить",
                                "target_lang_sense_clarification": "разговорное, часто с 'машину'"
                            },
                            {
                                "target_lang_word": "припарковаться",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae0787d7-fa02-3761-ab57-85f3e2a71f7f",
                    "sense_definition": "Iets of iemand tijdelijk ergens neerzetten of laten wachten.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "informal_placement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze <w>parkeerde</w> haar zware boodschappentas in de gang.",
                            "target_lang_translations": {
                                "en": "She <w>parked</w> her heavy shopping bag in the hallway.",
                                "ru": "Она <w>поставила</w> свою тяжелую сумку с продуктами в коридоре."
                            }
                        },
                        {
                            "text": "Hij <w>parkeerde</w> de kinderen even voor de televisie zodat hij kon koken.",
                            "target_lang_translations": {
                                "en": "He <w>parked</w> the children in front of the television for a bit so he could cook.",
                                "ru": "Он ненадолго <w>усадил</w> детей перед телевизором, чтобы приготовить еду."
                            }
                        }
                    ],
                    "synonyms": [
                        "dumpen",
                        "plaatsen"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used for people or objects in a casual way"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To temporarily postpone a subject, idea, or problem to return to it later.",
                        "ru": "Временно отложить тему, идею или проблему, чтобы вернуться к ней позже."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "park",
                                "target_lang_sense_clarification": "a topic or idea"
                            },
                            {

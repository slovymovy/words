                    "sense_id": "76ede55d-de1f-3a70-a519-1bc52332fb63",
                    "sense_definition": "Meervoud van het zelfstandig naamwoord <w>mijn</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "plural_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De sluiting van de <w>mijnen</w> had grote gevolgen voor de regio.",
                            "target_lang_translations": {
                                "ru": "Закрытие <w>шахт</w> имело серьезные последствия для региона."
                            }
                        },
                        {
                            "text": "Er liggen nog steeds gevaarlijke <w>mijnen</w> in het voormalige oorlogsgebied.",
                            "target_lang_translations": {
                                "ru": "В бывшей зоне военных действий до сих пор лежат опасные <w>мины</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de mijnen sluiten",
                        "op een mijn trappen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число существительного \"mijn\" (шахта, рудник или мина)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шахты",
                                "target_lang_sense_clarification": "места добычи ископаемых"
                            },
                            {
                                "target_lang_word": "мины",
                                "target_lang_sense_clarification": "взрывные устройства"
                            },
                            {
                                "target_lang_word": "рудники",
                                "target_lang_sense_clarification": "горные предприятия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1d428b5-cd0b-38e5-a109-6ec99e9eac43",
                    "sense_definition": "Meervoud van het zelfstandig naamwoord <w>mijne</w> (verwijzend naar familie of verwanten).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "plural_forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil graag het beste voor mij en de <w>mijnen</w>.",
                            "target_lang_translations": {
                                "ru": "Я хочу лучшего для себя и <w>своих близких</w>."
                            }
                        },
                        {
                            "text": "Hij verdedigde de belangen van hem en de <w>mijnen</w> met verve.",
                            "target_lang_translations": {
                                "ru": "Он с жаром защищал свои интересы и интересы <w>своих родных</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ik en de mijnen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от \"mijne\", используемое для обозначения семьи или близких родственников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мои близкие",
                                "target_lang_sense_clarification": "семья, родственники"

                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "get_stuck_phys",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Колеса машины начали <завязать> в глубокой грязи.",
                            "target_lang_translations": {
                                "en": "The car's wheels began to <get stuck> in the deep mud."
                            }

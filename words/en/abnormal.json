    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ba1dd450-563d-3c5e-be1b-8711cd814fff",
                    "sense_definition": "Not conforming to rule or system; deviating from the usual or normal type.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor was concerned about the <w>abnormal</w> results of the blood test.",
                            "target_lang_translations": {
                                "ru": "Врач был обеспокоен <w>аномальными</w> результатами анализа крови."
                            }
                        },
                        {
                            "text": "It is <w>abnormal</w> to have such hot weather in the middle of winter.",
                            "target_lang_translations": {
                                "ru": "Это <w>ненормально</w>, что в середине зимы стоит такая жаркая погода."
                            }
                        }
                    ],
                    "synonyms": [
                        "unusual",
                        "atypical",
                        "irregular",
                        "strange",
                        "anomalous"
                    ],
                    "antonyms": [
                        "normal",
                        "typical",
                        "regular",
                        "ordinary",
                        "common"
                    ],
                    "common_phrases": [
                        "abnormal behavior",
                        "abnormal condition",
                        "abnormal amount",
                        "abnormal rhythm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не соответствующий правилам или системе; отклоняющийся от обычного или нормального типа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ненормальный",
                                "target_lang_sense_clarification": "общее значение; отличающийся от нормы"
                            },
                            {
                                "target_lang_word": "аномальный",
                                "target_lang_sense_clarification": "часто используется в медицине или науке"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0580d170-fda7-3d80-a185-28d5a104ac70",
                    "sense_definition": "A person or object that is not normal.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The machine automatically discards any <w>abnormals</w> from the production line.",
                            "target_lang_translations": {
                                "ru": "Машина автоматически удаляет любые <w>дефектные изделия</w> с производственной линии."
                            }
                        },
                        {
                            "text": "He felt like an <w>abnormal</w> among the crowd of ordinary people.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал себя <w>ненормальным</w> в толпе обычных людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "anomalous",
                        "irregular"
                    ],
                    "antonyms": [
                        "normal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или объект, который не является нормальным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ненормальный",
                                "target_lang_sense_clarification": "о человеке"
                            },
                            {
                                "target_lang_word": "аномалия",
                                "target_lang_sense_clarification": "об объекте или явлении"

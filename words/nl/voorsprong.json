{
    "word_family": [
        "sprong",
        "springen",
        "voorspringen"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cbf17243-e257-3bb4-8841-1b8f6c0da9b3",
                    "sense_definition": "Een positie waarin men voorligt op tegenstanders in een wedstrijd of competitie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_lead",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De wielrenner had een ruime <w>voorsprong</w> op het peloton.",
                            "target_lang_translations": {
                                "en": "The cyclist had a large <w>lead</w> over the peloton.",
                                "ru": "У велогонщика был большой <w>отрыв</w> от пелотона."
                            }
                        },
                        {
                            "text": "We moeten onze <w>voorsprong</w> verdedigen om de wedstrijd te winnen.",
                            "target_lang_translations": {
                                "en": "We must defend our <w>lead</w> to win the match.",
                                "ru": "Мы должны защитить свое <w>преимущество</w>, чтобы выиграть матч."
                            }
                        }
                    ],
                    "antonyms": [
                        "achterstand"
                    ],
                    "common_phrases": [
                        "een voorsprong nemen",
                        "voorsprong verdedigen",
                        "grote voorsprong"
                    ],
                    "target_lang_definitions": {
                        "en": "A position where one is ahead of opponents in a match or competition.",
                        "ru": "Положение, при котором участник опережает соперников в состязании или соревновании."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lead",
                                "target_lang_sense_clarification": "position ahead"
                            },
                            {
                                "target_lang_word": "advantage",
                                "target_lang_sense_clarification": "favorable position"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "преимущество",
                                "target_lang_sense_clarification": "лидерство в счете или положении"
                            },
                            {
                                "target_lang_word": "отрыв",
                                "target_lang_sense_clarification": "дистанция между лидером и преследователями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2594eaba-3df8-3385-b43b-994b0506ad22",
                    "sense_definition": "De situatie waarin men verder gevorderd of beter ontwikkeld is dan anderen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_lead",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf heeft een technologische <w>voorsprong</w> op de concurrentie.",
                            "target_lang_translations": {
                                "en": "The company has a technological <w>edge</w> over the competition.",
                                "ru": "У компании есть технологическое <w>преимущество</w> перед конкурентами."
                            }
                        },
                        {
                            "text": "Dankzij haar extra opleiding heeft ze een <w>voorsprong</w> op de arbeidsmarkt.",
                            "target_lang_translations": {
                                "en": "Thanks to her extra education, she has a <w>head start</w> in the labor market.",
                                "ru": "Благодаря дополнительному образованию у нее есть <w>преимущество</w> на рынке труда."
                            }
                        }
                    ],
                    "antonyms": [
                        "achterstand"
                    ],
                    "common_phrases": [
                        "technische voorsprong",
                        "voorsprong opbouwen",
                        "voorsprong behouden"
                    ],
                    "target_lang_definitions": {
                        "en": "The situation of being further advanced or better developed than others.",
                        "ru": "Ситуация, когда кто-то продвинулся дальше или развит лучше других."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "edge",
                                "target_lang_sense_clarification": "competitive advantage"
                            },
                            {
                                "target_lang_word": "lead",
                                "target_lang_sense_clarification": "advanced position"
                            },
                            {
                                "target_lang_word": "head start",
                                "target_lang_sense_clarification": "advantageous start"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "преимущество",
                                "target_lang_sense_clarification": "более выгодное положение или развитие"
                            },
                            {
                                "target_lang_word": "превосходство",
                                "target_lang_sense_clarification": "более высокий уровень"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65f50d08-e5e6-318d-9a99-88090a5b05d4",
                    "sense_definition": "Een voordeel in tijd of afstand dat men krijgt of heeft bij het begin van een activiteit.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "group_lead",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De lopers kregen vijf minuten <w>voorsprong</w> bij de start van de race.",
                            "target_lang_translations": {
                                "en": "The runners were given a five-minute <w>head start</w> at the start of the race.",
                                "ru": "Бегунам дали пять минут <w>форы</w> на старте гонки."
                            }
                        },
                        {
                            "text": "Hij begon met een kleine <w>voorsprong</w> aan het examen omdat hij al voorkennis had.",
                            "target_lang_translations": {
                                "en": "He started the exam with a small <w>head start</w> because he already had prior knowledge.",
                                "ru": "Он начал экзамен с небольшой <w>форой</w>, так как у него уже были предварительные знания."
                            }
                        }
                    ],
                    "antonyms": [
                        "achterstand"
                    ],
                    "common_phrases": [
                        "een voorsprong geven",
                        "met voorsprong beginnen"
                    ],
                    "target_lang_definitions": {
                        "en": "An advantage in time or distance received at the start of an activity.",
                        "ru": "Преимущество во времени или расстоянии, полученное или имеющееся в начале деятельности."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "head start",
                                "target_lang_sense_clarification": "advantage at the beginning"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "фора",
                                "target_lang_sense_clarification": "начальное преимущество"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
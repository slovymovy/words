    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d8f2fea6-5194-3b68-a189-ee3fd80a4588",
                    "sense_definition": "A substance or medication that prevents blood from clotting.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor prescribed an <w>anticoagulant</w> to reduce the risk of a stroke.",
                            "target_lang_translations": {
                                "ru": "Врач назначил <w>антикоагулянт</w> для снижения риска инсульта."
                            }
                        },
                        {
                            "text": "Patients taking a strong <w>anticoagulant</w> should be careful to avoid injuries that cause bleeding.",
                            "target_lang_translations": {
                                "ru": "Пациентам, принимающим сильный <w>антикоагулянт</w>, следует остерегаться травм, вызывающих кровотечение."
                            }
                        }
                    ],
                    "synonyms": [
                        "blood thinner"
                    ],
                    "antonyms": [
                        "coagulant#Noun"
                    ],
                    "common_phrases": [
                        "oral anticoagulant",
                        "prescribed an anticoagulant",
                        "take anticoagulants"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещество или лекарственный препарат, препятствующий свертыванию крови."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "антикоагулянт",
                                "target_lang_sense_clarification": "медицинский термин"
                            },
                            {
                                "target_lang_word": "средство для разжижения крови",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },
        {

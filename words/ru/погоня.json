    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "71573b2b-291e-307e-a682-d0721e16cff8",
                    "sense_definition": "Преследование кого-либо, кто убегает или скрывается, с целью поимки.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полицейская <w>погоня</w> за преступником продолжалась несколько часов.",
                            "target_lang_translations": {
                                "en": "The police <w>chase</w> after the criminal lasted for several hours."
                            }
                        },
                        {
                            "text": "Охотники пустились в <w>погоню</w> за раненым зверем.",
                            "target_lang_translations": {
                                "en": "The hunters set off in <w>pursuit</w> of the wounded beast."
                            }
                        }
                    ],
                    "synonyms": [
                        "пресле́дование"
                    ],
                    "common_phrases": [
                        "пуститься в погоню",
                        "броситься в погоню",
                        "уйти от погони"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of pursuing someone who is running away or hiding in order to catch them."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chase",
                                "target_lang_sense_clarification": "act of pursuing"
                            },
                            {
                                "target_lang_word": "pursuit",
                                "target_lang_sense_clarification": "act of chasing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5fe749a-8fdb-3112-b96f-574c55838958",
                    "sense_definition": "Усиленное стремление достигнуть чего-либо или добиться какой-либо цели (часто в переносном смысле).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В <w>погоне</w> за деньгами он совсем забыл о своей семье.",
                            "target_lang_translations": {
                                "en": "In the <w>pursuit</w> of money, he completely forgot about his family."
                            }
                        },
                        {
                            "text": "Эта компания потеряла качество в <w>погоне</w> за количеством.",
                            "target_lang_translations": {
                                "en": "This company lost quality in the <w>chase</w> for quantity."
                            }
                        }
                    ],
                    "common_phrases": [
                        "в погоне за счастьем",
                        "в погоне за славой",
                        "в погоне за прибылью"
                    ],
                    "target_lang_definitions": {
                        "en": "An intense effort to achieve a goal or obtain something, often used figuratively."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pursuit",
                                "target_lang_sense_clarification": "striving for a goal"
                            },
                            {
                                "target_lang_word": "chase",
                                "target_lang_sense_clarification": "striving for something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f51b8c7-40b7-396f-9e23-eaa5d0eeb320",
                    "sense_definition": "Группа людей (иногда с собаками), которая преследует кого-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Беглецы услышали лай собак и поняли, что <w>погоня</w> уже близко.",
                            "target_lang_translations": {
                                "en": "The fugitives heard the barking of dogs and realized that the <w>pursuers</w> were already close."
                            }
                        },
                        {
                            "text": "Князь приказал снарядить <w>погоню</w>, чтобы вернуть пленников.",
                            "target_lang_translations": {
                                "en": "The prince ordered a <w>search party</w> to be equipped to return the captives."
                            }
                        }
                    ],
                    "synonyms": [
                        "преследователи"
                    ],
                    "common_phrases": [
                        "выслать погоню",
                        "уйти от погони"
                    ],
                    "target_lang_definitions": {
                        "en": "A group of people, sometimes with dogs, sent to pursue and catch someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pursuers",
                                "target_lang_sense_clarification": "group of people chasing"
                            },
                            {
                                "target_lang_word": "search party",
                                "target_lang_sense_clarification": "organized group searching for someone"
                            },
                            {
                                "target_lang_word": "posse",
                                "target_lang_sense_clarification": "armed body of men"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "f4032b6f-d327-3e0e-9bdc-581c3fa4e47a",
                    "sense_definition": "Исторический герб Великого княжества Литовского, а также исторический государственный герб Беларуси, изображающий вооружённого всадника.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "historical_name",
                    "examples": [
                        {
                            "text": "На старинных знамёнах часто изображалась <w>Погоня</w>.",
                            "target_lang_translations": {
                                "en": "The <w>Pahonia</w> was often depicted on ancient banners."
                            }
                        },
                        {
                            "text": "Историки обсуждали значение символов, из которых состоит <w>Погоня</w>.",
                            "target_lang_translations": {
                                "en": "Historians discussed the meaning of the symbols that make up the <w>Pahonia</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "герб Погоня"
                    ],
                    "target_lang_definitions": {
                        "en": "The historical coat of arms of the Grand Duchy of Lithuania and Belarus, depicting an armed horseman."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Pahonia",
                                "target_lang_sense_clarification": "historical coat of arms"
                            },
                            {
                                "target_lang_word": "Pogonya",
                                "target_lang_sense_clarification": "transliterated name"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
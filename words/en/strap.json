    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "890fd6ac-469d-32fa-bd3b-ff0e1c108d93",
                    "sense_definition": "A narrow and flexible strip of leather, cloth, or other material, often used to fasten, secure, or carry something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "material_strip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She adjusted the <w>strap</w> on her heavy backpack to make it more comfortable.",
                            "target_lang_translations": {
                                "ru": "Она поправила <w>лямку</w> своего тяжелого рюкзака, чтобы было удобнее."
                            }
                        },
                        {
                            "text": "The watch comes with a genuine leather <w>strap</w> that looks very elegant.",
                            "target_lang_translations": {
                                "ru": "Эти часы продаются с <w>ремешком</w> из натуральной кожи, который выглядит очень элегантно."
                            }
                        }
                    ],
                    "synonyms": [
                        "strapline",
                        "strapline"
                    ],
                    "common_phrases": [
                        "shoulder strap",
                        "adjustable strap",
                        "leather strap",
                        "watch strap"
                    ],
                    "target_lang_definitions": {
                        "ru": "Узкая и гибкая полоска кожи, ткани или другого материала, часто используемая для застегивания, закрепления или переноски чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ремешок",
                                "target_lang_sense_clarification": "для часов, обуви или небольших сумок"
                            },
                            {
                                "target_lang_word": "лямка",
                                "target_lang_sense_clarification": "рюкзака или одежды"
                            },
                            {
                                "target_lang_word": "ремень",
                                "target_lang_sense_clarification": "общий термин, более широкий и прочный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "487d8a72-0e5a-3c87-a153-a677947fb8a9",
                    "sense_definition": "A flat strip of metal used to hold things together or reinforce a structure.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "material_strip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The builder used a metal <w>strap</w> to secure the pipe firmly to the wall.",
                            "target_lang_translations": {
                                "ru": "Строитель использовал металлическую <w>скобу</w>, чтобы надежно прикрепить трубу к стене."
                            }
                        },
                        {
                            "text": "A rusty iron <w>strap</w> held the corners of the old wooden crate together.",
                            "target_lang_translations": {
                                "ru": "Ржавая железная <w>лента</w> скрепляла углы старого деревянного ящика."
                            }
                        }
                    ],
                    "common_phrases": [
                        "metal strap",
                        "steel strap"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоская металлическая полоса, используемая для скрепления предметов или укрепления конструкции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скоба",
                                "target_lang_sense_clarification": "крепежная деталь"
                            },
                            {
                                "target_lang_word": "хомут",
                                "target_lang_sense_clarification": "для труб"
                            },
                            {
                                "target_lang_word": "металлическая лента",
                                "target_lang_sense_clarification": "строительный материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97acd7b0-5d41-3d6e-b632-9ea58f84765c",
                    "sense_definition": "A strip of thick leather used for hitting someone as a form of punishment.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "punishment_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The strict headmaster kept a leather <w>strap</w> in his desk drawer for discipline.",
                            "target_lang_translations": {
                                "ru": "Строгий директор хранил в ящике стола кожаный <w>ремень</w> для поддержания дисциплины."
                            }
                        },
                        {
                            "text": "He was afraid of getting the <w>strap</w> if he misbehaved in class again.",
                            "target_lang_translations": {
                                "ru": "Он боялся получить <w>ремня</w>, если снова будет плохо вести себя в классе."
                            }
                        }
                    ],
                    "synonyms": [
                        "strapline"
                    ],
                    "common_phrases": [
                        "get the strap"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "referring to corporal punishment"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Полоса толстой кожи, используемая для ударов в качестве наказания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ремень",
                                "target_lang_sense_clarification": "орудие наказания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce5c5556-dee0-319a-8882-0f1de55372d6",
                    "sense_definition": "A gun, usually a handgun.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "weapon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gangster was arrested for carrying a hidden <w>strap</w> under his jacket.",
                            "target_lang_translations": {
                                "ru": "Гангстера арестовали за то, что он носил спрятанную <w>пушку</w> под курткой."
                            }
                        },
                        {
                            "text": "He warned the others that he had a <w>strap</w> in his car for protection.",
                            "target_lang_translations": {
                                "ru": "Он предупредил остальных, что у него в машине есть <w>ствол</w> для защиты."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "street slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Огнестрельное оружие, обычно пистолет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пушка",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "ствол",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "49c34fce-4020-308e-b7bd-e0b607cbf880",
                    "sense_definition": "To fasten or secure something in place using a strap.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action_fasten",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Make sure to <w>strap</w> the boxes down so they don't fall off the truck.",
                            "target_lang_translations": {
                                "ru": "Обязательно <w>привяжите</w> коробки, чтобы они не упали с грузовика."
                            }
                        },
                        {
                            "text": "Passengers must <w>strap</w> themselves in before the plane takes off.",
                            "target_lang_translations": {
                                "ru": "Пассажиры должны <w>пристегнуться</w> перед взлетом самолета."
                            }
                        }
                    ],
                    "antonyms": [
                        "unstrap"
                    ],
                    "common_phrases": [
                        "strap in",
                        "strap down",
                        "strap on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прикреплять или фиксировать что-либо на месте с помощью ремня."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привязывать",
                                "target_lang_sense_clarification": "закреплять ремнями"
                            },
                            {
                                "target_lang_word": "пристёгивать",
                                "target_lang_sense_clarification": "ремнем безопасности"
                            },
                            {
                                "target_lang_word": "крепить",
                                "target_lang_sense_clarification": "фиксировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bfe60daa-ea46-3ac8-976e-ef68e375d85b",
                    "sense_definition": "To hit or punish someone with a strap.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "action_punish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The villain threatened to <w>strap</w> the hero if he didn't reveal the secret.",
                            "target_lang_translations": {
                                "ru": "Злодей угрожал <w>отхлестать</w> героя, если тот не раскроет тайну."
                            }
                        },
                        {
                            "text": "In the old days, they would <w>strap</w> prisoners for disobedience.",
                            "target_lang_translations": {
                                "ru": "В былые времена заключенных могли <w>пороть</w> за непослушание."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бить или наказывать кого-либо ремнем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пороть",
                                "target_lang_sense_clarification": "бить ремнем"
                            },
                            {
                                "target_lang_word": "отхлестать",
                                "target_lang_sense_clarification": "наносить удары"
                            }
                        ]
                    }
                }
            ]
        }
    ]

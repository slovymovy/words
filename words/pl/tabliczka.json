{
    "word_family": [
        "tablica",
        "tablicowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ea82f83a-3ff8-3bfc-84d2-14bad69533f7",
                    "sense_definition": "prostokątny kawałek czekolady, zwykle podzielony na mniejsze kostki",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "chocolate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na deser zjedliśmy całą <w>tabliczkę</w> mlecznej czekolady.",
                            "target_lang_translations": {
                                "ru": "На десерт мы съели целую <w>плитку</w> молочного шоколада."
                            }
                        },
                        {
                            "text": "Kup mi proszę jedną <w>tabliczkę</w> gorzkiej czekolady do ciasta.",
                            "target_lang_translations": {
                                "ru": "Купи мне, пожалуйста, одну <w>плитку</w> горького шоколада для торта."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Górny Śląsk) tabulka"
                    ],
                    "common_phrases": [
                        "tabliczka czekolady",
                        "pół tabliczki",
                        "kostka z tabliczki"
                    ],
                    "target_lang_definitions": {
                        "ru": "прямоугольный кусок шоколада, обычно разделенный на дольки"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плитка",
                                "target_lang_sense_clarification": "шоколада"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e798cbb4-24b7-3052-9e04-1be0db261172",
                    "sense_definition": "potocznie: zestawienie wyników mnożenia liczb, którego uczą się dzieci w szkole; tabliczka mnożenia",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "math",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jasiu musi nauczyć się <w>tabliczki</w> mnożenia na pamięć.",
                            "target_lang_translations": {
                                "ru": "Ясю нужно выучить <w>таблицу</w> умножения наизусть."
                            }
                        },
                        {
                            "text": "Nauczycielka odpytywała uczniów ze znajomości <w>tabliczki</w>.",
                            "target_lang_translations": {
                                "ru": "Учительница спрашивала учеников на знание <w>таблицы</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tabliczka mnożenia",
                        "znać tabliczkę",
                        "uczyć się tabliczki"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "jako skrót od 'tabliczka mnożenia'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "разг. список результатов умножения чисел, который дети учат в школе; таблица умножения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "таблица",
                                "target_lang_sense_clarification": "умножения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea146e0c-a174-3c52-b74e-a16c47b73288",
                    "sense_definition": "mała tablica lub płytka z napisem informacyjnym, ostrzegawczym albo numerem",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sign",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na drzwiach gabinetu wisiała złota <w>tabliczka</w> z nazwiskiem lekarza.",
                            "target_lang_translations": {
                                "ru": "На двери кабинета висела золотая <w>табличка</w> с фамилией врача."
                            }
                        },
                        {
                            "text": "Zwróć uwagę na <w>tabliczkę</w> z ostrzeżeniem przed psem.",
                            "target_lang_translations": {
                                "ru": "Обрати внимание на <w>табличку</w> с предупреждением о собаке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tabliczka znamionowa",
                        "tabliczka na drzwi",
                        "tabliczka informacyjna",
                        "tabliczka z numerem"
                    ],
                    "target_lang_definitions": {
                        "ru": "небольшая доска или пластина с информационной, предупреждающей надписью или номером"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "табличка",
                                "target_lang_sense_clarification": "с надписью"
                            },
                            {
                                "target_lang_word": "вывеска",
                                "target_lang_sense_clarification": "на здании/двери"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "046f74ff-ca08-3dde-a0fc-42eab92d9dd9",
                    "sense_definition": "zdrobnienie od: <w>tablica</w>; niewielka tablica do pisania lub rysowania",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sign",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko rysowało kredą na małej <w>tabliczce</w>.",
                            "target_lang_translations": {
                                "ru": "Ребёнок рисовал мелом на маленькой <w>дощечке</w>."
                            }
                        },
                        {
                            "text": "W muzeum oglądaliśmy glinianą <w>tabliczkę</w> z pismem klinowym.",
                            "target_lang_translations": {
                                "ru": "В музее мы рассматривали глиняную <w>табличку</w> с клинописью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gliniana tabliczka",
                        "woskowa tabliczka"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "zdrobnienie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "уменьш. от 'таблица' (доска); небольшая доска для письма или рисования"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "табличка",
                                "target_lang_sense_clarification": "глиняная/восковая"
                            },
                            {
                                "target_lang_word": "дощечка",
                                "target_lang_sense_clarification": "для письма"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
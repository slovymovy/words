    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "79249733-c9b1-3a71-ba11-1410b9a32e06",
                    "sense_definition": "Zmysł umożliwiający patrzenie i widzenie; zdolność do postrzegania rzeczywistości za pomocą oczu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sense_faculty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Noszę okulary, ponieważ mam wadę <w>wzroku</w>.",
                            "target_lang_translations": {
                                "ru": "Я ношу очки, потому что у меня плохое <w>зрение</w>."
                            }
                        },
                        {
                            "text": "Musisz dbać o swój <w>wzrok</w>, pracując długo przy komputerze.",
                            "target_lang_translations": {
                                "ru": "Ты должен беречь свое <w>зрение</w>, работая подолгу за компьютером."
                            }
                        }
                    ],
                    "synonyms": [
                        "widzenie"
                    ],
                    "common_phrases": [
                        "wada wzroku",
                        "stracić wzrok",
                        "dobry wzrok",
                        "słaby wzrok",
                        "pole wzroku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство, позволяющее видеть; способность воспринимать окружающий мир с помощью глаз."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зрение",
                                "target_lang_sense_clarification": "способность видеть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df00a3b0-6a09-30a6-804c-160df1eecaa4",
                    "sense_definition": "Skierowanie oczu na kogoś lub coś; czyjeś spojrzenie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "gaze",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój <w>wzrok</w> mimochodem padł na kuszące słodycze na wystawie.",
                            "target_lang_translations": {
                                "ru": "Мой <w>взгляд</w> случайно упал на манящие сладости на витрине."
                            }
                        },
                        {
                            "text": "Chłopiec ze wstydem spuścił <w>wzrok</w> na podłogę.",
                            "target_lang_translations": {
                                "ru": "Мальчик со стыдом опустил <w>взгляд</w> в пол."
                            }
                        }
                    ],
                    "synonyms": [
                        "spojrzenie"
                    ],
                    "common_phrases": [
                        "utkwić wzrok",
                        "odwrócić wzrok",
                        "spuścić wzrok",
                        "przeszywający wzrok",
                        "mierzyć wzrokiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направление глаз на кого-либо или что-либо; чей-то взгляд."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взгляд",
                                "target_lang_sense_clarification": "направленность глаз"
                            },
                            {
                                "target_lang_word": "взор",
                                "target_lang_sense_clarification": "книжное/поэтическое"
                            }
                        ]
                    }
                }
            ]
        }
    ]

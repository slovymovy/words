    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8c195a7e-ecf4-3552-81e2-79b27f5f18a9",
                    "sense_definition": "Someone connected by blood, marriage, or adoption; someone in the same family.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "family_member",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We invited all our close <w>relatives</w> to the wedding.",
                            "target_lang_translations": {
                                "ru": "Мы пригласили всех наших близких <w>родственников</w> на свадьбу."
                            }
                        },
                        {
                            "text": "He has no living <w>relatives</w> in this country.",
                            "target_lang_translations": {
                                "ru": "У него нет живых <w>родственников</w> в этой стране."
                            }
                        }
                    ],
                    "synonyms": [
                        "relation"
                    ],
                    "common_phrases": [
                        "close relative",
                        "distant relative",
                        "blood relative"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, связанный с кем-либо кровными узами, браком или усыновлением; член той же семьи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "родственник",
                                "target_lang_sense_clarification": "член семьи"
                            },
                            {
                                "target_lang_word": "родня",
                                "target_lang_sense_clarification": "собирательное, разговорное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ba44a1dd-c559-3bd6-a603-42b54db7309e",
                    "sense_definition": "Connected to, depending on, or judged in comparison with something else.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "comparative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>relative</w> cost of living is much lower in rural areas.",
                            "target_lang_translations": {
                                "ru": "<w>Относительная</w> стоимость жизни намного ниже в сельской местности."
                            }
                        },
                        {
                            "text": "Success is a <w>relative</w> term that means different things to different people.",
                            "target_lang_translations": {
                                "ru": "Успех — это <w>относительное</w> понятие, которое означает разные вещи для разных людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "comparative",
                        "conditional"
                    ],
                    "antonyms": [
                        "absolute"
                    ],
                    "common_phrases": [
                        "relative to",
                        "relative humidity",
                        "relative term"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с чем-либо, зависящий от чего-либо или оцениваемый в сравнении с чем-либо другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "относительный",
                                "target_lang_sense_clarification": "устанавливаемый по сравнению"
                            },
                            {
                                "target_lang_word": "сравнительный",
                                "target_lang_sense_clarification": "основанный на сравнении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "825d1525-0630-30d1-9236-f7e9f78d3285",
                    "sense_definition": "Relevant or pertinent to a specific subject or matter.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "relevant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please provide all facts <w>relative</w> to the investigation.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, предоставьте все факты, <w>относящиеся</w> к расследованию."
                            }
                        },
                        {
                            "text": "Her questions were not <w>relative</w> to the topic we were discussing.",
                            "target_lang_translations": {
                                "ru": "Ее вопросы не были <w>релевантны</w> теме, которую мы обсуждали."
                            }
                        }
                    ],
                    "common_phrases": [
                        "relative to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий отношение к определенному предмету или вопросу; соответствующий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "относящийся",
                                "target_lang_sense_clarification": "имеющий касательство"
                            },
                            {
                                "target_lang_word": "релевантный",
                                "target_lang_sense_clarification": "книжное, соответствующий делу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "707c066a-0008-3062-976a-57c03c57bb51",
                    "sense_definition": "Having the same key signature but differing in being major or minor.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A minor is the <w>relative</w> minor of C major.",
                            "target_lang_translations": {
                                "ru": "Ля минор — это <w>параллельный</w> минор До мажора."
                            }
                        },
                        {
                            "text": "Musicians must understand the connection between <w>relative</w> keys.",
                            "target_lang_translations": {
                                "ru": "Музыканты должны понимать связь между <w>параллельными</w> тональностями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "relative minor",
                        "relative major"
                    ],
                    "target_lang_definitions": {
                        "ru": "О тональностях: имеющий одинаковые ключевые знаки, но отличающийся ладом (мажор или минор)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "параллельный",
                                "target_lang_sense_clarification": "музыкальный термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "91b189c2-70e2-34ad-8873-14fff45c5d66",
                    "sense_definition": "To a certain degree; relatively.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "adverb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was a <w>relative</w> small event compared to last year.",
                            "target_lang_translations": {
                                "ru": "Это было <w>сравнительно</w> небольшое событие по сравнению с прошлым годом."
                            }
                        },
                        {
                            "text": "They managed to keep the house <w>relative</w> clean despite the chaos.",
                            "target_lang_translations": {
                                "ru": "Им удалось сохранить дом <w>относительно</w> чистым, несмотря на хаос."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often considered nonstandard for 'relatively'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В определенной степени; сравнительно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "относительно",
                                "target_lang_sense_clarification": "наречие"
                            },
                            {
                                "target_lang_word": "сравнительно",
                                "target_lang_sense_clarification": "наречие"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "supporter",
        "supportive",
        "supportable",
        "unsupported"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "69f05047-a4bc-3836-8572-de5b4203f6b9",
                    "sense_definition": "To agree with, encourage, or defend a person, group, or idea.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "aid_advocacy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>support</w> the decision to build a new park.",
                            "target_lang_translations": {
                                "ru": "Я <w>поддерживаю</w> решение построить новый парк."
                            }
                        },
                        {
                            "text": "Most people in the city <w>support</w> the local football team.",
                            "target_lang_translations": {
                                "ru": "Большинство людей в городе <w>поддерживают</w> местную футбольную команду."
                            }
                        }
                    ],
                    "synonyms": [
                        "back",
                        "favor",
                        "uphold"
                    ],
                    "antonyms": [
                        "oppose"
                    ],
                    "common_phrases": [
                        "support a team",
                        "support a cause",
                        "strongly support"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соглашаться с кем-либо, поощрять или защищать человека, группу или идею."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поддерживать",
                                "target_lang_sense_clarification": "выражать согласие, одобрять"
                            },
                            {
                                "target_lang_word": "выступать за",
                                "target_lang_sense_clarification": "быть на стороне кого-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7b6ab5f-188e-3ce8-899a-16679a545d99",
                    "sense_definition": "To provide the money, food, or home necessary for someone to live.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "financial_sustenance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He works two jobs to <w>support</w> his large family.",
                            "target_lang_translations": {
                                "ru": "Он работает на двух работах, чтобы <w>содержать</w> свою большую семью."
                            }
                        },
                        {
                            "text": "The scholarship <w>supports</w> students who cannot afford tuition.",
                            "target_lang_translations": {
                                "ru": "Стипендия <w>поддерживает</w> студентов, которые не могут оплатить обучение."
                            }
                        }
                    ],
                    "synonyms": [
                        "sustain",
                        "provide for"
                    ],
                    "common_phrases": [
                        "support a family",
                        "support oneself",
                        "financially support"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обеспечивать деньгами, едой или жильем, необходимыми для жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "содержать",
                                "target_lang_sense_clarification": "материально обеспечивать семью или иждивенцев"
                            },
                            {
                                "target_lang_word": "обеспечивать",
                                "target_lang_sense_clarification": "предоставлять средства к существованию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f8aa5be-841c-30b9-96b8-5422d3cf3557",
                    "sense_definition": "To hold something firmly or bear its weight so that it does not fall or collapse.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_bearing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "These thick walls <w>support</w> the entire roof of the building.",
                            "target_lang_translations": {
                                "ru": "Эти толстые стены <w>поддерживают</w> всю крышу здания."
                            }
                        },
                        {
                            "text": "The injured player was <w>supported</w> by his teammates as he walked off the field.",
                            "target_lang_translations": {
                                "ru": "Травмированного игрока <w>поддерживали</w> товарищи по команде, когда он уходил с поля."
                            }
                        }
                    ],
                    "synonyms": [
                        "hold up",
                        "bear",
                        "carry"
                    ],
                    "common_phrases": [
                        "support the weight",
                        "support the load"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удерживать что-либо или нести вес чего-либо, чтобы оно не упало."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поддерживать",
                                "target_lang_sense_clarification": "служить физической опорой"
                            },
                            {
                                "target_lang_word": "держать",
                                "target_lang_sense_clarification": "нести вес"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4dfa9fc-f4a6-3641-ad0c-ae22f6e3e766",
                    "sense_definition": "To accept or work successfully with a specific computer program, device, or file format.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "technical_compatibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This old computer does not <w>support</w> the latest operating system.",
                            "target_lang_translations": {
                                "ru": "Этот старый компьютер не <w>поддерживает</w> новейшую операционную систему."
                            }
                        },
                        {
                            "text": "The application <w>supports</w> several different languages.",
                            "target_lang_translations": {
                                "ru": "Приложение <w>поддерживает</w> несколько разных языков."
                            }
                        }
                    ],
                    "common_phrases": [
                        "support a format",
                        "support a device"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in technology contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть совместимым с определенной компьютерной программой, устройством или форматом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поддерживать",
                                "target_lang_sense_clarification": "о программном или аппаратном обеспечении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b513cbc-848d-3f45-869d-48e1de5eeb05",
                    "sense_definition": "To help show that a statement, theory, or claim is true using facts or evidence.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "evidence_proof",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The results of the experiment <w>support</w> our original theory.",
                            "target_lang_translations": {
                                "ru": "Результаты эксперимента <w>подтверждают</w> нашу первоначальную теорию."
                            }
                        },
                        {
                            "text": "There is no evidence to <w>support</w> the accusations made against him.",
                            "target_lang_translations": {
                                "ru": "Нет никаких доказательств, чтобы <w>подтвердить</w> выдвинутые против него обвинения."
                            }
                        }
                    ],
                    "synonyms": [
                        "corroborate",
                        "substantiate",
                        "verify",
                        "prove"
                    ],
                    "common_phrases": [
                        "support a claim",
                        "support a theory",
                        "evidence to support"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доказывать истинность утверждения или теории с помощью фактов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подтверждать",
                                "target_lang_sense_clarification": "служить доказательством"
                            },
                            {
                                "target_lang_word": "подкреплять",
                                "target_lang_sense_clarification": "обосновывать фактами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fff7aea-3f36-3191-905a-f70f4313dfdd",
                    "sense_definition": "To endure or tolerate something difficult or unpleasant.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "endurance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She could not <w>support</w> the insolence of his remarks any longer.",
                            "target_lang_translations": {
                                "ru": "Она больше не могла <w>выносить</w> дерзость его замечаний."
                            }
                        },
                        {
                            "text": "It was a grief too heavy to <w>support</w> alone.",
                            "target_lang_translations": {
                                "ru": "Это было горе, слишком тяжелое, чтобы <w>вынести</w> его в одиночку."
                            }
                        }
                    ],
                    "synonyms": [
                        "bear",
                        "tolerate",
                        "endure"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often literary or formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выносить или терпеть что-то трудное или неприятное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выносить",
                                "target_lang_sense_clarification": "терпеть трудности"
                            },
                            {
                                "target_lang_word": "терпеть",
                                "target_lang_sense_clarification": "мириться с чем-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "20127246-87b3-3540-bb5b-e0dec8545b70",
                    "sense_definition": "Encouragement, approval, money, or practical help given to someone or something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "aid_advocacy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Thank you for your <w>support</w> during this difficult time.",
                            "target_lang_translations": {
                                "ru": "Спасибо за вашу <w>поддержку</w> в это трудное время."
                            }
                        },
                        {
                            "text": "The project received financial <w>support</w> from the government.",
                            "target_lang_translations": {
                                "ru": "Проект получил финансовую <w>поддержку</w> от правительства."
                            }
                        }
                    ],
                    "synonyms": [
                        "help",
                        "assistance",
                        "backing",
                        "aid"
                    ],
                    "common_phrases": [
                        "financial support",
                        "emotional support",
                        "give support",
                        "show support"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поощрение, одобрение, деньги или практическая помощь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поддержка",
                                "target_lang_sense_clarification": "помощь или содействие"
                            },
                            {
                                "target_lang_word": "помощь",
                                "target_lang_sense_clarification": "содействие в чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a1280d1-15e9-3683-bac6-910380eec284",
                    "sense_definition": "A service provided by a company to help customers solve problems with a product.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "technical_compatibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If the software crashes, please contact technical <w>support</w>.",
                            "target_lang_translations": {
                                "ru": "Если программа выйдет из строя, пожалуйста, свяжитесь с технической <w>поддержкой</w>."
                            }
                        },
                        {
                            "text": "The company offers 24-hour customer <w>support</w> by phone.",
                            "target_lang_translations": {
                                "ru": "Компания предлагает круглосуточную <w>поддержку</w> клиентов по телефону."
                            }
                        }
                    ],
                    "synonyms": [
                        "help desk",
                        "customer service"
                    ],
                    "common_phrases": [
                        "technical support",
                        "customer support",
                        "contact support"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служба компании, помогающая клиентам решать проблемы с продуктом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "служба поддержки",
                                "target_lang_sense_clarification": "сервис для клиентов"
                            },
                            {
                                "target_lang_word": "техподдержка",
                                "target_lang_sense_clarification": "техническая помощь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0f17e75-f378-3db7-bf51-098fba7f23c2",
                    "sense_definition": "A physical object that holds something up to keep it from falling.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_bearing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bridge rests on concrete <w>supports</w>.",
                            "target_lang_translations": {
                                "ru": "Мост покоится на бетонных <w>опорах</w>."
                            }
                        },
                        {
                            "text": "Don't move that beam because it is a main <w>support</w> for the roof.",
                            "target_lang_translations": {
                                "ru": "Не двигай эту балку, потому что это главная <w>опора</w> крыши."
                            }
                        }
                    ],
                    "synonyms": [
                        "prop",
                        "brace",
                        "foundation"
                    ],
                    "common_phrases": [
                        "structural support"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физический объект, который удерживает что-то, не давая упасть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опора",
                                "target_lang_sense_clarification": "несущая конструкция"
                            },
                            {
                                "target_lang_word": "подпорка",
                                "target_lang_sense_clarification": "предмет для подпирания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b3408e2-a6d3-32af-b1a5-a0cfa065d868",
                    "sense_definition": "Facts or information that prove that something is true.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "evidence_proof",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scientist found new <w>support</w> for her theory.",
                            "target_lang_translations": {
                                "ru": "Ученый нашла новое <w>подтверждение</w> своей теории."
                            }
                        },
                        {
                            "text": "He offered no evidence in <w>support</w> of his claims.",
                            "target_lang_translations": {
                                "ru": "Он не представил никаких доказательств в <w>поддержку</w> своих заявлений."
                            }
                        }
                    ],
                    "synonyms": [
                        "evidence",
                        "proof",
                        "confirmation"
                    ],
                    "common_phrases": [
                        "in support of",
                        "support for"
                    ],
                    "target_lang_definitions": {
                        "ru": "Факты или информация, доказывающие истинность чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подтверждение",
                                "target_lang_sense_clarification": "доказательство истинности"
                            },
                            {
                                "target_lang_word": "обоснование",
                                "target_lang_sense_clarification": "аргументы в пользу чего-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
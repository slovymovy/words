    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4794f422-dfd4-36d3-bb69-9d468ccf7846",
                    "sense_definition": "Umieszczać przedmioty w określonym porządku, miejscu lub pozycji.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_arrangement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę codziennie <w>układać</w> swoje ubrania w szafie, żeby mieć porządek.",
                            "target_lang_translations": {
                                "ru": "Я должен каждый день <w>складывать</w> свою одежду в шкафу, чтобы был порядок."
                            }
                        },
                        {
                            "text": "Kelner zaczął starannie <w>układać</w> sztućce na stole przed przyjściem gości.",
                            "target_lang_translations": {
                                "ru": "Официант начал тщательно <w>раскладывать</w> столовые приборы на столе перед приходом гостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "kłaść",
                        "porządkować"
                    ],
                    "common_phrases": [
                        "układać ubrania",
                        "układać książki",
                        "układać na półce",
                        "układać do snu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Размещать предметы в определенном порядке, месте или положении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "складывать",
                                "target_lang_sense_clarification": "класть вещи в порядке"
                            },
                            {
                                "target_lang_word": "раскладывать",
                                "target_lang_sense_clarification": "распределять по поверхности"
                            },
                            {
                                "target_lang_word": "укладывать",
                                "target_lang_sense_clarification": "аккуратно размещать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05247cd7-ad83-3ed0-8a78-7517fd0432d8",
                    "sense_definition": "Tworzyć całość, dopasowując do siebie poszczególne elementy, na przykład klocki lub puzzle.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W deszczowe dni lubimy siedzieć w domu i <w>układać</w> puzzle.",
                            "target_lang_translations": {
                                "ru": "В дождливые дни мы любим сидеть дома и <w>собирать</w> пазлы."
                            }
                        },
                        {
                            "text": "Mały chłopiec potrafi <w>układać</w> wysokie wieże z drewnianych klocków.",
                            "target_lang_translations": {
                                "ru": "Маленький мальчик умеет <w>складывать</w> высокие башни из деревянных кубиков."
                            }
                        }
                    ],
                    "synonyms": [
                        "składać się"
                    ],
                    "common_phrases": [
                        "układać puzzle",
                        "układać klocki",
                        "układać mozaikę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать целое, соединяя отдельные элементы, например кубики или пазлы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "пазлы, конструктор"
                            },
                            {
                                "target_lang_word": "складывать",
                                "target_lang_sense_clarification": "соединять части"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d047200-c13d-3c44-9c9e-bf65332725a2",
                    "sense_definition": "Pakować rzeczy do walizki lub torby, przygotowując się do podróży.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_arrangement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zacząłem już <w>układać</w> rzeczy w walizce, ponieważ jutro rano wyjeżdżam.",
                            "target_lang_translations": {
                                "ru": "Я уже начал <w>укладывать</w> вещи в чемодан, так как завтра утром уезжаю."
                            }
                        },
                        {
                            "text": "Nie lubię <w>układać</w> ubrań w plecaku, bo zawsze brakuje mi miejsca.",
                            "target_lang_translations": {
                                "ru": "Я не люблю <w>складывать</w> одежду в рюкзак, потому что мне всегда не хватает места."
                            }
                        }
                    ],
                    "common_phrases": [
                        "układać rzeczy",
                        "układać w walizce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упаковывать вещи в чемодан или сумку, готовясь к поездке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укладывать",
                                "target_lang_sense_clarification": "вещи в багаж"
                            },
                            {
                                "target_lang_word": "складывать",
                                "target_lang_sense_clarification": "помещать внутрь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36af2376-e3eb-3a3f-b052-d04ee5c84ff8",
                    "sense_definition": "Tworzyć coś nowego, na przykład tekst, muzykę lub plan, łącząc myśli lub dźwięki w całość.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "creation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Poeta lubi <w>układać</w> rymowane wiersze o pięknie otaczającej nas przyrody.",
                            "target_lang_translations": {
                                "ru": "Поэт любит <w>сочинять</w> рифмованные стихи о красоте окружающей нас природы."
                            }
                        },
                        {
                            "text": "Musimy wspólnie <w>układać</w> szczegółowy plan naszej przyszłej wycieczki zagranicznej.",
                            "target_lang_translations": {
                                "ru": "Мы должны вместе <w>составлять</w> подробный план нашей будущей заграничной поездки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "układać wiersz",
                        "układać piosenkę",
                        "układać plan",
                        "układać scenariusz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать что-то новое, например текст, музыку или план, объединяя мысли или звуки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сочинять",
                                "target_lang_sense_clarification": "стихи, музыку"
                            },
                            {
                                "target_lang_word": "составлять",
                                "target_lang_sense_clarification": "план, список"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42df0da8-1a36-34fd-a25f-4684263db0be",
                    "sense_definition": "Nadawać włosom określony kształt lub formę, stylizować fryzurę.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "styling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Fryzjerka zaczęła <w>układać</w> moje włosy w elegancki kok na wesele.",
                            "target_lang_translations": {
                                "ru": "Парикмахер начала <w>укладывать</w> мои волосы в элегантный пучок на свадьбу."
                            }
                        },
                        {
                            "text": "Moja siostra potrafi pięknie <w>układać</w> włosy używając tylko szczotki i suszarki.",
                            "target_lang_translations": {
                                "ru": "Моя сестра умеет красиво <w>укладывать</w> волосы, используя только щетку и фен."
                            }
                        }
                    ],
                    "common_phrases": [
                        "układać włosy",
                        "układać fryzurę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Придавать волосам определенную форму, делать укладку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укладывать",
                                "target_lang_sense_clarification": "волосы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90ba0d77-811f-3faa-854c-cc10c6f445ae",
                    "sense_definition": "Pomagać komuś przyjąć pozycję leżącą, zazwyczaj kładąc kogoś do łóżka.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "care",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rodzice powinni <w>układać</w> małe dzieci do snu o stałej porze każdego dnia.",
                            "target_lang_translations": {
                                "ru": "Родители должны <w>укладывать</w> маленьких детей спать в одно и то же время каждый день."
                            }
                        },
                        {
                            "text": "Pielęgniarka pomagała ostrożnie <w>układać</w> chorego pacjenta na wygodnym łóżku.",
                            "target_lang_translations": {
                                "ru": "Медсестра помогала осторожно <w>укладывать</w> больного пациента на удобную кровать."
                            }
                        }
                    ],
                    "synonyms": [
                        "kłaść"
                    ],
                    "antonyms": [
                        "podnosić się",
                        "wstawać"
                    ],
                    "common_phrases": [
                        "układać do snu",
                        "układać do łóżka",
                        "układać dziecko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помогать кому-то принять лежачее положение, обычно укладывая спать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укладывать",
                                "target_lang_sense_clarification": "спать, в постель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a84e8900-2544-31d0-9125-d34e2dd6bdda",
                    "sense_definition": "O życiu, sprawach lub sytuacji: przebiegać lub rozwijać się w określony sposób (zwykle pomyślnie).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "situation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam nadzieję, że twoje życie prywatne zacznie się w końcu dobrze <w>układać</w>.",
                            "target_lang_translations": {
                                "ru": "Я надеюсь, что твоя личная жизнь наконец-то начнет хорошо <w>складываться</w>."
                            }
                        },
                        {
                            "text": "Niestety, nasze plany wakacyjne przestały się <w>układać</w> po nagłej awarii samochodu.",
                            "target_lang_translations": {
                                "ru": "К сожалению, наши планы на отпуск перестали <w>складываться</w> после внезапной поломки машины."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wszystko się układa",
                        "życie się układa",
                        "dobrze się układać"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О жизни, делах или ситуации: протекать или развиваться определенным образом (обычно успешно)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "складываться",
                                "target_lang_sense_clarification": "о ситуации, жизни"
                            },
                            {
                                "target_lang_word": "ладиться",
                                "target_lang_sense_clarification": "идти успешно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57205392-f06e-3e9d-9039-3d9aadca86f4",
                    "sense_definition": "Dochodzić do porozumienia z kimś, negocjować warunki lub kończyć spór.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "negotiation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Politycy musieli się <w>układać</w> z opozycją, aby uchwalić nową, ważną ustawę.",
                            "target_lang_translations": {
                                "ru": "Политикам пришлось <w>договариваться</w> с оппозицией, чтобы принять новый важный закон."
                            }
                        },
                        {
                            "text": "Nie zamierzam się <w>układać</w> z ludźmi, którzy mnie wcześniej oszukali.",
                            "target_lang_translations": {
                                "ru": "Я не собираюсь <w>договариваться</w> с людьми, которые меня раньше обманули."
                            }
                        }
                    ],
                    "synonyms": [
                        "dogadywać się",
                        "negocjować",
                        "porozumiewać się"
                    ],
                    "common_phrases": [
                        "układać się z wrogiem",
                        "układać się z rządem"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Приходить к соглашению с кем-либо, вести переговоры или прекращать спор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "договариваться",
                                "target_lang_sense_clarification": "приходить к согласию"
                            }
                        ]
                    }
                }
            ]
        }
    ]

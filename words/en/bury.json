                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заби́ть",
                                "target_lang_sense_clarification": "to score"
                            },
                            {
                                "target_lang_word": "вколоти́ть",
                                "target_lang_sense_clarification": "slang, to strike forcefully into the net"
                            }

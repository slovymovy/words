    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e542ddeb-0d0c-35c1-abf9-50dd2e731c45",
                    "sense_definition": "A line of people or vehicles waiting for their turn to do something.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_line",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We waited in a long <w>lineup</w> to buy tickets for the movie.",
                            "target_lang_translations": {
                                "ru": "Мы ждали в длинной <w>очереди</w>, чтобы купить билеты в кино."
                            }
                        },
                        {
                            "text": "There was a <w>lineup</w> of customers outside the store before it opened.",
                            "target_lang_translations": {
                                "ru": "Перед открытием магазина снаружи стояла <w>очередь</w> покупателей."
                            }
                        }
                    ],
                    "synonyms": [
                        "queue",
                        "line"
                    ],
                    "common_phrases": [
                        "wait in a lineup",
                        "long lineup",
                        "join the lineup"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "common in North America"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очередь людей или транспортных средств, ожидающих своей очереди что-то сделать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очередь",
                                "target_lang_sense_clarification": "людей или машин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9dbff1de-84de-3fa7-89bd-d6b7dd9f17bd",
                    "sense_definition": "The group of players selected to play for a sports team in a specific game.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "personnel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The manager announced the starting <w>lineup</w> an hour before the match.",
                            "target_lang_translations": {
                                "ru": "Тренер объявил стартовый <w>состав</w> за час до матча."
                            }
                        },
                        {
                            "text": "After his injury, he struggled to get back into the team's <w>lineup</w>.",
                            "target_lang_translations": {
                                "ru": "После травмы он с трудом пытался вернуться в <w>состав</w> команды."
                            }
                        }
                    ],
                    "common_phrases": [
                        "starting lineup",
                        "strong lineup",
                        "team lineup"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа игроков, выбранных для игры за спортивную команду в конкретном матче."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "состав",
                                "target_lang_sense_clarification": "команды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5b49f10-4fbe-3102-9610-86e6bfdfe5d8",
                    "sense_definition": "A list of performers, bands, or acts scheduled to appear at a concert or festival.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "scheduled_list",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The music festival features an impressive <w>lineup</w> of international artists.",
                            "target_lang_translations": {
                                "ru": "На музыкальном фестивале представлен впечатляющий <w>состав</w> международных артистов."
                            }
                        },
                        {
                            "text": "Fans were excited to see their favorite band included in this year's <w>lineup</w>.",
                            "target_lang_translations": {
                                "ru": "Фанаты были рады увидеть свою любимую группу в <w>списке участников</w> этого года."
                            }
                        }
                    ],
                    "common_phrases": [
                        "festival lineup",
                        "concert lineup",
                        "star-studded lineup"
                    ],
                    "target_lang_definitions": {
                        "ru": "Список исполнителей или групп, заявленных для участия в концерте или фестивале."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "состав участников",
                                "target_lang_sense_clarification": "концерта, фестиваля"
                            },
                            {
                                "target_lang_word": "программа",
                                "target_lang_sense_clarification": "мероприятия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aef8e38f-144e-396f-9397-9f8d1c408bf8",
                    "sense_definition": "The specific members forming a music group or band at a particular time.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "personnel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The band returned to the stage with a new <w>lineup</w> after the lead singer left.",
                            "target_lang_translations": {
                                "ru": "Группа вернулась на сцену в новом <w>составе</w> после ухода солиста."
                            }
                        },
                        {
                            "text": "Many fans prefer the original <w>lineup</w> of the group over the current members.",
                            "target_lang_translations": {
                                "ru": "Многие фанаты предпочитают оригинальный <w>состав</w> группы нынешним участникам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "original lineup",
                        "new lineup",
                        "band lineup"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретные участники, составляющие музыкальную группу в определенное время."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "состав",
                                "target_lang_sense_clarification": "музыкальной группы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15b83551-928f-36b6-9c14-a49fe8bf22e7",
                    "sense_definition": "A row of people, including a suspect, shown to a witness to identify the perpetrator of a crime.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_line",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The witness was able to pick the thief out of a police <w>lineup</w>.",
                            "target_lang_translations": {
                                "ru": "Свидетель смог указать на вора во время полицейского <w>опознания</w>."
                            }
                        },
                        {
                            "text": "He was placed in a <w>lineup</w> with five other men of similar height and build.",
                            "target_lang_translations": {
                                "ru": "Его поставили в <w>ряд</w> с пятью другими мужчинами похожего роста и телосложения."
                            }
                        }
                    ],
                    "synonyms": [
                        "identity parade"
                    ],
                    "common_phrases": [
                        "police lineup",
                        "pick out of a lineup"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ряд людей, включая подозреваемого, предъявляемый свидетелю для опознания преступника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опознание",
                                "target_lang_sense_clarification": "полицейская процедура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45668805-131d-3346-a1c6-023f5153eb05",
                    "sense_definition": "A range of products or services offered by a specific manufacturer or brand.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "scheduled_list",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company has added several electric vehicles to its product <w>lineup</w>.",
                            "target_lang_translations": {
                                "ru": "Компания добавила несколько электромобилей в свою продуктовую <w>линейку</w>."
                            }
                        },
                        {
                            "text": "Their fall <w>lineup</w> includes new smartphones and tablets.",
                            "target_lang_translations": {
                                "ru": "Их осенний <w>модельный ряд</w> включает новые смартфоны и планшеты."
                            }
                        }
                    ],
                    "common_phrases": [
                        "product lineup",
                        "model lineup",
                        "vehicle lineup"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ассортимент товаров или услуг, предлагаемых определенным брендом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "линейка",
                                "target_lang_sense_clarification": "продукции"
                            },
                            {
                                "target_lang_word": "модельный ряд",
                                "target_lang_sense_clarification": "техники"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
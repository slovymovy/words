            "senses": [
                {
                    "sense_id": "cc625457-13bf-3ed9-9e6c-c657513b31ab",
                    "sense_definition": "Wydzielone pomieszczenie w domu lub mieszkaniu służące do przechowywania odzieży, obuwia i ubierania się.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "room_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marzę o dużej <w>garderobie</w> z wielkim lustrem.",
                            "target_lang_translations": {
                                "ru": "Я мечтаю о большой <w>гардеробной</w> с огромным зеркалом."
                            }
                        },
                        {
                            "text": "Z sypialni wchodziło się bezpośrednio do przestronnej <w>garderoby</w>.",
                            "target_lang_translations": {
                                "ru": "Из спальни можно было попасть прямо в просторную <w>гардеробную</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wejść do garderoby",
                        "pojemna garderoba",
                        "drzwi do garderoby"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельное помещение в доме или квартире для хранения одежды, обуви и переодевания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гардеробная",
                                "target_lang_sense_clarification": "комната в доме"

                {
                    "sense_id": "78cdf382-05a3-35c1-85c8-a2d4e3815653",
                    "sense_definition": "Использовать голос для исполнения музыкальных звуков или песен",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "music_performance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Моя сестра очень любит <w>петь</w> весёлые песни.",
                            "target_lang_translations": {
                                "en": "My sister really loves to <w>sing</w> cheerful songs."
                            }
                        },
                        {
                            "text": "Мы будем громко <w>петь</w> в хоре на празднике.",
                            "target_lang_translations": {
                                "en": "We will <w>sing</w> loudly in the choir at the celebration."
                            }
                        },
                        {
                            "text": "Она умеет красиво <w>петь</w> под гитару.",
                            "target_lang_translations": {
                                "en": "She knows how to <w>sing</w> beautifully to the guitar."
                            }
                        }
                    ],
                    "synonyms": [
                        "воспевать",
                        "восхвалять"
                    ],
                    "antonyms": [
                        "молчать"
                    ],
                    "common_phrases": [
                        "петь песню",
                        "петь в хоре",
                        "громко петь",
                        "петь под гитару",
                        "красиво петь"
                    ],
                    "target_lang_definitions": {
                        "en": "To use the voice to perform musical sounds or songs"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sing",
                                "target_lang_sense_clarification": "to produce musical sounds with the voice"
                            }
                        ]
                    }
                },
                {

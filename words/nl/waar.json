{
    "word_family": [
        "waarheid",
        "waarmaken",
        "waarachtig",
        "onwaar",
        "voorwaar",
        "waarlijk",
        "waarnemen"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "77b4f318-691b-35ed-ae12-653f72f7ba55",
                    "sense_definition": "Vragend: op welke plaats of plek?",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "locatie_vraag",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Waar</w> heb je mijn sleutels gelaten?",
                            "target_lang_translations": {
                                "en": "<w>Where</w> did you leave my keys?",
                                "ru": "<w>Где</w> ты оставил мои ключи?"
                            }
                        },
                        {
                            "text": "Ik vraag me af <w>waar</w> het dichtstbijzijnde station is.",
                            "target_lang_translations": {
                                "en": "I wonder <w>where</w> the nearest station is.",
                                "ru": "Интересно, <w>где</w> находится ближайшая станция."
                            }
                        }
                    ],
                    "common_phrases": [
                        "waar ben je",
                        "waar kom je vandaan",
                        "waar ga je heen"
                    ],
                    "target_lang_definitions": {
                        "en": "Interrogative: in what place or spot?",
                        "ru": "Вопросительное слово: в каком месте?"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "where",
                                "target_lang_sense_clarification": "interrogative"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "где",
                                "target_lang_sense_clarification": "вопросительное слово"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f695eed-dff3-39b0-9929-83684b7a73c9",
                    "sense_definition": "Betrekkelijk: de plaats die of dat; verwijst terug naar een locatie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "locatie_referentie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is het huis <w>waar</w> ik tien jaar gewoond heb.",
                            "target_lang_translations": {
                                "en": "This is the house <w>where</w> I lived for ten years.",
                                "ru": "Это дом, <w>где</w> я прожил десять лет."
                            }
                        },
                        {
                            "text": "Hij wees naar de plek <w>waar</w> het ongeluk gebeurde.",
                            "target_lang_translations": {
                                "en": "He pointed to the spot <w>where</w> the accident happened.",
                                "ru": "Он указал на место, <w>где</w> произошла авария."
                            }
                        }
                    ],
                    "common_phrases": [
                        "daar waar",
                        "de plek waar"
                    ],
                    "target_lang_definitions": {
                        "en": "Relative: the place which or that; refers back to a location.",
                        "ru": "Относительное слово: место, которое; отсылает к определенной локации."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "where",
                                "target_lang_sense_clarification": "relative"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "где",
                                "target_lang_sense_clarification": "союзное слово"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5ac3347-b8ba-3a38-b7fd-2f414ddb7a63",
                    "sense_definition": "Onderdeel van een voornaamwoordelijk bijwoord; vervangt 'wat' in combinatie met een voorzetsel.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "pronominaal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Waar</w> wacht je eigenlijk op?",
                            "target_lang_translations": {
                                "en": "<w>What</w> are you actually waiting for?",
                                "ru": "<w>Чего</w> ты, собственно, ждешь?"
                            }
                        },
                        {
                            "text": "Dat is iets <w>waar</w> ik liever niet over praat.",
                            "target_lang_translations": {
                                "en": "That is something <w>that</w> I would rather not talk about.",
                                "ru": "Это то, о <w>чём</w> я предпочел бы не говорить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "waar ... mee",
                        "waar ... op",
                        "waar ... over"
                    ],
                    "target_lang_definitions": {
                        "en": "Part of a pronominal adverb; replaces 'what' or 'which' in combination with a preposition.",
                        "ru": "Часть местоименного наречия; заменяет 'что' в сочетании с предлогом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "what",
                                "target_lang_sense_clarification": "in questions (e.g., what for)"
                            },
                            {
                                "target_lang_word": "which",
                                "target_lang_sense_clarification": "relative reference"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "в сочетании с предлогом (на что, о чем)"
                            },
                            {
                                "target_lang_word": "чего",
                                "target_lang_sense_clarification": "форма родительного падежа (чего ждешь)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "25181d98-c947-3f07-8508-277349352024",
                    "sense_definition": "In overeenstemming met de werkelijkheid; niet gelogen of verzonnen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "waarheid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is <w>waar</w> dat hij gisteren niet op zijn werk was.",
                            "target_lang_translations": {
                                "en": "It is <w>true</w> that he was not at work yesterday.",
                                "ru": "Это <w>правда</w>, что его вчера не было на работе."
                            }
                        },
                        {
                            "text": "Het leek te mooi om <w>waar</w> te zijn, en dat was het ook.",
                            "target_lang_translations": {
                                "en": "It seemed too good to be <w>true</w>, and it was.",
                                "ru": "Это казалось слишком хорошим, чтобы быть <w>правдой</w>, так оно и было."
                            }
                        }
                    ],
                    "synonyms": [
                        "correct",
                        "kloppend",
                        "echt",
                        "reëel"
                    ],
                    "antonyms": [
                        "onwaar",
                        "vals",
                        "onecht"
                    ],
                    "common_phrases": [
                        "echt waar",
                        "niet waar",
                        "dat is waar"
                    ],
                    "target_lang_definitions": {
                        "en": "In accordance with reality; not lied or invented.",
                        "ru": "Соответствующий действительности; не ложь и не выдумка."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "true",
                                "target_lang_sense_clarification": "factually correct"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "правда",
                                "target_lang_sense_clarification": "истина, соответствие фактам"
                            },
                            {
                                "target_lang_word": "верный",
                                "target_lang_sense_clarification": "правильный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc74fa66-b4ef-3b96-981a-83378c4f5a65",
                    "sense_definition": "Authentiek of oprecht; zoals het hoort te zijn.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "authenticiteit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij is een <w>ware</w> vriendin die me altijd steunt.",
                            "target_lang_translations": {
                                "en": "She is a <w>true</w> friend who always supports me.",
                                "ru": "Она <w>настоящая</w> подруга, которая всегда меня поддерживает."
                            }
                        },
                        {
                            "text": "Hij toonde zijn <w>ware</w> gezicht toen hij boos werd.",
                            "target_lang_translations": {
                                "en": "He showed his <w>true</w> colors when he got angry.",
                                "ru": "Он показал свое <w>истинное</w> лицо, когда разозлился."
                            }
                        }
                    ],
                    "synonyms": [
                        "authentiek",
                        "echt"
                    ],
                    "antonyms": [
                        "onecht",
                        "vals"
                    ],
                    "common_phrases": [
                        "ware liefde",
                        "een ware held"
                    ],
                    "target_lang_definitions": {
                        "en": "Authentic or sincere; as it ought to be.",
                        "ru": "Подлинный или искренний; такой, каким должен быть."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "true",
                                "target_lang_sense_clarification": "authentic"
                            },
                            {
                                "target_lang_word": "real",
                                "target_lang_sense_clarification": "genuine"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "настоящий",
                                "target_lang_sense_clarification": "подлинный"
                            },
                            {
                                "target_lang_word": "истинный",
                                "target_lang_sense_clarification": "неподдельный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "43d5eb06-055e-3a26-9b29-ccb930683037",
                    "sense_definition": "Terwijl; drukt een tegenstelling uit tussen twee situaties.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tegenstelling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Waar</w> de meeste mensen kozen voor de auto, nam hij de fiets.",
                            "target_lang_translations": {
                                "en": "<w>Whereas</w> most people chose the car, he took the bike.",
                                "ru": "<w>В то время как</w> большинство людей выбирали машину, он взял велосипед."
                            }
                        },
                        {
                            "text": "<w>Waar</w> Nederland zich zorgen maakt over de economie, groeit deze in andere landen juist.",
                            "target_lang_translations": {
                                "en": "<w>While</w> the Netherlands worries about the economy, it is actually growing in other countries.",
                                "ru": "<w>Тогда как</w> Нидерланды беспокоятся об экономике, в других странах она растет."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "While; expresses a contrast between two situations.",
                        "ru": "Тогда как; выражает противопоставление двух ситуаций."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "whereas",
                                "target_lang_sense_clarification": "contrast"
                            },
                            {
                                "target_lang_word": "while",
                                "target_lang_sense_clarification": "contrast"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "в то время как",
                                "target_lang_sense_clarification": "союз противопоставления"
                            },
                            {
                                "target_lang_word": "тогда как",
                                "target_lang_sense_clarification": "союз"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9136990b-064f-36b1-8f9d-6a8849298d24",
                    "sense_definition": "Goederen of producten die bedoeld zijn voor de verkoop.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "handel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De marktkoopman prees zijn verse <w>waar</w> luidkeels aan.",
                            "target_lang_translations": {
                                "en": "The market merchant loudly praised his fresh <w>wares</w>.",
                                "ru": "Торговец громко расхваливал свой свежий <w>товар</w>."
                            }
                        },
                        {
                            "text": "Je moet goed controleren of de geleverde <w>waar</w> niet beschadigd is.",
                            "target_lang_translations": {
                                "en": "You must check carefully whether the delivered <w>goods</w> are not damaged.",
                                "ru": "Нужно хорошо проверить, не поврежден ли доставленный <w>товар</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "goederen",
                        "authentiek"
                    ],
                    "common_phrases": [
                        "verse waar"
                    ],
                    "target_lang_definitions": {
                        "en": "Goods or products intended for sale.",
                        "ru": "Товары или продукты, предназначенные для продажи."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "merchandise",
                                "target_lang_sense_clarification": "products for sale"
                            },
                            {
                                "target_lang_word": "wares",
                                "target_lang_sense_clarification": "goods offered for sale"
                            },
                            {
                                "target_lang_word": "goods",
                                "target_lang_sense_clarification": "commercial items"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "товар",
                                "target_lang_sense_clarification": "предмет торговли"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
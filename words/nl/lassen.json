                {
                    "sense_id": "16d71621-3319-3cba-b5f4-a7eb06babbf4",
                    "sense_definition": "Door plaatselijke sterke verhitting twee metalen voorwerpen met elkaar verbinden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Van roestvrij stalen buis <laste> ik het frame.",
                            "target_lang_translations": {
                                "en": "I <w>welded</w> the frame from stainless steel pipe.",
                                "ru": "Я <w>сварил</w> раму из трубы из нержавеющей стали."
                            }
                        },
                        {
                            "text": "De arbeider moest de twee platen stevig aan elkaar <lassen>.",
                            "target_lang_translations": {
                                "en": "The worker had to <w>weld</w> the two plates firmly together.",
                                "ru": "Рабочий должен был прочно <w>сварить</w> две пластины друг с другом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aan elkaar lassen",
                        "elektrisch lassen"
                    ],
                    "target_lang_definitions": {
                        "en": "To join two metal objects together by applying intense heat.",
                        "ru": "Соединять два металлических предмета друг с другом путем сильного местного нагрева."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "weld",
                                "target_lang_sense_clarification": "join metal by heating"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сваривать",
                                "target_lang_sense_clarification": "соединять металл"
                            },
                            {
                                "target_lang_word": "варить",
                                "target_lang_sense_clarification": "разговорное, профессиональное"
                            }
                        ]
                    }
                }
            ]
        },

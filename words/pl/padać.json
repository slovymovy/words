    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "29f472c3-21a1-3913-ad44-9d46bf21975c",
                    "sense_definition": "O opadach atmosferycznych (deszczu, śniegu, gradzie): lecieć z chmur na ziemię.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziś od samego rana <pada> gęsty śnieg.",
                            "target_lang_translations": {
                                "ru": "Сегодня с самого утра <падает> густой снег."
                            }
                        },
                        {
                            "text": "Jesienią w Polsce często <pada> deszcz.",
                            "target_lang_translations": {
                                "ru": "Осенью в Польше часто <идёт> дождь."
                            }
                        }
                    ],
                    "synonyms": [
                        "lać",
                        "siąpić",
                        "sypać"
                    ],
                    "common_phrases": [
                        "pada deszcz",
                        "pada śnieg",
                        "pada grad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Об атмосферных осадках (дожде, снеге, граде): литься или сыпаться с облаков на землю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "о дожде или снеге"
                            },
                            {
                                "target_lang_word": "падать",
                                "target_lang_sense_clarification": "преимущественно о снеге"
                            },
                            {
                                "target_lang_word": "лить",
                                "target_lang_sense_clarification": "о сильном дожде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad35a005-d56e-3194-acc2-95672a92af7d",
                    "sense_definition": "Przewracać się na ziemię lub lecieć w dół, tracąc równowagę.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Liście powoli <padają> z drzew na chodnik.",
                            "target_lang_translations": {
                                "ru": "Листья медленно <падают> с деревьев на тротуар."
                            }
                        },
                        {
                            "text": "Zmęczeni żołnierze <padają> na ziemię, by odpocząć.",
                            "target_lang_translations": {
                                "ru": "Уставшие солдаты <падают> на землю, чтобы отдохнуть."
                            }
                        }
                    ],
                    "synonyms": [
                        "upadać",
                        "przewracać się",
                        "spadać"
                    ],
                    "antonyms": [
                        "wstawać"
                    ],
                    "common_phrases": [
                        "padać na twarz",
                        "padać na kolana"
                    ],
                    "target_lang_definitions": {
                        "ru": "Опрокидываться на землю или лететь вниз, теряя равновесие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "падать",
                                "target_lang_sense_clarification": "терять равновесие"
                            },
                            {
                                "target_lang_word": "валиться",
                                "target_lang_sense_clarification": "обрушиваться вниз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a4fff4a-f5ef-3766-8a50-6c2681f5bf28",
                    "sense_definition": "Być bardzo zmęczonym, tracić siły fizyczne.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po całym dniu ciężkiej pracy po prostu <padam>.",
                            "target_lang_translations": {
                                "ru": "После целого дня тяжелой работы я просто <валюсь с ног>."
                            }
                        },
                        {
                            "text": "Wracamy z górskiej wycieczki i <padamy> ze zmęczenia.",
                            "target_lang_translations": {
                                "ru": "Мы возвращаемся с горной экскурсии и <падаем> от усталости."
                            }
                        }
                    ],
                    "synonyms": [
                        "słaniać się"
                    ],
                    "common_phrases": [
                        "padać z nóg",
                        "padać ze zmęczenia",
                        "padać na pysk"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть очень уставшим, терять физические силы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "валиться с ног",
                                "target_lang_sense_clarification": "от усталости"
                            },
                            {
                                "target_lang_word": "падать",
                                "target_lang_sense_clarification": "от изнеможения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52d735ae-3ae3-3e37-9c33-d1fecf1238d5",
                    "sense_definition": "O urządzeniach lub bateriach: przestawać działać, psuć się lub wyczerpywać energię.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój stary laptop często <pada> bez ostrzeżenia.",
                            "target_lang_translations": {
                                "ru": "Мой старый ноутбук часто <вырубается> без предупреждения."
                            }
                        },
                        {
                            "text": "Bateria w telefonie mi <pada>, muszę kończyć rozmowę.",
                            "target_lang_translations": {
                                "ru": "Батарея в телефоне <садится>, мне нужно заканчивать разговор."
                            }
                        }
                    ],
                    "synonyms": [
                        "psuć się",
                        "wysiadać"
                    ],
                    "common_phrases": [
                        "bateria pada",
                        "system pada",
                        "serwer pada"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Об устройствах или батареях: переставать работать, ломаться или разряжаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "садиться",
                                "target_lang_sense_clarification": "о батарее/аккумуляторе"
                            },
                            {
                                "target_lang_word": "падать",
                                "target_lang_sense_clarification": "о сервере или системе (слэнг)"
                            },
                            {
                                "target_lang_word": "ломаться",
                                "target_lang_sense_clarification": "выходить из строя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c88949f-6303-385d-acee-45cfdb78e747",
                    "sense_definition": "Pojawiać się w rozmowie, być wypowiadanym (np. o słowach, pytaniach, propozycjach).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas dyskusji często <padają> trudne pytania.",
                            "target_lang_translations": {
                                "ru": "Во время дискуссии часто <звучат> трудные вопросы."
                            }
                        },
                        {
                            "text": "W tym towarzystwie rzadko <padają> wulgarne słowa.",
                            "target_lang_translations": {
                                "ru": "В этой компании редко <звучат> вульгарные слова."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pada pytanie",
                        "pada propozycja",
                        "pada odpowiedź"
                    ],
                    "target_lang_definitions": {
                        "ru": "Появляться в разговоре, быть произнесённым (о словах, вопросах, предложениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звучать",
                                "target_lang_sense_clarification": "быть произнесённым"
                            },
                            {
                                "target_lang_word": "раздаваться",
                                "target_lang_sense_clarification": "слышаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e83cbe17-2c88-3f10-9ec8-d1ef87912243",
                    "sense_definition": "Umierać (zwykle o zwierzętach) lub ginąć masowo i gwałtownie (o ludziach).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas epidemii zwierzęta gospodarskie <padały> jedno po drugim.",
                            "target_lang_translations": {
                                "ru": "Во время эпидемии домашний скот <дох> один за другим."
                            }
                        },
                        {
                            "text": "Wielu żołnierzy <padało> na polu bitwy.",
                            "target_lang_translations": {
                                "ru": "Много солдат <гибло> на поле битвы."
                            }
                        }
                    ],
                    "synonyms": [
                        "ginąć",
                        "zdychać",
                        "umierać"
                    ],
                    "common_phrases": [
                        "padać jak muchy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Умирать (обычно о животных) или гибнуть массово и насильственно (о людях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дохнуть",
                                "target_lang_sense_clarification": "о животных"
                            },
                            {
                                "target_lang_word": "гибнуть",
                                "target_lang_sense_clarification": "о людях (на войне)"
                            },
                            {
                                "target_lang_word": "падать",
                                "target_lang_sense_clarification": "умирать (устар./книжн.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bfe6cc22-bc13-3014-9072-28418d8fd44e",
                    "sense_definition": "O mieście lub twierdzy: poddawać się, ulegać siłom wroga.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kolejne bastiony <padały> pod naporem armii nieprzyjaciela.",
                            "target_lang_translations": {
                                "ru": "Очередные бастионы <падали> под натиском армии неприятеля."
                            }
                        },
                        {
                            "text": "Rząd <pada>, gdy traci poparcie większości w parlamencie.",
                            "target_lang_translations": {
                                "ru": "Правительство <падает>, когда теряет поддержку большинства в парламенте."
                            }
                        }
                    ],
                    "synonyms": [
                        "poddawać się",
                        "kapitulować",
                        "upadać"
                    ],
                    "common_phrases": [
                        "rząd pada",
                        "twierdza pada"
                    ],
                    "target_lang_definitions": {
                        "ru": "О городе или крепости: сдаваться, уступать силам врага."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "падать",
                                "target_lang_sense_clarification": "быть поверженным (о городе/правительстве)"
                            },
                            {
                                "target_lang_word": "сдаваться",
                                "target_lang_sense_clarification": "капитулировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a35ae52a-4c58-3185-ac88-10d67b213ec5",
                    "sense_definition": "Mówić, opowiadać (używane w gwarze).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "dialect_speech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starka <padali>, że dawniej żyło się tu inaczej.",
                            "target_lang_translations": {
                                "ru": "Бабушка <говорила>, что раньше здесь жилось иначе."
                            }
                        },
                        {
                            "text": "Wszyscy we wsi tak <padają>.",
                            "target_lang_translations": {
                                "ru": "Все в деревне так <говорят>."
                            }
                        }
                    ],
                    "synonyms": [
                        "mówić",
                        "gadać"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Śląsk Cieszyński"
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить, рассказывать (используется в диалекте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "говорить",
                                "target_lang_sense_clarification": "разговаривать"
                            },
                            {
                                "target_lang_word": "болтать",
                                "target_lang_sense_clarification": "разг."
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1e06d7b9-6a90-36dd-8811-bed6a0fa3503",
                    "sense_definition": "Политическое направление, признающее монархию единственной или наилучшей формой государственной власти.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ideology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В начале двадцатого века <монархизм> в стране начал терять свою популярность.",
                            "target_lang_translations": {
                                "en": "In the early twentieth century, <monarchism> in the country began to lose its popularity."
                            }
                        },
                        {
                            "text": "Русский <монархизм> имеет свои уникальные исторические и культурные особенности.",
                            "target_lang_translations": {
                                "en": "Russian <monarchism> has its own unique historical and cultural features."
                            }
                        }
                    ],
                    "synonyms": [
                        "роялизм"
                    ],
                    "antonyms": [
                        "республиканство"
                    ],
                    "common_phrases": [
                        "идеи монархизма",
                        "конституционный монархизм",
                        "русский монархизм"
                    ],
                    "target_lang_definitions": {
                        "en": "A political movement or ideology advocating for monarchy as the best form of government."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "monarchism",
                                "target_lang_sense_clarification": "political ideology"
                            },
                            {
                                "target_lang_word": "royalism",
                                "target_lang_sense_clarification": "support for a specific monarch or dynasty"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e0e457e-af07-3f47-b102-45a5420e397d",
                    "sense_definition": "Личная приверженность монархическому строю; убеждения и взгляды монархиста.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ideology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его искренний <монархизм> часто становился темой для споров с друзьями.",
                            "target_lang_translations": {
                                "en": "His sincere <monarchism> often became a subject of arguments with friends."
                            }
                        },
                        {
                            "text": "Писатель через своих героев выражал собственный <монархизм> и тоску по прошлому.",
                            "target_lang_translations": {
                                "en": "Through his characters, the writer expressed his own <monarchism> and longing for the past."
                            }
                        }
                    ],
                    "antonyms": [
                        "республиканство"
                    ],
                    "common_phrases": [
                        "убеждённый монархизм",
                        "скрывать монархизм"
                    ],
                    "target_lang_definitions": {
                        "en": "Personal belief in or support for the monarchical system."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "monarchism",
                                "target_lang_sense_clarification": "personal belief"
                            },
                            {
                                "target_lang_word": "monarchist views",
                                "target_lang_sense_clarification": "set of beliefs"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16922e64-e885-3314-a4e1-67b2ed810adf",
                    "sense_definition": "Форма государственного правления, при которой верховная власть принадлежит единоличному правителю (обычно используется слово «монархия»).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Древний <монархизм> постепенно уступал место более демократичным институтам власти.",
                            "target_lang_translations": {
                                "en": "Ancient <monarchism> gradually gave way to more democratic institutions of power."
                            }
                        },
                        {
                            "text": "Критики считали, что абсолютный <монархизм> тормозит экономическое развитие государства.",
                            "target_lang_translations": {
                                "en": "Critics believed that absolute <monarchism> hinders the economic development of the state."
                            }
                        }
                    ],
                    "antonyms": [
                        "демократия"
                    ],
                    "target_lang_definitions": {
                        "en": "A system of government ruled by a monarch (often referred to as monarchy)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "monarchy",
                                "target_lang_sense_clarification": "form of government"
                            },
                            {
                                "target_lang_word": "monarchism",
                                "target_lang_sense_clarification": "system of rule"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
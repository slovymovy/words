                {
                    "sense_id": "7b92144f-87cd-3374-b8d0-9899bd9c5990",
                    "sense_definition": "De toestand of eigenschap van vlak en effen zijn.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De aannemer controleerde de <w>vlakte</w> van de pas gestorte vloer.",
                            "target_lang_translations": {
                                "en": "The contractor checked the <w>flatness</w> of the newly poured floor.",
                                "ru": "Подрядчик проверил <w>ровность</w> только что залитого пола."
                            }
                        },
                        {
                            "text": "Tijdens het lastige gesprek hield hij zich wijselijk op de <w>vlakte</w>.",
                            "target_lang_translations": {
                                "en": "During the difficult conversation, he wisely remained <w>non-committal</w>.",
                                "ru": "Во время трудного разговора он мудро держался <w>нейтрально</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "vlakheid",
                        "effenheid"
                    ],
                    "antonyms": [
                        "oneffenheid",
                        "ruwheid"
                    ],
                    "common_phrases": [
                        "zich op de vlakte houden"
                    ],
                    "target_lang_definitions": {
                        "en": "The quality or state of being flat, smooth, or level.",
                        "ru": "Состояние или свойство быть плоским и ровным."
                    },

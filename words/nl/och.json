{
    "word_family": [
        "ocharm"
    ],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "a2d734ea-b91f-3b51-8b3b-566e393aa745",
                    "sense_definition": "Uitroep om medelijden of verdriet uit te drukken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emotion_pity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heeft ze haar been gebroken? <w>Och</w>, wat zielig voor haar!",
                            "target_lang_translations": {
                                "en": "Did she break her leg? <w>Oh dear</w>, how sad for her!"
                            }
                        },
                        {
                            "text": "<w>Och</w> arme jongen, heb je je pijn gedaan?",
                            "target_lang_translations": {
                                "en": "<w>Oh</w> poor boy, did you hurt yourself?"
                            }
                        }
                    ],
                    "synonyms": [
                        "ach"
                    ],
                    "common_phrases": [
                        "och arme",
                        "och god",
                        "och heden"
                    ],
                    "target_lang_definitions": {
                        "en": "Exclamation to express pity or sadness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "oh dear",
                                "target_lang_sense_clarification": "expression of sympathy or concern"
                            },
                            {
                                "target_lang_word": "oh",
                                "target_lang_sense_clarification": "general expression of emotion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d0cca12-7315-347e-8cb8-7b4019cde74f",
                    "sense_definition": "Uitroep om een situatie te relativeren, goed te praten of iemand gerust te stellen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mitigation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Och</w>, dat bedoelt hij vast niet zo gemeen als het klinkt.",
                            "target_lang_translations": {
                                "en": "<w>Oh</w>, he probably doesn't mean it as meanly as it sounds."
                            }
                        },
                        {
                            "text": "Ben je je boek vergeten? <w>Och</w>, dat geeft helemaal niets.",
                            "target_lang_translations": {
                                "en": "Did you forget your book? <w>Oh</w>, that doesn't matter at all."
                            }
                        }
                    ],
                    "synonyms": [
                        "ach"
                    ],
                    "common_phrases": [
                        "och ja",
                        "och kom",
                        "och nee"
                    ],
                    "target_lang_definitions": {
                        "en": "Exclamation to relativize a situation, make excuses, or reassure someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "oh",
                                "target_lang_sense_clarification": "dismissive or reassuring"
                            },
                            {
                                "target_lang_word": "well",
                                "target_lang_sense_clarification": "used to soften a statement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e4ee4a1-2aca-3b30-9376-ceab521679e6",
                    "sense_definition": "Uitroep van vertedering bij het zien van iets liefs of moois.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion_endearment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Och</w>, wat een schatje is die kleine puppy!",
                            "target_lang_translations": {
                                "en": "<w>Aww</w>, what a cutie that little puppy is!"
                            }
                        },
                        {
                            "text": "Kijk nou naar die foto, <w>och</w> wat lief.",
                            "target_lang_translations": {
                                "en": "Just look at that photo, <w>aww</w> how sweet."
                            }
                        }
                    ],
                    "synonyms": [
                        "ach",
                        "oh",
                        "o"
                    ],
                    "common_phrases": [
                        "och wat lief",
                        "och wat mooi",
                        "och wat leuk"
                    ],
                    "target_lang_definitions": {
                        "en": "Exclamation of endearment upon seeing something sweet or beautiful."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "aww",
                                "target_lang_sense_clarification": "sound of endearment"
                            },
                            {
                                "target_lang_word": "oh",
                                "target_lang_sense_clarification": "expression of delight"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09aec542-61e6-3ae2-a0c9-865519e09d13",
                    "sense_definition": "Uitroep aan het begin van een wens of verzuchting, vaak over iets dat niet meer kan veranderen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "wish_regret",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Och</w>, was ik maar bij moeder thuisgebleven.",
                            "target_lang_translations": {
                                "en": "<w>Oh</w>, if only I had stayed home with mother."
                            }
                        },
                        {
                            "text": "<w>Och</w>, had ik maar eerder geweten dat je zou komen.",
                            "target_lang_translations": {
                                "en": "<w>Oh</w>, if only I had known sooner that you were coming."
                            }
                        }
                    ],
                    "synonyms": [
                        "ach"
                    ],
                    "common_phrases": [
                        "och was ik maar",
                        "och mocht het"
                    ],
                    "target_lang_definitions": {
                        "en": "Exclamation at the start of a wish or sigh, often regarding something unchangeable."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "oh",
                                "target_lang_sense_clarification": "sigh of regret"
                            },
                            {
                                "target_lang_word": "ah",
                                "target_lang_sense_clarification": "expression of longing"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "57b58d7d-3c6e-381f-b54d-88f684eef375",
                    "sense_definition": "Een levend wezen dat geen mens en geen plant is; een dier.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animal_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pas op, dat <w>beest</w> kan hard bijten.",
                            "target_lang_translations": {
                                "en": "Watch out, that <w>beast</w> can bite hard.",
                                "ru": "Осторожно, этот <w>зверь</w> может сильно укусить."
                            }
                        },
                        {
                            "text": "In de dierentuin zagen we een groot en gevaarlijk <w>beest</w>.",
                            "target_lang_translations": {
                                "en": "In the zoo we saw a large and dangerous <w>beast</w>.",
                                "ru": "В зоопарке мы видели большого и опасного <w>зверя</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dier"
                    ],
                    "antonyms": [
                        "mens",
                        "plant"
                    ],
                    "common_phrases": [
                        "wild beest",
                        "aard van het beestje",
                        "vreemd beest"
                    ],
                    "target_lang_definitions": {
                        "en": "A living creature that is not a human or a plant; an animal.",
                        "ru": "Живое существо, не являющееся ни человеком, ни растением; животное."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beast",
                                "target_lang_sense_clarification": "often implies a large or wild animal"
                            },
                            {
                                "target_lang_word": "animal",
                                "target_lang_sense_clarification": "general term"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "зверь",
                                "target_lang_sense_clarification": "дикое или крупное животное"
                            },
                            {
                                "target_lang_word": "животное",
                                "target_lang_sense_clarification": "общее понятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13e2d870-dad8-3248-a293-685254351909",
                    "sense_definition": "Een persoon die zich wreed, onbeschoft, wild of onmenselijk gedraagt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dronken man schreeuwde en gedroeg zich als een <w>beest</w>.",
                            "target_lang_translations": {
                                "en": "The drunken man screamed and behaved like a <w>beast</w>.",
                                "ru": "Пьяный мужчина кричал и вел себя как <w>зверь</w>."
                            }
                        },
                        {
                            "text": "Ze noemden de dictator een <w>beest</w> vanwege zijn vreselijke daden.",
                            "target_lang_translations": {
                                "en": "They called the dictator a <w>beast</w> because of his terrible deeds.",
                                "ru": "Диктатора называли <w>зверем</w> из-за его ужасных деяний."
                            }
                        }
                    ],
                    "synonyms": [
                        "onmens",
                        "barbaar",
                        "monster"
                    ],
                    "common_phrases": [
                        "zich gedragen als een beest",
                        "het beest uithangen"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who behaves in a cruel, rude, wild, or inhumane manner.",
                        "ru": "Человек, который ведет себя жестоко, грубо, дико или бесчеловечно."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beast",
                                "target_lang_sense_clarification": "a cruel or uncivilized person"
                            },
                            {
                                "target_lang_word": "brute",
                                "target_lang_sense_clarification": "a coarse or insensitive person"
                            },
                            {
                                "target_lang_word": "animal",
                                "target_lang_sense_clarification": "someone behaving without social restraint"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "зверь",
                                "target_lang_sense_clarification": "жестокий или дикий человек"
                            },
                            {
                                "target_lang_word": "скотина",
                                "target_lang_sense_clarification": "грубый, неприятный человек"
                            },
                            {
                                "target_lang_word": "животное",
                                "target_lang_sense_clarification": "о человеке без моральных устоев"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8d582c3-af27-3110-a07d-c4dac82cb38b",
                    "sense_definition": "Iemand die bewondering oproept door uitzonderlijke fysieke kracht, uithoudingsvermogen of prestaties.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij traint elke dag en is echt een <w>beest</w> in de sportschool.",
                            "target_lang_translations": {
                                "en": "He trains every day and is truly a <w>beast</w> in the gym.",
                                "ru": "Он тренируется каждый день, он просто <w>зверь</w> в спортзале."
                            }
                        },
                        {
                            "text": "Die verdediger is een <w>beest</w>, niemand komt erlangs.",
                            "target_lang_translations": {
                                "en": "That defender is a <w>beast</w>, no one gets past him.",
                                "ru": "Этот защитник — <w>зверь</w>, никто не может его пройти."
                            }
                        }
                    ],
                    "synonyms": [
                        "krachtpatser",
                        "kanjer"
                    ],
                    "antonyms": [
                        "zwakkeling"
                    ],
                    "common_phrases": [
                        "wat een beest"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "used to express admiration for strength or endurance"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who evokes admiration through exceptional physical strength, endurance, or performance.",
                        "ru": "Тот, кто вызывает восхищение исключительной физической силой, выносливостью или результатами."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beast",
                                "target_lang_sense_clarification": "slang for someone very strong or skilled"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "зверь",
                                "target_lang_sense_clarification": "сленг: очень сильный или умелый человек"
                            },
                            {
                                "target_lang_word": "машина",
                                "target_lang_sense_clarification": "сленг: неутомимый человек"
                            }
                        ]
                    }
                }
            ]
        },
        {

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5467ab02-b63c-3a00-bb71-120079d11c4f",
                    "sense_definition": "Datgene wat gunstig of voordelig is voor de aandeelhouders van een bedrijf, vaak met betrekking tot financiële waarde.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De directeur stelt het <w>aandeelhoudersbelang</w> boven de belangen van het personeel.",
                            "target_lang_translations": {
                                "ru": "Директор ставит <w>интересы акционеров</w> выше интересов персонала."
                            }
                        },
                        {
                            "text": "Het bestuur moet het <w>aandeelhoudersbelang</w> dienen om investeerders tevreden te houden.",
                            "target_lang_translations": {
                                "ru": "Правление должно служить <w>интересам акционеров</w>, чтобы инвесторы оставались довольны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het aandeelhoudersbelang dienen",
                        "opkomen voor aandeelhoudersbelangen"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что приносит выгоду или пользу акционерам компании, часто в финансовом отношении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "интересы акционеров",
                                "target_lang_sense_clarification": "выгода или польза для владельцев акций"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9408747d-961c-3112-a436-70bafe3f15f1",
                    "sense_definition": "Het bezit van een hoeveelheid aandelen in een onderneming.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft zijn <w>aandeelhoudersbelang</w> in het bedrijf vorig jaar verkocht.",
                            "target_lang_translations": {
                                "ru": "В прошлом году он продал свою <w>акционерную долю</w> в компании."
                            }
                        },
                        {
                            "text": "Door de overname is hun <w>aandeelhoudersbelang</w> aanzienlijk verwaterd.",
                            "target_lang_translations": {
                                "ru": "В результате поглощения их <w>доля участия</w> значительно размылась."
                            }
                        }
                    ],
                    "synonyms": [
                        "aandelenbelang",
                        "participatie"
                    ],
                    "common_phrases": [
                        "een aandeelhoudersbelang hebben",
                        "aandeelhoudersbelang vergroten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Владение определенным количеством акций предприятия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "акционерная доля",
                                "target_lang_sense_clarification": "доля владения в компании"
                            },
                            {
                                "target_lang_word": "доля участия",
                                "target_lang_sense_clarification": "участие в капитале"
                            }
                        ]
                    }
                }
            ]
        }
    ]

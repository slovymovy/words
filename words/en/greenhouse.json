    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4a09dddf-02c7-3e65-ad24-181c91ecdb47",
                    "sense_definition": "A building with glass or plastic roof and walls, used to grow plants that need protection from cold weather or controlled conditions.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "building",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We grow tomatoes and peppers in the <w>greenhouse</w> throughout the winter.",
                            "target_lang_translations": {
                                "ru": "Мы выращиваем помидоры и перцы в <w>теплице</w> всю зиму."
                            }
                        },
                        {
                            "text": "The sun warmed the air inside the <w>greenhouse</w>, creating a tropical environment.",
                            "target_lang_translations": {
                                "ru": "Солнце нагрело воздух внутри <w>теплицы</w>, создав тропическую среду."
                            }
                        }
                    ],
                    "synonyms": [
                        "glasshouse",
                        "hothouse",
                        "conservatory",
                        "plant-house"
                    ],
                    "antonyms": [
                        "icehouse"
                    ],
                    "common_phrases": [
                        "greenhouse gas",
                        "greenhouse effect",
                        "commercial greenhouse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Строение со стеклянными или пластиковыми стенами и крышей, используемое для выращивания растений, требующих защиты от холода или контролируемых условий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "теплица",
                                "target_lang_sense_clarification": "отапливаемое помещение для выращивания растений"
                            },
                            {
                                "target_lang_word": "парник",
                                "target_lang_sense_clarification": "небольшое, обычно неотапливаемое сооружение"
                            },
                            {
                                "target_lang_word": "оранжерея",
                                "target_lang_sense_clarification": "помещение для экзотических растений, зимний сад"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70349db0-58c6-319e-b71e-f0796136e36b",
                    "sense_definition": "A state of global climate characterized by high temperatures due to the trapping of heat.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Scientists study ancient rock formations to understand past <w>greenhouse</w> periods on Earth.",
                            "target_lang_translations": {
                                "ru": "Ученые изучают древние скальные образования, чтобы понять прошлые <w>парниковые</w> периоды на Земле."
                            }
                        },
                        {
                            "text": "The planet may be entering a permanent <w>greenhouse</w> state if emissions are not reduced.",
                            "target_lang_translations": {
                                "ru": "Планета может войти в постоянное <w>парниковое</w> состояние, если выбросы не будут сокращены."
                            }
                        }
                    ],
                    "common_phrases": [
                        "greenhouse earth"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "climatology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние глобального климата, характеризующееся высокими температурами из-за удержания тепла атмосферой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "парниковый",
                                "target_lang_sense_clarification": "относящийся к эффекту потепления климата"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4584af4-aa58-3d5c-92b3-03c3ce5213b1",
                    "sense_definition": "The glass covering of an airplane's cockpit.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pilot scanned the sky through the <w>greenhouse</w> of his fighter plane.",
                            "target_lang_translations": {
                                "ru": "Пилот осматривал небо через <w>фонарь</w> своего истребителя."
                            }
                        },
                        {
                            "text": "Sunlight reflected sharply off the <w>greenhouse</w> as the aircraft turned.",
                            "target_lang_translations": {
                                "ru": "Солнечный свет резко отразился от <w>остекления кабины</w> при развороте самолета."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "aviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Стеклянное покрытие кабины пилота самолета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фонарь",
                                "target_lang_sense_clarification": "прозрачная часть кабины пилота"
                            },
                            {
                                "target_lang_word": "остекление кабины",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e91acc0-3c99-3e63-8c46-974e7b74103b",
                    "sense_definition": "A structure placed over an operating table to protect the patient from bacteria during surgery.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The surgeon performed the delicate procedure inside a sterile <w>greenhouse</w>.",
                            "target_lang_translations": {
                                "ru": "Хирург выполнил тонкую процедуру внутри стерильного <w>бокса</w>."
                            }
                        },
                        {
                            "text": "Specialized airflow within the <w>greenhouse</w> kept the surgical area free of contaminants.",
                            "target_lang_translations": {
                                "ru": "Специализированный поток воздуха внутри <w>изолятора</w> защищал операционную зону от загрязнений."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "medical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Конструкция, размещаемая над операционным столом для защиты пациента от бактерий во время операции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стерильный бокс",
                                "target_lang_sense_clarification": "медицинское оборудование"
                            },
                            {
                                "target_lang_word": "изолятор",
                                "target_lang_sense_clarification": "защитная конструкция"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "86fa9fbb-825d-36c4-ab08-d0266d61ef92",
                    "sense_definition": "To place plants inside a greenhouse.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to <w>greenhouse</w> the tropical flowers before the first frost arrives.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>поместить в теплицу</w> тропические цветы до наступления первых заморозков."
                            }
                        },
                        {
                            "text": "The gardener <w>greenhoused</w> the seedlings to accelerate their growth.",
                            "target_lang_translations": {
                                "ru": "Садовник <w>поместил в теплицу</w> рассаду, чтобы ускорить её рост."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать растения в теплицу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помещать в теплицу",
                                "target_lang_sense_clarification": "действие с растениями"
                            },
                            {
                                "target_lang_word": "выращивать в тепличных условиях",
                                "target_lang_sense_clarification": "процесс выращивания"
                            }
                        ]

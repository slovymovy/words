{
    "word_family": [
        "allowance",
        "allowable",
        "disallow"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7183fbe7-ea14-3751-a256-6159323050ac",
                    "sense_definition": "Permitted or authorized to do something or to happen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Smoking is not <w>allowed</w> inside the office building.",
                            "target_lang_translations": {
                                "ru": "Курение внутри офисного здания не <w>разрешено</w>."
                            }
                        },
                        {
                            "text": "Are we <w>allowed</w> to bring our own food to the cinema?",
                            "target_lang_translations": {
                                "ru": "Нам <w>разрешено</w> приносить свою еду в кинотеатр?"
                            }
                        }
                    ],
                    "antonyms": [
                        "forbidden",
                        "prohibited",
                        "banned"
                    ],
                    "common_phrases": [
                        "not allowed",
                        "allowed to",
                        "legally allowed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрешённый или дозволенный для выполнения чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разрешённый",
                                "target_lang_sense_clarification": "санкционированный, не запрещённый"
                            },
                            {
                                "target_lang_word": "дозволенный",
                                "target_lang_sense_clarification": "разрешённый законом или правилами (более формально)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ccc9f6f-3c7e-3dc9-b129-b040eba3f8fb",
                    "sense_definition": "Allotted or assigned as a share or limit.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "allocation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must complete the exam within the <w>allowed</w> time.",
                            "target_lang_translations": {
                                "ru": "Вы должны завершить экзамен в течение <w>отведённого</w> времени."
                            }
                        },
                        {
                            "text": "The passenger exceeded the <w>allowed</w> weight for luggage.",
                            "target_lang_translations": {
                                "ru": "Пассажир превысил <w>допустимый</w> вес багажа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "allowed time",
                        "allowed amount",
                        "maximum allowed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выделенный или назначенный в качестве доли или лимита."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "допустимый",
                                "target_lang_sense_clarification": "о лимитах, весе, количестве"
                            },
                            {
                                "target_lang_word": "отведённый",
                                "target_lang_sense_clarification": "о времени или выделенной доле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "775ecb9e-2aa8-3d39-846f-6e3572411202",
                    "sense_definition": "Acknowledged or admitted to be true.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "acknowledgment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is generally <w>allowed</w> that the new policy has failed.",
                            "target_lang_translations": {
                                "ru": "Обще<w>признано</w>, что новая политика провалилась."
                            }
                        },
                        {
                            "text": "The <w>allowed</w> facts of the case were not open to debate.",
                            "target_lang_translations": {
                                "ru": "<w>Признанные</w> факты по делу не подлежали обсуждению."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "formal or literary context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Признанный или принятый за истину."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "признанный",
                                "target_lang_sense_clarification": "считающийся верным или существующим"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d31b49fe-11c1-3351-856d-3b02eb4620f1",
                    "sense_definition": "Simple past and past participle of <w>allow</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My parents <w>allowed</w> me to go to the party.",
                            "target_lang_translations": {
                                "ru": "Мои родители <w>разрешили</w> мне пойти на вечеринку."
                            }
                        },
                        {
                            "text": "The security guard <w>allowed</w> us to pass through the gate.",
                            "target_lang_translations": {
                                "ru": "Охранник <w>позволил</w> нам пройти через ворота."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от <w>allow</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разрешил",
                                "target_lang_sense_clarification": "дал согласие (прошедшее время)"
                            },
                            {
                                "target_lang_word": "позволил",
                                "target_lang_sense_clarification": "сделал возможным (прошедшее время)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "betreffende",
        "desbetreffend"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "96437c0e-6dc5-3a9f-9353-85f0d81dc9f5",
                    "sense_definition": "Als onderwerp of inhoud hebben; ergens over gaan.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De brief <w>betreft</w> uw aanvraag voor een nieuwe parkeervergunning.",
                            "target_lang_translations": {
                                "ru": "Письмо <w>касается</w> вашей заявки на новое разрешение на парковку."
                            }
                        },
                        {
                            "text": "Wat de resultaten <w>betreft</w>, kunnen we zeer tevreden zijn over dit jaar.",
                            "target_lang_translations": {
                                "ru": "Что <w>касается</w> результатов, мы можем быть очень довольны этим годом."
                            }
                        },
                        {
                            "text": "Dat dit een eenmalig bezoek <w>betrof</w>, stond bij haar vast.",
                            "target_lang_translations": {
                                "ru": "То, что это <w>касалось</w> разового визита, для нее было несомненным."
                            }
                        }
                    ],
                    "synonyms": [
                        "gaan over",
                        "handelen over"
                    ],
                    "common_phrases": [
                        "wat betreft",
                        "voor zover het mij betreft",
                        "wat mij betreft"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь темой или содержанием; касаться чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "касаться",
                                "target_lang_sense_clarification": "иметь отношение к чему-либо"
                            },
                            {
                                "target_lang_word": "относиться",
                                "target_lang_sense_clarification": "быть связанным с чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df60dc8c-bea2-3d4e-ab9a-832416178dbf",
                    "sense_definition": "Van belang zijn voor of invloed hebben op iemand of iets.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze nieuwe veiligheidsmaatregelen <w>betreffen</w> alle medewerkers van het bedrijf.",
                            "target_lang_translations": {
                                "ru": "Эти новые меры безопасности <w>затрагивают</w> всех сотрудников компании."
                            }
                        },
                        {
                            "text": "Het ongeval <w>betrof</w> gelukkig alleen materiële schade en geen personen.",
                            "target_lang_translations": {
                                "ru": "К счастью, авария <w>затронула</w> только материальный ущерб, а не людей."
                            }
                        },
                        {
                            "text": "Dit bericht <w>betreft</w> jullie allemaal.",
                            "target_lang_translations": {
                                "ru": "Это сообщение <w>касается</w> вас всех."
                            }
                        }
                    ],
                    "synonyms": [
                        "aangaan",
                        "raken"
                    ],
                    "common_phrases": [
                        "het betreft hier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь значение для кого-то или влиять на кого-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "затрагивать",
                                "target_lang_sense_clarification": "оказывать влияние"
                            },
                            {
                                "target_lang_word": "касаться",
                                "target_lang_sense_clarification": "иметь прямое отношение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88e2a65f-4765-3754-aacd-ec492020bc95",
                    "sense_definition": "Aantasten of ziek maken.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jonge hondjes kunnen daardoor al na twee maanden <w>betroffen</w> worden.",
                            "target_lang_translations": {
                                "ru": "Из-за этого молодые щенки могут быть <w>поражены</w> уже через два месяца."
                            }
                        },
                        {
                            "text": "Het gewas werd in dat jaar zwaar <w>betroffen</w> door de aanhoudende droogte.",
                            "target_lang_translations": {
                                "ru": "В тот год урожай был сильно <w>поражен</w> продолжительной засухой."
                            }
                        }
                    ],
                    "synonyms": [
                        "aantasten",
                        "treffen"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "In modern Dutch 'treffen' or 'getroffen worden' is preferred for this meaning."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поражать болезнью или наносить ущерб."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поражать",
                                "target_lang_sense_clarification": "о болезни или бедствии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
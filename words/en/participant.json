    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f55f70cc-d849-394f-837d-25ee881457b9",
                    "sense_definition": "A person who takes part in an activity or event.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "general_participation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was an active <w>participant</w> in the class discussion.",
                            "target_lang_translations": {
                                "ru": "Она была активной <w>участницей</w> классной дискуссии."
                            }
                        },
                        {
                            "text": "Every <w>participant</w> received a certificate of attendance.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>участник</w> получил сертификат о посещении."
                            }
                        }
                    ],
                    "synonyms": [
                        "participator"
                    ],
                    "common_phrases": [
                        "active participant",
                        "key participant",
                        "willing participant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, принимающий участие в какой-либо деятельности или мероприятии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участник",
                                "target_lang_sense_clarification": "тот, кто участвует в чём-либо"
                            },
                            {
                                "target_lang_word": "участница",
                                "target_lang_sense_clarification": "женский род к «участник»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61ea281a-df78-3917-8e40-73ff19ee0104",
                    "sense_definition": "A person who is studied in a scientific experiment or trial.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "scientific_subject",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Each research <w>participant</w> signed a consent form before the study began.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>участник</w> исследования подписал форму согласия перед началом работы."
                            }
                        },
                        {
                            "text": "The doctors monitored the health of every study <w>participant</w> closely.",
                            "target_lang_translations": {
                                "ru": "Врачи внимательно следили за здоровьем каждого <w>участника</w> исследования."
                            }
                        }
                    ],
                    "common_phrases": [
                        "study participant",
                        "research participant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, выступающий объектом изучения в научном эксперименте или исследовании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участник",
                                "target_lang_sense_clarification": "в исследовании или эксперименте"
                            },
                            {
                                "target_lang_word": "испытуемый",
                                "target_lang_sense_clarification": "субъект эксперимента"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {

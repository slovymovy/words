                        "wydział kryminalny",
                        "służba kryminalna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с преследованием преступников и расследованием преступлений службами"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уголовный",
                                "target_lang_sense_clarification": "о розыске"
                            },
                            {
                                "target_lang_word": "криминальный",
                                "target_lang_sense_clarification": "о полиции"
                            }
                        ]
                    }
                }

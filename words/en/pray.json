{
    "word_family": [
        "prayer",
        "prayerful",
        "prayerfully",
        "prayerless",
        "unprayed"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "814f3d88-84ff-3306-a8b7-7ddeaa5327c0",
                    "sense_definition": "To speak to a god or deity to give thanks, worship, or ask for help.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "religious_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The family gathers to <w>pray</w> together before every meal.",
                            "target_lang_translations": {
                                "ru": "Семья собирается, чтобы вместе <w>помолиться</w> перед каждой трапезой."
                            }
                        },
                        {
                            "text": "She went to the temple to <w>pray</w> for the health of her parents.",
                            "target_lang_translations": {
                                "ru": "Она пошла в храм <w>помолиться</w> о здоровье своих родителей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pray to God",
                        "pray for forgiveness",
                        "pray for help",
                        "pray for peace"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обращаться к богу или божеству с благодарностью, поклонением или просьбой о помощи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молиться",
                                "target_lang_sense_clarification": "обращаться к Богу"
                            },
                            {
                                "target_lang_word": "помолиться",
                                "target_lang_sense_clarification": "совершить молитву (сов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e9d6ab3-97f7-30a4-aff1-8a84aa52b76c",
                    "sense_definition": "To hope very strongly for a particular outcome or result.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "strong_hope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We can only <w>pray</w> that the storm passes without causing damage.",
                            "target_lang_translations": {
                                "ru": "Нам остаётся только <w>молиться</w>, чтобы буря прошла, не причинив вреда."
                            }
                        },
                        {
                            "text": "I am <w>praying</w> for a miracle to fix this difficult situation.",
                            "target_lang_translations": {
                                "ru": "Я <w>молюсь</w> о чуде, чтобы исправить эту сложную ситуацию."
                            }
                        }
                    ],
                    "synonyms": [
                        "hope",
                        "wish"
                    ],
                    "common_phrases": [
                        "pray for a miracle",
                        "pray that"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень сильно надеяться на определенный исход или результат."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молиться",
                                "target_lang_sense_clarification": "страстно желать, надеяться на чудо"
                            },
                            {
                                "target_lang_word": "надеяться",
                                "target_lang_sense_clarification": "ожидать чего-либо желаемого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31250b27-06b5-3612-9c1f-bf4a42a66da2",
                    "sense_definition": "To ask someone earnestly or humbly for something; to beg or implore.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "entreaty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>pray</w> you, do not leave us in this time of need.",
                            "target_lang_translations": {
                                "ru": "<w>Молю</w> вас, не оставляйте нас в эту трудную минуту."
                            }
                        },
                        {
                            "text": "The prisoner <w>prayed</w> the court for mercy during the trial.",
                            "target_lang_translations": {
                                "ru": "Заключённый <w>молил</w> суд о милосердии во время процесса."
                            }
                        }
                    ],
                    "synonyms": [
                        "beg",
                        "implore",
                        "beseech",
                        "entreat"
                    ],
                    "common_phrases": [
                        "pray tell"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "in legal or very formal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Настойчиво или смиренно просить кого-либо о чем-либо; умолять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молить",
                                "target_lang_sense_clarification": "слезно просить, взывать"
                            },
                            {
                                "target_lang_word": "умолять",
                                "target_lang_sense_clarification": "настойчиво просить"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "96549e49-ae31-3d0e-9c28-ba0a51a22004",
                    "sense_definition": "Used to make a polite request or to introduce a question; equivalent to 'please'.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "polite_request",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Pray</w> continue your story, as we are all very interested.",
                            "target_lang_translations": {
                                "ru": "<w>Прошу</w>, продолжайте свой рассказ, так как нам всем очень интересно."
                            }
                        },
                        {
                            "text": "What, <w>pray</w>, is the meaning of this unexpected interruption?",
                            "target_lang_translations": {
                                "ru": "В чём, <w>скажите на милость</w>, смысл этого неожиданного вмешательства?"
                            }
                        }
                    ],
                    "synonyms": [
                        "please"
                    ],
                    "common_phrases": [
                        "pray tell"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "formal or literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения вежливой просьбы или ввода вопроса; эквивалент слова 'пожалуйста'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прошу",
                                "target_lang_sense_clarification": "вежливое обращение"
                            },
                            {
                                "target_lang_word": "пожалуйста",
                                "target_lang_sense_clarification": "вводное слово"
                            },
                            {
                                "target_lang_word": "скажите на милость",
                                "target_lang_sense_clarification": "в вопросах (устар., ирон.)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "90320b6d-7788-3279-b7c5-2cef75c9ea72",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Pray</w> arrived late to the meeting.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Прей</w> опоздал на встречу."
                            }
                        },
                        {
                            "text": "We looked up the <w>Pray</w> family in the local registry.",
                            "target_lang_translations": {
                                "ru": "Мы искали семью <w>Прей</w> в местном реестре."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Прей",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
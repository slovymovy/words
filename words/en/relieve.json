{
    "word_family": [
        "relief",
        "reliever",
        "relievable",
        "unrelieved"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "31aeba25-4956-33e4-bbbb-1cac8880bc3f",
                    "sense_definition": "To stop someone from feeling anxious, worried, or distressed; to make a problem or situation less serious.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "alleviation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hearing that everyone was safe helped to <w>relieve</w> our anxiety.",
                            "target_lang_translations": {
                                "ru": "Известие о том, что все в безопасности, помогло <w>облегчить</w> нашу тревогу."
                            }
                        },
                        {
                            "text": "A joke can sometimes <w>relieve</w> the tension in a quiet room.",
                            "target_lang_translations": {
                                "ru": "Шутка иногда может <w>разрядить</w> напряжение в тихой комнате."
                            }
                        }
                    ],
                    "synonyms": [
                        "soothe",
                        "comfort",
                        "calm",
                        "allay",
                        "assuage"
                    ],
                    "antonyms": [
                        "worry",
                        "distress",
                        "upset",
                        "aggravate"
                    ],
                    "common_phrases": [
                        "relieve stress",
                        "relieve anxiety",
                        "relieve tension",
                        "relieve boredom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшить чувство тревоги, беспокойства или страдания; сделать проблему или ситуацию менее серьезной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "облегчить",
                                "target_lang_sense_clarification": "уменьшить страдания или тревогу"
                            },
                            {
                                "target_lang_word": "снять",
                                "target_lang_sense_clarification": "устранить напряжение или стресс"
                            },
                            {
                                "target_lang_word": "разрядить",
                                "target_lang_sense_clarification": "сделать обстановку менее напряженной"
                            },
                            {
                                "target_lang_word": "успокоить",
                                "target_lang_sense_clarification": "принести утешение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1390ac20-679e-38f7-a588-40197ccbb57f",
                    "sense_definition": "To make physical pain or discomfort less unpleasant or severe.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "alleviation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor prescribed medicine to <w>relieve</w> the pain in his back.",
                            "target_lang_translations": {
                                "ru": "Врач прописал лекарство, чтобы <w>облегчить</w> боль в его спине."
                            }
                        },
                        {
                            "text": "Applying ice can help <w>relieve</w> the swelling and discomfort.",
                            "target_lang_translations": {
                                "ru": "Прикладывание льда может помочь <w>снять</w> отек и дискомфорт."
                            }
                        }
                    ],
                    "synonyms": [
                        "alleviate",
                        "soothe",
                        "ease",
                        "dull"
                    ],
                    "antonyms": [
                        "worsen",
                        "exacerbate",
                        "intensify"
                    ],
                    "common_phrases": [
                        "relieve pain",
                        "relieve symptoms",
                        "relieve a headache",
                        "relieve congestion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать физическую боль или дискомфорт менее неприятными или сильными."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "облегчить",
                                "target_lang_sense_clarification": "сделать боль менее сильной"
                            },
                            {
                                "target_lang_word": "снять",
                                "target_lang_sense_clarification": "устранить боль или симптомы"
                            },
                            {
                                "target_lang_word": "унять",
                                "target_lang_sense_clarification": "успокоить боль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "247ea11a-24f3-3a9e-9e04-43357fdbc953",
                    "sense_definition": "To take the place of someone who is on duty so that they can rest or stop working.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "replacement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The night guard arrived at ten o'clock to <w>relieve</w> me.",
                            "target_lang_translations": {
                                "ru": "Ночной охранник пришел в десять часов, чтобы <w>сменить</w> меня."
                            }
                        },
                        {
                            "text": "I need someone to <w>relieve</w> me at the front desk while I eat lunch.",
                            "target_lang_translations": {
                                "ru": "Мне нужно, чтобы кто-нибудь <w>подменил</w> меня на стойке регистрации, пока я обедаю."
                            }
                        }
                    ],
                    "synonyms": [
                        "replace",
                        "take over from",
                        "substitute for"
                    ],
                    "common_phrases": [
                        "relieve the guard",
                        "relieve the watch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заменить кого-либо на дежурстве или работе, чтобы этот человек мог отдохнуть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сменить",
                                "target_lang_sense_clarification": "занять чье-то место на посту"
                            },
                            {
                                "target_lang_word": "подменить",
                                "target_lang_sense_clarification": "временно заменить кого-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4360d032-0379-3f19-b277-cab62208afd7",
                    "sense_definition": "To free someone from a burdensome task, responsibility, or unwanted situation.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "replacement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The officer was <w>relieved</w> of his command after the incident.",
                            "target_lang_translations": {
                                "ru": "Офицер был <w>отстранен</w> от командования после инцидента."
                            }
                        },
                        {
                            "text": "Let me <w>relieve</w> you of those heavy bags so you can open the door.",
                            "target_lang_translations": {
                                "ru": "Позволь мне <w>освободить</w> тебя от этих тяжелых сумок, чтобы ты мог открыть дверь."
                            }
                        }
                    ],
                    "synonyms": [
                        "exempt",
                        "discharge",
                        "excuse",
                        "free"
                    ],
                    "antonyms": [
                        "burden",
                        "encumber"
                    ],
                    "common_phrases": [
                        "relieve of duty",
                        "relieve of responsibility",
                        "relieve of command"
                    ],
                    "target_lang_definitions": {
                        "ru": "Освободить кого-либо от обременительной задачи, ответственности или нежелательной ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "освободить",
                                "target_lang_sense_clarification": "избавить от обязанности или ноши"
                            },
                            {
                                "target_lang_word": "избавить",
                                "target_lang_sense_clarification": "спасти от чего-то неприятного"
                            },
                            {
                                "target_lang_word": "отстранить",
                                "target_lang_sense_clarification": "лишить должности или полномочий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68404c10-1bda-38ae-af48-2ac4016ab001",
                    "sense_definition": "To urinate or defecate; used reflexively.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "bodily_function",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He stopped the car by the side of the road to <w>relieve</w> himself.",
                            "target_lang_translations": {
                                "ru": "Он остановил машину на обочине, чтобы <w>справить нужду</w>."
                            }
                        },
                        {
                            "text": "Owners must clean up after their dogs when they <w>relieve</w> themselves in the park.",
                            "target_lang_translations": {
                                "ru": "Владельцы должны убирать за своими собаками, когда те <w>справляют нужду</w> в парке."
                            }
                        }
                    ],
                    "synonyms": [
                        "urinate",
                        "defecate",
                        "ease oneself",
                        "void"
                    ],
                    "common_phrases": [
                        "relieve oneself"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "euphemistic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Справлять естественные надобности (мочиться или испражняться); используется как возвратный глагол."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "справить нужду",
                                "target_lang_sense_clarification": "нейтральное выражение"
                            },
                            {
                                "target_lang_word": "облегчиться",
                                "target_lang_sense_clarification": "разговорное, эвфемизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cc7eb1e-7a9c-3939-a292-1722c4aa1dbb",
                    "sense_definition": "To bring military assistance to a besieged town or fortress and end the siege.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The general sent a large force to <w>relieve</w> the besieged city.",
                            "target_lang_translations": {
                                "ru": "Генерал направил крупные силы, чтобы <w>деблокировать</w> осажденный город."
                            }
                        },
                        {
                            "text": "Supplies were dropped by air to <w>relieve</w> the troops trapped in the valley.",
                            "target_lang_translations": {
                                "ru": "Припасы были сброшены с воздуха, чтобы <w>помочь</w> войскам, заблокированным в долине."
                            }
                        }
                    ],
                    "synonyms": [
                        "rescue",
                        "liberate",
                        "save"
                    ],
                    "antonyms": [
                        "besiege"
                    ],
                    "common_phrases": [
                        "relieve the siege"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прийти на помощь осажденному городу или крепости и снять осаду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "деблокировать",
                                "target_lang_sense_clarification": "снять блокаду или осаду"
                            },
                            {
                                "target_lang_word": "освободить",
                                "target_lang_sense_clarification": "спасти от захвата или осады"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
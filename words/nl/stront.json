                },
                {
                    "sense_id": "72ab03fe-4c93-3fa2-807b-1d4d41c505f1",
                    "sense_definition": "Een zeer problematische of vervelende situatie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "trouble",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je zoveel geld leent, zit je bij een beetje tegenslag al gauw in de <w>stront</w>.",
                            "target_lang_translations": {
                                "ru": "Если занимаешь столько денег, то при малейшей неудаче быстро окажешься в <w>дерьме</w>."
                            }
                        },
                        {
                            "text": "Er is <w>stront</w> aan de knikker nu de directeur van de fouten op de hoogte is.",
                            "target_lang_translations": {
                                "ru": "Теперь, когда директор знает об ошибках, дело — <w>дрянь</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "schijt",
                        "poep",
                        "drek",
                        "mest"
                    ],
                    "common_phrases": [
                        "in de stront zitten",
                        "stront aan de knikker"
                    ],

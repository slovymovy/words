                {
                    "sense_id": "301be853-433f-3ab7-9d58-6e8c96de04ec",
                    "sense_definition": "Komenda nakazująca natychmiastowe zatrzymanie się lub przerwanie wykonywanej czynności.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "stopping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Stop</w>! Nie możesz tam wejść bez przepustki.",

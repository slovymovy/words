                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ремень",
                                "target_lang_sense_clarification": "безопасности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "875acb6f-4408-3ac3-a58a-a39cb00552ed",
                    "sense_definition": "A trophy in the shape of a belt, generally awarded for rank or victory in martial arts and boxing.",

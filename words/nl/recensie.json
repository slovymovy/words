            "senses": [
                {
                    "sense_id": "b88f4eac-fadb-35d1-98ef-3b813e3c399d",
                    "sense_definition": "Een kritische bespreking of beoordeling, meestal van een cultureel werk zoals een boek, film of toneelstuk.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De journalist schreef een lovende <w>recensie</w> over de nieuwe film.",
                            "target_lang_translations": {
                                "en": "The journalist wrote a glowing <w>review</w> of the new movie.",
                                "ru": "Журналист написал хвалебную <w>рецензию</w> на новый фильм."
                            }
                        },
                        {
                            "text": "Na het lezen van de slechte <w>recensie</w> besloot ik het boek niet te kopen.",
                            "target_lang_translations": {
                                "en": "After reading the bad <w>review</w>, I decided not to buy the book.",
                                "ru": "Прочитав плохую <w>рецензию</w>, я решил не покупать книгу."
                            }
                        }
                    ],
                    "synonyms": [
                        "bespreking",
                        "kritiek",
                        "review"
                    ],
                    "common_phrases": [
                        "lovende recensie",
                        "vernietigende recensie",
                        "recensie schrijven"
                    ],
                    "target_lang_definitions": {
                        "en": "A critical discussion or assessment, usually of a cultural work such as a book, film, or play.",
                        "ru": "Критический разбор или оценка, обычно культурного произведения, такого как книга, фильм или спектакль."
                    },
                    "translations": {
                        "en": [
                            {

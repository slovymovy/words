    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "57f4f1c5-a874-31d8-b36c-2664b6b1ad2d",
                    "sense_definition": "Pierwszy moment trwania czegoś lub wstępna faza jakiegoś zdarzenia",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_beginning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na <w>początku</w> film był trochę nudny, ale potem akcja przyspieszyła.",
                            "target_lang_translations": {
                                "ru": "В <w>начале</w> фильм был немного скучным, но потом действие ускорилось."
                            }
                        },
                        {
                            "text": "To dopiero <w>początek</w> naszej długiej podróży przez Europę.",
                            "target_lang_translations": {
                                "ru": "Это только <w>начало</w> нашего долгого путешествия по Европе."
                            }
                        },
                        {
                            "text": "Musimy zacząć wszystko od <w>początku</w>, ponieważ popełniliśmy błąd.",
                            "target_lang_translations": {
                                "ru": "Мы должны начать всё с <w>начала</w>, так как совершили ошибку."
                            }
                        }
                    ],
                    "synonyms": [
                        "start",
                        "inauguracja"
                    ],
                    "antonyms": [
                        "koniec",
                        "kres",
                        "schyłek"
                    ],
                    "common_phrases": [
                        "na początku",
                        "od początku",
                        "dobry początek",
                        "początek roku",
                        "na samym początku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Первый момент или начальная фаза какого-либо события"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начало",
                                "target_lang_sense_clarification": "время/событие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4e771ee-a615-3c4a-9f79-4977942ad56f",
                    "sense_definition": "Miejsce, w którym coś się zaczyna przestrzennie, np. pierwszy odcinek drogi lub obiektu",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "space_beginning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz sklep znajduje się na samym <w>początku</w> ulicy Długiej.",
                            "target_lang_translations": {
                                "ru": "Наш магазин находится в самом <w>начале</w> улицы Длугой."
                            }
                        },
                        {
                            "text": "Stanęliśmy na <w>początku</w> kolejki, żeby kupić bilety.",
                            "target_lang_translations": {
                                "ru": "Мы встали в <w>начале</w> очереди, чтобы купить билеты."
                            }
                        }
                    ],
                    "antonyms": [
                        "koniec"
                    ],
                    "common_phrases": [
                        "początek ulicy",
                        "początek kolejki",
                        "początek drogi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, где что-то начинается в пространстве, например, первый отрезок пути"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начало",
                                "target_lang_sense_clarification": "место/пространство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9db48e74-fc3f-3937-94fd-231f0288d22a",
                    "sense_definition": "Źródło, przyczyna lub punkt wyjścia powstania czegoś",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "origin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To małe nieporozumienie dało <w>początek</w> wielkiemu konfliktowi.",
                            "target_lang_translations": {
                                "ru": "Это маленькое недоразумение положило <w>начало</w> большому конфликту."
                            }
                        },
                        {
                            "text": "Rzeka Wisła bierze swój <w>początek</w> w górach Beskidu Śląskiego.",
                            "target_lang_translations": {
                                "ru": "Река Висла берёт своё <w>начало</w> в горах Силезских Бескидов."
                            }
                        }
                    ],
                    "synonyms": [
                        "źródło",
                        "geneza",
                        "przyczyna",
                        "powód",
                        "zaczątek",
                        "zalążek"
                    ],
                    "common_phrases": [
                        "dać początek",
                        "brać początek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Источник, причина или исходная точка возникновения чего-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начало",
                                "target_lang_sense_clarification": "источник/происхождение"
                            },
                            {
                                "target_lang_word": "почин",
                                "target_lang_sense_clarification": "инициатива/начинание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a100dcc3-8191-3292-9ddd-2bf66ab9b792",
                    "sense_definition": "Wczesny okres istnienia czegoś, pierwsze próby lub etapy rozwoju (często w liczbie mnogiej)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "early_stages",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moje <w>początki</w> w nowej firmie były bardzo stresujące, ale szybko się nauczyłem.",
                            "target_lang_translations": {
                                "ru": "Моё <w>начало</w> работы в новой фирме было очень стрессовым, но я быстро научился."
                            }
                        },
                        {
                            "text": "Pamiętam trudne <w>początki</w> internetu w Polsce w latach dziewięćdziesiątych.",
                            "target_lang_translations": {
                                "ru": "Я помню трудное <w>начало</w> интернета в Польше в девяностых годах."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaranie"
                    ],
                    "common_phrases": [
                        "trudne początki",
                        "początki kariery"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ранний период существования чего-либо, первые попытки или этапы развития"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начало",
                                "target_lang_sense_clarification": "ранний этап"
                            }
                        ]
                    }
                }
            ]
        }
    ]

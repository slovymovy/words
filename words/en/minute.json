{
    "word_family": [
        "minutely",
        "minuteness"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7674c70b-48a6-3fc4-abe5-57ae56abadc9",
                    "sense_definition": "A unit of time equal to sixty seconds, or one sixtieth of an hour.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The train to London leaves in ten <w>minutes</w>.",
                            "target_lang_translations": {
                                "ru": "Поезд в Лондон отправляется через десять <w>минут</w>."
                            }
                        },
                        {
                            "text": "She managed to run a mile in less than five <w>minutes</w>.",
                            "target_lang_translations": {
                                "ru": "Ей удалось пробежать милю менее чем за пять <w>минут</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "per minute",
                        "ten minutes",
                        "five minutes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица времени, равная шестидесяти секундам, или одной шестидесятой часа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "минута",
                                "target_lang_sense_clarification": "единица времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab6aa90e-31a9-3f88-9c31-4c9ad1da55af",
                    "sense_definition": "A very short, unspecified period of time.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "short_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you wait a <w>minute</w> while I find my keys?",
                            "target_lang_translations": {
                                "ru": "Можешь подождать <w>минуту</w>, пока я найду ключи?"
                            }
                        },
                        {
                            "text": "I will be with you in a <w>minute</w>.",
                            "target_lang_translations": {
                                "ru": "Я буду с вами через <w>минуту</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "moment",
                        "second",
                        "instant",
                        "jiffy"
                    ],
                    "common_phrases": [
                        "wait a minute",
                        "just a minute",
                        "in a minute",
                        "hold on a minute"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень короткий, неопределенный промежуток времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "минута",
                                "target_lang_sense_clarification": "момент"
                            },
                            {
                                "target_lang_word": "минутка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ed47c84-a769-3352-80ec-2899195a0a33",
                    "sense_definition": "A written record of what was said and decided at a meeting.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "record",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The secretary took the <w>minutes</w> during the board meeting.",
                            "target_lang_translations": {
                                "ru": "Секретарь вел <w>протокол</w> во время заседания совета."
                            }
                        },
                        {
                            "text": "We need to approve the <w>minutes</w> from the last session before we proceed.",
                            "target_lang_translations": {
                                "ru": "Нам нужно утвердить <w>протокол</w> последнего заседания, прежде чем мы продолжим."
                            }
                        }
                    ],
                    "synonyms": [
                        "record",
                        "notes",
                        "proceedings"
                    ],
                    "common_phrases": [
                        "take minutes",
                        "meeting minutes"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Письменная запись того, что было сказано и решено на собрании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "протокол",
                                "target_lang_sense_clarification": "документ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3de7eefb-6587-3afd-8d4e-56c6562f5c65",
                    "sense_definition": "A long period of time.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "long_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I haven't seen him in a <w>minute</w>; it must be years.",
                            "target_lang_translations": {
                                "ru": "Я не видел его <w>сто лет</w>; должно быть, прошли годы."
                            }
                        },
                        {
                            "text": "It's been a <w>minute</w> since we last visited this place.",
                            "target_lang_translations": {
                                "ru": "Прошла <w>вечность</w> с тех пор, как мы в последний раз посещали это место."
                            }
                        }
                    ],
                    "synonyms": [
                        "while",
                        "ages"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Долгий период времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вечность",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "сто лет",
                                "target_lang_sense_clarification": "разговорное выражение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "149b4810-c531-3de1-b7e2-ca8c825844d8",
                    "sense_definition": "A unit of angular measurement equal to one sixtieth of a degree.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "angle_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The map coordinates are accurate to within one <w>minute</w> of latitude.",
                            "target_lang_translations": {
                                "ru": "Координаты карты точны до одной <w>минуты</w> широты."
                            }
                        },
                        {
                            "text": "Each degree is divided into sixty <w>minutes</w>.",
                            "target_lang_translations": {
                                "ru": "Каждый градус делится на шестьдесят <w>минут</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "minute of arc"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "technical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица измерения углов, равная одной шестидесятой градуса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "минута",
                                "target_lang_sense_clarification": "геометрия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "65cc9cf5-5fb1-3da4-b6cb-b6de3ef2f2cd",
                    "sense_definition": "Extremely small in size, amount, or degree.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "small_size",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Only a <w>minute</w> amount of the chemical is needed for the reaction.",
                            "target_lang_translations": {
                                "ru": "Для реакции требуется лишь <w>ничтожное</w> количество химиката."
                            }
                        },
                        {
                            "text": "The detective found <w>minute</w> traces of soil on the suspect's shoes.",
                            "target_lang_translations": {
                                "ru": "Детектив обнаружил <w>мельчайшие</w> следы почвы на обуви подозреваемого."
                            }
                        }
                    ],
                    "synonyms": [
                        "tiny",
                        "minuscule",
                        "infinitesimal",
                        "microscopic",
                        "insignificant"
                    ],
                    "antonyms": [
                        "huge",
                        "enormous",
                        "vast",
                        "colossal",
                        "big",
                        "tremendous"
                    ],
                    "common_phrases": [
                        "minute amount",
                        "minute quantity",
                        "minute particle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чрезвычайно маленький по размеру, количеству или степени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мельчайший",
                                "target_lang_sense_clarification": "очень маленький"
                            },
                            {
                                "target_lang_word": "ничтожный",
                                "target_lang_sense_clarification": "незначительный"
                            },
                            {
                                "target_lang_word": "крошечный",
                                "target_lang_sense_clarification": "малый размер"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94e3d4d9-ed9f-368c-b9f8-30500f5ab186",
                    "sense_definition": "Very careful, precise, and paying attention to the smallest details.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "detailed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lawyer conducted a <w>minute</w> examination of the contract.",
                            "target_lang_translations": {
                                "ru": "Адвокат провел <w>тщательное</w> изучение контракта."
                            }
                        },
                        {
                            "text": "She described the events of that night in <w>minute</w> detail.",
                            "target_lang_translations": {
                                "ru": "Она описала события той ночи в <w>мельчайших</w> подробностях."
                            }
                        }
                    ],
                    "synonyms": [
                        "detailed",
                        "precise",
                        "thorough",
                        "meticulous",
                        "exhaustive",
                        "scrupulous",
                        "exact"
                    ],
                    "common_phrases": [
                        "minute detail",
                        "minute examination"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень внимательный, точный и уделяющий внимание мельчайшим деталям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тщательный",
                                "target_lang_sense_clarification": "внимательный"
                            },
                            {
                                "target_lang_word": "детальный",
                                "target_lang_sense_clarification": "подробный"
                            },
                            {
                                "target_lang_word": "скрупулёзный",
                                "target_lang_sense_clarification": "точный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "480469e1-416b-38d9-95b2-ec82c3a3cdb0",
                    "sense_definition": "To make a written record of the proceedings of a meeting.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "to_record",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will <w>minute</w> the decisions made during this evening's session.",
                            "target_lang_translations": {
                                "ru": "Я <w>запротоколирую</w> решения, принятые на сегодняшнем вечернем заседании."
                            }
                        },
                        {
                            "text": "The committee's objections were <w>minuted</w> for the official record.",
                            "target_lang_translations": {
                                "ru": "Возражения комитета были <w>занесены в протокол</w> для официальной отчетности."
                            }
                        }
                    ],
                    "synonyms": [
                        "record",
                        "note",
                        "document"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать письменную запись хода собрания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вести протокол",
                                "target_lang_sense_clarification": "записывать ход собрания"
                            },
                            {
                                "target_lang_word": "протоколировать",
                                "target_lang_sense_clarification": "фиксировать документально"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
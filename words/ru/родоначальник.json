    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dc4e0787-2182-39f9-aa53-a1d858158965",
                    "sense_definition": "Предок, от которого ведёт своё начало род или династия",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот князь считается <w>родоначальником</w> правящей династии.",
                            "target_lang_translations": {
                                "en": "This prince is considered the <w>founder</w> of the ruling dynasty."
                            }
                        },
                        {
                            "text": "В старинных документах упомянут <w>родоначальник</w> их знатной фамилии.",
                            "target_lang_translations": {
                                "en": "The <w>progenitor</w> of their noble family is mentioned in ancient documents."
                            }
                        }
                    ],
                    "synonyms": [
                        "предок"
                    ],
                    "common_phrases": [
                        "родоначальник династии",
                        "легендарный родоначальник",
                        "родоначальник фамилии"
                    ],
                    "target_lang_definitions": {
                        "en": "An ancestor from whom a family line or dynasty originates"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "founder",
                                "target_lang_sense_clarification": "of a dynasty or family line"
                            },
                            {
                                "target_lang_word": "progenitor",
                                "target_lang_sense_clarification": "formal; biological or genealogical ancestor"
                            },
                            {
                                "target_lang_word": "patriarch",
                                "target_lang_sense_clarification": "male head or founder of a family"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3cab85c7-45f4-3822-8d4f-216587b75cbd",
                    "sense_definition": "Тот, кто положил начало чему-либо, создал новое направление в науке или искусстве",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Писатель стал <w>родоначальником</w> нового литературного жанра.",
                            "target_lang_translations": {
                                "en": "The writer became the <w>founder</w> of a new literary genre."
                            }
                        },
                        {
                            "text": "Этого учёного называют <w>родоначальником</w> современной космонавтики.",
                            "target_lang_translations": {
                                "en": "This scientist is called the <w>father</w> of modern astronautics."
                            }
                        }
                    ],
                    "common_phrases": [
                        "родоначальник жанра",
                        "родоначальник направления",
                        "родоначальник традиции"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who initiates something or creates a new direction in science or art"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "founder",
                                "target_lang_sense_clarification": "of a movement, school, or genre"
                            },
                            {
                                "target_lang_word": "father",
                                "target_lang_sense_clarification": "figurative; the originator of a field or idea"
                            },
                            {
                                "target_lang_word": "originator",
                                "target_lang_sense_clarification": "one who starts or creates something"
                            }
                        ]
                    }
                }

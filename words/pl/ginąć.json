{
    "word_family": [
        "zginąć",
        "zaginąć",
        "wyginąć",
        "zaginiony",
        "ginięcie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "66837401-f273-338b-8d92-9248eca1760b",
                    "sense_definition": "Gubić się, stawać się niemożliwym do znalezienia (o przedmiotach lub ludziach).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "disappearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moje klucze ciągle gdzieś <giną> w tym dużym domu.",
                            "target_lang_translations": {
                                "ru": "Мои ключи постоянно где-то <теряются> в этом большом доме."
                            }
                        },
                        {
                            "text": "Małe dzieci łatwo <giną> w tłumie na rynku.",
                            "target_lang_translations": {
                                "ru": "Маленькие дети легко <теряются> в толпе на рынке."
                            }
                        }
                    ],
                    "synonyms": [
                        "gubić się",
                        "zapodziewać się"
                    ],
                    "antonyms": [
                        "znajdować się"
                    ],
                    "common_phrases": [
                        "ginąć bez wieści",
                        "ginąć bez śladu",
                        "ciągle mi ginie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Теряться, пропадать, становиться невозможным для нахождения (о предметах или людях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "теряться",
                                "target_lang_sense_clarification": "становиться невозможным для нахождения; исчезать из виду"
                            },
                            {
                                "target_lang_word": "пропадать",
                                "target_lang_sense_clarification": "исчезать неизвестно куда; отсутствовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a07e39d0-f86f-31a1-9a1e-b87743258e81",
                    "sense_definition": "Tracić życie, umierać (często w sposób gwałtowny, w wypadku lub na wojnie).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielu żołnierzy <ginie> na froncie każdego dnia.",
                            "target_lang_translations": {
                                "ru": "Много солдат <гибнет> на фронте каждый день."
                            }
                        },
                        {
                            "text": "Ludzie często <giną> w wypadkach samochodowych przez nadmierną prędkość.",
                            "target_lang_translations": {
                                "ru": "Люди часто <гибнут> в автомобильных авариях из-за превышения скорости."
                            }
                        }
                    ],
                    "synonyms": [
                        "umierać",
                        "konać",
                        "tracić życie"
                    ],
                    "common_phrases": [
                        "ginąć na wojnie",
                        "ginąć w wypadku",
                        "ginąć za ojczyznę",
                        "ginąć śmiercią tragiczną"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишаться жизни, умирать (часто насильственной смертью, в результате несчастного случая или на войне)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гибнуть",
                                "target_lang_sense_clarification": "умирать трагической или насильственной смертью"
                            },
                            {
                                "target_lang_word": "погибать",
                                "target_lang_sense_clarification": "синоним \"гибнуть\", часто используется в контексте войны или катастроф"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ac2e6d7-8382-3903-a141-78e30cb48b90",
                    "sense_definition": "Przestawać być widocznym lub słyszalnym; znikać w otoczeniu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "disappearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wysoki budynek powoli <ginął> w gęstej mgle.",
                            "target_lang_translations": {
                                "ru": "Высокое здание медленно <исчезало> в густом тумане."
                            }
                        },
                        {
                            "text": "Jej cichy głos <ginął> w hałasie ulicznym.",
                            "target_lang_translations": {
                                "ru": "Её тихий голос <терялся> в уличном шуме."
                            }
                        }
                    ],
                    "synonyms": [
                        "znikać",
                        "rozpływać się"
                    ],
                    "common_phrases": [
                        "ginąć w tłumie",
                        "ginąć w mroku",
                        "ginąć w oczach",
                        "ginąć we mgle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переставать быть видимым или слышимым; исчезать, сливаясь с окружающей средой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исчезать",
                                "target_lang_sense_clarification": "переставать быть видимым; скрываться из виду"
                            },
                            {
                                "target_lang_word": "растворяться",
                                "target_lang_sense_clarification": "перен. становиться незаметным, сливаясь с окружением (например, в тумане или толпе)"
                            },
                            {
                                "target_lang_word": "теряться",
                                "target_lang_sense_clarification": "становиться плохо различимым на фоне чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7d40131-c981-3885-9908-9c57230a252c",
                    "sense_definition": "Przestawać istnieć jako gatunek lub grupa; wymierać.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiele rzadkich gatunków zwierząt <ginie> z powodu zmian klimatycznych.",
                            "target_lang_translations": {
                                "ru": "Многие редкие виды животных <исчезают> из-за изменения климата."
                            }
                        },
                        {
                            "text": "Stare tradycje ludowe powoli <giną> w nowoczesnym świecie.",
                            "target_lang_translations": {
                                "ru": "Старые народные традиции медленно <умирают> в современном мире."
                            }
                        }
                    ],
                    "synonyms": [
                        "wymierać"
                    ],
                    "common_phrases": [
                        "ginąć bezpowrotnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переставать существовать как вид или группа; вымирать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вымирать",
                                "target_lang_sense_clarification": "переставать существовать (о биологических видах или группах)"
                            },
                            {
                                "target_lang_word": "исчезать",
                                "target_lang_sense_clarification": "прекращать существование (о традициях, культурах)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "позировать",
                                "target_lang_sense_clarification": "принимать позу для художника или фотографа"
                            },
                            {
                                "target_lang_word": "принимать позу",
                                "target_lang_sense_clarification": "располагаться определенным образом"
                            }

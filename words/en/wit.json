    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f129b4b5-2686-3f9d-a68a-2384d407ecc2",
                    "sense_definition": "The ability to use words in a clever and amusing way.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "humour_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His speech was full of <w>wit</w> and charm, delighting the entire audience.",
                            "target_lang_translations": {
                                "ru": "Его речь была полна <w>остроумия</w> и обаяния, восхитив всю аудиторию."
                            }
                        },
                        {
                            "text": "She is known for her dry <w>wit</w> and ability to make everyone laugh.",
                            "target_lang_translations": {
                                "ru": "Она известна своим сухим <w>остроумием</w> и способностью рассмешить любого."
                            }
                        }
                    ],
                    "synonyms": [
                        "repartee"
                    ],
                    "common_phrases": [
                        "dry wit",
                        "sharp wit",
                        "quick wit",
                        "ready wit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность использовать слова остроумно и забавно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остроумие",
                                "target_lang_sense_clarification": "способность шутить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a7a827a-224b-3154-a2e7-7778e5dedf22",
                    "sense_definition": "The ability to think quickly and make sensible decisions; intelligence.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mind_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He didn't even have the <w>wit</w> to apologize for his mistake.",
                            "target_lang_translations": {
                                "ru": "У него даже не хватило <w>ума</w> извиниться за свою ошибку."
                            }
                        },
                        {
                            "text": "It is beyond the <w>wit</w> of man to predict exactly when the earthquake will happen.",
                            "target_lang_translations": {
                                "ru": "Точно предсказать, когда произойдет землетрясение — выше человеческого <w>разума</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "have the wit to",
                        "beyond the wit of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность быстро думать и принимать разумные решения; интеллект."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ум",
                                "target_lang_sense_clarification": "интеллект"
                            },
                            {
                                "target_lang_word": "сообразительность",
                                "target_lang_sense_clarification": "смекалка"
                            },
                            {
                                "target_lang_word": "разум",
                                "target_lang_sense_clarification": "рассудительность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab63eaf8-376b-344f-95bc-962c337c3f5e",
                    "sense_definition": "Mental balance or sanity; the ability to think clearly in a difficult situation.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mind_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was at my <w>wits</w>' end trying to figure out how to fix the broken engine.",
                            "target_lang_translations": {
                                "ru": "Я окончательно потерял <w>голову</w>, пытаясь понять, как починить сломанный двигатель."
                            }
                        },
                        {
                            "text": "The sudden loud noise in the dark scared the poor child out of his <w>wits</w>.",
                            "target_lang_translations": {
                                "ru": "Внезапный громкий шум в темноте до смерти напугал бедного ребенка, лишив его <w>рассудка</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "at one's wits' end",
                        "scare out of one's wits",
                        "keep one's wits about one",
                        "gather one's wits"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Душевное равновесие или рассудок; способность ясно мыслить в сложной ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рассудок",
                                "target_lang_sense_clarification": "психическое здоровье"
                            },
                            {
                                "target_lang_word": "самообладание",
                                "target_lang_sense_clarification": "хладнокровие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2960f4d-9ea3-3c11-a3c5-1cab1596b1b9",
                    "sense_definition": "A person who is famous for making clever and funny remarks.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "humour_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oscar Wilde was a famous <w>wit</w> who was celebrated for his clever plays and sayings.",
                            "target_lang_translations": {
                                "ru": "Оскар Уайльд был известным <w>остряком</w>, прославившимся своими умными пьесами и высказываниями."
                            }
                        },
                        {
                            "text": "Your friend is quite a <w>wit</w>; he had us laughing all evening.",
                            "target_lang_translations": {
                                "ru": "Твой друг — настоящий <w>остряк</w>; он смешил нас весь вечер."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, известный своими остроумными и смешными замечаниями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остряк",
                                "target_lang_sense_clarification": "остроумный человек"
                            },
                            {
                                "target_lang_word": "шутник",
                                "target_lang_sense_clarification": "любитель пошутить"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ac9346e9-bb5f-3348-9fe6-e5c1948b0f6b",
                    "sense_definition": "To know or be aware of something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "know_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The contract covers three specific areas, to <w>wit</w>: sales, marketing, and distribution.",
                            "target_lang_translations": {
                                "ru": "Контракт охватывает три конкретные области, <w>а именно</w>: продажи, маркетинг и дистрибуцию."
                            }
                        },
                        {
                            "text": "They did not <w>wit</w> of the danger that was approaching them in the forest.",
                            "target_lang_translations": {
                                "ru": "Они не <w>ведали</w> об опасности, которая приближалась к ним в лесу."
                            }
                        }
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e2e6f726-4f75-3199-8308-1f4c9c96089e",
                    "sense_definition": "Wyrażać niezadowolenie, smutek lub informować o dolegliwościach fizycznych (używane z zaimkiem zwrotnym \"się\").",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "complaining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pacjent przyszedł do lekarza, aby <w>skarżyć</w> się na silny ból głowy.",
                            "target_lang_translations": {
                                "ru": "Пациент пришел к врачу, чтобы <w>пожаловаться</w> на сильную головную боль."
                            }
                        },
                        {
                            "text": "Mieszkańcy zaczęli <w>skarżyć</w> się na hałas dobiegający z pobliskiej budowy.",
                            "target_lang_translations": {
                                "ru": "Жители начали <w>жаловаться</w> на шум, доносящийся с соседней стройки."
                            }
                        }
                    ],
                    "synonyms": [
                        "narzekać",
                        "żalić się",
                        "biadolić",
                        "utyskiwać",
                        "uskarżać się"
                    ],
                    "antonyms": [
                        "cieszyć się",
                        "chwalić"
                    ],
                    "common_phrases": [
                        "skarżyć się na ból",
                        "skarżyć się na los",
                        "skarżyć się na zdrowie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать недовольство, печаль или сообщать о физических недугах (используется с возвратным местоимением \"się\")."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жаловаться",
                                "target_lang_sense_clarification": "выражать недовольство или боль"
                            },
                            {
                                "target_lang_word": "сетовать",
                                "target_lang_sense_clarification": "выражать сожаление или упрек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c244a165-01a2-36b9-bf10-ffd72c4caa82",
                    "sense_definition": "Informować kogoś (zazwyczaj rodziców lub nauczycieli) o czyimś złym zachowaniu; donosić na kogoś.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "telling_on",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie powinieneś ciągle <w>skarżyć</w> na kolegów z klasy nauczycielowi.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует постоянно <w>ябедничать</w> учителю на одноклассников."
                            }
                        },
                        {
                            "text": "Mała dziewczynka pobiegła <w>skarżyć</w> mamie, że brat zabrał jej lalkę.",
                            "target_lang_translations": {
                                "ru": "Маленькая девочка побежала <w>пожаловаться</w> маме, что брат забрал у нее куклу."
                            }
                        }
                    ],
                    "synonyms": [
                        "donosić"
                    ],
                    "common_phrases": [
                        "skarżyć na kolegę",
                        "skarżyć rodzicom",
                        "skarżyć nauczycielowi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сообщать кому-либо (обычно родителям или учителям) о чьем-либо плохом поведении; доносить на кого-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ябедничать",
                                "target_lang_sense_clarification": "доносить (обычно о детях)"
                            },
                            {
                                "target_lang_word": "жаловаться",
                                "target_lang_sense_clarification": "сообщать о чьем-то поведении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bee8fae4-6582-34e0-99a1-e98cb6e39248",
                    "sense_definition": "Wnosić oficjalne oskarżenie do sądu lub domagać się sprawiedliwości na drodze prawnej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Firma postanowiła <w>skarżyć</w> nieuczciwego kontrahenta o wysokie odszkodowanie.",
                            "target_lang_translations": {
                                "ru": "Компания решила <w>подать в суд</w> на недобросовестного контрагента, требуя крупную компенсацию."
                            }
                        },
                        {
                            "text": "Możesz go <w>skarżyć</w> do sądu, jeśli nie odda ci pożyczonych pieniędzy.",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>подать</w> на него <w>в суд</w>, если он не вернет тебе одолженные деньги."
                            }
                        }
                    ],
                    "synonyms": [
                        "oskarżać",
                        "pozywać"
                    ],
                    "antonyms": [
                        "bronić"
                    ],
                    "common_phrases": [
                        "skarżyć do sądu",
                        "skarżyć o odszkodowanie",
                        "skarżyć decyzję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предъявлять официальное обвинение в суде или искать справедливости правовым путем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подавать в суд",
                                "target_lang_sense_clarification": "инициировать судебный процесс"
                            },
                            {
                                "target_lang_word": "судиться",
                                "target_lang_sense_clarification": "вести судебную тяжбу"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
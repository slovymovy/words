    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7e0b80be-faa4-39cf-8115-d8c28d0d9272",
                    "sense_definition": "Geluid waarnemen met de oren",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>hoor</w> de vogels 's ochtends vroeg al zingen in de tuin.",
                            "target_lang_translations": {
                                "en": "I <w>hear</w> the birds singing in the garden early in the morning.",
                                "ru": "Я уже рано утром <w>слышу</w>, как в саду поют птицы."
                            }
                        },
                        {
                            "text": "Heb je het laatste nieuws over de verkiezingen al <w>gehoord</w>?",
                            "target_lang_translations": {
                                "en": "Have you <w>heard</w> the latest news about the elections?",
                                "ru": "Ты уже <w>слышал</w> последние новости о выборах?"
                            }
                        }
                    ],
                    "synonyms": [
                        "waarnemen"
                    ],
                    "common_phrases": [
                        "laten horen",
                        "van zich laten horen"
                    ],
                    "target_lang_definitions": {
                        "en": "To perceive sound with the ears",
                        "ru": "Воспринимать звук ушами"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hear",
                                "target_lang_sense_clarification": "to perceive sound"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "слышать",
                                "target_lang_sense_clarification": "воспринимать звуки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18075707-fc90-334d-8af7-2b990a1348b6",
                    "sense_definition": "Een vaste plaats hebben of deel uitmaken van een groep",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "belonging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze boeken <w>horen</w> op de bovenste plank van de kast.",
                            "target_lang_translations": {
                                "en": "These books <w>belong</w> on the top shelf of the bookcase.",
                                "ru": "Этим книгам <w>место</w> на верхней полке шкафа."
                            }
                        },
                        {
                            "text": "Zij <w>horen</w> al jaren bij onze hechte vriendengroep.",
                            "target_lang_translations": {
                                "en": "They have <w>belonged</w> to our close group of friends for years.",
                                "ru": "Они уже много лет <w>принадлежат</w> к нашей тесной компании друзей."
                            }
                        }
                    ],
                    "synonyms": [
                        "thuishoren",
                        "behoren",
                        "passen"
                    ],
                    "common_phrases": [
                        "erbij horen",
                        "thuis horen",
                        "bij elkaar horen"
                    ],
                    "target_lang_definitions": {
                        "en": "To have a fixed place or be part of a group",
                        "ru": "Иметь постоянное место или быть частью группы"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "belong",
                                "target_lang_sense_clarification": "to be in the right place or group"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "принадлежать",
                                "target_lang_sense_clarification": "быть частью чего-либо"
                            },
                            {
                                "target_lang_word": "относиться",
                                "target_lang_sense_clarification": "иметь отношение к группе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7f4a3f4-0f29-3e8c-93fd-58dae44d44a6",
                    "sense_definition": "Volgens de regels of verwachtingen verplicht zijn iets te doen",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je <w>hoort</w> je huiswerk te maken voordat je televisie gaat kijken.",
                            "target_lang_translations": {
                                "en": "You <w>are supposed</w> to do your homework before you watch television.",
                                "ru": "Ты <w>должен</w> сделать домашнее задание, прежде чем смотреть телевизор."
                            }
                        },
                        {
                            "text": "We <w>horen</w> eigenlijk om acht uur op kantoor te zijn.",
                            "target_lang_translations": {
                                "en": "We <w>are</w> actually <w>supposed</w> to be at the office at eight o'clock.",
                                "ru": "Вообще-то мы <w>должны</w> быть в офисе в восемь часов."
                            }
                        }
                    ],
                    "synonyms": [
                        "moeten",
                        "dienen"
                    ],
                    "common_phrases": [
                        "horen te"
                    ],
                    "target_lang_definitions": {
                        "en": "To be obliged to do something according to rules or expectations",
                        "ru": "Быть обязанным сделать что-то согласно правилам или ожиданиям"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be supposed to",
                                "target_lang_sense_clarification": "to be expected or obliged to"
                            },
                            {
                                "target_lang_word": "ought to",
                                "target_lang_sense_clarification": "indicating duty"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "быть должным",
                                "target_lang_sense_clarification": "иметь обязательство"
                            },
                            {
                                "target_lang_word": "полагаться",
                                "target_lang_sense_clarification": "быть предписанным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "184c36dd-a3af-327f-b8b9-50d21f929459",
                    "sense_definition": "Gepast zijn volgens de sociale normen en waarden",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "etiquette",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>hoort</w> niet om met volle mond te praten tijdens het eten.",
                            "target_lang_translations": {
                                "en": "It is not <w>proper</w> to talk with your mouth full while eating.",
                                "ru": "<w>Не принято</w> разговаривать с набитым ртом во время еды."
                            }
                        },
                        {
                            "text": "Hij stelde zich netjes aan iedereen voor, precies zoals het <w>hoort</w>.",
                            "target_lang_translations": {
                                "en": "He introduced himself politely to everyone, exactly as it <w>should be</w>.",
                                "ru": "Он вежливо представился всем, в точности как <w>полагается</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "passen",
                        "betamen"
                    ],
                    "common_phrases": [
                        "zoals het hoort",
                        "dat hoort niet"
                    ],
                    "target_lang_definitions": {
                        "en": "To be appropriate according to social norms and values",
                        "ru": "Соответствовать социальным нормам и правилам приличия"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be proper",
                                "target_lang_sense_clarification": "to be socially acceptable"
                            },
                            {
                                "target_lang_word": "be fitting",
                                "target_lang_sense_clarification": "to be suitable"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "подобать",
                                "target_lang_sense_clarification": "быть приличным"
                            },
                            {
                                "target_lang_word": "быть принятым",
                                "target_lang_sense_clarification": "соответствовать этикету"
                            }
                        ]
                    }
                }
            ]
        },

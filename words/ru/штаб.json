            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a4d123c4-9ee8-3e0d-86c0-c9cafb5b0540",
                    "sense_definition": "Орган управления войсками или воинскими подразделениями, а также здание, где он располагается.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "military_command",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Генеральный <w>штаб</w> принял решение о начале военной операции.",
                            "target_lang_translations": {
                                "en": "The General <w>Staff</w> made the decision to start the military operation."
                            }
                        },
                        {
                            "text": "Командир полка срочно вызвал всех офицеров в <w>штаб</w>.",
                            "target_lang_translations": {
                                "en": "The regiment commander urgently summoned all officers to <w>headquarters</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ставка",
                        "командование"
                    ],
                    "common_phrases": [
                        "генеральный штаб",
                        "начальник штаба",
                        "штаб армии",
                        "офицер штаба"
                    ],
                    "target_lang_definitions": {
                        "en": "A body for commanding troops or military units, as well as the building where it is located."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "headquarters",
                                "target_lang_sense_clarification": "military command center or building"
                            },
                            {
                                "target_lang_word": "staff",
                                "target_lang_sense_clarification": "body of officers, e.g., General Staff"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "270b61fa-1b46-350b-8c53-8b3ac03baf0f",
                    "sense_definition": "Центральный орган, руководящий какой-либо общественной работой, организацией или мероприятием.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "management_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кандидат в президенты приехал в свой предвыборный <w>штаб</w> для встречи с прессой.",
                            "target_lang_translations": {
                                "en": "The presidential candidate arrived at his campaign <w>headquarters</w> to meet the press."
                            }
                        },
                        {
                            "text": "Для борьбы с наводнением в городе был создан оперативный <w>штаб</w>.",
                            "target_lang_translations": {
                                "en": "An operational <w>headquarters</w> was established in the city to combat the flood."
                            }
                        }
                    ],
                    "synonyms": [
                        "центр",
                        "руководство",
                        "управление"
                    ],
                    "common_phrases": [
                        "предвыборный штаб",
                        "избирательный штаб",
                        "оперативный штаб",
                        "штаб строительства"
                    ],
                    "target_lang_definitions": {
                        "en": "A central body directing a public activity, organization, or event."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "headquarters",
                                "target_lang_sense_clarification": "center of operations for a campaign or event"
                            }
                        ]
                    }
                }
            ]
        }
    ]

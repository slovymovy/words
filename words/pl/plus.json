            "senses": [
                {
                    "sense_id": "d1d4af3d-0d33-3cb4-baa5-6d28fea61d01",
                    "sense_definition": "Oznaczający wartość dodatnią (powyżej zera) lub obecność określonego czynnika (np. we krwi).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "adj_positive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja siostra ma grupę krwi A <w>plus</w>, co jest ważną informacją medyczną.",
                            "target_lang_translations": {
                                "ru": "У моей сестры группа крови А <w>плюс</w>, что является важной медицинской информацией."
                            }
                        },
                        {
                            "text": "Mimo kalendarzowej zimy temperatura na zewnątrz jest dziś na <w>plus</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на календарную зиму, температура на улице сегодня в <w>плюсе</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dodać"
                    ],
                    "antonyms": [
                        "minus"
                    ],
                    "common_phrases": [
                        "Rh plus",
                        "grupa krwi plus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначающий положительную величину (выше нуля) или наличие определенного фактора (например, в крови)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плюс",
                                "target_lang_sense_clarification": "положительное значение"
                            },
                            {
                                "target_lang_word": "положительный",
                                "target_lang_sense_clarification": "о температуре, крови"
                            }
                        ]
                    }
                }
            ]
        }
    ]

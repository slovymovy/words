                        {
                            "text": "These <w>beardeds</w> require a second feeding in late summer.",
                            "target_lang_translations": {
                                "ru": "Эти <w>бородатые ирисы</w> требуют второй подкормки в конце лета."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бородатый ирис (растение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бородатый ирис",
                                "target_lang_sense_clarification": "садовое растение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

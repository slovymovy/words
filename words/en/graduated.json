{
    "word_family": [
        "graduation",
        "undergraduate",
        "postgraduate",
        "gradual"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4f9e69da-064d-374f-8108-6adb62ae6508",
                    "sense_definition": "Simple past and past participle of <w>graduate</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "education_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <graduated> from the university with a degree in biology.",
                            "target_lang_translations": {
                                "ru": "Она <окончила> университет со степенью по биологии."
                            }
                        },
                        {
                            "text": "After years of practice, he finally <graduated> to the advanced class.",
                            "target_lang_translations": {
                                "ru": "После многих лет практики он наконец <перешёл> в продвинутый класс."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время от глагола graduate (оканчивать учебное заведение, переходить на новый уровень)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окончил",
                                "target_lang_sense_clarification": "завершил обучение"
                            },
                            {
                                "target_lang_word": "выпустился",
                                "target_lang_sense_clarification": "получил диплом"
                            },
                            {
                                "target_lang_word": "перешёл",
                                "target_lang_sense_clarification": "на следующий уровень"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "99382ece-2db7-3abf-85d4-5a8448dd759e",
                    "sense_definition": "Marked with lines or divisions to indicate measurement.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chemist carefully poured the solution into a <graduated> cylinder.",
                            "target_lang_translations": {
                                "ru": "Химик осторожно налил раствор в <градуированный> цилиндр."
                            }
                        },
                        {
                            "text": "Use a <graduated> jug to measure exactly one liter of water.",
                            "target_lang_translations": {
                                "ru": "Используйте <мерный> кувшин, чтобы отмерить ровно один литр воды."
                            }
                        }
                    ],
                    "synonyms": [
                        "calibrated",
                        "marked"
                    ],
                    "antonyms": [
                        "unmarked"
                    ],
                    "common_phrases": [
                        "graduated cylinder",
                        "graduated pipette",
                        "graduated beaker"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий нанесенные деления или отметки для измерения объема или количества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "градуированный",
                                "target_lang_sense_clarification": "с нанесенной шкалой"
                            },
                            {
                                "target_lang_word": "мерный",
                                "target_lang_sense_clarification": "предназначенный для измерения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5cadc7c9-6918-30b8-be76-50ca17764e16",
                    "sense_definition": "Arranged by grade, level, or degree; increasing in steps.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "progression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government introduced a <graduated> tax system where the wealthy pay more.",
                            "target_lang_translations": {
                                "ru": "Правительство ввело <прогрессивную> систему налогообложения, при которой богатые платят больше."
                            }
                        },
                        {
                            "text": "We need a <graduated> response to the crisis, starting with minor measures.",
                            "target_lang_translations": {
                                "ru": "Нам нужен <поэтапный> ответ на кризис, начиная с незначительных мер."
                            }
                        }
                    ],
                    "synonyms": [
                        "progressive",
                        "incremental",
                        "tiered",
                        "step-by-step"
                    ],
                    "antonyms": [
                        "uniform",
                        "flat"
                    ],
                    "common_phrases": [
                        "graduated tax",
                        "graduated response",
                        "graduated scale"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упорядоченный по степеням или уровням; возрастающий поэтапно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прогрессивный",
                                "target_lang_sense_clarification": "о налогах или ставках"
                            },
                            {
                                "target_lang_word": "ступенчатый",
                                "target_lang_sense_clarification": "изменяющийся по ступеням"
                            },
                            {
                                "target_lang_word": "поэтапный",
                                "target_lang_sense_clarification": "происходящий шаг за шагом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b787a78-0bb5-3b58-9d6d-c7d5691f6de2",
                    "sense_definition": "Having a university degree; having completed training.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "education_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hospital is currently hiring <graduated> nurses for the new ward.",
                            "target_lang_translations": {
                                "ru": "Больница в настоящее время нанимает <дипломированных> медсестер для нового отделения."
                            }
                        },
                        {
                            "text": "A <graduated> engineer is expected to understand these basic principles.",
                            "target_lang_translations": {
                                "ru": "Ожидается, что <дипломированный> инженер понимает эти основные принципы."
                            }
                        }
                    ],
                    "synonyms": [
                        "qualified",
                        "certified",
                        "degreed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий университетскую степень или диплом; прошедший профессиональную подготовку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дипломированный",
                                "target_lang_sense_clarification": "имеющий диплом специалиста"
                            },
                            {
                                "target_lang_word": "квалифицированный",
                                "target_lang_sense_clarification": "обладающий квалификацией"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
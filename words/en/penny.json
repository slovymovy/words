    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "19ecf7c4-f657-35b8-8872-e5276f7e3ad8",
                    "sense_definition": "A coin worth one cent, used in the United States and formerly in Canada; one hundredth of a dollar.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "coins",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I found a lucky <w>penny</w> on the sidewalk this morning.",
                            "target_lang_translations": {
                                "ru": "Сегодня утром я нашёл на тротуаре счастливый <w>пенни</w>."
                            }
                        },
                        {
                            "text": "She saved every <w>penny</w> she earned to buy a new bicycle.",
                            "target_lang_translations": {
                                "ru": "Она откладывала каждую <w>копейку</w>, которую зарабатывала, чтобы купить новый велосипед."
                            }
                        }
                    ],
                    "synonyms": [
                        "cent"
                    ],
                    "common_phrases": [
                        "penny for your thoughts",
                        "pretty penny",
                        "every penny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Монета достоинством в один цент, используемая в США и ранее в Канаде; одна сотая доллара."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "цент",
                                "target_lang_sense_clarification": "номинал монеты"
                            },
                            {
                                "target_lang_word": "пенни",
                                "target_lang_sense_clarification": "разговорное название монеты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1a70959-aba3-32fe-9215-d91e6d192796",
                    "sense_definition": "A coin worth one hundredth of a pound sterling, used in the United Kingdom.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "coins",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sweet shop sells candies for just a <w>penny</w> each.",
                            "target_lang_translations": {
                                "ru": "В кондитерской продаются конфеты всего за один <w>пенс</w>."
                            }
                        },
                        {
                            "text": "Do you have a <w>penny</w> I could borrow for the machine?",
                            "target_lang_translations": {
                                "ru": "У тебя не найдётся <w>пенни</w>, чтобы заплатить за автомат?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "spend a penny",
                        "the penny drops"
                    ],
                    "target_lang_definitions": {
                        "ru": "Монета достоинством в одну сотую фунта стерлингов, используемая в Великобритании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пенс",
                                "target_lang_sense_clarification": "денежная единица и монета"
                            },
                            {
                                "target_lang_word": "пенни",
                                "target_lang_sense_clarification": "монета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b331cb0-94fc-33e4-a3cf-471a8f6ff481",
                    "sense_definition": "A very small sum of money, or money in general.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "money_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It did not cost a <w>penny</w> to enter the museum today.",
                            "target_lang_translations": {
                                "ru": "Вход в музей сегодня не стоил ни <w>копейки</w>."
                            }
                        },
                        {
                            "text": "He arrived in the city without a single <w>penny</w> to his name.",
                            "target_lang_translations": {
                                "ru": "Он приехал в город, не имея за душой ни <w>гроша</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "not a penny",
                        "turn an honest penny",
                        "pennies from heaven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень маленькая сумма денег или деньги в целом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "копейка",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "грош",
                                "target_lang_sense_clarification": "разговорное, ничтожная сумма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8badcdbb-308a-30e1-a641-d1896b99693b",
                    "sense_definition": "A former British coin used before decimalisation, worth 1/240 of a pound.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "coins",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the old days, you could buy a loaf of bread for a <w>penny</w>.",
                            "target_lang_translations": {
                                "ru": "В былые времена буханку хлеба можно было купить за <w>пенни</w>."
                            }
                        },
                        {
                            "text": "He found an old <w>penny</w> buried in the garden soil.",
                            "target_lang_translations": {
                                "ru": "Он нашел старый <w>пенни</w>, зарытый в садовой земле."
                            }
                        }
                    ],
                    "synonyms": [
                        "old penny"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "pre-decimalisation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинная британская монета, использовавшаяся до перехода на десятичную систему, равная 1/240 фунта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пенни",
                                "target_lang_sense_clarification": "историческая монета"
                            },
                            {
                                "target_lang_word": "пенс",
                                "target_lang_sense_clarification": "номинал"
                            }
                        ]
                    }
                }
            ]
        },
        {

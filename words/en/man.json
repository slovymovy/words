                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения удивления, восхищения, гнева или для усиления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блин",
                                "target_lang_sense_clarification": "эвфемизм, выражает досаду или удивление"
                            },
                            {
                                "target_lang_word": "чёрт",
                                "target_lang_sense_clarification": "выражает досаду"
                            }
                        ]
                    }
                }
            ]
        },
        {

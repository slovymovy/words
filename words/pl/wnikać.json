    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "eed87f4a-2782-3b4d-970d-15a857007948",
                    "sense_definition": "Przedostawać się do wnętrza czegoś, przenikać przez coś.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_penetration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deszcz powoli <wnikał> w suchą ziemię po długiej suszy.",
                            "target_lang_translations": {
                                "ru": "Дождь медленно <проникал> в сухую землю после долгой засухи."
                            }
                        },
                        {
                            "text": "Ten krem bardzo szybko <wnika> w skórę i dobrze ją nawilża.",
                            "target_lang_translations": {
                                "ru": "Этот крем очень быстро <впитывается> в кожу и хорошо её увлажняет."
                            }
                        }
                    ],
                    "synonyms": [
                        "penetrować"
                    ],
                    "antonyms": [
                        "wydostawać się"
                    ],
                    "common_phrases": [
                        "wnikać w głąb",
                        "wnikać w strukturę",
                        "wnikać w skórę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проникать внутрь чего-либо, проходить сквозь что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проникать",
                                "target_lang_sense_clarification": "попадать внутрь"
                            },
                            {
                                "target_lang_word": "впитываться",
                                "target_lang_sense_clarification": "о жидкости или креме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ea3f5a3-faaa-32ec-bab4-332f249d94b6",
                    "sense_definition": "Zgłębiać jakąś sprawę, analizować szczegóły, próbować coś zrozumieć.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "mental_investigation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie chcę <wnikać> w twoje prywatne sprawy, jeśli nie masz ochoty o nich mówić.",
                            "target_lang_translations": {
                                "ru": "Я не хочу <вникать> в твои личные дела, если ты не хочешь о них говорить."
                            }
                        },
                        {
                            "text": "Nauczyciel prosił, abyśmy nie <wnikali> w nieistotne szczegóły, lecz skupili się na ogólnym sensie.",
                            "target_lang_translations": {
                                "ru": "Учитель просил, чтобы мы не <вдавались> в несущественные детали, а сосредоточились на общем смысле."
                            }
                        }
                    ],
                    "synonyms": [
                        "dociekać"
                    ],
                    "antonyms": [
                        "ignorować",
                        "pomijać"
                    ],
                    "common_phrases": [
                        "nie wnikać w szczegóły",
                        "wnikać w temat",
                        "wnikać w motywy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Углубляться в суть дела, анализировать детали, пытаться понять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вникать",
                                "target_lang_sense_clarification": "разбираться в сути"
                            },
                            {
                                "target_lang_word": "вдаваться",
                                "target_lang_sense_clarification": "в подробности"
                            },
                            {
                                "target_lang_word": "углубляться",
                                "target_lang_sense_clarification": "изучать детально"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a216c96b-5987-3d56-894c-4a0e301eed27",
                    "sense_definition": "To give a portion of something to others so that they can use or consume it.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_distribute",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will <w>share</w> my sandwich with you because you are hungry.",
                            "target_lang_translations": {
                                "ru": "Я <w>поделюсь</w> с тобой своим бутербродом, потому что ты голоден."
                            }
                        },
                        {
                            "text": "The children were taught to <w>share</w> their toys with each other.",
                            "target_lang_translations": {
                                "ru": "Детей учили <w>делиться</w> игрушками друг с другом."
                            }
                        }
                    ],
                    "synonyms": [
                        "distribute",
                        "give"
                    ],
                    "antonyms": [
                        "keep",
                        "withhold"
                    ],
                    "common_phrases": [
                        "share with",
                        "share between"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дать часть чего-либо другим для использования или потребления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делиться",
                                "target_lang_sense_clarification": "давать часть своего"
                            },
                            {
                                "target_lang_word": "поделиться",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bea8dfed-3a40-3b33-b661-3318adbcf6d0",
                    "sense_definition": "To have, use, or experience something at the same time as someone else.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_common",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>share</w> a small office in the city center.",
                            "target_lang_translations": {
                                "ru": "Мы <w>делим</w> маленький офис в центре города."
                            }
                        },
                        {
                            "text": "They <w>share</w> a strong interest in classical music.",
                            "target_lang_translations": {
                                "ru": "Они <w>разделяют</w> сильный интерес к классической музыке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "share a room",
                        "share an interest",
                        "share a life"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь, использовать или переживать что-либо одновременно с кем-то другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делить",
                                "target_lang_sense_clarification": "использовать совместно"
                            },
                            {
                                "target_lang_word": "разделять",
                                "target_lang_sense_clarification": "чувства, мнения, интересы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09c60309-b447-3bdc-b8af-43666c2979f9",
                    "sense_definition": "To tell someone about your thoughts, feelings, or information.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_communicate",
                    "name_type": "no",
                    "examples": [
                        {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "06ee94b9-7cb5-3077-848e-f8b2c15ea463",
                    "sense_definition": "Een moeilijke beslissing nemen na een periode van onzekerheid of twijfel, vaak om een probleem op te lossen",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "decision_making",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De directeur moest uiteindelijk de knoop <w>doorhakken</w> en het nieuwe beleid goedkeuren.",
                            "target_lang_translations": {
                                "ru": "Директору в конце концов пришлось <w>принять решение</w> и утвердить новую политику."
                            }
                        },
                        {
                            "text": "Na weken van discussie hebben we eindelijk de knoop <w>doorgehakt</w>.",
                            "target_lang_translations": {
                                "ru": "После недель обсуждений мы наконец <w>приняли решение</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "beslissen",
                        "besluiten"
                    ],
                    "antonyms": [
                        "twijfelen",
                        "uitstellen"
                    ],
                    "common_phrases": [
                        "de knoop doorhakken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принять трудное решение после периода неопределенности или сомнений, часто для решения проблемы"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "принять решение",
                                "target_lang_sense_clarification": "сделать выбор, решиться (в составе идиомы)"
                            },
                            {
                                "target_lang_word": "разрубить",
                                "target_lang_sense_clarification": "в выражении 'разрубить гордиев узел'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5aebd7ed-9a7f-3b94-9d1c-65b375270ff2",
                    "sense_definition": "Iets met kracht volledig in tweeën of in stukken slaan met een scherp voorwerp, zoals een bijl",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De houthakker gebruikte zijn bijl om de dikke boomstam <w>door</w> te <w>hakken</w>.",
                            "target_lang_translations": {
                                "ru": "Лесоруб использовал свой топор, чтобы <w>перерубить</w> толстый ствол дерева."
                            }
                        },
                        {
                            "text": "Hij moest het touw <w>doorhakken</w> om de boot los te krijgen van de kade.",
                            "target_lang_translations": {
                                "ru": "Ему пришлось <w>перерубить</w> канат, чтобы отвязать лодку от причала."
                            }
                        }
                    ],
                    "synonyms": [
                        "doorklieven",
                        "splijten"
                    ],
                    "common_phrases": [
                        "een touw doorhakken",
                        "hout doorhakken"
                    ],
                    "target_lang_definitions": {
                        "ru": "С силой разрубить что-либо надвое или на куски острым предметом, например топором"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перерубить",
                                "target_lang_sense_clarification": "разделить надвое ударом топора или другого орудия"
                            },
                            {
                                "target_lang_word": "разрубить",
                                "target_lang_sense_clarification": "рассечь на части"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
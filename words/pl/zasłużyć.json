{
    "word_family": [
        "zasługa",
        "zasługiwać",
        "zasłużony"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "71593e86-27f9-391b-b1b3-f0657d58fd74",
                    "sense_definition": "Stać się godnym czegoś (nagrody, kary, szacunku) dzięki swojemu zachowaniu lub działaniom; uzyskać do czegoś prawo.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "deserve",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uważam, że on w pełni <zasłużył> na ten awans swoją ciężką pracą.",
                            "target_lang_translations": {
                                "ru": "Я считаю, что он полностью <заслужил> это повышение своим упорным трудом."
                            }
                        },
                        {
                            "text": "Twoje nieodpowiedzialne zachowanie sprawiło, że <zasłużyłeś> na surową karę.",
                            "target_lang_translations": {
                                "ru": "Твое безответственное поведение привело к тому, что ты <заслужил> суровое наказание."
                            }
                        }
                    ],
                    "synonyms": [
                        "zapracować"
                    ],
                    "common_phrases": [
                        "zasłużyć na zaufanie",
                        "zasłużyć na szacunek",
                        "zasłużyć na nagrodę",
                        "zasłużyć na karę",
                        "zasłużyć na miano"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать достойным чего-либо (награды, наказания, уважения) благодаря своему поведению или действиям; получить право на что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заслужить",
                                "target_lang_sense_clarification": "стать достойным чего-либо (награды, похвалы, наказания)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8490a51c-2baa-310c-a289-d730feb69ccd",
                    "sense_definition": "Przyczynić się do dobra czegoś lub kogoś poprzez wybitne czyny lub ofiarną pracę (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "merit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego dziadek wybitnie <zasłużył> się w walce o niepodległość kraju.",
                            "target_lang_translations": {
                                "ru": "Его дедушка особо <отличился> в борьбе за независимость страны."
                            }
                        },
                        {
                            "text": "Ta fundacja bardzo <zasłużyła> się dla rozwoju lokalnej kultury.",
                            "target_lang_translations": {
                                "ru": "Этот фонд внес большой <вклад> в развитие местной культуры."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zasłużyć się ojczyźnie",
                        "zasłużyć się dla kraju",
                        "zasłużyć się społeczeństwu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поспособствовать благу чего-либо или кого-либо посредством выдающихся поступков или самоотверженной работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иметь заслуги",
                                "target_lang_sense_clarification": "перед кем-либо или чем-либо"
                            },
                            {
                                "target_lang_word": "внести вклад",
                                "target_lang_sense_clarification": "сделать что-то полезное для развития"
                            },
                            {
                                "target_lang_word": "отличиться",
                                "target_lang_sense_clarification": "проявить себя выдающимся образом (в бою, на службе)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
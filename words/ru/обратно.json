                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Ago (regarding time)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ago",
                                "target_lang_sense_clarification": "time past"
                            },
                            {
                                "target_lang_word": "back",
                                "target_lang_sense_clarification": "time past"
                            }
                        ]
                    }
                }
            ]
        },

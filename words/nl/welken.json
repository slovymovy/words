{
    "word_family": [
        "verwelken",
        "onverwelkbaar"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7b0a8ee5-8d45-3587-a8f1-051547b0ac62",
                    "sense_definition": "Slap worden en uitdrogen, met name gezegd van bloemen, bladeren en planten.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je de planten geen water geeft, zullen ze snel <w>welken</w>.",
                            "target_lang_translations": {
                                "en": "If you don't water the plants, they will quickly <w>wilt</w>."
                            }
                        },
                        {
                            "text": "De rozen in de vaas begonnen na een paar dagen te <w>welken</w>.",
                            "target_lang_translations": {
                                "en": "The roses in the vase started to <w>wilt</w> after a few days."
                            }
                        }
                    ],
                    "synonyms": [
                        "verwelken",
                        "verdorren",
                        "verleppen",
                        "verflensen"
                    ],
                    "antonyms": [
                        "bloeien"
                    ],
                    "common_phrases": [
                        "bloemen welken"
                    ],
                    "target_lang_definitions": {
                        "en": "To become limp and dry out, especially said of flowers, leaves, and plants."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wilt",
                                "target_lang_sense_clarification": "to become limp through heat, loss of water, or disease"
                            },
                            {
                                "target_lang_word": "wither",
                                "target_lang_sense_clarification": "to become dry and shriveled"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ecb88ef1-31cf-38e5-aad0-e5cd9c372f3c",
                    "sense_definition": "Het verliezen van kracht, frisheid of schoonheid (figuurlijk gebruikt).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Haar jeugdige schoonheid begon met de jaren te <w>welken</w>.",
                            "target_lang_translations": {
                                "en": "Her youthful beauty began to <w>fade</w> over the years."
                            }
                        },
                        {
                            "text": "Zonder nieuwe inspiratie zal zijn talent langzaam <w>welken</w>.",
                            "target_lang_translations": {
                                "en": "Without new inspiration, his talent will slowly <w>wither</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wegkwijnen"
                    ],
                    "antonyms": [
                        "opbloeien"
                    ],
                    "target_lang_definitions": {
                        "en": "To lose strength, freshness, or beauty (figurative)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fade",
                                "target_lang_sense_clarification": "to lose freshness or vigor"
                            },
                            {
                                "target_lang_word": "wither",
                                "target_lang_sense_clarification": "to decline or decay"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "37c5be88-7c8c-379b-bd38-6ab42543d1df",
                    "sense_definition": "Oude verbogen vorm van <w>welk</w>, gebruikt als betrekkelijk of vragend voornaamwoord.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De mannen <w>welken</w> wij gisteren spraken, zijn reeds vertrokken.",
                            "target_lang_translations": {
                                "en": "The men <w>whom</w> we spoke to yesterday have already left."
                            }
                        },
                        {
                            "text": "Aan <w>welken</w> van deze heren heeft u de brief overhandigd?",
                            "target_lang_translations": {
                                "en": "To <w>which</w> of these gentlemen did you hand the letter?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "deze naamvalsvorm is in modern Nederlands ongebruikelijk"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Old inflected form of 'welk' (which/who), used as a relative or interrogative pronoun."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "whom",
                                "target_lang_sense_clarification": "archaic/formal object case referring to people"
                            },
                            {
                                "target_lang_word": "which",
                                "target_lang_sense_clarification": "archaic/formal object case referring to a selection"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "dec6fd9b-9e2a-326f-a65f-d921891d864f",
                    "sense_definition": "Relating to, operated by, or produced by machines or machinery.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "machines",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The flight was delayed due to a <w>mechanical</w> failure in the engine.",
                            "target_lang_translations": {
                                "ru": "Рейс был задержан из-за <w>механической</w> неполадки в двигателе."
                            }
                        },
                        {
                            "text": "She decided to study <w>mechanical</w> engineering at the university.",
                            "target_lang_translations": {
                                "ru": "Она решила изучать <w>механическую</w> инженерию в университете."
                            }
                        }
                    ],
                    "synonyms": [
                        "pasteup"
                    ],
                    "antonyms": [
                        "manual"
                    ],
                    "common_phrases": [
                        "mechanical engineering",
                        "mechanical failure",
                        "mechanical device"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к машинам, механизмам или производимый ими."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "механический",
                                "target_lang_sense_clarification": "связанный с машинами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48e22382-10bb-3f79-920d-2e517e7d79fe",
                    "sense_definition": "Relating to the branch of physics that deals with the action of forces on matter and motion.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "machines",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>mechanical</w> energy of the system is conserved during the experiment.",
                            "target_lang_translations": {
                                "ru": "<w>Механическая</w> энергия системы сохраняется во время эксперимента."
                            }
                        },
                        {
                            "text": "Students learned about <w>mechanical</w> forces in the physics lab.",
                            "target_lang_translations": {
                                "ru": "Студенты узнали о <w>механических</w> силах в физической лаборатории."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mechanical energy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к разделу физики, изучающему движение и взаимодействие тел."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "механический",
                                "target_lang_sense_clarification": "физический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "829560cd-d182-325d-ac00-a68434bb1fcf",
                    "sense_definition": "Done without thought, feeling, or spontaneity, as if by a machine.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He gave a <w>mechanical</w> reply to the question, not really listening to what was asked.",
                            "target_lang_translations": {
                                "ru": "Он дал <w>машинальный</w> ответ на вопрос, не особо слушая, о чем его спросили."
                            }
                        },
                        {
                            "text": "Her movements were stiff and <w>mechanical</w> due to extreme exhaustion.",
                            "target_lang_translations": {
                                "ru": "Её движения были скованными и <w>механическими</w> из-за крайнего истощения."
                            }
                        }
                    ],
                    "synonyms": [
                        "pasteup"
                    ],
                    "antonyms": [
                        "emotional",
                        "spontaneous"
                    ],
                    "common_phrases": [
                        "mechanical response",
                        "mechanical repetition"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершаемый автоматически, без участия сознания или чувств; похожий на действия машины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "машинальный",
                                "target_lang_sense_clarification": "автоматический, неосознанный"
                            },
                            {
                                "target_lang_word": "механический",
                                "target_lang_sense_clarification": "безэмоциональный, скованный"
                            }
                        ]

{
    "word_family": [
        "process",
        "processing",
        "microprocessor",
        "coprocessor",
        "multiprocessor",
        "preprocessor"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6ea09549-85c9-326e-9818-131360e8faf2",
                    "sense_definition": "The part of a computer that controls all the other parts and performs operations on data.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "computing_hardware",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My new laptop has a very fast <w>processor</w>.",
                            "target_lang_translations": {
                                "ru": "У моего нового ноутбука очень быстрый <w>процессор</w>."
                            }
                        },
                        {
                            "text": "The <w>processor</w> is often considered the brain of the computer.",
                            "target_lang_translations": {
                                "ru": "<w>Процессор</w> часто считают мозгом компьютера."
                            }
                        }
                    ],
                    "synonyms": [
                        "CPU",
                        "central processing unit"
                    ],
                    "common_phrases": [
                        "fast processor",
                        "computer processor",
                        "dual-core processor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть компьютера, которая управляет всеми остальными частями и выполняет операции с данными."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "процессор",
                                "target_lang_sense_clarification": "компонент компьютера"
                            },
                            {
                                "target_lang_word": "центральный процессор",
                                "target_lang_sense_clarification": "ЦП"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92b8cb27-3916-3a44-8562-a75a58ccd550",
                    "sense_definition": "A machine or device used to prepare, treat, or change something, especially food.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "machines",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She used a food <w>processor</w> to chop the vegetables quickly.",
                            "target_lang_translations": {
                                "ru": "Она использовала кухонный <w>комбайн</w>, чтобы быстро нарезать овощи."
                            }
                        },
                        {
                            "text": "This old word <w>processor</w> is no longer compatible with modern printers.",
                            "target_lang_translations": {
                                "ru": "Этот старый текстовый <w>процессор</w> больше не совместим с современными принтерами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "food processor",
                        "word processor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Машина или устройство, используемое для подготовки, обработки или изменения чего-либо, особенно пищи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кухонный комбайн",
                                "target_lang_sense_clarification": "для обработки пищи"
                            },
                            {
                                "target_lang_word": "процессор",
                                "target_lang_sense_clarification": "устройство или программа (напр. текстовый процессор)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be9b31cd-7b99-3099-b8b2-ef91a5aecd42",
                    "sense_definition": "A person or organization responsible for handling documents, applications, or data.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "roles",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The loan <w>processor</w> will review your application within three days.",
                            "target_lang_translations": {
                                "ru": "<w>Специалист</w> по оформлению кредитов рассмотрит вашу заявку в течение трех дней."
                            }
                        },
                        {
                            "text": "He works as a data <w>processor</w> for a large insurance company.",
                            "target_lang_translations": {
                                "ru": "Он работает <w>обработчиком</w> данных в крупной страховой компании."
                            }
                        }
                    ],
                    "common_phrases": [
                        "loan processor",
                        "data processor",
                        "claims processor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или организация, ответственные за обработку документов, заявок или данных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обработчик",
                                "target_lang_sense_clarification": "тот, кто обрабатывает данные"
                            },
                            {
                                "target_lang_word": "оператор",
                                "target_lang_sense_clarification": "должность (напр. оператор данных)"
                            },
                            {
                                "target_lang_word": "специалист",
                                "target_lang_sense_clarification": "сотрудник, работающий с документами"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "frequency": "VeryLow",
                    "semantic_group_id": "horse_gear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пастух надел на ноги коня тяжелое <w>путло</w>, чтобы тот не ушел далеко в лес."
                        },
                        {
                            "text": "Лошадь медленно передвигалась по полю, так как ей мешало <w>путло</w>."
                        }
                    ],
                    "synonyms": [
                        "путы",
                        "путлище"
                    ],
                    "common_phrases": [
                        "надеть путло",
                        "конское путло"

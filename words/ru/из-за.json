            "senses": [
                {
                    "sense_id": "0c82dc28-dffe-340b-a17a-384d13864cd1",
                    "sense_definition": "Обозначает движение из места, находящегося позади или с обратной стороны чего-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_origin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Внезапно <w>из-за</w> угла выехала большая машина."
                        },
                        {
                            "text": "Яркое солнце наконец выглянуло <w>из-за</w> тёмных туч."
                        }
                    ],
                    "common_phrases": [
                        "из-за угла",
                        "из-за туч",
                        "из-за границы",
                        "из-за двери",
                        "из-за спины"
                    ]
                },
                {
                    "sense_id": "44ffc8a8-03cf-3631-a304-4c1a7bcac157",
                    "sense_definition": "Указывает на причину, основание или виновника действия или состояния (часто с оттенком негативного результата).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "causal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы опоздали на поезд <w>из-за</w> сильного дождя."
                        },
                        {
                            "text": "Друзья поссорились <w>из-за</w> какой-то ерунды."
                        }
                    ],
                    "synonyms": [
                        "по причине",
                        "вследствие",
                        "ввиду"
                    ],
                    "common_phrases": [
                        "из-за этого",
                        "из-за тебя",
                        "из-за денег",
                        "из-за ерунды",
                        "из-за проблем"
                    ]
                },
                {
                    "sense_id": "79e7193f-c3a7-3254-94c6-b38a803a8316",
                    "sense_definition": "Обозначает движение от предмета, за которым человек сидел или занимался каким-либо делом.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец поблагодарил за обед и встал <w>из-за</w> стола."
                        },
                        {
                            "text": "Ученик вышел <w>из-за</w> парты и направился к доске."
                        }
                    ],
                    "common_phrases": [
                        "встать из-за стола",
                        "выйти из-за парты",
                        "встать из-за рояля"
                    ]
                }
            ]
        }
    ]

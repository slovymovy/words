    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "af70ac4c-11d0-3512-b8bf-d3659e3da428",
                    "sense_definition": "Iemand die de leiding heeft over een militaire eenheid, een leger of een vloot.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "leader",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>commandant</w> gaf het bevel om de aanval te beginnen.",
                            "target_lang_translations": {
                                "en": "The <w>commander</w> gave the order to start the attack.",
                                "ru": "<w>Командир</w> отдал приказ начать атаку."
                            }
                        },
                        {
                            "text": "Alle soldaten moeten onmiddellijk luisteren naar hun <w>commandant</w>.",
                            "target_lang_translations": {
                                "en": "All soldiers must listen to their <w>commander</w> immediately.",
                                "ru": "Все солдаты должны немедленно слушаться своего <w>командира</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bevelhebber",
                        "bevelvoerder",
                        "legeraanvoerder"
                    ],
                    "common_phrases": [
                        "militaire commandant",
                        "commandant der strijdkrachten",
                        "opperste commandant"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who is in charge of a military unit, an army, or a fleet.",
                        "ru": "Тот, кто командует воинской частью, армией или флотом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "commander",
                                "target_lang_sense_clarification": "military leader"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "командир",
                                "target_lang_sense_clarification": "воинской части"
                            },
                            {
                                "target_lang_word": "командующий",
                                "target_lang_sense_clarification": "армией или флотом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8525f1e-e2ba-391f-adef-5ace36f16c7b",
                    "sense_definition": "De leidinggevende van een groep politieagenten of een brandweerkorps.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "leader",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>commandant</w> van de brandweer gaf het sein dat de brand meester was.",
                            "target_lang_translations": {
                                "en": "The fire <w>commander</w> gave the signal that the fire was under control.",
                                "ru": "<w>Начальник</w> пожарной команды дал сигнал, что пожар под контролем."
                            }
                        },
                        {
                            "text": "Na het incident sprak de <w>commandant</w> van de politie met de pers.",
                            "target_lang_translations": {
                                "en": "After the incident, the police <w>commander</w> spoke to the press.",
                                "ru": "После происшествия <w>начальник</w> полиции пообщался с прессой."
                            }
                        }
                    ],
                    "synonyms": [
                        "bevelvoerder"
                    ],
                    "common_phrases": [
                        "commandant brandweer",
                        "plaatsvervangend commandant"
                    ],
                    "target_lang_definitions": {
                        "en": "The person in charge of a group of police officers or a fire brigade.",
                        "ru": "Руководитель группы полицейских или пожарной команды."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "commander",
                                "target_lang_sense_clarification": "police or fire brigade leader"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "начальник",
                                "target_lang_sense_clarification": "полиции или пожарной службы"
                            },
                            {
                                "target_lang_word": "командир",
                                "target_lang_sense_clarification": "подразделения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "06c22eaa-3c40-3b0a-a02f-fa38f1531d57",
                    "sense_definition": "A document or card that shows who a person is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to show some form of <w>identification</w> to enter the secure area.",
                            "target_lang_translations": {
                                "ru": "Вам нужно предъявить какое-либо <w>удостоверение личности</w>, чтобы войти в охраняемую зону."
                            }
                        },
                        {
                            "text": "Please keep your <w>identification</w> with you at all times while traveling.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, всегда держите <w>документы</w> при себе во время путешествия."
                            }
                        }
                    ],
                    "synonyms": [
                        "ID",
                        "papers",
                        "documents"
                    ],
                    "common_phrases": [
                        "photo identification",
                        "valid identification",
                        "show identification",
                        "form of identification"
                    ],
                    "target_lang_definitions": {
                        "ru": "Документ, удостоверяющий личность человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удостоверение личности",
                                "target_lang_sense_clarification": "официальный документ"
                            },
                            {
                                "target_lang_word": "документы",
                                "target_lang_sense_clarification": "разговорное, общее обозначение паспорта или прав"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb39b1be-088a-3f3a-85ae-8528b7283565",
                    "sense_definition": "The act of recognizing someone or something and proving who or what they are.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The witness assisted the police in the <w>identification</w> of the suspect.",
                            "target_lang_translations": {
                                "ru": "Свидетель помог полиции в <w>опознании</w> подозреваемого."
                            }
                        },
                        {
                            "text": "Accurate <w>identification</w> of the problem is the first step to solving it.",
                            "target_lang_translations": {
                                "ru": "Точная <w>идентификация</w> проблемы — это первый шаг к её решению."
                            }
                        }
                    ],
                    "synonyms": [
                        "documents",
                        "ID",
                        "papers"
                    ],
                    "common_phrases": [
                        "identification of",
                        "positive identification"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс распознавания кого-либо или установления тождества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опознание",
                                "target_lang_sense_clarification": "установление личности (обычно в криминалистике)"
                            },
                            {
                                "target_lang_word": "идентификация",
                                "target_lang_sense_clarification": "технический или научный процесс установления тождества"
                            },
                            {
                                "target_lang_word": "установление личности",
                                "target_lang_sense_clarification": "формальная процедура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b107588b-db2e-36a4-b9d7-c24482f39d53",
                    "sense_definition": "A strong feeling of sympathy, understanding, or shared connection with someone or something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her strong <w>identification</w> with the main character made the movie very emotional for her.",
                            "target_lang_translations": {
                                "ru": "Её сильное <w>отождествление</w> себя с главной героиней сделало фильм очень эмоциональным для неё."
                            }
                        },
                        {
                            "text": "The author felt a deep <w>identification</w> with the struggles of the working class.",
                            "target_lang_translations": {
                                "ru": "Автор чувствовал глубокую <w>солидарность</w> с борьбой рабочего класса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "identification with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство глубокой эмоциональной связи, понимания или отождествления себя с кем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отождествление",
                                "target_lang_sense_clarification": "психологический процесс"
                            },
                            {
                                "target_lang_word": "солидарность",
                                "target_lang_sense_clarification": "чувство единства и поддержки"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
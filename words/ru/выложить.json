            "senses": [
                {
                    "sense_id": "3eafc179-2af0-3ae9-8d49-c5ca5d0d802a",
                    "sense_definition": "Достать что-либо откуда-нибудь (из сумки, кармана и т.п.) и положить на поверхность.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я попросил брата <выложить> все вещи из рюкзака на пол для проверки.",
                            "target_lang_translations": {
                                "en": "I asked my brother to <take> all the things <out> of the backpack onto the floor for inspection."
                            }
                        },
                        {
                            "text": "Покупатель подошёл к кассе и начал <выкладывать> продукты на ленту.",
                            "target_lang_translations": {
                                "en": "The customer approached the register and started <laying out> the groceries on the belt."
                            }
                        }
                    ],
                    "synonyms": [
                        "достать",
                        "вынуть"
                    ],
                    "antonyms": [
                        "убрать",
                        "спрятать",
                        "сложить"
                    ],
                    "common_phrases": [
                        "выложить вещи",
                        "выложить из сумки",
                        "выложить на стол",
                        "выложить продукты"
                    ],
                    "target_lang_definitions": {
                        "en": "To take something out from somewhere (a bag, pocket, etc.) and place it on a surface."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take out",
                                "target_lang_sense_clarification": "general action of removing items"
                            },
                            {
                                "target_lang_word": "lay out",
                                "target_lang_sense_clarification": "to place and arrange on a surface"
                            },
                            {
                                "target_lang_word": "empty out",
                                "target_lang_sense_clarification": "to remove all contents"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1643550-2014-3f78-850e-138c792698a6",
                    "sense_definition": "Опубликовать информацию, фотографию, видео или файл в интернете (в социальной сети, на сайте).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вчера вечером она <выложила> новые фотографии из отпуска в свой профиль.",
                            "target_lang_translations": {
                                "en": "Last night she <posted> new photos from her vacation to her profile."
                            }
                        },
                        {
                            "text": "Блогер пообещал завтра <выложить> видео с обзором нового телефона.",
                            "target_lang_translations": {
                                "en": "The blogger promised to <upload> a video with a review of the new phone tomorrow."
                            }
                        }
                    ],
                    "synonyms": [
                        "опубликовать",
                        "разместить",
                        "запостить"
                    ],
                    "antonyms": [
                        "удалить"
                    ],
                    "common_phrases": [
                        "выложить фото",
                        "выложить пост",
                        "выложить в сеть",
                        "выложить видео"
                    ],
                    "target_lang_definitions": {
                        "en": "To publish information, a photo, a video, or a file on the internet (on a social network, on a website)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "post",
                                "target_lang_sense_clarification": "on social media or forums"
                            },
                            {
                                "target_lang_word": "upload",
                                "target_lang_sense_clarification": "files or videos to a server"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73aaff53-6c86-324c-99db-af9bf80e4cf8",
                    "sense_definition": "Покрыть поверхность чем-либо (плиткой, камнем) или соорудить что-либо из строительного материала.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мастера должны <выложить> плиткой стены в ванной комнате до конца недели.",
                            "target_lang_translations": {
                                "en": "The workmen must <tile> the walls in the bathroom by the end of the week."
                            }
                        },
                        {
                            "text": "Дедушка решил самостоятельно <выложить> печь на даче из красного кирпича.",
                            "target_lang_translations": {
                                "en": "Grandpa decided to <build> a stove out of red brick at the dacha himself."
                            }

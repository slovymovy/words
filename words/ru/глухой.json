                    "sense_id": "98327469-7378-355f-b5e6-2205ea429e35",
                    "sense_definition": "Лишённый слуха, не способный слышать.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ebd13f55-d48a-365b-afb7-99371b51477e",
                    "sense_definition": "Принять пищу внутрь; проглотить что-либо съедобное.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу <съесть> это красное яблоко.",
                            "target_lang_translations": {
                                "en": "I want to <w>eat</w> this red apple."
                            }
                        },
                        {
                            "text": "Дети быстро <съели> весь суп и попросили добавки.",
                            "target_lang_translations": {
                                "en": "The children quickly <w>ate</w> all the soup and asked for seconds."
                            }
                        }
                    ],
                    "synonyms": [
                        "пое́сть",
                        "скушать",
                        "сожрать"
                    ],
                    "antonyms": [
                        "изрыгнуть"
                    ],
                    "common_phrases": [
                        "съесть завтрак",
                        "съесть кусок",
                        "быстро съесть",
                        "хотеть съесть"
                    ],
                    "target_lang_definitions": {
                        "en": "To take food into the body; to swallow something edible."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "eat",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "consume",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2272d9e2-74bf-3d2b-ad7a-ee6f2e1a6112",
                    "sense_definition": "В настольных играх (шахматы, шашки): убрать фигуру противника с доски, заняв её место.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "games",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой конь <съел> твою ладью на следующем ходу.",
                            "target_lang_translations": {
                                "en": "My knight <w>took</w> your rook on the next move."
                            }
                        },
                        {
                            "text": "Ты можешь <съесть> эту пешку, но это ловушка.",
                            "target_lang_translations": {
                                "en": "You can <w>take</w> this pawn, but it's a trap."
                            }
                        }
                    ],
                    "synonyms": [
                        "побить"
                    ],
                    "common_phrases": [
                        "съесть пешку",
                        "съесть ферзя",
                        "съесть фигуру"
                    ],
                    "target_lang_definitions": {
                        "en": "In board games (chess, checkers): to remove an opponent's piece from the board by occupying its square."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "chess/checkers context"
                            },
                            {
                                "target_lang_word": "capture",
                                "target_lang_sense_clarification": "formal game context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1bbbc7f1-56f1-38ec-bc13-71705c617ea1",
                    "sense_definition": "Полностью истратить или уничтожить что-либо (деньги, время, ресурсы).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ремонт квартиры <съел> все наши семейные сбережения.",
                            "target_lang_translations": {

{
    "word_family": [
        "integral",
        "integrate",
        "integration",
        "integer"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "860e4f5a-a6e0-309e-9816-95420a407483",
                    "sense_definition": "The quality of being honest and having strong moral principles.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "moral_character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is a woman of great <w>integrity</w> who would never lie to her clients.",
                            "target_lang_translations": {
                                "ru": "Она женщина исключительной <w>честности</w>, которая никогда не солжет своим клиентам."
                            }
                        },
                        {
                            "text": "It is important to act with <w>integrity</w> even when no one is watching.",
                            "target_lang_translations": {
                                "ru": "Важно сохранять <w>порядочность</w>, даже когда никто не видит."
                            }
                        }
                    ],
                    "synonyms": [
                        "honesty",
                        "uprightness",
                        "probity",
                        "rectitude",
                        "sincerity",
                        "virtue",
                        "decency",
                        "goodness"
                    ],
                    "antonyms": [
                        "dishonesty",
                        "corruption",
                        "deceit"
                    ],
                    "common_phrases": [
                        "man of integrity",
                        "personal integrity",
                        "act with integrity",
                        "professional integrity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество, заключающееся в честности и наличии твердых моральных принципов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "честность",
                                "target_lang_sense_clarification": "правдивость, искренность"
                            },
                            {
                                "target_lang_word": "порядочность",
                                "target_lang_sense_clarification": "строгое соблюдение норм морали, неспособность к низким поступкам"
                            },
                            {
                                "target_lang_word": "принципиальность",
                                "target_lang_sense_clarification": "верность своим принципам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc1b4e56-8283-3fd4-9a87-5ddd4c96cad4",
                    "sense_definition": "The state of being whole, complete, and undivided.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "completeness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The engineer checked the structural <w>integrity</w> of the bridge after the earthquake.",
                            "target_lang_translations": {
                                "ru": "Инженер проверил структурную <w>целостность</w> моста после землетрясения."
                            }
                        },
                        {
                            "text": "The treaty guarantees the territorial <w>integrity</w> of the nation.",
                            "target_lang_translations": {
                                "ru": "Договор гарантирует территориальную <w>целостность</w> страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "wholeness",
                        "unity"
                    ],
                    "antonyms": [
                        "fragmentation",
                        "incompleteness"
                    ],
                    "common_phrases": [
                        "structural integrity",
                        "territorial integrity",
                        "maintain integrity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние цельности, полноты и нераздельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "целостность",
                                "target_lang_sense_clarification": "полнота, отсутствие повреждений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76dc06e7-0a01-3e19-9f8c-854930ee033a",
                    "sense_definition": "The condition of data being accurate, consistent, and protected from unauthorized alteration.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "data_security",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We use advanced encryption to ensure the <w>integrity</w> of the transmitted files.",
                            "target_lang_translations": {
                                "ru": "Мы используем передовое шифрование для обеспечения <w>целостности</w> передаваемых файлов."
                            }
                        },
                        {
                            "text": "A checksum is often used to verify data <w>integrity</w> during transfer.",
                            "target_lang_translations": {
                                "ru": "Контрольная сумма часто используется для проверки <w>целостности</w> данных при передаче."
                            }
                        }
                    ],
                    "common_phrases": [
                        "data integrity",
                        "verify integrity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние данных, при котором они точны, последовательны и защищены от несанкционированного изменения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "целостность",
                                "target_lang_sense_clarification": "неизменность и достоверность информации"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
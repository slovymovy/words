{
    "word_family": [
        "bunchy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b182b7ba-fb9f-334b-9ab8-cc1f02b368be",
                    "sense_definition": "A group of similar things, either growing together, or in a cluster or clump, usually fastened together.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_physical_collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bought a fresh <w>bunch</w> of bananas from the market stall.",
                            "target_lang_translations": {
                                "ru": "Она купила свежую <w>связку</w> бананов в палатке на рынке."
                            }
                        },
                        {
                            "text": "I cannot find the right key on this large <w>bunch</w>.",
                            "target_lang_translations": {
                                "ru": "Я не могу найти нужный ключ в этой большой <w>связке</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cluster",
                        "group"
                    ],
                    "common_phrases": [
                        "bunch of grapes",
                        "bunch of keys",
                        "bunch of flowers",
                        "bunch of bananas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа одинаковых предметов, растущих вместе или собранных в кластер, обычно скрепленных вместе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связка",
                                "target_lang_sense_clarification": "ключей, бананов"
                            },
                            {
                                "target_lang_word": "пучок",
                                "target_lang_sense_clarification": "зелени, редиса"
                            },
                            {
                                "target_lang_word": "гроздь",
                                "target_lang_sense_clarification": "винограда, ягод"
                            },
                            {
                                "target_lang_word": "букет",
                                "target_lang_sense_clarification": "цветов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19d52586-b8ec-3e96-ba26-24ede8232f3a",
                    "sense_definition": "An informal body of friends or a specific group of people.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He went to the cinema with a <w>bunch</w> of his school friends.",
                            "target_lang_translations": {
                                "ru": "Он пошел в кино с <w>компанией</w> своих школьных друзей."
                            }
                        },
                        {
                            "text": "They are a really friendly <w>bunch</w> once you get to know them.",
                            "target_lang_translations": {
                                "ru": "Они очень дружелюбная <w>компания</w>, когда узнаешь их поближе."
                            }
                        }
                    ],
                    "synonyms": [
                        "group",
                        "circle",
                        "gang",
                        "pack"
                    ],
                    "common_phrases": [
                        "the whole bunch",
                        "wild bunch",
                        "friendly bunch"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неформальная группа друзей или определенная группа людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "компания",
                                "target_lang_sense_clarification": "друзей, людей"
                            },
                            {
                                "target_lang_word": "группа",
                                "target_lang_sense_clarification": "людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3555a1d-4270-3059-8d43-12a14c436975",
                    "sense_definition": "A considerable amount or number of something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We have a <w>bunch</w> of work to finish before the weekend.",
                            "target_lang_translations": {
                                "ru": "У нас <w>куча</w> работы, которую нужно закончить до выходных."
                            }
                        },
                        {
                            "text": "I have seen that movie a <w>bunch</w> of times already.",
                            "target_lang_translations": {
                                "ru": "Я видел этот фильм уже <w>кучу</w> раз."
                            }
                        }
                    ],
                    "common_phrases": [
                        "a bunch of stuff",
                        "thanks a bunch",
                        "whole bunch"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Значительное количество или число чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "куча",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "масса",
                                "target_lang_sense_clarification": "большое количество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2dea3e41-b7b1-36e6-b77e-9513adf5f459",
                    "sense_definition": "The main group of riders formed during a bicycle race; the peloton.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_cycling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cyclist tried to break away from the <w>bunch</w> near the finish line.",
                            "target_lang_translations": {
                                "ru": "Велосипедист попытался оторваться от <w>пелотона</w> возле финишной черты."
                            }
                        },
                        {
                            "text": "Riding inside the <w>bunch</w> helps athletes save energy by reducing wind resistance.",
                            "target_lang_translations": {
                                "ru": "Езда внутри <w>пелотона</w> помогает спортсменам экономить энергию за счет снижения сопротивления ветра."
                            }
                        }
                    ],
                    "common_phrases": [
                        "back of the bunch",
                        "sprint from the bunch"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "cycling terminology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Основная группа гонщиков в велосипедной гонке; пелотон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пелотон",
                                "target_lang_sense_clarification": "велоспорт"
                            },
                            {
                                "target_lang_word": "основная группа",
                                "target_lang_sense_clarification": "гонщиков"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4be8569-2f10-368a-bd70-938bb56a1994",
                    "sense_definition": "An unusual concentration of ore in a lode or a small, discontinuous occurrence of ore.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_geology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The miners were lucky to discover a rich <w>bunch</w> of copper.",
                            "target_lang_translations": {
                                "ru": "Шахтерам повезло обнаружить богатое <w>гнездо</w> меди."
                            }
                        },
                        {
                            "text": "Geologists mapped every small <w>bunch</w> of minerals found in the rock wall.",
                            "target_lang_translations": {
                                "ru": "Геологи нанесли на карту каждое небольшое <w>скопление</w> минералов, найденное в скальной стене."
                            }
                        }
                    ],
                    "synonyms": [
                        "pocket",
                        "nest",
                        "kidney"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "geology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Необычное скопление руды в жиле или небольшое прерывистое проявление руды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гнездо",
                                "target_lang_sense_clarification": "рудное"
                            },
                            {
                                "target_lang_word": "скопление",
                                "target_lang_sense_clarification": "минералов"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3d227af1-39e5-3fdd-8ce7-6f0a029b31c7",
                    "sense_definition": "To gather or form into a cluster or group.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_physical_collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The runners started to <w>bunch</w> together as they approached the narrow turn.",
                            "target_lang_translations": {
                                "ru": "Бегуны начали <w>сбиваться в кучу</w>, приближаясь к узкому повороту."
                            }
                        },
                        {
                            "text": "Please <w>bunch</w> the flowers tightly before you tie them with a ribbon.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, плотно <w>соберите</w> цветы <w>в пучок</w>, прежде чем перевязывать их лентой."
                            }
                        }
                    ],
                    "synonyms": [
                        "cluster",
                        "group"
                    ],
                    "common_phrases": [
                        "bunch together",
                        "bunch up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собираться или формироваться в кластер или группу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбиваться в кучу",
                                "target_lang_sense_clarification": "о людях, животных"
                            },
                            {
                                "target_lang_word": "собирать в пучок",
                                "target_lang_sense_clarification": "о цветах, волосах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbb6d8a8-2ddd-3e67-968e-57723c0e1415",
                    "sense_definition": "To gather fabric into folds or to become gathered in folds.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_fabric",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her thick socks started to <w>bunch</w> up inside her winter boots.",
                            "target_lang_translations": {
                                "ru": "Ее толстые носки начали <w>сбориться</w> внутри зимних ботинок."
                            }
                        },
                        {
                            "text": "The curtains tend to <w>bunch</w> at the floor because they are too long.",
                            "target_lang_translations": {
                                "ru": "Шторы имеют тенденцию <w>собираться в складки</w> на полу, потому что они слишком длинные."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bunch up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собирать ткань в складки или собираться складками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбориться",
                                "target_lang_sense_clarification": "о ткани, одежде"
                            },
                            {
                                "target_lang_word": "собираться в складки",
                                "target_lang_sense_clarification": "о ткани"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "ecc66e9e-929a-362c-aee1-530cb8593bf1",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Bunch</w> has been our neighbor for over twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Банч</w> был нашим соседом более двадцати лет."
                            }
                        },
                        {
                            "text": "We filed the documents under the name <w>Bunch</w> in the cabinet.",
                            "target_lang_translations": {
                                "ru": "Мы подшили документы под фамилией <w>Банч</w> в картотеку."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Банч",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
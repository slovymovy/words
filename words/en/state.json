    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b917265b-7822-3a8d-900e-90f5fcaafec3",
                    "sense_definition": "A condition or set of circumstances applying at any given time.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "condition_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The house was in a poor <w>state</w> of repair when we bought it.",
                            "target_lang_translations": {
                                "ru": "Дом был в плохом <w>состоянии</w>, когда мы его купили."
                            }
                        },
                        {
                            "text": "She was in a <w>state</w> of shock after hearing the bad news.",
                            "target_lang_translations": {
                                "ru": "Она была в <w>состоянии</w> шока, услышав плохие новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "condition",
                        "situation",
                        "shape",
                        "status",
                        "position"
                    ],
                    "common_phrases": [
                        "state of mind",
                        "state of affairs",
                        "steady state",
                        "mental state"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическое или душевное положение, в котором находится кто-либо или что-либо; обстоятельства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "состоя́ние",
                                "target_lang_sense_clarification": "общее физическое или моральное самочувствие, вид"
                            },
                            {
                                "target_lang_word": "положе́ние",
                                "target_lang_sense_clarification": "ситуация, обстоятельства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "557a690c-765c-3cf9-a580-5f2f6f7ea2ef",
                    "sense_definition": "A political division of a federation retaining a notable degree of autonomy, such as in the United States.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "politics_group",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "There are fifty <w>states</w> in the United States of America.",
                            "target_lang_translations": {
                                "ru": "В Соединенных Штатах Америки пятьдесят <w>штатов</w>."
                            }
                        },
                        {
                            "text": "He moved to the <w>state</w> of California last year.",
                            "target_lang_translations": {
                                "ru": "В прошлом году он переехал в <w>штат</w> Калифорния."
                            }
                        }
                    ],
                    "synonyms": [
                        "province",
                        "region"
                    ],
                    "common_phrases": [
                        "United States",
                        "state line",
                        "state capital"
                    ],
                    "target_lang_definitions": {
                        "ru": "Административно-территориальная единица в ряде федеративных государств (например, в США, Индии, Австралии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штат",
                                "target_lang_sense_clarification": "административная единица в США и других странах"
                            },
                            {
                                "target_lang_word": "земля́",
                                "target_lang_sense_clarification": "административная единица в Германии и Австрии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dba63a0d-92fb-3655-b316-ed97e7ab59ca",
                    "sense_definition": "A nation or territory considered as an organized political community under one government.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "politics_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>state</w> provides free education for all children.",
                            "target_lang_translations": {
                                "ru": "<w>Государство</w> предоставляет бесплатное образование всем детям."
                            }
                        },
                        {
                            "text": "Church and <w>state</w> are separate institutions in this country.",
                            "target_lang_translations": {
                                "ru": "Церковь и <w>государство</w> — это раздельные институты в этой стране."
                            }
                        }
                    ],
                    "synonyms": [
                        "country",
                        "nation",
                        "government",
                        "polity"
                    ],
                    "common_phrases": [
                        "head of state",
                        "church and state",
                        "welfare state",
                        "police state"
                    ],
                    "target_lang_definitions": {
                        "ru": "Политическая организация общества с определённой территорией и властью; страна."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "госуда́рство",
                                "target_lang_sense_clarification": "политическая организация общества"
                            },
                            {
                                "target_lang_word": "держа́ва",
                                "target_lang_sense_clarification": "сильное, независимое государство (высокий стиль)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e25e1b2b-88a1-3c6b-9281-0fcc2434f1c2",
                    "sense_definition": "The physical property of matter as solid, liquid, gas, or plasma.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "condition_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Water can exist in a solid, liquid, or gaseous <w>state</w>.",
                            "target_lang_translations": {
                                "ru": "Вода может существовать в твердом, жидком или газообразном <w>состоянии</w>."
                            }
                        },
                        {
                            "text": "Heat causes a change of <w>state</w> from ice to water.",
                            "target_lang_translations": {
                                "ru": "Тепло вызывает изменение <w>состояния</w> (превращение) льда в воду."
                            }
                        }
                    ],
                    "synonyms": [
                        "phase",
                        "form"
                    ],
                    "common_phrases": [
                        "solid state",
                        "liquid state",
                        "change of state"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "scientific"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Агрегатная форма существования материи (твёрдая, жидкая, газообразная)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "состоя́ние",
                                "target_lang_sense_clarification": "физическая форма материи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f712a80d-b9bf-301f-8b70-dede66e9dfcd",
                    "sense_definition": "A mess, disorder, or a bad condition; also, a state of emotional agitation.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "condition_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Look at the <w>state</w> of this room; it is completely untidy!",
                            "target_lang_translations": {
                                "ru": "Посмотри, в каком <w>состоянии</w> эта комната; здесь полный беспорядок!"
                            }
                        },
                        {
                            "text": "Don't get yourself into a <w>state</w> over such a small problem.",
                            "target_lang_translations": {
                                "ru": "Не доводи себя до такого <w>состояния</w> из-за такой мелкой проблемы."
                            }
                        }
                    ],
                    "synonyms": [
                        "mess",
                        "pickle",
                        "plight",
                        "predicament",
                        "fix",
                        "jam"
                    ],
                    "common_phrases": [

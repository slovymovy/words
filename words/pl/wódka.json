    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8a17e388-bfcb-3b70-ac0e-6df9cc8324fe",
                    "sense_definition": "Wysokoprocentowy napój alkoholowy składający się z destylatu alkoholowego i wody, czasem z dodatkiem innych składników.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "alcohol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na weselu goście pili zimną <w>wódkę</w> i jedli tradycyjne polskie potrawy.",
                            "target_lang_translations": {
                                "ru": "На свадьбе гости пили холодную <w>водку</w> и ели традиционные польские блюда."
                            }
                        },
                        {
                            "text": "Mój dziadek wypija jeden kieliszek <w>wódki</w> do niedzielnego obiadu.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка выпивает одну рюмку <w>водки</w> за воскресным обедом."
                            }
                        }
                    ],
                    "synonyms": [
                        "gorzałka",
                        "czysta"
                    ],
                    "common_phrases": [
                        "kieliszek wódki",
                        "butelka wódki",
                        "pić wódkę",
                        "czysta wódka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крепкий алкогольный напиток, состоящий из спиртового дистиллята и воды, иногда с добавлением других ингредиентов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "водка",
                                "target_lang_sense_clarification": "алкогольный напиток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba597190-3cc7-319d-97a6-0bc3f7b7da3c",
                    "sense_definition": "Dawne określenie na płynny kosmetyk zapachowy, perfumy lub wodę toaletową, a także na lecznicze płyny ziołowe.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "cosmetic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej koronkowa chusteczka pachniała delikatną <w>wódką</w> różaną.",
                            "target_lang_translations": {
                                "ru": "Её кружевной платочек пах нежной розовой <w>водкой</w>."
                            }
                        },
                        {
                            "text": "Aptekarz przygotował leczniczą <w>wódkę</w> z ziół na bóle głowy.",
                            "target_lang_translations": {
                                "ru": "Аптекарь приготовил лечебную <w>водку</w> из трав от головной боли."
                            }
                        }
                    ],
                    "synonyms": [
                        "perfumy",
                        "pachnidło"
                    ],
                    "common_phrases": [
                        "wódka kolońska",
                        "wódka lawendowa"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "współcześnie słowo to oznacza prawie wyłącznie alkohol"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее название жидкого парфюмерного средства, духов или туалетной воды, а также лечебных травяных настоев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "водка",
                                "target_lang_sense_clarification": "устар. настой, косметическое средство"
                            }
                        ]
                    }
                }
            ]
        }
    ]

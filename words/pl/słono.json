            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "994beaa9-4640-3a26-acec-38c156b52b0a",
                    "sense_definition": "W sposób, który ma smak soli lub zawiera dużo soli.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "taste",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta zupa smakuje dzisiaj wyjątkowo <w>słono</w>.",
                            "target_lang_translations": {
                                "ru": "Этот суп сегодня на вкус исключительно <w>солёный</w>."
                            }
                        },
                        {
                            "text": "Zazwyczaj jem potrawy przygotowane <w>słono</w>, bo lubię wyraziste smaki.",
                            "target_lang_translations": {
                                "ru": "Обычно я ем <w>солёные</w> блюда, так как люблю выразительные вкусы."
                            }
                        }
                    ],
                    "antonyms": [
                        "słodko"
                    ],
                    "common_phrases": [
                        "na słono",
                        "zbyt słono",
                        "smakować słono"
                    ],
                    "target_lang_definitions": {
                        "ru": "Так, что чувствуется вкус соли, или с большим содержанием соли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "солёно",
                                "target_lang_sense_clarification": "о вкусе"
                            },
                            {
                                "target_lang_word": "солоно",
                                "target_lang_sense_clarification": "вариант произношения/написания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b7b3ca6-8e7b-3743-84c0-1adce0d9bd56",
                    "sense_definition": "Bardzo drogo; ponosząc wysoki koszt finansowy lub moralny.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cost",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Za ten błąd będziesz musiał <w>słono</w> zapłacić.",
                            "target_lang_translations": {
                                "ru": "За эту ошибку тебе придется <w>дорого</w> заплатить."
                            }
                        },
                        {
                            "text": "Naprawa starego samochodu kosztowała nas bardzo <w>słono</w>.",
                            "target_lang_translations": {
                                "ru": "Ремонт старой машины обошелся нам очень <w>дорого</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "drogo"
                    ],
                    "antonyms": [
                        "niewiele"
                    ],
                    "common_phrases": [
                        "słono zapłacić",
                        "słono kosztować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень дорого; неся большие финансовые или моральные издержки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дорого",
                                "target_lang_sense_clarification": "о высокой цене или расплате"
                            }
                        ]
                    }
                }
            ]
        }
    ]

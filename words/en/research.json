{
    "word_family": [
        "researcher",
        "researchable",
        "unresearched"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4146b1a2-af21-3cb0-91a8-72846d2d1001",
                    "sense_definition": "The systematic investigation into and study of materials and sources in order to establish facts and reach new conclusions.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scientist spent years conducting <w>research</w> to find a cure for the disease.",
                            "target_lang_translations": {
                                "ru": "Учёный потратил годы на <w>исследования</w>, чтобы найти лекарство от болезни."
                            }
                        },
                        {
                            "text": "Before launching the product, the company did extensive market <w>research</w>.",
                            "target_lang_translations": {
                                "ru": "Перед запуском продукта компания провела обширное маркетинговое <w>исследование</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "investigation",
                        "study",
                        "inquiry",
                        "examination",
                        "exploration"
                    ],
                    "common_phrases": [
                        "market research",
                        "scientific research",
                        "conduct research",
                        "do research",
                        "latest research"
                    ],
                    "target_lang_definitions": {
                        "ru": "Систематическое изучение материалов и источников с целью установления фактов и получения новых выводов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исследование",
                                "target_lang_sense_clarification": "процесс научного познания"
                            },
                            {
                                "target_lang_word": "изучение",
                                "target_lang_sense_clarification": "глубокий анализ или рассмотрение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "131c9696-dbd1-300e-8609-f10e9d15b66f",
                    "sense_definition": "A particular instance or piece of research; often used in the plural to refer to specific studies.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His historical <w>researches</w> have shed new light on the ancient civilization.",
                            "target_lang_translations": {
                                "ru": "Его исторические <w>исследования</w> пролили новый свет на древнюю цивилизацию."
                            }
                        },
                        {
                            "text": "She published the results of her <w>researches</w> in a prestigious academic journal.",
                            "target_lang_translations": {
                                "ru": "Она опубликовала результаты своих <w>исследований</w> в престижном академическом журнале."
                            }
                        }
                    ],
                    "synonyms": [
                        "study",
                        "investigation"
                    ],
                    "common_phrases": [
                        "publish researches",
                        "historical researches"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный научный труд или отдельный акт исследования; часто используется во множественном числе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исследование",
                                "target_lang_sense_clarification": "конкретная научная работа"
                            },
                            {
                                "target_lang_word": "научный труд",
                                "target_lang_sense_clarification": "результат исследовательской деятельности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b98ff11c-fe92-3888-9d9e-aeec31cb084a",
                    "sense_definition": "To study a subject in detail, especially in order to discover new information or reach a new understanding.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>research</w> the company thoroughly before my job interview.",
                            "target_lang_translations": {
                                "ru": "Мне нужно тщательно <w>изучить</w> компанию перед собеседованием."
                            }
                        },
                        {
                            "text": "The author <w>researched</w> the historical period for five years to ensure accuracy.",
                            "target_lang_translations": {
                                "ru": "Автор <w>исследовал</w> этот исторический период в течение пяти лет, чтобы гарантировать точность."
                            }
                        }
                    ],
                    "synonyms": [
                        "investigation",
                        "study",
                        "examination",
                        "exploration"
                    ],
                    "common_phrases": [
                        "research thoroughly",
                        "research a topic",
                        "well researched"
                    ],
                    "target_lang_definitions": {
                        "ru": "Детально изучать предмет, особенно для открытия новой информации или достижения нового понимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исследовать",
                                "target_lang_sense_clarification": "подвергать научному изучению"
                            },
                            {
                                "target_lang_word": "изучать",
                                "target_lang_sense_clarification": "внимательно рассматривать, учить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "722dcba5-2200-3884-b20c-a3e8f7177ff6",
                    "sense_definition": "To receive a certain response or evaluation during market research testing.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_marketing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new advertising campaign <w>researched</w> well among the target demographic.",
                            "target_lang_translations": {
                                "ru": "Новая рекламная кампания <w>показала хорошие результаты</w> среди целевой аудитории."
                            }
                        },
                        {
                            "text": "Unfortunately, the proposed slogan <w>researched</w> poorly and was rejected.",
                            "target_lang_translations": {
                                "ru": "К сожалению, предложенный слоган <w>получил плохие отзывы</w> при тестировании и был отвергнут."
                            }
                        }
                    ],
                    "common_phrases": [
                        "research well",
                        "research poorly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получить определенный отклик или оценку в ходе тестирования (маркетинговый термин)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "показать результаты",
                                "target_lang_sense_clarification": "продемонстрировать эффективность при тестировании"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
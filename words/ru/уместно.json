                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ваше замечание справедливо и вполне <w>уместно</w>.",
                            "target_lang_translations": {
                                "en": "Your remark is fair and quite <w>appropriate</w>."

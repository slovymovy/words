                            {
                                "target_lang_word": "встревоженный",
                                "target_lang_sense_clarification": "охваченный тревогой"
                            },
                            {
                                "target_lang_word": "лишенный покоя",
                                "target_lang_sense_clarification": "не находящий себе места"
                            }

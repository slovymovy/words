                    ],
                    "synonyms": [
                        "single-handedly",
                        "unassisted",
                        "on one's own"
                    ],
                    "common_phrases": [
                        "do it alone",
                        "go it alone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без посторонней помощи или поддержки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сам",
                                "target_lang_sense_clarification": "своими силами"
                            },
                            {
                                "target_lang_word": "самостоятельно",
                                "target_lang_sense_clarification": "независимо"
                            },
                            {
                                "target_lang_word": "в одиночку",
                                "target_lang_sense_clarification": "без помощников"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa9b1345-249f-32d5-a116-e547f358ac44",
                    "sense_definition": "Exclusively; only; not involving anyone or anything else.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "focus_adverb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The president <w>alone</w> has the authority to make that decision.",
                            "target_lang_translations": {
                                "ru": "<w>Только</w> президент обладает полномочиями принимать это решение."
                            }
                        },
                        {
                            "text": "Time <w>alone</w> will tell if we made the right choice.",
                            "target_lang_translations": {
                                "ru": "<w>Только</w> время покажет, сделали ли мы правильный выбор."
                            }
                        }
                    ],
                    "synonyms": [
                        "solely",
                        "exclusively",
                        "only"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "placed immediately after the noun it modifies"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Исключительно; только; не вовлекая никого и ничего другого."

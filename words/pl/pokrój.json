            "senses": [
                {
                    "sense_id": "9e877396-3ff5-30f9-9c7b-01952f959e59",
                    "sense_definition": "Zespół cech charakterystycznych dla kogoś lub czegoś; rodzaj, typ lub kategoria (często używane w odniesieniu do ludzi).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nieczęsto spotyka się polityków tego <w>pokroju</w>, którzy naprawdę dotrzymują swoich obietnic.",
                            "target_lang_translations": {
                                "ru": "Нечасто встретишь политиков такого <w>калибра</w>, которые действительно выполняют свои обещания."
                            }
                        },
                        {
                            "text": "Osoby tego <w>pokroju</w> zazwyczaj unikają odpowiedzialności za swoje czyny.",
                            "target_lang_translations": {
                                "ru": "Люди такого <w>склада</w> обычно избегают ответственности за свои поступки."
                            }

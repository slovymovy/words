{
    "word_family": [
        "flooring",
        "floorboard",
        "floorless"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "46cea428-9ffc-3e31-afce-1549aed43c6d",
                    "sense_definition": "The interior bottom surface of a room on which people walk or stand.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "surface_bottom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The baby sat on the kitchen <w>floor</w> playing with a spoon.",
                            "target_lang_translations": {
                                "ru": "Ребенок сидел на кухонном <w>полу</w>, играя с ложкой."
                            }
                        },
                        {
                            "text": "Please pick up your clothes from the <w>floor</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, подними свою одежду с <w>пола</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "ceiling",
                        "roof"
                    ],
                    "common_phrases": [
                        "wooden floor",
                        "wet floor",
                        "sweep the floor",
                        "mop the floor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нижняя поверхность помещения, по которой ходят люди."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пол",
                                "target_lang_sense_clarification": "покрытие в помещении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78a1f454-58c0-3453-b930-212df85d410b",
                    "sense_definition": "A particular level of a building.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "building_level",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My apartment is on the third <w>floor</w> of this building.",
                            "target_lang_translations": {
                                "ru": "Моя квартира находится на третьем <w>этаже</w> этого здания."
                            }
                        },
                        {
                            "text": "Take the elevator to the ground <w>floor</w>.",
                            "target_lang_translations": {
                                "ru": "Спуститесь на лифте на первый <w>этаж</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "flight",
                        "possession"
                    ],
                    "common_phrases": [
                        "ground floor",
                        "first floor",
                        "top floor",
                        "upper floor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенный уровень здания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "этаж",
                                "target_lang_sense_clarification": "уровень здания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a0e5305-5d56-3a48-a4c1-6c93362862c8",
                    "sense_definition": "The bottom surface of a natural space, such as an ocean, forest, or cave.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "surface_bottom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many strange creatures live on the ocean <w>floor</w>.",
                            "target_lang_translations": {
                                "ru": "Многие странные существа живут на океанском <w>дне</w>."
                            }
                        },
                        {
                            "text": "Thick leaves covered the forest <w>floor</w> in autumn.",
                            "target_lang_translations": {
                                "ru": "Толстый слой листьев покрывал лесную <w>землю</w> осенью."
                            }
                        }
                    ],
                    "synonyms": [
                        "ground"
                    ],
                    "common_phrases": [
                        "ocean floor",
                        "sea floor",
                        "forest floor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нижняя поверхность природного пространства, например океана или леса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дно",
                                "target_lang_sense_clarification": "водоема"
                            },
                            {
                                "target_lang_word": "земля",
                                "target_lang_sense_clarification": "поверхность суши"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53f79c3f-d507-3b1a-9e4a-2a51da6afe7d",
                    "sense_definition": "An area in a building, such as a club or hall, designated for dancing.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "surface_bottom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As soon as the music started, everyone ran to the <w>floor</w>.",
                            "target_lang_translations": {
                                "ru": "Как только заиграла музыка, все выбежали на <w>танцпол</w>."
                            }
                        },
                        {
                            "text": "The couple moved gracefully across the dance <w>floor</w>.",
                            "target_lang_translations": {
                                "ru": "Пара грациозно двигалась по <w>танцполу</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dance floor",
                        "on the floor",
                        "hit the floor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зона в здании, например в клубе, предназначенная для танцев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "танцпол",
                                "target_lang_sense_clarification": "место для танцев"
                            },
                            {
                                "target_lang_word": "площадка",
                                "target_lang_sense_clarification": "танцевальная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8b157c7-3584-346b-bfe6-3252fafceaed",
                    "sense_definition": "The right to speak during a formal meeting, debate, or public event.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_right",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chairman gave the <w>floor</w> to the next speaker.",
                            "target_lang_translations": {
                                "ru": "Председатель предоставил <w>слово</w> следующему оратору."
                            }
                        },
                        {
                            "text": "Please wait until you have the <w>floor</w> before asking your question.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, подождите, пока вам дадут <w>слово</w>, прежде чем задавать вопрос."
                            }
                        }
                    ],
                    "common_phrases": [
                        "have the floor",
                        "take the floor",
                        "yield the floor",
                        "give the floor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Право выступать во время официального собрания или дебатов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слово",
                                "target_lang_sense_clarification": "право говорить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "343ea1d2-daad-3505-a730-5442a19c6491",
                    "sense_definition": "A lower limit imposed on a price, wage, or rate.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_limit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government established a price <w>floor</w> for agricultural products.",
                            "target_lang_translations": {
                                "ru": "Правительство установило <w>нижний предел</w> цен на сельскохозяйственную продукцию."
                            }
                        },
                        {
                            "text": "There is a <w>floor</w> on how low the interest rate can go.",
                            "target_lang_translations": {
                                "ru": "Существует <w>нижний предел</w> того, насколько может опуститься процентная ставка."
                            }
                        }
                    ],
                    "antonyms": [
                        "ceiling"
                    ],
                    "common_phrases": [
                        "price floor",
                        "wage floor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нижний предел цены, зарплаты или ставки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нижний предел",
                                "target_lang_sense_clarification": "минимум"
                            },
                            {
                                "target_lang_word": "минимальный уровень",
                                "target_lang_sense_clarification": "ограничение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37531bc7-d664-399c-acf5-5c384654abb6",
                    "sense_definition": "The largest integer that is less than or equal to a given number.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "abstract_limit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In this equation, the <w>floor</w> of 4.8 is 4.",
                            "target_lang_translations": {
                                "ru": "В этом уравнении <w>пол</w> числа 4,8 равен 4."
                            }
                        },
                        {
                            "text": "You need to calculate the <w>floor</w> value for the result.",
                            "target_lang_translations": {
                                "ru": "Вам нужно вычислить значение <w>пола</w> для результата."
                            }
                        }
                    ],
                    "antonyms": [
                        "ceiling"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "mathematics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наибольшее целое число, которое меньше или равно данному числу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пол",
                                "target_lang_sense_clarification": "математическая функция"
                            },
                            {
                                "target_lang_word": "антье",
                                "target_lang_sense_clarification": "целая часть числа"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5d44a28b-2d07-38f5-8083-ce7cfa9420a5",
                    "sense_definition": "To surprise, confuse, or shock someone very much.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_impact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was completely <w>floored</w> by the news of their engagement.",
                            "target_lang_translations": {
                                "ru": "Я был совершенно <w>сражен</w> новостью об их помолвке."
                            }
                        },
                        {
                            "text": "Her incredible singing voice <w>floored</w> the entire audience.",
                            "target_lang_translations": {
                                "ru": "Ее невероятный голос <w>сразил</w> всю аудиторию."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильно удивить или шокировать кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сразить",
                                "target_lang_sense_clarification": "удивить"
                            },
                            {
                                "target_lang_word": "ошарашить",
                                "target_lang_sense_clarification": "шокировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37e58229-1b98-3ef4-962b-28cc3750a4cd",
                    "sense_definition": "To hit someone so hard that they fall to the ground.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boxer <w>floored</w> his opponent with a powerful punch.",
                            "target_lang_translations": {
                                "ru": "Боксер <w>сбил с ног</w> своего противника мощным ударом."
                            }
                        },
                        {
                            "text": "He was <w>floored</w> during the fight but managed to stand up again.",
                            "target_lang_translations": {
                                "ru": "Его <w>повалили</w> во время драки, но он смог снова встать."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ударить кого-то так сильно, что он падает на землю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбить с ног",
                                "target_lang_sense_clarification": "ударом"
                            },
                            {
                                "target_lang_word": "повалить",
                                "target_lang_sense_clarification": "на пол"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5f1b60b-f7e6-3df6-9131-85c498a7a654",
                    "sense_definition": "To press the accelerator pedal of a vehicle fully down to the floor.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "verb_drive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>floored</w> the car to overtake the slow truck.",
                            "target_lang_translations": {
                                "ru": "Он <w>вдавил педаль в пол</w>, чтобы обогнать медленный грузовик."
                            }
                        },
                        {
                            "text": "We are late, so just <w>floor</w> it!",
                            "target_lang_translations": {
                                "ru": "Мы опаздываем, так что <w>жми на газ</w>!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "floor it"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью выжать педаль газа автомобиля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вдавить педаль в пол",
                                "target_lang_sense_clarification": "газовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d53148a1-2098-3fdf-aca9-c2855ac1acb7",
                    "sense_definition": "To cover or furnish a surface with a floor.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They decided to <w>floor</w> the cabin with pine wood.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>настелить пол</w> в хижине сосновой доской."
                            }
                        },
                        {
                            "text": "The workers <w>floored</w> the new room yesterday.",
                            "target_lang_translations": {
                                "ru": "Рабочие <w>настелили пол</w> в новой комнате вчера."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрыть поверхность полом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "настилать пол",
                                "target_lang_sense_clarification": "строительство"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
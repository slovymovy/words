{
    "word_family": [
        "distant",
        "distantly"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d3f087fb-a714-3927-8066-9be4ccaec6cc",
                    "sense_definition": "An amount of space between two points or places.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "space_measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>distance</w> from my house to the station is about two miles."
                        },
                        {
                            "text": "Please keep a safe <w>distance</w> from the edge of the cliff."
                        }
                    ],
                    "synonyms": [
                        "length"
                    ],
                    "antonyms": [
                        "closeness",
                        "proximity"
                    ],
                    "common_phrases": [
                        "walking distance",
                        "long distance",
                        "short distance",
                        "safe distance"
                    ]
                },
                {
                    "sense_id": "0ffb4420-413b-30d1-abfe-f8044182b15f",
                    "sense_definition": "A place or point that is far away, often visible but remote.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "space_measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We could see the lights of the city twinkling in the <w>distance</w>."
                        },
                        {
                            "text": "The mountains looked beautiful from a <w>distance</w>."
                        }
                    ],
                    "common_phrases": [
                        "in the distance",
                        "from a distance"
                    ]
                },
                {
                    "sense_id": "95f0efbe-6861-3566-82ff-ee30a354d5b0",
                    "sense_definition": "An interval or amount of time between two events.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "time_measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "At a <w>distance</w> of twenty years, it is easier to forgive what happened."
                        },
                        {
                            "text": "The <w>distance</w> between the two meetings was too short to prepare a report."
                        }
                    ],
                    "common_phrases": [
                        "distance of time"
                    ]
                },
                {
                    "sense_id": "f01f275b-4e8d-3c8b-8a49-04926fafa5d8",
                    "sense_definition": "A lack of friendliness, intimacy, or involvement with other people; emotional separation.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "social_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After the argument, there was a noticeable <w>distance</w> between the two friends."
                        },
                        {
                            "text": "She treated the strangers with a polite but cold <w>distance</w>."
                        }
                    ],
                    "synonyms": [
                        "unfriendliness",
                        "standoffishness",
                        "alienation",
                        "unsociableness"
                    ],
                    "antonyms": [
                        "closeness",
                        "intimacy",
                        "warmth"
                    ],
                    "common_phrases": [
                        "keep one's distance"
                    ]
                },
                {
                    "sense_id": "0352cc7d-d89d-3270-a35b-40fb8863aede",
                    "sense_definition": "The completion of a full course or a difficult task, often used in the phrase 'go the distance'.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "idiomatic_progress",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He started the project with enthusiasm but did not have the stamina to go the <w>distance</w>."
                        },
                        {
                            "text": "The boxer managed to go the <w>distance</w> and finish all twelve rounds."
                        }
                    ],
                    "common_phrases": [
                        "go the distance"
                    ]
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9e513717-3686-3325-aa0e-851b95f42fc7",
                    "sense_definition": "To become less involved or connected with someone or something, often to avoid association.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "social_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The politician tried to <w>distance</w> himself from the controversial statement."
                        },
                        {
                            "text": "It is healthy to <w>distance</w> yourself from people who constantly bring you down."
                        }
                    ],
                    "common_phrases": [
                        "distance oneself"
                    ]
                },
                {
                    "sense_id": "6fcf6bf2-4243-380a-9ea5-5ca715db485d",
                    "sense_definition": "To leave others behind in a race or competition; to run much faster than opponents.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "competition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The champion runner easily <w>distanced</w> the other competitors in the final lap."
                        },
                        {
                            "text": "Our team has <w>distanced</w> all rivals in terms of innovation and sales."
                        }
                    ],
                    "synonyms": [
                        "outdistance",
                        "outrun"
                    ]
                }
            ]
        }
    ]
}
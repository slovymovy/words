            "senses": [
                {
                    "sense_id": "8ecbf37c-633c-32cb-a1a1-b29f581a4b66",
                    "sense_definition": "Отдать обратно то, что было взято, одолжено или получено ранее.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "return_restore",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я обещаю <w>вернуть</w> тебе деньги на следующей неделе.",
                            "target_lang_translations": {
                                "en": "I promise to <w>return</w> the money to you next week."
                            }
                        },
                        {
                            "text": "Не забудь <w>вернуть</w> книгу в библиотеку вовремя.",
                            "target_lang_translations": {
                                "en": "Don't forget to <w>return</w> the book to the library on time."
                            }
                        }
                    ],
                    "synonyms": [
                        "возвратить",
                        "отдать"
                    ],
                    "antonyms": [
                        "взять",
                        "забрать"
                    ],
                    "common_phrases": [
                        "вернуть долг",
                        "вернуть деньги",
                        "вернуть книгу",
                        "вернуть кредит"
                    ],
                    "target_lang_definitions": {
                        "en": "To give back something that was taken, borrowed, or received earlier."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "return",
                                "target_lang_sense_clarification": "general / formal"
                            },
                            {
                                "target_lang_word": "give back",
                                "target_lang_sense_clarification": "conversational"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3876af8c-3c8e-39c5-8212-89ed129c3c04",
                    "sense_definition": "Вновь обрести, получить обратно то, что было утрачено или потеряно (здоровье, чувства, состояние).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "return_restore",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ему удалось <w>вернуть</w> доверие своих старых друзей.",
                            "target_lang_translations": {
                                "en": "He managed to <w>regain</w> the trust of his old friends."
                            }
                        },
                        {
                            "text": "Мы хотим <w>вернуть</w> мир и спокойствие в наш дом.",
                            "target_lang_translations": {
                                "en": "We want to <w>restore</w> peace and quiet to our home."
                            }
                        }
                    ],
                    "synonyms": [
                        "возвратить"
                    ],
                    "antonyms": [
                        "потерять",
                        "утратить"
                    ],
                    "common_phrases": [
                        "вернуть здоровье",
                        "вернуть доверие",
                        "вернуть надежду",
                        "вернуть былое"
                    ],
                    "target_lang_definitions": {
                        "en": "To get something back that was lost, such as health, feelings, or status."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get back",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "regain",
                                "target_lang_sense_clarification": "formal, often used with abstract concepts like trust"
                            },
                            {
                                "target_lang_word": "restore",
                                "target_lang_sense_clarification": "to bring back a state or condition"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46d1f864-89e1-3a91-ac18-a39bd05784b8",
                    "sense_definition": "Заставить или помочь кому-либо прийти, приехать обратно.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "return_restore",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врачи сделали всё возможное, чтобы <w>вернуть</w> пациента к жизни.",
                            "target_lang_translations": {
                                "en": "The doctors did everything possible to <w>bring</w> the patient <w>back</w> to life."
                            }
                        },
                        {
                            "text": "Она пыталась <w>вернуть</w> мужа в семью, но он отказался.",
                            "target_lang_translations": {
                                "en": "She tried to <w>bring</w> her husband <w>back</w> to the family, but he refused."
                            }
                        }
                    ],
                    "synonyms": [

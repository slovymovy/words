    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "36a149c5-a5d9-35b7-8673-e8435dd83375",
                    "sense_definition": "Общий для всего народа, охватывающий всё население страны.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "nationwide_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "День Победы — это великий <w>всенародный</w> праздник, который отмечают в каждой семье.",
                            "target_lang_translations": {
                                "en": "Victory Day is a great <w>national</w> holiday celebrated in every family."
                            }
                        },
                        {
                            "text": "Решение президента получило широкую <w>всенародную</w> поддержку во всех регионах.",
                            "target_lang_translations": {
                                "en": "The president's decision received broad <w>nationwide</w> support in all regions."
                            }
                        }
                    ],
                    "synonyms": [
                        "общенациональный",
                        "общенародный"
                    ],
                    "common_phrases": [
                        "всенародный праздник",
                        "всенародная любовь",
                        "всенародное голосование",
                        "всенародная поддержка"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to or involving the entire population of a country."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nationwide",
                                "target_lang_sense_clarification": "extending throughout the whole country"
                            },
                            {
                                "target_lang_word": "national",
                                "target_lang_sense_clarification": "shared by the whole nation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb17d361-902d-378a-b6f7-b03731e20b21",
                    "sense_definition": "Принадлежащий всему народу, являющийся собственностью всех граждан.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ownership",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Природные ресурсы страны должны быть <w>всенародным</w> достоянием.",
                            "target_lang_translations": {
                                "en": "The country's natural resources must be <w>national</w> wealth."
                            }
                        },
                        {
                            "text": "После революции земля была объявлена <w>всенародной</w> собственностью.",
                            "target_lang_translations": {
                                "en": "After the revolution, the land was declared <w>public</w> property."
                            }
                        }
                    ],
                    "synonyms": [
                        "общенародный"
                    ],
                    "common_phrases": [
                        "всенародное достояние",
                        "всенародная собственность",
                        "всенародное богатство"
                    ],
                    "target_lang_definitions": {
                        "en": "Belonging to the entire people collectively."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "national",
                                "target_lang_sense_clarification": "owned by the nation"
                            },
                            {
                                "target_lang_word": "public",
                                "target_lang_sense_clarification": "belonging to the community"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b77d846e-9602-3ff9-9d8c-c78d17dfc526",
                    "sense_definition": "Совершаемый открыто, в присутствии большого количества людей; публичный.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "public_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На площади состоялось <w>всенародное</w> оглашение королевского указа.",
                            "target_lang_translations": {
                                "en": "A <w>public</w> announcement of the royal decree took place in the square."
                            }
                        },
                        {
                            "text": "Он принёс <w>всенародное</w> покаяние за свои прошлые ошибки.",
                            "target_lang_translations": {
                                "en": "He made a <w>public</w> repentance for his past mistakes."
                            }
                        }
                    ],
                    "synonyms": [
                        "публичный"
                    ],
                    "common_phrases": [
                        "всенародное покаяние",
                        "всенародное оглашение"
                    ],
                    "target_lang_definitions": {
                        "en": "Performed openly in the presence of many people."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "public",
                                "target_lang_sense_clarification": "done in open view"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
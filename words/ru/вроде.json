{
    "word_family": [
        "род",
        "народ",
        "природа",
        "порода",
        "рождение",
        "родитель"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "59260b09-a2b3-39f0-a017-8b0fa00d2336",
                    "sense_definition": "Выражает неуверенность, сомнение или предположение о достоверности высказывания; кажется, как будто.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "uncertainty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы <вроде> договорились встретиться завтра утром.",
                            "target_lang_translations": {
                                "en": "We <sort of> agreed to meet tomorrow morning."
                            }
                        },
                        {
                            "text": "Этот магазин <вроде> ещё открыт, давай проверим.",
                            "target_lang_translations": {
                                "en": "This shop <seems> to be still open, let's check."
                            }
                        }
                    ],
                    "synonyms": [
                        "кажется",
                        "как бу́дто",
                        "бу́дто бы"
                    ],
                    "common_phrases": [
                        "вроде бы",
                        "вроде того",
                        "вроде нет"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to express uncertainty, doubt, or an assumption; sort of, it seems."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "kind of",
                                "target_lang_sense_clarification": "adverb expressing vagueness or uncertainty"
                            },
                            {
                                "target_lang_word": "sort of",
                                "target_lang_sense_clarification": "adverb expressing vagueness or uncertainty"
                            },
                            {
                                "target_lang_word": "it seems",
                                "target_lang_sense_clarification": "indicating that something appears to be true"
                            },
                            {
                                "target_lang_word": "apparently",
                                "target_lang_sense_clarification": "based on what is known or seen"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "f4e800cf-3cdd-323d-9110-d1d75ff11163",
                    "sense_definition": "Указывает на сходство с кем-либо или чем-либо; наподобие, подобно.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "similarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне нужно купить что-то <вроде> тёплого зимнего шарфа.",
                            "target_lang_translations": {
                                "en": "I need to buy something <like> a warm winter scarf."
                            }
                        },
                        {
                            "text": "На поляне стояло сооружение <вроде> старого шалаша.",
                            "target_lang_translations": {
                                "en": "On the clearing stood a structure <like> an old hut."
                            }
                        }
                    ],
                    "synonyms": [
                        "наподо́бие",
                        "подо́бно"
                    ],
                    "common_phrases": [
                        "что-то вроде",
                        "нечто вроде"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates similarity to someone or something; resembling."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "like",
                                "target_lang_sense_clarification": "having the characteristics of"
                            },
                            {
                                "target_lang_word": "similar to",
                                "target_lang_sense_clarification": "resembling"
                            },
                            {
                                "target_lang_word": "something like",
                                "target_lang_sense_clarification": "used to describe something vaguely"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
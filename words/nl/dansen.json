    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3fa88990-bde4-3a11-84d8-d40547c63b79",
                    "sense_definition": "Sierlijk en ritmisch bewegen, gewoonlijk op muziek",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mag ik met je <w>dansen</w>?",
                            "target_lang_translations": {
                                "ru": "Можно с тобой <w>потанцевать</w>?"
                            }
                        },
                        {
                            "text": "Er werd de hele avond <w>gedanst</w> op muziek uit de jaren dertig.",
                            "target_lang_translations": {
                                "ru": "Весь вечер <w>танцевали</w> под музыку тридцатых годов."
                            }
                        },
                        {
                            "text": "Ze <w>dansten</w> samen de tango.",
                            "target_lang_translations": {
                                "ru": "Они вместе <w>танцевали</w> танго."
                            }
                        }
                    ],
                    "synonyms": [
                        "swingen"
                    ],
                    "antonyms": [
                        "stilstaan"
                    ],
                    "common_phrases": [
                        "naar iemands pijpen dansen",
                        "de sterren van de hemel dansen",
                        "de tango dansen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ритмично двигаться, обычно под музыку"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "танцевать",
                                "target_lang_sense_clarification": "двигаться под музыку"
                            },
                            {
                                "target_lang_word": "плясать",
                                "target_lang_sense_clarification": "энергично танцевать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aca044d3-8410-32c0-9798-1d755be6484a",
                    "sense_definition": "Niet stilstaan; onrustig en willekeurig bewegen",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "movement_metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de enorme vermoeidheid <w>dansten</w> de letters voor mijn ogen.",
                            "target_lang_translations": {
                                "ru": "Из-за сильной усталости буквы <w>плясали</w> у меня перед глазами."
                            }
                        },
                        {
                            "text": "De kleine boot <w>danste</w> op de hoge golven.",
                            "target_lang_translations": {
                                "ru": "Маленькая лодка <w>танцевала</w> на высоких волнах."
                            }
                        }
                    ],
                    "synonyms": [
                        "bewegen"
                    ],
                    "antonyms": [
                        "stilstaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не стоять на месте; двигаться беспокойно и хаотично"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "танцевать",
                                "target_lang_sense_clarification": "перен. колебаться, дрожать"
                            },
                            {
                                "target_lang_word": "плясать",
                                "target_lang_sense_clarification": "перен. хаотично двигаться"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "94af0f5a-5ef2-3aba-8dc0-4526bb788b6b",
                    "sense_definition": "Meervoud van <w>dans</w>",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben gisteren verschillende traditionele <w>dansen</w> geleerd.",
                            "target_lang_translations": {
                                "ru": "Вчера мы выучили различные традиционные <w>танцы</w>."
                            }
                        },
                        {
                            "text": "De <w>dansen</w> op het feest waren erg energiek.",
                            "target_lang_translations": {
                                "ru": "<w>Танцы</w> на вечеринке были очень энергичными."
                            }
                        }
                    ],

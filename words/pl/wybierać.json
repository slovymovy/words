    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "460b33ac-8269-3a8b-80b6-d6d59efdd0b8",
                    "sense_definition": "decydować się na jedną rzecz, osobę lub opcję spośród wielu dostępnych",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "choice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie wiem, którą sukienkę <wybierać> na dzisiejszą kolację.",
                            "target_lang_translations": {
                                "ru": "Я не знаю, какое платье <w>выбрать</w> на сегодняшний ужин."
                            }
                        },
                        {
                            "text": "Klienci chętniej <wybierają> produkty znanych i sprawdzonych marek.",
                            "target_lang_translations": {
                                "ru": "Клиенты охотнее <w>выбирают</w> продукты известных и проверенных марок."
                            }
                        }
                    ],
                    "synonyms": [
                        "selekcjonować",
                        "dobierać"
                    ],
                    "antonyms": [
                        "odrzucać"
                    ],
                    "common_phrases": [
                        "wybierać prezent",
                        "wybierać zawód",
                        "wybierać numer"
                    ],
                    "target_lang_definitions": {
                        "ru": "останавливать свой выбор на ком-либо или чём-либо из ряда возможных вариантов"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбирать",
                                "target_lang_sense_clarification": "из множества вариантов"
                            },
                            {
                                "target_lang_word": "подбирать",
                                "target_lang_sense_clarification": "по соответствию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "890f100d-43ab-3dd9-93a3-4bf8ff6233b7",
                    "sense_definition": "planować pójście lub wyjazd w określone miejsce; szykować się do wyjścia (używane z zaimkiem \"się\")",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W przyszłym tygodniu <wybieramy> się całą rodziną nad morze.",
                            "target_lang_translations": {
                                "ru": "На следующей неделе мы всей семьёй <w>собираемся</w> на море."
                            }
                        },
                        {
                            "text": "Czy ty też <wybierasz> się wieczorem do kina z przyjaciółmi?",
                            "target_lang_translations": {
                                "ru": "Ты тоже <w>собираешься</w> вечером в кино с друзьями?"
                            }
                        }
                    ],
                    "synonyms": [
                        "udawać się",
                        "wyruszać",
                        "iść"
                    ],
                    "antonyms": [
                        "wracać",
                        "zostawać"
                    ],
                    "common_phrases": [
                        "wybierać się na spacer",
                        "wybierać się w podróż",
                        "wybierać się do domu"
                    ],
                    "target_lang_definitions": {
                        "ru": "планировать пойти или поехать куда-либо; готовиться к выходу"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собираться",
                                "target_lang_sense_clarification": "планировать пойти/поехать"
                            },
                            {
                                "target_lang_word": "намереваться",
                                "target_lang_sense_clarification": "иметь намерение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe9f7181-11b3-3760-99c2-6336eb7028f9",
                    "sense_definition": "powoływać kogoś na stanowisko lub funkcję poprzez głosowanie",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "politics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Co cztery lata obywatele <wybierają> nowy parlament w wyborach powszechnych.",
                            "target_lang_translations": {
                                "ru": "Каждые четыре года граждане <w>избирают</w> новый парламент на всеобщих выборах."
                            }
                        },
                        {
                            "text": "Wczoraj uczniowie musieli <wybierać> przewodniczącego swojej klasy.",
                            "target_lang_translations": {
                                "ru": "Вчера ученики должны были <w>выбрать</w> старосту своего класса."
                            }
                        }
                    ],
                    "synonyms": [
                        "powoływać"
                    ],
                    "antonyms": [
                        "odwoływać"
                    ],
                    "common_phrases": [
                        "wybierać prezydenta",
                        "wybierać zarząd",
                        "wybierać władze"
                    ],
                    "target_lang_definitions": {
                        "ru": "назначать кого-либо на должность или функцию путём голосования"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "избирать",
                                "target_lang_sense_clarification": "голосованием"
                            },
                            {
                                "target_lang_word": "выбирать",
                                "target_lang_sense_clarification": "на должность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a249f5e3-37ba-3af1-b0ae-400cd8ec99b5",
                    "sense_definition": "wyjmować, usuwać lub czerpać coś z wnętrza czegoś, często w celu opróżnienia",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "extraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wielkiej ulewie strażacy pomagali mieszkańcom <wybierać> wodę z piwnic.",
                            "target_lang_translations": {
                                "ru": "После сильного ливня пожарные помогали жителям <w>вычерпывать</w> воду из подвалов."
                            }
                        },
                        {
                            "text": "Robotnicy zaczęli <wybierać> ziemię pod fundamenty nowego domu.",
                            "target_lang_translations": {
                                "ru": "Рабочие начали <w>вынимать</w> грунт под фундамент нового дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyjmować",
                        "czerpać",
                        "opróżniać",
                        "wydobywać"
                    ],
                    "antonyms": [
                        "wsypywać",
                        "wlewać"
                    ],
                    "common_phrases": [
                        "wybierać wodę",
                        "wybierać ziemię",
                        "wybierać jajka"
                    ],
                    "target_lang_definitions": {
                        "ru": "извлекать, удалять или вычерпывать что-либо откуда-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вычерпывать",
                                "target_lang_sense_clarification": "жидкость"
                            },
                            {
                                "target_lang_word": "выбирать",
                                "target_lang_sense_clarification": "извлекать"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "074d2465-74fa-33e0-92ab-9c7e013b3330",
                    "sense_definition": "Nauka chrześcijańska o życiu i nauczaniu Jezusa Chrystusa; Dobra Nowina o zbawieniu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "religia_chrześcijańska",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Misjonarze wyjechali do odległych krajów, aby głosić <w>ewangelię</w>.",
                            "target_lang_translations": {
                                "ru": "Миссионеры отправились в далекие страны, чтобы проповедовать <w>евангелие</w>."
                            }
                        },
                        {
                            "text": "Wielu ludzi stara się żyć na co dzień zgodnie z zasadami <w>ewangelii</w>.",
                            "target_lang_translations": {
                                "ru": "Многие люди стараются жить каждый день в соответствии с принципами <w>евангелия</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Dobra Nowina",
                        "Słowo Boże"
                    ],
                    "antonyms": [
                        "antyewangelia"
                    ],
                    "common_phrases": [
                        "głosić ewangelię",
                        "wierzyć w ewangelię",
                        "światło ewangelii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Христианское учение о жизни и проповеди Иисуса Христа; Благая Весть о спасении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "евангелие",
                                "target_lang_sense_clarification": "учение / Благая Весть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c81c0fd-d13e-3ca3-84f2-89ed4c628207",
                    "sense_definition": "Fragment jednej z czterech Ewangelii odczytywany uroczyście podczas mszy świętej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "religia_chrześcijańska",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy wierni wstali, aby wysłuchać słów <w>ewangelii</w>.",
                            "target_lang_translations": {
                                "ru": "Все верующие встали, чтобы выслушать слова <w>евангелия</w>."
                            }
                        },
                        {
                            "text": "Po przeczytaniu <w>ewangelii</w> ksiądz rozpoczął wygłaszanie kazania.",
                            "target_lang_translations": {
                                "ru": "После чтения <w>евангелия</w> священник начал проповедь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czytanie ewangelii",
                        "słowa ewangelii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отрывок из одного из четырех Евангелий, торжественно читаемый во время церковной службы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "евангелие",
                                "target_lang_sense_clarification": "чтение на литургии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1be84366-90b5-3936-b7bb-07c4ea50e21f",
                    "sense_definition": "Wczesnochrześcijański utwór literacki opisujący życie, śmierć i zmartwychwstanie Jezusa.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "religia_chrześcijańska",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Historycy badają apokryficzne <w>ewangelie</w>, które nie weszły do kanonu biblijnego.",
                            "target_lang_translations": {
                                "ru": "Историки изучают апокрифические <w>евангелия</w>, которые не вошли в библейский канон."
                            }
                        },
                        {
                            "text": "Styl literacki tej <w>ewangelii</w> różni się od pozostałych pism Nowego Testamentu.",
                            "target_lang_translations": {
                                "ru": "Литературный стиль этого <w>евангелия</w> отличается от остальных писаний Нового Завета."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ewangelie synoptyczne",
                        "ewangelie apokryficzne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Раннехристианское литературное произведение, описывающее жизнь, смерть и воскресение Иисуса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "евангелие",
                                "target_lang_sense_clarification": "литературное произведение / текст"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13f9d75b-c48e-3827-94d8-4459be28ec3b",
                    "sense_definition": "Prawda niepodważalna i ostateczna, autorytet w danej dziedzinie (użycie przenośne).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie traktuj słów swojego szefa jak objawionej <w>ewangelii</w>.",
                            "target_lang_translations": {
                                "ru": "Не воспринимай слова своего начальника как божественное <w>откровение</w>."
                            }
                        },
                        {
                            "text": "Dla młodych ekonomistów ta książka stała się rynkową <w>ewangelią</w>.",
                            "target_lang_translations": {
                                "ru": "Для молодых экономистов эта книга стала рыночным <w>евангелием</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przyjmować za ewangelię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неоспоримая и окончательная истина, авторитет в данной области (переносное значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "евангелие",
                                "target_lang_sense_clarification": "непреложная истина (перен.)"
                            },
                            {
                                "target_lang_word": "истина",
                                "target_lang_sense_clarification": "неоспоримый факт"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "09cf28a6-87cc-3afc-8edc-ef3228e3ae69",
                    "sense_definition": "Jedna z czterech ksiąg Nowego Testamentu (według świętego Mateusza, Marka, Łukasza lub Jana).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "religia_chrześcijańska",
                    "name_type": "document_or_program_name",
                    "examples": [
                        {
                            "text": "Na stoliku nocnym mojej babci zawsze leżała otwarta <w>Ewangelia</w>.",
                            "target_lang_translations": {
                                "ru": "На ночном столике моей бабушки всегда лежало открытое <w>Евангелие</w>."
                            }
                        },
                        {
                            "text": "Dzisiaj na lekcji religii omawialiśmy fragment <w>Ewangelii</w> według świętego Łukasza.",
                            "target_lang_translations": {
                                "ru": "Сегодня на уроке религии мы обсуждали отрывок из <w>Евангелия</w> от Луки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Ewangelia według świętego Marka",
                        "cztery Ewangelie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из четырех книг Нового Завета (от Матфея, Марка, Луки или Иоанна)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Евангелие",
                                "target_lang_sense_clarification": "Священное Писание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
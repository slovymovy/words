    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1b63a52d-62ac-340f-9803-adf2ebce9cf9",
                    "sense_definition": "The state of communicating with someone or the act of establishing communication.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "communication_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We have kept in <w>contact</w> with each other ever since we left school.",
                            "target_lang_translations": {
                                "ru": "Мы поддерживали <w>связь</w> друг с другом с тех пор, как окончили школу."
                            }
                        },
                        {
                            "text": "Please make <w>contact</w> with the reception desk if you need any assistance.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>свяжитесь</w> со стойкой регистрации, если вам нужна помощь."
                            }
                        }
                    ],
                    "synonyms": [
                        "communication",
                        "connection",
                        "correspondence"
                    ],
                    "antonyms": [
                        "isolation"
                    ],
                    "common_phrases": [
                        "in contact with",
                        "keep in contact",
                        "lose contact",
                        "make contact",
                        "point of contact"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние общения с кем-либо или действие по установлению связи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "контакт",
                                "target_lang_sense_clarification": "общение"
                            },
                            {
                                "target_lang_word": "связь",
                                "target_lang_sense_clarification": "соединение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c78821d-b03d-3e7b-9e6a-ba2811a3273a",
                    "sense_definition": "The act of touching physically or being close enough to touch.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "physical_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The disease can spread through direct physical <w>contact</w> with an infected person.",
                            "target_lang_translations": {
                                "ru": "Болезнь может передаваться через прямой физический <w>контакт</w> с зараженным человеком."
                            }
                        },
                        {
                            "text": "Wash your hands immediately if they come into <w>contact</w> with the chemicals.",
                            "target_lang_translations": {
                                "ru": "Немедленно вымойте руки, если они войдут в <w>контакт</w> с химикатами."
                            }
                        }
                    ],
                    "synonyms": [
                        "touch",
                        "connection"
                    ],
                    "antonyms": [
                        "separation"
                    ],
                    "common_phrases": [
                        "come into contact",
                        "physical contact",
                        "direct contact",
                        "eye contact"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие физического прикосновения или нахождения в непосредственной близости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "контакт",
                                "target_lang_sense_clarification": "физический"
                            },
                            {
                                "target_lang_word": "соприкосновение",
                                "target_lang_sense_clarification": "касание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24824082-a075-3d9d-b4d1-0ba31b4f8c2c",
                    "sense_definition": "A person, such as an acquaintance or colleague, who can provide information or assistance.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "communication_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has a useful <w>contact</w> in the marketing department who can help us.",
                            "target_lang_translations": {
                                "ru": "У нее есть полезный <w>контакт</w> в отделе маркетинга, который может нам помочь."
                            }
                        },
                        {
                            "text": "He built up a list of valuable business <w>contacts</w> during his career.",
                            "target_lang_translations": {
                                "ru": "За свою карьеру он создал список ценных деловых <w>контактов</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "connection",
                        "acquaintance",
                        "associate"
                    ],
                    "common_phrases": [
                        "business contact",
                        "useful contact",
                        "list of contacts"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, например, знакомый или коллега, который может предоставить информацию или помощь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "контакт",
                                "target_lang_sense_clarification": "человек"
                            },
                            {
                                "target_lang_word": "связь",
                                "target_lang_sense_clarification": "знакомство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac7457a1-991b-34bb-91e9-dd0a39b57354",
                    "sense_definition": "A thin plastic lens placed directly on the surface of the eye to correct vision.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I usually wear glasses, but I put in my <w>contacts</w> when I play sports.",
                            "target_lang_translations": {
                                "ru": "Обычно я ношу очки, но надеваю <w>линзы</w>, когда занимаюсь спортом."
                            }
                        },
                        {
                            "text": "Her eyes felt dry and irritated after wearing <w>contacts</w> all day.",
                            "target_lang_translations": {
                                "ru": "Ее глаза казались сухими и раздраженными после ношения <w>линз</w> весь день."
                            }
                        }
                    ],
                    "synonyms": [
                        "contact lens"
                    ],
                    "common_phrases": [
                        "wear contacts",
                        "put in contacts"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тонкая пластиковая линза, помещаемая непосредственно на поверхность глаза для коррекции зрения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "контактная линза",
                                "target_lang_sense_clarification": "оптика"
                            },
                            {
                                "target_lang_word": "линза",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b477840-4e37-3f56-a97f-9ece5efd5670",
                    "sense_definition": "A part of an electrical circuit that touches another part to allow current to flow.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The battery <w>contacts</w> were corroded, preventing the device from turning on.",
                            "target_lang_translations": {
                                "ru": "<w>Контакты</w> батареи окислились, не давая устройству включиться."
                            }
                        },
                        {
                            "text": "You should clean the metal <w>contact</w> to ensure a good electrical connection.",
                            "target_lang_translations": {
                                "ru": "Вам следует очистить металлический <w>контакт</w>, чтобы обеспечить хорошее электрическое соединение."
                            }
                        }
                    ],
                    "synonyms": [
                        "terminal",
                        "connection"
                    ],
                    "common_phrases": [
                        "electrical contact",
                        "poor contact"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть электрической цепи, которая соприкасается с другой частью для протекания тока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "контакт",
                                "target_lang_sense_clarification": "электрика"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5f7fc35c-e6e9-369f-8266-c09b1c82ce86",
                    "sense_definition": "To communicate with someone, for example by phone, email, or letter.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "communication_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I tried to <w>contact</w> him yesterday, but he did not answer his phone.",
                            "target_lang_translations": {
                                "ru": "Я пытался <w>связаться</w> с ним вчера, но он не ответил на звонок."
                            }
                        },
                        {
                            "text": "Please <w>contact</w> our support team if you have any further questions.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>свяжитесь</w> с нашей службой поддержки, если у вас возникнут дополнительные вопросы."
                            }
                        }
                    ],
                    "synonyms": [
                        "reach",
                        "call",
                        "email",
                        "message"
                    ],
                    "antonyms": [

        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e619de8c-cef1-3e77-9551-e7b51466172a",
                    "sense_definition": "Not the same as another person or thing; unlike in nature, quality, or form.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "dissimilarity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My opinion is <w>different</w> from yours on this matter.",
                            "target_lang_translations": {
                                "ru": "Моё мнение по этому вопросу <w>отличается</w> от твоего."
                            }
                        },
                        {
                            "text": "The result was no <w>different</w> than what we expected.",
                            "target_lang_translations": {
                                "ru": "Результат <w>не отличался</w> от того, что мы ожидали."
                            }
                        },
                        {
                            "text": "Is your brother feeling any better? — No <w>different</w>, he is still ill.",
                            "target_lang_translations": {
                                "ru": "Твоему брату лучше? — <w>Без изменений</w>, он всё ещё болен."
                            }
                        }
                    ],
                    "synonyms": [
                        "dissimilar",
                        "unlike",
                        "unalike",
                        "differing"
                    ],
                    "antonyms": [
                        "identical",
                        "similar",
                        "undifferent",
                        "homogeneous"
                    ],
                    "common_phrases": [
                        "different from",
                        "different to",
                        "different than",
                        "no different"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не такой, как кто-либо или что-либо другое; отличающийся по природе, качеству или форме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "другой",
                                "target_lang_sense_clarification": "не этот, не такой же"
                            },
                            {
                                "target_lang_word": "отличный",
                                "target_lang_sense_clarification": "имеющий отличия (обычно с предлогом 'от')"
                            },
                            {
                                "target_lang_word": "не такой",
                                "target_lang_sense_clarification": "отличающийся от сравнения"
                            }
                        ]
                    }
                },
                {

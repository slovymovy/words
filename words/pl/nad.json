{
    "word_family": [
        "ponad",
        "znad"
    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "eab4ef72-cdb6-35f7-9b9a-ec57d3bc830d",
                    "sense_definition": "Wskazuje na miejsce znajdujące się wyżej niż dany obiekt lub osoba.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_vertical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lampa wisi bezpośrednio <w>nad</w> dużym stołem w jadalni.",
                            "target_lang_translations": {
                                "ru": "Лампа висит прямо <w>над</w> большим столом в столовой."
                            }
                        },
                        {
                            "text": "Wczoraj widzieliśmy piękny księżyc <w>nad</w> dachem naszego domu.",
                            "target_lang_translations": {
                                "ru": "Вчера мы видели красивую луну <w>над</w> крышей нашего дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "ponad",
                        "powyżej"
                    ],
                    "antonyms": [
                        "pod"
                    ],
                    "common_phrases": [
                        "tuż nad",
                        "wysoko nad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на место, находящееся выше данного объекта или человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "над",
                                "target_lang_sense_clarification": "пространственное положение выше чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b15b47d-8a9b-391c-9f1d-bfbc22499e60",
                    "sense_definition": "Wskazuje na lokalizację w bezpośrednim sąsiedztwie czegoś, najczęściej zbiornika wodnego.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_proximity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi dziadkowie mieszkają w małym domku <w>nad</w> morzem.",
                            "target_lang_translations": {
                                "ru": "Мои бабушка и дедушка живут в маленьком домике <w>у</w> моря."
                            }
                        },
                        {
                            "text": "Lubimy spędzać letnie wieczory <w>nad</w> rzeką.",
                            "target_lang_translations": {
                                "ru": "Мы любим проводить летние вечера <w>на</w> реке."
                            }
                        }
                    ],
                    "synonyms": [
                        "obok"
                    ],
                    "common_phrases": [
                        "nad morzem",
                        "nad rzeką",
                        "nad jeziorem",
                        "nad wodą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на местонахождение в непосредственной близости от чего-либо, чаще всего водоема."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "у",
                                "target_lang_sense_clarification": "в непосредственной близости, возле"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "на берегу (водоема)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bbcd561c-d6c3-3366-8906-c11ac336c574",
                    "sense_definition": "Wskazuje na kierunek ruchu w stronę bezpośredniego sąsiedztwa czegoś, najczęściej zbiornika wodnego.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_proximity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W przyszłym tygodniu jedziemy z rodziną <w>nad</w> jezioro.",
                            "target_lang_translations": {
                                "ru": "На следующей неделе мы с семьей едем <w>на</w> озеро."
                            }
                        },
                        {
                            "text": "Czy chcesz pójść z nami <w>nad</w> rzekę?",
                            "target_lang_translations": {
                                "ru": "Ты хочешь пойти с нами <w>на</w> реку?"
                            }
                        }
                    ],
                    "synonyms": [
                        "do"
                    ],
                    "antonyms": [
                        "znad"
                    ],
                    "common_phrases": [
                        "jechać nad morze",
                        "iść nad rzekę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на направление движения в сторону непосредственной близости чего-либо, чаще всего водоема."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "направление движения к берегу водоема"
                            },
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "по направлению к"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eef3b675-c05a-383c-a296-2877fe9750b2",
                    "sense_definition": "Wskazuje na ruch w kierunku miejsca znajdującego się wyżej niż dany obiekt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_vertical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ptak szybko przeleciał <w>nad</w> wysokim drzewem.",
                            "target_lang_translations": {
                                "ru": "Птица быстро пролетела <w>над</w> высоким деревом."
                            }
                        },
                        {
                            "text": "Podnieś obie ręce wysoko <w>nad</w> głowę.",
                            "target_lang_translations": {
                                "ru": "Подними обе руки высоко <w>над</w> головой."
                            }
                        }
                    ],
                    "synonyms": [
                        "ponad"
                    ],
                    "common_phrases": [
                        "przelecieć nad",
                        "skoczyć nad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на движение в направлении места, находящегося выше данного объекта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "над",
                                "target_lang_sense_clarification": "движение поверх чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0b3c8d0-ea5b-3f7e-b581-be99d32f6ccd",
                    "sense_definition": "Wskazuje na przedmiot, temat lub cel wykonywanej czynności umysłowej lub pracy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_topic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz zespół pracuje obecnie <w>nad</w> nowym projektem.",
                            "target_lang_translations": {
                                "ru": "Наша команда сейчас работает <w>над</w> новым проектом."
                            }
                        },
                        {
                            "text": "Musimy poważnie zastanowić się <w>nad</w> twoją propozycją.",
                            "target_lang_translations": {
                                "ru": "Мы должны серьезно подумать <w>над</w> твоим предложением."
                            }
                        }
                    ],
                    "common_phrases": [
                        "praca nad",
                        "dyskusja nad",
                        "zastanawiać się nad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на предмет, тему или цель выполняемой умственной деятельности или работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "над",
                                "target_lang_sense_clarification": "объект работы или размышлений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7e1ed1c-8c26-3643-ba44-b3e4c5bbea81",
                    "sense_definition": "Wskazuje na przewagę, władzę, kontrolę lub opiekę nad kimś lub czymś.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dobry nauczyciel zawsze panuje <w>nad</w> swoją klasą.",
                            "target_lang_translations": {
                                "ru": "Хороший учитель всегда сохраняет контроль <w>над</w> своим классом."
                            }
                        },
                        {
                            "text": "Armia przejęła pełną kontrolę <w>nad</w> stolicą kraju.",
                            "target_lang_translations": {
                                "ru": "Армия взяла полный контроль <w>над</w> столицей страны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kontrola nad",
                        "władza nad",
                        "panować nad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на превосходство, власть, контроль или опеку над кем-то или чем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "над",
                                "target_lang_sense_clarification": "контроль или власть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5a5ae07-568c-3871-bb9b-0d594ccf1a18",
                    "sense_definition": "Określa czas w przybliżeniu, zwykle bezpośrednio przed nadejściem jakiejś pory dnia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "temporal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Impreza skończyła się dopiero <w>nad</w> ranem.",
                            "target_lang_translations": {
                                "ru": "Вечеринка закончилась только <w>под</w> утро."
                            }
                        },
                        {
                            "text": "Wróciliśmy do domu zmęczeni <w>nad</w> wieczorem.",
                            "target_lang_translations": {
                                "ru": "Мы вернулись домой уставшими <w>под</w> вечер."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nad ranem",
                        "nad wieczorem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначает приблизительное время, обычно непосредственно перед наступлением какого-либо времени суток."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "под",
                                "target_lang_sense_clarification": "приближение к определенному времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83fd3793-bb6a-301e-88be-fca66835e75b",
                    "sense_definition": "Wskazuje na wyższy stopień cechy lub intensywność przekraczającą pewną miarę.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kocham moją rodzinę <w>nad</w> życie.",
                            "target_lang_translations": {
                                "ru": "Я люблю свою семью <w>больше</w> жизни."
                            }
                        },
                        {
                            "text": "To zadanie jest trudne <w>nad</w> wyraz.",
                            "target_lang_translations": {
                                "ru": "Это задание трудно <w>сверх</w> всякой меры."
                            }
                        }
                    ],
                    "synonyms": [
                        "ponad"
                    ],
                    "common_phrases": [
                        "nad życie",
                        "nad wyraz",
                        "nad podziw"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на высшую степень качества или интенсивность, превышающую определенную меру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "больше",
                                "target_lang_sense_clarification": "сравнение, превышение меры"
                            },
                            {
                                "target_lang_word": "сверх",
                                "target_lang_sense_clarification": "чрезмерность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa650f09-c4e2-3bc2-a83e-f2907a800840",
                    "sense_definition": "W konstrukcjach z powtórzonym rzeczownikiem oznacza najwyższy stopień doskonałości lub natężenia cechy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "abstract_superlative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To była dla mnie pieśń <w>nad</w> pieśniami.",
                            "target_lang_translations": {
                                "ru": "Для меня это была песнь <w>песней</w>."
                            }
                        },
                        {
                            "text": "Uważano go za rycerza <w>nad</w> rycerzami.",
                            "target_lang_translations": {
                                "ru": "Его считали лучшим <w>из</w> рыцарей."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "idiomatic construction"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В конструкциях с повторением существительного означает высшую степень совершенства или интенсивности признака."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "из",
                                "target_lang_sense_clarification": "выделение из группы (суперлатив)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
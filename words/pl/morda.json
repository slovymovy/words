    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "98bb329e-a2b3-3638-ae3d-7aaa176c64f3",
                    "sense_definition": "Pospolite, często lekceważące lub obraźliwe określenie twarzy albo ust człowieka.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uważaj co mówisz, bo zaraz możesz dostać w <mordę>.",
                            "target_lang_translations": {
                                "ru": "Следи за языком, а то сейчас получишь по <морде>."
                            }
                        },
                        {
                            "text": "Ten aktor ma bardzo charakterystyczną <mordę>, idealną do ról gangsterów.",
                            "target_lang_translations": {
                                "ru": "У этого актёра очень характерная <рожа>, идеальная для ролей гангстеров."
                            }
                        }
                    ],
                    "synonyms": [
                        "gęba",
                        "pysk",
                        "ryj",
                        "jadaczka"
                    ],
                    "common_phrases": [
                        "dać w mordę",
                        "drzeć mordę",
                        "obić mordę",
                        "zamknij mordę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo często odbierane jako wulgarne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Просторечное, часто пренебрежительное или оскорбительное название лица или рта человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "морда",
                                "target_lang_sense_clarification": "грубое название лица"
                            },
                            {
                                "target_lang_word": "рожа",
                                "target_lang_sense_clarification": "неприятное лицо"
                            },
                            {
                                "target_lang_word": "рыло",
                                "target_lang_sense_clarification": "крайне грубо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b327137-a460-3ca0-9b2d-6ed84156b666",
                    "sense_definition": "Przednia, wydłużona część głowy zwierzęcia, obejmująca nos i pysk.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój pies położył swoją mokrą <mordę> na moich kolanach.",
                            "target_lang_translations": {
                                "ru": "Мой пёс положил свою мокрую <морду> мне на колени."
                            }
                        },
                        {
                            "text": "Czarna krowa wesoło przeżuwała trawę, kręcąc przy tym <mordą>.",
                            "target_lang_translations": {
                                "ru": "Чёрная корова весело жевала траву, крутя при этом <мордой>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pysk",
                        "ryj"
                    ],
                    "common_phrases": [
                        "psia morda",
                        "końska morda"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передняя, удлинённая часть головы животного, включающая нос и пасть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "морда",
                                "target_lang_sense_clarification": "животного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2dc8bf67-78c4-373e-a53a-7d21dc4f1e13",
                    "sense_definition": "Poufały, slangowy zwrot do bliskiego kolegi lub przyjaciela.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "address",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Siema <mordo>, dawno cię nie widziałem na naszym osiedlu.",
                            "target_lang_translations": {
                                "ru": "Здорово, <братан>, давно тебя не видел у нас на районе."
                            }
                        },
                        {
                            "text": "Nie martw się <mordo>, na pewno wszystko się jakoś ułoży.",
                            "target_lang_translations": {
                                "ru": "Не парься, <дружище>, всё точно как-нибудь образуется."
                            }
                        }
                    ],
                    "common_phrases": [
                        "siema mordo",
                        "co tam mordo"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "używane głównie przez młodzież lub w grupach rówieśniczych"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамильярное, сленговое обращение к близкому приятелю или другу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "братан",
                                "target_lang_sense_clarification": "обращение"
                            },
                            {
                                "target_lang_word": "дружище",
                                "target_lang_sense_clarification": "обращение"
                            },
                            {
                                "target_lang_word": "кентафурик",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "65ce214c-bfb8-310a-887a-a98e79c19df9",
                    "sense_definition": "Wulgarne i agresywne polecenie nakazujące komuś natychmiastowe uciszenie się.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "command",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Morda> w kubeł i rób to, co ci kazałem!",
                            "target_lang_translations": {
                                "ru": "<Пасть> закрой и делай то, что я тебе сказал!"
                            }
                        },
                        {
                            "text": "Krzyknął tylko krótko: <morda>!, i wyszedł z pokoju trzaskając drzwiami.",
                            "target_lang_translations": {
                                "ru": "Он лишь коротко крикнул: «<Заткнись>!» — и вышел из комнаты, хлопнув дверью."
                            }
                        }
                    ],
                    "synonyms": [
                        "morda w kubeł!",
                        "stul mordę!",
                        "zamknij mordę!",
                        "zamknij się!"
                    ],
                    "common_phrases": [
                        "morda w kubeł"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "bardzo nieuprzejme, obraźliwe"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вульгарный и агрессивный приказ кому-либо немедленно замолчать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заткнись",
                                "target_lang_sense_clarification": "приказ"
                            },
                            {
                                "target_lang_word": "пасть",
                                "target_lang_sense_clarification": "в выражении 'закрой пасть'"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "sense_id": "c871f861-1ada-360d-85f6-4377f8b7129c",
                    "sense_definition": "Meervoud van <w>been</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de lange wandeling deden mijn <w>benen</w> pijn.",
                            "target_lang_translations": {
                                "en": "After the long walk, my <w>legs</w> hurt.",
                                "ru": "После долгой прогулки у меня болели <w>ноги</w>."
                            }
                        },
                        {
                            "text": "Lawrie sloeg zijn <w>benen</w> over elkaar.",
                            "target_lang_translations": {
                                "en": "Lawrie crossed his <w>legs</w>.",
                                "ru": "Лори скрестил <w>ноги</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "poten",
                        "ledematen"
                    ],
                    "antonyms": [
                        "armen"
                    ],
                    "common_phrases": [
                        "de benen nemen",
                        "op de benen blijven"
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of 'leg'; the limbs on which a person or animal stands and walks.",
                        "ru": "Множественное число от 'been'; конечности, на которых стоит и ходит человек или животное."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "legs",
                                "target_lang_sense_clarification": "body parts"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ноги",
                                "target_lang_sense_clarification": "части тела"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "67abcded-bfab-35a2-8689-a806e89703f1",
                    "sense_definition": "Met forse pas lopen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>beende</w> vol ergernis naar buiten.",
                            "target_lang_translations": {
                                "en": "He <w>strode</w> outside full of annoyance.",
                                "ru": "Полный раздражения, он <w>зашагал</w> наружу."
                            }
                        },
                        {
                            "text": "Met grote stappen <w>beende</w> hij naar de bank.",
                            "target_lang_translations": {
                                "en": "He <w>strode</w> towards the sofa with big steps.",
                                "ru": "Широкими шагами он <w>зашагал</w> к дивану."
                            }
                        }
                    ],
                    "synonyms": [
                        "schrijden",
                        "stappen"
                    ],
                    "antonyms": [
                        "slenteren"
                    ],
                    "target_lang_definitions": {

            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "9e2c6ca9-371f-3f78-8ed5-a7698df07401",
                    "sense_definition": "To the fullest extent or degree; totally.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "completeness_degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>completely</w> agree with your decision to stay home.",
                            "target_lang_translations": {
                                "ru": "Я <w>полностью</w> согласен с твоим решением остаться дома."
                            }
                        },
                        {
                            "text": "The two sisters look <w>completely</w> different from each other.",
                            "target_lang_translations": {
                                "ru": "Две сестры выглядят <w>совершенно</w> непохожими друг на друга."
                            }
                        }
                    ],
                    "synonyms": [
                        "totally",
                        "absolutely",
                        "entirely",
                        "wholly",
                        "utterly"
                    ],
                    "antonyms": [
                        "partially",
                        "somewhat",
                        "hardly",
                        "partly"
                    ],
                    "common_phrases": [
                        "completely different",
                        "completely wrong",
                        "completely agree",
                        "completely new",
                        "completely understand"
                    ],
                    "target_lang_definitions": {
                        "ru": "В полной мере или степени; абсолютно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совершенно",
                                "target_lang_sense_clarification": "указывает на высшую степень качества или состояния"
                            },
                            {
                                "target_lang_word": "полностью",
                                "target_lang_sense_clarification": "целиком, во всём (например, согласен)"
                            },
                            {
                                "target_lang_word": "совсем",
                                "target_lang_sense_clarification": "абсолютно, очень (часто в разговорной речи)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3de6e491-d15d-313c-aa16-2871751012fa",
                    "sense_definition": "In a way that finishes every part of a task or action; thoroughly.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "completeness_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>completely</w> fill out the application form before submitting it.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>полностью</w> заполните анкету перед её подачей."
                            }
                        },
                        {
                            "text": "The fire <w>completely</w> destroyed the old wooden barn.",
                            "target_lang_translations": {
                                "ru": "Пожар <w>полностью</w> уничтожил старый деревянный сарай."
                            }
                        }
                    ],
                    "synonyms": [
                        "thoroughly",
                        "fully"
                    ],
                    "antonyms": [
                        "incompletely",
                        "partially",
                        "partly"
                    ],
                    "common_phrases": [
                        "completely fill",
                        "completely finish",
                        "completely remove",
                        "completely cover"
                    ],
                    "target_lang_definitions": {
                        "ru": "Таким образом, что действие выполнено до конца; исчерпывающе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полностью",
                                "target_lang_sense_clarification": "до конца, без остатка"

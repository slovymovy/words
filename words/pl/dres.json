    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "26d6605a-77a0-341c-89e2-3d4d5d75e1c7",
                    "sense_definition": "Wygodny strój sportowy składający się zazwyczaj z luźnych spodni i bluzy, noszony podczas treningu lub jako ubranie domowe.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po powrocie z pracy lubię przebrać się w wygodny <w>dres</w>.",
                            "target_lang_translations": {
                                "ru": "Вернувшись с работы, я люблю переодеться в удобный <w>спортивный костюм</w>."
                            }
                        },
                        {
                            "text": "Na zajęcia z wychowania fizycznego musisz przynieść czysty <w>dres</w>.",
                            "target_lang_translations": {
                                "ru": "На урок физкультуры ты должен принести чистый <w>спортивный костюм</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dresy",
                        "strój sportowy"
                    ],
                    "common_phrases": [
                        "chodzić w dresie",
                        "szary dres",
                        "założyć dres",
                        "ortalionowy dres"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удобная спортивная одежда, обычно состоящая из свободных брюк и кофты, которую носят во время тренировок или как домашнюю одежду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спортивный костюм",
                                "target_lang_sense_clarification": "комплект одежды"
                            },
                            {
                                "target_lang_word": "тренировочный костюм",
                                "target_lang_sense_clarification": "одежда для тренировок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9f1ffbc-463c-3273-87c9-e0fea10633bf",
                    "sense_definition": "Potoczne, często lekceważące określenie młodego mężczyzny, kojarzonego z noszeniem dresu, przebywaniem na blokowiskach i agresywnym zachowaniem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_subculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pod klatką schodową stał jakiś <w>dres</w> i głośno krzyczał.",
                            "target_lang_translations": {
                                "ru": "У подъезда стоял какой-то <w>гопник</w> и громко кричал."
                            }
                        },
                        {
                            "text": "Nie każdy chłopak w sportowej bluzie to od razu typowy <w>dres</w>.",
                            "target_lang_translations": {
                                "ru": "Не каждый парень в спортивной кофте — сразу типичный <w>гопник</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dresiarz"
                    ],
                    "common_phrases": [
                        "typowy dres",
                        "osiedlowy dres"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często pejoratywne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное, часто пренебрежительное название молодого человека, ассоциирующегося с ношением спортивного костюма, проживанием в спальных районах и агрессивным поведением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гопник",
                                "target_lang_sense_clarification": "представитель субкультуры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a847a70-8bd8-35e2-940c-7fd5911c2631",
                    "sense_definition": "Regionalna nazwa rdestu ptasiego, pospolitej rośliny zielnej.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "plant_regional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja babcia zbierała <w>dres</w> na łące, by robić z niego napary.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка собирала <w>спорыш</w> на лугу, чтобы делать из него настои."
                            }
                        },
                        {
                            "text": "W tym regionie na rdest mówi się po prostu <w>dres</w>.",
                            "target_lang_translations": {
                                "ru": "В этом регионе спорыш называют просто <w>дрес</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Tarnów"
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Региональное название горца птичьего (спорыша), распространенного травянистого растения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спорыш",
                                "target_lang_sense_clarification": "растение"
                            },
                            {
                                "target_lang_word": "горец птичий",
                                "target_lang_sense_clarification": "ботаническое название"
                            }
                        ]
                    }
                }
            ]
        }

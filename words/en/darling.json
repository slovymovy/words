        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ac143cea-5318-3fe7-8e2b-7fde70b46dc3",
                    "sense_definition": "A person who is very dear to one; often used as an affectionate term of address.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "beloved_dear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pass the wine, would you, <w>darling</w>?",
                            "target_lang_translations": {
                                "ru": "Передай, пожалуйста, вино, <w>дорогой</w>?"
                            }
                        },
                        {
                            "text": "Sleep, my <w>darling</w>, tenderly!",
                            "target_lang_translations": {
                                "ru": "Спи, мой <w>любимый</w>, нежно!"
                            }
                        }
                    ],
                    "synonyms": [
                        "honey",
                        "sweetheart",
                        "love",
                        "dear",
                        "babe",
                        "pet"
                    ],
                    "common_phrases": [
                        "my darling",
                        "hello darling",
                        "yes darling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который очень дорог; часто используется как ласковое обращение."

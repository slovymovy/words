    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e9d0b87b-4091-3233-b056-6d7905be9ace",
                    "sense_definition": "Чувствовать глубокую привязанность, преданность и сильное влечение к человеку, семье или родине.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "emotional_attachment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я очень <w>люблю</w> свою семью и всегда помогаю им.",
                            "target_lang_translations": {
                                "en": "I really <w>love</w> my family and always help them."
                            }
                        },
                        {
                            "text": "Они обещали <w>любить</w> друг друга и быть вместе всю жизнь.",
                            "target_lang_translations": {
                                "en": "They promised to <w>love</w> each other and be together their whole lives."
                            }
                        },
                        {
                            "text": "Настоящий гражданин должен <w>любить</w> родину и заботиться о ней.",
                            "target_lang_translations": {
                                "en": "A true citizen must <w>love</w> their homeland and care for it."
                            }
                        }
                    ],
                    "synonyms": [
                        "обожать"
                    ],
                    "antonyms": [
                        "ненавидеть",
                        "презирать"
                    ],
                    "common_phrases": [
                        "любить всем сердцем",
                        "любить друг друга",
                        "любить родину",
                        "безумно любить"
                    ],
                    "target_lang_definitions": {
                        "en": "To feel deep affection, devotion, and strong attraction to a person, family, or homeland."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "love",
                                "target_lang_sense_clarification": "deep affection"
                            },
                            {
                                "target_lang_word": "adore",
                                "target_lang_sense_clarification": "love deeply"
                            },
                            {
                                "target_lang_word": "cherish",
                                "target_lang_sense_clarification": "hold dear"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a86c6278-6e93-3f02-bff1-e0498127e831",
                    "sense_definition": "Испытывать удовольствие, интерес или вкус к чему-либо; иметь склонность к какому-либо занятию.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "preferences",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой брат <w>любит</w> читать фантастику по вечерам.",
                            "target_lang_translations": {
                                "en": "My brother <w>loves</w> reading science fiction in the evenings."
                            }
                        },
                        {
                            "text": "Я не <w>люблю</w> сладкое, поэтому пью чай без сахара.",
                            "target_lang_translations": {
                                "en": "I don't <w>like</w> sweets, so I drink tea without sugar."
                            }
                        },
                        {
                            "text": "Мы <w>любим</w> путешествовать и открывать новые страны.",
                            "target_lang_translations": {
                                "en": "We <w>love</w> to travel and discover new countries."
                            }
                        }
                    ],
                    "synonyms": [
                        "обожать"
                    ],
                    "antonyms": [
                        "ненавидеть"
                    ],
                    "common_phrases": [
                        "любить читать",
                        "любить сладкое",
                        "любить путешествовать",
                        "не люблю ждать"
                    ],
                    "target_lang_definitions": {
                        "en": "To experience pleasure, interest, or a liking for something; to be inclined towards an activity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "like",
                                "target_lang_sense_clarification": "find agreeable"
                            },
                            {
                                "target_lang_word": "love",
                                "target_lang_sense_clarification": "enjoy very much"
                            },
                            {
                                "target_lang_word": "enjoy",
                                "target_lang_sense_clarification": "take pleasure in"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60c758aa-9479-3519-8a9b-ac97b8a345c8",
                    "sense_definition": "Нуждаться в определённых условиях для жизни и роста (о растениях и животных).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "biological_needs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эти комнатные цветы <w>любят</w> яркое солнце и тепло.",
                            "target_lang_translations": {
                                "en": "These indoor flowers <w>love</w> bright sun and warmth."
                            }
                        },
                        {
                            "text": "Капуста очень <w>любит</w> воду, поэтому её нужно часто поливать.",
                            "target_lang_translations": {
                                "en": "Cabbage really <w>loves</w> water, so it needs to be watered often."
                            }
                        }
                    ],
                    "common_phrases": [
                        "любить воду",
                        "любить солнце",
                        "любить тень",
                        "любить тепло"
                    ],
                    "target_lang_definitions": {
                        "en": "To need specific conditions for life and growth (regarding plants and animals)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "love",
                                "target_lang_sense_clarification": "thrive in"
                            },
                            {
                                "target_lang_word": "need",
                                "target_lang_sense_clarification": "require conditions"
                            }
                        ]
                    }
                }
            ]
        }
    ]

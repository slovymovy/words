    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0d3265e3-1c93-3ff0-9b6d-f3adc284b3c4",
                    "sense_definition": "Podawać do wiadomości coś, co było tajne, ukryte lub nieznane; wyjawiać sekret lub informację.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "making_known",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziennikarz śledczy postanowił <ujawniać> przypadki korupcji w lokalnym urzędzie.",
                            "target_lang_translations": {
                                "ru": "Следственный журналист решил <раскрывать> случаи коррупции в местном ведомстве."
                            }
                        },
                        {
                            "text": "Nie powinieneś nikomu <ujawniać> swojego hasła do konta bankowego.",
                            "target_lang_translations": {
                                "ru": "Ты не должен никому <сообщать> свой пароль от банковского счета."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyjawiać",
                        "zdradzać",
                        "odtajniać",
                        "prezentować"
                    ],
                    "antonyms": [
                        "ukrywać",
                        "zatajać"
                    ],
                    "common_phrases": [
                        "ujawniać prawdę",
                        "ujawniać tajemnicę",
                        "ujawniać szczegóły",
                        "ujawniać informacje",
                        "ujawniać źródła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать известным то, что было тайным, скрытым или неизвестным; разглашать секрет или информацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раскрывать",
                                "target_lang_sense_clarification": "делать тайное явным (секреты, преступления)"
                            },
                            {
                                "target_lang_word": "разглашать",
                                "target_lang_sense_clarification": "сообщать секретную информацию (пароли, тайны)"
                            },
                            {
                                "target_lang_word": "обнародовать",
                                "target_lang_sense_clarification": "публично объявлять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae3ce56d-d9db-3003-b6a1-cd784aa8a70a",
                    "sense_definition": "Sprawiać, że coś staje się widoczne, zauważalne lub możliwe do wykrycia (np. cechy, objawy, przedmioty).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "making_known",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szczegółowe badania krwi mogą <ujawniać> obecność rzadkich chorób.",
                            "target_lang_translations": {
                                "ru": "Подробные анализы крови могут <выявлять> наличие редких заболеваний."
                            }
                        },
                        {
                            "text": "Artysta poprzez swoje obrazy starał się <ujawniać> ukryte piękno natury.",
                            "target_lang_translations": {
                                "ru": "Художник через свои картины старался <раскрывать> скрытую красоту природы."
                            }
                        }
                    ],
                    "synonyms": [
                        "demonstrować",
                        "odtajniać",
                        "prezentować",
                        "wyjawiać"
                    ],
                    "antonyms": [
                        "maskować",
                        "zasłaniać"

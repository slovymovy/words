                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "ruthless",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De realiteit kwam <w>keihard</w> binnen toen hij het nieuws hoorde.",
                            "target_lang_translations": {
                                "ru": "Реальность ударила <w>жестоко</w>, когда он услышал новости."
                            }

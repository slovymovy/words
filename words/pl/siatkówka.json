                {
                    "sense_id": "bd41fde4-cc44-3db9-9037-bf755037b064",
                    "sense_definition": "Gra sportowa dla dwóch drużyn polegająca na odbijaniu piłki nad specjalną siatką.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W drużynie <w>siatkówki</w> gra sześciu zawodników.",
                            "target_lang_translations": {
                                "ru": "В команде по <w>волейболу</w> играют шесть игроков."
                            }
                        },
                        {
                            "text": "Latem bardzo często chodzimy na plażę, aby grać w <w>siatkówkę</w>.",
                            "target_lang_translations": {
                                "ru": "Летом мы очень часто ходим на пляж, чтобы поиграть в <w>волейбол</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "piłka siatkowa",
                        "siatka",
                        "siata"
                    ],
                    "common_phrases": [
                        "grać w siatkówkę",
                        "mecz siatkówki",
                        "drużyna siatkówki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спортивная игра для двух команд, заключающаяся в перебрасывании мяча через специальную сетку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волейбол",
                                "target_lang_sense_clarification": "вид спорта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67337f04-2513-3e5b-aeb7-a2b90d490810",
                    "sense_definition": "Część oka odpowiadająca za przetwarzanie obrazu na impulsy nerwowe.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat ma uszkodzoną <w>siatkówkę</w> i gorzej widzi na prawe oko.",
                            "target_lang_translations": {
                                "ru": "У моего брата повреждена <w>сетчатка</w>, и он хуже видит правым глазом."
                            }
                        },
                        {
                            "text": "Okulista zalecił mi pilne badanie <w>siatkówki</w> z powodu pogorszenia wzroku.",
                            "target_lang_translations": {
                                "ru": "Окулист порекомендовал мне срочное обследование <w>сетчатки</w> из-за ухудшения зрения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "odklejenie siatkówki",
                        "badanie siatkówki",
                        "uszkodzenie siatkówki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть глаза, отвечающая за преобразование изображения в нервные импульсы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сетчатка",
                                "target_lang_sense_clarification": "часть глаза"

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "86168db3-badc-384c-966f-9954bc8fe0a2",
                    "sense_definition": "Перестать спать, перейти от состояния сна к бодрствованию.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <w>проснулся</w> сегодня очень рано из-за яркого солнца.",
                            "target_lang_translations": {
                                "en": "I <w>woke up</w> very early today because of the bright sun."
                            }
                        },
                        {
                            "text": "Тебе нужно <w>проснуться</w> завтра в шесть утра, чтобы успеть на поезд.",
                            "target_lang_translations": {
                                "en": "You need to <w>wake up</w> tomorrow at six in the morning to catch the train."
                            }
                        }
                    ],
                    "synonyms": [
                        "пробудиться"
                    ],
                    "antonyms": [
                        "заснуть"
                    ],
                    "common_phrases": [
                        "проснуться рано",
                        "проснуться утром",
                        "проснуться от шума",
                        "тяжело проснуться"
                    ],
                    "target_lang_definitions": {
                        "en": "To stop sleeping, to transition from a state of sleep to wakefulness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wake up",
                                "target_lang_sense_clarification": "most common"
                            },
                            {
                                "target_lang_word": "awake",
                                "target_lang_sense_clarification": "formal or literary"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34bd4763-d49f-37e7-bd28-06bdb17dafc7",
                    "sense_definition": "Возникнуть, начать проявляться (о чувствах, желаниях или природных явлениях).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В его душе внезапно <w>проснулась</w> надежда на лучшее будущее.",
                            "target_lang_translations": {
                                "en": "Hope for a better future suddenly <w>awoke</w> in his soul."
                            }
                        },
                        {
                            "text": "После долгой зимы природа наконец <w>проснулась</w> и зацвела.",
                            "target_lang_translations": {
                                "en": "After the long winter, nature finally <w>woke up</w> and blossomed."
                            }
                        }
                    ],
                    "synonyms": [
                        "пробудиться",
                        "появиться"
                    ],
                    "common_phrases": [
                        "проснулся интерес",
                        "проснулась совесть",
                        "проснулся аппетит",
                        "природа проснулась"
                    ],
                    "target_lang_definitions": {
                        "en": "To arise, to start manifesting (about feelings, desires, or natural phenomena)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "awaken",
                                "target_lang_sense_clarification": "feelings or nature"
                            },
                            {
                                "target_lang_word": "wake up",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "arise",
                                "target_lang_sense_clarification": "feelings"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5df5c75-252c-32ce-927f-3a1d9b166355",
                    "sense_definition": "Стать активным, деятельным; прийти в движение после периода застоя.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "К середине дня сотрудники офиса <w>проснулись</w> и начали активно работать.",
                            "target_lang_translations": {
                                "en": "By the middle of the day, the office staff <w>woke up</w> and started working actively."
                            }
                        },
                        {
                            "text": "Ночной город <w>проснулся</w> и наполнился огнями и музыкой.",
                            "target_lang_translations": {
                                "en": "The night city <w>woke up</w> and filled with lights and music."
                            }
                        }
                    ],
                    "synonyms": [
                        "активизироваться",
                        "обнаружиться"
                    ],
                    "common_phrases": [
                        "город проснулся",
                        "вулкан проснулся"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To become active or energetic; to come into motion after a period of stagnation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wake up",
                                "target_lang_sense_clarification": "metaphorical"
                            },
                            {
                                "target_lang_word": "come to life",
                                "target_lang_sense_clarification": "idiomatic"
                            },
                            {
                                "target_lang_word": "stir",
                                "target_lang_sense_clarification": "to become active"
                            }
                        ]
                    }
                }
            ]
        }
    ]

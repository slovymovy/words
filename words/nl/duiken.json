                {
                    "sense_id": "238400d9-0050-391f-aaa2-bfbb7bedf9e9",
                    "sense_definition": "Met het hoofd en de armen eerst in het water springen",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "water_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen leren van de hoge springplank te <w>duiken</w>.",
                            "target_lang_translations": {
                                "en": "The children are learning to <w>dive</w> from the high diving board.",
                                "ru": "Дети учатся <w>нырять</w> с высокой вышки."
                            }
                        },
                        {
                            "text": "Hij durfde niet in het koude water te <w>duiken</w>.",
                            "target_lang_translations": {
                                "en": "He didn't dare to <w>dive</w> into the cold water.",
                                "ru": "Он не осмелился <w>нырнуть</w> в холодную воду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in het diepe duiken",
                        "van de plank duiken",
                        "het water in duiken"
                    ],
                    "target_lang_definitions": {

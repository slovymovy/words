    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1b30d37a-0d1a-3bd2-a16b-03f0918fde72",
                    "sense_definition": "Obszar dzikiej przyrody, gęsto porośnięty roślinnością i niezagospodarowany przez człowieka.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "place_nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podróżnicy musieli przedzierać się przez gęstą <w>dzicz</w> amazońskiej dżungli.",
                            "target_lang_translations": {
                                "ru": "Путешественникам пришлось пробираться сквозь густые <w>дебри</w> амазонских джунглей."
                            }
                        },
                        {
                            "text": "W tej okolicy panuje prawdziwa <w>dzicz</w>, nie ma tu żadnych dróg ani domów.",
                            "target_lang_translations": {
                                "ru": "В этой округе царит настоящая <w>глушь</w>, здесь нет ни дорог, ни домов."
                            }
                        }
                    ],
                    "synonyms": [
                        "pustkowie",
                        "odludzie"
                    ],
                    "antonyms": [
                        "cywilizacja",
                        "miasto"
                    ],
                    "common_phrases": [
                        "leśna dzicz",
                        "prawo dziczy",
                        "dziewicza dzicz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дикая местность, густо поросшая растительностью и не освоенная человеком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дебри",
                                "target_lang_sense_clarification": "густые, труднопроходимые заросли"
                            },
                            {
                                "target_lang_word": "глушь",
                                "target_lang_sense_clarification": "дикое, безлюдное место"
                            },
                            {
                                "target_lang_word": "дикая природа",
                                "target_lang_sense_clarification": "нетронутая природа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5e10d15-3cb1-3bce-8a99-2a986453d4f3",
                    "sense_definition": "Miejsce bardzo oddalone od centrów cywilizacyjnych, zacofane i pozbawione udogodnień.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "place_nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wyprowadzili się z miasta i teraz mieszkają w totalnej <w>dziczy</w>.",
                            "target_lang_translations": {
                                "ru": "Они уехали из города и теперь живут в полной <w>глуши</w>."
                            }
                        },
                        {
                            "text": "To straszna <w>dzicz</w>, gdzie diabeł mówi dobranoc i nic się nie dzieje.",
                            "target_lang_translations": {
                                "ru": "Это жуткая <w>дыра</w>, где черт ногу сломит и ничего не происходит."
                            }
                        }
                    ],
                    "synonyms": [
                        "prowincja",
                        "dziura",
                        "wygwizdów",
                        "zapadły kąt",
                        "zadupie"
                    ],
                    "antonyms": [
                        "centrum",
                        "metropolia"
                    ],
                    "common_phrases": [
                        "mieszkać w dziczy",
                        "totalna dzicz"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to describe a remote or backward place negatively"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, удаленное от цивилизации, отсталое и лишенное удобств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глухомань",
                                "target_lang_sense_clarification": "очень отдаленное место"
                            },
                            {
                                "target_lang_word": "захолустье",
                                "target_lang_sense_clarification": "провинциальное, отсталое место"
                            },
                            {
                                "target_lang_word": "дыра",
                                "target_lang_sense_clarification": "разг. плохое, скучное место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b6daa2f-c6d9-3e06-9480-da4a66aaea70",
                    "sense_definition": "Grupa ludzi zachowujących się w sposób niecywilizowany, agresywny lub nieopanowany.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "people_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tłum kibiców zachowywał się jak <w>dzicz</w>, niszcząc wszystko na swojej drodze.",
                            "target_lang_translations": {
                                "ru": "Толпа болельщиков вела себя как <w>дикари</w>, круша всё на своем пути."
                            }
                        },
                        {
                            "text": "Ta <w>dzicz</w> nie ma za grosz kultury ani szacunku dla innych ludzi.",
                            "target_lang_translations": {
                                "ru": "Эти <w>дикари</w> не имеют ни капли культуры или уважения к другим людям."
                            }
                        }
                    ],
                    "synonyms": [
                        "hołota"
                    ],
                    "common_phrases": [
                        "zachowywać się jak dzicz"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, ведущих себя нецивилизованно, агрессивно или дико."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дикари",
                                "target_lang_sense_clarification": "нецивилизованные люди"
                            },
                            {
                                "target_lang_word": "варвары",
                                "target_lang_sense_clarification": "грубые, жестокие люди"
                            },
                            {
                                "target_lang_word": "зверьё",
                                "target_lang_sense_clarification": "разг. агрессивная толпа"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
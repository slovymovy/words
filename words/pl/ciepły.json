                {
                    "sense_id": "373e343b-fe1e-3c70-9c41-43d3431bda5f",
                    "sense_definition": "Mający temperaturę wyższą od otoczenia lub ciała ludzkiego, ale nie gorący",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "temperature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ten <ciepły> dzień poszliśmy na długi spacer do parku.",

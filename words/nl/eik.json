                    "sense_id": "0ef64a58-f083-3dc0-8cd6-8c780c6761fb",
                    "sense_definition": "Een loofboom van het geslacht Quercus die eikels als vruchten draagt",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "tree_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude <w>eik</w> in het midden van het plein biedt veel schaduw.",
                            "target_lang_translations": {
                                "en": "The old <w>oak</w> in the middle of the square offers a lot of shade.",
                                "ru": "Старый <w>дуб</w> посреди площади дает много тени."
                            }
                        },
                        {
                            "text": "In de herfst vallen er honderden eikels van de grote <w>eik</w>.",
                            "target_lang_translations": {
                                "en": "In autumn, hundreds of acorns fall from the large <w>oak</w>.",
                                "ru": "Осенью с большого <w>дуба</w> падают сотни желудей."
                            }
                        }
                    ],
                    "synonyms": [
                        "eikenboom"
                    ],
                    "common_phrases": [
                        "een oude eik",
                        "onder de eik"
                    ],
                    "target_lang_definitions": {
                        "en": "A deciduous tree of the genus Quercus that bears acorns as fruit",
                        "ru": "Лиственное дерево рода Quercus, приносящее плоды — жёлуди"
                    },

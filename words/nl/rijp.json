            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b4e2a9df-2548-31a1-b2fd-9982075dcf65",
                    "sense_definition": "De toestand waarin vruchten of gewassen volledig ontwikkeld zijn en klaar om gegeten of geoogst te worden.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>rijpe</w> appels vielen door de wind van de boom.",
                            "target_lang_translations": {
                                "en": "The <w>ripe</w> apples fell from the tree due to the wind.",
                                "ru": "Из-за ветра <w>спелые</w> яблоки падали с дерева."
                            }
                        },
                        {
                            "text": "In de zomer eten we graag zoet en <w>rijp</w> fruit.",
                            "target_lang_translations": {
                                "en": "In the summer we like to eat sweet and <w>ripe</w> fruit.",
                                "ru": "Летом мы любим есть сладкие и <w>спелые</w> фрукты."
                            }
                        }
                    ],
                    "antonyms": [
                        "onrijp"
                    ],
                    "common_phrases": [
                        "rijpe appels",
                        "rijp fruit",
                        "rijpe bananen"
                    ],
                    "target_lang_definitions": {
                        "en": "The state in which fruit or crops are fully developed and ready to be eaten or harvested.",
                        "ru": "Состояние, при котором фрукты или посевы полностью развились и готовы к употреблению или сбору урожая."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ripe",
                                "target_lang_sense_clarification": "fully grown and ready to eat"
                            },
                            {

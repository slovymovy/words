    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "24380681-852f-3313-9594-68c03655d215",
                    "sense_definition": "Een feestelijke gebeurtenis of gedenkwaardig moment herdenken of beleven",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "celebrate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij <w>vieren</w> morgen mijn verjaardag met de hele familie.",
                            "target_lang_translations": {
                                "en": "We are <w>celebrating</w> my birthday tomorrow with the whole family.",
                                "ru": "Мы <w>празднуем</w> мой день рождения завтра всей семьей."
                            }
                        },
                        {
                            "text": "Ze hebben besloten hun overwinning groots te <w>vieren</w> in de stad.",
                            "target_lang_translations": {
                                "en": "They have decided to <w>celebrate</w> their victory in a big way in the city.",
                                "ru": "Они решили с размахом <w>отпраздновать</w> свою победу в городе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "verjaardag vieren",
                        "feest vieren",
                        "succes vieren",
                        "kerst vieren"
                    ],
                    "target_lang_definitions": {
                        "en": "To mark a special occasion or event with festivities",
                        "ru": "Отмечать праздничное событие или памятный момент"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "celebrate",
                                "target_lang_sense_clarification": "to observe a notable occasion"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "праздновать",
                                "target_lang_sense_clarification": "проводить праздник"
                            },
                            {
                                "target_lang_word": "отмечать",
                                "target_lang_sense_clarification": "выделять событие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f7e8f71-b6f0-34f9-989a-882bca2ad4f3",
                    "sense_definition": "Een touw of kabel losser maken of laten uitlopen",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slacken",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet het touw een beetje <w>vieren</w> zodat de boot kan zakken.",
                            "target_lang_translations": {
                                "en": "You need to <w>slacken</w> the rope a bit so the boat can lower.",
                                "ru": "Тебе нужно немного <w>ослабить</w> веревку, чтобы лодка могла опуститься."
                            }
                        },
                        {
                            "text": "De zeilers lieten de schoot <w>vieren</w> om snelheid te minderen.",
                            "target_lang_translations": {
                                "en": "The sailors let the sheet <w>slacken</w> to reduce speed.",
                                "ru": "Моряки <w>ослабили</w> шкот, чтобы сбавить скорость."
                            }
                        }
                    ],
                    "antonyms": [
                        "aanhalen",
                        "spannen",
                        "straktrekken"
                    ],
                    "common_phrases": [
                        "touw vieren",
                        "de teugels vieren"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a rope or cable looser or allow it to run out",
                        "ru": "Ослаблять натяжение веревки или троса, отпускать"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slacken",
                                "target_lang_sense_clarification": "to make less tight"
                            },
                            {
                                "target_lang_word": "pay out",
                                "target_lang_sense_clarification": "to let a line run out"
                            },
                            {
                                "target_lang_word": "veer",
                                "target_lang_sense_clarification": "nautical: to let out cable or chain"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ослаблять",
                                "target_lang_sense_clarification": "делать менее тугим"
                            },
                            {
                                "target_lang_word": "травить",
                                "target_lang_sense_clarification": "морской термин"
                            },
                            {
                                "target_lang_word": "отпускать",
                                "target_lang_sense_clarification": "давать свободу натяжению"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3e204995-0090-3ed5-bd91-6a722cbd1081",
                    "sense_definition": "Vorm van <w>vier</w> gebruikt in tijdsaanduidingen na een voorzetsel",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "time_indication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was al over <w>vieren</w> toen we eindelijk thuis waren.",
                            "target_lang_translations": {
                                "en": "It was already past <w>four</w> when we finally got home.",
                                "ru": "Было уже после <w>четырех</w>, когда мы наконец вернулись домой."
                            }
                        },
                        {
                            "text": "We spreken meestal rond <w>vieren</w> af in het park.",
                            "target_lang_translations": {
                                "en": "We usually meet around <w>four</w> in the park.",
                                "ru": "Мы обычно договариваемся встретиться в парке около <w>четырех</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "over vieren",
                        "voor vieren",
                        "rond vieren",
                        "na vieren"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "datief"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Reference to the time of four o'clock",
                        "ru": "Форма числительного «четыре», используемая при указании времени после предлога"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "four",
                                "target_lang_sense_clarification": "time reference"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "четыре",
                                "target_lang_sense_clarification": "о времени суток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "840dd7ed-5c8a-3543-af50-ca35b74555cc",
                    "sense_definition": "Meervoud van het zelfstandig naamwoord <w>vier</w>",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "plural_number",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In zijn rapport stonden helaas twee <w>vieren</w> voor wiskunde.",
                            "target_lang_translations": {
                                "en": "Unfortunately, his report contained two <w>fours</w> for mathematics.",
                                "ru": "В его табеле, к сожалению, стояли две <w>четверки</w> по математике."
                            }
                        },
                        {
                            "text": "Hij gooide twee <w>vieren</w> met de dobbelstenen.",
                            "target_lang_translations": {
                                "en": "He rolled two <w>fours</w> with the dice.",
                                "ru": "Он выбросил две <w>четверки</w> на игральных костях."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "meervoud"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The plural form of the number four",
                        "ru": "Множественное число существительного «четверка»"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fours",
                                "target_lang_sense_clarification": "plural number"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "четверки",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a5d4edc4-abb6-3ef2-9741-d2fd8c272448",
                    "sense_definition": "Osobisty stosunek do kogoś lub czegoś, sposób myślenia i postrzegania rzeczywistości.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "attitude_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli ciągle będziesz z negatywnym <w>nastawieniem</w> podchodził do nauki, to nie zajdziesz w życiu daleko.",
                            "target_lang_translations": {
                                "ru": "Если ты постоянно будешь подходить к учебе с негативным <w>настроем</w>, то далеко в жизни не уйдешь."
                            }
                        },
                        {
                            "text": "Jego optymistyczne <w>nastawienie</w> do życia pomaga mu pokonywać codzienne trudności.",
                            "target_lang_translations": {
                                "ru": "Его оптимистичное <w>отношение</w> к жизни помогает ему преодолевать повседневные трудности."
                            }
                        }
                    ],
                    "synonyms": [
                        "podejście",
                        "stosunek"
                    ],
                    "common_phrases": [
                        "pozytywne nastawienie",
                        "negatywne nastawienie",
                        "złe nastawienie",
                        "nastawienie do życia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Личное отношение к кому-либо или чему-либо, образ мыслей и восприятия действительности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "настрой",
                                "target_lang_sense_clarification": "психологическое состояние, расположение духа"
                            },
                            {
                                "target_lang_word": "отношение",
                                "target_lang_sense_clarification": "взгляд на вещи, подход"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eaebf1d2-ea3c-3fac-8dd0-276aa24ba3d9",
                    "sense_definition": "Sposób uregulowania urządzenia lub mechanizmu; wybrane parametry działania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "configuration_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przy tych <w>nastawieniach</w> radio odbiera sygnały z bardzo daleka.",
                            "target_lang_translations": {
                                "ru": "При этих <w>настройках</w> радио принимает сигналы с очень большого расстояния."
                            }
                        },
                        {
                            "text": "Operator maszyny sprawdził każde <w>nastawienie</w> przed rozpoczęciem produkcji.",
                            "target_lang_translations": {
                                "ru": "Оператор станка проверил каждую <w>настройку</w> перед началом производства."
                            }
                        }
                    ],
                    "synonyms": [
                        "ustawienie"
                    ],
                    "common_phrases": [
                        "zmienić nastawienie",
                        "nastawienie parametrów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способ регулировки устройства или механизма; выбранные параметры работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "настройка",
                                "target_lang_sense_clarification": "параметр устройства"
                            },
                            {
                                "target_lang_word": "установка",
                                "target_lang_sense_clarification": "конфигурация, параметры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95bd4c2f-42cd-3491-a2b8-570cf505a7c6",
                    "sense_definition": "Rzeczownik odczasownikowy od <w>nastawić</w> (wykonanie czynności, np. regulacji zegara lub składania kości).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz wykonał bolesne <w>nastawienie</w> złamanej kości u pacjenta.",
                            "target_lang_translations": {
                                "ru": "Врач выполнил болезненное <w>вправление</w> сломанной кости пациенту."
                            }
                        },
                        {
                            "text": "Dokładne <w>nastawienie</w> budzika jest kluczowe, aby nie spóźnić się do pracy.",
                            "target_lang_translations": {
                                "ru": "Точная <w>установка</w> будильника имеет ключевое значение, чтобы не опоздать на работу."
                            }
                        }
                    ],
                    "antonyms": [
                        "nienastawienie"
                    ],
                    "common_phrases": [
                        "nastawienie budzika",
                        "nastawienie kości"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verbal noun"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отглагольное существительное от \"nastawić\" (выполнение действия, напр. регулировка часов или вправление кости)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вправление",
                                "target_lang_sense_clarification": "медицинское (кости, сустава)"
                            },
                            {
                                "target_lang_word": "установка",
                                "target_lang_sense_clarification": "времени, будильника"
                            }
                        ]
                    }
                }

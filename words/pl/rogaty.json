                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строптивый",
                                "target_lang_sense_clarification": "о характере"
                            },
                            {
                                "target_lang_word": "упрямый",
                                "target_lang_sense_clarification": "о характере"
                            },
                            {
                                "target_lang_word": "непокорный",
                                "target_lang_sense_clarification": "о характере"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8175cfec-4996-3f77-a8de-b30866300742",
                    "sense_definition": "O księżycu: mający kształt zbliżony do sierpa, z widocznymi ostrymi końcami.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "shape_moon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nad lasem wisiał srebrny, <w>rogaty</w> księżyc.",

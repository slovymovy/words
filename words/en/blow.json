                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Blow</w> arrived late for the meeting.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Блоу</w> опоздал на встречу."
                            }

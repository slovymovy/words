{
    "word_family": [
        "sender",
        "resend",
        "unsend",
        "missend",
        "sending"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c69a17de-c760-32fa-a306-dcd5b6a65687",
                    "sense_definition": "To cause a message or object to go from one place to another, or to be delivered to a person.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>send</w> the report to my office by tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>отправьте</w> отчет в мой офис к завтрашнему утру."
                            }
                        },
                        {
                            "text": "She <w>sent</w> a birthday card to her grandmother in the mail.",
                            "target_lang_translations": {
                                "ru": "Она <w>послала</w> бабушке поздравительную открытку по почте."
                            }
                        },
                        {
                            "text": "I will <w>send</w> you a text message when I arrive at the station.",
                            "target_lang_translations": {
                                "ru": "Я <w>отправлю</w> тебе сообщение, когда прибуду на станцию."
                            }
                        }
                    ],
                    "synonyms": [
                        "dispatch",
                        "transmit",
                        "forward",
                        "convey",
                        "mail"
                    ],
                    "antonyms": [
                        "receive",
                        "keep"
                    ],
                    "common_phrases": [
                        "send a message",
                        "send an email",
                        "send a letter",
                        "send money",
                        "send away"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить сообщение или предмет переместиться из одного места в другое или быть доставленным человеку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отправлять",
                                "target_lang_sense_clarification": "наиболее нейтральный и универсальный термин"
                            },
                            {
                                "target_lang_word": "посылать",
                                "target_lang_sense_clarification": "очень часто используется в разговорной речи"
                            },
                            {
                                "target_lang_word": "слать",
                                "target_lang_sense_clarification": "разговорный вариант, часто о письмах или сообщениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e81df6a-bdfb-393b-9e2e-ce2fddde52e0",
                    "sense_definition": "To request someone's presence or assistance by dispatching a messenger or making a call.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "When he collapsed, we immediately <w>sent</w> for an ambulance.",
                            "target_lang_translations": {
                                "ru": "Когда он потерял сознание, мы немедленно <w>послали</w> за скорой помощью."
                            }
                        },
                        {
                            "text": "The manager <w>sent</w> for the employee to discuss the new project.",
                            "target_lang_translations": {
                                "ru": "Менеджер <w>послал</w> за сотрудником, чтобы обсудить новый проект."
                            }
                        }
                    ],
                    "synonyms": [
                        "summon",
                        "call"
                    ],
                    "common_phrases": [
                        "send for help",
                        "send for a doctor",
                        "send for someone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запросить чье-то присутствие или помощь, отправив посыльного или сделав звонок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посылать (за)",
                                "target_lang_sense_clarification": "отправлять кого-то, чтобы привести другого человека или помощь"
                            },
                            {
                                "target_lang_word": "вызывать",
                                "target_lang_sense_clarification": "просить прийти или приехать (например, врача)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26eab8f6-a6f4-3656-b20a-e6f2282f733b",
                    "sense_definition": "To cause someone or something to enter a particular state or condition.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "causative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boring lecture nearly <w>sent</w> the students to sleep.",
                            "target_lang_translations": {
                                "ru": "Скучная лекция чуть не <w>усыпила</w> студентов."
                            }
                        },
                        {
                            "text": "That loud noise is going to <w>send</w> me crazy if it doesn't stop.",
                            "target_lang_translations": {
                                "ru": "Этот громкий шум <w>сведет</w> меня с ума, если не прекратится."
                            }
                        }
                    ],
                    "synonyms": [
                        "drive",
                        "make",
                        "render"
                    ],
                    "common_phrases": [
                        "send crazy",
                        "send mad",
                        "send to sleep",
                        "send into a panic"
                    ],
                    "target_lang_definitions": {
                        "ru": "Привести кого-то или что-то в определенное состояние или положение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доводить",
                                "target_lang_sense_clarification": "приводить к какому-либо состоянию (часто негативному)"
                            },
                            {
                                "target_lang_word": "сводить",
                                "target_lang_sense_clarification": "используется в фразах типа 'сводить с ума'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab5e11e4-1e8e-3637-8b33-2c302f781a48",
                    "sense_definition": "To pursue a course of action with maximum commitment, enthusiasm, or recklessness.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "slang_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I wasn't sure if I could make the jump, but I decided to just <w>send</w> it.",
                            "target_lang_translations": {
                                "ru": "Я не был уверен, смогу ли совершить прыжок, но решил просто <w>рискнуть</w>."
                            }
                        },
                        {
                            "text": "The skiers really <w>sent</w> it down the steep mountain slope.",
                            "target_lang_translations": {
                                "ru": "Лыжники реально <w>выложились</w> на крутом горном склоне."
                            }
                        }
                    ],
                    "synonyms": [
                        "go for it"
                    ],
                    "common_phrases": [
                        "just send it",
                        "full send"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "common in sports and youth culture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действовать решительно, с полной отдачей или безрассудством (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рисковать",
                                "target_lang_sense_clarification": "совершать рискованное действие"
                            },
                            {
                                "target_lang_word": "выкладываться",
                                "target_lang_sense_clarification": "делать что-то на полную мощность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d526f549-f506-3293-bb5d-24fcac272a2d",
                    "sense_definition": "To move to excitement, rapture, or delight.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I don't know what it is about this song, but it really <w>sends</w> me.",
                            "target_lang_translations": {
                                "ru": "Не знаю, что такого в этой песне, но она меня реально <w>заводит</w>."
                            }
                        },
                        {
                            "text": "His incredible performance on stage <w>sent</w> the audience.",
                            "target_lang_translations": {
                                "ru": "Его невероятное выступление на сцене <w>привело в восторг</w> публику."
                            }
                        }
                    ],
                    "synonyms": [
                        "thrill",
                        "delight",
                        "excite",
                        "enrapture"
                    ],
                    "antonyms": [
                        "bore"
                    ],
                    "common_phrases": [
                        "you send me"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mid-20th century slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить в восторг, волновать или восхищать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волновать",
                                "target_lang_sense_clarification": "вызывать сильные эмоции"
                            },
                            {
                                "target_lang_word": "заводить",
                                "target_lang_sense_clarification": "приводить в возбужденное состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ea3ba92-21e1-3adc-87fc-c4d41f9831d9",
                    "sense_definition": "To successfully climb a route without falling or resting on the gear.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She managed to <w>send</w> the difficult route on her first attempt.",
                            "target_lang_translations": {
                                "ru": "Ей удалось <w>пройти</w> сложный маршрут с первой попытки."
                            }
                        },
                        {
                            "text": "After weeks of practice, he finally <w>sent</w> the project.",
                            "target_lang_translations": {
                                "ru": "После недель тренировок он наконец <w>прошел</w> этот проект."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "climbing terminology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Успешно пройти маршрут без срывов (альпинизм)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проходить",
                                "target_lang_sense_clarification": "успешно завершать маршрут"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ae9ef689-0ed5-3cc5-9442-56680f160be0",
                    "sense_definition": "The action of transmitting data, or the button/icon used to initiate this action.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Make sure you check the attachment before you hit <w>send</w>.",
                            "target_lang_translations": {
                                "ru": "Убедитесь, что проверили вложение, прежде чем нажать <w>отправить</w>."
                            }
                        },
                        {
                            "text": "I accidentally clicked <w>send</w> before finishing the email.",
                            "target_lang_translations": {
                                "ru": "Я случайно нажал <w>отправить</w>, не закончив письмо."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hit send",
                        "click send"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "computing context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие отправки данных или кнопка для этого действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отправить",
                                "target_lang_sense_clarification": "надпись на кнопке в интерфейсе"
                            },
                            {
                                "target_lang_word": "отправка",
                                "target_lang_sense_clarification": "процесс передачи данных"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "917d1849-77ab-396e-b5a6-eaf79935fb43",
                    "sense_definition": "A village and civil parish in Surrey, England.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "location",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They visited a small cottage in <w>Send</w> last summer.",
                            "target_lang_translations": {
                                "ru": "Прошлым летом они посетили маленький коттедж в <w>Сенде</w>."
                            }
                        },
                        {
                            "text": "The village of <w>Send</w> is located near Woking.",
                            "target_lang_translations": {
                                "ru": "Деревня <w>Сенд</w> расположена недалеко от Уокинга."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Деревня и гражданский приход в графстве Суррей, Англия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сенд",
                                "target_lang_sense_clarification": "географическое название"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8c1ca288-bac7-30e9-b42b-925aea0471a3",
                    "sense_definition": "Stać się widocznym dla obserwatora lub pojawić się w określonym miejscu (używane z zaimkiem „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "appear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po długim deszczu na niebie w końcu <ukazało> się słońce.",
                            "target_lang_translations": {
                                "ru": "После долгого дождя на небе наконец <показалось> солнце."
                            }
                        },
                        {
                            "text": "W drzwiach wejściowych nagle <ukazał> się tajemniczy gość.",
                            "target_lang_translations": {
                                "ru": "Во входной двери внезапно <появился> таинственный гость."
                            }
                        },
                        {
                            "text": "Jego najnowsza książka <ukaże> się drukiem w przyszłym miesiącu.",
                            "target_lang_translations": {
                                "ru": "Его новейшая книга <выйдет> в печать в следующем месяце."
                            }
                        }
                    ],
                    "synonyms": [
                        "pojawić się",
                        "wyłonić się",
                        "wystąpić"
                    ],
                    "antonyms": [
                        "zniknąć",
                        "przepaść",
                        "schować się"
                    ],
                    "common_phrases": [
                        "ukazać się oczom",
                        "ukazać się drukiem",
                        "nagle się ukazać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать видимым для наблюдателя или появиться в определенном месте (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "показаться",
                                "target_lang_sense_clarification": "стать видимым"
                            },
                            {
                                "target_lang_word": "появиться",
                                "target_lang_sense_clarification": "возникнуть"
                            },
                            {
                                "target_lang_word": "выйти",
                                "target_lang_sense_clarification": "об издании книг или журналов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "638de684-55db-3aed-bdb7-647570d3f913",
                    "sense_definition": "Przedstawić lub zaprezentować kogoś lub coś w określony sposób, czyniąc to widocznym lub zrozumiałym (często w sztuce lub opisie).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "show",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Autor powieści chciał <ukazać> trudne życie mieszkańców wsi.",
                            "target_lang_translations": {
                                "ru": "Автор романа хотел <изобразить> трудную жизнь сельских жителей."
                            }
                        },
                        {
                            "text": "To wydarzenie pozwoliło <ukazać> problem w zupełnie nowym świetle.",
                            "target_lang_translations": {
                                "ru": "Это событие позволило <показать> проблему в совершенно новом свете."
                            }
                        }
                    ],
                    "synonyms": [
                        "przedstawić",
                        "zaprezentować",
                        "uwidocznić",
                        "odmalować"
                    ],
                    "antonyms": [
                        "ukryć",
                        "zataić"
                    ],
                    "common_phrases": [
                        "ukazać prawdę",
                        "ukazać w świetle",
                        "ukazać oblicze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представить или изобразить кого-то или что-то определенным образом (часто в искусстве или описании)."
                    },
                    "translations": {
                        "ru": [
                            {

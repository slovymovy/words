    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8567dd6e-1e2f-36eb-927e-b18e1f406b71",
                    "sense_definition": "Een groep mensen die kijkt en luistert naar een voorstelling, wedstrijd of evenement.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_of_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>publiek</w> klapte enthousiast na het einde van het concert.",
                            "target_lang_translations": {
                                "ru": "<w>Публика</w> восторженно аплодировала после окончания концерта.",
                                "en": "The <w>audience</w> clapped enthusiastically after the end of the concert."
                            }
                        },
                        {
                            "text": "Tijdens de spannende wedstrijd moedigde het <w>publiek</w> de spelers luidkeels aan.",
                            "target_lang_translations": {
                                "ru": "Во время захватывающего матча <w>публика</w> громко подбадривала игроков.",
                                "en": "During the exciting match, the <w>crowd</w> cheered the players on loudly."
                            }
                        }
                    ],
                    "synonyms": [
                        "openbaar",
                        "openbaar"
                    ],
                    "common_phrases": [
                        "groot publiek",
                        "enthousiast publiek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, которые смотрят и слушают представление, матч или другое мероприятие.",
                        "en": "A group of people watching and listening to a performance, match, or event."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "публика",
                                "target_lang_sense_clarification": "зрители, слушатели"
                            },
                            {
                                "target_lang_word": "зрители",
                                "target_lang_sense_clarification": "наблюдатели"
                            },
                            {
                                "target_lang_word": "аудитория",
                                "target_lang_sense_clarification": "слушатели, зал"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "audience",
                                "target_lang_sense_clarification": "people watching a performance"
                            },
                            {
                                "target_lang_word": "crowd",
                                "target_lang_sense_clarification": "large gathering of people"
                            },
                            {
                                "target_lang_word": "public",
                                "target_lang_sense_clarification": "spectators"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60edae66-7236-3080-8665-2a01bad8902f",
                    "sense_definition": "Mensen in het algemeen; bezoekers die niet specifiek zijn uitgenodigd.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_of_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het museum is vanaf volgende week weer geopend voor <w>publiek</w>.",
                            "target_lang_translations": {
                                "ru": "С следующей недели музей снова открыт для <w>публики</w>.",
                                "en": "The museum is open to the <w>public</w> again starting next week."
                            }
                        },
                        {
                            "text": "Dit ingewikkelde boek is niet echt geschikt voor het grote <w>publiek</w>.",
                            "target_lang_translations": {
                                "ru": "Эта сложная книга не очень подходит для широкой <w>публики</w>.",
                                "en": "This complicated book is not really suitable for the general <w>public</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "voor publiek geopend",
                        "het grote publiek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Люди в целом; посетители, которые не были специально приглашены.",
                        "en": "People in general; visitors who are not specifically invited."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "публика",
                                "target_lang_sense_clarification": "люди, общество"
                            },
                            {
                                "target_lang_word": "посетители",
                                "target_lang_sense_clarification": "гости заведения"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "public",
                                "target_lang_sense_clarification": "people in general"
                            },
                            {
                                "target_lang_word": "general public",
                                "target_lang_sense_clarification": "ordinary people"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "70697e6b-1085-30ed-9438-7f07ede8a2b2",
                    "sense_definition": "Voor iedereen toegankelijk en niet privé; openbaar.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "public_nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het park is een <w>publieke</w> ruimte waar iedereen vrij mag wandelen.",
                            "target_lang_translations": {
                                "ru": "Парк — это <w>общественное</w> пространство, где каждый может свободно гулять.",
                                "en": "The park is a <w>public</w> space where everyone can walk freely."
                            }
                        },
                        {
                            "text": "Hij durfde zijn mening niet te geven tijdens de <w>publieke</w> bijeenkomst.",
                            "target_lang_translations": {
                                "ru": "Он не осмелился высказать свое мнение во время <w>публичного</w> собрания.",
                                "en": "He did not dare to give his opinion during the <w>public</w> meeting."
                            }
                        }
                    ],
                    "synonyms": [
                        "openbaar"
                    ],
                    "antonyms": [
                        "privé"
                    ],
                    "common_phrases": [
                        "publieke ruimte",
                        "publiek domein"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доступный для всех, не частный; открытый для посещения.",
                        "en": "Accessible to everyone and not private; public."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "общественный",
                                "target_lang_sense_clarification": "доступный для общества"
                            },
                            {
                                "target_lang_word": "публичный",
                                "target_lang_sense_clarification": "открытый, гласный"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "public",
                                "target_lang_sense_clarification": "open to everyone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4829b9db-d917-372b-aa0e-fc8f1c6dbd68",
                    "sense_definition": "Wat betrekking heeft op de gemeenschap, de staat of de overheid.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "public_nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>publieke</w> omroep zendt dagelijks het nieuws en actualiteiten uit.",
                            "target_lang_translations": {
                                "ru": "<w>Общественное</w> вещание ежедневно транслирует новости и текущие события.",
                                "en": "The <w>public</w> broadcaster broadcasts news and current events daily."
                            }
                        },
                        {
                            "text": "Het is inmiddels een <w>publiek</w> geheim dat de directeur gaat vertrekken.",
                            "target_lang_translations": {
                                "ru": "То, что директор уходит, — уже <w>общеизвестный</w> секрет.",
                                "en": "It is by now an <w>open</w> secret that the director is leaving."
                            }
                        }
                    ],

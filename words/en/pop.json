{
    "word_family": [
        "popcorn",
        "popper"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0e9f284f-0af6-3c80-878c-fad3ed387c0c",
                    "sense_definition": "To cause something to burst with a short, explosive sound.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "bursting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boy <w>popped</w> the balloon with a sharp pin.",
                            "target_lang_translations": {
                                "ru": "Мальчик <w>лопнул</w> шарик острой булавкой."
                            }
                        },
                        {
                            "text": "If you heat the corn, it will <w>pop</w> and become a tasty snack.",
                            "target_lang_translations": {
                                "ru": "Если нагреть кукурузу, она <w>лопнет</w> и превратится в вкусную закуску."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить что-то лопнуть или взорваться с коротким резким звуком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лопнуть",
                                "target_lang_sense_clarification": "заставить лопнуть (разг.)"
                            },
                            {
                                "target_lang_word": "проткнуть",
                                "target_lang_sense_clarification": "о шарике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5606f848-d534-3f66-b40f-b4dde5c8d71b",
                    "sense_definition": "To make a short, sharp, explosive sound.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cork <w>popped</w> loudly as he opened the champagne bottle.",
                            "target_lang_translations": {
                                "ru": "Пробка громко <w>хлопнула</w>, когда он открыл бутылку шампанского."
                            }
                        },
                        {
                            "text": "The wood in the fire crackled and <w>popped</w> as it burned.",
                            "target_lang_translations": {
                                "ru": "Дрова в костре трещали и <w>хлопали</w>, сгорая."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать короткий, резкий, взрывной звук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хлопнуть",
                                "target_lang_sense_clarification": "издать звук"
                            },
                            {
                                "target_lang_word": "трещать",
                                "target_lang_sense_clarification": "о дровах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a324f40e-c1c1-3f15-ab1d-592574bd83f0",
                    "sense_definition": "To put or place something somewhere quickly or casually.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Just <w>pop</w> the leftovers in the fridge for tomorrow.",
                            "target_lang_translations": {
                                "ru": "Просто <w>сунь</w> остатки еды в холодильник на завтра."
                            }
                        },
                        {
                            "text": "She <w>popped</w> a letter into the mailbox on her way to work.",
                            "target_lang_translations": {
                                "ru": "Она <w>бросила</w> письмо в почтовый ящик по дороге на работу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pop it in",
                        "pop something into"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро или небрежно положить что-то куда-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сунуть",
                                "target_lang_sense_clarification": "положить быстро"
                            },
                            {
                                "target_lang_word": "положить",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "закинуть",
                                "target_lang_sense_clarification": "разг."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0cea3ce-49e6-3c6b-b4bf-02b1aa5671ea",
                    "sense_definition": "To go somewhere for a short time, often casually or unexpectedly.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>pop</w> into the store to buy some bread.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>заскочить</w> в магазин, чтобы купить хлеба."
                            }
                        },
                        {
                            "text": "He <w>popped</w> round to his friend's house for a quick chat.",
                            "target_lang_translations": {
                                "ru": "Он <w>заглянул</w> к другу домой поболтать."
                            }
                        }
                    ],
                    "synonyms": [
                        "nip"
                    ],
                    "common_phrases": [
                        "pop in",
                        "pop out",
                        "pop round",
                        "pop over"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пойти куда-то ненадолго, часто неожиданно или по пути."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заскочить",
                                "target_lang_sense_clarification": "зайти ненадолго"
                            },
                            {
                                "target_lang_word": "заглянуть",
                                "target_lang_sense_clarification": "навестить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db80f8d0-2cb5-3c47-8e24-01fcbfbf6110",
                    "sense_definition": "To experience a sudden release of pressure in the ears.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physiological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My ears always <w>pop</w> when the airplane begins to land.",
                            "target_lang_translations": {
                                "ru": "У меня всегда <w>закладывает</w> уши, когда самолет начинает приземляться."
                            }
                        },
                        {
                            "text": "Yawning can help your ears <w>pop</w> when you drive up a high mountain.",
                            "target_lang_translations": {
                                "ru": "Зевота может помочь <w>отложить</w> уши, когда едешь высоко в гору."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ears pop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ощущать резкое изменение давления в ушах (закладывание или откладывание)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закладывать",
                                "target_lang_sense_clarification": "об ушах (ощущение давления)"
                            },
                            {
                                "target_lang_word": "откладывать",
                                "target_lang_sense_clarification": "об ушах (снятие давления)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "646cd207-e4e1-3f31-afce-cf8965b034a0",
                    "sense_definition": "To swallow or consume a pill or tablet.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>pops</w> a vitamin pill every morning with his breakfast.",
                            "target_lang_translations": {
                                "ru": "Он <w>глотает</w> витаминку каждое утро за завтраком."
                            }
                        },
                        {
                            "text": "The doctor warned him not to <w>pop</w> painkillers like candy.",
                            "target_lang_translations": {
                                "ru": "Врач предупредил его не <w>глотать</w> обезболивающие как конфеты."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pop a pill"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Проглотить или принять таблетку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глотать",
                                "target_lang_sense_clarification": "принимать лекарство"
                            },
                            {
                                "target_lang_word": "выпить",
                                "target_lang_sense_clarification": "таблетку"
                            }
                        ]

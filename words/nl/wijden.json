{
    "word_family": [
        "wijding",
        "toewijden",
        "inwijden",
        "verwijden"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "11108f3b-19e3-3d98-a5cf-f6de43d54a00",
                    "sense_definition": "Tijd, aandacht of middelen specifiek voor een bepaald doel of onderwerp gebruiken.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "dedicate_resources",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar <w>wijdde</w> veel aandacht aan de grammatica tijdens de les.",
                            "target_lang_translations": {
                                "en": "The teacher <w>devoted</w> much attention to grammar during the lesson.",
                                "ru": "Учитель <w>уделил</w> много внимания грамматике во время урока."
                            }
                        },
                        {
                            "text": "Zij <w>wijdde</w> haar nieuwe boek aan de geschiedenis van haar familie.",
                            "target_lang_translations": {
                                "en": "She <w>dedicated</w> her new book to the history of her family.",
                                "ru": "Она <w>посвятила</w> свою новую книгу истории своей семьи."
                            }
                        }
                    ],
                    "synonyms": [
                        "besteden",
                        "spenderen",
                        "opdragen"
                    ],
                    "antonyms": [
                        "verwaarlozen"
                    ],
                    "common_phrases": [
                        "aandacht wijden aan",
                        "tijd wijden aan",
                        "een boek wijden aan"
                    ],
                    "target_lang_definitions": {
                        "en": "To use time, effort, or resources for a specific purpose, or to address a creative work to someone.",
                        "ru": "Использовать время, внимание или ресурсы специально для определенной цели или темы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "devote",
                                "target_lang_sense_clarification": "time or attention"
                            },
                            {
                                "target_lang_word": "dedicate",
                                "target_lang_sense_clarification": "resources or a book"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "уделять",
                                "target_lang_sense_clarification": "время, внимание"
                            },
                            {
                                "target_lang_word": "посвящать",
                                "target_lang_sense_clarification": "книгу, труд, ресурсы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3cd98618-1c43-396f-b3ca-dc172e7c4022",
                    "sense_definition": "Zichzelf met volledige inzet en toewijding richten op een taak, roeping of persoon.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "dedicate_self",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na zijn pensioen <w>wijdde</w> hij zich volledig aan het schilderen.",
                            "target_lang_translations": {
                                "en": "After his retirement, he <w>dedicated</w> himself fully to painting.",
                                "ru": "После выхода на пенсию он полностью <w>посвятил</w> себя живописи."
                            }
                        },
                        {
                            "text": "Zij heeft besloten zich te <w>wijden</w> aan de zorg voor hulpbehoevende dieren.",
                            "target_lang_translations": {
                                "en": "She has decided to <w>devote</w> herself to the care of animals in need.",
                                "ru": "Она решила <w>посвятить</w> себя заботе о нуждающихся животных."
                            }
                        }
                    ],
                    "synonyms": [
                        "zich toeleggen op",
                        "zich inzetten voor"
                    ],
                    "common_phrases": [
                        "zich wijden aan",
                        "zijn leven wijden aan"
                    ],
                    "target_lang_definitions": {
                        "en": "To commit oneself fully to a task, calling, or person.",
                        "ru": "Полностью сосредоточиться на задаче, призвании или человеке с полной самоотдачей."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dedicate oneself",
                                "target_lang_sense_clarification": "to a goal or task"
                            },
                            {
                                "target_lang_word": "devote oneself",
                                "target_lang_sense_clarification": "to a calling or cause"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "посвящать себя",
                                "target_lang_sense_clarification": "делу, занятию"
                            },
                            {
                                "target_lang_word": "отдаваться",
                                "target_lang_sense_clarification": "делу целиком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "edfcb1f3-b84a-3f9b-ac54-cdc926a80620",
                    "sense_definition": "Iemand of iets door een plechtige, rituele handeling een heilige status of geestelijke functie geven.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "consecrate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De nieuwe bisschop werd gisteren tijdens een lange ceremonie <w>gewijd</w>.",
                            "target_lang_translations": {
                                "en": "The new bishop was <w>consecrated</w> yesterday during a long ceremony.",
                                "ru": "Новый епископ был <w>рукоположен</w> вчера во время долгой церемонии."
                            }
                        },
                        {
                            "text": "Deze oude kapel is <w>gewijd</w> aan de beschermheilige van de stad.",
                            "target_lang_translations": {
                                "en": "This old chapel is <w>dedicated</w> to the city's patron saint.",
                                "ru": "Эта старая часовня <w>посвящена</w> святому покровителю города."
                            }
                        }
                    ],
                    "synonyms": [
                        "inzegenen",
                        "heiligen",
                        "consacreren"
                    ],
                    "antonyms": [
                        "ontwijden"
                    ],
                    "common_phrases": [
                        "tot priester wijden",
                        "een kerk wijden"
                    ],
                    "target_lang_definitions": {
                        "en": "To formally declare something holy or invest someone with a religious office.",
                        "ru": "Наделить кого-либо или что-либо священным статусом или духовной функцией посредством торжественного ритуального действия."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ordain",
                                "target_lang_sense_clarification": "a priest or minister"
                            },
                            {
                                "target_lang_word": "consecrate",
                                "target_lang_sense_clarification": "a bishop or sacred object"
                            },
                            {
                                "target_lang_word": "dedicate",
                                "target_lang_sense_clarification": "a church or altar"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "рукополагать",
                                "target_lang_sense_clarification": "в духовный сан"
                            },
                            {
                                "target_lang_word": "освящать",
                                "target_lang_sense_clarification": "храм, предметы культа"
                            },
                            {
                                "target_lang_word": "посвящать",
                                "target_lang_sense_clarification": "святому (о храме)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

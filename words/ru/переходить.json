    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "008fbe74-92c4-3a3d-8b95-210ec4879a09",
                    "sense_definition": "Пересекать какое-либо пространство, перемещаться с одной стороны чего-либо на другую.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы всегда <переходим> улицу только на зелёный сигнал светофора.",
                            "target_lang_translations": {
                                "en": "We always <w>cross</w> the street only on the green traffic light."
                            }
                        },
                        {
                            "text": "Туристы осторожно <переходили> реку по старому деревянному мосту.",
                            "target_lang_translations": {
                                "en": "The tourists were carefully <w>crossing</w> the river over the old wooden bridge."
                            }
                        }
                    ],
                    "synonyms": [
                        "пересекать"
                    ],
                    "common_phrases": [
                        "переходить улицу",
                        "переходить дорогу",
                        "переходить через мост",
                        "переходить границу"
                    ],
                    "target_lang_definitions": {
                        "en": "To cross a space or move from one side of something to the other."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cross",
                                "target_lang_sense_clarification": "a street, river, or border"
                            },
                            {
                                "target_lang_word": "go across",
                                "target_lang_sense_clarification": "movement from one side to another"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92233ec5-1fb6-3725-b307-269705b8b106",
                    "sense_definition": "Менять место работы, учёбы или перечисляться из одной группы людей в другую.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "change_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой младший брат в этом году <переходит> в пятый класс.",
                            "target_lang_translations": {
                                "en": "My younger brother is <w>moving</w> to the fifth grade this year."
                            }
                        },
                        {
                            "text": "Он решил уволиться из банка и <переходить> на новую работу в IT-компанию.",
                            "target_lang_translations": {
                                "en": "He decided to quit the bank and <w>switch</w> to a new job at an IT company."
                            }
                        }
                    ],
                    "synonyms": [
                        "переводиться"
                    ],
                    "common_phrases": [
                        "переходить в другой класс",
                        "переходить на новую работу",
                        "переходить на сторону врага"
                    ],
                    "target_lang_definitions": {
                        "en": "To change one's place of work or study, or to move from one group to another."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "move",
                                "target_lang_sense_clarification": "to a new class, level, or job"
                            },
                            {
                                "target_lang_word": "transfer",
                                "target_lang_sense_clarification": "between institutions or departments"
                            },
                            {
                                "target_lang_word": "switch",
                                "target_lang_sense_clarification": "jobs or sides"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53150f55-24b8-374d-bd24-72cbc69cb188",
                    "sense_definition": "Заканчивать одно действие и начинать другое; приступать к обсуждению следующего вопроса.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "progression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Давайте не будем терять время и сразу <переходим> к делу.",
                            "target_lang_translations": {
                                "en": "Let's not waste time and <w>get</w> straight to business."
                            }
                        },
                        {
                            "text": "После короткого вступления лектор <переходит> к основной теме занятия.",
                            "target_lang_translations": {
                                "en": "After a short introduction, the lecturer <w>moves on</w> to the main topic of the lesson."
                            }
                        }
                    ],
                    "synonyms": [
                        "приступать"
                    ],
                    "common_phrases": [
                        "переходить к делу",
                        "переходить к вопросу",
                        "переходить к следующей теме"
                    ],
                    "target_lang_definitions": {
                        "en": "To finish one action and start another; to proceed to the next topic."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "move on",
                                "target_lang_sense_clarification": "to the next topic or stage"
                            },
                            {
                                "target_lang_word": "proceed",
                                "target_lang_sense_clarification": "formal; to the next item"
                            },
                            {
                                "target_lang_word": "get",
                                "target_lang_sense_clarification": "start doing something (e.g., get to business)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2708f8bb-847c-3899-a169-99f9fd1c76c1",
                    "sense_definition": "Изменяться, превращаться в другое состояние или качество.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "transformation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "При кипении вода <переходит> в пар.",
                            "target_lang_translations": {
                                "en": "When boiling, water <w>turns</w> into steam."
                            }
                        },
                        {
                            "text": "Обычный спор быстро <переходил> в серьёзный конфликт.",
                            "target_lang_translations": {
                                "en": "A normal argument quickly <w>turned</w> into a serious conflict."
                            }
                        }
                    ],
                    "synonyms": [
                        "превращаться",
                        "становиться"
                    ],
                    "common_phrases": [
                        "переходить в пар",
                        "переходить в привычку",
                        "переходить в наступление"
                    ],
                    "target_lang_definitions": {
                        "en": "To change or turn into a different state or quality."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn",
                                "target_lang_sense_clarification": "into something else"
                            },
                            {
                                "target_lang_word": "change",
                                "target_lang_sense_clarification": "state or quality"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f8e8afa-9377-33c4-a151-769ff32bbea3",
                    "sense_definition": "Становиться чьей-либо собственностью, передаваться от одного владельца к другому.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот старинный дом <переходил> по наследству от отца к сыну на протяжении веков.",
                            "target_lang_translations": {
                                "en": "This old house <w>passed</w> down from father to son for centuries."
                            }
                        },
                        {
                            "text": "Инициатива в матче постоянно <переходила> от одной команды к другой.",
                            "target_lang_translations": {
                                "en": "The initiative in the match constantly <w>passed</w> from one team to the other."
                            }
                        }
                    ],
                    "synonyms": [
                        "передаваться",
                        "доставаться"
                    ],
                    "common_phrases": [
                        "переходить по наследству",
                        "переходить из рук в руки",
                        "переходить в собственность"
                    ],
                    "target_lang_definitions": {
                        "en": "To become someone's property or pass from one owner to another."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pass",
                                "target_lang_sense_clarification": "ownership or inheritance"
                            },
                            {
                                "target_lang_word": "change hands",
                                "target_lang_sense_clarification": "ownership"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea092e70-3522-350a-91a1-9b8ddcde9c35",
                    "sense_definition": "Выходить за пределы допустимого, нарушать какую-либо меру или норму.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "limits",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его грубое поведение начинает <переходить> все границы приличия.",
                            "target_lang_translations": {
                                "en": "His rude behavior is starting to <w>cross</w> all boundaries of decency."
                            }
                        },
                        {
                            "text": "Ты не должен <переходить> черту в разговоре со старшими.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>cross</w> the line when talking to elders."
                            }
                        }
                    ],
                    "synonyms": [
                        "превышать"
                    ],
                    "common_phrases": [
                        "переходить границы",
                        "переходить черту",
                        "переходить на личности"
                    ],
                    "target_lang_definitions": {
                        "en": "To go beyond acceptable limits or violate a norm."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cross",
                                "target_lang_sense_clarification": "a line or boundary"
                            },
                            {
                                "target_lang_word": "overstep",
                                "target_lang_sense_clarification": "limits or authority"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a60fb4b5-4f02-3c6f-b501-faacb398fda9",
                    "sense_definition": "Вынашивать ребёнка дольше положенного медицинского срока.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "pregnancy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врачи сказали, что она <переходила> срок на две недели, поэтому нужна госпитализация.",
                            "target_lang_translations": {
                                "en": "The doctors said she <w>went overdue</w> by two weeks, so hospitalization is needed."
                            }
                        },
                        {
                            "text": "Многие женщины боятся <переходить> беременность из-за риска для ребёнка.",
                            "target_lang_translations": {
                                "en": "Many women are afraid of <w>going overdue</w> due to the risk to the baby."
                            }
                        }
                    ],
                    "common_phrases": [
                        "переходить срок"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in context of pregnancy"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To carry a child longer than the standard medical term."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go overdue",
                                "target_lang_sense_clarification": "pregnancy"
                            },
                            {
                                "target_lang_word": "carry past term",
                                "target_lang_sense_clarification": "pregnancy"
                            }
                        ]
                    }
                }
            ]
        }
    ]

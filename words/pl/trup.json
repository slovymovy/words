                            },
                            {
                                "target_lang_word": "мертвец",
                                "target_lang_sense_clarification": "умерший человек"
                            },
                            {
                                "target_lang_word": "покойник",
                                "target_lang_sense_clarification": "умерший, покойный человек"
                            }
                        ]

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cacfde5e-934f-3ca7-9f29-db250dfce6f3",
                    "sense_definition": "Очень известный, прославленный, вошедший в историю благодаря своим выдающимся качествам или поступкам.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "famous",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот <w>легендарный</w> музыкант изменил историю рока своими песнями.",
                            "target_lang_translations": {
                                "en": "This <w>legendary</w> musician changed the history of rock with his songs."
                            }
                        },
                        {
                            "text": "Мы посмотрели <w>легендарный</w> фильм, который получил множество наград.",
                            "target_lang_translations": {
                                "en": "We watched a <w>legendary</w> film that received many awards."
                            }
                        }
                    ],
                    "synonyms": [
                        "прославленный",
                        "знаменитый",
                        "великий",
                        "выдающийся"
                    ],
                    "antonyms": [
                        "неизвестный",
                        "безвестный",
                        "забытый"
                    ],
                    "common_phrases": [
                        "легендарная личность",
                        "легендарный фильм",
                        "легендарный успех",
                        "легендарный герой"
                    ],
                    "target_lang_definitions": {
                        "en": "Very famous, renowned, having entered history thanks to outstanding qualities or deeds."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "legendary",
                                "target_lang_sense_clarification": "very famous"
                            },
                            {
                                "target_lang_word": "renowned",
                                "target_lang_sense_clarification": "celebrated"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52621722-3029-3805-85ff-b4786afad60e",
                    "sense_definition": "Относящийся к легендам, мифический; существующий только в преданиях и сказаниях.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mythical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Король Артур — это <w>легендарный</w> правитель древней Британии.",
                            "target_lang_translations": {
                                "en": "King Arthur is a <w>legendary</w> ruler of ancient Britain."
                            }
                        },
                        {
                            "text": "Туристы мечтали увидеть <w>легендарное</w> озеро, где, по слухам, живёт чудовище.",
                            "target_lang_translations": {
                                "en": "Tourists dreamed of seeing the <w>legendary</w> lake where, according to rumors, a monster lives."
                            }
                        }
                    ],
                    "synonyms": [
                        "мифический",
                        "сказочный",
                        "былинный"
                    ],
                    "antonyms": [
                        "реальный",
                        "исторический",
                        "фактический"
                    ],
                    "common_phrases": [
                        "легендарные времена",
                        "легендарное чудовище",
                        "легендарный меч"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to legends, mythical; existing only in traditions and tales."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "legendary",
                                "target_lang_sense_clarification": "of legends"
                            },
                            {
                                "target_lang_word": "mythical",
                                "target_lang_sense_clarification": "fictitious"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fbd0bfce-0c44-37e6-b0b2-50db595321f9",
                    "sense_definition": "Неправдоподобный, вымышленный или невероятный по своим масштабам (часто используется иронично или для усиления).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "incredible",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его <w>легендарная</w> лень мешает ему найти хорошую работу.",
                            "target_lang_translations": {
                                "en": "His <w>legendary</w> laziness prevents him from finding a good job."
                            }
                        },
                        {
                            "text": "О богатстве этого купца ходили совершенно <w>легендарные</w> слухи.",
                            "target_lang_translations": {
                                "en": "Absolutely <w>legendary</w> rumors circulated about this merchant's wealth."
                            }
                        }
                    ],
                    "synonyms": [
                        "невероятный",
                        "фантастический",
                        "неправдоподобный"
                    ],
                    "antonyms": [
                        "правдоподобный",
                        "обычный",
                        "достоверный"
                    ],
                    "common_phrases": [
                        "легендарная глупость",
                        "легендарная лень",
                        "легендарная жадность"
                    ],
                    "target_lang_definitions": {
                        "en": "Implausible, fictional, or incredible in scale (often used ironically or for emphasis)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "legendary",
                                "target_lang_sense_clarification": "incredible/extreme"
                            },
                            {
                                "target_lang_word": "incredible",
                                "target_lang_sense_clarification": "unbelievable"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d3d72692-2771-3223-9b2f-82fd912e6477",
                    "sense_definition": "Een gevoel van groot plezier, vreugde of tevredenheid.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was een waar <w>genot</w> om naar de prachtige muziek te luisteren.",
                            "target_lang_translations": {
                                "ru": "Было истинным <w>наслаждением</w> слушать эту прекрасную музыку."
                            }
                        },
                        {
                            "text": "Zij at haar ijsje met zichtbaar <w>genot</w> op in de zon.",
                            "target_lang_translations": {
                                "ru": "Она ела своё мороженое с видимым <w>удовольствием</w> на солнце."
                            }
                        }
                    ],
                    "synonyms": [
                        "genieting",
                        "geneugte"
                    ],
                    "common_phrases": [
                        "puur genot",
                        "met veel genot",
                        "tot mijn genot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство большого удовольствия, радости или удовлетворения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наслаждение",
                                "target_lang_sense_clarification": "высшая степень удовольствия"
                            },
                            {
                                "target_lang_word": "удовольствие",
                                "target_lang_sense_clarification": "чувство радости и довольства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "387e72c8-f585-336e-a575-5803cbf955a3",
                    "sense_definition": "Het recht op gebruik of het voordeel dat men ergens van heeft, vaak in juridische zin.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal_use",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De huurder heeft recht op het ongestoord <w>genot</w> van de woning.",
                            "target_lang_translations": {
                                "ru": "Арендатор имеет право на беспрепятственное <w>пользование</w> жильём."
                            }
                        },
                        {
                            "text": "Zij hebben het volledig <w>genot</w> van hun eigendommen tijdens hun leven.",
                            "target_lang_translations": {
                                "ru": "Они имеют полное право <w>пользования</w> своим имуществом при жизни."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het genot van",
                        "ongestoord genot"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Право пользования или выгода, получаемая от чего-либо, часто в юридическом смысле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пользование",
                                "target_lang_sense_clarification": "юридическое право использования имущества"
                            }
                        ]
                    }
                }
            ]
        }
    ]

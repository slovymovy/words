    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "bee45776-2d37-3aee-bf59-0bf20a3797bd",
                    "sense_definition": "Gelegen aan de kant die tegenover links ligt; de zijde van het lichaam die voor de meeste mensen de dominante kant is.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij schrijft en eet met zijn <w>rechter</w> hand.",
                            "target_lang_translations": {
                                "en": "He writes and eats with his <w>right</w> hand.",
                                "ru": "Он пишет и ест <w>правой</w> рукой."
                            }
                        },
                        {
                            "text": "Aan de <w>rechter</w> kant van de straat staat een groot gebouw.",
                            "target_lang_translations": {
                                "en": "On the <w>right</w> side of the street stands a large building.",
                                "ru": "На <w>правой</w> стороне улицы стоит большое здание."
                            }
                        }
                    ],
                    "synonyms": [
                        "rechts"
                    ],
                    "antonyms": [
                        "linker",
                        "links"
                    ],
                    "common_phrases": [
                        "rechter hand",
                        "rechter kant",
                        "rechter been"
                    ],
                    "target_lang_definitions": {
                        "en": "Located on the side opposite to the left; the side of the body that is dominant for most people.",
                        "ru": "Расположенный на стороне, противоположной левой; сторона тела, которая является доминирующей для большинства людей."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "right",
                                "target_lang_sense_clarification": "opposite of left"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "правый",
                                "target_lang_sense_clarification": "сторона"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5a558dd-6221-3e49-a5bd-534321164a67",
                    "sense_definition": "De vergrotende trap van <w>recht</w>; met minder bochten of afwijkingen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "shape_comparison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun je dat schilderij iets <w>rechter</w> hangen?",
                            "target_lang_translations": {
                                "en": "Can you hang that painting a little <w>straighter</w>?",
                                "ru": "Ты можешь повесить эту картину немного <w>ровнее</w>?"
                            }
                        },
                        {
                            "text": "Deze lijn is een stuk <w>rechter</w> dan die andere.",
                            "target_lang_translations": {
                                "en": "This line is a lot <w>straighter</w> than the other one.",
                                "ru": "Эта линия намного <w>прямее</w>, чем та другая."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "comparative"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The comparative form of 'straight'; having fewer bends or deviations.",
                        "ru": "Сравнительная степень от «прямой»; с меньшим количеством изгибов или отклонений."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "straighter",
                                "target_lang_sense_clarification": "comparative of straight"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "прямее",
                                "target_lang_sense_clarification": "сравнительная степень"
                            },
                            {
                                "target_lang_word": "ровнее",
                                "target_lang_sense_clarification": "сравнительная степень"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f84bbf92-f5a6-338b-9266-14d1cd884a17",
                    "sense_definition": "Een openbaar ambtenaar die rechtspreekt en vonnissen velt in een rechtbank.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "profession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>rechter</w> deed gisteren uitspraak in de lastige zaak.",
                            "target_lang_translations": {
                                "en": "The <w>judge</w> gave a verdict in the difficult case yesterday.",
                                "ru": "Вчера <w>судья</w> вынес решение по этому сложному делу."
                            }
                        },
                        {
                            "text": "Zij besloten naar de <w>rechter</w> te stappen om het conflict op te lossen.",
                            "target_lang_translations": {
                                "en": "They decided to go to the <w>judge</w> to resolve the conflict.",
                                "ru": "Они решили обратиться к <w>судье</w>, чтобы разрешить конфликт."
                            }
                        }
                    ],
                    "common_phrases": [
                        "naar de rechter stappen",
                        "voor de rechter verschijnen",
                        "de rijdende rechter"
                    ],
                    "target_lang_definitions": {
                        "en": "A public official who administers justice and passes sentences in a court of law.",
                        "ru": "Государственное должностное лицо, которое вершит правосудие и выносит приговоры в суде."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "judge",
                                "target_lang_sense_clarification": "legal official"
                            },
                            {
                                "target_lang_word": "justice",
                                "target_lang_sense_clarification": "magistrate or high-ranking judge"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "судья",
                                "target_lang_sense_clarification": "в суде"
                            }
                        ]
                    }
                }
            ]
        }
    ]

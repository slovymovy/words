    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "51da6f03-d50e-3464-b9a1-cf71da917cc5",
                    "sense_definition": "A large area of land covered with trees and plants, usually larger than a wood.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "nature_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We went for a long walk in the <w>forest</w>.",
                            "target_lang_translations": {
                                "ru": "Мы отправились на долгую прогулку в <w>лес</w>."
                            }
                        },
                        {
                            "text": "Many wild animals live deep in the <w>forest</w>.",
                            "target_lang_translations": {
                                "ru": "Многие дикие животные живут глубоко в <w>лесу</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "woods",
                        "woodland"
                    ],
                    "common_phrases": [
                        "rain forest",
                        "dense forest",
                        "pine forest",
                        "tropical forest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая территория, покрытая деревьями и растениями, обычно больше, чем роща."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лес",
                                "target_lang_sense_clarification": "пространство, заросшее деревьями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba9ed954-cb11-30c2-9a25-11f8bcdf4116",
                    "sense_definition": "A dark green color resembling the trees in a forest.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She chose a deep <w>forest</w> for the living room walls.",
                            "target_lang_translations": {
                                "ru": "Она выбрала насыщенный <w>тёмно-зелёный цвет</w> для стен гостиной."
                            }
                        },
                        {
                            "text": "The team's uniform is <w>forest</w> and white.",
                            "target_lang_translations": {
                                "ru": "Форма команды — <w>тёмно-зелёная</w> с белым."
                            }
                        }
                    ],
                    "common_phrases": [
                        "forest green"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тёмно-зелёный цвет, напоминающий цвет деревьев в лесу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тёмно-зелёный цвет",
                                "target_lang_sense_clarification": "оттенок"
                            },
                            {
                                "target_lang_word": "цвет лесной зелени",
                                "target_lang_sense_clarification": "оттенок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e13e10dd-c481-3b9c-b898-a103539e8178",
                    "sense_definition": "A large number of vertical objects standing close together.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher looked out at a <w>forest</w> of hands raised in the air.",
                            "target_lang_translations": {
                                "ru": "Учитель посмотрел на <w>лес</w> рук, поднятых в воздух."
                            }
                        },
                        {
                            "text": "He had to navigate through a <w>forest</w> of construction cranes.",
                            "target_lang_translations": {
                                "ru": "Ему пришлось пробираться сквозь <w>лес</w> строительных кранов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "forest of hands"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое количество вертикальных объектов, стоящих близко друг к другу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лес",
                                "target_lang_sense_clarification": "множество предметов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf598e4a-8764-3d4d-9c87-cfe88e38a9af",
                    "sense_definition": "An area of land legally set aside in England for royal hunting, often including trees and open land.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "historical_land",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king's laws were strictly enforced within the royal <w>forest</w>.",
                            "target_lang_translations": {
                                "ru": "Королевские законы строго соблюдались в пределах королевского <w>леса</w>."
                            }
                        },
                        {
                            "text": "Peasants were forbidden from hunting deer in the <w>forest</w>.",
                            "target_lang_translations": {
                                "ru": "Крестьянам было запрещено охотиться на оленей в <w>лесу</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "historical England"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Участок земли, законодательно выделенный в Англии для королевской охоты, часто включающий деревья и открытые пространства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "королевский лес",
                                "target_lang_sense_clarification": "историческое понятие"
                            },
                            {
                                "target_lang_word": "охотничьи угодья",
                                "target_lang_sense_clarification": "место для охоты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28a45a81-6e77-302f-a4bd-398e909b22a7",
                    "sense_definition": "A graph that contains no cycles and consists of a collection of trees.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "math_graph",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In graph theory, a <w>forest</w> is formed by a disjoint union of trees.",
                            "target_lang_translations": {
                                "ru": "В теории графов <w>лес</w> образуется несвязным объединением деревьев."
                            }
                        },
                        {
                            "text": "We analyzed the properties of the random <w>forest</w> algorithm.",
                            "target_lang_translations": {
                                "ru": "Мы проанализировали свойства алгоритма случайного <w>леса</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Граф, не содержащий циклов и состоящий из набора деревьев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лес",
                                "target_lang_sense_clarification": "теория графов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31b704ee-6c91-3122-a985-8e88ab087c00",
                    "sense_definition": "A collection of one or more domain trees that share a common schema.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "computing_domain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The administrator configured the trust relationship between the two <w>forests</w>.",
                            "target_lang_translations": {
                                "ru": "Администратор настроил доверительные отношения между двумя <w>лесами</w>."
                            }
                        },
                        {
                            "text": "A <w>forest</w> represents the top-level container in the directory structure.",
                            "target_lang_translations": {
                                "ru": "<w>Лес</w> представляет собой контейнер верхнего уровня в структуре каталогов."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность одного или нескольких деревьев доменов, имеющих общую схему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лес",
                                "target_lang_sense_clarification": "информатика"
                            }
                        ]
                    }
                }
            ]
        },
        {

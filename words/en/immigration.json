    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b63686eb-cc65-374f-8884-2bfb4632405b",
                    "sense_definition": "The place or process at a border, airport, or port where officials check the documents of people entering a country.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "border_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It took us nearly an hour to get through <w>immigration</w> after our flight landed.",
                            "target_lang_translations": {
                                "ru": "У нас ушел почти час на то, чтобы пройти <w>паспортный контроль</w> после приземления нашего рейса."
                            }
                        },
                        {
                            "text": "The <w>immigration</w> officer asked to see my passport and return ticket.",
                            "target_lang_translations": {
                                "ru": "Сотрудник <w>иммиграционного контроля</w> попросил предъявить паспорт и обратный билет."
                            }
                        }
                    ],
                    "synonyms": [
                        "passport control",
                        "border control"
                    ],
                    "common_phrases": [
                        "through immigration",
                        "immigration officer",
                        "immigration control",
                        "clear immigration"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место или процедура в аэропорту, порту или на границе, где официальные лица проверяют документы людей, въезжающих в страну."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паспортный контроль",
                                "target_lang_sense_clarification": "пункт проверки документов"
                            },
                            {
                                "target_lang_word": "иммиграционный контроль",
                                "target_lang_sense_clarification": "процедура проверки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4b0660e-3ae2-34f5-bfdb-4d07b8c71c71",
                    "sense_definition": "The act of coming to live permanently in a foreign country.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "human_migration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government is debating new laws to regulate <w>immigration</w> more strictly.",
                            "target_lang_translations": {
                                "ru": "Правительство обсуждает новые законы, чтобы более строго регулировать <w>иммиграцию</w>."
                            }
                        },
                        {
                            "text": "Mass <w>immigration</w> has significantly influenced the culture of the city.",
                            "target_lang_translations": {
                                "ru": "Массовая <w>иммиграция</w> значительно повлияла на культуру города."
                            }
                        }
                    ],
                    "antonyms": [
                        "emigration"
                    ],
                    "common_phrases": [
                        "illegal immigration",
                        "immigration policy",
                        "mass immigration",
                        "immigration laws"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс въезда в чужую страну для постоянного проживания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иммиграция",
                                "target_lang_sense_clarification": "въезд в страну"
                            },
                            {
                                "target_lang_word": "переселение",
                                "target_lang_sense_clarification": "смена места жительства"
                            }
                        ]
                    }
                }
            ]
        }
    ]

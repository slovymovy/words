                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "used in card games context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A declaration in poker to pass the turn without betting."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "check",
                                "target_lang_sense_clarification": "poker move"
                            }
                        ]
                    }
                },

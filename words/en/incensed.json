    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b94bd1ad-8f6e-349e-9487-8a90f7148a73",
                    "sense_definition": "Extremely angry; enraged.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "anger",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The customers were <w>incensed</w> by the sudden increase in prices.",
                            "target_lang_translations": {
                                "ru": "Клиенты были <w>разъярены</w> внезапным повышением цен."
                            }
                        },
                        {
                            "text": "She became <w>incensed</w> at the way she had been treated by the staff.",
                            "target_lang_translations": {
                                "ru": "Она была <w>взбешена</w> тем, как с ней обошёлся персонал."
                            }
                        }
                    ],
                    "synonyms": [
                        "enraged",
                        "infuriated",
                        "furious",
                        "irate",
                        "livid"
                    ],
                    "antonyms": [
                        "calm",
                        "pleased",
                        "delighted"
                    ],
                    "common_phrases": [
                        "incensed by",
                        "incensed at",
                        "become incensed",
                        "absolutely incensed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чрезвычайно сердитый; в состоянии ярости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разъярённый",
                                "target_lang_sense_clarification": "охваченный яростью"
                            },
                            {
                                "target_lang_word": "взбешённый",
                                "target_lang_sense_clarification": "крайне раздражённый, доведённый до бешенства"
                            },
                            {
                                "target_lang_word": "рассерженный",
                                "target_lang_sense_clarification": "испытывающий гнев"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6536a982-f1db-304f-b644-9c404b70cf23",
                    "sense_definition": "Depicted with fire issuing from the mouth and ears.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The family coat of arms features a lion <w>incensed</w> to symbolize ferocity.",
                            "target_lang_translations": {
                                "ru": "На фамильном гербе изображён лев, <w>извергающий пламя</w>, что символизирует свирепость."
                            }
                        },
                        {
                            "text": "In the design, the panther is <w>incensed</w> proper with flames visible.",
                            "target_lang_translations": {
                                "ru": "В этом дизайне пантера изображена <w>с пламенем</w> естественного цвета."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Изображённый с огнём, вырывающимся из пасти и ушей (в геральдике)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "извергающий пламя",
                                "target_lang_sense_clarification": "геральдический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {

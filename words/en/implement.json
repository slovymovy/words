            "senses": [
                {
                    "sense_id": "c81d74e5-942e-3b05-a823-d64a11169b64",
                    "sense_definition": "To put a plan, decision, or agreement into effect.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government promised to <w>implement</w> the new safety regulations immediately.",
                            "target_lang_translations": {
                                "ru": "Правительство пообещало немедленно <w>внедрить</w> новые правила техники безопасности."
                            }
                        },
                        {
                            "text": "It will take time and money to <w>implement</w> a plan of this magnitude.",
                            "target_lang_translations": {
                                "ru": "Потребуются время и деньги, чтобы <w>реализовать</w> план такого масштаба."
                            }
                        }
                    ],
                    "synonyms": [
                        "execute"
                    ],
                    "common_phrases": [

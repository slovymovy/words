    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6e4f4c63-57c5-34b0-ac58-a59bb53ae744",
                    "sense_definition": "Процесс выбора кого-либо голосованием для выполнения каких-либо обязанностей или занятия должности.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его <избрание> на пост директора поддержали все сотрудники компании.",
                            "target_lang_translations": {
                                "en": "All company employees supported his <election> to the post of director."
                            }
                        },
                        {
                            "text": "Порядок <избрания> делегатов был утверждён на общем собрании.",
                            "target_lang_translations": {
                                "en": "The procedure for the <election> of delegates was approved at the general meeting."
                            }
                        }
                    ],
                    "synonyms": [
                        "выбор"
                    ],
                    "antonyms": [
                        "смещение",
                        "отстранение"
                    ],
                    "common_phrases": [
                        "избрание президента",
                        "порядок избрания",
                        "день избрания",
                        "повторное избрание"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of choosing someone by voting to perform duties or hold a position."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "election",
                                "target_lang_sense_clarification": "voting process"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c0d601a-5ca9-3c40-95da-8258dc82a16f",
                    "sense_definition": "Выбор, предпочтение, отдаваемое кому-либо или чему-либо (обычно о жизненном пути, профессии или цели).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Правильное <избрание> будущей профессии определяет дальнейшую судьбу человека.",
                            "target_lang_translations": {
                                "en": "The correct <choice> of a future profession determines a person's future destiny."
                            }
                        },
                        {
                            "text": "Он никогда не сомневался в <избрании> своего творческого пути.",
                            "target_lang_translations": {
                                "en": "He never doubted the <choice> of his creative path."
                            }
                        }
                    ],
                    "synonyms": [
                        "выбор"
                    ],
                    "common_phrases": [
                        "избрание пути",
                        "избрание профессии",
                        "избрание цели"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of choosing or preferring someone or something (usually regarding a life path, profession, or goal)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "choice",
                                "target_lang_sense_clarification": "decision or preference"
                            },
                            {
                                "target_lang_word": "selection",
                                "target_lang_sense_clarification": "act of choosing"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
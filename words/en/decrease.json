{
    "word_family": [
        "decrement"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "93ae5fc7-e491-38d8-b23e-28e32e038d28",
                    "sense_definition": "To become smaller in size, amount, number, or intensity.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "general_reduction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The quality of our products has <w>decreased</w> since the main designer left the company.",
                            "target_lang_translations": {
                                "ru": "Качество нашей продукции <w>снизилось</w> с тех пор, как главный дизайнер покинул компанию."
                            }
                        },
                        {
                            "text": "Temperatures will <w>decrease</w> significantly during the night as the cold front moves in.",
                            "target_lang_translations": {
                                "ru": "Температура значительно <w>понизится</w> ночью, когда придет холодный фронт."
                            }
                        }
                    ],
                    "synonyms": [
                        "drop",
                        "fall",
                        "go down",
                        "decline",
                        "diminish",
                        "shrink",
                        "plummet"
                    ],
                    "antonyms": [
                        "increase",
                        "rise",
                        "grow",
                        "go up",
                        "soar",
                        "shoot up"
                    ],
                    "common_phrases": [
                        "decrease rapidly",
                        "decrease gradually",
                        "decrease in size",
                        "decrease in number"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться меньше по размеру, количеству, числу или интенсивности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уменьшаться",
                                "target_lang_sense_clarification": "становиться меньше (общий смысл)"
                            },
                            {
                                "target_lang_word": "снижаться",
                                "target_lang_sense_clarification": "становиться ниже по уровню, количеству или качеству"
                            },
                            {
                                "target_lang_word": "падать",
                                "target_lang_sense_clarification": "резко уменьшаться (о показателях, температуре)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c3b6e2a-6072-3e0f-a261-f30a13511c6c",
                    "sense_definition": "To make something smaller in size, amount, number, or intensity.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "general_reduction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Let's <w>decrease</w> the volume a little so we can hear each other talking.",
                            "target_lang_translations": {
                                "ru": "Давай немного <w>уменьшим</w> громкость, чтобы мы могли слышать друг друга."
                            }
                        },
                        {
                            "text": "Regular exercise and a healthy diet can <w>decrease</w> the risk of heart disease.",
                            "target_lang_translations": {
                                "ru": "Регулярные упражнения и здоровая диета могут <w>снизить</w> риск сердечных заболеваний."
                            }
                        }
                    ],
                    "synonyms": [
                        "reduce",
                        "lower",
                        "cut",
                        "lessen",
                        "diminish"
                    ],
                    "antonyms": [
                        "increase",
                        "raise"
                    ],
                    "common_phrases": [
                        "decrease the risk",
                        "decrease the amount",
                        "decrease the volume",
                        "decrease the speed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-либо меньше по размеру, количеству, числу или интенсивности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уменьшать",
                                "target_lang_sense_clarification": "делать меньше (общий смысл)"
                            },
                            {
                                "target_lang_word": "снижать",
                                "target_lang_sense_clarification": "делать ниже (о цене, скорости, рисках, громкости)"
                            },
                            {
                                "target_lang_word": "сокращать",
                                "target_lang_sense_clarification": "уменьшать количество или объем"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a30b3972-089e-30e1-a765-204d35d3afcf",
                    "sense_definition": "The process of becoming smaller, or the amount by which something is reduced.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "general_reduction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After six years of constant growth, the company reported a slight <w>decrease</w> in sales last year.",
                            "target_lang_translations": {
                                "ru": "После шести лет постоянного роста компания сообщила о незначительном <w>снижении</w> продаж в прошлом году."
                            }
                        },
                        {
                            "text": "There has been a steady <w>decrease</w> in the number of visitors to the museum recently.",
                            "target_lang_translations": {
                                "ru": "В последнее время наблюдается устойчивое <w>уменьшение</w> числа посетителей музея."
                            }
                        }
                    ],
                    "synonyms": [
                        "reduction",
                        "drop",
                        "decline",
                        "fall",
                        "cut",
                        "loss"
                    ],
                    "antonyms": [
                        "increase",
                        "rise",
                        "gain"
                    ],
                    "common_phrases": [
                        "decrease in",
                        "slight decrease",
                        "significant decrease",
                        "sharp decrease"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс уменьшения или величина, на которую что-либо уменьшено."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "снижение",
                                "target_lang_sense_clarification": "процесс перехода на более низкий уровень (продаж, показателей)"
                            },
                            {
                                "target_lang_word": "уменьшение",
                                "target_lang_sense_clarification": "общий термин для обозначения процесса"
                            },
                            {
                                "target_lang_word": "спад",
                                "target_lang_sense_clarification": "ухудшение или уменьшение активности (в экономике, торговле)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "adf5a522-4256-3e0f-a0e9-e677700f9e54",
                    "sense_definition": "A reduction in the number of stitches in knitting to shape the fabric.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to make a <w>decrease</w> at the end of this row to properly shape the sleeve.",
                            "target_lang_translations": {
                                "ru": "Вам нужно сделать <w>убавку</w> в конце этого ряда, чтобы правильно сформировать рукав."
                            }
                        },
                        {
                            "text": "The knitting pattern calls for a <w>decrease</w> every four stitches to create the neckline.",
                            "target_lang_translations": {
                                "ru": "Схема вязания требует делать <w>убавление</w> каждые четыре петли для создания горловины."
                            }
                        }
                    ],
                    "synonyms": [
                        "reduction"
                    ],
                    "antonyms": [
                        "increase"
                    ],
                    "common_phrases": [
                        "work a decrease"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшение количества петель при вязании для придания формы изделию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убавка",
                                "target_lang_sense_clarification": "термин в вязании"
                            },
                            {
                                "target_lang_word": "убавление",
                                "target_lang_sense_clarification": "процесс сокращения петель"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
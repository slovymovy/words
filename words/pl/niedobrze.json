{
    "word_family": [
        "dobry",
        "niedobry",
        "dobro",
        "dobroć"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c8dc6f37-5fbd-3da6-a27a-cfbd5513436a",
                    "sense_definition": "W sposób niepomyślny, niewłaściwy lub szkodliwy; ocena sytuacji jako negatywnej.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "general_negative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To <w>niedobrze</w>, że spóźniłeś się na ostatni autobus do domu.",
                            "target_lang_translations": {
                                "ru": "Это <w>нехорошо</w>, что ты опоздал на последний автобус домой."
                            }
                        },
                        {
                            "text": "Sprawy w firmie idą <w>niedobrze</w> i musimy zmienić plany.",
                            "target_lang_translations": {
                                "ru": "Дела в фирме идут <w>плохо</w>, и нам нужно изменить планы."
                            }
                        }
                    ],
                    "synonyms": [
                        "źle"
                    ],
                    "antonyms": [
                        "dobrze"
                    ],
                    "common_phrases": [
                        "to niedobrze",
                        "bardzo niedobrze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неблагоприятно, неправильно или вредно; негативная оценка ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плохо",
                                "target_lang_sense_clarification": "общая негативная оценка"
                            },
                            {
                                "target_lang_word": "нехорошо",
                                "target_lang_sense_clarification": "нежелательно, неправильно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7eaa6e7-c0ce-388d-8fd8-59c649b7bcbc",
                    "sense_definition": "O złym samopoczuciu fizycznym, często oznaczające mdłości lub osłabienie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "health",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zjadłem za dużo słodyczy i teraz jest mi <w>niedobrze</w>.",
                            "target_lang_translations": {
                                "ru": "Я съел слишком много сладостей, и теперь мне <w>плохо</w>."
                            }
                        },
                        {
                            "text": "Kiedy statek zaczął się kołysać, wielu pasażerów poczuło się <w>niedobrze</w>.",
                            "target_lang_translations": {
                                "ru": "Когда корабль начал качаться, многие пассажиры почувствовали себя <w>плохо</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "słabo",
                        "źle"
                    ],
                    "antonyms": [
                        "dobrze"
                    ],
                    "common_phrases": [
                        "jest mi niedobrze",
                        "robi mi się niedobrze",
                        "czuć się niedobrze"
                    ],
                    "target_lang_definitions": {
                        "ru": "О плохом физическом самочувствии, часто подразумевает тошноту или слабость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плохо",
                                "target_lang_sense_clarification": "о тошноте, сильном недомогании"
                            },
                            {
                                "target_lang_word": "нехорошо",
                                "target_lang_sense_clarification": "о слабости, головокружении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a08a262-9d57-3b02-b4d3-0c3ab89c8ab2",
                    "sense_definition": "W sposób nieuprzejmy, szorstki lub pozbawiony życzliwości wobec kogoś.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy kierownik traktuje pracowników bardzo <w>niedobrze</w>.",
                            "target_lang_translations": {
                                "ru": "Новый управляющий относится к сотрудникам очень <w>плохо</w>."
                            }
                        },
                        {
                            "text": "Zachowałeś się <w>niedobrze</w>, nie powinieneś był na nią krzyczeć.",
                            "target_lang_translations": {
                                "ru": "Ты поступил <w>нехорошо</w>, тебе не следовало на неё кричать."
                            }
                        }
                    ],
                    "synonyms": [
                        "niemiło",
                        "źle"
                    ],
                    "antonyms": [
                        "dobrze"
                    ],
                    "common_phrases": [
                        "traktować kogoś niedobrze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Невежливо, грубо или без доброжелательности по отношению к кому-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плохо",
                                "target_lang_sense_clarification": "грубо, без уважения"
                            },
                            {
                                "target_lang_word": "нехорошо",
                                "target_lang_sense_clarification": "непорядочно, осуждающе"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
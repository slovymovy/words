            "senses": [
                {
                    "sense_id": "70950d14-28c0-3b06-853c-e5b61c8b274d",
                    "sense_definition": "A practice of communicating with one's God, or with some spiritual entity.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "spiritual_communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She found great comfort in silent <w>prayer</w> during the difficult times.",
                            "target_lang_translations": {
                                "ru": "Она находила большое утешение в тихой <w>молитве</w> в трудные времена."
                            }
                        },
                        {
                            "text": "The community gathered in the hall for a moment of <w>prayer</w>.",
                            "target_lang_translations": {
                                "ru": "Община собралась в зале для краткой <w>молитвы</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "orison"
                    ],
                    "common_phrases": [
                        "in prayer",
                        "answer a prayer",
                        "power of prayer",
                        "call to prayer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Практика общения с Богом или духовной сущностью"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молитва",
                                "target_lang_sense_clarification": "действие"
                            },
                            {

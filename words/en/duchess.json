                            {
                                "target_lang_word": "герцогиня",
                                "target_lang_sense_clarification": "титул"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3d6ff4a-d594-3e82-a9f5-f934e2ac9b63",
                    "sense_definition": "The female ruler of a duchy.",

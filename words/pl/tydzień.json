    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9e4dbef4-b85b-39f9-940d-a9ab9eca13c6",
                    "sense_definition": "Okres siedmiu kolejnych dni, zwykle liczony od poniedziałku do niedzieli.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "calendar_week",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W przyszłym <w>tygodniu</w> wyjeżdżamy w góry.",
                            "target_lang_translations": {
                                "ru": "На следующей <w>неделе</w> мы уезжаем в горы."
                            }
                        },
                        {
                            "text": "Musisz czekać na wyniki do następnego <w>tygodnia</w>.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно ждать результатов до следующей <w>недели</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "tydz."
                    ],
                    "common_phrases": [
                        "w przyszłym tygodniu",
                        "w zeszłym tygodniu",
                        "dni tygodnia",
                        "koniec tygodnia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период из семи дней, обычно считаемый с понедельника по воскресенье."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неделя",
                                "target_lang_sense_clarification": "календарная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "199396aa-6681-36b1-bb30-1bc22410ae2e",
                    "sense_definition": "Każdy okres trwający siedem dni, niezależnie od kalendarza.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "period_7_days",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Za <w>tydzień</w> mam ważne spotkanie w pracy.",
                            "target_lang_translations": {
                                "ru": "Через <w>неделю</w> у меня важная встреча на работе."
                            }
                        },
                        {
                            "text": "Byłem chory przez cały <w>tydzień</w>.",
                            "target_lang_translations": {
                                "ru": "Я болел целую <w>неделю</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "za tydzień",
                        "cały tydzień",
                        "przed tygodniem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любой промежуток времени длительностью в семь дней, независимо от календаря."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неделя",
                                "target_lang_sense_clarification": "период в 7 дней"
                            }
                        ]
                    }
                }
            ]
        }
    ]

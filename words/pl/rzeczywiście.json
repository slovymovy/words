{
    "word_family": [
        "rzecz",
        "rzeczywisty",
        "rzeczywistość"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "7f072bb0-5377-3914-9fcc-5684d66f76d4",
                    "sense_definition": "W sposób zgodny z prawdą, faktami lub rzeczywistością; nie na niby.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "real_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten obraz wygląda <w>rzeczywiście</w> jak fotografia zrobiona w górach.",
                            "target_lang_translations": {
                                "ru": "Эта картина выглядит <w>действительно</w> как фотография, сделанная в горах."
                            }
                        },
                        {
                            "text": "Czy <w>rzeczywiście</w> musimy już wracać do domu, czy możemy zostać dłużej?",
                            "target_lang_translations": {
                                "ru": "<w>Действительно</w> ли нам уже нужно возвращаться домой, или мы можем остаться подольше?"
                            }
                        }
                    ],
                    "synonyms": [
                        "naprawdę",
                        "faktycznie",
                        "autentycznie",
                        "dosłownie"
                    ],
                    "antonyms": [
                        "niezgodnie z rzeczywistością",
                        "nie naprawdę"
                    ],
                    "common_phrases": [
                        "wyglądać rzeczywiście",
                        "brzmieć rzeczywiście"
                    ],
                    "target_lang_definitions": {
                        "ru": "В соответствии с правдой, фактами или реальностью; не понарошку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "действительно",
                                "target_lang_sense_clarification": "на самом деле, реально"
                            },
                            {
                                "target_lang_word": "на самом деле",
                                "target_lang_sense_clarification": "в действительности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07c15ad7-c188-38bd-8003-7198012a9f03",
                    "sense_definition": "Słowo służące do potwierdzenia prawdziwości wypowiedzi lub podkreślenia słuszności sądu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To <w>rzeczywiście</w> był bardzo trudny i męczący dzień w pracy.",
                            "target_lang_translations": {
                                "ru": "Это <w>действительно</w> был очень трудный и утомительный день на работе."
                            }
                        },
                        {
                            "text": "Masz rację, ten film jest <w>rzeczywiście</w> warty obejrzenia w kinie.",
                            "target_lang_translations": {
                                "ru": "Ты прав, этот фильм <w>действительно</w> стоит посмотреть в кинотеатре."
                            }
                        }
                    ],
                    "synonyms": [
                        "faktycznie",
                        "istotnie",
                        "w rzeczy samej",
                        "w istocie"
                    ],
                    "common_phrases": [
                        "rzeczywiście tak jest",
                        "to rzeczywiście prawda"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, служащее для подтверждения истинности высказывания или подчеркивания правильности суждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "действительно",
                                "target_lang_sense_clarification": "подтверждение факта"
                            },
                            {
                                "target_lang_word": "в самом деле",
                                "target_lang_sense_clarification": "усиление утверждения"
                            },
                            {
                                "target_lang_word": "фактически",
                                "target_lang_sense_clarification": "по сути"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "b5a22c01-7e42-3230-8410-bf4f19e20c24",
                    "sense_definition": "Wykrzyknienie w formie pytania, wyrażające wątpliwość, niedowierzanie lub zdziwienie wobec usłyszanej informacji.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "doubt_interjection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Rzeczywiście</w>? Nie wiedziałem, że twój brat potrafi tak świetnie gotować.",
                            "target_lang_translations": {
                                "ru": "<w>Правда</w>? Я не знал, что твой брат умеет так отлично готовить."
                            }
                        },
                        {
                            "text": "<w>Rzeczywiście</w>? Myślałam, że ten sklep jest dzisiaj zamknięty.",
                            "target_lang_translations": {
                                "ru": "<w>Серьезно</w>? Я думала, что этот магазин сегодня закрыт."
                            }
                        }
                    ],
                    "synonyms": [
                        "naprawdę",
                        "czyżby"
                    ],
                    "common_phrases": [
                        "no rzeczywiście"
                    ],
                    "target_lang_definitions": {
                        "ru": "Восклицание в форме вопроса, выражающее сомнение, недоверие или удивление по поводу услышанного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "правда?",
                                "target_lang_sense_clarification": "вопрос с удивлением"
                            },
                            {
                                "target_lang_word": "серьезно?",
                                "target_lang_sense_clarification": "разговорное выражение недоверия"
                            },
                            {
                                "target_lang_word": "неужели?",
                                "target_lang_sense_clarification": "сильное удивление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc8227b3-445b-36df-bdf9-89219d1222da",
                    "sense_definition": "Wykrzyknienie wyrażające nagłe uświadomienie sobie prawdy, przypomnienie czegoś lub przyznanie racji rozmówcy po chwili wahania.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "realization_interjection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Rzeczywiście</w>! Miałaś rację, zapomniałem dzisiaj zabrać klucze z biurka.",
                            "target_lang_translations": {
                                "ru": "<w>И правда</w>! Ты была права, я забыл сегодня забрать ключи со стола."
                            }
                        },
                        {
                            "text": "<w>Rzeczywiście</w>! To jest ten sam aktor, który grał w tamtym serialu.",
                            "target_lang_translations": {
                                "ru": "<w>Точно</w>! Это тот самый актер, который играл в том сериале."
                            }
                        }
                    ],
                    "synonyms": [
                        "faktycznie",
                        "no tak",
                        "nie inaczej",
                        "owszem"
                    ],
                    "common_phrases": [
                        "a rzeczywiście"
                    ],
                    "target_lang_definitions": {
                        "ru": "Восклицание, выражающее внезапное осознание истины, припоминание чего-либо или признание правоты собеседника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "и правда",
                                "target_lang_sense_clarification": "согласие с собеседником"
                            },
                            {
                                "target_lang_word": "точно",
                                "target_lang_sense_clarification": "внезапное воспоминание"
                            },
                            {
                                "target_lang_word": "действительно",
                                "target_lang_sense_clarification": "признание факта"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "87587e41-a330-30b8-bde1-38b4cfa26004",
                    "sense_definition": "Просторечный союз или частица, выражающие противопоставление или несоответствие тому, что ожидалось; соответствует по значению словам «а на самом деле», «но вдруг», «однако».",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы надеялись на солнечную погоду, <ан> дождь зарядил на весь день.",
                            "target_lang_translations": {
                                "en": "We hoped for sunny weather, <but> rain set in for the whole day."
                            }
                        },
                        {
                            "text": "Думал, что задача решится за минуту, <ан> нет, пришлось думать целый час.",
                            "target_lang_translations": {
                                "en": "I thought the problem would be solved in a minute, <but> no, I had to think for a whole hour."
                            }
                        }
                    ],
                    "synonyms": [
                        "а",
                        "но",
                        "однако",
                        "вдруг"
                    ],
                    "common_phrases": [
                        "ан нет",
                        "ан глядь"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A colloquial conjunction or particle used to express contrast or a discrepancy with what was expected; similar to \"but actually\", \"but suddenly\", or \"however\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "but",
                                "target_lang_sense_clarification": "expressing contrast or unexpected result"
                            },
                            {
                                "target_lang_word": "and yet",
                                "target_lang_sense_clarification": "indicating a situation contrary to expectation"
                            },
                            {
                                "target_lang_word": "but in fact",
                                "target_lang_sense_clarification": "emphasizing reality over expectation"
                            }
                        ]
                    }
                }
            ]
        },

                        "je reinste waanzin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полный и исключительный; сущий (часто используется для усиления качества)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чистейший",
                                "target_lang_sense_clarification": "абсолютный, полный"
                            },
                            {
                                "target_lang_word": "сущий",
                                "target_lang_sense_clarification": "настоящий, истинный"

                },
                {
                    "sense_id": "9febe7a2-1b4d-3e64-83d3-1b53c5e77d18",
                    "sense_definition": "Door iemand zelf gedaan of aanwezig, niet via iemand anders.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De burgemeester was <w>persoonlijk</w> aanwezig bij de opening.",
                            "target_lang_translations": {
                                "en": "The mayor was <w>personally</w> present at the opening.",
                                "ru": "Мэр <w>лично</w> присутствовал на открытии."
                            }
                        },
                        {
                            "text": "Ik zal er <w>persoonlijk</w> voor zorgen dat alles goed komt.",
                            "target_lang_translations": {
                                "en": "I will <w>personally</w> ensure that everything turns out well.",
                                "ru": "Я <w>лично</w> прослежу за тем, чтобы все было хорошо."
                            }
                        }
                    ],
                    "synonyms": [
                        "zelf",
                        "eigenhandig"
                    ],
                    "common_phrases": [
                        "persoonlijk aanwezig",
                        "persoonlijk contact",
                        "persoonlijk toezien"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cc889f45-8e30-37a3-a7cf-c39f12ad4086",
                    "sense_definition": "A person's daughter or son; a person's child.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "biological_descendant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are the <w>offspring</w> of Italian immigrants.",
                            "target_lang_translations": {
                                "ru": "Мы — <w>потомки</w> итальянских иммигрантов."
                            }
                        },
                        {
                            "text": "Parents naturally want to protect their <w>offspring</w> from any harm.",
                            "target_lang_translations": {
                                "ru": "Родители, естественно, хотят защитить свое <w>потомство</w> от любого вреда."
                            }
                        }
                    ],
                    "synonyms": [
                        "child",
                        "children",
                        "kid",
                        "kids",
                        "progeny",
                        "issue"
                    ],
                    "antonyms": [
                        "parent",
                        "father",
                        "mother",
                        "genitor",
                        "progenitor"
                    ],
                    "common_phrases": [
                        "produce offspring",
                        "female offspring",
                        "male offspring"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ребенок или дети человека; сын или дочь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потомство",
                                "target_lang_sense_clarification": "собирательное значение; дети"
                            },
                            {
                                "target_lang_word": "отпрыск",
                                "target_lang_sense_clarification": "единственное число; часто иронично или официально"
                            },
                            {
                                "target_lang_word": "дети",
                                "target_lang_sense_clarification": "множественное число; общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c89ea91a-4f2b-3c2a-8f0c-5b8ec56b6505",
                    "sense_definition": "An animal or plant's progeny or young.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "biological_descendant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lioness fiercely defended her <w>offspring</w> against the predator.",
                            "target_lang_translations": {
                                "ru": "Львица яростно защищала свое <w>потомство</w> от хищника."
                            }
                        },
                        {
                            "text": "Many plants rely on the wind to scatter seeds and produce new <w>offspring</w>.",
                            "target_lang_translations": {
                                "ru": "Многие растения полагаются на ветер, чтобы разбрасывать семена и давать новое <w>потомство</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "progeny",
                        "seed"
                    ],
                    "antonyms": [
                        "parent"
                    ],
                    "common_phrases": [
                        "viable offspring",
                        "produce offspring"
                    ],
                    "target_lang_definitions": {
                        "ru": "Детеныши животного или побеги растения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потомство",
                                "target_lang_sense_clarification": "собирательное; детеныши животных"
                            },
                            {
                                "target_lang_word": "детёныш",
                                "target_lang_sense_clarification": "одно животное"
                            },
                            {
                                "target_lang_word": "побег",
                                "target_lang_sense_clarification": "ботаническое; молодое растение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8df8ffd0-33cf-39d8-a966-d77783c10a15",
                    "sense_definition": "Any of a person's descendants, including of further generations.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biological_descendant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king hoped his <w>offspring</w> would rule the country for centuries.",
                            "target_lang_translations": {
                                "ru": "Король надеялся, что его <w>потомки</w> будут править страной веками."
                            }
                        },
                        {
                            "text": "She regarded all her future <w>offspring</w> and their families with pride.",
                            "target_lang_translations": {
                                "ru": "Она с гордостью думала обо всех своих будущих <w>потомках</w> и их семьях."
                            }
                        }
                    ],
                    "synonyms": [
                        "descendants",
                        "lineage",
                        "progeny",
                        "seed"
                    ],
                    "antonyms": [
                        "ancestors",
                        "forbear",
                        "forebear",
                        "forefather"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любой из потомков человека, включая последующие поколения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потомок",
                                "target_lang_sense_clarification": "человек по отношению к предкам"
                            },
                            {
                                "target_lang_word": "потомки",
                                "target_lang_sense_clarification": "множественное число; будущие поколения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa1b2691-91c5-32d4-a875-e25a0608751d",
                    "sense_definition": "Anything produced; the result of an entity's efforts.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Artists often treasure their works as their immortal <w>offspring</w>.",
                            "target_lang_translations": {
                                "ru": "Художники часто дорожат своими работами как своими бессмертными <w>детищами</w>."
                            }
                        },
                        {
                            "text": "This invention is the <w>offspring</w> of years of hard work and research.",
                            "target_lang_translations": {
                                "ru": "Это изобретение — <w>плод</w> долгих лет упорного труда и исследований."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что произведено; результат чьих-либо усилий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "детище",
                                "target_lang_sense_clarification": "переносное; результат труда или творчества"
                            },
                            {
                                "target_lang_word": "плод",
                                "target_lang_sense_clarification": "переносное; результат усилий"
                            },
                            {
                                "target_lang_word": "порождение",
                                "target_lang_sense_clarification": "результат возникновения чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39017bb5-8696-3f13-8c9c-b908cd2a0d1b",
                    "sense_definition": "A process launched by another process.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The main program spawned a new <w>offspring</w> to handle the request.",
                            "target_lang_translations": {
                                "ru": "Главная программа породила новый <w>дочерний процесс</w> для обработки запроса."
                            }
                        },
                        {
                            "text": "If the parent crashes, the <w>offspring</w> may become a zombie process.",
                            "target_lang_translations": {
                                "ru": "Если родительский процесс дает сбой, <w>дочерний процесс</w> может стать зомби-процессом."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс, запущенный другим процессом (в информатике)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дочерний процесс",
                                "target_lang_sense_clarification": "термин в программировании"
                            },
                            {
                                "target_lang_word": "процесс-потомок",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

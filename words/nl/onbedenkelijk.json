    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "38ac4049-5cec-32d3-b75b-b2d5d71abc1a",
                    "sense_definition": "Waar geen bezwaar tegen is; veilig of acceptabel.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het gebruik van dit middel is medisch gezien volledig <w>onbedenkelijk</w>.",
                            "target_lang_translations": {
                                "en": "From a medical perspective, the use of this substance is completely <w>harmless</w>.",
                                "ru": "С медицинской точки зрения использование этого средства абсолютно <w>безвредно</w>."
                            }
                        },
                        {
                            "text": "Zijn voorstel werd door de commissie als <w>onbedenkelijk</w> beschouwd.",
                            "target_lang_translations": {
                                "en": "His proposal was considered <w>unobjectionable</w> by the committee.",
                                "ru": "Комиссия сочла его предложение <w>не вызывающим возражений</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "bedenkelijk"
                    ],
                    "common_phrases": [
                        "medisch onbedenkelijk",
                        "juridisch onbedenkelijk"
                    ],
                    "target_lang_definitions": {
                        "en": "Against which there is no objection; safe or acceptable.",
                        "ru": "То, против чего нет возражений; безопасный или приемлемый."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "harmless",
                                "target_lang_sense_clarification": "safe, often used in medical or safety contexts"
                            },
                            {
                                "target_lang_word": "unobjectionable",
                                "target_lang_sense_clarification": "acceptable, causing no disapproval"
                            },
                            {
                                "target_lang_word": "safe",
                                "target_lang_sense_clarification": "free from risk or doubt"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "безвредный",
                                "target_lang_sense_clarification": "не наносящий вреда, безопасный для здоровья"
                            },
                            {
                                "target_lang_word": "безопасный",
                                "target_lang_sense_clarification": "не представляющий угрозы"
                            },
                            {
                                "target_lang_word": "не вызывающий возражений",
                                "target_lang_sense_clarification": "приемлемый, допустимый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b239079-942f-3934-a189-00d053c4403e",
                    "sense_definition": "Wat niet te bevatten of te verzinnen is; vreemd.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is haast <w>onbedenkelijk</w> dat hij zo'n grote fout heeft gemaakt.",
                            "target_lang_translations": {
                                "en": "It is almost <w>inconceivable</w> that he made such a big mistake.",
                                "ru": "Почти <w>немыслимо</w>, что он допустил такую большую ошибку."
                            }
                        },
                        {
                            "text": "Door een <w>onbedenkelijk</w> toeval kwamen ze elkaar weer tegen.",
                            "target_lang_translations": {
                                "en": "Through an <w>unimaginable</w> coincidence, they ran into each other again.",
                                "ru": "По какому-то <w>непостижимому</w> совпадению они снова встретились."
                            }
                        }
                    ],
                    "synonyms": [
                        "vreemd",
                        "verbazend"
                    ],
                    "target_lang_definitions": {
                        "en": "Impossible to grasp or invent; strange.",
                        "ru": "То, что невозможно постичь или придумать; странный или невероятный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "inconceivable",
                                "target_lang_sense_clarification": "impossible to imagine or believe"
                            },
                            {
                                "target_lang_word": "unimaginable",
                                "target_lang_sense_clarification": "difficult to conceive of"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "немыслимый",
                                "target_lang_sense_clarification": "который невозможно представить"
                            },
                            {
                                "target_lang_word": "непостижимый",
                                "target_lang_sense_clarification": "недоступный пониманию, удивительный"
                            }
                        ]
                    }
                }
            ]
        }
    ]

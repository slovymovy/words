                {
                    "sense_id": "fe2d156b-c695-33fe-b144-8591f9e3abd4",
                    "sense_definition": "Rzadko spotykany, inny niż to, co jest powszechnie znane; nietypowy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "uniqueness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To był dla nas wszystkich bardzo <w>niezwykły</w> dzień.",
                            "target_lang_translations": {
                                "ru": "Это был для всех нас очень <w>необычный</w> день."
                            }
                        },
                        {
                            "text": "W tym regionie występuje <w>niezwykły</w> gatunek ptaka.",
                            "target_lang_translations": {
                                "ru": "В этом регионе встречается <w>необычный</w> вид птиц."
                            }
                        }
                    ],
                    "synonyms": [
                        "rzadki",
                        "niespotykany",
                        "nietypowy",
                        "unikalny",
                        "wyjątkowy",
                        "oryginalny",
                        "niepowtarzalny"
                    ],
                    "antonyms": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9aaf0854-5c6f-352a-bdd3-75760ab72e16",
                    "sense_definition": "Центральный орган нервной системы человека и животных, расположенный в полости черепа (головной мозг).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "anatomy_organ",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Человеческий <мозг> является самым сложным биологическим механизмом.",
                            "target_lang_translations": {
                                "en": "The human <brain> is the most complex biological mechanism."
                            }
                        },
                        {
                            "text": "После аварии врачи диагностировали у него сотрясение <мозга>.",
                            "target_lang_translations": {
                                "en": "After the accident, doctors diagnosed him with a <brain> concussion."
                            }
                        }
                    ],
                    "common_phrases": [
                        "головной мозг",
                        "сотрясение мозга",
                        "рак мозга",
                        "воспаление мозга"
                    ],
                    "target_lang_definitions": {
                        "en": "The organ inside the head that controls thought, memory, feelings, and activity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "brain",
                                "target_lang_sense_clarification": "organ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c664242b-8f11-3eb8-ab9b-b0cc67746e0b",
                    "sense_definition": "Ум, рассудок, способность мыслить и соображать (часто используется во множественном числе).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "intellect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тебе нужно включить <мозги> и подумать над решением этой задачи.",
                            "target_lang_translations": {
                                "en": "You need to switch on your <brains> and think about the solution to this problem."
                            }
                        },
                        {
                            "text": "Где были твои <мозги>, когда ты подписывал этот странный договор?",
                            "target_lang_translations": {
                                "en": "Where were your <brains> when you signed this strange contract?"
                            }
                        }
                    ],
                    "synonyms": [
                        "ум",
                        "разум",
                        "рассудок",
                        "интеллект"
                    ],
                    "antonyms": [
                        "глупость"
                    ],
                    "common_phrases": [
                        "пудрить мозги",
                        "капать на мозги",
                        "вправить мозги",
                        "раскинуть мозгами",
                        "промывка мозгов"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Intellectual ability or intelligence; the ability to think and understand."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "brains",
                                "target_lang_sense_clarification": "intelligence"
                            },
                            {
                                "target_lang_word": "mind",
                                "target_lang_sense_clarification": "intellect"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "428cde44-3877-3f71-944d-15a8086dbe99",
                    "sense_definition": "Часть центральной нервной системы, расположенная в позвоночном канале (спинной мозг).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "anatomy_organ",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Спинной <мозг> отвечает за передачу нервных импульсов и рефлексы.",
                            "target_lang_translations": {
                                "en": "The spinal <cord> is responsible for transmitting nerve impulses and reflexes."
                            }
                        },
                        {
                            "text": "Травма спинного <мозга> может привести к серьёзным нарушениям движения.",
                            "target_lang_translations": {
                                "en": "A spinal <cord> injury can lead to serious movement disorders."
                            }
                        }
                    ],
                    "common_phrases": [
                        "спинной мозг"
                    ],
                    "target_lang_definitions": {
                        "en": "The thick cord of nerve tissue extending from the brain stem down the back."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spinal cord",
                                "target_lang_sense_clarification": "anatomy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d6726e9-22b5-3a3a-b59a-560c3f075f96",
                    "sense_definition": "Ткань, заполняющая полости костей и участвующая в кроветворении (костный мозг).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "anatomy_tissue",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пациенту срочно требовалась пересадка костного <мозга>.",
                            "target_lang_translations": {
                                "en": "The patient urgently needed a bone <marrow> transplant."
                            }
                        },
                        {
                            "text": "Красный костный <мозг> играет ключевую роль в создании новых клеток крови.",
                            "target_lang_translations": {
                                "en": "Red bone <marrow> plays a key role in creating new blood cells."
                            }
                        }
                    ],
                    "common_phrases": [
                        "костный мозг",
                        "пересадка костного мозга"
                    ],
                    "target_lang_definitions": {
                        "en": "The soft substance inside bones that produces blood cells."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "marrow",
                                "target_lang_sense_clarification": "substance in bones"
                            },
                            {
                                "target_lang_word": "bone marrow",
                                "target_lang_sense_clarification": "substance in bones"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc1d8026-c893-3a8c-862e-c3bd566b6044",
                    "sense_definition": "Человек, который является идейным вдохновителем, организатором или самым умным членом группы.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Костя — настоящий <мозг> нашей команды, он решает самые сложные вопросы.",
                            "target_lang_translations": {
                                "en": "Kostya is the real <brain> of our team; he solves the most difficult questions."
                            }
                        },
                        {
                            "text": "Следователи пытались найти человека, который был <мозгом> этой операции.",
                            "target_lang_translations": {
                                "en": "Investigators tried to find the person who was the <mastermind> of this operation."
                            }
                        }
                    ],
                    "synonyms": [
                        "вдохновитель",
                        "организатор",
                        "лидер"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An intelligent person who plans and directs a complex undertaking."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mastermind",
                                "target_lang_sense_clarification": "person"
                            },
                            {
                                "target_lang_word": "brains",
                                "target_lang_sense_clarification": "person"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "409809f2-6346-3b07-8c88-a795fa7e7a28",
                    "sense_definition": "Блюдо, приготовленное из мозга животного.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В меню французского ресторана были жареные говяжьи <мозги>.",
                            "target_lang_translations": {
                                "en": "Fried beef <brains> were on the menu at the French restaurant."
                            }
                        },
                        {
                            "text": "Не каждый гурман решится попробовать <мозги> обезьяны.",
                            "target_lang_translations": {
                                "en": "Not every gourmet would dare to try monkey <brains>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The brain of an animal used as food."
                    },
                    "translations": {

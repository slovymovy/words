                },
                {
                    "sense_id": "2a5bd146-018e-3319-98d7-80df76a53932",
                    "sense_definition": "Energy, excitement, or liveliness.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vitality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children were full of <w>life</w> after playing in the park.",
                            "target_lang_translations": {
                                "ru": "Дети были полны <w>жизни</w> после игры в парке."

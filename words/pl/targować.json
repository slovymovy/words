            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fc81a42f-d78a-33de-b97a-2bdc4ee8d66d",
                    "sense_definition": "Negocjować cenę towaru lub usługi w celu jej obniżenia lub uzyskania korzystniejszych warunków.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na lokalnym bazarze zawsze warto się <targować>, aby uzyskać lepszą cenę.",
                            "target_lang_translations": {
                                "ru": "На местном рынке всегда стоит <w>торговаться</w>, чтобы получить лучшую цену."
                            }
                        },
                        {
                            "text": "Klient próbował <targować> się o każdą złotówkę przy zakupie samochodu.",
                            "target_lang_translations": {
                                "ru": "Клиент пытался <w>торговаться</w> за каждый злотый при покупке автомобиля."
                            }
                        }
                    ],
                    "synonyms": [
                        "handlować",
                        "spierać się"
                    ],
                    "common_phrases": [
                        "targować się o cenę",
                        "twardo się targować",
                        "lubić się targować"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czasownik używany głównie z zaimkiem zwrotnym „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести переговоры о цене товара или услуги с целью её снижения или получения более выгодных условий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "торговаться",
                                "target_lang_sense_clarification": "о цене"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7900e0c-8f8e-34a1-90ba-0d8f497ebf90",
                    "sense_definition": "Zajmować się kupnem i sprzedażą towarów; prowadzić handel.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dawniej kupcy przyjeżdżali do miasta, aby <targować> skórami i zbożem.",
                            "target_lang_translations": {
                                "ru": "Раньше купцы приезжали в город, чтобы <w>торговать</w> шкурами и зерном."
                            }
                        },
                        {
                            "text": "Moi przodkowie potrafili skutecznie <targować> końmi na jarmarkach.",
                            "target_lang_translations": {
                                "ru": "Мои предки умели успешно <w>торговать</w> лошадьми на ярмарках."
                            }
                        }
                    ],
                    "synonyms": [
                        "handlować"
                    ],
                    "common_phrases": [
                        "targować końmi",
                        "targować zbożem"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "współcześnie częściej używa się słowa „handlować”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Заниматься куплей-продажей товаров; вести торговлю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "торговать",
                                "target_lang_sense_clarification": "продавать что-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af427cb7-2508-30e5-b4ef-f4d71a0f5260",
                    "sense_definition": "Prowadzić spór lub zawziętą dyskusję, próbując przekonać kogoś do swoich racji lub coś wymusić.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie zamierzam się z tobą <targować> o to, kto ma rację w tej sprawie.",
                            "target_lang_translations": {
                                "ru": "Я не собираюсь с тобой <w>торговаться</w> о том, кто прав в этом деле."
                            }
                        },
                        {
                            "text": "Politycy zaczęli się <targować> o podział stanowisk w nowym rządzie.",
                            "target_lang_translations": {
                                "ru": "Политики начали <w>торговаться</w> о распределении должностей в новом правительстве."
                            }
                        }
                    ],
                    "synonyms": [
                        "spierać się",
                        "wykłócać się"
                    ],
                    "common_phrases": [
                        "targować się o drobiazgi",
                        "targować się o szczegóły"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czasownik używany głównie z zaimkiem zwrotnym „się”"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести спор или ожесточённую дискуссию, пытаясь убедить кого-то в своей правоте или чего-то добиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "торговаться",
                                "target_lang_sense_clarification": "перен. спорить, вести переговоры"
                            },
                            {
                                "target_lang_word": "спорить",
                                "target_lang_sense_clarification": "дискутировать"
                            }
                        ]
                    }
                }

            "senses": [
                {
                    "sense_id": "742ea88a-0503-32a8-acf6-f85434eb6b0e",
                    "sense_definition": "Relating to or produced by magic or supernatural forces.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "literal_magic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wizard used his <w>magical</w> powers to open the heavy stone door.",
                            "target_lang_translations": {
                                "ru": "Волшебник использовал свои <w>магические</w> силы, чтобы открыть тяжелую каменную дверь."
                            }
                        },
                        {
                            "text": "Children love reading stories about dragons and other <w>magical</w> creatures.",
                            "target_lang_translations": {
                                "ru": "Дети любят читать истории о драконах и других <w>волшебных</w> существах."
                            }
                        }
                    ],
                    "synonyms": [
                        "supernatural",
                        "mystic"
                    ],
                    "antonyms": [
                        "natural",
                        "mundane"
                    ],
                    "common_phrases": [
                        "magical powers",
                        "magical creature",
                        "magical spell",
                        "magical world"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к магии или созданный сверхъестественными силами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "маги́ческий",
                                "target_lang_sense_clarification": "связанный с магией, оккультный"
                            },
                            {
                                "target_lang_word": "волше́бный",
                                "target_lang_sense_clarification": "обладающий чудесными свойствами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f8ae2d1f-ad0b-3372-9114-5b11addee3ac",
                    "sense_definition": "Extremely beautiful, delightful, or exciting, as if caused by magic.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "figurative_magic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fireworks created a <w>magical</w> atmosphere in the park last night.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером фейерверк создал в парке <w>волшебную</w> атмосферу."
                            }
                        },
                        {
                            "text": "We had a <w>magical</w> time walking through the snowy forest together.",
                            "target_lang_translations": {
                                "ru": "Мы провели <w>волшебное</w> время, гуляя вместе по заснеженному лесу."
                            }
                        }
                    ],
                    "synonyms": [
                        "enchanting",
                        "wonderful",
                        "delightful",
                        "charming"
                    ],
                    "antonyms": [
                        "ordinary",
                        "dull",
                        "boring"
                    ],
                    "common_phrases": [
                        "magical moment",
                        "magical atmosphere",
                        "magical experience",
                        "magical evening"
                    ],
                    "target_lang_definitions": {
                        "ru": "Необыкновенно красивый, восхитительный или захватывающий, словно созданный магией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волше́бный",
                                "target_lang_sense_clarification": "чарующий, восхитительный"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fa02e63f-ecdb-3232-a9f2-daf9bbdc2178",
                    "sense_definition": "A state of bad health or disease.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many working days this year have been lost through <w>illness</w>.",
                            "target_lang_translations": {
                                "ru": "В этом году много рабочих дней было пропущено по <w>болезни</w>."
                            }
                        },
                        {
                            "text": "He could not attend the event due to <w>illness</w>.",
                            "target_lang_translations": {
                                "ru": "Он не смог посетить мероприятие из-за <w>болезни</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sickness"
                    ],
                    "antonyms": [
                        "health"
                    ],
                    "common_phrases": [
                        "due to illness",
                        "through illness",
                        "serious illness"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние плохого здоровья или наличие болезни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "болезнь",
                                "target_lang_sense_clarification": "общее состояние нездоровья"
                            },
                            {
                                "target_lang_word": "недуг",
                                "target_lang_sense_clarification": "тяжелое или продолжительное недомогание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53505622-47b3-3573-aa79-e3f8e6248a1e",
                    "sense_definition": "An instance of a disease or poor health.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her grandmother had passed away after a long <w>illness</w>.",
                            "target_lang_translations": {
                                "ru": "Её бабушка скончалась после продолжительной <w>болезни</w>."
                            }
                        },
                        {
                            "text": "The doctor diagnosed him with a rare <w>illness</w>.",
                            "target_lang_translations": {
                                "ru": "Врач диагностировал у него редкое <w>заболевание</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sickness"
                    ],
                    "common_phrases": [
                        "long illness",
                        "mental illness",
                        "terminal illness",
                        "severe illness"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный случай заболевания или расстройства здоровья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заболевание",
                                "target_lang_sense_clarification": "медицинский термин, конкретный случай"
                            },
                            {
                                "target_lang_word": "болезнь",
                                "target_lang_sense_clarification": "конкретный вид недуга"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                        "единица"
                    ],
                    "common_phrases": [
                        "имя им легион"
                    ],
                    "target_lang_definitions": {
                        "en": "A huge, countless number of someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "legion",
                                "target_lang_sense_clarification": "great number"
                            },
                            {
                                "target_lang_word": "multitude",
                                "target_lang_sense_clarification": "large quantity"
                            }
                        ]
                    }
                },
                {

                    "frequency": "High",
                    "semantic_group_id": "update_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне нужно срочно <w>обновить</w> страницу, чтобы увидеть результаты матча.",
                            "target_lang_translations": {
                                "en": "I need to urgently <w>refresh</w> the page to see the match results."
                            }
                        },
                        {
                            "text": "Компания решила полностью <w>обновить</w> ассортимент товаров в этом сезоне.",
                            "target_lang_translations": {
                                "en": "The company decided to completely <w>update</w> its product range this season."
                            }
                        },
                        {
                            "text": "Не забудьте <w>обновить</w> приложение до последней версии.",
                            "target_lang_translations": {
                                "en": "Don't forget to <w>update</w> the app to the latest version."
                            }
                        }
                    ],
                    "synonyms": [
                        "модернизировать",
                        "актуализировать"
                    ],
                    "common_phrases": [
                        "обновить страницу",
                        "обновить приложение",
                        "обновить данные",
                        "обновить гардероб",
                        "обновить состав"
                    ],
                    "target_lang_definitions": {

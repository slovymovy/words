                        "wiórowato"
                    ],
                    "common_phrases": [
                        "soczyście smakować",
                        "zjeść soczyście"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обильно, с большим количеством сока; влажно и приятно на вкус."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сочно",
                                "target_lang_sense_clarification": "о еде, фруктах"

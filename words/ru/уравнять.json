            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f7555eb0-8b47-3760-acd9-634e567cd4ac",
                    "sense_definition": "Сделать равным, одинаковым по размеру, величине, количеству или значению.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "make_equal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На последних минутах матча наша команда смогла <w>уравнять</w> счёт.",
                            "target_lang_translations": {
                                "en": "In the last minutes of the match, our team managed to <w>equalize</w> the score."
                            }
                        },
                        {
                            "text": "Это неожиданное событие может <w>уравнять</w> шансы обоих соперников на победу.",
                            "target_lang_translations": {
                                "en": "This unexpected event could <w>equalize</w> both rivals' chances of winning."
                            }
                        }
                    ],
                    "synonyms": [
                        "сровнять"
                    ],
                    "common_phrases": [
                        "уравнять счёт",
                        "уравнять шансы",
                        "уравнять веса"
                    ],
                    "target_lang_definitions": {
                        "en": "To make equal in size, amount, or value."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "equalize",
                                "target_lang_sense_clarification": "to make the same in quantity, size, or degree"
                            },
                            {
                                "target_lang_word": "even up",
                                "target_lang_sense_clarification": "to make equal, especially regarding scores or amounts"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b9106b4-285d-3d65-a8d3-b31b39d8ddce",
                    "sense_definition": "Поставить в одинаковое положение с кем-либо или чем-либо, предоставить равные права.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "make_equal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Новый закон должен <w>уравнять</w> всех граждан страны в правах.",
                            "target_lang_translations": {
                                "en": "The new law should <w>make</w> all the country's citizens <w>equal</w> in rights."
                            }
                        },
                        {
                            "text": "Руководство компании решило <w>уравнять</w> зарплаты сотрудников разных отделов.",
                            "target_lang_translations": {
                                "en": "The company management decided to <w>equalize</w> the salaries of employees in different departments."
                            }
                        }
                    ],
                    "synonyms": [
                        "приравнять"
                    ],
                    "common_phrases": [
                        "уравнять в правах",
                        "уравнять в возможностях",
                        "уравнять в зарплате"
                    ],
                    "target_lang_definitions": {
                        "en": "To place in an equal position or grant equal rights."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "make equal",
                                "target_lang_sense_clarification": "to give the same status or rights"
                            },
                            {
                                "target_lang_word": "put on an equal footing",
                                "target_lang_sense_clarification": "to place in the same situation or status"
                            }
                        ]
                    }
                }
            ]
        }
    ]

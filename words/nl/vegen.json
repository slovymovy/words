    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "47904583-8178-3008-84ef-bf10fa8afd2b",
                    "sense_definition": "Schoonmaken of vuil verwijderen met behulp van een bezem of borstel.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cleaning_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vergeet niet de vloer nog te <w>vegen</w>!",
                            "target_lang_translations": {
                                "ru": "Не забудь еще <w>подмести</w> пол!",
                                "en": "Don't forget to <w>sweep</w> the floor!"
                            }
                        },
                        {
                            "text": "De winkelier <w>veegt</w> elke ochtend de stoep voor zijn zaak.",
                            "target_lang_translations": {
                                "ru": "Владелец магазина каждое утро <w>подметает</w> тротуар перед своим заведением.",
                                "en": "The shopkeeper <w>sweeps</w> the pavement in front of his shop every morning."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de vloer vegen",
                        "de straat vegen",
                        "schoon vegen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чистить или удалять грязь с помощью метлы или щетки.",
                        "en": "To clean or remove dirt using a broom or brush."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подметать",
                                "target_lang_sense_clarification": "убирать метлой"
                            },
                            {
                                "target_lang_word": "мести",
                                "target_lang_sense_clarification": "сметать мусор"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "sweep",
                                "target_lang_sense_clarification": "to clean with a broom"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a94b1ce5-b8d3-3e7d-81d2-b7e223624e79",
                    "sense_definition": "Iets verplaatsen, weghalen of schoonmaken door er met de hand, een doek of een ander voorwerp langs te strijken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cleaning_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij <w>veegde</w> de kruimels van haar shirt voordat ze opstond.",
                            "target_lang_translations": {
                                "ru": "Она <w>смахнула</w> крошки со своей рубашки, прежде чем встать.",
                                "en": "She <w>brushed</w> the crumbs off her shirt before she stood up."
                            }
                        },
                        {
                            "text": "Je moet goed je voeten <w>vegen</w> op de deurmat als je binnenkomt.",
                            "target_lang_translations": {
                                "ru": "Нужно хорошо <w>вытирать</w> ноги о коврик при входе.",
                                "en": "You must <w>wipe</w> your feet well on the doormat when you enter."
                            }
                        }
                    ],
                    "synonyms": [
                        "wissen"
                    ],
                    "common_phrases": [
                        "voeten vegen",
                        "tranen vegen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать, удалять что-либо или чистить, проводя по поверхности рукой, тряпкой или другим предметом.",
                        "en": "To move, remove, or clean something by stroking it with a hand, cloth, or other object."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вытирать",
                                "target_lang_sense_clarification": "удалять влагу или грязь тряпкой"
                            },
                            {
                                "target_lang_word": "смахивать",
                                "target_lang_sense_clarification": "удалять легким движением руки"
                            },
                            {
                                "target_lang_word": "стряхивать",
                                "target_lang_sense_clarification": "сбрасывать резким движением"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "wipe",
                                "target_lang_sense_clarification": "to clean or dry by rubbing"
                            },
                            {
                                "target_lang_word": "brush",
                                "target_lang_sense_clarification": "to remove with a sweeping motion"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [

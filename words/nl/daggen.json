    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f4a22c92-e277-371e-af65-be883420453c",
                    "sense_definition": "Voegen in metselwerk glad afwerken met een voegspijker of dagijzer.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "masonry_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De metselaar moest de voegen zorgvuldig <w>daggen</w> voor een strak resultaat.",
                            "target_lang_translations": {
                                "en": "The bricklayer had to carefully <w>point</w> the joints for a sleek result.",
                                "ru": "Каменщик должен был тщательно <w>расшить</w> швы для получения аккуратного результата."
                            }
                        },
                        {
                            "text": "Bij de restauratie van de oude gevel werd besloten het voegwerk opnieuw te <w>daggen</w>.",
                            "target_lang_translations": {
                                "en": "During the restoration of the old facade, it was decided to <w>point</w> the brickwork again.",
                                "ru": "При реставрации старого фасада было решено заново <w>расшить</w> швы кладки."
                            }
                        }
                    ],
                    "synonyms": [
                        "afvoegen"
                    ],
                    "common_phrases": [
                        "een muur daggen",
                        "voegen daggen"
                    ],
                    "target_lang_definitions": {
                        "en": "To finish joints in masonry smoothly using a jointer or pointing iron.",
                        "ru": "Гладкая отделка швов в кирпичной кладке с помощью расшивки."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "point",
                                "target_lang_sense_clarification": "masonry"
                            },
                            {
                                "target_lang_word": "joint",
                                "target_lang_sense_clarification": "masonry"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "расшивать",
                                "target_lang_sense_clarification": "швы в кладке"
                            },
                            {
                                "target_lang_word": "заделывать швы",
                                "target_lang_sense_clarification": "строительство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "519c98ae-9589-326b-9443-832d659b2820",
                    "sense_definition": "Iemand met een dolk steken.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "violent_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de oude kronieken stond beschreven hoe de verrader werd <w>gedagd</w>.",
                            "target_lang_translations": {
                                "en": "In the old chronicles, it was described how the traitor was <w>stabbed</w>.",
                                "ru": "В старых хрониках описывалось, как предатель был <w>заколот</w>."
                            }
                        },
                        {
                            "text": "De ridder dreigde zijn vijand te <w>daggen</w> tijdens het gevecht.",
                            "target_lang_translations": {
                                "en": "The knight threatened to <w>stab</w> his enemy during the fight.",
                                "ru": "Рыцарь грозился <w>заколоть</w> своего врага во время боя."
                            }
                        }
                    ],
                    "synonyms": [

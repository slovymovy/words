{
    "word_family": [
        "sprzętowy",
        "sprzęcik",
        "osprzęt"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "78d2c41d-0f5f-360a-9dd0-a0487ff72d50",
                    "sense_definition": "Zestaw przedmiotów, narzędzi lub urządzeń niezbędnych do wykonywania określonej czynności, pracy lub uprawiania sportu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat kupił nowy <w>sprzęt</w> narciarski na zimowy wyjazd.",
                            "target_lang_translations": {
                                "ru": "Мой брат купил новое лыжное <w>снаряжение</w> для зимней поездки."
                            }
                        },
                        {
                            "text": "W szpitalu znajduje się bardzo nowoczesny <w>sprzęt</w> medyczny ratujący życie.",
                            "target_lang_translations": {
                                "ru": "В больнице находится очень современное медицинское <w>оборудование</w>, спасающее жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbiór",
                        "zbiór"
                    ],
                    "common_phrases": [
                        "sprzęt sportowy",
                        "sprzęt medyczny",
                        "ciężki sprzęt",
                        "sprzęt grający"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор предметов, инструментов или устройств, необходимых для выполнения определенной деятельности, работы или занятий спортом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оборудование",
                                "target_lang_sense_clarification": "техническое, медицинское или профессиональное оснащение"
                            },
                            {
                                "target_lang_word": "снаряжение",
                                "target_lang_sense_clarification": "экипировка для спорта, туризма или военных целей"
                            },
                            {
                                "target_lang_word": "инвентарь",
                                "target_lang_sense_clarification": "предметы для спорта или хозяйственных работ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73cd1677-2606-311f-bcd3-135cd171fa01",
                    "sense_definition": "Pojedynczy przedmiot użytkowy, urządzenie lub mebel, stanowiący element wyposażenia domu lub innego miejsca.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy wymienić stary <w>sprzęt</w> AGD w kuchni na bardziej energooszczędny.",
                            "target_lang_translations": {
                                "ru": "Нам нужно заменить старую бытовую <w>технику</w> на кухне на более энергоэффективную."
                            }
                        },
                        {
                            "text": "W salonie stały zabytkowe <w>sprzęty</w>, które odziedziczyła po babci.",
                            "target_lang_translations": {
                                "ru": "В гостиной стояли старинные <w>предметы обстановки</w>, которые она унаследовала от бабушки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sprzęt AGD",
                        "sprzęt domowy",
                        "sprzęty kuchenne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный предмет обихода, устройство или мебель, являющиеся частью обстановки дома."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "техника",
                                "target_lang_sense_clarification": "бытовые приборы, электроника (напр. sprzęt AGD)"
                            },
                            {
                                "target_lang_word": "утварь",
                                "target_lang_sense_clarification": "предметы домашнего обихода, посуда"
                            },
                            {
                                "target_lang_word": "предметы обстановки",
                                "target_lang_sense_clarification": "мебель, вещи в доме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a774f36-47c9-3184-b9e2-a0eb6e42609d",
                    "sense_definition": "Czynność zbierania płodów rolnych z pola, sadu lub ogrodu i zwożenia ich do gospodarstwa.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnicy zaplanowali <w>sprzęt</w> zboża na przyszły tydzień, jeśli pogoda dopisze.",
                            "target_lang_translations": {
                                "ru": "Фермеры запланировали <w>уборку</w> зерна на следующую неделю, если погода позволит."
                            }
                        },
                        {
                            "text": "Ciągłe deszcze utrudniały jesienny <w>sprzęt</w> ziemniaków w całym regionie.",
                            "target_lang_translations": {
                                "ru": "Постоянные дожди затрудняли осеннюю <w>уборку</w> картофеля во всем регионе."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbiór"
                    ],
                    "common_phrases": [
                        "sprzęt zboża",
                        "sprzęt siana"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по сбору сельскохозяйственных культур с поля, сада или огорода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уборка",
                                "target_lang_sense_clarification": "сбор урожая, сельскохозяйственные работы"
                            },
                            {
                                "target_lang_word": "сбор",
                                "target_lang_sense_clarification": "процесс собирания плодов или зерна"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
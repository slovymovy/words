            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c5444469-da88-3451-98b1-ae8f2d3eaf9d",
                    "sense_definition": "Akt prawny mający moc ustawy, wydany przez organ władzy wykonawczej, a nie ustawodawczej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_acts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prezydent podpisał wczoraj nowy <w>dekret</w> dotyczący bezpieczeństwa narodowego.",
                            "target_lang_translations": {
                                "ru": "Вчера президент подписал новый <w>декрет</w>, касающийся национальной безопасности."
                            }
                        },
                        {
                            "text": "W sytuacji kryzysowej rząd może rządzić, wydając specjalny <w>dekret</w>.",
                            "target_lang_translations": {
                                "ru": "В кризисной ситуации правительство может править, издавая специальный <w>декрет</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "edykt"
                    ],
                    "common_phrases": [
                        "wydać dekret",
                        "podpisać dekret",
                        "dekret prezydencki",
                        "rządzić dekretami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Правовой акт, имеющий силу закона, изданный органом исполнительной власти, а не законодательной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "декрет",
                                "target_lang_sense_clarification": "правовой акт"
                            },
                            {
                                "target_lang_word": "указ",
                                "target_lang_sense_clarification": "распоряжение главы государства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00995973-d6a5-34d8-9b15-824dcb09339b",
                    "sense_definition": "Oficjalny dokument wydawany przez biskupa lub władzę kościelną, zawierający decyzje administracyjne, np. o mianowaniu księdza.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "church_documents",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody wikariusz z niecierpliwością czekał na <w>dekret</w> biskupa w sprawie nowej parafii.",
                            "target_lang_translations": {
                                "ru": "Молодой викарий с нетерпением ждал <w>декрета</w> епископа касательно нового прихода."
                            }
                        },
                        {
                            "text": "Kuria opublikowała <w>dekret</w> ustanawiający nowe sanktuarium w diecezji.",
                            "target_lang_translations": {
                                "ru": "Курия опубликовала <w>декрет</w>, учреждающий новое святилище в епархии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dekret biskupi",
                        "otrzymać dekret",
                        "dekret nominacyjny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальный документ, издаваемый епископом или церковной властью, содержащий административные решения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "декрет",
                                "target_lang_sense_clarification": "церковный документ"
                            },
                            {
                                "target_lang_word": "указ",
                                "target_lang_sense_clarification": "архиерейский"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c4c6d1d-a031-385d-987f-f89aab2a3b29",
                    "sense_definition": "Historyczny akt prawny wydawany samodzielnie przez króla w dawnej Polsce.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "historical_law",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Król ogłosił <w>dekret</w>, który nadawał miastu nowe przywileje handlowe.",
                            "target_lang_translations": {
                                "ru": "Король объявил <w>декрет</w>, дарующий городу новые торговые привилегии."
                            }
                        },
                        {
                            "text": "Historycy analizują stary <w>dekret</w> znaleziony w archiwach zamkowych.",
                            "target_lang_translations": {
                                "ru": "Историки анализируют старый <w>декрет</w>, найденный в замковых архивах."
                            }
                        }
                    ],
                    "synonyms": [
                        "edykt"
                    ],
                    "common_phrases": [
                        "dekret królewski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исторический правовой акт, издаваемый единолично королем в старой Польше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "декрет",
                                "target_lang_sense_clarification": "исторический акт"
                            },
                            {
                                "target_lang_word": "королевский указ",
                                "target_lang_sense_clarification": "распоряжение монарха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f9138e7-f884-3095-8c6a-9190eecb2e78",
                    "sense_definition": "Wyrok sądu w dawnym prawie polskim lub rzymskim.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "historical_law",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd wydał surowy <w>dekret</w> skazujący oskarżonego na banicję.",
                            "target_lang_translations": {
                                "ru": "Суд вынес суровый <w>приговор</w>, осуждающий обвиняемого на изгнание."
                            }
                        },
                        {
                            "text": "Strony sporu musiały zaakceptować ostateczny <w>dekret</w> trybunału.",
                            "target_lang_translations": {
                                "ru": "Стороны спора должны были принять окончательное <w>решение</w> трибунала."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyrok",
                        "orzeczenie"
                    ],

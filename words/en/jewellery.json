    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8b73d2c1-749c-3492-a091-36b94a08b8be",
                    "sense_definition": "Collectively, personal ornamentation such as rings, necklaces, brooches and bracelets, made of precious metals and sometimes set with gemstones.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>jewellery</w> was very expensive in that shop.",
                            "target_lang_translations": {
                                "ru": "<w>Ювелирные изделия</w> в том магазине были очень дорогими."
                            }
                        },
                        {
                            "text": "She likes to wear gold <w>jewellery</w> on special occasions.",
                            "target_lang_translations": {
                                "ru": "Она любит носить золотые <w>украшения</w> по особым случаям."
                            }
                        }
                    ],
                    "synonyms": [
                        "jewelry",
                        "bling"
                    ],
                    "common_phrases": [
                        "piece of jewellery",
                        "gold jewellery",
                        "wear jewellery",
                        "expensive jewellery"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British spelling"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предметы личного украшения (кольца, ожерелья, броши, браслеты), изготовленные из драгоценных металлов и иногда инкрустированные драгоценными камнями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "украше́ния",
                                "target_lang_sense_clarification": "общее название для предметов украшения"
                            },
                            {
                                "target_lang_word": "ювели́рные изде́лия",
                                "target_lang_sense_clarification": "изделия из драгоценных металлов и камней"
                            },
                            {
                                "target_lang_word": "драгоце́нности",
                                "target_lang_sense_clarification": "ценные вещи, предметы роскоши"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74de7041-04ec-3776-a333-b69210d39996",
                    "sense_definition": "Any tools, instruments, devices, etc., placed within a drill pipe.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crew checked the <w>jewellery</w> on the drill string.",
                            "target_lang_translations": {
                                "ru": "Бригада проверила <w>внутрискважинное оборудование</w> на бурильной колонне."
                            }
                        },
                        {
                            "text": "Special <w>jewellery</w> is often needed for deep drilling operations.",
                            "target_lang_translations": {
                                "ru": "Для глубокого бурения часто требуется специальное <w>скважинное оборудование</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Любые инструменты, приборы или устройства, размещаемые внутри бурильной трубы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внутрисква́жинное обору́дование",
                                "target_lang_sense_clarification": "технический термин в бурении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bba9d1bf-6e21-3028-bd17-2843243a8d8c",
                    "sense_definition": "The shop from which a jeweller trades or makes jewellery.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He stopped by the <w>jewellery</w> to pick up the repaired watch.",
                            "target_lang_translations": {
                                "ru": "Он зашел в <w>ювелирный магазин</w>, чтобы забрать отремонтированные часы."
                            }
                        },
                        {
                            "text": "The local <w>jewellery</w> has a wide selection of rings.",
                            "target_lang_translations": {
                                "ru": "В местном <w>ювелирном магазине</w> большой выбор колец."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "referring to the shop"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Магазин, в котором ювелир продает или изготавливает ювелирные изделия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ювели́рный магази́н",
                                "target_lang_sense_clarification": "место продажи украшений"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
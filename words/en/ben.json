                {
                    "sense_id": "0cf51d42-595f-3a8b-a084-1df447bbdb47",
                    "sense_definition": "A short form of the male given names <w>Benjamin</w> or <w>Benedict</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "<w>Ben</w> is coming to visit us this weekend.",
                            "target_lang_translations": {
                                "ru": "<w>Бен</w> приедет навестить нас в эти выходные."
                            }
                        },
                        {
                            "text": "I asked <w>Ben</w> if he wanted to join our football team.",
                            "target_lang_translations": {
                                "ru": "Я спросил <w>Бена</w>, хочет ли он присоединиться к нашей футбольной команде."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Краткая форма мужских имен Бенджамин или Бенедикт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бен",
                                "target_lang_sense_clarification": "мужское имя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ec156012-1881-38bb-b586-b6640eed49f8",
                    "sense_definition": "A mountain or high peak, used especially in names of mountains in Scotland and Ireland.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to climb the <w>ben</w> early in the morning to catch the sunrise.",
                            "target_lang_translations": {
                                "ru": "Мы решили подняться на <w>гору</w> рано утром, чтобы застать рассвет."
                            }
                        },
                        {
                            "text": "Snow can still be seen on the top of the <w>ben</w> even in late spring.",
                            "target_lang_translations": {
                                "ru": "Снег всё ещё можно увидеть на вершине <w>горы</w> даже поздней весной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Ben Nevis"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Scotland, Ireland"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Гора или высокий пик (используется преимущественно в названиях гор в Шотландии и Ирландии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гора",
                                "target_lang_sense_clarification": "в Шотландии и Ирландии"
                            },
                            {
                                "target_lang_word": "вершина",
                                "target_lang_sense_clarification": "горная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3c03328-4e72-3a0b-a298-3217b0ef4cf8",
                    "sense_definition": "A US one hundred dollar bill.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He pulled a few <w>bens</w> out of his wallet to pay for the expensive dinner.",
                            "target_lang_translations": {
                                "ru": "Он вытащил несколько <w>стодолларовых купюр</w> из бумажника, чтобы расплатиться за дорогой ужин."
                            }
                        },
                        {
                            "text": "It is rare to see people paying with <w>bens</w> at a grocery store.",
                            "target_lang_translations": {
                                "ru": "Редко можно увидеть людей, расплачивающихся <w>стодолларовыми купюрами</w> в продуктовом магазине."
                            }
                        }
                    ],
                    "synonyms": [
                        "Benjamin"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Стодолларовая купюра США."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стодолларовая купюра",
                                "target_lang_sense_clarification": "разг."

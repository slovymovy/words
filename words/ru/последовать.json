{
    "word_family": [
        "следовать",
        "след",
        "последователь",
        "последовательность",
        "последствие",
        "следствие",
        "наследовать",
        "преследовать"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "18553842-c0b9-37fd-8c31-f3814880dcae",
                    "sense_definition": "Пойти или поехать вслед за кем-либо или чем-либо, двигаясь в том же направлении.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он встал из-за стола и <последовал> за мной в коридор.",
                            "target_lang_translations": {
                                "en": "He got up from the table and <followed> me into the corridor."
                            }
                        },
                        {
                            "text": "Верная собака повсюду <последовала> за своим хозяином.",
                            "target_lang_translations": {
                                "en": "The faithful dog <followed> its master everywhere."
                            }
                        }
                    ],
                    "synonyms": [
                        "пойти́ сле́дом за",
                        "пойти́ вслед за"
                    ],
                    "common_phrases": [
                        "последовать за ним",
                        "последовать за проводником",
                        "последовать за ней"
                    ],
                    "target_lang_definitions": {
                        "en": "To go or move after someone or something in the same direction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "follow",
                                "target_lang_sense_clarification": "move behind someone"
                            },
                            {
                                "target_lang_word": "come after",
                                "target_lang_sense_clarification": "move behind"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8701c1e8-1e63-3d67-8dbe-3df1ff73bfba",
                    "sense_definition": "Поступить подобно кому-либо или в соответствии с чем-либо (советом, примером, правилом).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action_accordance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я решил <последовать> твоему мудрому совету и отдохнуть.",
                            "target_lang_translations": {
                                "en": "I decided to <follow> your wise advice and take a rest."
                            }
                        },
                        {
                            "text": "Многие сотрудники <последовали> примеру директора и пришли вовремя.",
                            "target_lang_translations": {
                                "en": "Many employees <followed> the director's example and arrived on time."
                            }
                        }
                    ],
                    "synonyms": [
                        "руково́дствоваться"
                    ],
                    "common_phrases": [
                        "последовать совету",
                        "последовать примеру",
                        "последовать призыву"
                    ],
                    "target_lang_definitions": {
                        "en": "To act in accordance with something, such as advice, an example, or a rule."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "follow",
                                "target_lang_sense_clarification": "act according to advice or example"
                            },
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "accept and act on (advice)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ef82d3c-d41a-3596-acca-7830895acdf7",
                    "sense_definition": "Наступить, произойти или появиться непосредственно после чего-либо во времени или пространстве.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За громким заявлением политика <последовало> долгое молчание.",
                            "target_lang_translations": {
                                "en": "A long silence <followed> the politician's loud statement."
                            }
                        },
                        {
                            "text": "Вскоре после первой вспышки <последовал> мощный взрыв.",
                            "target_lang_translations": {
                                "en": "A powerful explosion <followed> soon after the first flash."
                            }
                        }
                    ],
                    "common_phrases": [
                        "за этим последовало",
                        "вскоре последовал ответ",
                        "затем последовала тишина"
                    ],
                    "target_lang_definitions": {
                        "en": "To happen, occur, or appear immediately after something in time or space."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "follow",
                                "target_lang_sense_clarification": "happen after"
                            },
                            {
                                "target_lang_word": "ensue",
                                "target_lang_sense_clarification": "happen subsequently"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af97ac9c-56ba-3caf-88a3-a1f97045a304",
                    "sense_definition": "Явиться следствием, результатом или логическим выводом из чего-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За грубым нарушением правил немедленно <последовало> наказание.",
                            "target_lang_translations": {
                                "en": "Punishment immediately <followed> the gross violation of the rules."
                            }
                        },
                        {
                            "text": "Из этого факта <последовал> вполне неожиданный вывод.",
                            "target_lang_translations": {
                                "en": "A quite unexpected conclusion <followed> from this fact."
                            }
                        }
                    ],
                    "common_phrases": [
                        "наказание последовало",
                        "реакция последовала",
                        "отсюда последовал вывод"
                    ],
                    "target_lang_definitions": {
                        "en": "To be a consequence, result, or logical conclusion of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "follow",
                                "target_lang_sense_clarification": "be a logical result"
                            },
                            {
                                "target_lang_word": "result",
                                "target_lang_sense_clarification": "occur as a consequence"
                            },
                            {
                                "target_lang_word": "ensue",
                                "target_lang_sense_clarification": "happen as a result"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "71068a90-164c-3fef-a777-4001b916e5e5",
                    "sense_definition": "In isolation, a request for repetition or clarification of what has just been said.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "request_repetition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "—I didn't hear you clearly. —<w>Eh</w>? Can you say that again?"
                        },
                        {
                            "text": "—We need to leave now. —<w>Eh</w>? I thought we had more time."
                        }
                    ],
                    "synonyms": [
                        "pardon",
                        "sorry",
                        "what",
                        "huh",
                        "come again"
                    ],
                    "common_phrases": [
                        "eh? what?",
                        "say again, eh?"
                    ]
                },
                {
                    "sense_id": "2565dc42-8cfe-39ae-93fb-e74141ffa3b9",
                    "sense_definition": "Used as a tag question, to emphasise what goes before or to request that the listener express an opinion about what has been said.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "tag_question",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is pretty cold outside today, <w>eh</w>?"
                        },
                        {
                            "text": "You really like this movie, <w>eh</w>?"
                        }
                    ],
                    "synonyms": [
                        "y'know",
                        "huh",
                        "what"
                    ],
                    "common_phrases": [
                        "good, eh",
                        "right, eh"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in Canadian and British English"
                        }
                    ]
                },
                {
                    "sense_id": "5e9a4388-5ce0-3641-9cb7-c0e89752cfbd",
                    "sense_definition": "In isolation, expressing surprise or confusion.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "surprise",
                    "name_type": "no",
                    "examples": [

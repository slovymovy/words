            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fbcd2bbd-d6c8-3424-b368-3b93cb5a9989",
                    "sense_definition": "Umieszczać przedmioty w walizce, torbie, pudełku lub innym pojemniku w celu ich przechowania, transportu lub wysyłki.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "packing_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę dzisiaj <pakować> walizki, ponieważ jutro rano wylatuję na wakacje.",
                            "target_lang_translations": {
                                "ru": "Мне нужно сегодня <паковать> чемоданы, так как завтра утром я улетаю в отпуск."
                            }
                        },
                        {
                            "text": "Sprzedawca zaczął starannie <pakować> zakupiony prezent w ozdobny papier.",
                            "target_lang_translations": {
                                "ru": "Продавец начал аккуратно <упаковывать> купленный подарок в декоративную бумагу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pakować walizki",
                        "pakować plecak",
                        "pakować zakupy",
                        "pakować prezent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать предметы в чемодан, сумку, коробку или другую емкость для хранения или транспортировки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паковать",
                                "target_lang_sense_clarification": "укладывать вещи"
                            },
                            {
                                "target_lang_word": "укладывать",
                                "target_lang_sense_clarification": "размещать внутри чего-либо"
                            },
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "готовить багаж (напр. собирать чемодан)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "136e1ef1-e23d-39f6-9d5d-ca3fe0660c2e",
                    "sense_definition": "Przygotowywać swoje rzeczy do podróży, układając je w bagażu (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "packing_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zacznij <pakować> się już teraz, żebyśmy nie spóźnili się na pociąg.",
                            "target_lang_translations": {
                                "ru": "Начни <собираться> уже сейчас, чтобы мы не опоздали на поезд."
                            }
                        },
                        {
                            "text": "Nie lubię <pakować> się na ostatnią chwilę, bo zawsze o czymś zapominam.",
                            "target_lang_translations": {
                                "ru": "Я не люблю <собираться> в последнюю минуту, потому что всегда о чем-то забываю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pakować się na wyjazd",
                        "pakować się do drogi",
                        "szybko się pakować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовиться к поездке, складывая свои вещи в багаж (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собираться",
                                "target_lang_sense_clarification": "готовиться к отъезду"
                            },

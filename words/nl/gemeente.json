                    "sense_id": "d7f82c35-a29e-3460-a466-ab6bb2e72a12",
                    "sense_definition": "Een gebied met een eigen bestuur, zoals een stad of een groep dorpen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ga naar de <w>gemeente</w> om mijn nieuwe paspoort op te halen.",
                            "target_lang_translations": {
                                "en": "I am going to the <w>municipality</w> to pick up my new passport."
                            }
                        },
                        {
                            "text": "In onze <w>gemeente</w> wordt het huisvuil elke maandag opgehaald.",
                            "target_lang_translations": {
                                "en": "In our <w>municipality</w>, household waste is collected every Monday."
                            }
                        }
                    ],
                    "common_phrases": [
                        "naar de gemeente",
                        "de gemeente Amsterdam",
                        "het bestuur van de gemeente"
                    ],
                    "target_lang_definitions": {
                        "en": "An area with its own administration, such as a city or a group of villages."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "municipality",
                                "target_lang_sense_clarification": "administrative division"
                            }
                        ]

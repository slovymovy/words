    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ec50c8e0-d5c3-30f8-93f8-ddac815a888b",
                    "sense_definition": "The showing of a film or video, typically by projecting it on a screen for an audience.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "film_showing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We attended a private <w>screening</w> of the documentary before its official release.",
                            "target_lang_translations": {
                                "ru": "Мы посетили закрытый <w>показ</w> документального фильма перед его официальным выходом."
                            }
                        },
                        {
                            "text": "The cinema scheduled a late-night <w>screening</w> of the classic horror movie.",
                            "target_lang_translations": {
                                "ru": "Кинотеатр запланировал ночной <w>сеанс</w> классического фильма ужасов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "film screening",
                        "private screening",
                        "public screening",
                        "special screening"
                    ],
                    "target_lang_definitions": {
                        "ru": "Показ фильма или видеоматериала, обычно на большом экране для зрителей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "показ",
                                "target_lang_sense_clarification": "демонстрация фильма"
                            },
                            {
                                "target_lang_word": "просмотр",
                                "target_lang_sense_clarification": "ознакомление с фильмом"
                            },
                            {
                                "target_lang_word": "сеанс",
                                "target_lang_sense_clarification": "время показа в кинотеатре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c13bec64-ed8a-34e9-a2d8-fa43b6ed648d",
                    "sense_definition": "The process of checking people, luggage, or items to detect problems, dangers, or illegal substances.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "examination_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Passengers must go through security <w>screening</w> before entering the departure gate.",
                            "target_lang_translations": {
                                "ru": "Пассажиры должны пройти <w>досмотр</w> службы безопасности перед выходом на посадку."
                            }
                        },
                        {
                            "text": "The airport introduced stricter baggage <w>screening</w> to ensure passenger safety.",
                            "target_lang_translations": {
                                "ru": "Аэропорт ввел более строгий <w>досмотр</w> багажа для обеспечения безопасности пассажиров."
                            }
                        }
                    ],
                    "common_phrases": [
                        "security screening",
                        "airport screening",
                        "baggage screening"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс проверки людей, багажа или предметов для выявления угроз или запрещенных веществ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "досмотр",
                                "target_lang_sense_clarification": "официальная проверка службой безопасности"
                            },
                            {
                                "target_lang_word": "проверка",
                                "target_lang_sense_clarification": "общий контроль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6cb4fec8-edc2-3618-a6b9-06a63cde3061",
                    "sense_definition": "A medical test or examination performed on a group of people to detect a disease before symptoms appear.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "examination_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Regular <w>screening</w> is important for the early detection of certain types of cancer.",
                            "target_lang_translations": {
                                "ru": "Регулярный <w>скрининг</w> важен для раннего выявления определенных видов рака."
                            }
                        },
                        {
                            "text": "The clinic offers free health <w>screening</w> for residents over the age of fifty.",
                            "target_lang_translations": {
                                "ru": "Клиника предлагает бесплатное медицинское <w>обследование</w> для жителей старше пятидесяти лет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cancer screening",
                        "health screening",
                        "routine screening",
                        "genetic screening"
                    ],
                    "target_lang_definitions": {
                        "ru": "Медицинское обследование группы людей для выявления заболеваний на ранней стадии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скрининг",
                                "target_lang_sense_clarification": "массовое обследование"
                            },
                            {
                                "target_lang_word": "обследование",
                                "target_lang_sense_clarification": "медицинский осмотр"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a764269-53a7-323b-a982-6df70c774a81",
                    "sense_definition": "The process of evaluating job applicants or candidates to determine their suitability for a position.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "examination_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The HR department handles the initial <w>screening</w> of all resumes received.",
                            "target_lang_translations": {
                                "ru": "Отдел кадров занимается первичным <w>отбором</w> всех полученных резюме."
                            }
                        },
                        {
                            "text": "After a phone <w>screening</w>, only five candidates were invited for an in-person interview.",
                            "target_lang_translations": {
                                "ru": "После телефонного <w>отбора</w> только пять кандидатов были приглашены на личное собеседование."
                            }
                        }
                    ],
                    "common_phrases": [
                        "candidate screening",
                        "pre-employment screening",
                        "phone screening",
                        "applicant screening"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс оценки кандидатов для определения их пригодности на должность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отбор",
                                "target_lang_sense_clarification": "выбор подходящих кандидатов"
                            },
                            {
                                "target_lang_word": "скрининг",
                                "target_lang_sense_clarification": "первичная оценка резюме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7bf45257-f981-3ab2-8f39-c80510d59cd6",
                    "sense_definition": "Mesh material made of wire or plastic used to cover openings like windows or doors to keep insects out.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "material_mesh",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to buy some new <w>screening</w> to fix the hole in the back door.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новую <w>сетку</w>, чтобы заделать дыру в задней двери."
                            }
                        },
                        {
                            "text": "The porch was enclosed with fine <w>screening</w> to protect us from mosquitoes.",
                            "target_lang_translations": {
                                "ru": "Веранда была огорожена мелкой <w>сеткой</w> для защиты от комаров."
                            }
                        }
                    ],
                    "synonyms": [
                        "screen#Noun"
                    ],
                    "common_phrases": [
                        "window screening",
                        "wire screening",
                        "insect screening"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сетчатый материал для защиты окон или дверей от насекомых."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сетка",
                                "target_lang_sense_clarification": "материал или изделие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd6517b6-1dd3-38be-9b16-f17ee821934c",
                    "sense_definition": "The act of passing a substance through a mesh or sieve to separate particles of different sizes.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "separation_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>screening</w> of the soil removed large rocks and debris from the garden bed.",
                            "target_lang_translations": {
                                "ru": "<w>Просеивание</w> почвы удалило крупные камни и мусор с грядки."
                            }
                        },
                        {
                            "text": "Industrial <w>screening</w> is a crucial step in processing raw minerals.",
                            "target_lang_translations": {
                                "ru": "Промышленная <w>сортировка</w> является важным этапом переработки минерального сырья."
                            }
                        }
                    ],
                    "synonyms": [
                        "sieving#Noun",
                        "sifting#Noun",
                        "straining#Noun"
                    ],
                    "common_phrases": [
                        "sand screening",
                        "soil screening"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс пропускания вещества через сито для разделения частиц разного размера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просеивание",
                                "target_lang_sense_clarification": "разделение сыпучих материалов"
                            },
                            {
                                "target_lang_word": "сортировка",
                                "target_lang_sense_clarification": "разделение на фракции"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [

                    }
                },
                {
                    "sense_id": "b3d0cf9c-d4f6-326c-b8de-def1e6069d38",
                    "sense_definition": "A premeditated murder, usually committed for money or political reasons.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "crime",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Police believe the murder was a professional <w>hit</w> ordered by a rival gang.",
                            "target_lang_translations": {
                                "ru": "Полиция считает, что убийство было профессиональным <w>заказом</w>, организованным конкурирующей бандой."
                            }
                        },
                        {
                            "text": "The detective investigated who might have paid for the <w>hit</w>.",
                            "target_lang_translations": {
                                "ru": "Детектив расследовал, кто мог заплатить за <w>убийство</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "assassination",
                        "murder"
                    ],
                    "common_phrases": [
                        "order a hit"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "criminal context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Преднамеренное убийство, обычно совершаемое за деньги или по политическим мотивам."

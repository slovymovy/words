        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "c9d09392-2107-3568-95d6-9f1cc94e70c9",
                    "sense_definition": "A female given name.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "person_name_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My sister <w>Victoria</w> is coming to visit us next week.",
                            "target_lang_translations": {
                                "ru": "Моя сестра <w>Виктория</w> приедет навестить нас на следующей неделе."
                            }
                        },
                        {
                            "text": "Queen <w>Victoria</w> is one of the most famous monarchs in British history.",
                            "target_lang_translations": {
                                "ru": "Королева <w>Виктория</w> — один из самых известных монархов в истории Великобритании."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Queen Victoria"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Виктория",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20b1f23c-2f7d-389a-bdbb-bed40526c391",
                    "sense_definition": "A state in southeastern Australia, with Melbourne as its capital.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "place_name_group",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They decided to move to <w>Victoria</w> to enjoy the coastal lifestyle.",
                            "target_lang_translations": {
                                "ru": "Они решили переехать в <w>Викторию</w>, чтобы насладиться жизнью на побережье."
                            }
                        },
                        {
                            "text": "The bushfires in <w>Victoria</w> were a major news story last summer.",
                            "target_lang_translations": {
                                "ru": "Лесные пожары в <w>Виктории</w> стали главной новостью прошлым летом."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Штат на юго-востоке Австралии со столицей в Мельбурне."

{
    "word_family": [
        "nagranie",
        "nagrywarka"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "13061c07-b4f3-3a7f-aab5-48c7ab9a2a46",
                    "sense_definition": "Utrwalać dźwięk, obraz lub informacje na nośniku danych, aby móc je później odtworzyć.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "recording",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <nagrywać> ten wykład, żeby później zrobić z niego dokładne notatki.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <записывать> эту лекцию, чтобы потом сделать по ней подробные заметки."
                            }
                        },
                        {
                            "text": "Zespół wszedł do studia, aby <nagrywać> swój najnowszy album muzyczny.",
                            "target_lang_translations": {
                                "ru": "Группа вошла в студию, чтобы <записывать> свой новый музыкальный альбом."
                            }
                        }
                    ],
                    "synonyms": [
                        "rejestrować",
                        "utrwalać"
                    ],
                    "common_phrases": [
                        "nagrywać film",
                        "nagrywać płytę",
                        "nagrywać rozmowę",
                        "nagrywać wideo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сохранять звук, изображение или информацию на носителе данных для последующего воспроизведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "записывать",
                                "target_lang_sense_clarification": "аудио, данные"
                            },
                            {
                                "target_lang_word": "снимать",
                                "target_lang_sense_clarification": "видео, фильм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37f0caf9-495d-38fc-89ea-ef7b642f2675",
                    "sense_definition": "Rejestrować swój własny głos lub wizerunek, często w celu zostawienia wiadomości lub sprawdzenia efektu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "recording",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie lubię <nagrywać> się na pocztę głosową, bo mój głos brzmi wtedy dziwnie.",
                            "target_lang_translations": {
                                "ru": "Я не люблю <записывать> свой голос на автоответчик, потому что он звучит странно."
                            }
                        },
                        {
                            "text": "Aktorzy często muszą się <nagrywać> podczas prób, aby poprawić swoją grę.",
                            "target_lang_translations": {
                                "ru": "Актеры часто должны <записывать> себя во время репетиций, чтобы улучшить свою игру."
                            }
                        }
                    ],
                    "synonyms": [
                        "rejestrować"
                    ],
                    "common_phrases": [
                        "nagrywać się na pocztę",
                        "nagrywać się telefonem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Записывать собственный голос или изображение, часто чтобы оставить сообщение или проверить результат."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "записывать",
                                "target_lang_sense_clarification": "себя, свой голос"
                            },
                            {
                                "target_lang_word": "наговаривать",
                                "target_lang_sense_clarification": "сообщение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c1176de-c418-34a0-a1c9-20870eb41393",
                    "sense_definition": "Załatwiać lub organizować coś dla kogoś, często nieoficjalnie lub dzięki znajomościom.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "arranging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat próbuje mi <nagrywać> dobrą pracę w firmie swojego kolegi.",
                            "target_lang_translations": {
                                "ru": "Мой брат пытается <устроить> мне хорошую работу в фирме своего коллеги."
                            }
                        },
                        {
                            "text": "On zawsze potrafi <nagrywać> bilety na koncerty, które są już dawno wyprzedane.",
                            "target_lang_translations": {
                                "ru": "Он всегда умеет <доставать> билеты на концерты, которые уже давно распроданы."
                            }
                        }
                    ],
                    "synonyms": [
                        "załatwiać"
                    ],
                    "common_phrases": [
                        "nagrywać robotę",
                        "nagrywać interesy",
                        "nagrywać temat"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устраивать или организовывать что-то для кого-то, часто неофициально или по знакомству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устраивать",
                                "target_lang_sense_clarification": "работу, встречу"
                            },
                            {
                                "target_lang_word": "доставать",
                                "target_lang_sense_clarification": "билеты, дефицит"
                            },
                            {
                                "target_lang_word": "подгонять",
                                "target_lang_sense_clarification": "разг. организовывать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
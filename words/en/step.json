                {
                    "sense_id": "71738c88-043b-3d4b-8b43-8c5b598a3ea2",
                    "sense_definition": "The interval between two adjacent notes in a musical scale; a tone or semitone.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "music_theory",
                    "name_type": "no",
                    "examples": [

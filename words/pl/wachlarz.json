    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "26392613-9806-30a6-963f-bab199591bba",
                    "sense_definition": "Ręczny przedmiot służący do wytwarzania ruchu powietrza w celu chłodzenia twarzy, często składany w kształt półkola.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <w>wachlarz</w> został wyprodukowany w Japonii i jest bardzo cenny.",
                            "target_lang_translations": {
                                "ru": "Этот <w>веер</w> был произведен в Японии и очень ценен."
                            }
                        },
                        {
                            "text": "W upalny dzień starsza pani chłodziła się kolorowym <w>wachlarzem</w>.",
                            "target_lang_translations": {
                                "ru": "В жаркий день пожилая дама обмахивалась цветным <w>веером</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wachel"
                    ],
                    "common_phrases": [
                        "rozłożyć wachlarz",
                        "machać wachlarzem",
                        "składany wachlarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ручной предмет, используемый для создания потока воздуха с целью охлаждения лица, часто складывающийся в форме полукруга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "веер",
                                "target_lang_sense_clarification": "предмет для обмахивания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df0bde1d-6795-34e7-af1f-fe077ede2e10",
                    "sense_definition": "Duża różnorodność lub szeroki zakres dostępnych elementów, opcji lub zjawisk.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_range",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza firma oferuje klientom pełny <w>wachlarz</w> usług ubezpieczeniowych.",
                            "target_lang_translations": {
                                "ru": "Наша фирма предлагает клиентам полный <w>спектр</w> страховых услуг."
                            }
                        },
                        {
                            "text": "Mamy do wyboru szeroki <w>wachlarz</w> możliwości rozwoju kariery zawodowej.",
                            "target_lang_translations": {
                                "ru": "У нас есть широкий <w>диапазон</w> возможностей для развития карьеры."
                            }
                        }
                    ],
                    "synonyms": [
                        "wachel",
                        "drafting",
                        "jazda na wachlarzu"
                    ],
                    "common_phrases": [
                        "szeroki wachlarz",
                        "wachlarz możliwości",
                        "wachlarz usług",
                        "pełny wachlarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое разнообразие или широкий диапазон доступных элементов, вариантов или явлений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спектр",
                                "target_lang_sense_clarification": "многообразие"
                            },
                            {
                                "target_lang_word": "диапазон",
                                "target_lang_sense_clarification": "охват, объём"
                            },
                            {
                                "target_lang_word": "веер",
                                "target_lang_sense_clarification": "перен. множество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "834f15ce-54c1-3558-94b4-392c886cb2e5",
                    "sense_definition": "Specyficzny szyk jazdy grupy kolarzy ustawionej ukośnie do kierunku jazdy w celu osłony przed bocznym wiatrem.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "cycling_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Silny boczny wiatr zmusił zawodników do uformowania <w>wachlarza</w>.",
                            "target_lang_translations": {
                                "ru": "Сильный боковой ветер заставил гонщиков сформировать <w>веер</w>."
                            }
                        },
                        {
                            "text": "Podczas wyścigu peleton rozerwał się na mniejsze <w>wachlarze</w>.",
                            "target_lang_translations": {
                                "ru": "Во время гонки пелотон разорвался на более мелкие <w>эшелоны</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "drafting"
                    ],
                    "common_phrases": [
                        "jazda na wachlarzu",
                        "uformować wachlarz",
                        "rozerwać wachlarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специфическое построение группы велогонщиков по диагонали к направлению движения для защиты от бокового ветра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "веер",
                                "target_lang_sense_clarification": "велоспорт: построение"
                            },
                            {
                                "target_lang_word": "эшелон",
                                "target_lang_sense_clarification": "велоспорт: группа"
                            }
                        ]
                    }
                }
            ]
        }

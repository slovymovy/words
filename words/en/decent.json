                },
                {
                    "sense_id": "2816b314-9a08-35de-bb98-05c1576f1596",
                    "sense_definition": "Honest, polite, and following accepted moral standards.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "moral_social",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is a <w>decent</w> person who always tries to help her neighbors.",

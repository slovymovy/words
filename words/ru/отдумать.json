            "senses": [
                {
                    "sense_id": "e1f2c9b8-3bb7-39cc-842d-c0a87940e31b",
                    "sense_definition": "Изменить своё первоначальное решение или намерение; отказаться от того, что предполагал сделать.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "change_mind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы хотели пойти в кино, но из-за плохой погоды <w>отдумали</w>.",
                            "target_lang_translations": {
                                "en": "We wanted to go to the cinema, but because of the bad weather we <w>changed our minds</w>."
                            }
                        },
                        {
                            "text": "Он сначала согласился помочь нам с переездом, а потом внезапно <w>отдумал</w>.",
                            "target_lang_translations": {
                                "en": "He first agreed to help us with the move, but then suddenly <w>changed his mind</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "передумать"
                    ],
                    "common_phrases": [
                        "отдумать ехать",
                        "совсем отдумал"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To change one's initial decision or intention; to abandon a plan."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "change one's mind",
                                "target_lang_sense_clarification": "general meaning"
                            },
                            {
                                "target_lang_word": "decide against",
                                "target_lang_sense_clarification": "to abandon a specific plan or action"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c30a33cb-126e-349c-8068-fac97c5eafe8",
                    "sense_definition": "Тщательно продумать или обдумать что-либо, вникнуть в суть дела.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "think_through",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ему нужно было время, чтобы спокойно <w>отдумать</w> сложившуюся ситуацию.",

                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "wordt vaak gebruikt in informele spreektaal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A stupid or clumsy person.",
                        "ru": "Глупый или неуклюжий человек."
                    },
                    "translations": {

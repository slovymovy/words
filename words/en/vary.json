    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2ae99624-045f-3ab2-ac17-b182b75775c4",
                    "sense_definition": "To change or be different according to the situation; not to stay the same.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "change_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The price of the ticket <w>varies</w> depending on when you book it.",
                            "target_lang_translations": {
                                "ru": "Цена билета <w>варьируется</w> в зависимости от того, когда вы его бронируете."
                            }
                        },
                        {
                            "text": "Symptoms of the disease can <w>vary</w> widely from person to person.",
                            "target_lang_translations": {
                                "ru": "Симптомы болезни могут сильно <w>различаться</w> у разных людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "fluctuate",
                        "change"
                    ],
                    "common_phrases": [
                        "vary widely",
                        "vary from",
                        "vary between",
                        "vary depending on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Меняться или отличаться в зависимости от ситуации; не оставаться неизменным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "варьироваться",
                                "target_lang_sense_clarification": "изменяться в определенных пределах"
                            },
                            {
                                "target_lang_word": "различаться",
                                "target_lang_sense_clarification": "иметь отличия друг от друга"
                            },
                            {
                                "target_lang_word": "меняться",
                                "target_lang_sense_clarification": "становиться иным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7800f10b-891a-393d-9318-78545cd6e5ab",
                    "sense_definition": "To make something different or to introduce changes to something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "modify_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should <w>vary</w> your exercise routine to avoid getting bored.",
                            "target_lang_translations": {
                                "ru": "Вам следует <w>разнообразить</w> свою программу упражнений, чтобы не заскучать."
                            }
                        },
                        {
                            "text": "The chef <w>varies</w> the menu every week to use fresh ingredients.",
                            "target_lang_translations": {
                                "ru": "Шеф-повар <w>меняет</w> меню каждую неделю, чтобы использовать свежие ингредиенты."
                            }
                        }
                    ],
                    "synonyms": [
                        "alter",
                        "change"
                    ],
                    "common_phrases": [
                        "vary the diet",
                        "vary the routine",
                        "vary the speed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-то иным или вносить изменения во что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разнообразить",
                                "target_lang_sense_clarification": "делать более разнообразным"
                            },
                            {
                                "target_lang_word": "менять",
                                "target_lang_sense_clarification": "вносить изменения"
                            },
                            {
                                "target_lang_word": "варьировать",
                                "target_lang_sense_clarification": "видоизменять"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f7b59636-ac59-3681-a9bf-de0fd9063f11",
                    "sense_definition": "Iets krijgen of in bezit nemen dat aan jou wordt gegeven of gestuurd, zoals geld, goederen of berichten.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "receive_goods",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>ontving</w> zijn salaris niet op tijd.",
                            "target_lang_translations": {
                                "en": "He did not <w>receive</w> his salary on time."
                            }
                        },
                        {
                            "text": "Heb jij mijn berichtje al <w>ontvangen</w>?",
                            "target_lang_translations": {
                                "en": "Have you <w>received</w> my message yet?"
                            }
                        },
                        {
                            "text": "De winnaar <w>ontving</w> een gouden medaille voor zijn prestatie.",
                            "target_lang_translations": {
                                "en": "The winner <w>received</w> a gold medal for his performance."
                            }
                        }
                    ],
                    "synonyms": [
                        "krijgen",
                        "verkrijgen",
                        "in ontvangst nemen"
                    ],
                    "antonyms": [
                        "versturen",
                        "verzenden",
                        "geven"
                    ],
                    "common_phrases": [
                        "een brief ontvangen",
                        "geld ontvangen",
                        "hulp ontvangen",
                        "een signaal ontvangen"
                    ],
                    "target_lang_definitions": {
                        "en": "To get or take possession of something given or sent to you, such as money, goods, or messages."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "receive",
                                "target_lang_sense_clarification": "to be given or sent something"
                            },
                            {
                                "target_lang_word": "get",
                                "target_lang_sense_clarification": "to obtain or receive (informal)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb8ccf2a-1d1a-3388-8a5d-e199b04d3d51",
                    "sense_definition": "Gasten verwelkomen of toelaten in een ruimte.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "receive_guests",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij werden er als vorsten <w>ontvangen</w>.",
                            "target_lang_translations": {
                                "en": "We were <w>received</w> there like royalty."
                            }
                        },
                        {
                            "text": "Zij <w>ontvangt</w> haar vrienden altijd met koffie en taart.",
                            "target_lang_translations": {
                                "en": "She always <w>welcomes</w> her friends with coffee and cake."
                            }
                        }
                    ],
                    "synonyms": [
                        "verwelkomen",
                        "onthalen",
                        "binnenlaten"
                    ],
                    "antonyms": [
                        "wegsturen",
                        "uitzwaaien"
                    ],
                    "common_phrases": [
                        "gasten ontvangen",
                        "hartelijk ontvangen",
                        "met open armen ontvangen"
                    ],
                    "target_lang_definitions": {
                        "en": "To welcome guests or admit them into a space."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "receive",
                                "target_lang_sense_clarification": "to act as a host for guests"
                            },
                            {
                                "target_lang_word": "welcome",
                                "target_lang_sense_clarification": "to greet someone upon arrival"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7645c5bc-c5c8-3fad-a2d2-22368aef5523",
                    "sense_definition": "Voltooid deelwoord van <w>ontvangen</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>ontvangen</w> geld is niet genoeg om de rekening te betalen.",
                            "target_lang_translations": {
                                "en": "The <w>received</w> money is not enough to pay the bill."
                            }
                        },
                        {
                            "text": "De <w>ontvangen</w> goederen moeten direct worden gecontroleerd op schade.",
                            "target_lang_translations": {
                                "en": "The <w>received</w> goods must be checked for damage immediately."

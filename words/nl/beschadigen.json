{
    "word_family": [
        "beschadiging"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d8634726-b33e-3441-9509-9e01070786fe",
                    "sense_definition": "Schade toebrengen aan een object of persoon, waardoor het kapotgaat, minder goed functioneert of minder waard wordt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zware storm heeft het dak van ons huis ernstig <w>beschadigd</w>.",
                            "target_lang_translations": {
                                "ru": "Сильный шторм серьезно <w>повредил</w> крышу нашего дома."
                            }
                        },
                        {
                            "text": "Pas op dat je de lak van de auto niet <w>beschadigt</w> met je sleutels.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен, чтобы не <w>повредить</w> лак автомобиля ключами."
                            }
                        }
                    ],
                    "synonyms": [
                        "kapotmaken",
                        "vernielen",
                        "toetakelen"
                    ],
                    "antonyms": [
                        "repareren",
                        "herstellen",
                        "maken"
                    ],
                    "common_phrases": [
                        "zwaar beschadigd",
                        "licht beschadigd",
                        "onherstelbaar beschadigd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наносить ущерб объекту или человеку, приводя к поломке, ухудшению функций или потере ценности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повреждать",
                                "target_lang_sense_clarification": "наносить физический ущерб"
                            },
                            {
                                "target_lang_word": "портить",
                                "target_lang_sense_clarification": "приводить в негодность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bfd36a8-b307-3cc3-b4ee-814daf145646",
                    "sense_definition": "Nadeel toebrengen aan iets abstracts, zoals iemands goede naam, reputatie of vertrouwen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het schandaal heeft zijn imago als betrouwbare politicus blijvend <w>beschadigd</w>.",
                            "target_lang_translations": {
                                "ru": "Скандал навсегда <w>испортил</w> его имидж надежного политика."
                            }
                        },
                        {
                            "text": "Door steeds te liegen <w>beschadig</w> je het vertrouwen dat je ouders in je hebben.",
                            "target_lang_translations": {
                                "ru": "Постоянно обманывая, ты <w>подрываешь</w> доверие своих родителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "aantasten",
                        "schaden",
                        "bezoedelen"
                    ],
                    "antonyms": [
                        "herstellen",
                        "verbeteren"
                    ],
                    "common_phrases": [
                        "reputatie beschadigen",
                        "vertrouwen beschadigen",
                        "imago beschadigen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наносить вред чему-то абстрактному, например, чьей-то репутации, доброму имени или доверию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подрывать",
                                "target_lang_sense_clarification": "ослаблять (доверие, авторитет)"
                            },
                            {
                                "target_lang_word": "пятнать",
                                "target_lang_sense_clarification": "портить репутацию"
                            },
                            {
                                "target_lang_word": "вредить",
                                "target_lang_sense_clarification": "наносить ущерб интересам или имиджу"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
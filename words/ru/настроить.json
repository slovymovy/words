            "senses": [
                {
                    "sense_id": "47b96432-3c70-380a-ae3c-8d93eb05c8ab",
                    "sense_definition": "Привести техническое устройство, механизм или программу в рабочее состояние, отрегулировав их параметры.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "adjustment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне нужно <настроить> новый телефон, чтобы он работал быстрее.",
                            "target_lang_translations": {
                                "en": "I need to <set up> my new phone so it works faster."
                            }
                        },
                        {
                            "text": "Специалист помог нам <настроить> локальную сеть в офисе за один час.",
                            "target_lang_translations": {
                                "en": "The specialist helped us <configure> the local network in the office in one hour."
                            }
                        }
                    ],
                    "synonyms": [
                        "наладить",
                        "отрегулировать",
                        "установить"
                    ],
                    "antonyms": [
                        "расстроить",
                        "сбить"
                    ],
                    "common_phrases": [
                        "настроить компьютер",
                        "настроить интернет",
                        "настроить канал",
                        "настроить громкость"
                    ],
                    "target_lang_definitions": {
                        "en": "To put a technical device, mechanism, or program into a working state by adjusting its parameters."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "configure",
                                "target_lang_sense_clarification": "software, networks, or complex systems"
                            },
                            {
                                "target_lang_word": "set up",
                                "target_lang_sense_clarification": "general devices, phones, or equipment"
                            },
                            {
                                "target_lang_word": "tune",
                                "target_lang_sense_clarification": "radios, engines, or fine adjustments"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed5607ea-eed8-3af6-9a9a-99d7018e55d6",
                    "sense_definition": "Придать музыкальному инструменту правильную высоту звука и согласованность звучания.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "adjustment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Музыкант долго пытался <настроить> старую скрипку перед выступлением.",
                            "target_lang_translations": {
                                "en": "The musician tried for a long time to <tune> the old violin before the performance."
                            }
                        },
                        {
                            "text": "Чтобы пианино звучало чисто, его должен <настроить> профессионал.",
                            "target_lang_translations": {
                                "en": "For the piano to sound clear, a professional must <tune> it."
                            }
                        }
                    ],
                    "synonyms": [
                        "наладить"
                    ],
                    "antonyms": [
                        "расстроить"
                    ],
                    "common_phrases": [
                        "настроить гитару",
                        "настроить пианино",
                        "настроить рояль",
                        "настроить инструмент"

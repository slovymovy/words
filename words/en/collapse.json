{
    "word_family": [
        "collapsible",
        "collapsibility"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b3908eb0-defa-3de6-a3b9-21de69c52706",
                    "sense_definition": "To break apart and fall down suddenly, often because of a lack of support or structural weakness.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_structural_failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old wooden bridge <w>collapsed</w> into the river during the heavy storm.",
                            "target_lang_translations": {
                                "ru": "Старый деревянный мост <w>рухнул</w> в реку во время сильного шторма."
                            }
                        },
                        {
                            "text": "If you remove that supporting wall, the entire roof might <w>collapse</w>.",
                            "target_lang_translations": {
                                "ru": "Если убрать ту несущую стену, вся крыша может <w>обвалиться</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cave in",
                        "fall down",
                        "crumble"
                    ],
                    "common_phrases": [
                        "roof collapsed",
                        "building collapsed",
                        "suddenly collapse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрушиться и внезапно упасть, часто из-за отсутствия опоры или структурной слабости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рухнуть",
                                "target_lang_sense_clarification": "внезапно упасть"
                            },
                            {
                                "target_lang_word": "обвалиться",
                                "target_lang_sense_clarification": "о стенах, потолках, грунте"
                            },
                            {
                                "target_lang_word": "разрушиться",
                                "target_lang_sense_clarification": "потерять целостность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65424de7-8797-327e-87e2-879fd328a92d",
                    "sense_definition": "To fail suddenly and completely, often referring to systems, organizations, economies, or negotiations.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_structural_failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The peace talks <w>collapsed</w> after both sides refused to compromise.",
                            "target_lang_translations": {
                                "ru": "Мирные переговоры <w>провалились</w> после того, как обе стороны отказались идти на компромисс."
                            }
                        },
                        {
                            "text": "Many small businesses <w>collapsed</w> during the economic recession.",
                            "target_lang_translations": {
                                "ru": "Многие малые предприятия <w>потерпели крах</w> во время экономического спада."
                            }
                        }
                    ],
                    "synonyms": [
                        "fail",
                        "break down",
                        "crash"
                    ],
                    "common_phrases": [
                        "economy collapsed",
                        "negotiations collapsed",
                        "government collapsed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно и полностью потерпеть неудачу (о системах, организациях, экономике или переговорах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рухнуть",
                                "target_lang_sense_clarification": "об экономике, режиме"
                            },
                            {
                                "target_lang_word": "потерпеть крах",
                                "target_lang_sense_clarification": "о бизнесе, надеждах"
                            },
                            {
                                "target_lang_word": "провалиться",
                                "target_lang_sense_clarification": "о переговорах, планах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c4b4b57-78c6-3039-963c-7a3cf672543d",
                    "sense_definition": "To fall down and become unconscious or very weak, usually due to illness or exhaustion.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_health",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The marathon runner <w>collapsed</w> from exhaustion just after crossing the finish line.",
                            "target_lang_translations": {
                                "ru": "Марафонец <w>упал без сил</w> от истощения сразу после пересечения финишной черты."
                            }
                        },
                        {
                            "text": "He almost <w>collapsed</w> on the floor when he heard the shocking news.",
                            "target_lang_translations": {
                                "ru": "Он чуть не <w>осел</w> на пол, когда услышал шокирующую новость."
                            }
                        }
                    ],
                    "synonyms": [
                        "faint",
                        "pass out"
                    ],
                    "common_phrases": [
                        "collapsed from exhaustion",
                        "collapsed on the floor",
                        "nearly collapsed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упасть и потерять сознание или сильно ослабеть, обычно из-за болезни или истощения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свалиться",
                                "target_lang_sense_clarification": "разг. упасть от болезни или усталости"
                            },
                            {
                                "target_lang_word": "упасть без сил",
                                "target_lang_sense_clarification": "от изнеможения"
                            },
                            {
                                "target_lang_word": "осесть",
                                "target_lang_sense_clarification": "медленно опуститься на пол"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8fad384f-4b33-3d56-a5f4-d09b2a4ec7b4",
                    "sense_definition": "To fold something compactly into a smaller size for storage or transport.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_compression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can <w>collapse</w> the table to make more room in the kitchen.",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>сложить</w> стол, чтобы освободить место на кухне."
                            }
                        },
                        {
                            "text": "Please <w>collapse</w> the stroller before you get on the bus.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>сложите</w> коляску перед тем, как войти в автобус."
                            }
                        }
                    ],
                    "synonyms": [
                        "fold",
                        "fold up"
                    ],
                    "antonyms": [
                        "expand"
                    ],
                    "common_phrases": [
                        "collapse the chair",
                        "collapse the box",
                        "easy to collapse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компактно сложить что-либо для хранения или транспортировки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сложить",
                                "target_lang_sense_clarification": "сделать компактным (мебель)"
                            },
                            {
                                "target_lang_word": "свернуть",
                                "target_lang_sense_clarification": "скатать (палатку, ковер)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab9c7a69-9338-3f59-b6cf-6d645c9c07d4",
                    "sense_definition": "To hide the details or sub-items of a list or menu in a computer interface.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_compression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Click the minus sign to <w>collapse</w> the folder and hide its contents.",
                            "target_lang_translations": {
                                "ru": "Нажмите на знак минуса, чтобы <w>свернуть</w> папку и скрыть её содержимое."
                            }
                        },
                        {
                            "text": "Users can expand or <w>collapse</w> the navigation menu on the website.",
                            "target_lang_translations": {
                                "ru": "Пользователи могут развернуть или <w>свернуть</w> навигационное меню на сайте."
                            }
                        }
                    ],
                    "antonyms": [
                        "expand"
                    ],
                    "common_phrases": [
                        "collapse the menu",
                        "collapse all"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Скрыть детали или подпункты списка или меню в компьютерном интерфейсе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свернуть",
                                "target_lang_sense_clarification": "комп. скрыть содержимое"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "36ca7512-2bca-3da1-b185-b0870cff74e4",
                    "sense_definition": "The sudden failure, falling down, or breakdown of a structure, system, or person's health.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_structural_failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sudden <w>collapse</w> of the banking system caused panic across the country.",
                            "target_lang_translations": {
                                "ru": "Внезапный <w>крах</w> банковской системы вызвал панику по всей стране."
                            }
                        },
                        {
                            "text": "Engineers are investigating the cause of the bridge <w>collapse</w>.",
                            "target_lang_translations": {
                                "ru": "Инженеры расследуют причину <w>обрушения</w> моста."
                            }
                        }
                    ],
                    "synonyms": [
                        "breakdown",
                        "downfall",
                        "failure"
                    ],
                    "common_phrases": [
                        "economic collapse",
                        "sudden collapse",
                        "state of collapse",
                        "imminent collapse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапный отказ, падение или разрушение структуры, системы или здоровья человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крах",
                                "target_lang_sense_clarification": "финансовый, системный"
                            },
                            {
                                "target_lang_word": "обрушение",
                                "target_lang_sense_clarification": "зданий, конструкций"
                            },
                            {
                                "target_lang_word": "обвал",
                                "target_lang_sense_clarification": "цен, грунта"
                            },
                            {
                                "target_lang_word": "коллапс",
                                "target_lang_sense_clarification": "медицинский, транспортный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "85587a30-585f-333c-bb75-69704caa9e6c",
                    "sense_definition": "An initialism for <w>computed</w> <w>tomography</w>; a medical imaging technique that uses X-rays to create detailed pictures of the inside of the body.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor ordered a <w>CT</w> to check for any internal injuries.",
                            "target_lang_translations": {
                                "ru": "Врач назначил <w>КТ</w>, чтобы проверить наличие внутренних повреждений."
                            }
                        },
                        {
                            "text": "A <w>CT</w> scan is often used to examine the brain and chest.",
                            "target_lang_translations": {
                                "ru": "<w>Компьютерная томография</w> часто используется для обследования головного мозга и грудной клетки."
                            }
                        }
                    ],
                    "synonyms": [
                        "CT scan",
                        "CAT scan",
                        "computed tomography"
                    ],
                    "common_phrases": [
                        "CT scan",
                        "head CT",
                        "abdominal CT"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "initialism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от «computed tomography» (компьютерная томография); метод медицинской визуализации, использующий рентгеновские лучи для создания детальных изображений внутренних органов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "КТ",
                                "target_lang_sense_clarification": "аббревиатура"
                            },
                            {
                                "target_lang_word": "компьютерная томография",
                                "target_lang_sense_clarification": "полная форма"
                            }
                        ]
                    }
                },

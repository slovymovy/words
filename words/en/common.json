                {
                    "sense_id": "dc14c7e0-3982-3383-8b3a-4363506e0df5",
                    "sense_definition": "Mutual good or benefit shared by a group.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shared_mutual",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They worked together for the <w>common</w> good of the community.",
                            "target_lang_translations": {
                                "ru": "Они работали вместе ради <w>общего</w> блага общества."
                            }
                        },
                        {
                            "text": "The new policy was designed to promote the <w>common</w> welfare.",
                            "target_lang_translations": {
                                "ru": "Новая политика была разработана для содействия <w>всеобщему</w> благосостоянию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "common good",
                        "common weal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взаимное благо или выгода, разделяемая группой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "общее",
                                "target_lang_sense_clarification": "as in 'common good'"
                            }
                        ]
                    }
                }
            ]
        },

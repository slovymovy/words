            "pos": "noun",
            "senses": [
                {
                    "sense_id": "63728352-83e7-394a-9780-c26e681bb925",
                    "sense_definition": "wzór doskonałości, najwyższy cel dążeń lub coś, co jest uznawane za doskonałe i pozbawione wad",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "perfection_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dla tego malarza grecka rzeźba stanowiła absolutny <w>ideał</w> piękna.",
                            "target_lang_translations": {
                                "ru": "Для этого художника греческая скульптура представляла собой абсолютный <w>идеал</w> красоты."
                            }
                        },
                        {
                            "text": "Musimy dążyć do <w>ideału</w>, nawet jeśli wydaje się on nieosiągalny.",
                            "target_lang_translations": {
                                "ru": "Мы должны стремиться к <w>идеалу</w>, даже если он кажется недостижимым."
                            }
                        }
                    ],
                    "synonyms": [
                        "wzorzec",
                        "nieskazitelność",
                        "probierz"
                    ],
                    "antonyms": [
                        "niedoskonałość",
                        "ułomność",
                        "niedoróbka"
                    ],
                    "common_phrases": [
                        "niedościgły ideał",
                        "dążyć do ideału",
                        "ideał piękna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образец совершенства, высшая цель стремлений или нечто, признаваемое совершенным и лишенным недостатков"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идеал",
                                "target_lang_sense_clarification": "образец совершенства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20f81d7a-10be-378b-84d4-46973407ad05",
                    "sense_definition": "osoba posiadająca same zalety, będąca wzorem do naśladowania lub spełnieniem czyichś marzeń",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "perfection_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój mąż to prawdziwy <w>ideał</w>, na którym zawsze mogę polegać.",
                            "target_lang_translations": {
                                "ru": "Мой муж — настоящий <w>идеал</w>, на которого я всегда могу положиться."
                            }
                        },
                        {
                            "text": "Ona wciąż szuka swojego <w>ideału</w> i odrzuca każdego kandydata.",
                            "target_lang_translations": {
                                "ru": "Она всё ещё ищет свой <w>идеал</w> и отвергает каждого кандидата."
                            }
                        }
                    ],
                    "synonyms": [
                        "ucieleśnienie cnót",
                        "najdoskonalszy okaz"
                    ],
                    "common_phrases": [
                        "ideał mężczyzny",
                        "spotkać ideał",
                        "szukać ideału"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, обладающий одними достоинствами, являющийся образцом для подражания или воплощением чьих-либо мечтаний"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идеал",
                                "target_lang_sense_clarification": "совершенный человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fab51a18-d1b5-3d67-9911-0fcc12e8fe7a",
                    "sense_definition": "najwyższe wartości, zasady moralne lub cele, którymi ktoś kieruje się w życiu",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "values_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młodzi ludzie często są gotowi walczyć za swoje <w>ideały</w>.",
                            "target_lang_translations": {
                                "ru": "Молодые люди часто готовы бороться за свои <w>идеалы</w>."
                            }
                        },
                        {
                            "text": "Z biegiem lat polityk pozostał wierny swoim młodzieńczym <w>ideałom</w>.",
                            "target_lang_translations": {
                                "ru": "С годами политик остался верен своим юношеским <w>идеалам</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wierny ideałom",
                        "zdradzić ideały",
                        "walczyć o ideały"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "zwykle w liczbie mnogiej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Высшие ценности, моральные принципы или цели, которыми кто-либо руководствуется в жизни"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идеал",
                                "target_lang_sense_clarification": "нравственный принцип"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65e88cce-82d4-3dca-be8f-77daeace5640",
                    "sense_definition": "w algebrze: specjalny podzbiór pierścienia lub innej struktury, spełniający określone warunki działań",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "math_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W teorii pierścieni <w>ideał</w> odgrywa kluczową rolę przy konstrukcji struktur ilorazowych.",
                            "target_lang_translations": {
                                "ru": "В теории колец <w>идеал</w> играет ключевую роль при построении факторструктур."
                            }
                        },
                        {
                            "text": "Student musiał udowodnić na egzaminie, że ten zbiór jest <w>ideałem</w> pierwszym.",
                            "target_lang_translations": {
                                "ru": "Студент должен был доказать на экзамене, что это множество является простым <w>идеалом</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ideał pierwszy",
                        "ideał maksymalny",
                        "ideał główny"
                    ],
                    "target_lang_definitions": {
                        "ru": "В алгебре: специальное подмножество кольца или другой структуры, удовлетворяющее определенным условиям операций"
                    },
                    "translations": {
                        "ru": [
                            {

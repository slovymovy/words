    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5f8575e9-aa2c-3c51-8881-e77120132e6a",
                    "sense_definition": "Chinees deegwaar van lange, dunne ronde slierten gemaakt uit tarwemeel",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik scheurde het pakje open, deed de kruiden over de droge <w>mie</w> en begon te knagen.",
                            "target_lang_translations": {
                                "ru": "Я разорвал пакетик, высыпал приправы на сухую <w>лапшу</w> и начал грызть."
                            }
                        },
                        {
                            "text": "In dit restaurant serveren ze heerlijke gebakken <w>mie</w> met groenten en kip.",
                            "target_lang_translations": {
                                "ru": "В этом ресторане подают вкусную жареную <w>лапшу</w> с овощами и курицей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gebakken mie",
                        "Chinese mie",
                        "mie met kip"
                    ],
                    "target_lang_definitions": {
                        "ru": "Китайские макаронные изделия в виде длинных тонких нитей из пшеничной муки"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лапша",
                                "target_lang_sense_clarification": "макаронное изделие"
                            },
                            {
                                "target_lang_word": "китайская лапша",
                                "target_lang_sense_clarification": "вид лапши"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4552ce0d-6f6b-3908-8ecd-1a901cf714e3",
                    "sense_definition": "Man die een seksuele voorkeur voor mannen heeft",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij werd op het schoolplein vaak uitgescholden voor <w>mie</w> door de andere jongens.",
                            "target_lang_translations": {
                                "ru": "Другие мальчишки на школьном дворе часто обзывали его <w>педиком</w>."
                            }
                        },
                        {
                            "text": "Het woord <w>mie</w> wordt soms als scheldwoord gebruikt, maar hij trekt zich er niets van aan.",
                            "target_lang_translations": {
                                "ru": "Слово <w>педик</w> иногда используется как ругательство, но он не обращает на это внимания."
                            }
                        }
                    ],
                    "synonyms": [
                        "flikker",
                        "homo",
                        "miet"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "often pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, испытывающий сексуальное влечение к мужчинам (грубое, сленг)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "педик",
                                "target_lang_sense_clarification": "оскорбительное"
                            },
                            {
                                "target_lang_word": "гомик",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b18351d8-da5f-3693-a6a1-bcfcfd8999ed",
                    "sense_definition": "Vrouw (vaak gebruikt in een volkse of ietwat denigrerende context)",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die oude <w>mie</w> van hiernaast staat altijd voor het raam naar buiten te gluren.",
                            "target_lang_translations": {
                                "ru": "Эта старая <w>баба</w> по соседству вечно стоит у окна и глазеет на улицу."
                            }
                        },
                        {
                            "text": "Ze gedraagt zich soms als een zeurderige <w>mie</w> als ze haar zin niet krijgt.",
                            "target_lang_translations": {
                                "ru": "Она иногда ведет себя как нудная <w>баба</w>, если не получает желаемого."
                            }
                        }
                    ],
                    "synonyms": [
                        "trees"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often somewhat derogatory"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина (часто используется в просторечном или несколько уничижительном контексте)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "баба",
                                "target_lang_sense_clarification": "просторечное"
                            },
                            {
                                "target_lang_word": "тётка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4620e9aa-fc7b-3c2e-8fe0-ca1f07fdc36b",
                    "sense_definition": "The place of origin or the specific source from which something comes, especially the location where an archaeological artifact was found.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "origin_record",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The archaeologist carefully recorded the exact <w>provenience</w> of every shard found at the site.",
                            "target_lang_translations": {
                                "ru": "Археолог тщательно зафиксировал точное <w>место находки</w> каждого черепка, обнаруженного на раскопе."
                            }
                        },
                        {
                            "text": "Without knowing the <w>provenience</w> of the rock sample, the geological data is useless.",
                            "target_lang_translations": {
                                "ru": "Без знания <w>происхождения</w> образца горной породы геологические данные бесполезны."
                            }
                        }
                    ],
                    "synonyms": [
                        "origin",
                        "source",
                        "findspot"
                    ],
                    "antonyms": [
                        "destination"
                    ],
                    "common_phrases": [
                        "exact provenience",
                        "unknown provenience",
                        "place of provenience"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место происхождения или конкретный источник, откуда что-либо появилось, особенно место обнаружения археологической находки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "происхожде́ние",
                                "target_lang_sense_clarification": "начало, источник чего-либо"
                            },
                            {
                                "target_lang_word": "ме́сто нахо́дки",
                                "target_lang_sense_clarification": "в археологии"
                            },
                            {
                                "target_lang_word": "исто́чник",
                                "target_lang_sense_clarification": "откуда что-либо берется"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5ffcb71-9f64-3639-8f29-400dd2007b67",
                    "sense_definition": "The history of ownership or the record of the background of an object, often used to verify authenticity.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "origin_record",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum refused to purchase the painting because its <w>provenience</w> was unclear.",
                            "target_lang_translations": {
                                "ru": "Музей отказался приобретать картину, поскольку её <w>провенанс</w> был неясен."
                            }
                        },
                        {
                            "text": "Collectors place a high value on items with a documented <w>provenience</w> dating back centuries.",
                            "target_lang_translations": {
                                "ru": "Коллекционеры высоко ценят предметы с документально подтверждённым <w>происхождением</w>, насчитывающим столетия."
                            }
                        }
                    ],
                    "synonyms": [
                        "provenance",
                        "history",
                        "background"
                    ],
                    "common_phrases": [
                        "documented provenience",
                        "uncertain provenience",
                        "record of provenience"
                    ],
                    "target_lang_definitions": {
                        "ru": "История владения предметом или запись о его происхождении, часто используемая для подтверждения подлинности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "провена́нс",
                                "target_lang_sense_clarification": "история владения предметом искусства"
                            },
                            {
                                "target_lang_word": "происхожде́ние",
                                "target_lang_sense_clarification": "история создания и бытования"
                            },
                            {
                                "target_lang_word": "исто́рия владе́ния",
                                "target_lang_sense_clarification": "последовательность владельцев"
                            }
                        ]
                    }
                }
            ]
        }
    ]

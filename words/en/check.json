                    "target_lang_definitions": {
                        "ru": "Узор, состоящий из квадратов чередующихся цветов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клетка",
                                "target_lang_sense_clarification": "узор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f0d87e6-433d-32e1-81d2-c9d20a30c80e",

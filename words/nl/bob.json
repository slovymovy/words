                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten afspreken wie vanavond de <w>bob</w> is.",
                            "target_lang_translations": {

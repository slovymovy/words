    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2cdf14c0-45ed-3272-b49f-4d40a1577786",
                    "sense_definition": "Działalność polegająca na masowym kupowaniu określonych produktów, surowców lub płodów rolnych od wielu sprzedawców, często w celu dalszej odsprzedaży lub przetwórstwa.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_skup_trade",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszej okolicy prowadzony jest <w>skup</w> owoców miękkich.",
                            "target_lang_translations": {
                                "ru": "В нашей округе ведется <w>скупка</w> ягод."
                            }
                        },
                        {
                            "text": "Rolnicy narzekają na niskie ceny w <w>skupie</w> zboża.",
                            "target_lang_translations": {
                                "ru": "Фермеры жалуются на низкие цены на <w>заготовке</w> зерна."
                            }
                        }
                    ],
                    "synonyms": [
                        "nabywanie",
                        "zakup"
                    ],
                    "antonyms": [
                        "sprzedaż"
                    ],
                    "common_phrases": [
                        "skup złomu",
                        "skup walut",
                        "skup żywca",
                        "punkt skupu",
                        "prowadzić skup"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деятельность, заключающаяся в массовой закупке определенных продуктов, сырья или сельскохозяйственных культур у множества продавцов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скупка",
                                "target_lang_sense_clarification": "действие, массовое приобретение"
                            },
                            {
                                "target_lang_word": "заготовка",
                                "target_lang_sense_clarification": "сельскохозяйственной продукции или сырья"
                            },
                            {
                                "target_lang_word": "закупка",
                                "target_lang_sense_clarification": "оптовое приобретение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9fe382bb-a147-3687-8b5c-e0c635cd63d4",
                    "sense_definition": "Miejsce lub punkt usługowy, w którym odbywa się przyjmowanie i kupowanie określonych towarów od dostawców.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_skup_trade",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę zawieźć stare gazety do pobliskiego <w>skupu</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно отвезти старые газеты в ближайший <w>пункт приема</w>."
                            }
                        },
                        {
                            "text": "Ten <w>skup</w> jest otwarty tylko w dni robocze do godziny piętnastej.",
                            "target_lang_translations": {
                                "ru": "Этот <w>пункт приема</w> открыт только в рабочие дни до пятнадцати часов."
                            }
                        }
                    ],
                    "synonyms": [
                        "punkt skupu"
                    ],
                    "common_phrases": [
                        "iść do skupu",
                        "pracować w skupie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место или пункт обслуживания, где происходит прием и покупка определенных товаров от поставщиков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пункт приема",
                                "target_lang_sense_clarification": "место сдачи вторсырья или товаров"
                            },
                            {
                                "target_lang_word": "скупка",
                                "target_lang_sense_clarification": "разговорное название места (напр. ломбард)"
                            },
                            {
                                "target_lang_word": "заготовительный пункт",
                                "target_lang_sense_clarification": "официальное название пункта сбора"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c7b41112-2019-3320-95e2-2663852a5c80",
                    "sense_definition": "Druga osoba liczby pojedynczej trybu rozkazującego czasownika <w>skupić</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Skup</w> się na swoim zadaniu i nie rozpraszaj innych.",
                            "target_lang_translations": {
                                "ru": "<w>Сосредоточься</w> на своем задании и не отвлекай других."
                            }
                        },
                        {
                            "text": "<w>Skup</w> całą swoją uwagę na tym, co teraz powiem.",
                            "target_lang_translations": {
                                "ru": "<w>Сосредоточь</w> все свое внимание на том, что я сейчас скажу."
                            }
                        }

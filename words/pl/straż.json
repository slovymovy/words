{
    "word_family": [
        "strażak",
        "strażnik",
        "strażnica",
        "strzec"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "83c7bce8-1a97-388d-a83a-0865fa1f14d4",
                    "sense_definition": "Potoczne określenie straży pożarnej; zorganizowana służba ratownicza zajmująca się gaszeniem pożarów i pomocą w nagłych wypadkach.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fire_brigade",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąsiedzi natychmiast wezwali <straż>, gdy zobaczyli gęsty dym.",
                            "target_lang_translations": {
                                "ru": "Соседи немедленно вызвали <пожарных>, когда увидели густой дым."
                            }
                        },
                        {
                            "text": "<Straż> przyjechała na miejsce wypadku w ciągu kilku minut.",
                            "target_lang_translations": {
                                "ru": "<Пожарная охрана> прибыла на место происшествия в течение нескольких минут."
                            }
                        }
                    ],
                    "common_phrases": [
                        "straż pożarna",
                        "wezwać straż",
                        "wóz straży"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название пожарной охраны; организованная спасательная служба, занимающаяся тушением пожаров и помощью в чрезвычайных ситуациях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пожарная охрана",
                                "target_lang_sense_clarification": "организация"
                            },
                            {
                                "target_lang_word": "пожарные",
                                "target_lang_sense_clarification": "люди, сотрудники службы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "828c575e-b727-3249-986b-d9389509a547",
                    "sense_definition": "Grupa osób lub osoba wyznaczona do pilnowania, ochrony kogoś lub czegoś oraz dbania o porządek.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "guard_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed wejściem do pałacu stoi uzbrojona <straż>.",
                            "target_lang_translations": {
                                "ru": "Перед входом во дворец стоит вооружённая <стража>."
                            }
                        },
                        {
                            "text": "Nocna <straż> regularnie patroluje teren całej fabryki.",
                            "target_lang_translations": {
                                "ru": "Ночная <охрана> регулярно патрулирует территорию всего завода."
                            }
                        }
                    ],
                    "synonyms": [
                        "warta"
                    ],
                    "common_phrases": [
                        "straż graniczna",
                        "straż miejska",
                        "zmiana straży"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей или лицо, назначенное для охраны кого-либо или чего-либо, а также для поддержания порядка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "охрана",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "стража",
                                "target_lang_sense_clarification": "вооруженная или почетная, часто историч."
                            },
                            {
                                "target_lang_word": "караул",
                                "target_lang_sense_clarification": "военная охрана"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be9b1702-2df8-3a9b-ae94-2925e1088aac",
                    "sense_definition": "Czynność polegająca na czuwaniu nad bezpieczeństwem i gotowości do obrony przed zagrożeniem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "guard_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wierny pies pełnił <straż> przy domu swojego pana przez całą noc.",
                            "target_lang_translations": {
                                "ru": "Верный пёс нёс <караул> у дома своего хозяина всю ночь."
                            }
                        },
                        {
                            "text": "Wszyscy obywatele powinni stać na <straży> demokracji i wolności.",
                            "target_lang_translations": {
                                "ru": "Все граждане должны стоять на <страже> демократии и свободы."
                            }
                        }
                    ],
                    "synonyms": [
                        "warta"
                    ],
                    "common_phrases": [
                        "stać na straży",
                        "pełnić straż",
                        "trzymać straż"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, заключающееся в наблюдении за безопасностью и готовности к защите от угрозы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стража",
                                "target_lang_sense_clarification": "в выражении «стоять на страже»"
                            },
                            {
                                "target_lang_word": "караул",
                                "target_lang_sense_clarification": "несение службы по охране"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a5533f70-3e86-32f7-8d41-68db368c4217",
                    "sense_definition": "Переместить кого-либо или что-либо с одного места на другое, взяв в руки или используя транспорт.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец помог мне <перенести> тяжёлый диван в другую комнату."
                        },
                        {
                            "text": "Пострадавшего пришлось <перенести> в машину скорой помощи на носилках."
                        }
                    ],
                    "common_phrases": [
                        "перенести вещи",
                        "перенести через дорогу",
                        "перенести на руках"
                    ]
                },
                {
                    "sense_id": "e5e9e3ab-cfe3-3e9c-89c3-c7a3aaefc57c",
                    "sense_definition": "Назначить другое время или дату для какого-либо события; отложить на более поздний срок.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за сильного дождя мы решили <перенести> пикник на следующие выходные."
                        },
                        {
                            "text": "Директор попросил <перенести> встречу на два часа дня."
                        }
                    ],
                    "common_phrases": [
                        "перенести встречу",
                        "перенести на завтра",
                        "перенести сроки"
                    ]
                },
                {
                    "sense_id": "4e631937-42a8-36af-8423-227b1b1f3061",
                    "sense_definition": "Выдержать, пережить что-либо тяжёлое, неприятное или болезненное (болезнь, горе, трудности).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пациент хорошо <перенёс> сложную операцию на сердце."
                        },
                        {
                            "text": "Ей было очень трудно <перенести> разлуку с семьёй."
                        }
                    ],
                    "synonyms": [
                        "вынести",
                        "вытерпеть"
                    ],
                    "common_phrases": [
                        "перенести операцию",
                        "перенести болезнь",
                        "тяжело перенести"
                    ]
                },
                {
                    "sense_id": "5d26fea9-e086-3e16-b2cd-840506012111",
                    "sense_definition": "Разделить слово для написания его части на следующей строке по правилам правописания.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель объяснил, как правильно <перенести> это длинное слово."
                        },
                        {
                            "text": "Если слово не помещается на строке, его нужно <перенести>."
                        }
                    ],
                    "common_phrases": [
                        "перенести слово"
                    ]
                },
                {
                    "sense_id": "d356dd4b-2fc9-378c-a818-7846de06d9a0",
                    "sense_definition": "Воспроизвести что-либо в другом месте или в другой форме (скопировать рисунок, переместить данные, распространить чувства).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Художник смог точно <перенести> красоту пейзажа на холст."
                        },
                        {
                            "text": "Психолог объяснил, что нельзя <перенести> свои страхи на ребёнка."
                        }
                    ],
                    "synonyms": [
                        "спроецировать"
                    ],
                    "common_phrases": [
                        "перенести данные",
                        "перенести рисунок",
                        "перенести опыт"
                    ]
                }
            ]
        }
    ]

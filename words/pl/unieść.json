            "senses": [
                {
                    "sense_id": "1e1f3b06-6fde-3943-a4be-6a46e53b892b",
                    "sense_definition": "Skierować coś lub kogoś w górę; sprawić, że znajdzie się wyżej.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement_up",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uczeń chciał <unieść> rękę, aby odpowiedzieć na pytanie nauczyciela.",
                            "target_lang_translations": {
                                "ru": "Ученик хотел <поднять> руку, чтобы ответить на вопрос учителя."
                            }
                        },
                        {
                            "text": "Pudło było tak ciężkie, że nie byłem w stanie go samodzielnie <unieść>.",
                            "target_lang_translations": {
                                "ru": "Коробка была такой тяжелой, что я не был в состоянии самостоятельно её <поднять>."
                            }
                        }
                    ],
                    "synonyms": [
                        "podnieść",
                        "podnieść"
                    ],
                    "antonyms": [
                        "opuścić"
                    ],
                    "common_phrases": [
                        "unieść rękę",
                        "unieść głowę",
                        "unieść brwi",
                        "unieść wzrok"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направить что-то или кого-то вверх; сделать так, чтобы объект оказался выше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поднять",
                                "target_lang_sense_clarification": "переместить вверх"
                            },
                            {
                                "target_lang_word": "приподнять",
                                "target_lang_sense_clarification": "немного поднять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8daeeff5-711d-3074-9f08-a4191ece2874",
                    "sense_definition": "Zmienić swoją pozycję na wyższą, wstać lub wzlecieć w górę (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement_up",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samolot rozpędził się na pasie startowym i po chwili gładko <uniósł> się w powietrze.",
                            "target_lang_translations": {
                                "ru": "Самолет разогнался на взлетной полосе и через мгновение плавно <поднялся> в воздух."
                            }
                        },
                        {
                            "text": "Mężczyzna powoli <uniósł> się z fotela, aby przywitać wchodzących gości.",
                            "target_lang_translations": {
                                "ru": "Мужчина медленно <приподнялся> с кресла, чтобы поприветствовать вошедших гостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "podnieść",
                        "podnieść",
                        "podnieść"
                    ],
                    "antonyms": [
                        "opuścić",
                        "opuścić",
                        "opuścić"
                    ],
                    "common_phrases": [
                        "unieść się w powietrze",
                        "unieść się z miejsca",
                        "mgła się uniosła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменить свое положение на более высокое, встать или взлететь вверх (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подняться",
                                "target_lang_sense_clarification": "встать или переместиться вверх"
                            },
                            {
                                "target_lang_word": "взмыть",
                                "target_lang_sense_clarification": "стремительно подняться в воздух"
                            },
                            {
                                "target_lang_word": "привстать",
                                "target_lang_sense_clarification": "немного подняться с места"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8bdd6ab-9485-378e-8a69-2ac072c8c663",
                    "sense_definition": "Zdołać utrzymać duży ciężar fizyczny lub poradzić sobie z trudną sytuacją psychiczną albo odpowiedzialnością.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "endurance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Obawiam się, że ten stary drewniany most może nie <unieść> ciężaru ciężarówki.",
                            "target_lang_translations": {
                                "ru": "Боюсь, что этот старый деревянный мост может не <выдержать> веса грузовика."
                            }
                        },
                        {
                            "text": "Młody pracownik nie potrafił <unieść> odpowiedzialności za tak duży i skomplikowany projekt.",
                            "target_lang_translations": {
                                "ru": "Молодой сотрудник не смог <справиться> с ответственностью за такой большой и сложный проект."
                            }
                        }
                    ],
                    "synonyms": [
                        "podnieść",
                        "podnieść"
                    ],
                    "common_phrases": [
                        "unieść ciężar",
                        "unieść odpowiedzialność",
                        "unieść brzemię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Суметь удержать большой физический вес или справиться с трудной ситуацией либо ответственностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выдержать",
                                "target_lang_sense_clarification": "справиться с нагрузкой или весом"
                            },
                            {
                                "target_lang_word": "вынести",
                                "target_lang_sense_clarification": "справиться с тяжестью или испытанием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62f210e4-00e2-3141-bf27-0b9dd00adb1b",
                    "sense_definition": "Zostać zabranym i przemieszczonym przez żywioł, taki jak wiatr lub woda.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_movement_up",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Silny podmuch wiatru porwał latawiec i <uniósł> go wysoko ponad drzewa.",
                            "target_lang_translations": {
                                "ru": "Сильный порыв ветра подхватил воздушного змея и <поднял> его высоко над деревьями."
                            }
                        },
                        {
                            "text": "Wzburzona fala uderzyła w łódkę i <uniosła> ją daleko od brzegu.",
                            "target_lang_translations": {
                                "ru": "Бурная волна ударила в лодку и <унесла> её далеко от берега."
                            }
                        }
                    ],
                    "synonyms": [
                        "podnieść"
                    ],
                    "common_phrases": [
                        "wiatr uniósł",
                        "fala uniosła",
                        "prąd uniósł"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть захваченным и перемещенным стихией, например ветром или водой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "унести",
                                "target_lang_sense_clarification": "переместить прочь (ветром, течением)"
                            },
                            {
                                "target_lang_word": "подхватить",
                                "target_lang_sense_clarification": "поднять в воздух силой ветра"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3065d1a2-b58d-33df-8a77-bd935ff9029a",
                    "sense_definition": "Gwałtownie zareagować emocjonalnie, zwykle okazując gniew lub urażoną dumę (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emotional_reaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor niepotrzebnie <uniósł> się gniewem, gdy pracownik zadał mu niewygodne pytanie.",
                            "target_lang_translations": {
                                "ru": "Директор напрасно <вспылил>, когда сотрудник задал ему неудобный вопрос."
                            }
                        },
                        {
                            "text": "Zamiast przyjąć oferowaną pomoc, ona wolała <unieść> się honorem i odmówić.",
                            "target_lang_translations": {
                                "ru": "Вместо того чтобы принять предложенную помощь, она предпочла <проявить> гордость и отказаться."
                            }
                        }
                    ],
                    "common_phrases": [
                        "unieść się gniewem",
                        "unieść się honorem",
                        "unieść się dumą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бурно отреагировать эмоционально, обычно проявляя гнев или уязвленную гордость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вспылить",
                                "target_lang_sense_clarification": "внезапно рассердиться"
                            },
                            {
                                "target_lang_word": "возмутиться",
                                "target_lang_sense_clarification": "выразить крайнее недовольство"
                            }
                        ]
                    }
                }
            ]
        }
    ]

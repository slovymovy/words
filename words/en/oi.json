{
    "word_family": [],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "feaebbde-31e9-3101-a2e2-ddd3772cdd85",
                    "sense_definition": "Said to get someone's attention, often abruptly or aggressively.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "attention_call",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Oi</w>, you with the red hat, come back here!",
                            "target_lang_translations": {
                                "ru": "<w>Эй</w>, ты в красной шапке, вернись сюда!"
                            }
                        },
                        {
                            "text": "<w>Oi</w>! Stop doing that right now.",
                            "target_lang_translations": {
                                "ru": "<w>Эй</w>! Прекрати это немедленно."
                            }
                        }
                    ],
                    "synonyms": [
                        "hey",
                        "yo"
                    ],
                    "common_phrases": [
                        "oi you",
                        "oi mate"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "British, Australian"
                        },
                        {
                            "trait_type": "slang",
                            "comment": "often impolite"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Окрик, используемый для привлечения чьего-либо внимания, часто грубый или агрессивный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эй",
                                "target_lang_sense_clarification": "оклик для привлечения внимания"
                            },
                            {
                                "target_lang_word": "слышь",
                                "target_lang_sense_clarification": "грубое обращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "980c75ec-44e3-3893-82b7-49516b31f4de",
                    "sense_definition": "An informal greeting, similar to hi.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "greeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Oi</w>! How is it going with you?",
                            "target_lang_translations": {
                                "ru": "<w>Привет</w>! Как у тебя дела?"
                            }
                        },
                        {
                            "text": "<w>Oi</w>! It is good to see you again.",
                            "target_lang_translations": {
                                "ru": "<w>Здорово</w>! Рад тебя снова видеть."
                            }
                        }
                    ],
                    "synonyms": [
                        "hi",
                        "hey",
                        "hello",
                        "yo"
                    ],
                    "antonyms": [
                        "bye",
                        "goodbye"
                    ],
                    "common_phrases": [
                        "oi oi"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "British, Australian"
                        },
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неформальное приветствие, похожее на «привет»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привет",
                                "target_lang_sense_clarification": "дружеское приветствие"
                            },
                            {
                                "target_lang_word": "здорово",
                                "target_lang_sense_clarification": "просторечное приветствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55f800f3-e35d-3739-b082-d5a5bed60d26",
                    "sense_definition": "An expression of surprise or realization.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "surprise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Oi</w>! I did not expect to see that happen.",
                            "target_lang_translations": {
                                "ru": "<w>Ого</w>! Я не ожидал, что это произойдет."
                            }
                        },
                        {
                            "text": "<w>Oi</w>! This is something completely new.",
                            "target_lang_translations": {
                                "ru": "<w>Ой</w>! Это что-то новенькое."
                            }
                        }
                    ],
                    "synonyms": [
                        "blimey",
                        "wow",
                        "whoa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражение удивления или внезапного осознания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ого",
                                "target_lang_sense_clarification": "возглас удивления"
                            },
                            {
                                "target_lang_word": "ой",
                                "target_lang_sense_clarification": "возглас при неожиданности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ec052b22-d2ba-3966-a7f4-db0099613ed4",
                    "sense_definition": "A working-class punk rock subgenre of the 1970s.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "music_genre",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The band plays loud <w>Oi</w> music in the local club.",
                            "target_lang_translations": {
                                "ru": "Группа играет громкую <w>ой</w>-музыку в местном клубе."
                            }
                        },
                        {
                            "text": "Many <w>Oi</w> songs feature simple guitar riffs and chanting choruses.",
                            "target_lang_translations": {
                                "ru": "Многие песни в стиле <w>ой</w> отличаются простыми гитарными риффами и скандируемыми припевами."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поджанр панк-рока рабочего класса 1970-х годов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ой",
                                "target_lang_sense_clarification": "музыкальный жанр"

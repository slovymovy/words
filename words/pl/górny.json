            "senses": [
                {
                    "sense_id": "bc126046-d4ba-336e-a7a0-2660ed34dba8",
                    "sense_definition": "Polskie nazwisko, noszone zarówno przez kobiety, jak i mężczyzn.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Górny</w> został wczoraj wybrany na nowego przewodniczącego rady osiedla.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Гурный</w> был вчера избран новым председателем совета жилого комплекса."
                            }
                        },
                        {
                            "text": "W spisie lokatorów nie znalazłem nikogo, kto nazywałby się <w>Górny</w>.",
                            "target_lang_translations": {
                                "ru": "В списке жильцов я не нашел никого по фамилии <w>Гурный</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская фамилия, которую носят как женщины, так и мужчины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Гурный",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8098e78e-1491-3371-b015-c1a5a07c9eec",
                    "sense_definition": "Wypowiadać dźwięki mowy, słowa lub zdania w określony sposób, nadając im brzmienie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cudzoziemcy często mają problem, aby poprawnie <wymawiać> trudne polskie głoski.",
                            "target_lang_translations": {
                                "ru": "Иностранцам часто трудно правильно <произносить> сложные польские звуки."
                            }
                        },
                        {
                            "text": "Staraj się <wymawiać> końcówki wyrazów bardzo wyraźnie i dokładnie.",
                            "target_lang_translations": {
                                "ru": "Старайся <выговаривать> окончания слов очень четко и точно."
                            }
                        }
                    ],
                    "synonyms": [
                        "artykułować",
                        "wypowiadać"
                    ],
                    "common_phrases": [
                        "wymawiać wyraźnie",
                        "wymawiać poprawnie",
                        "wymawiać słowa",
                        "wymawiać nazwisko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Произносить звуки речи, слова или предложения определенным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "произносить",
                                "target_lang_sense_clarification": "говорить, озвучивать"
                            },
                            {
                                "target_lang_word": "выговаривать",
                                "target_lang_sense_clarification": "четко артикулировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af0e1ca2-1355-3a89-9a73-891b24f798e9",
                    "sense_definition": "Zrywać umowę lub oficjalne porozumienie, składając stosowne oświadczenie woli.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pracodawca zamierza <wymawiać> umowy pracownikom, którzy notorycznie łamią regulamin.",
                            "target_lang_translations": {
                                "ru": "Работодатель намерен <расторгать> договоры с сотрудниками, которые постоянно нарушают правила."
                            }
                        },
                        {
                            "text": "Zgodnie z prawem właściciel może <wymawiać> najem tylko w ściśle określonych przypadkach.",
                            "target_lang_translations": {
                                "ru": "По закону владелец может <расторгать> договор аренды только в строго определенных случаях."
                            }
                        }
                    ],
                    "synonyms": [
                        "wypowiadać"
                    ],
                    "common_phrases": [
                        "wymawiać umowę",
                        "wymawiać pracę",
                        "wymawiać mieszkanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расторгать договор или прекращать действие официального соглашения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расторгать",
                                "target_lang_sense_clarification": "аннулировать договор"
                            },
                            {
                                "target_lang_word": "прекращать",
                                "target_lang_sense_clarification": "останавливать действие соглашения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9cd69e3-efb1-388d-bd27-997284ee8e5b",
                    "sense_definition": "Unikać wykonania jakiegoś obowiązku lub zadania, podając wymówkę (używane z zaimkiem 'się').",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie możesz ciągle <wymawiać> się zmęczeniem, gdy trzeba posprzątać mieszkanie.",
                            "target_lang_translations": {
                                "ru": "Ты не можешь постоянно <отговариваться> усталостью, когда нужно убрать квартиру."
                            }
                        },
                        {
                            "text": "Próbował <wymawiać> się od wizyty u lekarza pilnymi sprawami w pracy.",
                            "target_lang_translations": {
                                "ru": "Он пытался <отговариваться> от визита к врачу срочными делами на работе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wymawiać się chorobą",
                        "wymawiać się brakiem czasu",
                        "wymawiać się od odpowiedzi"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czasownik zwrotny (wymaga 'się')"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уклоняться от выполнения обязанности, ссылаясь на какую-либо причину (используется с 'się')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отговариваться",
                                "target_lang_sense_clarification": "находить отговорки"
                            },
                            {
                                "target_lang_word": "оправдываться",
                                "target_lang_sense_clarification": "приводить оправдания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7cbd3d7-67e9-31e3-9997-7d2c1e243f1a",
                    "sense_definition": "Zwracać komuś uwagę z pretensją, przypominając o wyświadczonych przysługach lub popełnionych błędach.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To bardzo nieelegancko <wymawiać> komuś pomoc, którą ofiarowało się dobrowolnie.",
                            "target_lang_translations": {
                                "ru": "Очень некрасиво <попрекать> кого-то помощью, которую предложили добровольно."
                            }
                        },
                        {
                            "text": "W trakcie kłótni zaczęła <wymawiać> mężowi wszystkie jego dawne potknięcia.",
                            "target_lang_translations": {
                                "ru": "Во время ссоры она начала <припоминать> мужу все его старые оплошности."
                            }
                        }
                    ],
                    "synonyms": [
                        "wypominać"
                    ],
                    "common_phrases": [
                        "wymawiać komuś błędy",
                        "wymawiać przysługi",
                        "wymawiać każdy grosz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Упрекать кого-либо, напоминая об оказанных услугах или прошлых ошибках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "попрекать",
                                "target_lang_sense_clarification": "ставить в упрек помощь или услуги"
                            },
                            {
                                "target_lang_word": "припоминать",
                                "target_lang_sense_clarification": "напоминать о старых обидах"
                            }
                        ]
                    }
                }
            ]
        }

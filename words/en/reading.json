    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cfa81083-ed78-391f-bc58-e582776fae43",
                    "sense_definition": "The action of looking at and understanding the meaning of written or printed matter; the present participle and gerund of <w>read</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "action_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>reading</w> a very interesting book about history.",
                            "target_lang_translations": {
                                "ru": "Я <w>читаю</w> очень интересную книгу об истории."
                            }
                        },
                        {
                            "text": "She was <w>reading</w> the newspaper when the phone rang.",
                            "target_lang_translations": {
                                "ru": "Она <w>читала</w> газету, когда зазвонил телефон."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, заключающееся в восприятии и понимании смысла написанного или напечатанного текста; причастие настоящего времени и герундий от глагола <w>read</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чтение",
                                "target_lang_sense_clarification": "процесс"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "781e830d-1d1b-32d4-9270-a8f7002297e8",
                    "sense_definition": "A person who has unusually high intelligence or a special skill in a particular area.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is a <w>genius</w> at solving complex mathematical puzzles.",
                            "target_lang_translations": {
                                "ru": "Она <w>гений</w> в решении сложных математических головоломок."
                            }
                        },
                        {
                            "text": "The young <w>genius</w> composed his first symphony at the age of six.",
                            "target_lang_translations": {
                                "ru": "Юный <w>гений</w> сочинил свою первую симфонию в возрасте шести лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "intellectual",
                        "brain",
                        "brainiac",
                        "Einstein",
                        "thinker"
                    ],
                    "antonyms": [
                        "idiot"
                    ],
                    "common_phrases": [
                        "musical genius",
                        "math genius",
                        "evil genius",
                        "creative genius"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, обладающий необычайно высоким интеллектом или особым талантом в определенной области."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гений",
                                "target_lang_sense_clarification": "о человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b37c4d8-a787-3e87-896f-4387837ca0bf",
                    "sense_definition": "Great natural ability, intelligence, or creativity.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her latest novel is a work of <w>genius</w> that will be read for generations.",
                            "target_lang_translations": {
                                "ru": "Её последний роман — это творение <w>гения</w>, которое будут читать поколениями."
                            }
                        },
                        {
                            "text": "It took a stroke of <w>genius</w> to fix the broken machine with just a paperclip.",
                            "target_lang_translations": {
                                "ru": "Потребовалась вспышка <w>гениальности</w>, чтобы починить сломанную машину одной лишь скрепкой."
                            }

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8f5ceb90-03d2-3dfb-b439-338606d62291",
                    "sense_definition": "Een staatsvorm waarbij de macht volledig in handen is van één persoon of een kleine groep.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de staatsgreep veranderde het land in een strenge <w>dictatuur</w>.",
                            "target_lang_translations": {
                                "en": "After the coup, the country turned into a strict <w>dictatorship</w>.",
                                "ru": "После государственного переворота страна превратилась в жесткую <w>диктатуру</w>."
                            }
                        },
                        {
                            "text": "De bevolking kwam in opstand tegen de militaire <w>dictatuur</w>.",
                            "target_lang_translations": {
                                "en": "The population rose up against the military <w>dictatorship</w>.",
                                "ru": "Население восстало против военной <w>диктатуры</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "alleenheerschappij",
                        "tyrannie",
                        "totalitarisme"
                    ],
                    "antonyms": [
                        "democratie"
                    ],
                    "common_phrases": [
                        "militaire dictatuur",
                        "leven in een dictatuur"
                    ],
                    "target_lang_definitions": {
                        "en": "A form of government where power is held entirely by one person or a small group.",
                        "ru": "Форма правления, при которой власть полностью находится в руках одного человека или небольшой группы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dictatorship",
                                "target_lang_sense_clarification": "form of government"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "диктатура",
                                "target_lang_sense_clarification": "политический режим"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc029ba5-43a6-390b-938d-0639be365db1",
                    "sense_definition": "Een dwingende overheersing of invloed in een niet-politieke context.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij wilde zich niet aanpassen aan de <w>dictatuur</w> van de mode.",
                            "target_lang_translations": {
                                "en": "He did not want to conform to the <w>dictatorship</w> of fashion.",
                                "ru": "Он не хотел подстраиваться под <w>диктатуру</w> моды."
                            }
                        },
                        {
                            "text": "De bewoners zijn de <w>dictatuur</w> van de auto in hun straat beu.",
                            "target_lang_translations": {
                                "en": "The residents are fed up with the <w>dictatorship</w> of the car in their street.",
                                "ru": "Жители устали от <w>диктатуры</w> автомобилей на своей улице."
                            }
                        }
                    ],
                    "synonyms": [
                        "tyrannie"
                    ],
                    "common_phrases": [
                        "dictatuur van de meerderheid"
                    ],
                    "target_lang_definitions": {
                        "en": "Compelling domination or influence in a non-political context.",
                        "ru": "Навязываемое господство или влияние в неполитическом контексте."
                    },

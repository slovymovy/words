                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "твой",
                                "target_lang_sense_clarification": "единственное число, неформальное"
                            },
                            {
                                "target_lang_word": "ваш",
                                "target_lang_sense_clarification": "множественное число или вежливая форма"
                            },
                            {
                                "target_lang_word": "свой",
                                "target_lang_sense_clarification": "возвратное притяжательное местоимение (принадлежащий подлежащему)"
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ccea8670-5b4a-3c7c-94e7-0fe699eb8994",
                    "sense_definition": "Określona część dłuższego procesu, rozwoju lub działania, następująca po innej i poprzedzająca kolejną.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To dopiero początkowy <w>etap</w> naszych negocjacji z nowym klientem.",
                            "target_lang_translations": {
                                "ru": "Это лишь начальный <w>этап</w> наших переговоров с новым клиентом."
                            }
                        },
                        {
                            "text": "Musimy zakończyć ten <w>etap</w> prac przed nadejściem zimy.",
                            "target_lang_translations": {
                                "ru": "Мы должны завершить этот <w>этап</w> работ до наступления зимы."
                            }
                        }
                    ],
                    "synonyms": [
                        "faza",
                        "stadium",
                        "okres"
                    ],
                    "common_phrases": [
                        "nowy etap",
                        "etap końcowy",
                        "na tym etapie",
                        "etap rozwoju"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенная часть более длительного процесса, развития или действия, следующая за одной и предшествующая другой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "этап",
                                "target_lang_sense_clarification": "часть процесса"
                            },
                            {
                                "target_lang_word": "стадия",
                                "target_lang_sense_clarification": "ступень развития"
                            },
                            {
                                "target_lang_word": "фаза",
                                "target_lang_sense_clarification": "период изменения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f5b2a5b-1dfb-3984-be47-79ca86b9b4de",
                    "sense_definition": "Wyodrębniony fragment drogi lub trasy do przebycia, często w kontekście wyścigów sportowych lub podróży.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polski kolarz wygrał górski <w>etap</w> wyścigu we Francji.",
                            "target_lang_translations": {
                                "ru": "Польский велогонщик выиграл горный <w>этап</w> гонки во Франции."
                            }
                        },
                        {
                            "text": "Pierwszy <w>etap</w> naszej wędrówki był bardzo męczący ze względu na upał.",
                            "target_lang_translations": {
                                "ru": "Первый <w>этап</w> нашего похода был очень утомительным из-за жары."
                            }
                        }
                    ],
                    "synonyms": [
                        "odcinek"
                    ],
                    "common_phrases": [
                        "zwycięzca etapu",
                        "etap wyścigu",
                        "trudny etap",
                        "meta etapu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный участок пути или дистанции, часто в контексте спортивных соревнований или путешествий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "этап",
                                "target_lang_sense_clarification": "часть дистанции"
                            },
                            {
                                "target_lang_word": "участок",
                                "target_lang_sense_clarification": "часть пути"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1c6b6b8-7941-3c28-b0d4-32275f59504e",
                    "sense_definition": "Miejsce postoju i odpoczynku dla transportowanych więźniów lub maszerującego wojska (znaczenie historyczne).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zesłańcy spędzili noc na brudnym i zimnym <w>etapie</w>.",
                            "target_lang_translations": {
                                "ru": "Ссыльные провели ночь на грязном и холодном <w>этапе</w>."
                            }
                        },
                        {
                            "text": "Transport więźniów zatrzymywał się na każdym wyznaczonym <w>etapie</w>.",
                            "target_lang_translations": {
                                "ru": "Транспорт с заключенными останавливался на каждом назначенном <w>этапе</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "używane głównie w kontekście historycznym (zsyłki, dawne wojskowość)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Место остановки и отдыха для перемещаемых заключенных или войск (историческое значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "этап",
                                "target_lang_sense_clarification": "пункт остановки арестантов"
                            }
                        ]
                    }
                }
            ]
        }
    ]

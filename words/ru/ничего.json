    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "33ea06db-8bbb-3f8a-8adf-47b6af215050",
                    "sense_definition": "Форма родительного падежа местоимения <w>ничто</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я совершенно <ничего> не знаю об этом случае.",
                            "target_lang_translations": {
                                "en": "I know absolutely <nothing> about this case."
                            }
                        },
                        {
                            "text": "В холодильнике совсем <ничего> не осталось на ужин.",
                            "target_lang_translations": {
                                "en": "There is absolutely <nothing> left in the fridge for dinner."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ничего подобного",
                        "ничего лишнего",
                        "ровным счётом ничего"
                    ],
                    "target_lang_definitions": {
                        "en": "Genitive form of the pronoun *nothing*; used to indicate the absence of anything."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nothing",
                                "target_lang_sense_clarification": "pronoun indicating absence of anything"
                            },
                            {
                                "target_lang_word": "anything",
                                "target_lang_sense_clarification": "used in negative sentences"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "797d41d2-b8ef-3cab-9fda-5194a3270487",
                    "sense_definition": "Выражение прощения, успокоения или согласия; неважно, всё в порядке.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "— Извините, я наступил вам на ногу. — <Ничего>, не больно.",
                            "target_lang_translations": {
                                "en": "— Excuse me, I stepped on your foot. — <It's okay>, it doesn't hurt."
                            }
                        },
                        {
                            "text": "— Я забыл купить хлеба. — <Ничего>, обойдёмся без него.",
                            "target_lang_translations": {
                                "en": "— I forgot to buy bread. — <Never mind>, we'll manage without it."
                            }
                        }
                    ],
                    "synonyms": [
                        "ладно",
                        "пускай"
                    ],
                    "common_phrases": [
                        "ничего страшного",
                        "да ничего"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Expression of forgiveness, reassurance, or agreement; it doesn't matter, everything is fine."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "it's okay",
                                "target_lang_sense_clarification": "reassurance or forgiveness"
                            },
                            {
                                "target_lang_word": "never mind",
                                "target_lang_sense_clarification": "indicating something is unimportant"
                            },
                            {
                                "target_lang_word": "no problem",
                                "target_lang_sense_clarification": "response to an apology"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "599d5c8a-b438-3db8-bd53-2650a7ce78c9",
                    "sense_definition": "Довольно хорошо, сносно, вполне приемлемо (часто в ответ на вопрос о делах или здоровье).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "— Как твоя новая работа? — <Ничего>, мне нравится.",
                            "target_lang_translations": {
                                "en": "— How is your new job? — <Not bad>, I like it."
                            }
                        },
                        {
                            "text": "Этот фильм оказался очень даже <ничего>, советую посмотреть.",
                            "target_lang_translations": {
                                "en": "This movie turned out to be actually <not bad>, I recommend watching it."
                            }
                        }
                    ],
                    "synonyms": [
                        "неплохо",
                        "нормально",
                        "сносно"
                    ],
                    "antonyms": [
                        "плохо",
                        "ужасно"
                    ],
                    "common_phrases": [
                        "очень даже ничего"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Quite good, tolerable, fully acceptable (often in response to questions about affairs or health)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "not bad",
                                "target_lang_sense_clarification": "reasonably good"
                            },
                            {
                                "target_lang_word": "okay",
                                "target_lang_sense_clarification": "acceptable"
                            },
                            {
                                "target_lang_word": "so-so",
                                "target_lang_sense_clarification": "neither very good nor very bad"
                            }
                        ]
                    }
                }
            ]
        }
    ]

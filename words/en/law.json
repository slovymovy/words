    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2db64981-2ca3-3037-a9f4-f568759ccdf3",
                    "sense_definition": "The body of binding rules, regulations, customs, and standards established in a community by its legislative and judicial authorities.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "legal_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stealing money from the bank is strictly against the <w>law</w>.",
                            "target_lang_translations": {
                                "ru": "Кража денег из банка строго запрещена <w>законом</w>."
                            }
                        },
                        {
                            "text": "We need to maintain <w>law</w> and order to ensure everyone is safe.",
                            "target_lang_translations": {
                                "ru": "Нам нужно поддерживать <w>закон</w> и порядок, чтобы обеспечить безопасность каждого."
                            }
                        }
                    ],
                    "synonyms": [
                        "legislation",
                        "code",
                        "constitution"
                    ],
                    "common_phrases": [
                        "against the law",
                        "break the law",
                        "law and order",
                        "rule of law",
                        "by law"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность обязательных правил, норм и обычаев, установленных в обществе законодательной и судебной властью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закон",
                                "target_lang_sense_clarification": "система правил"
                            },
                            {
                                "target_lang_word": "право",
                                "target_lang_sense_clarification": "юридическая система"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8535445-9d36-3627-9ec1-f2b669213060",
                    "sense_definition": "A specific binding regulation, statute, or custom established in a community.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "legal_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government passed a new <w>law</w> to protect the environment from pollution.",
                            "target_lang_translations": {
                                "ru": "Правительство приняло новый <w>закон</w> для защиты окружающей среды от загрязнения."
                            }
                        },
                        {
                            "text": "There is a <w>law</w> that forbids driving a car without a valid license.",
                            "target_lang_translations": {
                                "ru": "Существует <w>закон</w>, запрещающий водить машину без действующих прав."
                            }
                        }
                    ],
                    "synonyms": [
                        "act",
                        "statute",
                        "regulation",
                        "rule",
                        "ordinance",
                        "decree",
                        "edict",
                        "enactment",
                        "mandate"
                    ],
                    "common_phrases": [
                        "pass a law",
                        "federal law",
                        "new law",
                        "enforce a law"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретное обязательное правило, статут или обычай, установленный в обществе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закон",
                                "target_lang_sense_clarification": "нормативный акт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f68171cf-34ef-3b70-9f2c-6f2d92b0043f",
                    "sense_definition": "The profession that deals with legal rules, or the field of knowledge encompassing them.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to go to university to study <w>law</w> and become a solicitor.",
                            "target_lang_translations": {
                                "ru": "Она решила поступить в университет, чтобы изучать <w>право</w> и стать адвокатом."
                            }
                        },
                        {
                            "text": "He has practiced <w>law</w> at a prestigious firm in New York for twenty years.",
                            "target_lang_translations": {
                                "ru": "Он двадцать лет занимался <w>правом</w> в престижной фирме в Нью-Йорке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "practice law",
                        "study law",
                        "law school",
                        "law firm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Профессия, связанная с юридическими нормами, или область знаний, охватывающая их."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "право",
                                "target_lang_sense_clarification": "наука, дисциплина"
                            },
                            {
                                "target_lang_word": "юриспруденция",
                                "target_lang_sense_clarification": "область знаний"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb4f5698-7ded-36a5-aa2b-ce9ae75c4386",
                    "sense_definition": "A statement of an observed order, sequence, or relationship of phenomena which is invariable under certain conditions.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "scientific_principle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>law</w> of gravity explains why apples fall from trees to the ground.",
                            "target_lang_translations": {
                                "ru": "<w>Закон</w> всемирного тяготения объясняет, почему яблоки падают с деревьев на землю."
                            }
                        },
                        {
                            "text": "Scientists are constantly testing the fundamental <w>laws</w> of physics to understand the universe.",
                            "target_lang_translations": {
                                "ru": "Ученые постоянно проверяют фундаментальные <w>законы</w> физики, чтобы понять вселенную."
                            }
                        }
                    ],
                    "synonyms": [
                        "law of nature",
                        "rule",
                        "canon",
                        "maxim"
                    ],
                    "common_phrases": [
                        "law of nature",
                        "laws of physics",
                        "law of gravity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Утверждение о наблюдаемом порядке, последовательности или взаимосвязи явлений, неизменное при определенных условиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закон",
                                "target_lang_sense_clarification": "научный принцип"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e19be75-a36b-34ff-b960-2cc6075862d1",
                    "sense_definition": "A person or group that acts with authority to uphold rules and order, such as the police.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The robbers ran away as soon as they saw the <w>law</w> arriving at the scene.",
                            "target_lang_translations": {
                                "ru": "Грабители убежали, как только увидели прибывшую на место <w>полицию</w>."
                            }
                        },
                        {
                            "text": "He spent his whole life running from the <w>law</w> after escaping from prison.",
                            "target_lang_translations": {
                                "ru": "Он провел всю жизнь, скрываясь от <w>закона</w> после побега из тюрьмы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the law"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to refer to police officers"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или группа людей, действующих с полномочиями поддерживать правила и порядок, например, полиция."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полиция",
                                "target_lang_sense_clarification": "правоохранительные органы"
                            },
                            {
                                "target_lang_word": "закон",
                                "target_lang_sense_clarification": "представители власти (разг.)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "bf2df2e7-c1b9-385e-b01f-f2e9119f6193",
                    "sense_definition": "A surname originating as a patronymic or topographic name.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "surname_group",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "I have an appointment with Mr <w>Law</w> at three o'clock this afternoon.",
                            "target_lang_translations": {
                                "ru": "У меня встреча с мистером <w>Ло</w> сегодня в три часа дня."
                            }
                        },
                        {
                            "text": "Jude <w>Law</w> is a well-known actor who has appeared in many popular movies.",
                            "target_lang_translations": {
                                "ru": "Джуд <w>Ло</w> — известный актер, который снялся во многих популярных фильмах."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия, происходящая от отчества или топографического названия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ло",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "151f5344-5434-34a9-9854-d18e7d8b3af5",
                    "sense_definition": "The commandments and moral principles contained in the Books of Moses or Christian scriptures.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "religious_text",
                    "name_type": "document_or_program_name",
                    "examples": [
                        {
                            "text": "The priest read from the <w>Law</w> during the morning service at the church.",
                            "target_lang_translations": {
                                "ru": "Священник читал из <w>Закона</w> во время утренней службы в церкви."
                            }
                        },
                        {
                            "text": "He studied the <w>Law</w> of Moses with great dedication to understand his faith.",
                            "target_lang_translations": {
                                "ru": "Он с большим усердием изучал <w>Закон</w> Моисея, чтобы понять свою веру."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the Law"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заповеди и моральные принципы, содержащиеся в Книгах Моисея или христианских писаниях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Закон",
                                "target_lang_sense_clarification": "религиозный"
                            }
                        ]
                    }
                }
            ]
        }
    ]

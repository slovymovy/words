                {
                    "sense_id": "88375330-35ad-37a7-8f69-943fd650b96e",
                    "sense_definition": "Rysunki, obrazy lub napisy wykonywane w przestrzeni publicznej, najczęściej na murach lub ścianach budynków, często w sposób nielegalny",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "modern_art",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zarabiał na życie swoim hobby, którym było malowanie <w>graffiti</w>."
                        },
                        {
                            "text": "Władze miasta wydały dużo pieniędzy na usuwanie nielegalnego <w>graffiti</w> z elewacji kamienic."
                        }
                    ],
                    "synonyms": [
                        "wrzut"
                    ],
                    "common_phrases": [
                        "malować graffiti",
                        "usuwać graffiti",
                        "nielegalne graffiti",
                        "kolorowe graffiti"

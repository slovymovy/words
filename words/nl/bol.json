                {
                    "sense_id": "4c550414-d89a-3fac-a9c9-7a83e9b1f41a",
                    "sense_definition": "Een ronde hoeveelheid ijs, geschept met een speciale lepel.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mag ik een hoorntje met twee <w>bollen</w> vanille-ijs?",
                            "target_lang_translations": {
                                "en": "May I have a cone with two <w>scoops</w> of vanilla ice cream?",
                                "ru": "Можно мне рожок с двумя <w>шариками</w> ванильного мороженого?"
                            }
                        },
                        {
                            "text": "Mijn dochter wil graag een <w>bolletje</w> aardbeienijs.",
                            "target_lang_translations": {
                                "en": "My daughter would like a <w>scoop</w> of strawberry ice cream.",
                                "ru": "Моя дочь хочет <w>шарик</w> клубничного мороженого."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een bol ijs",
                        "twee bollen"
                    ],
                    "target_lang_definitions": {
                        "en": "A round amount of ice cream, served with a special spoon.",
                        "ru": "Порция мороженого круглой формы, набранная специальной ложкой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scoop",
                                "target_lang_sense_clarification": "of ice cream"
                            }

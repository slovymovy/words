            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1ad7b353-1782-3d57-8bc3-c4fcba2cf22c",
                    "sense_definition": "Een eenvoudige eetgelegenheid, vaak informeel, waar men maaltijden kan kopen en nuttigen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "establishment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben gisteren heerlijk gegeten in een gezellige <w>eterij</w> in het centrum.",
                            "target_lang_translations": {
                                "ru": "Вчера мы вкусно поели в уютном <w>кафе</w> в центре.",
                                "en": "We had a delicious meal yesterday in a cozy <w>eatery</w> in the city center."
                            }
                        },
                        {
                            "text": "Deze <w>eterij</w> staat bekend om zijn verse en betaalbare gerechten.",
                            "target_lang_translations": {
                                "ru": "Это <w>бистро</w> известно своими свежими и доступными блюдами.",
                                "en": "This <w>eatery</w> is known for its fresh and affordable dishes."
                            }
                        }
                    ],
                    "synonyms": [
                        "eetcafé",
                        "bistro",
                        "eetsalon",
                        "restaurant",
                        "cafetaria",
                        "bodega",
                        "snackbar",
                        "friture"
                    ],
                    "common_phrases": [
                        "gezellige eterij",
                        "lokale eterij"
                    ],
                    "target_lang_definitions": {
                        "ru": "Простое заведение общественного питания, часто неформальное, где можно купить и съесть еду.",
                        "en": "A simple dining establishment, often informal, where one can buy and consume meals."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кафе",
                                "target_lang_sense_clarification": "небольшое уютное заведение"
                            },
                            {
                                "target_lang_word": "закусочная",
                                "target_lang_sense_clarification": "простое место для еды"
                            },
                            {
                                "target_lang_word": "бистро",
                                "target_lang_sense_clarification": "маленький ресторанчик"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "eatery",
                                "target_lang_sense_clarification": "informal dining place"
                            },
                            {
                                "target_lang_word": "bistro",
                                "target_lang_sense_clarification": "small, modest restaurant"
                            },
                            {
                                "target_lang_word": "diner",
                                "target_lang_sense_clarification": "casual restaurant"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75064b9d-2fa9-3c01-b0eb-e0efc2a4c4f4",
                    "sense_definition": "Zaken die men kan eten; voedsel of proviand.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bezorger zette de zak met <w>eterij</w> voor de deur en vertrok.",
                            "target_lang_translations": {
                                "ru": "Курьер поставил пакет с <w>едой</w> перед дверью и ушел.",
                                "en": "The delivery person put the bag of <w>food</w> in front of the door and left."
                            }
                        },
                        {
                            "text": "Er was genoeg <w>eterij</w> in huis om de hele groep te voeden.",
                            "target_lang_translations": {
                                "ru": "В доме было достаточно <w>съестного</w>, чтобы накормить всю группу.",
                                "en": "There was enough <w>food</w> in the house to feed the whole group."
                            }
                        }
                    ],
                    "synonyms": [
                        "eten",
                        "voedsel",
                        "voeding",
                        "maaltijd"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что можно есть; пища или провизия.",
                        "en": "Things that can be eaten; food or provisions."
                    },
                    "translations": {

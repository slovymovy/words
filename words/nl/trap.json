    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9ec6c44a-42f6-3889-b8d9-4268fccf9aab",
                    "sense_definition": "Een vaste constructie met treden om van de ene naar de andere verdieping te gaan.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "climbing_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij loopt snel de <w>trap</w> op naar zijn kamer.",
                            "target_lang_translations": {
                                "en": "He walks quickly up the <w>stairs</w> to his room.",
                                "ru": "Он быстро поднимается по <w>лестнице</w> в свою комнату."
                            }
                        },
                        {
                            "text": "De houten <w>trap</w> in de gang kraakt als je erop loopt.",
                            "target_lang_translations": {
                                "en": "The wooden <w>staircase</w> in the hallway creaks when you walk on it.",
                                "ru": "Деревянная <w>лестница</w> в коридоре скрипит, когда по ней идешь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de trap op",
                        "de trap af",
                        "met de trap"
                    ],
                    "target_lang_definitions": {
                        "en": "A fixed structure with steps to go from one floor to another.",
                        "ru": "Неподвижная конструкция со ступенями для перехода с одного этажа на другой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stairs",
                                "target_lang_sense_clarification": "general term for a set of steps"
                            },
                            {
                                "target_lang_word": "staircase",
                                "target_lang_sense_clarification": "structure containing stairs and railings"
                            },
                            {
                                "target_lang_word": "stairway",
                                "target_lang_sense_clarification": "passage or way with stairs"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "лестница",
                                "target_lang_sense_clarification": "в здании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8f4cfe1-46de-3278-8b44-62618ba4a458",
                    "sense_definition": "Een verplaatsbaar klimwerktuig, vaak inklapbaar, om bij hoge plaatsen te komen.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "climbing_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pak even de <w>trap</w> zodat ik de lamp kan vervangen.",
                            "target_lang_translations": {
                                "en": "Just grab the <w>stepladder</w> so I can replace the lamp.",
                                "ru": "Возьми <w>стремянку</w>, чтобы я мог заменить лампочку."
                            }
                        },
                        {
                            "text": "De schilder stond op een wankele <w>trap</w> te werken.",
                            "target_lang_translations": {
                                "en": "The painter was standing on a wobbly <w>ladder</w> working.",
                                "ru": "Маляр работал, стоя на шаткой <w>стремянке</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "schop"
                    ],
                    "common_phrases": [
                        "op een trap staan",
                        "een trapje pakken"
                    ],
                    "target_lang_definitions": {
                        "en": "A movable climbing tool, often foldable, to reach high places.",
                        "ru": "Переносное приспособление для лазания, часто складное, чтобы добраться до высоких мест."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stepladder",
                                "target_lang_sense_clarification": "self-supporting portable ladder"
                            },
                            {
                                "target_lang_word": "ladder",
                                "target_lang_sense_clarification": "general climbing tool"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "стремянка",
                                "target_lang_sense_clarification": "складная лестница"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40813180-c7e4-3c9d-8c5a-6e275db29670",
                    "sense_definition": "Een harde stoot of slag die met de voet wordt gegeven.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "kick_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De voetballer gaf een harde <w>trap</w> tegen de bal.",
                            "target_lang_translations": {
                                "en": "The football player gave the ball a hard <w>kick</w>.",
                                "ru": "Футболист нанес сильный <w>удар</w> по мячу."
                            }
                        },
                        {
                            "text": "Hij kreeg een gemene <w>trap</w> tegen zijn scheenbeen.",
                            "target_lang_translations": {
                                "en": "He received a nasty <w>kick</w> to his shin.",
                                "ru": "Он получил подлый <w>удар</w> ногой в голень."
                            }
                        }
                    ],
                    "synonyms": [
                        "schop"
                    ],
                    "common_phrases": [
                        "een trap geven",
                        "een trap krijgen",
                        "een harde trap"
                    ],
                    "target_lang_definitions": {
                        "en": "A hard blow or strike given with the foot.",
                        "ru": "Сильный удар или толчок, наносимый ногой."
                    },
                    "translations": {

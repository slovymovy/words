                {
                    "sense_id": "c7852408-635f-35f6-ac29-2624a32cc6ce",
                    "sense_definition": "W sposób zgodny z obowiązującymi przepisami, procedurami, etykietą lub stanem prawnym; oficjalnie.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "official",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo że nie mieszkają razem, <w>formalnie</w> wciąż są małżeństwem.",
                            "target_lang_translations": {
                                "ru": "Хотя они не живут вместе, <w>формально</w> они всё ещё женаты."
                            }
                        },
                        {
                            "text": "Dyrektor powitał gości bardzo <w>formalnie</w>, dbając o każdy szczegół etykiety.",
                            "target_lang_translations": {
                                "ru": "Директор приветствовал гостей очень <w>формально</w>, соблюдая все тонкости этикета."
                            }
                        }
                    ],
                    "synonyms": [
                        "oficjalnie",
                        "urzędowo"
                    ],
                    "antonyms": [
                        "nieformalnie"
                    ],
                    "common_phrases": [
                        "formalnie rzecz biorąc",
                        "załatwić formalnie",
                        "czysto formalnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "В соответствии с действующими правилами, процедурами, этикетом или правовым статусом; официально."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "формально",
                                "target_lang_sense_clarification": "по форме, согласно правилам или документам"
                            },
                            {
                                "target_lang_word": "официально",
                                "target_lang_sense_clarification": "в официальном порядке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25c336a6-b7ec-31a7-bf76-fbb12f57c64f",
                    "sense_definition": "Biorąc pod uwagę zewnętrzną formę, strukturę lub budowę czegoś, a nie jego treść.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "structural",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten wiersz jest <w>formalnie</w> doskonały, ale jego treść jest banalna.",
                            "target_lang_translations": {
                                "ru": "Это стихотворение <w>формально</w> безупречно, но его содержание банально."
                            }
                        },
                        {
                            "text": "Analiza dzieła sztuki powinna przebiegać zarówno <w>formalnie</w>, jak i treściowo.",
                            "target_lang_translations": {
                                "ru": "Анализ произведения искусства должен проводиться как <w>формально</w>, так и содержательно."
                            }

                    "learner_level": "B2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hard</w> je hart niet tegen de waarheid.",
                            "target_lang_translations": {
                                "ru": "Не <w>ожесточай</w> своё сердце против правды."
                            }

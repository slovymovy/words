    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fb194512-c616-3525-a3e5-d8dd5c3f85d0",
                    "sense_definition": "An imaginary line around which an object spins or is symmetrically arranged.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "geometric_line",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The earth rotates on its <w>axis</w> once every twenty-four hours.",
                            "target_lang_translations": {
                                "ru": "Земля вращается вокруг своей <w>оси</w> один раз в двадцать четыре часа."
                            }
                        },
                        {
                            "text": "The <w>axis</w> of the wheel must be perfectly straight for the car to drive smoothly.",
                            "target_lang_translations": {
                                "ru": "<w>Ось</w> колеса должна быть идеально прямой, чтобы автомобиль ехал плавно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "axis of rotation",
                        "on its axis",
                        "major axis",
                        "minor axis"
                    ],
                    "target_lang_definitions": {
                        "ru": "Воображаемая линия, вокруг которой вращается объект или которая является центром симметрии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ось",
                                "target_lang_sense_clarification": "линия вращения или симметрии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "74e9b375-c563-3417-b4ff-feebc6200e0b",
                    "sense_definition": "A fixed reference line used to position and locate data in a graph or coordinate system.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "geometric_line",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The horizontal <w>axis</w> on the chart represents time in years.",
                            "target_lang_translations": {
                                "ru": "Горизонтальная <w>ось</w> на диаграмме представляет время в годах."
                            }
                        },
                        {
                            "text": "You should plot the dependent variable on the vertical <w>axis</w>.",
                            "target_lang_translations": {
                                "ru": "Вам следует отметить зависимую переменную на вертикальной <w>оси</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "x-axis",
                        "y-axis",
                        "coordinate axis",
                        "vertical axis",
                        "horizontal axis"
                    ],
                    "target_lang_definitions": {
                        "ru": "Фиксированная линия отсчета, используемая для построения и определения координат данных на графике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ось",
                                "target_lang_sense_clarification": "координатная прямая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "404979a6-6204-3909-a7f1-73e56c32484c",
                    "sense_definition": "An alliance or coalition between states or groups.",
                    "learner_level": "C1",
                    "frequency": "Low",

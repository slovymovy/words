                },
                {
                    "sense_id": "d7f60521-eb0c-30df-9317-b73d50f30ce3",
                    "sense_definition": "O kolorze: intensywny, nasycony, głęboki (przeciwieństwo bladego).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "light_and_color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Justynie bardzo do twarzy w <w>ciemnej</w> zieleni.",
                            "target_lang_translations": {
                                "ru": "Юстине очень к лицу в <w>тёмной</w> зелени."
                            }
                        },
                        {
                            "text": "Ściany w salonie pomalowaliśmy na <w>ciemny</w> fiolet.",
                            "target_lang_translations": {
                                "ru": "Стены в гостиной мы покрасили в <w>тёмно</w>-фиолетовый цвет."
                            }
                        }
                    ],
                    "antonyms": [
                        "blady",
                        "jasny"
                    ],
                    "common_phrases": [
                        "ciemna zieleń",
                        "ciemny granat",
                        "ciemna czerwień"
                    ],
                    "target_lang_definitions": {
                        "ru": "О цвете: интенсивный, насыщенный, глубокий (противоположность бледному)."

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b35c751d-8002-3d6a-a575-d5d61371d4e3",
                    "sense_definition": "Rodzaj widowiska teatralnego, w którym głównym środkiem wyrazu jest taniec artystyczny wykonywany według choreografii z towarzyszeniem muzyki.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "art_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj wieczorem poszliśmy do teatru na klasyczny <w>balet</w>.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером мы пошли в театр на классический <w>балет</w>."
                            }
                        },
                        {
                            "text": "Moja córka od dziecka marzy o tym, by występować w <w>balecie</w>.",
                            "target_lang_translations": {
                                "ru": "Моя дочь с детства мечтает выступать в <w>балете</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "balet klasyczny",
                        "balet nowoczesny",
                        "szkoła baletu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид театрального искусства, основным выразительным средством которого является танец в сопровождении музыки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "балет",
                                "target_lang_sense_clarification": "вид искусства / спектакль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e23c2f3-e80e-3c12-9db7-b59388ac14b1",
                    "sense_definition": "Zespół tancerzy i tancerek występujący w widowisku baletowym.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "art_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cały <w>balet</w> wyjechał na zagraniczne tournée do Francji.",
                            "target_lang_translations": {
                                "ru": "Весь <w>балет</w> уехал на зарубежные гастроли во Францию."
                            }
                        },
                        {
                            "text": "Dyrektor opery przedstawił publiczności nowy <w>balet</w> podczas premiery.",
                            "target_lang_translations": {
                                "ru": "Директор оперы представил публике новый <w>балет</w> во время премьеры."
                            }
                        }
                    ],
                    "common_phrases": [
                        "solista baletu",
                        "członek baletu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Коллектив танцоров и балерин, участвующих в балетных спектаклях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "балет",
                                "target_lang_sense_clarification": "труппа"
                            },
                            {
                                "target_lang_word": "балетная труппа",
                                "target_lang_sense_clarification": "коллектив"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70bbe65a-d75b-32fa-9226-aa5801ff4c69",
                    "sense_definition": "Huczna impreza lub zabawa taneczna, często z alkoholem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "party",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sobotę szykują się niezłe <w>balety</w> u moich znajomych.",
                            "target_lang_translations": {
                                "ru": "В субботу намечается неплохая <w>гулянка</w> у моих знакомых."
                            }
                        },
                        {
                            "text": "Po zdanych egzaminach studenci urządzili sobie wielki <w>balet</w> w akademiku.",
                            "target_lang_translations": {
                                "ru": "После сданных экзаменов студенты устроили себе грандиозную <w>тусовку</w> в общежитии."
                            }
                        }
                    ],
                    "synonyms": [
                        "zabawa",
                        "potańcówka"
                    ],
                    "common_phrases": [
                        "iść na balety",
                        "ostry balet"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used frequently in plural form 'balety'"
                        },
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Шумная вечеринка или танцевальное мероприятие, часто с употреблением алкоголя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гулянка",
                                "target_lang_sense_clarification": "разг."
                            },
                            {
                                "target_lang_word": "тусовка",
                                "target_lang_sense_clarification": "жарг."
                            },
                            {
                                "target_lang_word": "вечеринка",
                                "target_lang_sense_clarification": "праздник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c7a04ff-7fe6-3de2-9cd7-bc6219441201",
                    "sense_definition": "Utwór muzyczny skomponowany specjalnie jako podkład do widowiska tanecznego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "art_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Piotr Czajkowski skomponował ten słynny <w>balet</w> w dziewiętnastym wieku.",
                            "target_lang_translations": {
                                "ru": "Пётр Чайковский сочинил этот знаменитый <w>балет</w> в девятнадцатом веке."
                            }
                        },
                        {
                            "text": "W filharmonii zagrano fragmenty <w>baletu</w> Igora Strawińskiego.",
                            "target_lang_translations": {
                                "ru": "В филармонии сыграли фрагменты <w>балета</w> Игоря Стравинского."
                            }
                        }
                    ],
                    "common_phrases": [
                        "muzyka do baletu",
                        "skomponować balet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкальное произведение, сочиненное специально для танцевального представления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "балет",
                                "target_lang_sense_clarification": "музыкальное произведение"
                            }
                        ]
                    }
                }
            ]
        }

            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ac899d49-c524-34a0-9413-a5894048d4db",
                    "sense_definition": "W sposób pełen optymizmu, nadziei i dobrego nastawienia do życia lub przyszłości.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo wielu problemów, ona zawsze patrzy <pozytywnie> na świat.",
                            "target_lang_translations": {
                                "ru": "Несмотря на множество проблем, она всегда смотрит на мир <позитивно>."
                            }
                        },
                        {
                            "text": "Musisz myśleć <pozytywnie>, żeby osiągnąć sukces w tym projekcie.",
                            "target_lang_translations": {
                                "ru": "Ты должен мыслить <позитивно>, чтобы добиться успеха в этом проекте."
                            }
                        }
                    ],
                    "synonyms": [
                        "dobrze"
                    ],
                    "antonyms": [
                        "negatywnie",
                        "źle"
                    ],
                    "common_phrases": [
                        "myśleć pozytywnie",
                        "pozytywnie nastawiony",
                        "patrzeć pozytywnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "С оптимизмом, надеждой и хорошим настроем по отношению к жизни или будущему."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "позитивно",
                                "target_lang_sense_clarification": "с оптимизмом"
                            },
                            {
                                "target_lang_word": "оптимистично",
                                "target_lang_sense_clarification": "с надеждой на лучшее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9f93830-8982-3585-b1c8-a5929899e27f",
                    "sense_definition": "W sposób wyrażający aprobatę, zgodę lub dobrą ocenę kogoś lub czegoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "evaluation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Komisja rekrutacyjna rozpatrzyła moje podanie <pozytywnie>.",
                            "target_lang_translations": {
                                "ru": "Приемная комиссия рассмотрела мое заявление <положительно>."
                            }
                        },
                        {
                            "text": "Krytycy filmowi wypowiadają się bardzo <pozytywnie> o tej produkcji.",
                            "target_lang_translations": {
                                "ru": "Кинокритики отзываются об этой постановке очень <положительно>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dobrze",
                        "twierdząco"
                    ],
                    "antonyms": [
                        "negatywnie",
                        "źle"
                    ],
                    "common_phrases": [
                        "oceniać pozytywnie",
                        "rozpatrzyć pozytywnie",
                        "wypowiadać się pozytywnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражая одобрение, согласие или хорошую оценку кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "положительно",

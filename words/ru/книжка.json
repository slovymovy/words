    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "82921dda-cc16-3c7f-b6c6-76176be9342b",
                    "sense_definition": "Уменьшительная форма слова <w>книга</w>; небольшая книга или (в разговорной речи) любая книга.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "print_media",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мама купила мне новую <книжку> с яркими картинками.",
                            "target_lang_translations": {
                                "en": "Mom bought me a new <book> with bright pictures."
                            }
                        },
                        {
                            "text": "Я люблю читать эту <книжку> перед сном.",
                            "target_lang_translations": {
                                "en": "I love reading this <book> before bed."
                            }
                        }
                    ],
                    "synonyms": [
                        "книга"
                    ],
                    "common_phrases": [
                        "интересная книжка",
                        "детская книжка",
                        "читать книжку"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A diminutive form of \"book\"; a small book or (colloquially) any book."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "book",
                                "target_lang_sense_clarification": "general / colloquial"
                            },
                            {
                                "target_lang_word": "booklet",
                                "target_lang_sense_clarification": "small book"
                            },
                            {
                                "target_lang_word": "paperback",
                                "target_lang_sense_clarification": "softcover book"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3bfb232-10ba-3b9e-b40b-4a0a2dbdede2",
                    "sense_definition": "Официальный документ или тетрадь в виде сшитых листов (например, трудовая, медицинская или записная).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "print_media",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "При устройстве на работу необходимо предъявить трудовую <книжку>.",
                            "target_lang_translations": {
                                "en": "When applying for a job, it is necessary to present an employment <record book>."
                            }
                        },
                        {
                            "text": "Он достал записную <книжку> и записал мой номер телефона.",
                            "target_lang_translations": {
                                "en": "He took out a <notebook> and wrote down my phone number."
                            }
                        }
                    ],
                    "common_phrases": [
                        "трудовая книжка",
                        "записная книжка",
                        "медицинская книжка",
                        "членская книжка"
                    ],
                    "target_lang_definitions": {
                        "en": "An official document or notebook consisting of bound pages (e.g., employment record, medical record, or notebook)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "notebook",
                                "target_lang_sense_clarification": "for writing notes"
                            },
                            {
                                "target_lang_word": "record book",
                                "target_lang_sense_clarification": "official document"
                            },
                            {
                                "target_lang_word": "book",
                                "target_lang_sense_clarification": "in compounds like 'employment book'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "205d8486-1e93-35aa-ae47-d915616a7d79",
                    "sense_definition": "Разговорное название сберегательной книжки; документ о наличии вклада в банке.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бабушка всегда хранила свои сбережения на <книжке>.",
                            "target_lang_translations": {
                                "en": "Grandma always kept her savings in a <passbook>."
                            }
                        },
                        {
                            "text": "Он решил снять с <книжки> все деньги для покупки машины.",
                            "target_lang_translations": {
                                "en": "He decided to withdraw all the money from the <passbook> to buy a car."
                            }
                        }
                    ],
                    "synonyms": [
                        "сберкнижка"
                    ],
                    "common_phrases": [
                        "деньги на книжке",
                        "снять с книжки",
                        "положить на книжку"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for сберегательная книжка"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Colloquial name for a savings book; a document showing a bank deposit."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "passbook",
                                "target_lang_sense_clarification": "banking"
                            },
                            {
                                "target_lang_word": "savings book",
                                "target_lang_sense_clarification": "banking"
                            }
                        ]
                    }
                },
                {

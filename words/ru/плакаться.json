    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bb499ff3-5796-38ca-acc7-25c8ec08f734",
                    "sense_definition": "Высказывать жалобы и сетования на неприятности, трудности или судьбу, часто с целью вызвать сочувствие.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "complaining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он привык постоянно <w>плакаться</w> на нехватку денег, хотя зарабатывает прилично.",
                            "target_lang_translations": {
                                "en": "He is used to constantly <w>complaining</w> about a lack of money, although he earns a decent amount."
                            }
                        },
                        {
                            "text": "Если тебе станет грустно, ты всегда можешь прийти и <w>плакаться</w> в жилетку мне.",
                            "target_lang_translations": {
                                "en": "If you get sad, you can always come and <w>cry</w> on my shoulder."
                            }
                        }
                    ],
                    "synonyms": [
                        "жаловаться",
                        "сетовать",
                        "ныть"
                    ],
                    "common_phrases": [
                        "плакаться в жилетку",
                        "плакаться на судьбу",
                        "плакаться на жизнь"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies seeking sympathy"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To complain about troubles, difficulties, or fate, often seeking sympathy."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "complain",
                                "target_lang_sense_clarification": "about hardships or bad luck"
                            },
                            {
                                "target_lang_word": "whine",
                                "target_lang_sense_clarification": "complain in a piteous or annoying way"
                            },
                            {
                                "target_lang_word": "cry",
                                "target_lang_sense_clarification": "in the idiom 'cry on someone's shoulder'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0dc610cf-7691-3aa1-9377-e97954c15819",
                    "sense_definition": "Находиться в состоянии, когда хочется плакать или слёзы текут сами собой (используется в безличных конструкциях).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "crying_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В такую дождливую погоду ей особенно легко <w>плачется</w>.",
                            "target_lang_translations": {
                                "en": "In such rainy weather, she <w>cries</w> especially easily."
                            }
                        },
                        {
                            "text": "Мне сегодня почему-то совсем не <w>плачется</w>, хотя на душе очень тяжело.",
                            "target_lang_translations": {
                                "en": "For some reason I don't <w>feel like crying</w> at all today, although my heart is very heavy."
                            }
                        }
                    ],
                    "common_phrases": [
                        "легко плачется",
                        "не плачется"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "impersonal construction"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To feel an urge to cry or to find oneself crying (used in impersonal constructions)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "feel like crying",
                                "target_lang_sense_clarification": "have the urge to cry"
                            },
                            {
                                "target_lang_word": "cry",
                                "target_lang_sense_clarification": "impersonal state"
                            }
                        ]
                    }
                }
            ]
        }
    ]

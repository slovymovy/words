            "senses": [
                {
                    "sense_id": "314a0c97-c132-3df0-a0cc-3dd6e5253507",
                    "sense_definition": "Items worn to cover the body, such as shirts, pants, and coats.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "clothing_items",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should wear warm <w>clothing</w> when it is cold outside.",
                            "target_lang_translations": {
                                "ru": "Тебе следует носить теплую <w>одежду</w>, когда на улице холодно."
                            }
                        },
                        {
                            "text": "A shirt is an article of <w>clothing</w> that covers the torso and arms.",
                            "target_lang_translations": {
                                "ru": "Рубашка — это предмет <w>одежды</w>, который закрывает туловище и руки."
                            }
                        }
                    ],
                    "synonyms": [
                        "clothes",
                        "attire",
                        "apparel",
                        "garment",
                        "wardrobe"
                    ],
                    "common_phrases": [
                        "warm clothing",
                        "article of clothing",
                        "clothing brand",
                        "protective clothing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предметы, надеваемые на тело, такие как рубашки, брюки и пальто."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одежда",
                                "target_lang_sense_clarification": "предметы гардероба"
                            },
                            {
                                "target_lang_word": "вещи",
                                "target_lang_sense_clarification": "разговорное, предметы одежды"
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "53b21b03-92b9-36c6-918b-f29e53d1c028",
                    "sense_definition": "Man die uit religieuze overwegingen teruggetrokken leeft, voornamelijk in een klooster.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "religious_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude <w>monnik</w> liep rustig door de kloostertuin.",
                            "target_lang_translations": {
                                "ru": "Старый <w>монах</w> спокойно шел по монастырскому саду."
                            }
                        },
                        {
                            "text": "Hij besefte dat het habijt de <w>monnik</w> niet maakt en bleef bescheiden.",
                            "target_lang_translations": {
                                "ru": "Он понимал, что ряса не делает <w>монаха</w>, и оставался скромным."
                            }
                        }
                    ],
                    "synonyms": [
                        "kloosterling"
                    ],
                    "common_phrases": [
                        "het habijt maakt de monnik niet",
                        "leven als een monnik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, посвятивший себя религии и живущий в уединении, обычно в монастыре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "монах",
                                "target_lang_sense_clarification": "член религиозной общины"
                            },
                            {
                                "target_lang_word": "инок",
                                "target_lang_sense_clarification": "православный монах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e875668-f3a8-3846-a015-38eed77fb1cc",
                    "sense_definition": "Middeleeuwse dakpan in de vorm van een halve afgeknotte holle kegel, vaak gebruikt in combinatie met een non-pan.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij de restauratie van het dak werd elke <w>monnik</w> zorgvuldig gecontroleerd.",
                            "target_lang_translations": {
                                "ru": "При реставрации крыши каждую черепицу типа <w>монах</w> тщательно проверяли."
                            }
                        },
                        {
                            "text": "Het dak is gedekt met pannen van het type <w>monnik</w> en non.",
                            "target_lang_translations": {
                                "ru": "Крыша покрыта черепицей типа <w>монах</w> и монашка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "monnik en non"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тип желобчатой черепицы, используемый в кладке «монах и монашка»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "монах",
                                "target_lang_sense_clarification": "тип черепицы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca0b8709-0f26-3de7-be79-c06d6f470d85",
                    "sense_definition": "Een nachtvlinder van het geslacht Cucullia.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bioloog ontdekte een zeldzame <w>monnik</w> op het blad.",
                            "target_lang_translations": {
                                "ru": "Биолог обнаружил редкую <w>капюшонницу</w> на листе."
                            }
                        },
                        {
                            "text": "Deze grauwe <w>monnik</w> is vooral actief tijdens de schemering.",
                            "target_lang_translations": {
                                "ru": "Эта серая <w>капюшонница</w> особенно активна в сумерках."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ночная бабочка из рода Cucullia (капюшонницы)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "капюшонница",
                                "target_lang_sense_clarification": "ночная бабочка"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
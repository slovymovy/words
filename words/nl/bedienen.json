                {
                    "sense_id": "834e18be-175a-3468-a875-a81c3f3e5e82",
                    "sense_definition": "Gasten in een horecagelegenheid van eten en drinken voorzien.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ober kwam ons snel <w>bedienen</w> toen we aan tafel gingen zitten.",
                            "target_lang_translations": {
                                "en": "The waiter came to <w>serve</w> us quickly when we sat down at the table.",
                                "ru": "Официант быстро подошел <w>обслужить</w> нас, когда мы сели за столик."
                            }
                        },
                        {
                            "text": "Zij vindt het leuk om mensen te <w>bedienen</w> in het drukke café.",
                            "target_lang_translations": {
                                "en": "She likes to <w>wait on</w> people in the busy café.",
                                "ru": "Ей нравится <w>обслуживать</w> людей в оживленном кафе."
                            }
                        }
                    ],
                    "synonyms": [
                        "serveren",
                        "opdienen"
                    ],
                    "common_phrases": [
                        "aan tafel bedienen",
                        "vlot bedienen",
                        "gasten bedienen"
                    ],
                    "target_lang_definitions": {
                        "en": "To provide guests with food and drink in a catering establishment.",
                        "ru": "Обслуживать гостей, подавая еду и напитки в заведении общепита."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "serve",
                                "target_lang_sense_clarification": "food or drinks"

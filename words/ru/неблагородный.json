            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "21900c3b-5c87-3640-a78a-25eb24ce0f45",
                    "sense_definition": "Лишённый благородства, честности и высоких моральных качеств (о человеке или поступке).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "moral_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Обманывать тех, кто тебе доверяет, — это очень <w>неблагородный</w> поступок.",
                            "target_lang_translations": {
                                "en": "Deceiving those who trust you is a very <w>ignoble</w> act."
                            }
                        },
                        {
                            "text": "Этот <w>неблагородный</w> человек предал своих друзей ради личной выгоды.",
                            "target_lang_translations": {
                                "en": "This <w>dishonorable</w> man betrayed his friends for personal gain."
                            }
                        }
                    ],
                    "synonyms": [
                        "бесчестный",
                        "низкий",
                        "подлый",
                        "непорядочный"
                    ],
                    "antonyms": [
                        "благородный",
                        "честный",
                        "порядочный",
                        "достойный"
                    ],
                    "common_phrases": [
                        "неблагородный поступок",
                        "неблагородный человек",
                        "неблагородное дело",
                        "неблагородное поведение"
                    ],
                    "target_lang_definitions": {
                        "en": "Lacking nobility, honesty, and high moral qualities (about a person or act)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ignoble",
                                "target_lang_sense_clarification": "lacking nobility of character"
                            },
                            {
                                "target_lang_word": "dishonorable",
                                "target_lang_sense_clarification": "lacking honor or integrity"
                            },
                            {
                                "target_lang_word": "base",
                                "target_lang_sense_clarification": "morally low or mean"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40413dcd-3301-3ca8-be16-473023166c2a",
                    "sense_definition": "Не принадлежащий к дворянству или высшему сословию по своему происхождению.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "social_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В прошлом веке <w>неблагородное</w> происхождение часто мешало карьере талантливых людей.",
                            "target_lang_translations": {
                                "en": "In the last century, <w>low-born</w> origin often hindered the careers of talented people."
                            }
                        },
                        {
                            "text": "Несмотря на <w>неблагородную</w> семью, он смог добиться высокого положения при дворе.",
                            "target_lang_translations": {
                                "en": "Despite a <w>non-noble</w> family, he managed to achieve a high position at court."
                            }
                        }
                    ],
                    "synonyms": [
                        "незнатный",
                        "простонародный",
                        "безродный"
                    ],
                    "antonyms": [
                        "знатный",
                        "дворянский",
                        "родовитый",
                        "благородный"
                    ],
                    "common_phrases": [
                        "неблагородное происхождение",
                        "неблагородное сословие",
                        "неблагородная кровь"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "истор."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Not belonging to the nobility or upper class by origin."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "low-born",
                                "target_lang_sense_clarification": "of humble birth"
                            },
                            {
                                "target_lang_word": "common",
                                "target_lang_sense_clarification": "not of the nobility"
                            },
                            {
                                "target_lang_word": "non-noble",
                                "target_lang_sense_clarification": "literal status"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27b0b814-365b-35ed-9cee-e0393aa24201",
                    "sense_definition": "Окисляющийся, подверженный коррозии (о металлах); не относящийся к категории драгоценных.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "material_properties",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Железо — это <w>неблагородный</w> металл, который быстро ржавеет во влажной среде.",
                            "target_lang_translations": {
                                "en": "Iron is a <w>base</w> metal that rusts quickly in a humid environment."
                            }
                        },
                        {
                            "text": "Ювелир объяснил, что этот сплав содержит <w>неблагородные</w> примеси.",
                            "target_lang_translations": {
                                "en": "The jeweler explained that this alloy contains <w>base</w> impurities."
                            }
                        }
                    ],
                    "synonyms": [
                        "недрагоценный"
                    ],
                    "antonyms": [
                        "благородный",
                        "драгоценный"
                    ],
                    "common_phrases": [
                        "неблагородный металл",
                        "неблагородный материал"
                    ],
                    "target_lang_definitions": {
                        "en": "Oxidizing, subject to corrosion (about metals); not belonging to the category of precious metals."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "base",
                                "target_lang_sense_clarification": "chemistry/metallurgy term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ae1e713-580d-3557-ae7e-01eccfe7f46b",
                    "sense_definition": "Лишённый изящества, грубый на вид или в манерах.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "aesthetic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его <w>неблагородные</w> черты лица казались ещё более грубыми в тусклом свете.",
                            "target_lang_translations": {
                                "en": "His <w>coarse</w> features seemed even rougher in the dim light."
                            }
                        },
                        {
                            "text": "У неё была резкая, <w>неблагородная</w> походка, лишённая всякой грации.",
                            "target_lang_translations": {
                                "en": "She had a sharp, <w>unrefined</w> gait, devoid of any grace."
                            }
                        }
                    ],
                    "synonyms": [
                        "грубый",
                        "неизящный",
                        "простоватый"
                    ],
                    "antonyms": [
                        "изящный",
                        "утончённый",
                        "аристократичный"
                    ],
                    "target_lang_definitions": {
                        "en": "Lacking elegance, coarse in appearance or manners."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "coarse",
                                "target_lang_sense_clarification": "rough in texture or manner"
                            },
                            {
                                "target_lang_word": "unrefined",
                                "target_lang_sense_clarification": "lacking sophistication"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3da3bc4b-0f1b-3d10-b243-73b6e301de9f",
                    "sense_definition": "Не имеющий чистой породы (о животных); смешанного происхождения.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот пёс был <w>неблагородный</w>, но очень умный и преданный.",
                            "target_lang_translations": {
                                "en": "This dog was <w>mixed-breed</w>, but very smart and loyal."
                            }
                        },
                        {
                            "text": "Мы взяли из приюта <w>неблагородную</w> кошку, и она стала любимицей семьи.",
                            "target_lang_translations": {
                                "en": "We took a <w>mixed-breed</w> cat from the shelter, and she became the family favorite."
                            }
                        }
                    ],
                    "synonyms": [
                        "беспородный",
                        "дворняжка"
                    ],
                    "antonyms": [
                        "породистый",
                        "чистокровный"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разг."
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Not having a pure breed (about animals); of mixed origin."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mixed-breed",
                                "target_lang_sense_clarification": "of mixed lineage"
                            },
                            {
                                "target_lang_word": "mongrel",
                                "target_lang_sense_clarification": "often derogatory or specific to dogs"
                            }
                        ]
                    }
                }
            ]
        }

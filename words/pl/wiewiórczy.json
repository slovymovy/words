    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "59a98d19-0702-3a22-b3e7-d51d44771f6a",
                    "sense_definition": "Związany z wiewiórką, dotyczący wiewiórki lub do niej należący.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "relational",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W parku znaleźliśmy małą <w>wiewiórczą</w> dziuplę wysoko na starym dębie.",
                            "target_lang_translations": {
                                "ru": "В парке мы нашли маленькое <w>беличье</w> дупло высоко на старом дубе."
                            }
                        },
                        {
                            "text": "Ten puszysty <w>wiewiórczy</w> ogon pomaga zwierzęciu utrzymać równowagę podczas skoków.",
                            "target_lang_translations": {
                                "ru": "Этот пушистый <w>беличий</w> хвост помогает животному удерживать равновесие во время прыжков."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wiewiórczy ogon",
                        "wiewiórcza dziupla",
                        "wiewiórcze zapasy",
                        "wiewiórcze futro"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с белкой, касающийся белки или принадлежащий ей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беличий",
                                "target_lang_sense_clarification": "относящийся к белке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c2704fb-499b-3fc0-8bcd-1d6edbc60d32",
                    "sense_definition": "Przypominający wiewiórkę, mający cechy charakterystyczne dla wiewiórki (np. zwinność, kolor).",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopak wspiął się na wysoki płot z prawdziwie <w>wiewiórczą</w> zwinnością.",
                            "target_lang_translations": {
                                "ru": "Мальчик вскарабкался на высокий забор с поистине <w>беличьей</w> ловкостью."
                            }
                        },
                        {
                            "text": "Jej rude włosy miały w słońcu piękny, <w>wiewiórczy</w> odcień.",
                            "target_lang_translations": {
                                "ru": "Её рыжие волосы имели на солнце красивый <w>беличий</w> оттенок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wiewiórcza zwinność",
                        "wiewiórczy spryt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Напоминающий белку, имеющий характерные черты белки (например, ловкость, цвет)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беличий",
                                "target_lang_sense_clarification": "напоминающий белку"
                            }
                        ]
                    }
                }
            ]
        }
    ]

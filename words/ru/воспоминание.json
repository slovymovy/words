            "senses": [
                {
                    "sense_id": "9b2812f4-5257-340f-af8b-da72b9eeff5c",
                    "sense_definition": "То, что сохранилось в памяти; мысленное воспроизведение чего-либо прошедшего.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "memory_content",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это <воспоминание> всегда вызывает у меня радостную улыбку.",
                            "target_lang_translations": {
                                "en": "This <w>memory</w> always brings a joyful smile to my face."
                            }
                        },
                        {
                            "text": "Старые фотографии пробудили в нём давно забытые <воспоминания> о детстве.",
                            "target_lang_translations": {
                                "en": "Old photos awakened long-forgotten <w>memories</w> of childhood in him."
                            }
                        }
                    ],
                    "antonyms": [
                        "забвение"
                    ],
                    "common_phrases": [
                        "детские воспоминания",
                        "приятное воспоминание",
                        "предаваться воспоминаниям",
                        "горькие воспоминания",
                        "свежо в воспоминании"
                    ],
                    "target_lang_definitions": {
                        "en": "Something that is preserved in the mind; a mental image or thought of the past."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "memory",
                                "target_lang_sense_clarification": "a specific thing remembered"
                            },
                            {
                                "target_lang_word": "recollection",
                                "target_lang_sense_clarification": "the action or result of remembering something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d5cc9bbb-cff3-38b5-982a-db7a16f4eeb3",
                    "sense_definition": "Литературные записки или рассказы о прошлых событиях, написанные их участником или очевидцем.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "literary_genre",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Известный писатель решил опубликовать свои <воспоминания> о жизни за границей.",
                            "target_lang_translations": {
                                "en": "The famous writer decided to publish his <w>memoirs</w> about life abroad."
                            }
                        },
                        {
                            "text": "Мы читали <воспоминания> современников о великом поэте.",
                            "target_lang_translations": {
                                "en": "We read the <w>memoirs</w> of contemporaries about the great poet."
                            }
                        }
                    ],
                    "synonyms": [
                        "мемуары",
                        "записки"
                    ],
                    "common_phrases": [
                        "писать воспоминания",
                        "книга воспоминаний",
                        "литературные воспоминания"
                    ],
                    "target_lang_definitions": {
                        "en": "Written accounts or stories about past events, typically written by a participant or eyewitness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "memoirs",
                                "target_lang_sense_clarification": "written historical or personal account"
                            },
                            {
                                "target_lang_word": "reminiscences",
                                "target_lang_sense_clarification": "collection of memories, often written"
                            }
                        ]

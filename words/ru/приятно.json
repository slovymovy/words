    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "8afa47ed-f49b-3884-a024-29526fd95ee6",
                    "sense_definition": "О чувстве удовольствия, радости или удовлетворения (используется как сказуемое).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне очень <w>приятно</w> с вами познакомиться.",
                            "target_lang_translations": {
                                "en": "It is very <w>nice</w> to meet you."
                            }
                        },
                        {
                            "text": "Нам было <w>приятно</w> получить ваше письмо.",
                            "target_lang_translations": {
                                "en": "We were <w>pleased</w> to receive your letter."
                            }
                        }
                    ],
                    "antonyms": [
                        "неприятно"
                    ],
                    "common_phrases": [
                        "очень приятно",
                        "мне приятно",
                        "приятно слышать",
                        "приятно познакомиться"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to express a feeling of pleasure, joy, or satisfaction (often used as a predicate)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nice",
                                "target_lang_sense_clarification": "common in greetings and expressions of satisfaction"
                            },
                            {
                                "target_lang_word": "pleased",
                                "target_lang_sense_clarification": "feeling or showing pleasure and satisfaction"
                            },
                            {
                                "target_lang_word": "pleasant",
                                "target_lang_sense_clarification": "giving a sense of happy satisfaction or enjoyment"
                            }
                        ]
                    }
                },
                {

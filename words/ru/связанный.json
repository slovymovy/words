            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3aaf1e49-7949-30ad-86f4-829bd7335ab9",
                    "sense_definition": "Находящийся в какой-либо связи, имеющий отношение к кому-либо или чему-либо; обусловленный чем-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "connected_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот сложный вопрос тесно <w>связан</w> с экономическим развитием нашего региона.",
                            "target_lang_translations": {
                                "en": "This complex issue is closely <w>connected</w> with the economic development of our region."
                            }
                        },
                        {
                            "text": "Мы обсуждали различные проблемы, <w>связанные</w> с переездом в новую квартиру.",
                            "target_lang_translations": {
                                "en": "We discussed various problems <w>associated</w> with moving to a new apartment."
                            }
                        }
                    ],
                    "synonyms": [
                        "свободный"
                    ],
                    "antonyms": [
                        "несвязанный"
                    ],
                    "common_phrases": [
                        "тесно связанный",
                        "неразрывно связанный",
                        "связанный с работой",
                        "связанный с риском"
                    ],
                    "target_lang_definitions": {
                        "en": "Connected to or having a relationship with someone or something; conditioned by something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "connected",
                                "target_lang_sense_clarification": "joined or linked together"
                            },
                            {
                                "target_lang_word": "associated",
                                "target_lang_sense_clarification": "connected with something else, often causally or conceptually"
                            },
                            {
                                "target_lang_word": "related",
                                "target_lang_sense_clarification": "having a relationship or connection"
                            },
                            {
                                "target_lang_word": "linked",
                                "target_lang_sense_clarification": "connected physically or logically"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c796134a-8ef4-38f2-be7d-fee8c2dbd1ee",
                    "sense_definition": "Чувствующий неловкость, стеснение; лишённый непринуждённости и внутренней свободы.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "psychological_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В присутствии строгого начальника молодой сотрудник чувствовал себя <w>связанным</w>.",
                            "target_lang_translations": {
                                "en": "In the presence of the strict boss, the young employee felt <w>constrained</w>."
                            }
                        },
                        {
                            "text": "Её движения были <w>связанными</w> и неуверенными из-за сильного волнения.",
                            "target_lang_translations": {
                                "en": "Her movements were <w>stiff</w> and uncertain due to strong agitation."
                            }
                        }
                    ],
                    "common_phrases": [
                        "чувствовать себя связанным",
                        "выглядеть связанным"
                    ],
                    "target_lang_definitions": {
                        "en": "Feeling awkward or restricted; lacking ease and internal freedom."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "constrained",
                                "target_lang_sense_clarification": "forced, unnatural, or restricted in behavior"
                            },
                            {
                                "target_lang_word": "inhibited",
                                "target_lang_sense_clarification": "unable to act in a relaxed and natural way"
                            },
                            {
                                "target_lang_word": "stiff",
                                "target_lang_sense_clarification": "lacking ease or grace (of movements)"
                            }
                        ]
                    }
                }
            ]
        },

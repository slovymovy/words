    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "db451310-396c-3490-b61e-c1b2e9d770ad",
                    "sense_definition": "Producing a feeling of comfort and new energy, especially by being cool, clean, or pleasantly different.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "adj_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A cold glass of lemonade is very <w>refreshing</w> on a hot summer day.",
                            "target_lang_translations": {
                                "ru": "Стакан холодного лимонада очень <w>освежает</w> в жаркий летний день."
                            }
                        },
                        {
                            "text": "It is <w>refreshing</w> to hear such an honest opinion from a politician.",
                            "target_lang_translations": {
                                "ru": "Слышать столь честное мнение от политика — это так <w>свежо</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "invigorating",
                        "revitalizing",
                        "cooling",
                        "stimulating"
                    ],
                    "antonyms": [
                        "tiring",
                        "exhausting",
                        "wearying",
                        "stale"
                    ],
                    "common_phrases": [
                        "refreshing change",
                        "refreshing drink",
                        "refreshing breeze",
                        "refreshing shower"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызывающий чувство комфорта и прилив сил, особенно благодаря прохладе, чистоте или приятной новизне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "освежающий",
                                "target_lang_sense_clarification": "о напитках, воздухе или ощущениях"
                            },
                            {
                                "target_lang_word": "бодрящий",
                                "target_lang_sense_clarification": "придающий энергию"
                            },
                            {
                                "target_lang_word": "свежий",
                                "target_lang_sense_clarification": "о переменах, идеях или впечатлениях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86370c88-39f7-378f-9c99-2c745073286a",

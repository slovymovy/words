{
    "word_family": [
        "ervandaan",
        "hiervandaan",
        "daarvandaan",
        "waarvandaan"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "456f2cac-2674-3109-8f95-345be8f0a88a",
                    "sense_definition": "Aanduiding van de plaats of bron waar iemand of iets zijn oorsprong heeft, vaak gebruikt in combinatie met werkwoorden als 'komen'.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "origin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Waar kom je <w>vandaan</w>?",
                            "target_lang_translations": {
                                "en": "Where do you come <w>from</w>?",
                                "ru": "<w>Откуда</w> ты родом?"
                            }
                        },
                        {
                            "text": "Ik weet niet waar dat vreemde geluid <w>vandaan</w> komt.",
                            "target_lang_translations": {
                                "en": "I don't know where that strange sound is coming <w>from</w>.",
                                "ru": "Я не знаю, <w>откуда</w> исходит этот странный звук."
                            }
                        },
                        {
                            "text": "Veel van onze producten komen uit het buitenland <w>vandaan</w>.",
                            "target_lang_translations": {
                                "en": "Many of our products come <w>from</w> abroad.",
                                "ru": "Многие наши продукты поступают <w>из</w>-за границы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "waar vandaan",
                        "ergens vandaan komen",
                        "daar vandaan"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates the place or source where someone or something originates.",
                        "ru": "Указание на место или источник происхождения кого-либо или чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "from",
                                "target_lang_sense_clarification": "indicating origin or source"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "откуда",
                                "target_lang_sense_clarification": "в сочетании с 'waar' (где)"
                            },
                            {
                                "target_lang_word": "из",
                                "target_lang_sense_clarification": "указание на происхождение"
                            },
                            {
                                "target_lang_word": "от",
                                "target_lang_sense_clarification": "указание на источник"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca6c10b1-7b49-3e23-8928-f46766fff04d",
                    "sense_definition": "In een richting weg van een bepaalde plaats, object of persoon.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "direction_away",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ga onmiddellijk bij dat gevaarlijke raam <w>vandaan</w>!",
                            "target_lang_translations": {
                                "en": "Get <w>away from</w> that dangerous window immediately!",
                                "ru": "Отойди немедленно <w>от</w> этого опасного окна!"
                            }
                        },
                        {
                            "text": "Je moet bij die agressieve hond <w>vandaan</w> blijven.",
                            "target_lang_translations": {
                                "en": "You must stay <w>away from</w> that aggressive dog.",
                                "ru": "Тебе нужно держаться <w>подальше</w> от этой агрессивной собаки."
                            }
                        }
                    ],
                    "synonyms": [
                        "weg"
                    ],
                    "antonyms": [
                        "naartoe"
                    ],
                    "common_phrases": [
                        "ergens vandaan blijven",
                        "ergens vandaan halen"
                    ],
                    "target_lang_definitions": {
                        "en": "In a direction away from a specific place, object, or person.",
                        "ru": "В направлении прочь от определенного места, объекта или человека."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "away from",
                                "target_lang_sense_clarification": "indicating movement or distance from something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "от",
                                "target_lang_sense_clarification": "предлог удаления или дистанцирования"
                            },
                            {
                                "target_lang_word": "прочь",
                                "target_lang_sense_clarification": "наречие направления"
                            },
                            {
                                "target_lang_word": "подальше",
                                "target_lang_sense_clarification": "на расстоянии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "024b6ada-79de-3aec-bc50-60073ac669af",
                    "sense_definition": "Anders worden; niet hetzelfde blijven",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "change_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het weer kan in dit land heel snel <w>veranderen</w>.",
                            "target_lang_translations": {
                                "en": "The weather can <w>change</w> very quickly in this country."
                            }
                        },
                        {
                            "text": "Er is in de afgelopen jaren gelukkig niets <w>veranderd</w>.",
                            "target_lang_translations": {
                                "en": "Fortunately, nothing has <w>changed</w> in recent years."
                            }
                        }
                    ],
                    "common_phrases": [
                        "van gedachten veranderen",
                        "tijden veranderen",
                        "de situatie verandert"
                    ],
                    "target_lang_definitions": {
                        "en": "To become different; not to stay the same"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "change",
                                "target_lang_sense_clarification": "to become different"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d26ad53-f0b7-3e52-9328-d30c5825fef0",
                    "sense_definition": "Iets of iemand anders maken; wijzigen of aanpassen",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "change_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil de kleur van de muur <w>veranderen</w>.",
                            "target_lang_translations": {
                                "en": "I want to <w>change</w> the color of the wall."
                            }
                        },
                        {
                            "text": "De schrijver heeft het einde van het boek <w>veranderd</w>.",
                            "target_lang_translations": {
                                "en": "The writer has <w>changed</w> the ending of the book."
                            }
                        }
                    ],
                    "synonyms": [
                        "wijzigen",
                        "aanpassen",
                        "modificeren"
                    ],
                    "common_phrases": [
                        "de wereld veranderen",
                        "plannen veranderen",
                        "van onderwerp veranderen"
                    ],
                    "target_lang_definitions": {
                        "en": "To make something or someone different; to modify or adjust"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "change",
                                "target_lang_sense_clarification": "to make different"
                            },
                            {
                                "target_lang_word": "alter",
                                "target_lang_sense_clarification": "to modify"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66b645f3-4c93-33ab-88d9-05d322de692e",
                    "sense_definition": "Zichzelf anders maken of ontwikkelen qua karakter of uiterlijk",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "change_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is moeilijk om je als mens echt te <w>veranderen</w>.",
                            "target_lang_translations": {
                                "en": "It is difficult to really <w>change</w> as a person."
                            }
                        },
                        {
                            "text": "Hij probeerde zich te <w>veranderen</w> om indruk op haar te maken.",
                            "target_lang_translations": {
                                "en": "He tried to <w>change</w> himself to impress her."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zich veranderen"
                    ],
                    "target_lang_definitions": {

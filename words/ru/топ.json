                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "top",
                                "target_lang_sense_clarification": "sexual role"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41754c53-9a1c-3a77-8908-09abd923d653",
                    "sense_definition": "Верхний конец мачты на парусном судне.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "nautical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Матрос ловко забрался на самый <w>топ</w> мачты.",
                            "target_lang_translations": {
                                "en": "The sailor deftly climbed to the very <w>top</w> of the mast."
                            }
                        },
                        {
                            "text": "Флаг развевался на <w>топе</w> высокой грот-мачты.",
                            "target_lang_translations": {
                                "en": "The flag fluttered at the <w>masthead</w> of the tall mainmast."
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "007bbc95-17aa-33ad-bf56-0f4392d5dd39",
                    "sense_definition": "A piece of furniture with shelves, drawers, or doors, used for storing or displaying items.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please put the clean plates back in the kitchen <w>cabinet</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, убери чистые тарелки обратно в кухонный <w>шкаф</w>."
                            }
                        },
                        {
                            "text": "He opened the filing <w>cabinet</w> to look for the important documents.",
                            "target_lang_translations": {
                                "ru": "Он открыл картотечный <w>шкаф</w>, чтобы найти важные документы."
                            }
                        }
                    ],
                    "synonyms": [
                        "cabinet department",
                        "program cabinet",
                        "superagency",
                        "cabinet agency"
                    ],
                    "common_phrases": [
                        "kitchen cabinet",
                        "medicine cabinet",
                        "filing cabinet",
                        "display cabinet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет мебели с полками, ящиками или дверцами, используемый для хранения или демонстрации вещей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шкаф",
                                "target_lang_sense_clarification": "мебель"
                            },
                            {
                                "target_lang_word": "шкафчик",
                                "target_lang_sense_clarification": "небольшой шкаф"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0be5cc8c-d60e-3009-b92e-9d59ee0767f1",
                    "sense_definition": "A group of high-ranking government officials or ministers who advise the head of state or government and help create policy.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The Prime Minister held a <w>cabinet</w> meeting to discuss the economic crisis.",
                            "target_lang_translations": {
                                "ru": "Премьер-министр провел заседание <w>кабинета министров</w>, чтобы обсудить экономический кризис."
                            }
                        },
                        {
                            "text": "She was appointed as a <w>cabinet</w> minister in the new administration.",
                            "target_lang_translations": {
                                "ru": "Она была назначена министром <w>кабинета</w> в новой администрации."
                            }
                        }
                    ],
                    "synonyms": [
                        "cabinet department",
                        "program cabinet",
                        "superagency",
                        "cabinet agency"
                    ],
                    "common_phrases": [
                        "cabinet meeting",
                        "cabinet minister",
                        "cabinet reshuffle",
                        "shadow cabinet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа высших государственных чиновников или министров, которые консультируют главу государства и участвуют в управлении страной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кабинет министров",
                                "target_lang_sense_clarification": "правительство"
                            },
                            {
                                "target_lang_word": "кабинет",
                                "target_lang_sense_clarification": "политический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5e8665b-dc1b-33c7-a0ca-2ed977c0ffce",
                    "sense_definition": "A protective housing or casing for mechanical or electrical equipment, such as a loudspeaker or computer.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wires are hidden inside the metal <w>cabinet</w> of the machine.",
                            "target_lang_translations": {
                                "ru": "Провода спрятаны внутри металлического <w>корпуса</w> станка."
                            }
                        },
                        {
                            "text": "He built a custom wooden <w>cabinet</w> for his new speakers.",
                            "target_lang_translations": {
                                "ru": "Он изготовил на заказ деревянный <w>корпус</w> для своих новых колонок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "speaker cabinet",
                        "arcade cabinet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Защитный корпус или оболочка для механического или электрического оборудования, например, динамика или компьютера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корпус",
                                "target_lang_sense_clarification": "оболочка устройства"

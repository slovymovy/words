                {
                    "sense_id": "445e3227-e622-3f51-a515-6d7f9a1d5216",
                    "sense_definition": "Niet schoon, vies of onhygiënisch.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het water uit de oude put was <w>onrein</w> en gevaarlijk om te drinken.",
                            "target_lang_translations": {
                                "en": "The water from the old well was <w>unclean</w> and dangerous to drink.",
                                "ru": "Вода из старого колодца была <w>нечистой</w> и опасной для питья."
                            }
                        },
                        {
                            "text": "De arts weigerde te werken met <w>onreine</w> instrumenten om infecties te voorkomen.",
                            "target_lang_translations": {
                                "en": "The doctor refused to work with <w>unclean</w> instruments to prevent infections.",
                                "ru": "Врач отказался работать с <w>грязными</w> инструментами, чтобы предотвратить инфекции."
                            }
                        }
                    ],
                    "synonyms": [
                        "vuil",
                        "vies",
                        "smerig",
                        "onzindelijk"
                    ],
                    "antonyms": [
                        "rein"
                    ],
                    "common_phrases": [
                        "onrein water"
                    ],
                    "target_lang_definitions": {
                        "en": "Not clean, dirty, or unhygienic.",
                        "ru": "Нечистый, грязный или негигиеничный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unclean",
                                "target_lang_sense_clarification": "not clean or sanitary"
                            },
                            {
                                "target_lang_word": "dirty",
                                "target_lang_sense_clarification": "covered with dirt or grime"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "грязный",
                                "target_lang_sense_clarification": "физически нечистый"
                            },
                            {
                                "target_lang_word": "нечистый",
                                "target_lang_sense_clarification": "содержащий примеси или грязь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c05c7af0-0e2b-3180-99dd-6218d70715dd",
                    "sense_definition": "Niet in overeenstemming met religieuze voorschriften of morele normen; ritueel onzuiver of zondig.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "moral_religious",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In bepaalde religies worden varkens beschouwd als <w>onreine</w> dieren die men niet mag eten.",
                            "target_lang_translations": {
                                "en": "In certain religions, pigs are considered <w>unclean</w> animals that one may not eat.",
                                "ru": "В некоторых религиях свиньи считаются <w>нечистыми</w> животными, которых нельзя есть."
                            }
                        },
                        {
                            "text": "Hij probeerde zijn geest te zuiveren van alle <w>onreine</w> gedachten.",
                            "target_lang_translations": {
                                "en": "He tried to purify his mind of all <w>impure</w> thoughts.",
                                "ru": "Он пытался очистить свой разум от всех <w>нечистых</w> мыслей."
                            }
                        }
                    ],
                    "synonyms": [
                        "onzuiver",
                        "zondig",
                        "immoreel"
                    ],
                    "antonyms": [
                        "rein"
                    ],
                    "common_phrases": [
                        "onreine dieren",
                        "onreine gedachten",
                        "onreine geesten"
                    ],
                    "target_lang_definitions": {
                        "en": "Not in accordance with religious regulations or moral standards; ritually impure or sinful.",
                        "ru": "Не соответствующий религиозным предписаниям или моральным нормам; ритуально нечистый или греховный."
                    },

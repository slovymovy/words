    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fa2226cc-3ff4-3570-9628-f204463f230f",
                    "sense_definition": "A person, usually a man, who is considered annoying, contemptible, or pitiable.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person_types",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He knew that the old <w>blighter</w> had to be humored in certain small ways.",
                            "target_lang_translations": {
                                "ru": "Он знал, что этому старому <w>хрычу</w> нужно потакать в некоторых мелочах."
                            }
                        },
                        {
                            "text": "The poor <w>blighter</w> has been waiting in the rain for over an hour.",
                            "target_lang_translations": {
                                "ru": "Этот <w>бедняга</w> прождал под дождем больше часа."
                            }
                        },
                        {
                            "text": "I cannot believe that lucky <w>blighter</w> won the lottery again.",
                            "target_lang_translations": {
                                "ru": "Не могу поверить, что этот удачливый <w>гад</w> снова выиграл в лотерею."
                            }
                        }
                    ],
                    "synonyms": [
                        "chap",
                        "fellow",
                        "wretch",
                        "rascal",
                        "beggar"
                    ],
                    "common_phrases": [
                        "poor blighter",
                        "lucky blighter",
                        "little blighter",
                        "old blighter"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        },
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек (обычно мужчина), которого считают раздражающим, презренным или жалким"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бедняга",
                                "target_lang_sense_clarification": "о человеке, вызывающем жалость"
                            },
                            {
                                "target_lang_word": "мерзавец",
                                "target_lang_sense_clarification": "о подлом, презренном человеке"
                            },
                            {
                                "target_lang_word": "негодяй",
                                "target_lang_sense_clarification": "бранное слово для неприятного человека"
                            },
                            {
                                "target_lang_word": "гад",
                                "target_lang_sense_clarification": "разг., о противном человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6473730-c07b-36ac-b688-f6efc20acf1e",
                    "sense_definition": "Something or someone that causes damage, ruin, or decay.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The severe frost acted as a <w>blighter</w> of the early spring harvest.",
                            "target_lang_translations": {
                                "ru": "Сильный мороз стал <w>губителем</w> раннего весеннего урожая."
                            }
                        },
                        {
                            "text": "He was seen by many as a <w>blighter</w> of youthful dreams and ambitions.",
                            "target_lang_translations": {
                                "ru": "Многие видели в нем <w>разрушителя</w> юношеских надежд и амбиций."
                            }
                        }
                    ],
                    "synonyms": [
                        "spoiler",
                        "destroyer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то или кто-то, причиняющий вред, разрушение или увядание"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "губитель",
                                "target_lang_sense_clarification": "тот, кто губит или уничтожает"
                            },
                            {
                                "target_lang_word": "разрушитель",
                                "target_lang_sense_clarification": "источник разрушения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "guide",
        "guidable",
        "misguide",
        "misguided"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ea8c0d91-8383-3830-936c-cb4d01790833",
                    "sense_definition": "Help or advice that tells you what to do, often provided by an authority or expert.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "advice_counsel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Students often seek <w>guidance</w> from their teachers when choosing a career path.",
                            "target_lang_translations": {
                                "ru": "Студенты часто обращаются за <w>советом</w> к своим преподавателям при выборе карьерного пути."
                            }
                        },
                        {
                            "text": "The manual provides clear <w>guidance</w> on how to operate the new machinery safely.",
                            "target_lang_translations": {
                                "ru": "В руководстве даются четкие <w>указания</w> по безопасной эксплуатации нового оборудования."
                            }
                        }
                    ],
                    "synonyms": [
                        "advice",
                        "counsel",
                        "instruction"
                    ],
                    "common_phrases": [
                        "seek guidance",
                        "offer guidance",
                        "provide guidance",
                        "spiritual guidance",
                        "career guidance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помощь или совет о том, как поступить, часто предоставляемые авторитетным лицом или экспертом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совет",
                                "target_lang_sense_clarification": "рекомендация"
                            },
                            {
                                "target_lang_word": "руководство",
                                "target_lang_sense_clarification": "наставление или инструкция"
                            },
                            {
                                "target_lang_word": "указания",
                                "target_lang_sense_clarification": "конкретные инструкции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b684228d-9773-32f4-b521-38fb2ab23d82",
                    "sense_definition": "The act of directing, supervising, or leading someone or something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "supervision_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The research project was completed successfully under the <w>guidance</w> of a senior scientist.",
                            "target_lang_translations": {
                                "ru": "Исследовательский проект был успешно завершен под <w>руководством</w> старшего научного сотрудника."
                            }
                        },
                        {
                            "text": "Parental <w>guidance</w> is strongly recommended for young children watching this film.",
                            "target_lang_translations": {
                                "ru": "Маленьким детям настоятельно рекомендуется смотреть этот фильм под <w>присмотром</w> родителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "direction",
                        "supervision",
                        "leadership",
                        "management"
                    ],
                    "antonyms": [
                        "misguidance"
                    ],
                    "common_phrases": [
                        "under the guidance of",
                        "parental guidance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс управления, наблюдения или руководства кем-либо или чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "руководство",
                                "target_lang_sense_clarification": "лидерство, управление процессом"
                            },
                            {
                                "target_lang_word": "присмотр",
                                "target_lang_sense_clarification": "наблюдение (например, за детьми)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "085d7781-1313-346b-8d93-b3bc4e361142",
                    "sense_definition": "A system or process used to control the path or direction of a vehicle or missile.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The missile's <w>guidance</w> system locked onto the target immediately after launch.",
                            "target_lang_translations": {
                                "ru": "Система <w>наведения</w> ракеты захватила цель сразу после запуска."
                            }
                        },
                        {
                            "text": "Modern aircraft rely on sophisticated <w>guidance</w> technology for safe navigation in bad weather.",
                            "target_lang_translations": {
                                "ru": "Современные самолеты полагаются на сложные технологии <w>наведения</w> для безопасной навигации в плохую погоду."
                            }
                        }
                    ],
                    "synonyms": [
                        "steering",
                        "navigation",
                        "control"
                    ],
                    "common_phrases": [
                        "guidance system",
                        "laser guidance",
                        "missile guidance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система или процесс управления траекторией или направлением движения транспортного средства или ракеты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наведение",
                                "target_lang_sense_clarification": "управление курсом ракеты или прибора"
                            },
                            {
                                "target_lang_word": "управление",
                                "target_lang_sense_clarification": "контроль движения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
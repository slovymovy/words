{
    "word_family": [
        "they",
        "their",
        "themselves"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "8098123b-34af-347f-8485-62c53da7c1ad",
                    "sense_definition": "That or those belonging to them; the possessive form of <w>they</w>, used without a following noun.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The blue car is ours, but the red one is <w>theirs</w>.",
                            "target_lang_translations": {
                                "ru": "Синяя машина — наша, а красная — <w>их</w>."
                            }
                        },
                        {
                            "text": "A good friend of <w>theirs</w> came to visit us yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера нас навестил один <w>их</w> хороший друг."
                            }
                        }
                    ],
                    "antonyms": [
                        "ours",
                        "yours",
                        "mine"
                    ],
                    "common_phrases": [
                        "of theirs",
                        "friend of theirs",
                        "fault of theirs"
                    ],
                    "target_lang_definitions": {
                        "ru": "То или те, что принадлежат им; притяжательная форма местоимения <w>they</w>, используемая без последующего существительного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "их",
                                "target_lang_sense_clarification": "принадлежащий им"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b55b9eeb-530c-3b79-91c8-43af27c75df5",
                    "sense_definition": "Their house or home.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to go back to <w>theirs</w> after the restaurant closed.",
                            "target_lang_translations": {
                                "ru": "Мы решили поехать <w>к ним</w> после закрытия ресторана."
                            }
                        },
                        {
                            "text": "Shall we meet at <w>theirs</w> before heading to the party?",
                            "target_lang_translations": {
                                "ru": "Может, встретимся <w>у них</w> перед тем, как отправиться на вечеринку?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "back to theirs",
                        "over to theirs",
                        "at theirs"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to refer to someone's place of residence"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Их дом или жилище."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "к ним",
                                "target_lang_sense_clarification": "в их дом (направление)"
                            },
                            {
                                "target_lang_word": "у них",
                                "target_lang_sense_clarification": "в их доме (местонахождение)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
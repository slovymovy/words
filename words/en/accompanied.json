            "senses": [
                {
                    "sense_id": "65b805e2-ca9f-3dbd-a0b2-c9e05ab0ecc4",
                    "sense_definition": "Having accompaniment; being part of a group of at least two.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Children must be <accompanied> by an adult at all times.",
                            "target_lang_translations": {
                                "ru": "Дети должны постоянно <сопровождаться> взрослыми."
                            }
                        },
                        {
                            "text": "The <accompanied> baggage allowance is strictly enforced.",
                            "target_lang_translations": {
                                "ru": "Норма провоза <сопровождаемого> багажа строго соблюдается."
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cff2b424-5bb6-3c74-87cd-75471ec800a9",
                    "sense_definition": "Een instrument waarvan de naald het magnetische noorden aanwijst, gebruikt om de richting te bepalen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "navigatie_instrument",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De wandelaar keek op zijn <w>kompas</w> om te zien waar het noorden was.",
                            "target_lang_translations": {
                                "ru": "Турист посмотрел на свой <w>компас</w>, чтобы узнать, где север."
                            }
                        },
                        {
                            "text": "Zonder een goed <w>kompas</w> kun je makkelijk verdwalen op zee.",
                            "target_lang_translations": {
                                "ru": "Без хорошего <w>компаса</w> можно легко заблудиться в море."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op het kompas",
                        "het kompas lezen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прибор, стрелка которого указывает на магнитный север, используемый для определения направления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "компас",
                                "target_lang_sense_clarification": "прибор для навигации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "699854df-75d5-30ed-8a63-e7fc68e19051",
                    "sense_definition": "Een richtlijn, principe of overtuiging die dient als leidraad voor gedrag of beslissingen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figuurlijk_leidraad",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn moreel <w>kompas</w> weerhield hem ervan om te liegen tegen zijn baas.",
                            "target_lang_translations": {
                                "ru": "Его моральный <w>компас</w> удержал его от лжи начальнику."
                            }
                        },
                        {
                            "text": "In tijden van crisis dient de grondwet als ons belangrijkste <w>kompas</w>.",
                            "target_lang_translations": {
                                "ru": "Во времена кризиса конституция служит нашим главным <w>компасом</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "moreel kompas",
                        "varen op eigen kompas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Руководящий принцип или убеждение, служащее ориентиром для поведения или принятия решений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "компас",
                                "target_lang_sense_clarification": "переносное значение: ориентир"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "26a2ef59-a3ae-39be-9a1a-2a4ab2599d8d",
                    "sense_definition": "Een sterrenbeeld aan de zuidelijke hemel, gelegen ten zuiden van de dierenriem.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "astronomie",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "Het sterrenbeeld <w>Kompas</w> is vanuit Nederland nauwelijks zichtbaar.",
                            "target_lang_translations": {
                                "ru": "Созвездие <w>Компас</w> едва видно из Нидерландов."
                            }
                        },
                        {
                            "text": "Astronomen hebben nieuwe sterren ontdekt in het <w>Kompas</w>.",
                            "target_lang_translations": {
                                "ru": "Астрономы открыли новые звезды в <w>Компасе</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Pyx",
                        "Pyxi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Созвездие южного полушария неба, расположенное к югу от зодиака."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Компас",
                                "target_lang_sense_clarification": "созвездие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

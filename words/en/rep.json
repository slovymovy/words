        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dfb0d43b-c7ff-373d-96a5-989548a6df22",
                    "sense_definition": "To act as a representative for someone or something, or to show support for a group or place.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_representative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is always <w>repping</w> his hometown by wearing the local team's jersey.",
                            "target_lang_translations": {
                                "ru": "Он всегда <w>представляет</w> свой родной город, надевая майку местной команды."
                            }
                        },
                        {
                            "text": "The lawyer <w>repped</w> the client during the difficult negotiations.",
                            "target_lang_translations": {
                                "ru": "Адвокат <w>представлял</w> клиента во время сложных переговоров."
                            }

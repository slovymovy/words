    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7e399d91-f3b9-3cc7-93f4-4f9242ab8278",
                    "sense_definition": "O wielu osobach: zdjąć z siebie ubranie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "reflexive_undress",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy goście weszli do ciepłego domu i szybko się <porozbierali>.",
                            "target_lang_translations": {
                                "ru": "Все гости вошли в теплый дом и быстро <разделись>."
                            }
                        },
                        {
                            "text": "Dzieci wróciły ze śniegu i od razu <porozbierały> się w szatni.",
                            "target_lang_translations": {
                                "ru": "Дети вернулись со снега и сразу <разделись> в раздевалке."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozebrać się"
                    ],
                    "antonyms": [
                        "poubierać się"
                    ],
                    "common_phrases": [
                        "wszyscy się porozbierali",
                        "szybko się porozbierać"
                    ],
                    "target_lang_definitions": {
                        "ru": "О многих людях: снять с себя одежду, раздеться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раздеться",
                                "target_lang_sense_clarification": "о многих людях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0eefbe5e-37c3-334c-9811-c70c6c0b04df",
                    "sense_definition": "Zdjąć z siebie wiele warstw ubrania lub wiele sztuk odzieży.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "reflexive_undress",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Było tak gorąco, że musiałem się <porozbierać> ze wszystkich swetrów.",
                            "target_lang_translations": {
                                "ru": "Было так жарко, что мне пришлось <снять> с себя все свитера."
                            }
                        },
                        {
                            "text": "Aktorzy <porozbierali> się z ciężkich kostiumów zaraz po spektaklu.",
                            "target_lang_translations": {
                                "ru": "Актеры <сняли> тяжелые костюмы сразу после спектакля."
                            }
                        }
                    ],
                    "antonyms": [
                        "poubierać się"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снять с себя много слоев одежды или несколько предметов гардероба."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раздеться",
                                "target_lang_sense_clarification": "снять много одежды"
                            },
                            {
                                "target_lang_word": "поснимать",
                                "target_lang_sense_clarification": "одежду (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c6f52fe-999c-38c9-87ec-2c96d12e8db2",
                    "sense_definition": "Zdjąć ubranie z wielu osób.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "transitive_undress",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Opiekunki w przedszkolu musiały <porozbierać> dzieci po powrocie ze spaceru.",
                            "target_lang_translations": {
                                "ru": "Воспитательницам в детском саду пришлось <раздеть> детей после возвращения с прогулки."
                            }
                        },
                        {
                            "text": "Pielęgniarki pomogły <porozbierać> pacjentów do badań lekarskich.",
                            "target_lang_translations": {
                                "ru": "Медсестры помогли <раздеть> пациентов для медицинского осмотра."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozebrać"
                    ],
                    "antonyms": [
                        "poubierać"
                    ],
                    "common_phrases": [
                        "porozbierać dzieci",
                        "porozbierać pacjentów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снять одежду с нескольких человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раздеть",
                                "target_lang_sense_clarification": "многих людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78aebb73-91df-3613-91ff-026efc5c5a1f",
                    "sense_definition": "Rozłożyć wiele przedmiotów lub urządzeń na części.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dismantle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mechanicy <porozbierali> silniki, aby znaleźć przyczynę awarii.",
                            "target_lang_translations": {
                                "ru": "Механики <разобрали> двигатели, чтобы найти причину поломки."
                            }
                        },
                        {
                            "text": "Złodzieje ukradli samochody i w ciągu nocy <porozbierali> je na części.",
                            "target_lang_translations": {
                                "ru": "Воры украли машины и за ночь <разобрали> их на запчасти."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozmontować"
                    ],
                    "antonyms": [
                        "poskładać",
                        "złożyć"
                    ],
                    "common_phrases": [
                        "porozbierać na części",
                        "porozbierać samochody"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разобрать на части несколько предметов или механизмов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разобрать",
                                "target_lang_sense_clarification": "много предметов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0aee675-1432-3655-9b39-fcf769a7cc2d",
                    "sense_definition": "Zdemontować lub usunąć wiele konstrukcji budowlanych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dismantle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Władze miasta nakazały <porozbierać> wszystkie nielegalne garaże na osiedlu.",
                            "target_lang_translations": {
                                "ru": "Городские власти приказали <разобрать> все незаконные гаражи в районе."
                            }
                        },
                        {
                            "text": "Robotnicy <porozbierali> stare rusztowania wokół odnowionego budynku.",
                            "target_lang_translations": {
                                "ru": "Рабочие <разобрали> старые строительные леса вокруг обновленного здания."
                            }
                        }
                    ],
                    "synonyms": [
                        "zdemontować"
                    ],
                    "antonyms": [
                        "pobudować",
                        "postawić"
                    ],
                    "common_phrases": [
                        "porozbierać mosty",
                        "porozbierać budynki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Демонтировать или разобрать несколько сооружений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разобрать",
                                "target_lang_sense_clarification": "конструкции"
                            },
                            {
                                "target_lang_word": "снести",
                                "target_lang_sense_clarification": "постройки"
                            }
                        ]
                    }
                }
            ]
        }
    ]

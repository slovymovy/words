{
    "word_family": [
        "northwestern",
        "northwesterly",
        "northwestward"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "433c5c76-021d-35ec-8968-fd013693812b",
                    "sense_definition": "The direction or compass point halfway between west and north.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The storm is approaching quickly from the <w>northwest</w>.",
                            "target_lang_translations": {
                                "ru": "Шторм быстро приближается с <w>северо-запада</w>."
                            }
                        },
                        {
                            "text": "Look to the <w>northwest</w> to see the distant mountains.",
                            "target_lang_translations": {
                                "ru": "Посмотрите на <w>северо-запад</w>, чтобы увидеть далекие горы."
                            }
                        }
                    ],
                    "antonyms": [
                        "southeast"
                    ],
                    "common_phrases": [
                        "in the northwest",
                        "to the northwest",
                        "from the northwest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направление или точка компаса посередине между западом и севером"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "северо-запад",
                                "target_lang_sense_clarification": "направление"
                            },
                            {
                                "target_lang_word": "норд-вест",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3a829c53-7add-321d-a134-2efbc83faaae",
                    "sense_definition": "Situated toward or in the direction of the northwest.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They live in the <w>northwest</w> part of the country.",
                            "target_lang_translations": {
                                "ru": "Они живут в <w>северо-западной</w> части страны."
                            }
                        },
                        {
                            "text": "We followed a <w>northwest</w> course to reach the island.",
                            "target_lang_translations": {
                                "ru": "Мы следовали <w>северо-западным</w> курсом, чтобы добраться до острова."
                            }
                        }
                    ],
                    "antonyms": [
                        "southeast"
                    ],
                    "common_phrases": [
                        "northwest coast",
                        "northwest corner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расположенный по направлению к северо-западу или находящийся на северо-западе"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "северо-западный",
                                "target_lang_sense_clarification": "относящийся к северо-западу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f50d502-83ee-366b-81b4-f2248f60f216",
                    "sense_definition": "Coming from the northwest, usually describing wind.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A cold <w>northwest</w> wind made us shiver outside.",
                            "target_lang_translations": {
                                "ru": "Холодный <w>северо-западный</w> ветер заставил нас дрожать на улице."
                            }
                        },
                        {
                            "text": "The <w>northwest</w> breeze cooled the hot afternoon air.",
                            "target_lang_translations": {
                                "ru": "<w>Северо-западный</w> бриз охладил жаркий дневной воздух."
                            }
                        }
                    ],
                    "antonyms": [
                        "southeast"
                    ],
                    "common_phrases": [
                        "northwest wind",
                        "northwest breeze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дующий с северо-запада (обычно о ветре)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "северо-западный",
                                "target_lang_sense_clarification": "о ветре"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "037d3ab8-22b7-3820-b7e0-1af8481d9b42",
                    "sense_definition": "Towards or in the direction of the northwest.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are travelling <w>northwest</w> to get to the coast.",
                            "target_lang_translations": {
                                "ru": "Мы путешествуем <w>на северо-запад</w>, чтобы добраться до побережья."
                            }
                        },
                        {
                            "text": "The birds flew <w>northwest</w> for the summer season.",
                            "target_lang_translations": {
                                "ru": "Птицы полетели <w>на северо-запад</w> на летний сезон."
                            }
                        }
                    ],
                    "antonyms": [
                        "southeast"
                    ],
                    "common_phrases": [
                        "travel northwest",
                        "move northwest",
                        "look northwest"
                    ],
                    "target_lang_definitions": {
                        "ru": "В направлении северо-запада"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на северо-запад",
                                "target_lang_sense_clarification": "направление движения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
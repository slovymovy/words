    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "22de9aee-61f5-313c-ac25-4014141e3c46",
                    "sense_definition": "Aan de zijkant van iemand of iets; in de directe nabijheid.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zit in de klas altijd <w>naast</w> mijn beste vriend.",
                            "target_lang_translations": {
                                "ru": "В классе я всегда сижу <w>рядом с</w> моим лучшим другом.",
                                "en": "In class, I always sit <w>next to</w> my best friend."
                            }
                        },
                        {
                            "text": "Het postkantoor bevindt zich direct <w>naast</w> de supermarkt.",
                            "target_lang_translations": {
                                "ru": "Почтовое отделение находится прямо <w>возле</w> супермаркета.",
                                "en": "The post office is located right <w>next to</w> the supermarket."
                            }
                        }
                    ],
                    "synonyms": [
                        "nevens"
                    ],
                    "common_phrases": [
                        "naast elkaar",
                        "hiernaast",
                        "ernaast"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сбоку от кого-то или чего-то; в непосредственной близости.",
                        "en": "At the side of someone or something; in immediate proximity."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рядом с",
                                "target_lang_sense_clarification": "предлог места"
                            },
                            {
                                "target_lang_word": "возле",
                                "target_lang_sense_clarification": "около"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "next to",
                                "target_lang_sense_clarification": "preposition: at the side of"
                            },
                            {
                                "target_lang_word": "beside",
                                "target_lang_sense_clarification": "preposition: by the side of"
                            },
                            {
                                "target_lang_word": "alongside",
                                "target_lang_sense_clarification": "preposition: close to the side of"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5959c98b-8fbb-3b6b-b756-568aa9abe4f7",
                    "sense_definition": "Bovenop of in toevoeging aan iets anders.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "addition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Naast</w> zijn studie werkt hij ook nog in een restaurant.",
                            "target_lang_translations": {
                                "ru": "<w>Помимо</w> учебы, он также работает в ресторане.",
                                "en": "<w>Besides</w> his studies, he also works in a restaurant."
                            }
                        },
                        {
                            "text": "Ze spreekt <w>naast</w> Nederlands ook vloeiend Engels en Frans.",
                            "target_lang_translations": {
                                "ru": "<w>Кроме</w> нидерландского, она также свободно говорит по-английски и по-французски.",
                                "en": "<w>In addition to</w> Dutch, she also speaks fluent English and French."
                            }
                        }
                    ],
                    "antonyms": [
                        "behalve"
                    ],
                    "common_phrases": [
                        "naast het feit dat"
                    ],
                    "target_lang_definitions": {
                        "ru": "В дополнение к чему-то другому; сверх того.",
                        "en": "On top of or in addition to something else."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помимо",
                                "target_lang_sense_clarification": "вдобавок"
                            },
                            {
                                "target_lang_word": "кроме",
                                "target_lang_sense_clarification": "исключая или дополняя"
                            },
                            {
                                "target_lang_word": "наряду с",
                                "target_lang_sense_clarification": "вместе с"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "besides",
                                "target_lang_sense_clarification": "preposition: in addition to"
                            },
                            {
                                "target_lang_word": "in addition to",
                                "target_lang_sense_clarification": "phrase: as well as"
                            },
                            {
                                "target_lang_word": "apart from",
                                "target_lang_sense_clarification": "preposition: excluding or adding to"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "1d65e28e-5552-3981-af26-eefa2eb88589",
                    "sense_definition": "Niet op de juiste plek; het doel missend.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "miss",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De spits schoot de bal helaas net <w>naast</w>.",
                            "target_lang_translations": {
                                "ru": "Нападающий, к сожалению, пробил мяч чуть <w>мимо</w>.",
                                "en": "Unfortunately, the striker shot the ball just <w>wide</w>."
                            }
                        },
                        {
                            "text": "Hij gooide het propje papier <w>naast</w> de prullenbak.",
                            "target_lang_translations": {
                                "ru": "Он бросил бумажный комок <w>мимо</w> мусорной корзины.",
                                "en": "He threw the ball of paper <w>next to</w> the trash can."
                            }
                        }
                    ],
                    "synonyms": [
                        "nevens"
                    ],
                    "antonyms": [
                        "raak"
                    ],
                    "common_phrases": [
                        "er naast zitten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не в цель; не на правильном месте.",
                        "en": "Not in the right place; missing the target."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мимо",
                                "target_lang_sense_clarification": "наречие: не попадая в цель"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "wide",
                                "target_lang_sense_clarification": "adverb: missing the target (sports)"
                            },
                            {
                                "target_lang_word": "off target",
                                "target_lang_sense_clarification": "adverb: not hitting the aim"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4b4400ce-d007-3c6c-be88-88d213c45fe3",
                    "sense_definition": "Overtreffende trap van <w>na</w>; het dichtstbij of meest verwant.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "superlative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Alleen zijn <w>naaste</w> familie was uitgenodigd voor de ceremonie.",
                            "target_lang_translations": {
                                "ru": "Только его <w>ближайшие</w> родственники были приглашены на церемонию.",
                                "en": "Only his <w>immediate</w> family was invited to the ceremony."
                            }
                        },
                        {
                            "text": "Zij is mijn <w>naaste</w> collega met wie ik dagelijks samenwerk.",
                            "target_lang_translations": {
                                "ru": "Она моя <w>ближайшая</w> коллега, с которой я работаю каждый день.",
                                "en": "She is my <w>closest</w> colleague with whom I work every day."
                            }
                        }
                    ],
                    "common_phrases": [
                        "naaste familie",
                        "naaste omgeving"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "superlative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся ближе всего или имеющий самое близкое родство.",
                        "en": "Closest or most related."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ближайший",
                                "target_lang_sense_clarification": "превосходная степень"
                            },
                            {
                                "target_lang_word": "близкий",
                                "target_lang_sense_clarification": "о родстве или отношениях"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "closest",
                                "target_lang_sense_clarification": "superlative adjective"
                            },
                            {
                                "target_lang_word": "immediate",
                                "target_lang_sense_clarification": "adjective: regarding family relations"
                            },
                            {
                                "target_lang_word": "nearest",
                                "target_lang_sense_clarification": "superlative adjective: proximity"
                            }
                        ]
                    }
                }
            ]
        },

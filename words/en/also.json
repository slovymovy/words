{
    "word_family": [],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "352fa140-03ae-315e-827f-2b5a8c95e0fd",
                    "sense_definition": "In addition; besides; as well.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "addition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Everyone had eggs for breakfast, but Alice <w>also</w> had toast.",
                            "target_lang_translations": {
                                "ru": "Все ели на завтрак яйца, но Элис <w>тоже</w> съела тост."
                            }
                        },
                        {
                            "text": "She speaks English, and she <w>also</w> knows French.",
                            "target_lang_translations": {
                                "ru": "Она говорит по-английски, а <w>также</w> знает французский."
                            }
                        }
                    ],
                    "synonyms": [
                        "too",
                        "as well"
                    ],
                    "common_phrases": [
                        "not only but also",
                        "see also",
                        "but also"
                    ],
                    "target_lang_definitions": {
                        "ru": "В дополнение; кроме того; также."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тоже",
                                "target_lang_sense_clarification": "указывает на тождественность действий или лиц; равным образом"
                            },
                            {
                                "target_lang_word": "также",
                                "target_lang_sense_clarification": "в равной мере; вместе с тем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a1176a0-d3a3-320f-81d2-83bb63b4b0ab",
                    "sense_definition": "To the same degree or extent; in like manner.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As the father is, <w>also</w> is the son.",
                            "target_lang_translations": {
                                "ru": "Как отец, <w>так</w> и сын."
                            }
                        },
                        {
                            "text": "He was a knight, and <w>also</w> was his companion.",
                            "target_lang_translations": {
                                "ru": "Он был рыцарем, и <w>таким же</w> был его спутник."
                            }
                        }
                    ],
                    "synonyms": [
                        "so"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Modern English typically uses 'so' or 'likewise' in this context."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В той же степени или мере; подобным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "так",
                                "target_lang_sense_clarification": "таким образом (устар.)"
                            },
                            {
                                "target_lang_word": "также",
                                "target_lang_sense_clarification": "аналогично"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "acdcbac7-15a2-3742-a322-68cd83844625",
                    "sense_definition": "And in addition; used to introduce a new point or idea.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "addition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Also</w>, remember to lock the door when you leave.",
                            "target_lang_translations": {
                                "ru": "<w>Кроме того</w>, не забудь запереть дверь, когда будешь уходить."
                            }
                        },
                        {
                            "text": "The room is very small. <w>Also</w>, it is quite cold.",
                            "target_lang_translations": {
                                "ru": "Комната очень маленькая. <w>К тому же</w>, в ней довольно холодно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "also known as"
                    ],
                    "target_lang_definitions": {
                        "ru": "И в дополнение; используется для введения новой мысли или идеи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кроме того",
                                "target_lang_sense_clarification": "вводное слово для добавления информации"
                            },
                            {
                                "target_lang_word": "к тому же",
                                "target_lang_sense_clarification": "в добавление к сказанному"
                            },
                            {
                                "target_lang_word": "ещё",
                                "target_lang_sense_clarification": "разговорное; при добавлении нового пункта"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
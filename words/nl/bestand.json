{
    "word_family": [
        "bestaan",
        "bestendig"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "66b1980c-5ba3-321e-a55b-cd96e1277540",
                    "sense_definition": "Een verzameling digitale gegevens die als één geheel op een computer wordt bewaard.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "digital_data",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun je dat <w>bestand</w> naar mij mailen?",
                            "target_lang_translations": {
                                "en": "Can you email that <w>file</w> to me?",
                                "ru": "Ты можешь отправить мне этот <w>файл</w> по электронной почте?"
                            }
                        },
                        {
                            "text": "Het <w>bestand</w> is te groot om als bijlage te versturen.",
                            "target_lang_translations": {
                                "en": "The <w>file</w> is too large to send as an attachment.",
                                "ru": "Этот <w>файл</w> слишком велик, чтобы отправить его как вложение."
                            }
                        }
                    ],
                    "synonyms": [
                        "computerbestand"
                    ],
                    "common_phrases": [
                        "bestand opslaan",
                        "bestand openen",
                        "bestand verwijderen",
                        "beschadigd bestand"
                    ],
                    "target_lang_definitions": {
                        "en": "A collection of digital data stored as a single unit on a computer.",
                        "ru": "Набор цифровых данных, хранящихся на компьютере как единое целое."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "file",
                                "target_lang_sense_clarification": "digital data"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "файл",
                                "target_lang_sense_clarification": "цифровой документ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc36d823-48f2-34af-b9b0-e23050161199",
                    "sense_definition": "Het totaal aantal aanwezige exemplaren van een bepaalde soort of groep in een gebied.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "population_stock",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door strenge regels is het <w>bestand</w> van de vissen weer toegenomen.",
                            "target_lang_translations": {
                                "en": "Due to strict rules, the fish <w>stock</w> has increased again.",
                                "ru": "Благодаря строгим правилам <w>популяция</w> рыб снова увеличилась."
                            }
                        },
                        {
                            "text": "De boswachter controleert jaarlijks het <w>bestand</w> aan herten in het park.",
                            "target_lang_translations": {
                                "en": "The ranger checks the deer <w>population</w> in the park annually.",
                                "ru": "Лесничий ежегодно проверяет <w>поголовье</w> оленей в парке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het visbestand",
                        "het wildbestand"
                    ],
                    "target_lang_definitions": {
                        "en": "The total number of specimens of a certain species or group present in an area.",
                        "ru": "Общее количество особей определенного вида или группы на определенной территории."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stock",
                                "target_lang_sense_clarification": "of animals or resources"
                            },
                            {
                                "target_lang_word": "population",
                                "target_lang_sense_clarification": "number of inhabitants/animals"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "популяция",
                                "target_lang_sense_clarification": "совокупность особей одного вида"
                            },
                            {
                                "target_lang_word": "поголовье",
                                "target_lang_sense_clarification": "количество животных (обычно скота или дичи)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "182782f4-568a-3ffa-ba9c-7abec59c57e2",
                    "sense_definition": "Een afspraak tussen strijdende partijen om tijdelijk te stoppen met vechten.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "truce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De generaals kwamen bijeen om over een <w>bestand</w> te onderhandelen.",
                            "target_lang_translations": {
                                "en": "The generals met to negotiate a <w>truce</w>.",
                                "ru": "Генералы встретились, чтобы договориться о <w>перемирии</w>."
                            }
                        },
                        {
                            "text": "Tijdens het <w>bestand</w> zwegen de wapens aan beide kanten.",
                            "target_lang_translations": {
                                "en": "During the <w>truce</w>, the weapons fell silent on both sides.",
                                "ru": "Во время <w>перемирия</w> оружие молчало с обеих сторон."
                            }
                        }
                    ],
                    "synonyms": [
                        "computerbestand"
                    ],
                    "common_phrases": [
                        "een bestand sluiten",
                        "Twaalfjarig Bestand"
                    ],
                    "target_lang_definitions": {
                        "en": "An agreement between warring parties to temporarily stop fighting.",
                        "ru": "Соглашение между враждующими сторонами о временном прекращении боевых действий."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "truce",
                                "target_lang_sense_clarification": "cessation of fighting"
                            },
                            {
                                "target_lang_word": "ceasefire",
                                "target_lang_sense_clarification": "military agreement"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "перемирие",
                                "target_lang_sense_clarification": "временное прекращение огня"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "91b303b6-605e-3244-b805-ae6808ed103e",
                    "sense_definition": "Sterk genoeg om iets te verdragen zonder beschadigd te raken; weerstand kunnende bieden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "resistant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze jas is goed <w>bestand</w> tegen zware regen en wind.",
                            "target_lang_translations": {
                                "en": "This jacket is well <w>resistant</w> to heavy rain and wind.",
                                "ru": "Эта куртка <w>устойчива</w> к сильному дождю и ветру."
                            }
                        },
                        {
                            "text": "Hij bleek niet <w>bestand</w> te zijn tegen de grote druk van zijn nieuwe baan.",
                            "target_lang_translations": {
                                "en": "He turned out not to be <w>resistant</w> to the high pressure of his new job.",
                                "ru": "Оказалось, что он не <w>устойчив</w> к сильному давлению на новой работе."
                            }
                        }
                    ],
                    "synonyms": [
                        "computerbestand"
                    ],
                    "common_phrases": [
                        "bestand zijn tegen",
                        "niet bestand tegen",
                        "bestand tegen water",
                        "bestand tegen kritiek"
                    ],
                    "target_lang_definitions": {
                        "en": "Strong enough to endure something without being damaged; capable of offering resistance.",
                        "ru": "Способный выдерживать что-либо без повреждений; оказывающий сопротивление."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "resistant",
                                "target_lang_sense_clarification": "able to withstand"
                            },
                            {
                                "target_lang_word": "proof",
                                "target_lang_sense_clarification": "impervious to"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "устойчивый",
                                "target_lang_sense_clarification": "способный сопротивляться воздействию"
                            },
                            {
                                "target_lang_word": "стойкий",
                                "target_lang_sense_clarification": "не поддающийся влиянию"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
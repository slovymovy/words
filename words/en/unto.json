        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "a7d8f034-e4e0-3ed5-954e-209cd7c19940",
                    "sense_definition": "To; indicating motion or direction towards a specific place, person, or thing.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The brave knight rode <w>unto</w> the nearby castle to seek shelter.",
                            "target_lang_translations": {
                                "ru": "Храбрый рыцарь поскакал <w>к</w> ближайшему замку, чтобы найти укрытие."
                            }
                        },
                        {
                            "text": "Come <w>unto</w> these yellow sands and take hands.",
                            "target_lang_translations": {
                                "ru": "Придите <w>на</w> эти желтые пески и возьмитесь за руки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "come unto",
                        "near unto",
                        "bring unto"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "used chiefly in literary, poetic, or biblical contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "К; указывает на движение или направление к определенному месту, человеку или предмету."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "предлог направления"
                            },
                            {
                                "target_lang_word": "до",
                                "target_lang_sense_clarification": "указывает на предел движения"
                            },
                            {
                                "target_lang_word": "на",
                                "target_lang_sense_clarification": "указывает на место назначения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e5f9c3b-47a7-39ed-8012-ac7f213ef951",
                    "sense_definition": "To; used to indicate the person or thing receiving an action, speech, or object.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king said <w>unto</w> his servants that they should prepare a great feast.",
                            "target_lang_translations": {
                                "ru": "Король обратился <w>к</w> своим слугам, сказав, что они должны приготовить большой пир."
                            }
                        },
                        {
                            "text": "Render <w>unto</w> Caesar the things that are Caesar's.",
                            "target_lang_translations": {
                                "ru": "Отдайте <w>кесарю</w> кесарево."
                            }
                        }
                    ],
                    "common_phrases": [
                        "said unto",
                        "spoke unto",
                        "given unto",
                        "render unto"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "common in religious texts like the Bible"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "К; используется для указания лица или предмета, на которое направлено действие, речь или объект."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "указание на адресата"
                            },
                            {
                                "target_lang_word": "ко",
                                "target_lang_sense_clarification": "вариант предлога 'к' (устар./книжн.)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "41f5559c-06c4-3ed3-b07f-c5c12a7c3935",
                    "sense_definition": "Up to the time that; until.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They pledged to defend the city <w>unto</w> the war was finally won.",
                            "target_lang_translations": {
                                "ru": "Они поклялись защищать город, <w>пока</w> война не будет окончательно выиграна."
                            }
                        },
                        {
                            "text": "He waited by the river <w>unto</w> the sun went down.",
                            "target_lang_translations": {
                                "ru": "Он ждал у реки, <w>пока</w> солнце не село."
                            }
                        }
                    ],
                    "synonyms": [

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7fead0a4-3f02-3f7e-b790-5d9568f900ec",
                    "sense_definition": "Serving as a symbol to represent an idea, quality, or situation.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "representative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The handshake was a <w>symbolic</w> gesture of peace between the two leaders.",
                            "target_lang_translations": {
                                "ru": "Рукопожатие было <w>символическим</w> жестом мира между двумя лидерами."
                            }
                        },
                        {
                            "text": "The destruction of the wall was <w>symbolic</w> of the end of the war.",
                            "target_lang_translations": {
                                "ru": "Разрушение стены было <w>символичным</w> для окончания войны."
                            }
                        }
                    ],
                    "synonyms": [
                        "emblematic",
                        "representative"
                    ],
                    "antonyms": [
                        "literal",
                        "actual"
                    ],
                    "common_phrases": [
                        "symbolic gesture",
                        "symbolic meaning",
                        "symbolic value",
                        "symbolic significance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служащий символом для представления идеи, качества или ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "символический",
                                "target_lang_sense_clarification": "служащий условным знаком чего-либо"
                            },
                            {
                                "target_lang_word": "символичный",
                                "target_lang_sense_clarification": "полный значения, скрытого смысла"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "722b2d79-e9e9-34c6-a0a7-fd9e681b5e42",
                    "sense_definition": "Relating to or involving the use of symbols.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "related_to_symbols",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The poet uses <w>symbolic</w> language to describe the beauty of nature.",
                            "target_lang_translations": {
                                "ru": "Поэт использует <w>символический</w> язык для описания красоты природы."
                            }
                        },
                        {
                            "text": "Mathematics relies heavily on <w>symbolic</w> notation to express complex theories.",
                            "target_lang_translations": {
                                "ru": "Математика сильно полагается на <w>символические</w> обозначения для выражения сложных теорий."
                            }
                        }
                    ],
                    "synonyms": [
                        "figurative"
                    ],
                    "common_phrases": [
                        "symbolic logic",
                        "symbolic representation",
                        "symbolic system"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к использованию символов или связанный с ними."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "символический",
                                "target_lang_sense_clarification": "использующий символы или условные обозначения"
                            }
                        ]
                    }

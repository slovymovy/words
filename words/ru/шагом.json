    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "21ad1249-b4f8-3cea-8d5b-a9268e39466a",
                    "sense_definition": "Способом передвижения, при котором ноги переступают, не переходя на бег; ходьбой.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement_mode",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили идти <шагом>, чтобы лучше рассмотреть город.",
                            "target_lang_translations": {
                                "en": "We decided to go <at a walking pace> to get a better look at the city."
                            }
                        },
                        {
                            "text": "Он вышел из комнаты быстрым <шагом>, ни на кого не глядя.",
                            "target_lang_translations": {
                                "en": "He left the room <at a quick pace>, without looking at anyone."
                            }
                        }
                    ],
                    "synonyms": [
                        "пешком"
                    ],
                    "antonyms": [
                        "бегом"
                    ],
                    "common_phrases": [
                        "быстрым шагом",
                        "шагом марш"
                    ],
                    "target_lang_definitions": {
                        "en": "By walking; at a walking pace; not running."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "at a walk",
                                "target_lang_sense_clarification": "movement mode"
                            },
                            {
                                "target_lang_word": "at a ... pace",
                                "target_lang_sense_clarification": "speed of movement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38aea326-ca12-3eb5-a88c-ce7bafe7c611",
                    "sense_definition": "Самым медленным аллюром (о движении лошади).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "movement_mode",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Уставшая лошадь пошла <шагом>, и всадник не стал её торопить.",
                            "target_lang_translations": {
                                "en": "The tired horse started moving <at a walk>, and the rider did not urge it on."
                            }
                        },
                        {
                            "text": "Кавалеристы двигались <шагом> вдоль лесной опушки.",
                            "target_lang_translations": {
                                "en": "The cavalrymen moved <at a walk> along the edge of the forest."
                            }
                        }
                    ],
                    "antonyms": [
                        "рысью",
                        "галопом"
                    ],
                    "common_phrases": [
                        "пустить шагом"
                    ],
                    "target_lang_definitions": {
                        "en": "At the slowest gait (regarding horse movement)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "at a walk",
                                "target_lang_sense_clarification": "horse gait"
                            }
                        ]
                    }
                }
            ]
        },

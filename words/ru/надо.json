{
    "word_family": [
        "надобность",
        "надобный"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "099bd39c-494b-3d45-88cd-2b1f2c32cd03",
                    "sense_definition": "Нужно, необходимо; выражает потребность, необходимость или долженствование совершить какое-либо действие.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "necessity_obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне срочно <надо> позвонить домой.",
                            "target_lang_translations": {
                                "en": "I urgently <need> to call home."
                            }
                        },
                        {
                            "text": "В библиотеке <надо> соблюдать полную тишину.",
                            "target_lang_translations": {
                                "en": "In the library, one <must> observe complete silence."
                            }
                        },
                        {
                            "text": "Так ему и <надо>!",
                            "target_lang_translations": {
                                "en": "Serves him <right>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "нужно",
                        "необходимо",
                        "тре́буется",
                        "следует",
                        "стоит",
                        "надобно",
                        "надлежи́т"
                    ],
                    "common_phrases": [
                        "так и надо",
                        "надо бы",
                        "как надо",
                        "что надо"
                    ],
                    "target_lang_definitions": {
                        "en": "It is necessary or needed; expresses a need, necessity, or obligation to perform an action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "need to",
                                "target_lang_sense_clarification": "general necessity or personal need"
                            },
                            {
                                "target_lang_word": "have to",
                                "target_lang_sense_clarification": "obligation or external necessity"
                            },
                            {
                                "target_lang_word": "must",
                                "target_lang_sense_clarification": "strong obligation or requirement"
                            },
                            {
                                "target_lang_word": "necessary",
                                "target_lang_sense_clarification": "formal indication that something is required"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "7ae3bf66-c6a6-3934-b5b7-d777fdfa2c51",
                    "sense_definition": "Вариант предлога «над», употребляемый для благозвучия перед словами, начинающимися с определённых сочетаний согласных (обычно «мн», «лб», «рв»).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Голубое небо сияло <надо> мной.",
                            "target_lang_translations": {
                                "en": "The blue sky shone <above> me."
                            }
                        },
                        {
                            "text": "Ветки деревьев склонились низко <надо> лбом.",
                            "target_lang_translations": {
                                "en": "The tree branches bent low <over> the forehead."
                            }
                        }
                    ],
                    "synonyms": [
                        "выше",
                        "поверх"
                    ],
                    "antonyms": [
                        "под",
                        "подо"
                    ],
                    "common_phrases": [
                        "надо мной"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "фонетический вариант предлога «над»"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A variant of the preposition 'over' or 'above', used for easier pronunciation before words beginning with certain consonant clusters."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "over",
                                "target_lang_sense_clarification": "spatial position directly above something"
                            },
                            {
                                "target_lang_word": "above",
                                "target_lang_sense_clarification": "at a higher level than something"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
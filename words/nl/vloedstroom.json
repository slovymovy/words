    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ebac8636-cd24-3fea-82cf-19e1cc630875",
                    "sense_definition": "De stroming van het zeewater richting de kust tijdens opkomend tij.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tide_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de sterke <w>vloedstroom</w> konden we sneller de haven binnenvaren.",
                            "target_lang_translations": {
                                "en": "Due to the strong <w>flood current</w>, we were able to enter the harbor faster.",
                                "ru": "Благодаря сильному <w>приливному течению</w> мы смогли быстрее зайти в гавань."
                            }
                        },
                        {
                            "text": "De zwemmers werden gewaarschuwd voor de gevaarlijke <w>vloedstroom</w> langs de kust.",
                            "target_lang_translations": {
                                "en": "The swimmers were warned about the dangerous <w>flood current</w> along the coast.",
                                "ru": "Пловцов предупредили об опасном <w>приливном течении</w> вдоль побережья."
                            }
                        }
                    ],
                    "synonyms": [
                        "getijdestroom"
                    ],
                    "antonyms": [
                        "ebstroom"
                    ],
                    "common_phrases": [
                        "sterke vloedstroom",
                        "opkomende vloedstroom"
                    ],
                    "target_lang_definitions": {
                        "en": "The flow of seawater towards the coast during rising tide.",
                        "ru": "Течение морской воды к берегу во время прилива."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flood current",
                                "target_lang_sense_clarification": "horizontal movement of water"
                            },
                            {
                                "target_lang_word": "flood stream",
                                "target_lang_sense_clarification": "nautical term"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "приливное течение",
                                "target_lang_sense_clarification": "движение воды"
                            },
                            {
                                "target_lang_word": "прилив",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57168642-3d35-3a91-8260-f26abf4c4d45",
                    "sense_definition": "Een krachtige en vaak plotselinge stroom van water, meestal veroorzaakt door noodweer.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "water_torrent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De auto werd door een hevige <w>vloedstroom</w> van de weg gespoeld.",
                            "target_lang_translations": {
                                "en": "The car was washed off the road by a violent <w>torrent</w>.",
                                "ru": "Автомобиль был смыт с дороги мощным <w>потоком</w> воды."
                            }
                        },
                        {
                            "text": "Na de zware regenval ontstond er een verwoestende <w>vloedstroom</w> in de vallei.",
                            "target_lang_translations": {
                                "en": "After the heavy rainfall, a devastating <w>torrent</w> arose in the valley.",
                                "ru": "После сильных дождей в долине образовался разрушительный <w>поток</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stortvloed"
                    ],
                    "common_phrases": [
                        "hevige vloedstroom",
                        "verwoestende vloedstroom"
                    ],
                    "target_lang_definitions": {
                        "en": "A powerful and often sudden stream of water, usually caused by severe weather.",
                        "ru": "Мощный и часто внезапный поток воды, обычно вызванный непогодой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "torrent",
                                "target_lang_sense_clarification": "violent stream of water"
                            },
                            {
                                "target_lang_word": "flood",
                                "target_lang_sense_clarification": "overflow of water"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "бурный поток",
                                "target_lang_sense_clarification": "сильное течение"
                            },
                            {
                                "target_lang_word": "поток",
                                "target_lang_sense_clarification": "движущаяся масса воды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1a75f88-248d-3c67-9178-f5107eb6d1e4",
                    "sense_definition": "Een grote, onafgebroken hoeveelheid mensen of dingen die ergens op afkomt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf kreeg een <w>vloedstroom</w> aan klachten na de storing.",
                            "target_lang_translations": {
                                "en": "The company received a <w>flood</w> of complaints after the outage.",
                                "ru": "После сбоя компания получила <w>поток</w> жалоб."
                            }
                        },
                        {
                            "text": "De oorlog veroorzaakte een <w>vloedstroom</w> van vluchtelingen naar de buurlanden.",
                            "target_lang_translations": {
                                "en": "The war caused a <w>stream</w> of refugees to the neighboring countries.",
                                "ru": "Война вызвала <w>поток</w> беженцев в соседние страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "stortvloed"
                    ],
                    "common_phrases": [
                        "vloedstroom aan informatie",
                        "vloedstroom van mensen"
                    ],
                    "target_lang_definitions": {
                        "en": "A large, continuous quantity of people or things coming towards something.",
                        "ru": "Большое, непрерывное количество людей или вещей, направляющееся куда-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flood",
                                "target_lang_sense_clarification": "figurative: overwhelming amount"
                            },
                            {
                                "target_lang_word": "stream",
                                "target_lang_sense_clarification": "continuous flow"
                            },
                            {
                                "target_lang_word": "deluge",
                                "target_lang_sense_clarification": "figurative: heavy influx"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "поток",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "наплыв",
                                "target_lang_sense_clarification": "большое количество прибывающих"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c76ee182-000f-3239-a737-0ae814fea32b",
                    "sense_definition": "Лишённый логики, противоречащий законам логики или здравому смыслу.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот вывод кажется мне совершенно <w>нелогичным</w> и необоснованным.",
                            "target_lang_translations": {
                                "en": "This conclusion seems completely <w>illogical</w> and unfounded to me."
                            }
                        },
                        {
                            "text": "Мы не можем принять такое <w>нелогичное</w> решение в данной ситуации.",
                            "target_lang_translations": {
                                "en": "We cannot make such an <w>illogical</w> decision in this situation."
                            }
                        }
                    ],
                    "synonyms": [
                        "алогичный",
                        "иллогичный",
                        "абсурдный"
                    ],
                    "antonyms": [
                        "логичный"
                    ],
                    "common_phrases": [
                        "нелогичный вывод",
                        "нелогичный шаг",
                        "абсолютно нелогичный"
                    ],
                    "target_lang_definitions": {
                        "en": "Lacking logic, contradicting the laws of logic or common sense."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "illogical",
                                "target_lang_sense_clarification": "contrary to logic"
                            },
                            {
                                "target_lang_word": "irrational",
                                "target_lang_sense_clarification": "lacking reason or sound judgment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ec6ee46-628c-36f5-8f01-5f6ae7762c33",
                    "sense_definition": "Непоследовательный в своих действиях, мнениях или поведении.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он очень <w>нелогичный</w> человек: говорит одно, а делает совсем другое.",
                            "target_lang_translations": {
                                "en": "He is a very <w>inconsistent</w> person: he says one thing but does something completely different."
                            }
                        },
                        {
                            "text": "Её <w>нелогичное</w> поведение удивило всех присутствующих на встрече.",
                            "target_lang_translations": {
                                "en": "Her <w>illogical</w> behavior surprised everyone present at the meeting."
                            }
                        }
                    ],
                    "synonyms": [
                        "непоследовательный"
                    ],
                    "antonyms": [
                        "логичный"
                    ],
                    "common_phrases": [
                        "нелогичный поступок",
                        "нелогичное поведение"
                    ],
                    "target_lang_definitions": {
                        "en": "Inconsistent in one's actions, opinions, or behavior."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "inconsistent",
                                "target_lang_sense_clarification": "lacking consistency in behavior"
                            },
                            {
                                "target_lang_word": "illogical",
                                "target_lang_sense_clarification": "lacking logic in behavior"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "distance_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is only a short <w>walk</w> from the station to the hotel.",
                            "target_lang_translations": {
                                "ru": "От вокзала до отеля всего несколько минут <w>ходьбы</w>."
                            }
                        },
                        {
                            "text": "The grocery store is about ten minutes <w>walk</w> from here.",
                            "target_lang_translations": {
                                "ru": "Продуктовый магазин находится примерно в десяти минутах <w>ходьбы</w> отсюда."
                            }
                        }
                    ],
                    "common_phrases": [
                        "short walk",
                        "minutes walk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расстояние, которое нужно пройти пешком, часто измеряемое во времени."
                    },
                    "translations": {

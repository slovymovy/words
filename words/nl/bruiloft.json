            "senses": [
                {
                    "sense_id": "a7a03248-1e2d-3895-9d63-f7994397af09",
                    "sense_definition": "Een feestelijke gelegenheid waarbij twee mensen trouwen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We zijn uitgenodigd voor de <w>bruiloft</w> van mijn buren.",
                            "target_lang_translations": {
                                "en": "We are invited to my neighbors' <w>wedding</w>.",
                                "ru": "Мы приглашены на <w>свадьбу</w> моих соседей."
                            }
                        },
                        {
                            "text": "Ze willen hun <w>bruiloft</w> vieren in een groot kasteel.",
                            "target_lang_translations": {
                                "en": "They want to celebrate their <w>wedding</w> in a large castle.",
                                "ru": "Они хотят отпраздновать свою <w>свадьбу</w> в большом замке."
                            }
                        }
                    ],
                    "synonyms": [
                        "trouwfeest",
                        "huwelijksfeest",
                        "trouwpartij"
                    ],
                    "common_phrases": [
                        "op de bruiloft",
                        "bruiloft vieren"
                    ],
                    "target_lang_definitions": {
                        "en": "A festive occasion where two people get married.",
                        "ru": "Праздничное мероприятие, на котором двое людей вступают в брак."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wedding",
                                "target_lang_sense_clarification": "general term for the ceremony and party"
                            },
                            {
                                "target_lang_word": "wedding party",
                                "target_lang_sense_clarification": "focus on the celebration aspect"
                            },
                            {
                                "target_lang_word": "wedding reception",
                                "target_lang_sense_clarification": "the party after the ceremony"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "свадьба",
                                "target_lang_sense_clarification": "церемония и праздник"
                            },
                            {
                                "target_lang_word": "бракосочетание",
                                "target_lang_sense_clarification": "официальная церемония"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55bcac4c-e461-357e-83d7-33c6f13a2a2c",
                    "sense_definition": "Een viering van de dag dat mensen een bepaald aantal jaren getrouwd zijn.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Volgende week vieren mijn ouders hun zilveren <w>bruiloft</w> met de hele familie.",
                            "target_lang_translations": {
                                "en": "Next week my parents are celebrating their silver <w>wedding</w> with the whole family.",
                                "ru": "На следующей неделе мои родители празднуют свою серебряную <w>свадьбу</w> со всей семьей."
                            }
                        },
                        {
                            "text": "Bij een koperen <w>bruiloft</w> ben je twaalf en een half jaar getrouwd.",
                            "target_lang_translations": {
                                "en": "At a copper <w>wedding anniversary</w>, you have been married for twelve and a half years.",
                                "ru": "Медная <w>свадьба</w> означает, что вы женаты двенадцать с половиной лет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gouden bruiloft",
                        "zilveren bruiloft",
                        "koperen bruiloft"
                    ],
                    "target_lang_definitions": {
                        "en": "A celebration of the day people have been married for a certain number of years.",
                        "ru": "Празднование годовщины бракосочетания (определенного количества лет в браке)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wedding anniversary",
                                "target_lang_sense_clarification": "annual celebration of the date of marriage"
                            },
                            {
                                "target_lang_word": "wedding",
                                "target_lang_sense_clarification": "used in phrases like 'silver wedding'"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "свадьба",
                                "target_lang_sense_clarification": "в названиях юбилеев (золотая, серебряная)"
                            },
                            {
                                "target_lang_word": "годовщина свадьбы",
                                "target_lang_sense_clarification": "ежегодный праздник"

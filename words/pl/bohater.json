            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1ce450fa-d20d-3434-b166-2f6a0df4a70b",
                    "sense_definition": "Postać w utworze literackim, filmowym lub teatralnym, biorąca udział w wydarzeniach.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Główny <w>bohater</w> tej powieści przeżywa wiele niesamowitych przygód w dżungli.",
                            "target_lang_translations": {
                                "ru": "Главный <w>герой</w> этого романа переживает множество невероятных приключений в джунглях."
                            }
                        },
                        {
                            "text": "Bardzo polubiłam tytułowego <w>bohatera</w> filmu, mimo że miał trudny charakter.",
                            "target_lang_translations": {
                                "ru": "Мне очень понравился заглавный <w>герой</w> фильма, несмотря на его непростой характер."
                            }
                        }
                    ],
                    "synonyms": [
                        "postać",
                        "protagonista"
                    ],
                    "antonyms": [
                        "antybohater"
                    ],
                    "common_phrases": [
                        "główny bohater",
                        "tytułowy bohater",
                        "bohater literacki",
                        "losy bohatera"
                    ],
                    "target_lang_definitions": {
                        "ru": "Персонаж литературного произведения, фильма или пьесы, участвующий в событиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "герой",
                                "target_lang_sense_clarification": "персонаж произведения"
                            },
                            {
                                "target_lang_word": "персонаж",
                                "target_lang_sense_clarification": "действующее лицо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6602954b-935c-34d5-8f6f-fdf8c1d21435",
                    "sense_definition": "Osoba, która odznaczyła się niezwykłą odwagą, męstwem i poświęceniem dla innych.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierz, który uratował swoich kolegów z pola walki, został uznany za <w>bohatera</w>.",
                            "target_lang_translations": {
                                "ru": "Солдат, спасший своих товарищей с поля боя, был признан <w>героем</w>."
                            }
                        },
                        {
                            "text": "Mój dziadek jest dla mnie prawdziwym <w>bohaterem</w>, ponieważ przeżył wojnę.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка для меня — настоящий <w>герой</w>, потому что он пережил войну."
                            }
                        }
                    ],
                    "synonyms": [
                        "heros"
                    ],
                    "common_phrases": [
                        "bohater narodowy",
                        "bohater wojenny",
                        "zginąć jak bohater"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, отличившийся необычайной отвагой, мужеством и самопожертвованием ради других."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "герой",
                                "target_lang_sense_clarification": "храбрый человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14e76e25-cd72-36a3-a187-4020fd2b22ec",
                    "sense_definition": "Osoba, która w danym momencie skupia na sobie uwagę otoczenia lub mediów.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po strzeleniu zwycięskiej bramki piłkarz stał się niekwestionowanym <w>bohaterem</w> dnia.",
                            "target_lang_translations": {
                                "ru": "Забив победный гол, футболист стал бесспорным <w>героем</w> дня."
                            }
                        },
                        {
                            "text": "Polityk stał się negatywnym <w>bohaterem</w> artykułów prasowych z powodu afery korupcyjnej.",
                            "target_lang_translations": {
                                "ru": "Политик стал отрицательным <w>героем</w> газетных статей из-за коррупционного скандала."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bohater dnia",
                        "bohater skandalu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который в данный момент привлекает к себе внимание окружающих или СМИ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "герой",
                                "target_lang_sense_clarification": "центр внимания"
                            }
                        ]
                    }
                }
            ]
        }
    ]

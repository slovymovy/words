    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "13423a67-30b2-3889-917e-79ba0c2f9ad9",
                    "sense_definition": "Pozioma powierzchnia konstrukcyjna dzieląca wnętrze statku, samolotu lub innego pojazdu na kondygnacje, po której można chodzić.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "transport_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kapitan zaprosił wszystkich pasażerów na górny <w>pokład</w> statku, aby mogli podziwiać widoki.",
                            "target_lang_translations": {
                                "ru": "Капитан пригласил всех пассажиров на верхнюю <w>палубу</w> корабля, чтобы они могли полюбоваться видами."
                            }
                        },
                        {
                            "text": "Witamy na <w>pokładzie</w> naszego samolotu i życzymy państwu przyjemnego lotu.",
                            "target_lang_translations": {
                                "ru": "Добро пожаловать на <w>борт</w> нашего самолета, желаем вам приятного полета."
                            }
                        }
                    ],
                    "synonyms": [
                        "dek"
                    ],
                    "common_phrases": [
                        "na pokładzie",
                        "wejść na pokład",
                        "pokład statku",
                        "górny pokład"
                    ],
                    "target_lang_definitions": {
                        "ru": "Горизонтальное перекрытие, разделяющее внутреннее пространство судна, самолета или другого транспортного средства на ярусы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "палуба",
                                "target_lang_sense_clarification": "на корабле"
                            },
                            {
                                "target_lang_word": "борт",
                                "target_lang_sense_clarification": "на самолете или судне (в контексте нахождения внутри)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cdc49af-9fd1-3970-b1fd-d5aad8b36243",
                    "sense_definition": "Warstwa surowca mineralnego lub skały znajdująca się w ziemi między innymi warstwami skalnymi.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "geology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Górnicy odkryli nowy, bogaty <w>pokład</w> węgla kamiennego głęboko pod ziemią.",
                            "target_lang_translations": {
                                "ru": "Шахтеры обнаружили новый богатый <w>пласт</w> каменного угля глубоко под землей."
                            }
                        },
                        {
                            "text": "Eksploatacja tego <w>pokładu</w> miedzi stała się nieopłacalna ze względów ekonomicznych.",
                            "target_lang_translations": {
                                "ru": "Разработка этого <w>пласта</w> меди стала нерентабельной по экономическим причинам."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwłoki"
                    ],
                    "common_phrases": [
                        "pokład węgla",
                        "eksploatacja pokładu",
                        "miąższość pokładu"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical/mining"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слой полезного ископаемого или горной породы, залегающий в земле между другими слоями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пласт",
                                "target_lang_sense_clarification": "геологический слой"
                            },
                            {
                                "target_lang_word": "залежь",
                                "target_lang_sense_clarification": "месторождение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a765a2fd-7db9-3abc-8cde-5a2d7d92bcc1",
                    "sense_definition": "Duże, często ukryte zasoby czegoś, zazwyczaj uczuć, cech charakteru lub energii.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_resources",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten młody sportowiec ma w sobie niesamowite <w>pokłady</w> energii, które wykorzystuje podczas zawodów.",
                            "target_lang_translations": {
                                "ru": "У этого молодого спортсмена есть невероятные <w>запасы</w> энергии, которые он использует во время соревнований."
                            }
                        },
                        {
                            "text": "Nauczycielka musiała uruchomić ogromne <w>pokłady</w> cierpliwości, pracując z tą trudną grupą.",
                            "target_lang_translations": {
                                "ru": "Учительнице пришлось задействовать огромные <w>запасы</w> терпения, работая с этой трудной группой."
                            }
                        }
                    ],
                    "synonyms": [
                        "skarbiec"
                    ],
                    "common_phrases": [
                        "pokłady energii",
                        "pokłady cierpliwości",
                        "ukryte pokłady",
                        "niewyczerpane pokłady"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большие, часто скрытые ресурсы чего-либо, обычно чувств, черт характера или энергии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запас",
                                "target_lang_sense_clarification": "ресурс, резерв"
                            },
                            {
                                "target_lang_word": "залежи",
                                "target_lang_sense_clarification": "переносное значение: обилие чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2229403d-ecde-3828-b7ba-463b2de72361",
                    "sense_definition": "Płytka orka wykonywana na polu latem, zaraz po zbiorach zbóż.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnik wyjechał ciągnikiem w pole, aby zrobić <w>pokład</w> natychmiast po żniwach.",
                            "target_lang_translations": {
                                "ru": "Фермер выехал на тракторе в поле, чтобы сделать <w>лущение</w> сразу после жатвы."
                            }
                        },
                        {
                            "text": "Prawidłowo wykonany <w>pokład</w> zapobiega nadmiernemu wysychaniu gleby w okresie letnim.",
                            "target_lang_translations": {
                                "ru": "Правильно выполненное <w>лущение</w> предотвращает чрезмерное высыхание почвы в летний период."
                            }
                        }
                    ],
                    "synonyms": [
                        "podorywka"
                    ],
                    "common_phrases": [
                        "wykonać pokład",
                        "robić pokład"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "agriculture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неглубокая вспашка поля летом, выполняемая сразу после уборки урожая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лущение",
                                "target_lang_sense_clarification": "сельскохозяйственная обработка почвы"
                            },
                            {
                                "target_lang_word": "взмёт",
                                "target_lang_sense_clarification": "вспашка стерни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3f3a6f6-c033-3cb8-8223-b28e6bce633f",
                    "sense_definition": "Dawne określenie na ciało zmarłego człowieka.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "obsolete_meanings",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starej kronice opisano, jak uroczyście złożono <w>pokład</w> rycerza do rodzinnego grobowca.",
                            "target_lang_translations": {
                                "ru": "В старой хронике описано, как торжественно положили <w>тело</w> рыцаря в семейную гробницу."
                            }
                        },
                        {
                            "text": "Żałobnicy w milczeniu patrzyli na <w>pokład</w> króla spoczywający na wysokich marach.",
                            "target_lang_translations": {
                                "ru": "Скорбящие молча смотрели на <w>тело</w> короля, покоящееся на высоком катафалке."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwłoki"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее название тела умершего человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тело",
                                "target_lang_sense_clarification": "останки"
                            },
                            {
                                "target_lang_word": "труп",
                                "target_lang_sense_clarification": "мертвое тело"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93d7163d-28ba-33d0-84d8-96b136c0ac1a",
                    "sense_definition": "Górna powierzchnia pomieszczenia zamykająca je od góry.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "construction_regional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej starej chacie <w>pokład</w> był wykonany z grubych, drewnianych belek.",
                            "target_lang_translations": {
                                "ru": "В этой старой хате <w>потолок</w> был сделан из толстых деревянных балок."
                            }
                        },
                        {
                            "text": "Niski <w>pokład</w> w piwnicy sprawiał, że musieliśmy się ciągle schylać.",
                            "target_lang_translations": {
                                "ru": "Низкий <w>потолок</w> в подвале заставлял нас постоянно нагибаться."
                            }
                        }
                    ],
                    "synonyms": [
                        "sufit",
                        "pułap"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "dialectal or archaic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхнее перекрытие помещения; потолок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потолок",
                                "target_lang_sense_clarification": "верхнее перекрытие комнаты"
                            }
                        ]
                    }
                }
            ]
        }
    ]

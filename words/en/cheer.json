    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "89b56943-e12e-3bbc-bdd2-ddecd9733ea7",
                    "sense_definition": "To shout loudly to show support, encouragement, approval, or happiness.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_vocal_support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The fans <w>cheered</w> wildly when their team scored the winning goal.",
                            "target_lang_translations": {
                                "ru": "Болельщики неистово <w>ликовали</w>, когда их команда забила победный гол."
                            }
                        },
                        {
                            "text": "We went to the stadium to <w>cheer</w> for our favorite athletes.",
                            "target_lang_translations": {
                                "ru": "Мы пошли на стадион <w>болеть</w> за наших любимых спортсменов."
                            }
                        }
                    ],
                    "synonyms": [
                        "applaud",
                        "shout",
                        "root",
                        "acclaim"
                    ],
                    "antonyms": [
                        "boo"
                    ],
                    "common_phrases": [
                        "cheer for",
                        "cheer on"
                    ],
                    "target_lang_definitions": {
                        "ru": "Громко кричать, выражая поддержку, одобрение или радость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "болеть",
                                "target_lang_sense_clarification": "поддерживать команду или спортсмена"
                            },
                            {
                                "target_lang_word": "приветствовать",
                                "target_lang_sense_clarification": "встречать возгласами одобрения"
                            },
                            {
                                "target_lang_word": "ликовать",
                                "target_lang_sense_clarification": "бурно выражать радость"
                            },
                            {
                                "target_lang_word": "подбадривать",
                                "target_lang_sense_clarification": "выкрикивать слова поддержки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2d999df-d861-3edd-b2e8-575d0cd591ba",
                    "sense_definition": "To make someone feel happier, more hopeful, or less sad; often used with 'up'.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I bought her some flowers to <w>cheer</w> her up after a bad day.",
                            "target_lang_translations": {
                                "ru": "Я купил ей цветы, чтобы <w>подбодрить</w> её после плохого дня."
                            }
                        },
                        {
                            "text": "The bright sunshine helped to <w>cheer</w> us after the long winter.",
                            "target_lang_translations": {
                                "ru": "Яркое солнце помогло <w>взбодрить</w> нас после долгой зимы."
                            }
                        }
                    ],
                    "synonyms": [
                        "gladden",
                        "comfort",
                        "encourage",
                        "brighten"
                    ],
                    "antonyms": [
                        "boo",
                        "boo"
                    ],
                    "common_phrases": [
                        "cheer up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поднимать настроение, вселять надежду или утешать; часто используется с 'up'."
                    },
                    "translations": {
                        "ru": [
                            {

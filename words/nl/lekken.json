                {
                    "sense_id": "eeda2e6a-090d-389d-b08a-5b1f3e978475",
                    "sense_definition": "Vloeistof of gas onbedoeld laten ontsnappen of doorlaten door een opening.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_leak",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude kraan in de keuken <w>lekt</w> al de hele dag water.",
                            "target_lang_translations": {
                                "ru": "Старый кран на кухне весь день <w>протекает</w>."
                            }
                        },
                        {
                            "text": "Ik zag vanochtend dat mijn auto olie <w>lekt</w> op de oprit.",
                            "target_lang_translations": {
                                "ru": "Сегодня утром я увидел, что из моей машины на подъездную дорожку <w>течет</w> масло."
                            }

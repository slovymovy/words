            "senses": [
                {
                    "sense_id": "f8716ad7-70fc-32d4-b1a6-4dd65c3702c3",
                    "sense_definition": "Iets wat men denkt; een product van het denkproces of de geest.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "mental_product",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>gedachte</w> aan de naderende vakantie maakt me erg vrolijk.",
                            "target_lang_translations": {
                                "en": "The <w>thought</w> of the approaching holiday makes me very cheerful.",
                                "ru": "<w>Мысль</w> о приближающемся отпуске очень меня радует."
                            }
                        },
                        {
                            "text": "Hij was zo diep in <w>gedachten</w> verzonken dat hij de bel niet hoorde.",
                            "target_lang_translations": {
                                "en": "He was so lost in <w>thought</w> that he didn't hear the doorbell.",
                                "ru": "Он был так глубоко погружен в <w>мысли</w>, что не услышал звонка."
                            }
                        },
                        {
                            "text": "Soms heb ik het gevoel dat ik een <w>gedachte</w> van mijn beste vriendin kan lezen.",
                            "target_lang_translations": {
                                "en": "Sometimes I have the feeling that I can read a <w>thought</w> of my best friend.",
                                "ru": "Иногда у меня такое чувство, что я могу прочитать <w>мысль</w> моей лучшей подруги."
                            }
                        }
                    ],
                    "synonyms": [
                        "idee",
                        "inval",
                        "overpeinzing"
                    ],
                    "common_phrases": [
                        "in gedachten",
                        "van gedachten wisselen",
                        "op twee gedachten hinken"
                    ],
                    "target_lang_definitions": {
                        "en": "Something one thinks; a product of the thinking process or the mind.",
                        "ru": "То, что кто-то думает; продукт мыслительного процесса или ума."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thought",
                                "target_lang_sense_clarification": "mental representation or product of thinking"
                            },
                            {
                                "target_lang_word": "idea",
                                "target_lang_sense_clarification": "concept, notion, or plan"
                            }
                        ],
                        "ru": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1d5d70b7-378d-34fe-9433-17bf02fca2fb",
                    "sense_definition": "Zakład przemysłowy, w którym wytapia się metale (np. stal, żelazo, miedź) z rud lub złomu oraz poddaje się je dalszej obróbce.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "zakład_przemysłowy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek przez trzydzieści lat pracował w <w>hucie</w> stali na Śląsku.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка тридцать лет проработал на сталелитейном <w>заводе</w> в Силезии."
                            }
                        },
                        {
                            "text": "Wielka <w>huta</w> miedzi zatrudnia tysiące pracowników z całego regionu.",
                            "target_lang_translations": {
                                "ru": "Огромный медеплавильный <w>завод</w> нанимает тысячи работников со всего региона."
                            }
                        }
                    ],
                    "common_phrases": [
                        "huta stali",
                        "huta żelaza",
                        "huta miedzi",
                        "pracować w hucie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Промышленное предприятие, на котором выплавляют металлы (например, сталь, железо, медь) из руды или лома и подвергают их дальнейшей обработке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "металлургический завод",
                                "target_lang_sense_clarification": "общее название предприятия"
                            },
                            {
                                "target_lang_word": "комбинат",
                                "target_lang_sense_clarification": "крупное промышленное объединение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "398e923a-b0dd-3937-914a-78adf13877d8",
                    "sense_definition": "Zakład przemysłowy zajmujący się wytwarzaniem szkła i wyrobów szklanych.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "zakład_przemysłowy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas wycieczki szkolnej zwiedziliśmy starą <w>hutę</w> szkła artystycznego.",
                            "target_lang_translations": {
                                "ru": "Во время школьной экскурсии мы посетили старый <w>завод</w> художественного стекла."
                            }
                        },
                        {
                            "text": "W tej <w>hucie</w> powstają ręcznie formowane wazony i kieliszki.",
                            "target_lang_translations": {
                                "ru": "На этом <w>заводе</w> изготавливают вазы и бокалы ручной работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "huta szkła"
                    ],
                    "common_phrases": [
                        "huta szkła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Промышленное предприятие, занимающееся производством стекла и стеклянных изделий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стекольный завод",
                                "target_lang_sense_clarification": "производство стекла"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
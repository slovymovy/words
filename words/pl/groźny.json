    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6e495faa-1996-3a51-a8e3-d0f47b30092d",
                    "sense_definition": "Stanowiący zagrożenie dla zdrowia, życia lub bezpieczeństwa.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "danger",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To jest bardzo <w>groźny</w> wirus, na którego nie ma jeszcze lekarstwa.",
                            "target_lang_translations": {
                                "ru": "Это очень <w>опасный</w> вирус, от которого ещё нет лекарства."
                            }
                        },
                        {
                            "text": "Epidemia <w>groźnej</w> śpiączki błyskawicznie się rozszerza w regionie.",
                            "target_lang_translations": {
                                "ru": "Эпидемия <w>опасной</w> сонной болезни молниеносно распространяется в регионе."
                            }
                        }
                    ],
                    "synonyms": [
                        "niebezpieczny",
                        "ryzykowny"
                    ],
                    "antonyms": [
                        "bezpieczny"
                    ],
                    "common_phrases": [
                        "groźna choroba",
                        "groźny wypadek",
                        "groźny przestępca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Представляющий угрозу для здоровья, жизни или безопасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опасный",
                                "target_lang_sense_clarification": "способный причинить вред"
                            },
                            {
                                "target_lang_word": "угрожающий",
                                "target_lang_sense_clarification": "несущий угрозу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "345ab1ed-d4ae-3390-a684-4f38f94324f9",
                    "sense_definition": "Wzbudzający strach swoim wyglądem lub zachowaniem; wyrażający wrogość.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "fear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>groźna</w> mina sprawiła, że wszyscy w pokoju zamilkli.",
                            "target_lang_translations": {
                                "ru": "Его <w>грозный</w> вид заставил всех в комнате замолчать."
                            }
                        },
                        {
                            "text": "Za płotem biegał duży i <w>groźny</w> pies sąsiadów.",
                            "target_lang_translations": {
                                "ru": "За забором бегала большая и <w>свирепая</w> соседская собака."
                            }
                        }
                    ],
                    "synonyms": [
                        "straszny",
                        "wrogi",
                        "gniewny",
                        "grożący"
                    ],
                    "antonyms": [
                        "przyjazny",
                        "przychylny"
                    ],
                    "common_phrases": [
                        "groźna mina",
                        "groźny wzrok",
                        "groźny pies"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внушающий страх своим видом или поведением; выражающий враждебность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грозный",
                                "target_lang_sense_clarification": "суровый, внушающий страх"
                            },
                            {
                                "target_lang_word": "свирепый",
                                "target_lang_sense_clarification": "хищный, злой (часто о животных)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2aaadebe-f7d6-3d68-9d67-60040f50719b",
                    "sense_definition": "Bardzo surowy, gwałtowny lub niosący dotkliwe skutki.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "severity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Błąd w obliczeniach może mieć <w>groźne</w> konsekwencje dla całego projektu.",
                            "target_lang_translations": {
                                "ru": "Ошибка в расчётах может иметь <w>серьёзные</w> последствия для всего проекта."
                            }
                        },
                        {
                            "text": "Nadciąga <w>groźny</w> huragan, który może zniszczyć nadmorskie miasto.",
                            "target_lang_translations": {
                                "ru": "Надвигается <w>опасный</w> ураган, который может разрушить прибрежный город."
                            }
                        }
                    ],
                    "synonyms": [
                        "surowy",
                        "dotkliwy",
                        "okrutny"
                    ],
                    "common_phrases": [
                        "groźne skutki",
                        "groźny sztorm",
                        "groźny przeciwnik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень суровый, сильный или влекущий за собой тяжёлые последствия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "серьёзный",
                                "target_lang_sense_clarification": "значительный (о последствиях)"
                            },
                            {
                                "target_lang_word": "тяжёлый",
                                "target_lang_sense_clarification": "суровый, трудный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
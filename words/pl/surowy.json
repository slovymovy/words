{
    "word_family": [
        "surowość",
        "surowiec",
        "surowo"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d0a8769a-56d6-3f93-a26c-fcf4527961a6",
                    "sense_definition": "Taki, który nie został poddany obróbce termicznej (nie jest ugotowany, usmażony ani upieczony).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zalecił mi jedzenie dużej ilości <w>surowych</w> warzyw i owoców każdego dnia.",
                            "target_lang_translations": {
                                "ru": "Врач порекомендовал мне есть много <w>сырых</w> овощей и фруктов каждый день."
                            }
                        },
                        {
                            "text": "W tej japońskiej restauracji podają wyśmienitą <w>surową</w> rybę z dodatkiem ostrego chrzanu.",
                            "target_lang_translations": {
                                "ru": "В этом японском ресторане подают отличную <w>сырую</w> рыбу с острым хреном."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieugotowany",
                        "świeży"
                    ],
                    "common_phrases": [
                        "surowe mięso",
                        "surowe warzywa",
                        "surowa ryba",
                        "jeść na surowo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не подвергшийся термической обработке (не вареный, не жареный и не печеный)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сырой",
                                "target_lang_sense_clarification": "о продуктах питания: не приготовленный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a0ddaff-050d-3959-a93c-8913ec20733a",
                    "sense_definition": "Bardzo wymagający, nieuznający sprzeciwu ani okoliczności łagodzących; rygorystycznie przestrzegający zasad.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz nowy nauczyciel matematyki jest bardzo <w>surowy</w> i rzadko stawia dobre oceny.",
                            "target_lang_translations": {
                                "ru": "Наш новый учитель математики очень <w>строгий</w> и редко ставит хорошие оценки."
                            }
                        },
                        {
                            "text": "Sąd wydał wyjątkowo <w>surowy</w> wyrok w sprawie kradzieży samochodów.",
                            "target_lang_translations": {
                                "ru": "Суд вынес исключительно <w>суровый</w> приговор по делу о краже автомобилей."
                            }
                        }
                    ],
                    "synonyms": [
                        "bezwzględny",
                        "srogi",
                        "rygorystyczny",
                        "niewyrozumiały",
                        "ostry",
                        "konsekwentny"
                    ],
                    "antonyms": [
                        "pobłażliwy",
                        "wyrozumiały",
                        "łagodny"
                    ],
                    "common_phrases": [
                        "surowy nauczyciel",
                        "surowa kara",
                        "surowy zakaz",
                        "surowy ojciec"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень требовательный, не допускающий возражений; жестко соблюдающий правила."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строгий",
                                "target_lang_sense_clarification": "требовательный (о человеке)"
                            },
                            {
                                "target_lang_word": "суровый",
                                "target_lang_sense_clarification": "беспощадный, жестокий (о наказании или характере)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34430cf4-298a-3b3e-a42d-7ed567b8188e",
                    "sense_definition": "Charakteryzujący się niskimi temperaturami i trudnymi, nieprzyjemnymi warunkami pogodowymi.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieszkańcy tej wyspy musieli przyzwyczaić się do życia w bardzo <w>surowym</w> klimacie.",
                            "target_lang_translations": {
                                "ru": "Жители этого острова были вынуждены привыкнуть к жизни в очень <w>суровом</w> климате."
                            }
                        },
                        {
                            "text": "Nadchodzi długa i <w>surowa</w> zima, więc musimy zgromadzić więcej opału.",
                            "target_lang_translations": {
                                "ru": "Грядёт долгая и <w>суровая</w> зима, поэтому нам нужно запасти больше топлива."
                            }
                        }
                    ],
                    "synonyms": [
                        "srogi",
                        "ostry"
                    ],
                    "antonyms": [
                        "łagodny",
                        "ciepły"
                    ],
                    "common_phrases": [
                        "surowy klimat",
                        "surowa zima",
                        "surowe warunki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характеризующийся низкими температурами и тяжелыми, неприятными погодными условиями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "суровый",
                                "target_lang_sense_clarification": "о климате или погоде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bc8ad65b-e1c8-3ddb-a54a-8ae67c44daa1",
                    "sense_definition": "Niepoddany obróbce, znajdujący się w stanie naturalnym lub niewykończonym (o materiałach i budynkach).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dom jest obecnie w stanie <w>surowym</w>, brakuje jeszcze okien, drzwi i tynków.",
                            "target_lang_translations": {
                                "ru": "Дом сейчас в <w>черновом</w> состоянии, не хватает еще окон, дверей и штукатурки."
                            }
                        },
                        {
                            "text": "Architekci coraz chętniej wykorzystują <w>surowy</w> beton w nowoczesnych wnętrzach biurowych.",
                            "target_lang_translations": {
                                "ru": "Архитекторы всё охотнее используют <w>необработанный</w> бетон в современных офисных интерьерах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stan surowy",
                        "surowy beton",
                        "surowy jedwab"
                    ],
                    "target_lang_definitions": {
                        "ru": "Необработанный, находящийся в естественном или незавершенном состоянии (о материалах и зданиях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "необработанный",
                                "target_lang_sense_clarification": "о материалах"
                            },
                            {
                                "target_lang_word": "черновой",
                                "target_lang_sense_clarification": "о стадии строительства или отделки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87a96551-8587-39ae-8c02-9935cf36d6cf",
                    "sense_definition": "Pozbawiony ozdób, wygód lub elementów dekoracyjnych; bardzo prosty i ascetyczny.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej mieszkanie urządzone jest w <w>surowym</w> stylu, bez zbędnych mebli i kolorowych dodatków.",
                            "target_lang_translations": {
                                "ru": "Её квартира обставлена в <w>строгом</w> стиле, без лишней мебели и цветных аксессуаров."
                            }
                        },
                        {
                            "text": "Mnisi wiedli bardzo <w>surowe</w> życie w odizolowanym od świata klasztorze.",
                            "target_lang_translations": {
                                "ru": "Монахи вели очень <w>аскетичную</w> жизнь в изолированном от мира монастыре."
                            }
                        }
                    ],
                    "synonyms": [
                        "prosty",
                        "skromny",
                        "spartański"
                    ],
                    "common_phrases": [
                        "surowy wystrój",
                        "surowe piękno",
                        "surowe wnętrze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишенный украшений, удобств или декоративных элементов; очень простой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аскетичный",
                                "target_lang_sense_clarification": "лишенный удобств и украшений"
                            },
                            {
                                "target_lang_word": "строгий",
                                "target_lang_sense_clarification": "простой, без излишеств (о стиле)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "ab7b1604-dc95-30de-9feb-32cd97badfdf",
                    "sense_definition": "Nazwisko używane w Polsce.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Wczoraj poznałem pana Piotra <w>Surowego</w>, który jest znanym lokalnym artystą.",
                            "target_lang_translations": {
                                "ru": "Вчера я познакомился с паном Петром <w>Суровым</w>, который является известным местным художником."
                            }
                        },
                        {
                            "text": "Na liście obecności brakuje tylko pani Anny <w>Surowej</w>.",
                            "target_lang_translations": {
                                "ru": "В списке присутствующих не хватает только пани Анны <w>Суровой</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия, используемая в Польше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Суровый",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
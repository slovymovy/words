    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bd467fb6-38cb-3b24-8a12-4d9bcc48cb8f",
                    "sense_definition": "Бросить вниз с чего-либо; заставить упасть вниз.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_drop",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рабочие <сбросили> старый снег с крыши дома.",
                            "target_lang_translations": {
                                "en": "The workers <w>threw</w> the old snow <w>off</w> the roof of the house."
                            }
                        },
                        {
                            "text": "Он <сбросил> тяжёлый рюкзак на землю и вздохнул с облегчением.",
                            "target_lang_translations": {
                                "en": "He <w>dropped</w> his heavy backpack to the ground and sighed with relief."
                            }
                        }
                    ],
                    "synonyms": [
                        "скинуть"
                    ],
                    "antonyms": [
                        "поднять"
                    ],
                    "common_phrases": [
                        "сбросить вниз",
                        "сбросить с крыши",
                        "сбросить бомбу",
                        "сбросить балласт"
                    ],
                    "target_lang_definitions": {
                        "en": "To throw something down from a height or let it fall."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "throw down",
                                "target_lang_sense_clarification": "from a height"
                            },
                            {
                                "target_lang_word": "drop",
                                "target_lang_sense_clarification": "let fall"
                            },
                            {
                                "target_lang_word": "throw off",
                                "target_lang_sense_clarification": "remove from a surface"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56626e70-2a25-327a-a041-984fcd3adcec",
                    "sense_definition": "Быстрым или небрежным движением снять с себя одежду или обувь.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "remove_clothes",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Придя домой, она сразу <сбросила> туфли в коридоре.",
                            "target_lang_translations": {
                                "en": "Arriving home, she immediately <w>kicked off</w> her shoes in the hallway."
                            }
                        },
                        {
                            "text": "Мальчик <сбросил> куртку и побежал купаться в реку.",
                            "target_lang_translations": {
                                "en": "The boy <w>threw off</w> his jacket and ran to swim in the river."
                            }
                        }
                    ],
                    "synonyms": [
                        "скинуть",
                        "скинуть"
                    ],
                    "antonyms": [
                        "надеть"
                    ],
                    "common_phrases": [
                        "сбросить одежду",
                        "сбросить обувь",
                        "сбросить пальто",
                        "сбросить одеяло"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To take off clothes or shoes quickly or carelessly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "kick off",
                                "target_lang_sense_clarification": "shoes"
                            },
                            {
                                "target_lang_word": "throw off",
                                "target_lang_sense_clarification": "clothes"
                            },
                            {
                                "target_lang_word": "slip off",
                                "target_lang_sense_clarification": "clothes or shoes"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff7460f3-9487-389a-8b80-b76381fc95ac",
                    "sense_definition": "Уменьшить, понизить уровень или количество чего-либо (скорость, вес).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "reduce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Водитель <сбросил> скорость перед крутым поворотом.",
                            "target_lang_translations": {
                                "en": "The driver <w>reduced</w> speed before the sharp turn."
                            }
                        },
                        {
                            "text": "К лету я планирую <сбросить> несколько лишних килограммов.",
                            "target_lang_translations": {
                                "en": "By summer, I plan to <w>lose</w> a few extra kilograms."
                            }
                        }
                    ],
                    "synonyms": [
                        "скинуть",
                        "скинуть"
                    ],
                    "antonyms": [
                        "набрать",
                        "увеличить"
                    ],
                    "common_phrases": [
                        "сбросить скорость",
                        "сбросить вес",
                        "сбросить газ"
                    ],
                    "target_lang_definitions": {
                        "en": "To reduce the amount or level of something, such as weight or speed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lose",
                                "target_lang_sense_clarification": "weight"
                            },
                            {
                                "target_lang_word": "reduce",
                                "target_lang_sense_clarification": "speed"
                            },
                            {
                                "target_lang_word": "shed",
                                "target_lang_sense_clarification": "weight"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "866c6a66-59ec-3407-a794-5482e12a446e",
                    "sense_definition": "Переписать информацию (файлы, фото) с одного носителя на другой или отправить кому-то через интернет.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "tech_transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Можешь <сбросить> мне эти фотографии в мессенджере?",
                            "target_lang_translations": {
                                "en": "Can you <w>send</w> me these photos in the messenger?"
                            }
                        },
                        {
                            "text": "Я <сбросил> все важные документы на флешку.",
                            "target_lang_translations": {
                                "en": "I <w>transferred</w> all important documents to the flash drive."
                            }
                        }
                    ],
                    "synonyms": [
                        "скинуть"
                    ],
                    "common_phrases": [
                        "сбросить на флешку",
                        "сбросить фотки",
                        "сбросить файл",
                        "сбросить ссылку"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "computer slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To send digital files or information to someone electronically."

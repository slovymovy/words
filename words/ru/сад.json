    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "44496d7d-9214-370d-9b1d-4000dff4b20d",
                    "sense_definition": "Участок земли, засаженный деревьями, кустарниками и цветами для красоты, отдыха или получения плодов.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "garden_plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В нашем <w>саду</w> каждую весну расцветают яблони и вишни."
                        },
                        {
                            "text": "Мы любим гулять в старом городском <w>саду</w>."
                        }
                    ],
                    "synonyms": [
                        "парк",
                        "сквер"
                    ],
                    "common_phrases": [
                        "фруктовый сад",
                        "зимний сад",
                        "ботанический сад",
                        "райский сад"
                    ]
                },
                {
                    "sense_id": "3d326f6f-3579-3a5b-a6ec-6ac7a877a7a3",
                    "sense_definition": "Разговорное сокращение от «детский сад»; учреждение для дневного пребывания и воспитания детей дошкольного возраста.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "kindergarten",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой сын уже второй год ходит в <w>сад</w>."
                        },
                        {
                            "text": "Нужно успеть забрать ребёнка из <w>сада</w> до шести вечера."
                        }
                    ],
                    "synonyms": [
                        "де́тский сад"
                    ],
                    "common_phrases": [
                        "ходить в сад",
                        "место в саду"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for детский сад"
                        }
                    ]
                },
                {
                    "sense_id": "573757f1-2faa-34fd-887a-ec80ae1bcace",
                    "sense_definition": "Земельный участок в садовом товариществе, часто с домом, используемый для отдыха и выращивания растений (дача).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "dacha",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На майские праздники мы всей семьёй поедем в <w>сад</w> жарить шашлыки."
                        },
                        {
                            "text": "Родители всё лето живут в <w>саду</w> и редко приезжают в город."
                        }
                    ],
                    "synonyms": [
                        "СНТ",
                        "СОТ"
                    ],
                    "common_phrases": [
                        "поехать в сад",
                        "домик в саду"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in regions to mean dacha or SNT"
                        }
                    ]
                },
                {
                    "sense_id": "ebc43ac2-1b30-3f0f-9f51-4e45009a7a4b",
                    "sense_definition": "Участок земли, предназначенный для выращивания овощей; огород.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "vegetable_garden",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бабушка с утра работает в <w>саду</w>, пропалывает грядки с морковью."
                        },
                        {
                            "text": "В этом году мы посадили в <w>саду</w> только картошку и лук."
                        }
                    ],
                    "synonyms": [
                        "огород"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "in some dialects сад implies vegetable garden"
                        }
                    ]
                }
            ]
        },

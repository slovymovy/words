{
    "word_family": [
        "studzienka",
        "studzienny",
        "studniarz",
        "studniówka"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7f6d36b7-9927-3941-97e6-3244a20a628a",
                    "sense_definition": "Głęboki otwór wykopany w ziemi, zazwyczaj obudowany, służący do pobierania wody z warstw wodonośnych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "water_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Woda z tej starej <w>studni</w> jest zawsze zimna i orzeźwiająca.",
                            "target_lang_translations": {
                                "ru": "Вода из этого старого <w>колодца</w> всегда холодная и освежающая."
                            }
                        },
                        {
                            "text": "Mój dziadek musiał kopać <w>studnię</w> przez wiele tygodni.",
                            "target_lang_translations": {
                                "ru": "Моему дедушке пришлось копать <w>колодец</w> много недель."
                            }
                        }
                    ],
                    "synonyms": [
                        "studnica"
                    ],
                    "common_phrases": [
                        "głęboka studnia",
                        "woda ze studni",
                        "kopać studnię",
                        "studnia życzeń"
                    ],
                    "target_lang_definitions": {
                        "ru": "Глубокая яма, вырытая в земле, обычно укрепленная, служащая для добычи воды из водоносных слоев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колодец",
                                "target_lang_sense_clarification": "источник воды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97ca84a5-0789-34fc-ba16-df20c2740b91",
                    "sense_definition": "Podwórko lub przestrzeń otoczona ze wszystkich stron wysokimi ścianami budynków, do której dociera mało światła.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Okna ich mieszkania wychodziły na ciemną, betonową <w>studnię</w>.",
                            "target_lang_translations": {
                                "ru": "Окна их квартиры выходили в темный бетонный <w>колодец</w>."
                            }
                        },
                        {
                            "text": "Dźwięki niosły się echem w tej kamiennej <w>studni</w>.",
                            "target_lang_translations": {
                                "ru": "Звуки эхом разносились в этом каменном <w>колодце</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "podwórko-studnia",
                        "mieszkać w studni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двор или пространство, окруженное со всех сторон высокими стенами зданий, куда попадает мало света."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двор-колодец",
                                "target_lang_sense_clarification": "тип двора"
                            },
                            {
                                "target_lang_word": "колодец",
                                "target_lang_sense_clarification": "замкнутое пространство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3c54acc-f47a-3971-af95-57e100d6da67",
                    "sense_definition": "Pionowy, podziemny korytarz, szyb lub naturalny otwór w jaskini.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geology_mining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Speleolodzy zjechali na linach do głębokiej <w>studni</w> jaskiniowej.",
                            "target_lang_translations": {
                                "ru": "Спелеологи спустились на веревках в глубокий пещерный <w>колодец</w>."
                            }
                        },
                        {
                            "text": "Woda deszczowa spływała w dół skalną <w>studnią</w>.",
                            "target_lang_translations": {
                                "ru": "Дождевая вода стекала вниз по скальному <w>колодцу</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "studnia krasowa",
                        "studnia kablowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вертикальный подземный коридор, шахта или естественное отверстие в пещере."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колодец",
                                "target_lang_sense_clarification": "геология, техника"
                            },
                            {
                                "target_lang_word": "шахта",
                                "target_lang_sense_clarification": "горное дело"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd7f55f4-855c-3087-9948-27a355a63366",
                    "sense_definition": "Zstępujący prąd powietrza, który może powodować nagłą utratę wysokości przez statek powietrzny.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "meteorology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samolot wpadł w <w>studnię</w> i pasażerowie poczuli gwałtowne szarpnięcie.",
                            "target_lang_translations": {
                                "ru": "Самолет попал в <w>воздушную яму</w>, и пассажиры почувствовали резкий толчок."
                            }
                        },
                        {
                            "text": "Pilot szybowca musiał ominąć niebezpieczną <w>studnię</w> powietrzną.",
                            "target_lang_translations": {
                                "ru": "Пилот планера должен был обойти опасную <w>воздушную яму</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "studnia powietrzna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нисходящий поток воздуха, который может вызвать внезапную потерю высоты воздушным судном."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воздушная яма",
                                "target_lang_sense_clarification": "турбулентность"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
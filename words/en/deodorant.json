    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e1e8d2f7-adf2-3d02-b936-a75091ab9254",
                    "sense_definition": "A substance applied to the skin, especially under the arms, to prevent or mask body odor.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "personal_care",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I always apply <w>deodorant</w> after taking a shower in the morning.",
                            "target_lang_translations": {
                                "ru": "Я всегда наношу <w>дезодорант</w> после утреннего душа."
                            }
                        },
                        {
                            "text": "She prefers to use a natural <w>deodorant</w> that does not contain aluminum.",
                            "target_lang_translations": {
                                "ru": "Она предпочитает использовать натуральный <w>дезодорант</w>, который не содержит алюминия."
                            }
                        }
                    ],
                    "common_phrases": [
                        "apply deodorant",
                        "wear deodorant",
                        "stick deodorant",
                        "roll-on deodorant",
                        "spray deodorant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещество, наносимое на кожу, особенно в области подмышек, для предотвращения или маскировки запаха тела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дезодорант",
                                "target_lang_sense_clarification": "косметическое средство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b60725e-4be2-323d-bd47-8e3064ddb741",
                    "sense_definition": "Any substance used to remove or hide unpleasant smells from an area or object.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "general_agent",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Baking soda acts as a simple <w>deodorant</w> for the refrigerator.",
                            "target_lang_translations": {
                                "ru": "Пищевая сода действует как простой <w>поглотитель запаха</w> для холодильника."
                            }
                        },
                        {
                            "text": "They used a strong carpet <w>deodorant</w> to remove the smell of smoke.",
                            "target_lang_translations": {
                                "ru": "Они использовали сильный <w>дезодорант</w> для ковров, чтобы удалить запах дыма."
                            }
                        }
                    ],
                    "synonyms": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fb61dbf5-c57f-3862-8ba6-e67bc06d0c8c",
                    "sense_definition": "Sposób, w jaki elementy są rozmieszczone, uporządkowane lub połączone ze sobą, tworząc pewną całość.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "arrangement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <układ> mebli w salonie sprawia, że pokój wydaje się większy.",
                            "target_lang_translations": {
                                "ru": "Эта <расстановка> мебели в гостиной делает комнату визуально больше."
                            }
                        },
                        {
                            "text": "Musimy zmienić <układ> graficzny naszej strony internetowej.",
                            "target_lang_translations": {
                                "ru": "Нам нужно изменить графический <макет> нашего веб-сайта."
                            }
                        }
                    ],
                    "synonyms": [
                        "konfiguracja"
                    ],
                    "antonyms": [
                        "bezład"
                    ],
                    "common_phrases": [
                        "układ graficzny",
                        "układ strony",
                        "w tym układzie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способ, которым элементы размещены, упорядочены или соединены друг с другом, образуя некое целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расположение",
                                "target_lang_sense_clarification": "размещение предметов в пространстве"
                            },
                            {
                                "target_lang_word": "планировка",
                                "target_lang_sense_clarification": "схема помещений или пространства"
                            },
                            {
                                "target_lang_word": "расстановка",
                                "target_lang_sense_clarification": "порядок размещения мебели или людей"
                            },
                            {
                                "target_lang_word": "макет",
                                "target_lang_sense_clarification": "графическое оформление страницы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2da5d0af-fe43-3707-ae26-8e2c9833397e",
                    "sense_definition": "Zespół narządów w organizmie, które współpracują ze sobą, pełniąc określoną funkcję życiową.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "biological_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stres ma bardzo negatywny wpływ na <układ> nerwowy człowieka.",
                            "target_lang_translations": {
                                "ru": "Стресс оказывает очень негативное влияние на нервную <систему> человека."
                            }
                        },
                        {
                            "text": "Zdrowa dieta wspomaga <układ> odpornościowy w walce z chorobami.",
                            "target_lang_translations": {
                                "ru": "Здоровая диета помогает иммунной <системе> бороться с болезнями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "układ nerwowy",
                        "układ pokarmowy",
                        "układ krwionośny",
                        "układ oddechowy"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "biologia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность органов в организме, взаимодействующих друг с другом для выполнения определённой жизненной функции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "система",
                                "target_lang_sense_clarification": "биологическая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e022e4b8-0a93-3853-97ab-cf794c36a5e8",
                    "sense_definition": "Zbiór powiązanych ze sobą elementów lub mechanizmów, które działają według określonych zasad.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "general_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ziemia jest jedną z planet wchodzących w skład <układu> słonecznego.",
                            "target_lang_translations": {
                                "ru": "Земля — одна из планет, входящих в состав Солнечной <системы>."
                            }
                        },
                        {
                            "text": "Awaria zablokowała cały <układ> sterowania w fabryce.",
                            "target_lang_translations": {
                                "ru": "Авария заблокировала всю <систему> управления на заводе."
                            }
                        }
                    ],
                    "synonyms": [
                        "system"
                    ],
                    "common_phrases": [
                        "układ słoneczny",
                        "układ odniesienia",
                        "układ zamknięty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор связанных элементов или механизмов, работающих по определённым правилам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "система",
                                "target_lang_sense_clarification": "техническая или астрономическая"
                            },
                            {
                                "target_lang_word": "устройство",
                                "target_lang_sense_clarification": "механизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28b82c4e-53d7-3a2c-a555-cefc9c4cafd9",
                    "sense_definition": "Oficjalne porozumienie lub umowa między państwami, organizacjami lub osobami.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dwa sąsiednie kraje podpisały <układ> o współpracy gospodarczej.",
                            "target_lang_translations": {
                                "ru": "Две соседние страны подписали <соглашение> об экономическом сотрудничестве."
                            }
                        },
                        {
                            "text": "To był tajny <układ> między politykami a biznesmenami.",
                            "target_lang_translations": {
                                "ru": "Это была тайная <сделка> между политиками и бизнесменами."
                            }
                        }
                    ],
                    "synonyms": [
                        "porozumienie",
                        "umowa"
                    ],
                    "common_phrases": [
                        "układ pokojowy",
                        "układ handlowy",
                        "układ z Schengen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное соглашение или договор между государствами, организациями или частными лицами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "договор",
                                "target_lang_sense_clarification": "официальный документ"
                            },
                            {
                                "target_lang_word": "соглашение",
                                "target_lang_sense_clarification": "взаимная договоренность"
                            },
                            {
                                "target_lang_word": "сделка",
                                "target_lang_sense_clarification": "деловое или тайное соглашение"
                            },
                            {
                                "target_lang_word": "пакт",
                                "target_lang_sense_clarification": "международный политический договор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9256fee-5e3f-3dca-949e-c4a939fb1092",
                    "sense_definition": "Element elektroniczny składający się z wielu połączonych części, często w formie małej płytki (czip).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "electronics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W moim komputerze przegrzał się <układ> graficzny.",
                            "target_lang_translations": {
                                "ru": "В моем компьютере перегрелся графический <чип>."
                            }
                        },
                        {
                            "text": "Ten mały <układ> scalony steruje pracą całego urządzenia.",
                            "target_lang_translations": {
                                "ru": "Эта маленькая интегральная <схема> управляет работой всего устройства."
                            }
                        }
                    ],
                    "common_phrases": [
                        "układ scalony",
                        "układ elektroniczny"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technika"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Электронный компонент, состоящий из множества соединённых частей, часто в виде небольшой пластины (чип)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "микросхема",
                                "target_lang_sense_clarification": "интегральная схема"
                            },
                            {
                                "target_lang_word": "чип",
                                "target_lang_sense_clarification": "разговорное название микросхемы"
                            },
                            {
                                "target_lang_word": "схема",
                                "target_lang_sense_clarification": "электронная цепь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b226a8b-5d0b-3b48-adca-d66e82af8e0f",
                    "sense_definition": "Zaplanowana sekwencja ruchów i figur w tańcu lub gimnastyce.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Para łyżwiarzy zaprezentowała bardzo trudny <układ> dowolny.",
                            "target_lang_translations": {
                                "ru": "Пара фигуристов представила очень сложную произвольную <программу>."
                            }
                        },
                        {
                            "text": "Tancerze ćwiczyli ten <układ> choreograficzny przez wiele tygodni.",
                            "target_lang_translations": {
                                "ru": "Танцоры репетировали эту хореографическую <композицию> много недель."
                            }
                        }
                    ],
                    "common_phrases": [
                        "układ taneczny",
                        "układ choreograficzny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запланированная последовательность движений и фигур в танце или гимнастике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "программа",
                                "target_lang_sense_clarification": "в спорте (фигурное катание, гимнастика)"
                            },
                            {
                                "target_lang_word": "композиция",
                                "target_lang_sense_clarification": "в танце"
                            },
                            {
                                "target_lang_word": "номер",
                                "target_lang_sense_clarification": "выступление"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                },
                {
                    "sense_id": "d9434e6c-7f80-33df-aba9-aac36c28c9cc",
                    "sense_definition": "To rely on someone or something for help, support, or comfort.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can always <w>lean</w> on your family and friends during difficult times.",
                            "target_lang_translations": {
                                "ru": "Ты всегда можешь <w>положиться</w> на семью и друзей в трудные времена."
                            }
                        },
                        {
                            "text": "The team had to <w>lean</w> heavily on their manager to get the project finished.",
                            "target_lang_translations": {
                                "ru": "Команде пришлось сильно <w>полагаться</w> на своего менеджера, чтобы завершить проект."
                            }
                        }
                    ],
                    "synonyms": [
                        "rely",
                        "depend",
                        "count on"
                    ],
                    "common_phrases": [

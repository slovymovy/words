    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2a4f4cd0-0e50-33d9-a214-012acdef2b5d",
                    "sense_definition": "Bardzo bystry, przenikliwy i ostry; zazwyczaj w odniesieniu do wzroku lub spostrzegawczości.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek mimo podeszłego wieku wciąż ma <w>sokoli</w> wzrok.",
                            "target_lang_translations": {
                                "ru": "У моего дедушки, несмотря на преклонный возраст, всё ещё <w>соколиное</w> зрение."
                            }
                        },
                        {
                            "text": "Nauczycielka rzuciła na klasę <w>sokole</w> spojrzenie, szukając ściągających uczniów.",
                            "target_lang_translations": {
                                "ru": "Учительница бросила на класс <w>соколиный</w> взгляд, выискивая списывающих учеников."
                            }
                        }
                    ],
                    "synonyms": [
                        "bystry",
                        "przenikliwy",
                        "ostry",
                        "uważny"
                    ],
                    "antonyms": [
                        "słaby",
                        "nieuważny"
                    ],
                    "common_phrases": [
                        "sokoli wzrok",
                        "sokole oko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень острый, проницательный; обычно относится к зрению или наблюдательности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соколиный",
                                "target_lang_sense_clarification": "о зрении, взгляде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8778891a-9506-38cd-b9f3-8fb92a8c7aa4",
                    "sense_definition": "Związany z sokołem, należący do sokoła lub pochodzący od niego.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "literal_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas wycieczki w góry znaleźliśmy opuszczone <w>sokole</w> gniazdo na skale.",
                            "target_lang_translations": {
                                "ru": "Во время похода в горы мы нашли на скале покинутое <w>соколиное</w> гнездо."
                            }
                        },
                        {
                            "text": "Jego kapelusz był ozdobiony pięknym <w>sokolim</w> piórem.",
                            "target_lang_translations": {
                                "ru": "Его шляпа была украшена красивым <w>соколиным</w> пером."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sokole gniazdo",
                        "sokole pióro"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с соколом, принадлежащий соколу или происходящий от него."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соколиный",
                                "target_lang_sense_clarification": "относящийся к соколу"
                            },
                            {
                                "target_lang_word": "соколий",
                                "target_lang_sense_clarification": "принадлежащий соколу"
                            }
                        ]
                    }
                }
            ]
        },

{
    "word_family": [
        "programmer",
        "programming",
        "programmable",
        "programmatic"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "31199160-e268-35f5-9963-00e104ad66ed",
                    "sense_definition": "A set of instructions that a computer follows to perform a specific task.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>program</w> runs on both Linux and Microsoft Windows.",
                            "target_lang_translations": {
                                "ru": "Эта <w>программа</w> работает как на Linux, так и на Microsoft Windows."
                            }
                        },
                        {
                            "text": "She learned how to write a simple computer <w>program</w> in school.",
                            "target_lang_translations": {
                                "ru": "В школе она научилась писать простую компьютерную <w>программу</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "application",
                        "computer program"
                    ],
                    "common_phrases": [
                        "computer program",
                        "install a program",
                        "run a program"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор инструкций, которые выполняет компьютер для решения конкретной задачи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "программа",
                                "target_lang_sense_clarification": "компьютерная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1496566-eb5c-3011-a6d7-aa2642874701",
                    "sense_definition": "A performance of a show or other broadcast on radio or television.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "media",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tonight’s <w>program</w> was hosted by a famous comedian.",
                            "target_lang_translations": {
                                "ru": "Ведущим сегодняшней <w>передачи</w> был известный комик."
                            }
                        },
                        {
                            "text": "My favorite television <w>program</w> starts at eight o'clock.",
                            "target_lang_translations": {
                                "ru": "Моя любимая телевизионная <w>передача</w> начинается в восемь часов."
                            }
                        }
                    ],
                    "synonyms": [
                        "show"
                    ],
                    "common_phrases": [
                        "television program",
                        "news program",
                        "radio program"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Standard US spelling; usually 'programme' in British English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выпуск шоу или другой трансляции по радио или телевидению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передача",
                                "target_lang_sense_clarification": "телерадиовещание"
                            },
                            {
                                "target_lang_word": "программа",
                                "target_lang_sense_clarification": "шоу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c51aa40a-89ee-3b64-80f5-b407ce2efb67",
                    "sense_definition": "A set of structured activities or a plan of action designed to achieve a specific goal.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "planning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Our <w>program</w> for today’s exercise class includes swimming and jogging.",
                            "target_lang_translations": {
                                "ru": "Наша <w>программа</w> сегодняшней тренировки включает плавание и бег."
                            }
                        },
                        {
                            "text": "The university offers a study abroad <w>program</w> for language students.",
                            "target_lang_translations": {
                                "ru": "Университет предлагает <w>программу</w> обучения за рубежом для студентов языковых специальностей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "training program",
                        "exchange program",
                        "get with the program"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Standard US spelling; usually 'programme' in British English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор структурированных мероприятий или план действий, разработанный для достижения конкретной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "программа",
                                "target_lang_sense_clarification": "план действий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c67ba6f0-00e1-3ab2-9f80-1ab9c26f4b3a",
                    "sense_definition": "A leaflet or booklet listing information about a play, game, or other event.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "media",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>program</w> listed the names of all the actors in the play.",
                            "target_lang_translations": {
                                "ru": "В <w>программке</w> были перечислены имена всех актеров, занятых в спектакле."
                            }
                        },
                        {
                            "text": "I bought a souvenir <w>program</w> before the concert started.",
                            "target_lang_translations": {
                                "ru": "Я купил сувенирную <w>программку</w> перед началом концерта."
                            }
                        }
                    ],
                    "synonyms": [
                        "playbill"
                    ],
                    "common_phrases": [
                        "souvenir program",
                        "read the program"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Standard US spelling; usually 'programme' in British English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Листовка или буклет с информацией о пьесе, игре или другом мероприятии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "программка",
                                "target_lang_sense_clarification": "театральная"
                            },
                            {
                                "target_lang_word": "программа",
                                "target_lang_sense_clarification": "буклет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d67dda13-0bfc-34a0-9a3c-2e5cef84c7d3",
                    "sense_definition": "A set of principal goals or policies which a political party or candidate supports.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "politics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The candidate explained his economic <w>program</w> to the voters.",
                            "target_lang_translations": {
                                "ru": "Кандидат разъяснил избирателям свою экономическую <w>программу</w>."
                            }
                        },
                        {
                            "text": "Social reform is a key part of the party's political <w>program</w>.",
                            "target_lang_translations": {
                                "ru": "Социальная реформа является ключевой частью политической <w>программы</w> партии."
                            }
                        }
                    ],
                    "synonyms": [
                        "platform"
                    ],
                    "common_phrases": [
                        "political program",
                        "party program"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Standard US spelling; usually 'programme' in British English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор основных целей или политик, которые поддерживает политическая партия или кандидат."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "программа",
                                "target_lang_sense_clarification": "политическая"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8a86d7b2-f88d-3e32-98a6-cc0ab5b7d63f",
                    "sense_definition": "To enter instructions into a computer or electronic device to make it perform a particular task.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>programmed</w> the DVR to record his favorite show automatically.",
                            "target_lang_translations": {
                                "ru": "Он <w>запрограммировал</w> видеорегистратор на автоматическую запись своего любимого шоу."
                            }
                        },
                        {
                            "text": "I need to <w>program</w> the thermostat to turn down the heat at night.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>запрограммировать</w> термостат на понижение температуры ночью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "program a computer",
                        "program a robot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вводить инструкции в компьютер или электронное устройство для выполнения определенной задачи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "программировать",
                                "target_lang_sense_clarification": "писать код"
                            },
                            {
                                "target_lang_word": "писать программу",
                                "target_lang_sense_clarification": "создавать ПО"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19cd5947-6d04-3c11-94b5-45d2acf9fd13",
                    "sense_definition": "To cause a person or animal to automatically behave in a particular way.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lab rat was <w>programmed</w> to press the lever when the bell rang.",
                            "target_lang_translations": {
                                "ru": "Лабораторная крыса была <w>запрограммирована</w> нажимать на рычаг при звонке."
                            }
                        },
                        {
                            "text": "Human beings are biologically <w>programmed</w> to seek food and shelter.",
                            "target_lang_translations": {
                                "ru": "Люди биологически <w>запрограммированы</w> искать пищу и укрытие."
                            }
                        }
                    ],
                    "common_phrases": [
                        "genetically programmed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять человека или животное автоматически вести себя определенным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "программировать",
                                "target_lang_sense_clarification": "внушать поведение"
                            },
                            {
                                "target_lang_word": "настраивать",
                                "target_lang_sense_clarification": "психологически"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "097ae155-47fc-3a91-b0e9-01edd93054ad",
                    "sense_definition": "To plan the schedule of an event or determine what will be broadcast.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "planning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mary will <w>program</w> the festivities for the holiday celebration.",
                            "target_lang_translations": {
                                "ru": "Мэри будет <w>составлять программу</w> праздничных мероприятий."
                            }
                        },
                        {
                            "text": "The station <w>programs</w> classical music every Sunday morning.",
                            "target_lang_translations": {
                                "ru": "Каждое воскресное утро станция <w>ставит в программу</w> классическую музыку."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Планировать расписание мероприятия или определять, что будет транслироваться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "составлять программу",
                                "target_lang_sense_clarification": "планировать"
                            },
                            {
                                "target_lang_word": "планировать",
                                "target_lang_sense_clarification": "расписание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
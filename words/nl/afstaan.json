                {
                    "sense_id": "3ae38539-49f5-3f68-9ed3-7dd771410f61",
                    "sense_definition": "Iets wat men bezit of waar men recht op heeft aan iemand anders geven of overdragen, vaak omdat het moet of gevraagd wordt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relinquish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij moest zijn rijbewijs <w>afstaan</w> nadat hij met te veel alcohol op had gereden.",
                            "target_lang_translations": {
                                "ru": "Ему пришлось <w>сдать</w> свои водительские права после того, как он вел машину в нетрезвом виде.",
                                "en": "He had to <w>surrender</w> his driver's license after driving under the influence of alcohol."
                            }
                        },
                        {
                            "text": "Zij heeft haar deel van de erfenis vrijwillig <w>afgestaan</w> aan een goed doel.",
                            "target_lang_translations": {
                                "ru": "Она добровольно <w>отдала</w> свою часть наследства на благотворительность.",
                                "en": "She voluntarily <w>gave up</w> her share of the inheritance to a charity."
                            }
                        }
                    ],
                    "synonyms": [
                        "overdragen",
                        "weggeven",
                        "inleveren",
                        "prijsgeven"
                    ],
                    "antonyms": [
                        "houden",
                        "behouden",
                        "opeisen"
                    ],
                    "common_phrases": [
                        "zijn plaats afstaan",
                        "bloed afstaan",
                        "rechten afstaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать или отдавать что-либо, чем владеешь или на что имеешь право, другому лицу (часто по необходимости или просьбе).",
                        "en": "To give or transfer something one owns or is entitled to to someone else, often because it is required or requested."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уступать",
                                "target_lang_sense_clarification": "место, право"
                            },
                            {
                                "target_lang_word": "сдавать",
                                "target_lang_sense_clarification": "кровь, документы"
                            },
                            {

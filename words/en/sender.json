{
    "word_family": [
        "send",
        "resend"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "19476fb7-4c61-3acd-8298-5208333e8eaa",
                    "sense_definition": "A person or entity that sends a message, package, or letter.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "agent_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The package was addressed to someone we didn't know, so we returned it to the <w>sender</w>.",
                            "target_lang_translations": {
                                "ru": "Посылка была адресована кому-то, кого мы не знали, поэтому мы вернули её <w>отправителю</w>."
                            }
                        },
                        {
                            "text": "Please check the <w>sender</w> address before opening any suspicious emails.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, проверьте адрес <w>отправителя</w>, прежде чем открывать подозрительные электронные письма."
                            }
                        }
                    ],
                    "antonyms": [
                        "recipient",
                        "receiver",
                        "addressee"
                    ],
                    "common_phrases": [
                        "return to sender",
                        "unknown sender",
                        "sender address"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо или организация, которые отправляют сообщение, посылку или письмо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отправитель",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "адресант",
                                "target_lang_sense_clarification": "официальный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5409133f-3328-345e-a3c3-af98d7aeced6",
                    "sense_definition": "A device or component that transmits signals or data, especially in telecommunications or computing.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "device_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>sender</w> transmits the encrypted signal to the main server.",
                            "target_lang_translations": {
                                "ru": "<w>Передатчик</w> передает зашифрованный сигнал на главный сервер."
                            }
                        },
                        {
                            "text": "If the <w>sender</w> malfunctions, the entire network communication might fail.",
                            "target_lang_translations": {
                                "ru": "Если <w>передатчик</w> выйдет из строя, вся сетевая коммуникация может нарушиться."
                            }
                        }
                    ],
                    "synonyms": [
                        "transmitter"
                    ],
                    "antonyms": [
                        "receiver"
                    ],
                    "common_phrases": [
                        "signal sender",
                        "data sender"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство или компонент, передающий сигналы или данные, особенно в телекоммуникациях или вычислительной технике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передатчик",
                                "target_lang_sense_clarification": "устройство"
                            },
                            {
                                "target_lang_word": "отправитель",
                                "target_lang_sense_clarification": "источник данных"
                            }
                        ]
                    }
                }
            ]
        },
        {

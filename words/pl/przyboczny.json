    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "79b95459-a840-3d56-b6ce-78c09a9bc4b2",
                    "sense_definition": "Pełniący służbę lub funkcję bezpośrednio przy ważnej osobie, często w celu jej ochrony lub pomocy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "role_service",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Straż <w>przyboczna</w> nie odstępowała króla nawet na krok.",
                            "target_lang_translations": {
                                "ru": "<w>Личная</w> охрана не отходила от короля ни на шаг."
                            }
                        },
                        {
                            "text": "Lekarz <w>przyboczny</w> stale monitorował stan zdrowia prezydenta.",
                            "target_lang_translations": {
                                "ru": "<w>Личный</w> врач постоянно следил за состоянием здоровья президента."
                            }
                        }
                    ],
                    "common_phrases": [
                        "straż przyboczna",
                        "lekarz przyboczny",
                        "gwardia przyboczna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Несущий службу или выполняющий функции непосредственно при важном лице (часто для охраны или помощи)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "личный",
                                "target_lang_sense_clarification": "о враче, охране"
                            },
                            {
                                "target_lang_word": "лейб-",
                                "target_lang_sense_clarification": "приставка в названиях придворных должностей (напр. лейб-гвардия, лейб-медик)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82b79d84-1ff1-3095-9937-23128a84d8d3",
                    "sense_definition": "Znajdujący się w bezpośrednim sąsiedztwie, tuż obok czegoś.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mały, <w>przyboczny</w> korytarz prowadził do wyjścia ewakuacyjnego.",
                            "target_lang_translations": {
                                "ru": "Маленький <w>боковой</w> коридор вел к аварийному выходу."
                            }
                        },
                        {
                            "text": "Zatrzymaliśmy się w <w>przybocznym</w> budynku, aby przeczekać burzę.",
                            "target_lang_translations": {
                                "ru": "Мы остановились в <w>соседнем</w> здании, чтобы переждать бурю."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyległy",
                        "sąsiedni",
                        "okoliczny",
                        "pobliski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в непосредственной близости, сбоку от чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "боковой",
                                "target_lang_sense_clarification": "расположенный сбоку"
                            },
                            {
                                "target_lang_word": "смежный",
                                "target_lang_sense_clarification": "соседний, прилегающий"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "50d19602-d685-3ed3-8e02-1f886be21983",
                    "sense_definition": "W harcerstwie: zastępca drużynowego, pomagający mu w prowadzeniu drużyny.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "scouting_rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako <w>przyboczny</w> muszę przygotować plan dzisiejszej zbiórki.",
                            "target_lang_translations": {
                                "ru": "Как <w>помощник вожатого</w>, я должен подготовить план сегодняшнего сбора."
                            }
                        },
                        {
                            "text": "Drużynowy mianował nowego <w>przybocznego</w> podczas ogniska.",
                            "target_lang_translations": {
                                "ru": "Вожатый назначил нового <w>помощника</w> во время костра."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mianować przybocznego",
                        "sznur przybocznego"
                    ],
                    "target_lang_definitions": {
                        "ru": "В скаутском движении: заместитель начальника отряда (дружинового), помогающий ему в руководстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помощник вожатого",
                                "target_lang_sense_clarification": "должность в скаутинге"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17be3ea8-be6d-325d-b066-41ecc4d0e80e",
                    "sense_definition": "Osoba z najbliższego otoczenia dowódcy lub władcy, pełniąca funkcje pomocnicze lub ochronne.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "historical_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wierny <w>przyboczny</w> podał generałowi mapę terenu.",
                            "target_lang_translations": {
                                "ru": "Верный <w>адъютант</w> подал генералу карту местности."
                            }
                        },
                        {
                            "text": "Książę ufał tylko swoim najbliższym <w>przybocznym</w>.",
                            "target_lang_translations": {
                                "ru": "Князь доверял только своим самым близким <w>приближённым</w>."
                            }
                        }
                    ],
                    "synonyms": [

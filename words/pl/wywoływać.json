    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "56f45750-7c38-37d9-927d-585d5d324d60",
                    "sense_definition": "Być przyczyną czegoś; sprawiać, że pojawia się jakieś zjawisko, uczucie lub reakcja.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "causation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego nieodpowiedzialne zachowanie może <w>wywoływać</w> niepotrzebne konflikty w zespole.",
                            "target_lang_translations": {
                                "ru": "Его безответственное поведение может <w>вызывать</w> ненужные конфликты в коллективе."
                            }
                        },
                        {
                            "text": "Ten lek może <w>wywoływać</w> skutki uboczne, takie jak senność czy zawroty głowy.",
                            "target_lang_translations": {
                                "ru": "Это лекарство может <w>вызывать</w> побочные эффекты, такие как сонливость или головокружение."
                            }
                        }
                    ],
                    "synonyms": [
                        "powodować",
                        "prowokować"
                    ],
                    "common_phrases": [
                        "wywoływać wilka z lasu",
                        "wywoływać uśmiech",
                        "wywoływać kontrowersje",
                        "wywoływać skutki uboczne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть причиной чего-либо; приводить к появлению какого-либо явления, чувства или реакции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызывать",
                                "target_lang_sense_clarification": "быть причиной"
                            },
                            {
                                "target_lang_word": "провоцировать",
                                "target_lang_sense_clarification": "побуждать к реакции"
                            },
                            {
                                "target_lang_word": "порождать",
                                "target_lang_sense_clarification": "давать начало"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7705502-fe07-3358-80e6-9d0882bb9421",
                    "sense_definition": "Wzywać kogoś głośno (zwykle po imieniu lub nazwisku), aby podszedł, odezwał się lub wystąpił.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczycielka matematyki lubiła często <w>wywoływać</w> uczniów do tablicy.",
                            "target_lang_translations": {
                                "ru": "Учительница математики любила часто <w>вызывать</w> учеников к доске."
                            }
                        },
                        {
                            "text": "Pielęgniarka zaczęła <w>wywoływać</w> kolejnych pacjentów czekających na korytarzu.",
                            "target_lang_translations": {
                                "ru": "Медсестра начала <w>вызывать</w> следующих пациентов, ожидающих в коридоре."
                            }
                        }
                    ],
                    "synonyms": [
                        "wzywać",
                        "przywoływać",
                        "przyzywać"
                    ],
                    "common_phrases": [
                        "wywoływać do tablicy",
                        "wywoływać po nazwisku",
                        "wywoływać do odpowiedzi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Громко звать кого-либо (обычно по имени), требуя подойти или отозваться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызывать",
                                "target_lang_sense_clarification": "просить выйти или подойти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30ce2200-20e0-31a2-a24f-62a67c628f26",
                    "sense_definition": "Przywoływać coś w pamięci, wyobraźni lub (w kontekście nadprzyrodzonym) próbować sprowadzić ducha.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stare fotografie potrafią <w>wywoływać</w> najpiękniejsze wspomnienia z czasów dzieciństwa.",
                            "target_lang_translations": {
                                "ru": "Старые фотографии могут <w>вызывать</w> самые прекрасные воспоминания из детства."
                            }
                        },
                        {
                            "text": "Bohaterowie filmu próbowali <w>wywoływać</w> duchy podczas nocnego seansu.",
                            "target_lang_translations": {
                                "ru": "Герои фильма пытались <w>вызывать</w> духов во время ночного сеанса."
                            }
                        }
                    ],
                    "synonyms": [
                        "wspominać",
                        "przywoływać"
                    ],
                    "common_phrases": [
                        "wywoływać wspomnienia",
                        "wywoływać duchy",
                        "wywoływać z pamięci"
                    ],
                    "target_lang_definitions": {
                        "ru": "Восстанавливать в памяти, воображении или (в мистике) пытаться призвать духа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызывать",
                                "target_lang_sense_clarification": "духов или воспоминания"
                            },
                            {
                                "target_lang_word": "пробуждать",
                                "target_lang_sense_clarification": "воспоминания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a75eafb-3658-3f6a-ac7e-732b9e5f1634",
                    "sense_definition": "Poddawać materiał fotograficzny (film, kliszę) obróbce chemicznej w celu uzyskania widocznego obrazu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "photography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek w młodości samodzielnie <w>wywoływał</w> czarno-białe zdjęcia w domowej ciemni.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка в молодости самостоятельно <w>проявлял</w> черно-белые фотографии в домашней темной комнате."
                            }
                        },
                        {
                            "text": "Musimy zanieść ten stary film do fotografa, żeby go w końcu <w>wywołać</w>.",
                            "target_lang_translations": {
                                "ru": "Мы должны отнести эту старую пленку фотографу, чтобы наконец её <w>проявить</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wywoływać zdjęcia",
                        "wywoływać film",
                        "wywoływać kliszę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обрабатывать фотоматериал химическим способом для получения изображения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проявлять",
                                "target_lang_sense_clarification": "фотопленку"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
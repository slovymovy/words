    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "04752d5b-7f2f-36e1-b4b7-42d8c6600f02",
                    "sense_definition": "Een (meestal strafbare of negatieve) handeling uitvoeren.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "commit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In Vlaanderen <w>plegen</w> elke dag drie mensen zelfmoord.",
                            "target_lang_translations": {
                                "en": "In Flanders, three people <w>commit</w> suicide every day.",
                                "ru": "Во Фландрии каждый день три человека <w>совершают</w> самоубийство."
                            }
                        },
                        {
                            "text": "De politie zoekt de daders die de overval <w>gepleegd</w> hebben.",
                            "target_lang_translations": {
                                "en": "The police are looking for the perpetrators who <w>committed</w> the robbery.",
                                "ru": "Полиция ищет преступников, которые <w>совершили</w> ограбление."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zelfmoord plegen",
                        "een moord plegen",
                        "verzet plegen",
                        "overspel plegen"
                    ],
                    "target_lang_definitions": {
                        "en": "To perform a (usually punishable or negative) action.",
                        "ru": "Совершать действие (обычно наказуемое или негативное)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "commit",
                                "target_lang_sense_clarification": "to perform a crime, sin, or negative act"
                            },
                            {
                                "target_lang_word": "perpetrate",
                                "target_lang_sense_clarification": "to commit a harmful or illegal action"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "совершать",
                                "target_lang_sense_clarification": "преступление, поступок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ddec20f-1692-3468-8a0a-e9fd06f81cbb",
                    "sense_definition": "De gewoonte hebben om iets te doen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "habit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn grootvader <w>pleegt</w> na het middageten een pijp te roken.",
                            "target_lang_translations": {
                                "en": "My grandfather <w>is in the habit of</w> smoking a pipe after lunch.",
                                "ru": "Мой дедушка <w>имеет обыкновение</w> курить трубку после обеда."
                            }
                        },
                        {
                            "text": "Op onze wandelvakanties <w>placht</w> het vaak te regenen.",
                            "target_lang_translations": {
                                "en": "On our walking holidays, it <w>used</w> to rain often.",
                                "ru": "Во время наших пеших походов часто <w>имел обыкновение</w> идти дождь."
                            }
                        }
                    ],
                    "synonyms": [
                        "gewoon zijn"
                    ],
                    "common_phrases": [
                        "plegen te doen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To have the habit of doing something.",
                        "ru": "Иметь обыкновение что-либо делать."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be in the habit of",
                                "target_lang_sense_clarification": "to do something regularly or customarily"
                            },
                            {
                                "target_lang_word": "be wont to",
                                "target_lang_sense_clarification": "formal: to be accustomed to doing something"
                            },
                            {
                                "target_lang_word": "tend to",
                                "target_lang_sense_clarification": "to be likely to do something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "иметь обыкновение",
                                "target_lang_sense_clarification": "делать что-либо (книжн.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a8a5102-2f06-3429-9623-1f850bf21f41",
                    "sense_definition": "Zorgen voor iemand of iets.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "care",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verpleegster <w>pleegde</w> de gewonde soldaten met veel geduld.",
                            "target_lang_translations": {
                                "en": "The nurse <w>tended</w> the wounded soldiers with much patience.",
                                "ru": "Медсестра с большим терпением <w>ухаживала</w> за ранеными солдатами."
                            }
                        },
                        {
                            "text": "Zij <w>pleegt</w> het onderhoud van de tuin met grote zorg.",
                            "target_lang_translations": {
                                "en": "She <w>tends to</w> the maintenance of the garden with great care.",
                                "ru": "Она с большой заботой <w>осуществляет</w> уход за садом."
                            }
                        }
                    ],
                    "synonyms": [
                        "verplegen",
                        "verzorgen"
                    ],
                    "target_lang_definitions": {
                        "en": "To take care of someone or something.",
                        "ru": "Заботиться о ком-то или чем-то."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tend",
                                "target_lang_sense_clarification": "to care for or look after"
                            },
                            {

                },
                {
                    "sense_id": "af54ba6d-d081-34c9-b6a2-d01fc56ca1e2",
                    "sense_definition": "Als betekenis toekennen aan iets; bedoelen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "definition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wat <w>verstaan</w> we in Nederland eigenlijk onder armoede?",

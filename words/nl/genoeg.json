    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "94924d5a-519f-3003-a860-5109360a2117",
                    "sense_definition": "In een mate die voldoende of toereikend is.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Is de auto groot <w>genoeg</w> voor drie kinderen?",
                            "target_lang_translations": {
                                "ru": "Машина <w>достаточно</w> большая для троих детей?"
                            }
                        },
                        {
                            "text": "Hij liep niet snel <w>genoeg</w> om de bus te halen.",
                            "target_lang_translations": {
                                "ru": "Он шел не <w>достаточно</w> быстро, чтобы успеть на автобус."
                            }
                        }
                    ],
                    "synonyms": [
                        "voldoende"
                    ],
                    "antonyms": [
                        "onvoldoende"
                    ],
                    "common_phrases": [
                        "goed genoeg",
                        "lang genoeg",
                        "snel genoeg",
                        "groot genoeg"
                    ],
                    "target_lang_definitions": {
                        "ru": "В степени, которая является достаточной или удовлетворительной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "достаточно",
                                "target_lang_sense_clarification": "наречие степени"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "3ee356e7-d1aa-38b1-bbfb-692a5d6cf0b9",
                    "sense_definition": "Een hoeveelheid die toereikend is; niet te veel en niet te weinig.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er zijn mensen <w>genoeg</w> die niet van thee houden.",
                            "target_lang_translations": {
                                "ru": "Есть <w>достаточно</w> людей, которые не любят чай."
                            }
                        },
                        {
                            "text": "We hebben <w>genoeg</w> tijd om rustig te ontbijten.",
                            "target_lang_translations": {
                                "ru": "У нас <w>достаточно</w> времени, чтобы спокойно позавтракать."
                            }
                        }
                    ],
                    "synonyms": [
                        "voldoende",
                        "zat"
                    ],
                    "antonyms": [
                        "te weinig",
                        "onvoldoende"
                    ],
                    "common_phrases": [
                        "meer dan genoeg",
                        "tijd genoeg",
                        "geld genoeg"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество, которое является достаточным; не слишком много и не слишком мало."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "достаточно",
                                "target_lang_sense_clarification": "о количестве"
                            },
                            {
                                "target_lang_word": "хватит",
                                "target_lang_sense_clarification": "разговорное, предикатив"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "a89986f4-44bb-37b5-9dc7-a2f83596a2ff",
                    "sense_definition": "Uitroep om aan te geven dat iets moet stoppen of dat de limiet bereikt is.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Genoeg</w> daarmee, ik wil er niets meer over horen!",
                            "target_lang_translations": {
                                "ru": "<w>Хватит</w> об этом, я больше ничего не хочу слышать!"
                            }
                        },
                        {
                            "text": "Nu is het <w>genoeg</w> geweest met dat lawaai.",
                            "target_lang_translations": {
                                "ru": "С этим шумом уже <w>хватит</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "voldoende",
                        "afdoende",
                        "zat"
                    ],
                    "common_phrases": [
                        "genoeg is genoeg",
                        "nu is het genoeg"
                    ],
                    "target_lang_definitions": {
                        "ru": "Восклицание, указывающее на то, что действие должно прекратиться или предел достигнут."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хватит",
                                "target_lang_sense_clarification": "приказ остановиться"
                            },
                            {
                                "target_lang_word": "довольно",
                                "target_lang_sense_clarification": "выражение предела"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6b0e3212-c969-38b7-8a9d-455eec37c0fa",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>genoegen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>genoeg</w> aan een klein beetje voedsel.",
                            "target_lang_translations": {
                                "ru": "Я <w>довольствуюсь</w> малым количеством еды."
                            }
                        },
                        {
                            "text": "Ik <w>genoeg</w> mij in de stilte van de natuur.",
                            "target_lang_translations": {
                                "ru": "Я <w>нахожу удовлетворение</w> в тишине природы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени глагола 'genoegen' (довольствоваться)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "довольствуюсь",
                                "target_lang_sense_clarification": "глагол"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8afe87d5-ac26-3be2-a160-71598f29b317",
                    "sense_definition": "Gebiedende wijs van <w>genoegen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Genoeg</w> u met wat u heeft gekregen.",
                            "target_lang_translations": {
                                "ru": "<w>Довольствуйтесь</w> тем, что вы получили."
                            }
                        },
                        {
                            "text": "<w>Genoeg</w> u in de eenvoud van het leven.",
                            "target_lang_translations": {
                                "ru": "<w>Находите удовлетворение</w> в простоте жизни."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола 'genoegen' (довольствоваться)."

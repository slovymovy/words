    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3cf5f250-4e0c-393d-ada0-052ff1ca0447",
                    "sense_definition": "Taki, który dąży do poszerzania swoich wpływów, terytorium lub rynków zbytu.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "growth_and_spread",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowa strategia firmy jest bardzo <w>ekspansywna</w> i zakłada wejście na rynki azjatyckie.",
                            "target_lang_translations": {
                                "ru": "Новая стратегия компании является весьма <w>экспансионистской</w> и предполагает выход на азиатские рынки."
                            }
                        },
                        {
                            "text": "Sąsiednie kraje obawiały się, że <w>ekspansywna</w> polityka mocarstwa doprowadzi do wojny.",
                            "target_lang_translations": {
                                "ru": "Соседние страны опасались, что <w>экспансионистская</w> политика державы приведет к войне."
                            }
                        }
                    ],
                    "synonyms": [
                        "ekspansjonistyczny",
                        "zaborczy"
                    ],
                    "common_phrases": [
                        "ekspansywna polityka",
                        "ekspansywna strategia",
                        "ekspansywne plany",
                        "ekspansywny rozwój"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стремящийся к расширению влияния, территории или рынков сбыта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экспансионистский",
                                "target_lang_sense_clarification": "связанный с политикой или стратегией расширения влияния"
                            },
                            {
                                "target_lang_word": "захватнический",
                                "target_lang_sense_clarification": "нацеленный на захват чужих территорий или ресурсов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90d6c106-6aca-3cf6-8331-dad14cc5e1b2",
                    "sense_definition": "Taki, który szybko się rozprzestrzenia, zwiększa swoją objętość lub zajmuje nowy obszar.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "growth_and_spread",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <w>ekspansywny</w> gatunek rośliny szybko wyparł rodzimą florę z naszego ogrodu.",
                            "target_lang_translations": {
                                "ru": "Этот <w>агрессивный</w> вид растений быстро вытеснил местную флору из нашего сада."
                            }
                        },
                        {
                            "text": "W budownictwie zastosowano cement <w>ekspansywny</w>, aby uszczelnić konstrukcję.",
                            "target_lang_translations": {
                                "ru": "В строительстве использовали <w>расширяющийся</w> цемент, чтобы загерметизировать конструкцию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ekspansywny gatunek",
                        "beton ekspansywny",
                        "gaz ekspansywny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро распространяющийся, увеличивающийся в объеме или занимающий новую территорию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расширяющийся",
                                "target_lang_sense_clarification": "увеличивающийся в объеме (например, о материалах)"
                            },
                            {
                                "target_lang_word": "агрессивный",
                                "target_lang_sense_clarification": "быстро захватывающий пространство (о растениях)"
                            },
                            {
                                "target_lang_word": "инвазивный",
                                "target_lang_sense_clarification": "биологический термин для чужеродных видов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f407ddd8-34d8-3183-b883-bbf4b978b83e",
                    "sense_definition": "Taki, który zachowuje się w sposób gwałtowny, wylewny i narzucający się innym swoją obecnością.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "personality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>ekspansywny</w> styl bycia sprawiał, że zawsze znajdował się w centrum uwagi.",
                            "target_lang_translations": {
                                "ru": "Его <w>экспансивная</w> манера поведения приводила к тому, что он всегда оказывался в центре внимания."
                            }
                        },
                        {
                            "text": "Była osobą niezwykle <w>ekspansywną</w>, która głośno wyrażała każdą swoją emocję.",
                            "target_lang_translations": {
                                "ru": "Она была чрезвычайно <w>экспансивной</w> личностью, громко выражавшей каждую свою эмоцию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ekspansywny styl bycia",
                        "ekspansywny charakter",
                        "zachowanie ekspansywne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проявляющий свои чувства бурно, несдержанно; навязывающий свое присутствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экспансивный",
                                "target_lang_sense_clarification": "бурно проявляющий эмоции, несдержанный"
                            },
                            {
                                "target_lang_word": "несдержанный",
                                "target_lang_sense_clarification": "не умеющий сдерживать свои порывы"
                            }
                        ]
                    }
                }
            ]
        }
    ]

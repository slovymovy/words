                    "frequency": "Middle",
                    "semantic_group_id": "legal_validity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Новый закон начнёт <действовать> с первого января следующего года.",
                            "target_lang_translations": {
                                "en": "The new law will start to <apply> from the first of January of next year."

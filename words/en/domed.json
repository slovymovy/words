                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "куполообразный",
                                "target_lang_sense_clarification": "напоминающий купол по форме"
                            },
                            {
                                "target_lang_word": "сводчатый",
                                "target_lang_sense_clarification": "имеющий форму свода"
                            },
                            {
                                "target_lang_word": "выпуклый",
                                "target_lang_sense_clarification": "округлый, выступающий наружу"
                            }
                        ]
                    }
                }
            ]
        },
        {

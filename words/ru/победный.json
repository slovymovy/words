    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c48b2f32-1310-34b9-9dea-dab55d61a8fb",
                    "sense_definition": "Заканчивающийся победой, приносящий успех в борьбе или соревновании.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "victory_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На последних минутах матча нападающий забил решающий <w>победный</w> гол.",
                            "target_lang_translations": {
                                "en": "In the final minutes of the match, the striker scored the decisive <w>winning</w> goal."
                            }
                        },
                        {
                            "text": "Мы будем бороться за свои права до <w>победного</w> конца.",
                            "target_lang_translations": {
                                "en": "We will fight for our rights to a <w>victorious</w> end."
                            }
                        }
                    ],
                    "synonyms": [
                        "победоносный"
                    ],
                    "common_phrases": [
                        "победный гол",
                        "до победного конца",
                        "победный счет",
                        "победный исход"
                    ],
                    "target_lang_definitions": {
                        "en": "Ending in victory or bringing success in a struggle or competition."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "winning",
                                "target_lang_sense_clarification": "used for the specific action or event that secures a victory (e.g., a goal)"
                            },
                            {
                                "target_lang_word": "victorious",
                                "target_lang_sense_clarification": "characterized by or resulting in victory"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d5ac600-36b4-3688-b49c-14408a91cc5a",
                    "sense_definition": "Выражающий торжество, радость и гордость по поводу достигнутого успеха.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "victory_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У него был такой <w>победный</w> вид, словно он уже выиграл главный приз.",
                            "target_lang_translations": {
                                "en": "He had such a <w>triumphant</w> look, as if he had already won the main prize."
                            }
                        },
                        {
                            "text": "С трибун раздался громкий <w>победный</w> клич болельщиков.",
                            "target_lang_translations": {
                                "en": "A loud <w>triumphant</w> cry from the fans rang out from the stands."
                            }
                        }
                    ],
                    "synonyms": [
                        "триумфальный"
                    ],
                    "common_phrases": [
                        "победный вид",
                        "победный клич",
                        "победный марш",
                        "победный взгляд"
                    ],
                    "target_lang_definitions": {
                        "en": "Expressing triumph, joy, and pride regarding achieved success."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "triumphant",
                                "target_lang_sense_clarification": "showing great joy or pride at a victory or success"
                            },
                            {
                                "target_lang_word": "triumphal",
                                "target_lang_sense_clarification": "serving to celebrate or commemorate a victory (e.g., a march or arch)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d52f8ae9-931a-37bc-a759-003af3007ac5",
                    "sense_definition": "Испытавший много бед и несчастий; горький, тяжелый (происходит от слова «беда»).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "trouble_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старуха часто жаловалась соседям на свою <w>победную</w> судьбу и одиночество.",
                            "target_lang_translations": {
                                "en": "The old woman often complained to neighbors about her <w>wretched</w> fate and loneliness."
                            }
                        },
                        {
                            "text": "Куда мне теперь преклонить свою <w>победную</w> голову?",
                            "target_lang_translations": {
                                "en": "Where can I now rest my <w>poor</w> head?"
                            }
                        }
                    ],
                    "synonyms": [
                        "горемычный",
                        "несчастный"
                    ],
                    "common_phrases": [
                        "победная голова",
                        "победная судьба"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "used in folk or dated literary contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Having experienced many troubles and misfortunes; bitter, heavy (derived from the word 'trouble')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wretched",
                                "target_lang_sense_clarification": "very unhappy, unfortunate, or full of misery (archaic usage)"
                            },
                            {
                                "target_lang_word": "poor",
                                "target_lang_sense_clarification": "deserving of pity; unfortunate"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "09f45393-7d44-3539-86a0-9ae0eb59a2c6",
                    "sense_definition": "Op de tweede plaats komend; volgend op het primaire of eerste stadium.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "secundair_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In België gaan leerlingen na de basisschool naar het <w>secundair</w> onderwijs.",
                            "target_lang_translations": {
                                "en": "In Belgium, students go to <w>secondary</w> education after primary school.",
                                "ru": "В Бельгии ученики после начальной школы переходят к <w>среднему</w> образованию."
                            }
                        },
                        {
                            "text": "De <w>secundaire</w> sector van de economie omvat de industrie en de bouw.",
                            "target_lang_translations": {
                                "en": "The <w>secondary</w> sector of the economy comprises industry and construction.",
                                "ru": "<w>Вторичный</w> сектор экономики включает в себя промышленность и строительство."
                            }
                        }
                    ],
                    "antonyms": [
                        "primair"
                    ],
                    "common_phrases": [
                        "secundair onderwijs",
                        "secundaire weg",
                        "secundaire sector"
                    ],
                    "target_lang_definitions": {
                        "en": "Coming in second place; following the primary or first stage.",
                        "ru": "Находящийся на втором месте; следующий за первичной или первой стадией."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "secondary",
                                "target_lang_sense_clarification": "general / education / economy"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вторичный",
                                "target_lang_sense_clarification": "общий смысл, стадии, секторы экономики"
                            },
                            {
                                "target_lang_word": "средний",
                                "target_lang_sense_clarification": "в контексте образования (средняя школа)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7dd3c322-7dee-3dc9-a4c1-d706026ef821",
                    "sense_definition": "Van minder belang; niet de hoofdzaak vormend.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "secundair_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kleur van de auto is voor mij van <w>secundair</w> belang, zolang hij maar goed rijdt.",
                            "target_lang_translations": {
                                "en": "The color of the car is of <w>secondary</w> importance to me, as long as it drives well.",
                                "ru": "Цвет машины для меня имеет <w>второстепенное</w> значение, лишь бы она хорошо ездила."
                            }
                        },
                        {
                            "text": "We moeten ons concentreren op de hoofdzaken en niet op <w>secundaire</w> details.",
                            "target_lang_translations": {
                                "en": "We must focus on the main issues and not on <w>secondary</w> details.",
                                "ru": "Мы должны сосредоточиться на главном, а не на <w>второстепенных</w> деталях."
                            }
                        }
                    ],
                    "synonyms": [
                        "secondair",
                        "secondair"
                    ],
                    "antonyms": [
                        "primair",
                        "hoofdzakelijk"
                    ],
                    "common_phrases": [
                        "van secundair belang",
                        "secundaire arbeidsvoorwaarden",
                        "secundaire effecten"
                    ],
                    "target_lang_definitions": {
                        "en": "Of lesser importance; not constituting the main issue.",
                        "ru": "Имеющий меньшее значение; не являющийся главным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "secondary",
                                "target_lang_sense_clarification": "importance"
                            },
                            {
                                "target_lang_word": "minor",
                                "target_lang_sense_clarification": "details / issues"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "второстепенный",
                                "target_lang_sense_clarification": "о значении, деталях или важности"
                            }
                        ]
                    }
                }
            ]
        },

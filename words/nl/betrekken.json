                    ],
                    "synonyms": [
                        "relateren",
                        "koppelen"
                    ],
                    "common_phrases": [
                        "op zichzelf betrekken",
                        "betrekken op"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устанавливать связь с чем-то другим; применять по отношению к кому-то."

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "67d9a22e-b024-3ce6-97e1-31b3f46fe69d",
                    "sense_definition": "An official document or mark in a passport that allows someone to enter, leave, or stay in a specific country for a certain period.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "travel_docs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She needs to apply for a tourist <w>visa</w> before her trip to China.",
                            "target_lang_translations": {
                                "ru": "Ей нужно подать заявление на туристическую <w>визу</w> перед поездкой в Китай."
                            }
                        },
                        {
                            "text": "The customs officer checked my passport to ensure I had a valid <w>visa</w>.",
                            "target_lang_translations": {
                                "ru": "Таможенник проверил мой паспорт, чтобы убедиться, что у меня есть действующая <w>виза</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "permit",
                        "endorsement"
                    ],
                    "common_phrases": [
                        "tourist visa",
                        "student visa",
                        "apply for a visa",
                        "entry visa",
                        "transit visa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальный документ или отметка в паспорте, разрешающая въезд, выезд или пребывание в определенной стране в течение определенного периода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "виза",
                                "target_lang_sense_clarification": "документ для въезда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35027106-f04b-386e-9ee5-b2ab5e88c9ea",
                    "sense_definition": "A credit card issued by the Visa company, often used to refer to the payment method itself.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "payment_methods",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The restaurant accepts cash, MasterCard, and <w>Visa</w>.",
                            "target_lang_translations": {
                                "ru": "Ресторан принимает наличные, MasterCard и <w>Visa</w>."
                            }
                        },
                        {
                            "text": "I paid for the groceries using my <w>Visa</w>.",
                            "target_lang_translations": {
                                "ru": "Я расплатился за продукты своей картой <w>Visa</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "credit card"
                    ],
                    "common_phrases": [
                        "pay by Visa",
                        "accept Visa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кредитная карта, выпущенная компанией Visa; часто используется для обозначения самого способа оплаты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "карта Visa",
                                "target_lang_sense_clarification": "банковская карта"
                            },
                            {
                                "target_lang_word": "Visa",
                                "target_lang_sense_clarification": "платежная система"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ab3e7a5-caa9-3808-b378-e7138372d3db",
                    "sense_definition": "An acronym for vancomycin-intermediate Staphylococcus aureus, a type of bacteria resistant to certain antibiotics.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doctors are concerned about the spread of <w>VISA</w> in hospitals.",
                            "target_lang_translations": {
                                "ru": "Врачи обеспокоены распространением <w>VISA</w> в больницах."
                            }
                        },
                        {
                            "text": "The patient was diagnosed with a <w>VISA</w> infection that required specialized treatment.",
                            "target_lang_translations": {
                                "ru": "У пациента была диагностирована инфекция <w>VISA</w>, требующая специализированного лечения."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура для золотистого стафилококка с промежуточной устойчивостью к ванкомицину (тип бактерий)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "VISA",
                                "target_lang_sense_clarification": "тип бактерий"
                            }

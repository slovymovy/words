                    "synonyms": [
                        "считать",
                        "вычислять",
                        "просчитывать"
                    ],
                    "common_phrases": [
                        "рассчитывать бюджет",
                        "рассчитывать время",
                        "рассчитывать стоимость",
                        "рассчитывать расстояние"
                    ],
                    "target_lang_definitions": {

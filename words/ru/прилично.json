    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "fcc34541-45d6-38a0-a801-2ce9a3346609",
                    "sense_definition": "Соответствуя принятым правилам поведения; пристойно, как подобает.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Гости вели себя очень <w>прилично</w> и вежливо.",
                            "target_lang_translations": {
                                "en": "The guests behaved very <w>decently</w> and politely."
                            }
                        },
                        {
                            "text": "На официальный приём нужно одеться <w>прилично</w>.",
                            "target_lang_translations": {
                                "en": "One needs to dress <w>decently</w> for an official reception."
                            }
                        }
                    ],
                    "synonyms": [
                        "пристойно"
                    ],
                    "antonyms": [
                        "неприлично",
                        "непристойно"
                    ],
                    "common_phrases": [
                        "вести себя прилично",
                        "выглядеть прилично"
                    ],
                    "target_lang_definitions": {
                        "en": "In a manner that conforms to accepted standards of behavior or propriety."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "decently",
                                "target_lang_sense_clarification": "in a respectable or moral way"
                            },
                            {
                                "target_lang_word": "properly",
                                "target_lang_sense_clarification": "correctly or in accordance with social rules"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db3d5415-8931-36db-b162-6d7f94ba7526",
                    "sense_definition": "Оценка ситуации или действий как соответствующих правилам приличия.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Опаздывать на важную встречу не очень <w>прилично</w>.",
                            "target_lang_translations": {
                                "en": "It is not very <w>polite</w> to be late for an important meeting."
                            }
                        },
                        {
                            "text": "Спрашивать у женщины о возрасте не всегда <w>прилично</w>.",
                            "target_lang_translations": {
                                "en": "It is not always <w>proper</w> to ask a woman about her age."
                            }
                        }
                    ],
                    "synonyms": [
                        "пристойно"
                    ],
                    "antonyms": [
                        "неприлично"
                    ],
                    "target_lang_definitions": {
                        "en": "Considered socially acceptable, polite, or appropriate."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "polite",
                                "target_lang_sense_clarification": "showing good manners"
                            },
                            {
                                "target_lang_word": "proper",
                                "target_lang_sense_clarification": "socially correct"
                            },
                            {
                                "target_lang_word": "decent",
                                "target_lang_sense_clarification": "conforming to standards of propriety"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b303083-28e5-3512-b60a-3c4acb64440b",
                    "sense_definition": "В значительном количестве или степени; довольно много.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он зарабатывает <w>прилично</w> и может позволить себе путешествовать.",
                            "target_lang_translations": {
                                "en": "He earns <w>decently</w> and can afford to travel."
                            }
                        },
                        {
                            "text": "Мы прошли уже <w>прилично</w>, пора сделать привал.",
                            "target_lang_translations": {
                                "en": "We have walked <w>quite a bit</w> already, it's time to take a break."
                            }
                        }
                    ],
                    "synonyms": [
                        "порядочно"
                    ],
                    "antonyms": [
                        "мало"
                    ],
                    "common_phrases": [
                        "зарабатывать прилично",
                        "стоит прилично"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A significant amount or to a considerable degree (colloquial)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "quite a bit",
                                "target_lang_sense_clarification": "a large amount or distance"
                            },
                            {
                                "target_lang_word": "pretty well",
                                "target_lang_sense_clarification": "to a good degree, often regarding money"
                            },
                            {
                                "target_lang_word": "decently",
                                "target_lang_sense_clarification": "sufficiently or generously"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a41521bc-79f9-39dd-bf44-0350796bf03f",
                    "sense_definition": "Краткая форма среднего рода прилагательного <w>приличный</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его поведение на вчерашнем вечере было вполне <w>прилично</w>.",
                            "target_lang_translations": {
                                "en": "His behavior at the party yesterday was quite <w>decent</w>."
                            }
                        },
                        {
                            "text": "Это пальто ещё <w>прилично</w>, его можно носить.",
                            "target_lang_translations": {
                                "en": "This coat is still <w>decent</w>, it can be worn."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Satisfactory, acceptable, or respectable in quality or condition."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "decent",
                                "target_lang_sense_clarification": "of acceptable quality or standard"
                            },
                            {

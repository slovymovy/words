                    "sense_id": "06e9d279-3ba8-3f1e-94aa-5e08dc171317",
                    "sense_definition": "duży kontynent położony na południe od Europy, oddzielony od niej Morzem Śródziemnym",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "geography_continent",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Sahara znajduje się w północnej <w>Afryce</w>.",
                            "target_lang_translations": {
                                "ru": "Сахара находится в Северной <w>Африке</w>."
                            }
                        },
                        {
                            "text": "Wiele dzikich zwierząt, takich jak lwy i słonie, żyje w <w>Afryce</w>.",
                            "target_lang_translations": {
                                "ru": "Многие дикие животные, такие как львы и слоны, живут в <w>Африке</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Czarny Ląd",
                        "Czarny Kontynent"
                    ],
                    "common_phrases": [

            "senses": [
                {
                    "sense_id": "07775286-c338-3977-bce2-f70347510897",
                    "sense_definition": "The quality or state of being definite, fixed, or clearly determined.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>determinacy</w> of the legal contract prevented any ambiguity regarding the deadline.",
                            "target_lang_translations": {
                                "ru": "<w>Определённость</w> юридического договора предотвратила любую двусмысленность в отношении крайнего срока."
                            }
                        },
                        {
                            "text": "Philosophers often debate the <w>determinacy</w> of meaning in ancient texts.",
                            "target_lang_translations": {
                                "ru": "Философы часто спорят об <w>определённости</w> смысла в древних текстах."
                            }
                        }
                    ],
                    "synonyms": [
                        "determinateness"
                    ],
                    "antonyms": [
                        "indeterminacy"
                    ],
                    "common_phrases": [
                        "determinacy of meaning",
                        "semantic determinacy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество или состояние быть определённым, фиксированным или чётко установленным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "определённость",
                                "target_lang_sense_clarification": "свойство быть точным и ясным"
                            },
                            {
                                "target_lang_word": "детерминированность",
                                "target_lang_sense_clarification": "обусловленность, предопределённость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6666184b-f2bb-3e57-ae6b-c96724e1cb34",
                    "sense_definition": "The condition in game theory where a game possesses a winning strategy for one of the players.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mathematician presented a proof involving the axiom of <w>determinacy</w>.",
                            "target_lang_translations": {
                                "ru": "Математик представил доказательство, включающее аксиому <w>детерминированности</w>."
                            }
                        },
                        {
                            "text": "In the study of infinite games, <w>determinacy</w> ensures that the outcome is not random.",
                            "target_lang_translations": {
                                "ru": "В изучении бесконечных игр <w>детерминированность</w> гарантирует, что исход не является случайным."
                            }
                        }
                    ],
                    "common_phrases": [
                        "axiom of determinacy",
                        "projective determinacy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние в теории игр, когда для одного из игроков существует выигрышная стратегия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "детерминированность",
                                "target_lang_sense_clarification": "математический термин (теория игр)"
                            }
                        ]
                    }
                }
            ]
        }

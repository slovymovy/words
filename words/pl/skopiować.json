            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f7a24f6b-aa46-356a-8d1a-490335dbe48b",
                    "sense_definition": "Utworzyć duplikat danych cyfrowych w pamięci komputera lub przenieść je na inny nośnik.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "reproduction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pamiętaj, aby regularnie <skopiować> swoje najważniejsze pliki na dysk zewnętrzny.",
                            "target_lang_translations": {
                                "ru": "Не забывай регулярно <копировать> свои самые важные файлы на внешний диск."
                            }
                        },
                        {
                            "text": "Możesz zaznaczyć ten fragment tekstu i <skopiować> go do schowka skrótem klawiszowym.",
                            "target_lang_translations": {
                                "ru": "Ты можешь выделить этот фрагмент текста и <скопировать> его в буфер обмена с помощью горячих клавиш."
                            }
                        }
                    ],
                    "synonyms": [
                        "zgrać"
                    ],
                    "antonyms": [
                        "wyciąć",
                        "usunąć"
                    ],
                    "common_phrases": [
                        "skopiować plik",
                        "skopiować dane",
                        "skopiować na dysk",
                        "skopiować i wkleić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создать цифровую копию данных в памяти компьютера или перенести их на другой носитель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скопировать",
                                "target_lang_sense_clarification": "файлы, данные, текст"

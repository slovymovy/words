    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "76dcacb6-e0d1-3e17-8595-7f7d160cb871",
                    "sense_definition": "Uderzyć w cel lub dotknąć wyznaczonego miejsca rzuconym przedmiotem, pociskiem lub częścią ciała.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_contact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rzucił kamieniem, ale nie <trafił> w okno.",
                            "target_lang_translations": {
                                "ru": "Он бросил камень, но не <попал> в окно."
                            }
                        },
                        {
                            "text": "Łucznik wycelował dokładnie i <trafił> w sam środek tarczy.",
                            "target_lang_translations": {
                                "ru": "Лучник точно прицелился и <попал> в самый центр мишени."
                            }
                        }
                    ],
                    "antonyms": [
                        "chybić",
                        "spudłować"
                    ],
                    "common_phrases": [
                        "trafić w cel",
                        "trafić w sedno",
                        "trafić piłką"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поразить цель или коснуться определенного места брошенным предметом, снарядом или частью тела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "попасть",
                                "target_lang_sense_clarification": "в цель"
                            },
                            {
                                "target_lang_word": "угодить",
                                "target_lang_sense_clarification": "попасть (часто случайно или неловко)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d05b6ecc-cd04-360e-bf10-2e6c3aede4a9",
                    "sense_definition": "Znaleźć właściwą drogę i przybyć do określonego miejsca.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bez mapy w telefonie nigdy bym tu nie <trafił>.",
                            "target_lang_translations": {
                                "ru": "Без карты в телефоне я бы никогда сюда не <попал>."
                            }
                        },
                        {
                            "text": "Czy wiesz, jak <trafić> na dworzec kolejowy?",
                            "target_lang_translations": {
                                "ru": "Ты знаешь, как <попасть> на вокзал?"
                            }
                        }
                    ],
                    "synonyms": [
                        "dotrzeć",
                        "dojść",
                        "przybyć"
                    ],
                    "common_phrases": [
                        "trafić do domu",
                        "trafić pod adres",
                        "trafić na miejsce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Найти правильную дорогу и прибыть в определенное место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "попасть",
                                "target_lang_sense_clarification": "добраться до места"
                            },
                            {
                                "target_lang_word": "добраться",
                                "target_lang_sense_clarification": "доехать, дойти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b87178de-0eb1-3083-a33e-2a6b22a17f3c",
                    "sense_definition": "Spotkać kogoś lub znaleźć coś przypadkiem.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "chance_encounter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sklepie <trafiłam> na świetną promocję cenową.",
                            "target_lang_translations": {
                                "ru": "В магазине я <попала> на отличную скидку."
                            }
                        },
                        {
                            "text": "Szukaliśmy grzybów, ale <trafiliśmy> tylko na stare puszki.",
                            "target_lang_translations": {
                                "ru": "Мы искали грибы, но <наткнулись> только на старые банки."
                            }
                        }
                    ],
                    "synonyms": [
                        "dojść",
                        "dotrzeć"
                    ],
                    "common_phrases": [
                        "trafić na okazję",
                        "trafić na kogoś",
                        "dobrze trafić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Встретить кого-то или найти что-то случайно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наткнуться",
                                "target_lang_sense_clarification": "случайно встретить или найти"
                            },
                            {
                                "target_lang_word": "попасть",
                                "target_lang_sense_clarification": "на что-то (напр. на акцию)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20512b43-bce7-328c-93b4-fbca81c69423",
                    "sense_definition": "Znaleźć się w jakimś miejscu lub sytuacji, często wbrew woli lub na skutek splotu wydarzeń.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wypadku ranny kierowca natychmiast <trafił> do szpitala.",
                            "target_lang_translations": {
                                "ru": "После аварии раненый водитель сразу <попал> в больницу."
                            }
                        },
                        {
                            "text": "Jeśli będziesz kradł, w końcu <trafisz> do więzienia.",
                            "target_lang_translations": {
                                "ru": "Если будешь воровать, в конце концов <попадешь> в тюрьму."
                            }
                        }
                    ],
                    "common_phrases": [
                        "trafić do szpitala",
                        "trafić do więzienia",
                        "trafić w ręce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказаться в каком-либо месте или ситуации, часто против воли или в результате стечения обстоятельств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "попасть",
                                "target_lang_sense_clarification": "оказаться (в больнице, в тюрьме)"
                            },
                            {
                                "target_lang_word": "угодить",
                                "target_lang_sense_clarification": "разг. попасть в неприятную ситуацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c5e4708-3e65-35de-9741-3ce0b4b5f8ad",
                    "sense_definition": "Dokonać właściwego wyboru, odgadnąć coś lub wygrać w grze losowej.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "chance_encounter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad <trafił> szóstkę w totka i kupił nowy dom.",
                            "target_lang_translations": {
                                "ru": "Мой сосед <выиграл> в лотерею и купил новый дом."
                            }
                        },
                        {
                            "text": "Idealnie <trafiłaś> z tym prezentem, właśnie o tym marzyłem.",
                            "target_lang_translations": {
                                "ru": "Ты идеально <угадала> с этим подарком, я как раз о нем мечтал."
                            }
                        }
                    ],
                    "antonyms": [
                        "chybić",
                        "spudłować"
                    ],
                    "common_phrases": [
                        "trafić szóstkę",
                        "trafić z prezentem",
                        "trafić z wyborem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать правильный выбор, угадать что-то или выиграть в азартной игре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угадать",
                                "target_lang_sense_clarification": "сделать верный выбор"
                            },
                            {
                                "target_lang_word": "выиграть",
                                "target_lang_sense_clarification": "в лотерею"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a68d27cc-2b85-3d5b-ba19-6b83084ec2a2",
                    "sense_definition": "Zdarzyć się lub pojawić niespodziewanie (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "occurrence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Taka okazja nie <trafi> się nam drugi raz w życiu.",
                            "target_lang_translations": {
                                "ru": "Такая возможность не <подвернется> нам второй раз в жизни."
                            }
                        },
                        {
                            "text": "W tej książce <trafiło> się kilka błędów ortograficznych.",
                            "target_lang_translations": {
                                "ru": "В этой книге <попалось> несколько орфографических ошибок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "trafić się okazja",
                        "trafić się gratka"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "reflexive with 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Случиться или появиться неожиданно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "попасться",
                                "target_lang_sense_clarification": "встретиться"
                            },
                            {
                                "target_lang_word": "подвернуться",
                                "target_lang_sense_clarification": "о случае или возможности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d8fc2e4-5bb9-3a3c-9499-b2e1f5e7e7a4",
                    "sense_definition": "Przekonać kogoś, wpłynąć na czyjeś uczucia lub zrozumienie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "mental_influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego argumenty w końcu <trafiły> do zebranych słuchaczy.",
                            "target_lang_translations": {
                                "ru": "Его аргументы наконец <дошли> до собравшихся слушателей."
                            }
                        },
                        {
                            "text": "Trudno <trafić> do kogoś, kto jest tak uparty.",
                            "target_lang_translations": {
                                "ru": "Трудно <достучаться> до того, кто так упрям."
                            }
                        }
                    ],
                    "synonyms": [
                        "dotrzeć"
                    ],
                    "common_phrases": [
                        "trafić do przekonania",
                        "trafić do rozsądku",
                        "trafić do serca"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in phrases regarding convincing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Убедить кого-то, повлиять на чьи-то чувства или понимание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "достучаться",
                                "target_lang_sense_clarification": "перен. убедить, быть услышанным"
                            },
                            {
                                "target_lang_word": "дойти",
                                "target_lang_sense_clarification": "стать понятным"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9d8a2aac-1c18-3a86-a4fc-8e4af6f2cd91",
                    "sense_definition": "Met de vorm van een vierkant; met vier gelijke zijden en rechte hoeken.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "shape_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn studentenkamer is twaalf <w>vierkante</w> meter groot.",
                            "target_lang_translations": {
                                "en": "My student room is twelve <w>square</w> meters.",
                                "ru": "Моя студенческая комната — двенадцать <w>квадратных</w> метров."
                            }
                        },
                        {
                            "text": "We hebben een nieuwe <w>vierkante</w> tafel voor in de keuken gekocht.",
                            "target_lang_translations": {
                                "en": "We bought a new <w>square</w> table for the kitchen.",
                                "ru": "Мы купили новый <w>квадратный</w> стол на кухню."
                            }
                        }
                    ],
                    "antonyms": [
                        "rond"
                    ],
                    "common_phrases": [
                        "vierkante meter",
                        "vierkante kilometer"
                    ],
                    "target_lang_definitions": {
                        "en": "Having the shape of a square; with four equal sides and right angles.",
                        "ru": "Имеющий форму квадрата; с четырьмя равными сторонами и прямыми углами."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "square",
                                "target_lang_sense_clarification": "shape or measurement"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "квадратный",
                                "target_lang_sense_clarification": "о форме или измерении площади"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2247812f-9a62-3c6e-ad76-0e6751b60d11",
                    "sense_definition": "Een geometrische figuur met vier gelijke zijden en vier hoeken van 90 graden.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "shape_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Teken een <w>vierkant</w> en kleur het rood in.",

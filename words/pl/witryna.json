{
    "word_family": [
        "witrynka",
        "witrynowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "19facf42-cb6c-3c9a-a5d3-f1e413d2a2d1",
                    "sense_definition": "Duże okno sklepowe przeznaczone do eksponowania towarów, aby zachęcić klientów do wejścia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "shop_display",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W <witrynie> tego butiku widziałam piękną czerwoną sukienkę.",
                            "target_lang_translations": {
                                "ru": "В <витрине> этого бутика я видела красивое красное платье."
                            }
                        },
                        {
                            "text": "Pracownicy sklepu myli <witrynę>, aby lepiej było widać nowe produkty.",
                            "target_lang_translations": {
                                "ru": "Работники магазина мыли <витрину>, чтобы лучше было видно новые товары."
                            }
                        }
                    ],
                    "synonyms": [
                        "wystawa"
                    ],
                    "common_phrases": [
                        "witryna sklepowa",
                        "oglądać witryny",
                        "aranżacja witryny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое окно магазина, предназначенное для демонстрации товаров с целью привлечения покупателей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "витрина",
                                "target_lang_sense_clarification": "окно магазина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd54030e-9b2f-35d9-8c83-76f0ff7a7712",
                    "sense_definition": "Zbiór powiązanych ze sobą stron w internecie, dostępnych pod jednym adresem, często o charakterze firmowym lub informacyjnym.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "internet",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza nowa <witryna> internetowa jest bardzo nowoczesna i łatwa w obsłudze.",
                            "target_lang_translations": {
                                "ru": "Наш новый веб-<сайт> очень современный и простой в использовании."
                            }
                        },
                        {
                            "text": "Musisz odświeżyć <witrynę>, aby zobaczyć wprowadzone zmiany.",
                            "target_lang_translations": {
                                "ru": "Ты должен обновить <сайт>, чтобы увидеть внесенные изменения."
                            }
                        }
                    ],
                    "synonyms": [
                        "serwis"
                    ],
                    "common_phrases": [
                        "witryna internetowa",
                        "projektowanie witryn",
                        "adres witryny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность связанных веб-страниц в интернете, доступных по одному адресу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сайт",
                                "target_lang_sense_clarification": "в интернете"
                            },
                            {
                                "target_lang_word": "веб-сайт",
                                "target_lang_sense_clarification": "полная форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cfbf0cb0-4127-36bd-829e-090d0898b147",
                    "sense_definition": "Przeszklona szafka lub mebel służący do przechowywania i pokazywania ozdobnych przedmiotów, takich jak porcelana czy szkło.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "furniture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Babcia trzymała swoją najlepszą zastawę stołową w dębowej <witrynie>.",
                            "target_lang_translations": {
                                "ru": "Бабушка хранила свой лучший столовый сервиз в дубовом <серванте>."
                            }
                        },
                        {
                            "text": "W muzeum oglądaliśmy starożytne monety wyeksponowane w podświetlanej <witrynie>.",
                            "target_lang_translations": {
                                "ru": "В музее мы рассматривали древние монеты, выставленные в подсвеченной <витрине>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gablota",
                        "witrynka"
                    ],
                    "common_phrases": [
                        "szklana witryna",
                        "stara witryna",
                        "witryna na książki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Застекленный шкаф или предмет мебели для хранения и показа декоративных предметов, таких как фарфор или стекло."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сервант",
                                "target_lang_sense_clarification": "шкаф для посуды"
                            },
                            {
                                "target_lang_word": "витрина",
                                "target_lang_sense_clarification": "шкаф со стеклом"
                            },
                            {
                                "target_lang_word": "горка",
                                "target_lang_sense_clarification": "вид мебели"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
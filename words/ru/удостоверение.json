    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fc0989f1-8093-3244-a7b4-7281039d0043",
                    "sense_definition": "Официальный документ, подтверждающий личность владельца, его должностной статус или определённые права.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "document_proof",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полицейский попросил водителя предъявить водительское <w>удостоверение</w>.",
                            "target_lang_translations": {
                                "en": "The police officer asked the driver to show their driver's <w>license</w>."
                            }
                        },
                        {
                            "text": "Для входа в офисное здание необходимо показать охраннику служебное <w>удостоверение</w>.",
                            "target_lang_translations": {
                                "en": "To enter the office building, it is necessary to show the security guard a service <w>ID</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "проверка",
                        "подтверждение"
                    ],
                    "common_phrases": [
                        "водительское удостоверение",
                        "служебное удостоверение",
                        "пенсионное удостоверение",
                        "предъявить удостоверение",
                        "выдать удостоверение"
                    ],
                    "target_lang_definitions": {
                        "en": "An official document confirming the holder's identity, official status, or specific rights."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ID card",
                                "target_lang_sense_clarification": "document proving identity"
                            },
                            {
                                "target_lang_word": "license",
                                "target_lang_sense_clarification": "permit for driving or specific activities"
                            },
                            {
                                "target_lang_word": "certificate",
                                "target_lang_sense_clarification": "official document of status"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3847c6a1-7e6c-30f0-9dfe-9cdc2f7a5326",
                    "sense_definition": "Цифровой файл или код, используемый в компьютерных системах для подтверждения подлинности пользователя или безопасности соединения.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "document_proof",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Браузер предупредил, что цифровое <w>удостоверение</w> сайта истекло.",
                            "target_lang_translations": {
                                "en": "The browser warned that the site's digital <w>certificate</w> had expired."
                            }
                        },
                        {
                            "text": "Вам нужно установить новое <w>удостоверение</w> безопасности для доступа к корпоративной сети.",
                            "target_lang_translations": {
                                "en": "You need to install a new security <w>certificate</w> to access the corporate network."
                            }
                        }
                    ],
                    "synonyms": [
                        "подтверждение"
                    ],
                    "common_phrases": [
                        "цифровое удостоверение",
                        "удостоверение безопасности"
                    ],
                    "target_lang_definitions": {
                        "en": "A digital file or code used in computer systems to authenticate a user or secure a connection."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "certificate",
                                "target_lang_sense_clarification": "digital security file"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2599768c-6561-3228-b68c-de758c085b44",
                    "sense_definition": "Официальная процедура или действие по подтверждению правильности, подлинности или истинности чего-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Нотариус произвёл <w>удостоверение</w> подписи на договоре купли-продажи.",
                            "target_lang_translations": {
                                "en": "The notary performed the <w>certification</w> of the signature on the sales contract."
                            }
                        },
                        {
                            "text": "В банке потребовалось дополнительное <w>удостоверение</w> личности клиента перед выдачей крупной суммы.",
                            "target_lang_translations": {
                                "en": "The bank required additional identity <w>verification</w> of the client before issuing a large sum."
                            }
                        }
                    ],
                    "synonyms": [
                        "подтверждение",
                        "проверка"
                    ],
                    "antonyms": [
                        "опровержение"
                    ],
                    "common_phrases": [
                        "удостоверение личности",
                        "нотариальное удостоверение",
                        "удостоверение сделки"
                    ],
                    "target_lang_definitions": {
                        "en": "An official procedure or action of confirming the correctness, authenticity, or truth of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "certification",
                                "target_lang_sense_clarification": "act of certifying"
                            },
                            {
                                "target_lang_word": "attestation",
                                "target_lang_sense_clarification": "formal confirmation"
                            },
                            {
                                "target_lang_word": "verification",
                                "target_lang_sense_clarification": "checking validity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3bc0cc4-0332-39fb-9cdf-afb486c88c42",
                    "sense_definition": "Доказательство или свидетельство чего-либо (устаревшее значение).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В <w>удостоверение</w> своей искренности он приложил руку к сердцу.",
                            "target_lang_translations": {
                                "en": "In <w>confirmation</w> of his sincerity, he placed his hand on his heart."
                            }
                        },
                        {
                            "text": "Эти слова послужили мне в <w>удостоверение</w> того, что решение было правильным.",
                            "target_lang_translations": {
                                "en": "These words served as <w>confirmation</w> to me that the decision was correct."
                            }
                        }
                    ],
                    "synonyms": [
                        "подтверждение",
                        "подтверждение"
                    ],
                    "antonyms": [
                        "опровержение"
                    ],
                    "common_phrases": [

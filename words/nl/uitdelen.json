{
    "word_family": [
        "uitdeling",
        "uitdeler"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "378871e2-9bf1-3ad9-a967-223c227f53cf",
                    "sense_definition": "Iets aan meerdere mensen geven zodat iedereen een deel of exemplaar ontvangt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_giving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar <deelde> de toetsen <uit> aan de leerlingen.",
                            "target_lang_translations": {
                                "en": "The teacher <handed out> the tests to the students.",
                                "ru": "Учитель <раздал> ученикам контрольные работы."
                            }
                        },
                        {
                            "text": "Vrijwilligers <delen> soep <uit> aan de mensen op straat.",
                            "target_lang_translations": {
                                "en": "Volunteers <distribute> soup to people on the street.",
                                "ru": "Волонтеры <раздают> суп людям на улице."
                            }
                        }
                    ],
                    "synonyms": [
                        "verspreiden",
                        "distribueren",
                        "ronddelen"
                    ],
                    "antonyms": [
                        "inzamelen",
                        "ophalen"
                    ],
                    "common_phrases": [
                        "folders uitdelen",
                        "kaarten uitdelen",
                        "eten uitdelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To give something to multiple people so that everyone receives a part or copy.",
                        "ru": "Раздавать что-либо нескольким людям, чтобы каждый получил часть или экземпляр."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "distribute",
                                "target_lang_sense_clarification": "formal or organized giving"
                            },
                            {
                                "target_lang_word": "hand out",
                                "target_lang_sense_clarification": "general, giving by hand"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "раздавать",
                                "target_lang_sense_clarification": "распределять предметы"
                            },
                            {
                                "target_lang_word": "распределять",
                                "target_lang_sense_clarification": "делить между людьми"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28a90d5e-ae3f-3080-893b-8bcc1b7d7051",
                    "sense_definition": "Iets lekkers of leuks aan anderen geven, vaak om iets te vieren.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_giving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op zijn verjaardag mocht hij snoep <uitdelen> in de klas.",
                            "target_lang_translations": {
                                "en": "On his birthday, he was allowed to <hand out> candy in class.",
                                "ru": "В свой день рождения ему разрешили <раздать> конфеты в классе."
                            }
                        },
                        {
                            "text": "Mijn collega <deelde> gebak <uit> omdat ze promotie had gemaakt.",
                            "target_lang_translations": {
                                "en": "My colleague <handed out> cake because she had been promoted.",
                                "ru": "Моя коллега <раздала> пирожные, так как получила повышение."
                            }
                        }
                    ],
                    "synonyms": [
                        "trakteren"
                    ],
                    "common_phrases": [
                        "snoep uitdelen",
                        "cadeautjes uitdelen",
                        "taart uitdelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To give something tasty or fun to others, often to celebrate something.",
                        "ru": "Давать другим что-то вкусное или приятное, часто по случаю праздника."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hand out",
                                "target_lang_sense_clarification": "giving treats or gifts"
                            },
                            {
                                "target_lang_word": "share",
                                "target_lang_sense_clarification": "dividing something among others"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "раздавать",
                                "target_lang_sense_clarification": "угощение"
                            },
                            {
                                "target_lang_word": "угощать",
                                "target_lang_sense_clarification": "делиться едой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afe193d2-f871-3427-9060-5eecf56f368d",
                    "sense_definition": "Een straf opleggen of een fysieke klap geven aan iemand.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_administering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De scheidsrechter <deelde> direct een gele kaart <uit> na de overtreding.",
                            "target_lang_translations": {
                                "en": "The referee immediately <handed out> a yellow card after the foul.",
                                "ru": "Судья сразу же <показал> желтую карточку после нарушения."
                            }
                        },
                        {
                            "text": "Tijdens de ruzie <deelde> hij een harde klap <uit>.",
                            "target_lang_translations": {
                                "en": "During the argument, he <dealt> a hard blow.",
                                "ru": "Во время ссоры он <нанес> сильный удар."
                            }
                        }
                    ],
                    "synonyms": [
                        "toedienen",
                        "geven",
                        "opleggen"
                    ],
                    "antonyms": [
                        "incasseren",
                        "ontvangen"
                    ],
                    "common_phrases": [
                        "een klap uitdelen",
                        "een boete uitdelen",
                        "straf uitdelen"
                    ],
                    "target_lang_definitions": {
                        "en": "To impose a punishment or deliver a physical blow to someone.",
                        "ru": "Назначать наказание или наносить физический удар кому-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deal",
                                "target_lang_sense_clarification": "to deliver a blow or strike"
                            },
                            {
                                "target_lang_word": "hand out",
                                "target_lang_sense_clarification": "to impose a penalty or fine"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "наносить",
                                "target_lang_sense_clarification": "удар"
                            },
                            {
                                "target_lang_word": "выдавать",
                                "target_lang_sense_clarification": "наказание или предупреждение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
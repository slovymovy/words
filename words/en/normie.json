                {
                    "sense_id": "285358c0-fb6f-32e3-8e2a-8d929106a792",
                    "sense_definition": "A person with conventional tastes, interests, or beliefs; an average or ordinary person.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "conventional_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He felt like a <w>normie</w> compared to the hardcore gamers at the convention.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал себя <w>нормисом</w> по сравнению с хардкорными геймерами на конвенте."
                            }
                        },
                        {
                            "text": "I just want a quiet, <w>normie</w> life without all this unnecessary drama.",
                            "target_lang_translations": {
                                "ru": "Я просто хочу тихой, <w>обывательской</w> жизни без всей этой ненужной драмы."
                            }
                        }
                    ],
                    "synonyms": [
                        "normal",
                        "square",
                        "mainstreamer"
                    ],
                    "common_phrases": [
                        "normie culture",
                        "total normie"
                    ],

                {
                    "sense_id": "e2485307-753b-32c5-84cc-4eb36ca58f30",
                    "sense_definition": "Een blokje stroef materiaal dat op de punt van een biljartkeu wordt aangebracht om glijden te voorkomen.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De speler doet wat <w>krijt</w> op zijn keu voor de stoot.",
                            "target_lang_translations": {
                                "en": "The player puts some <w>chalk</w> on his cue before the shot.",
                                "ru": "Игрок наносит немного <w>мела</w> на кий перед ударом."
                            }
                        },
                        {
                            "text": "Zonder <w>krijt</w> glijdt de keu makkelijk van de bal af.",
                            "target_lang_translations": {
                                "en": "Without <w>chalk</w>, the cue easily slips off the ball.",
                                "ru": "Без <w>мела</w> кий легко соскальзывает с шара."
                            }
                        }
                    ],
                    "target_lang_definitions": {

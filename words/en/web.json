    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "bb866b95-3c4a-33db-9194-1b837aa68410",
                    "sense_definition": "The World Wide Web; the system of connected documents and resources accessible via the Internet.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_network",
                    "name_type": "technology_or_software_name",
                    "examples": [
                        {
                            "text": "You can find almost any information you need on the <w>Web</w>.",
                            "target_lang_translations": {
                                "ru": "Вы можете найти практически любую необходимую информацию в <w>Сети</w>."
                            }
                        },
                        {
                            "text": "Many businesses now sell their products primarily through the <w>Web</w>.",
                            "target_lang_translations": {
                                "ru": "Многие компании сейчас продают свои товары преимущественно через <w>Интернет</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "web and pin"
                    ],
                    "common_phrases": [
                        "surf the Web",
                        "on the Web",
                        "browse the Web"
                    ],
                    "target_lang_definitions": {
                        "ru": "Всемирная паутина; система связанных документов и ресурсов, доступных через Интернет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "интернет",
                                "target_lang_sense_clarification": "глобальная сеть"
                            },
                            {
                                "target_lang_word": "сеть",
                                "target_lang_sense_clarification": "разговорное название интернета"
                            },
                            {
                                "target_lang_word": "всемирная паутина",
                                "target_lang_sense_clarification": "полное название"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "49e221c1-a2cf-3cd2-99b5-b78f93eb7b39",
                    "sense_definition": "A net-like structure made of sticky silk threads spun by a spider to catch prey.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_network",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The morning dew made the <w>web</w> sparkle in the sunlight.",
                            "target_lang_translations": {
                                "ru": "Утренняя роса заставила <w>паутину</w> сверкать на солнце."
                            }
                        },
                        {
                            "text": "We walked right into a sticky <w>web</w> hanging between the trees.",
                            "target_lang_translations": {
                                "ru": "Мы угодили прямо в липкую <w>паутину</w>, висящую между деревьями."
                            }
                        }
                    ],
                    "synonyms": [
                        "pin and web"
                    ],
                    "common_phrases": [
                        "spin a web",
                        "spider's web",
                        "caught in a web"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сетчатая структура из липких шелковых нитей, сплетенная пауком для ловли добычи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паутина",
                                "target_lang_sense_clarification": "сеть паука"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6c2aa09-0417-3bf0-a149-c68825b15270",
                    "sense_definition": "A complex system of interconnected elements, people, or things that resembles a spider's web.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_network",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police uncovered a complex <w>web</w> of criminal activity involving several countries.",
                            "target_lang_translations": {
                                "ru": "Полиция раскрыла сложную <w>сеть</w> преступной деятельности, охватывающую несколько стран."
                            }
                        },
                        {
                            "text": "She found herself trapped in a <w>web</w> of conflicting responsibilities.",
                            "target_lang_translations": {
                                "ru": "Она оказалась в ловушке <w>паутины</w> противоречивых обязанностей."
                            }
                        }
                    ],
                    "synonyms": [
                        "yarn"
                    ],
                    "common_phrases": [
                        "web of lies",
                        "web of deceit",
                        "tangled web"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сложная система взаимосвязанных элементов, людей или вещей, напоминающая паутину."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паутина",
                                "target_lang_sense_clarification": "перен. сложное сплетение лжи, интриг"
                            },
                            {
                                "target_lang_word": "сеть",
                                "target_lang_sense_clarification": "совокупность связей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24c05193-a7a5-3deb-9b7b-603562aeb49f",
                    "sense_definition": "A membrane or fold of skin connecting the toes of certain aquatic birds or animals.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The frog uses the <w>web</w> between its toes to swim efficiently.",
                            "target_lang_translations": {
                                "ru": "Лягушка использует <w>перепонку</w> между пальцами для эффективного плавания."
                            }
                        },
                        {
                            "text": "Ducks have a thick <w>web</w> of skin on their feet to help them paddle.",
                            "target_lang_translations": {
                                "ru": "У уток на лапах есть толстая кожная <w>перепонка</w>, помогающая им грести."
                            }
                        }
                    ],
                    "common_phrases": [
                        "duck's web"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мембрана или складка кожи, соединяющая пальцы некоторых водоплавающих птиц или животных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перепонка",
                                "target_lang_sense_clarification": "анатомическая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9a13d67-730b-3d78-bd67-b75de4a3896b",
                    "sense_definition": "A continuous long sheet of paper fed from a roll into a printing press.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The newspaper is printed on a continuous <w>web</w> of paper moving at high speed.",
                            "target_lang_translations": {
                                "ru": "Газета печатается на непрерывном бумажном <w>полотне</w>, движущемся с высокой скоростью."
                            }
                        },
                        {
                            "text": "If the <w>web</w> breaks during the printing process, the machine must be stopped immediately.",
                            "target_lang_translations": {
                                "ru": "Если <w>полотно</w> рвется во время печати, машину нужно немедленно остановить."
                            }
                        }
                    ],
                    "common_phrases": [
                        "web press",
                        "web printing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непрерывный длинный лист бумаги, подаваемый с рулона в печатную машину."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полотно",
                                "target_lang_sense_clarification": "бумажное, в полиграфии"
                            },
                            {
                                "target_lang_word": "лента",
                                "target_lang_sense_clarification": "бумажная"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9691af70-3cdc-38f2-a19e-ba91dac83191",
                    "sense_definition": "To cover with a network or to entangle as if in a web.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_network",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old window was <w>webbed</w> with tiny cracks after the impact.",
                            "target_lang_translations": {
                                "ru": "После удара старое окно <w>покрылось сетью</w> мелких трещин."
                            }
                        },
                        {
                            "text": "The spider <w>webbed</w> its prey tightly before retreating to the corner.",
                            "target_lang_translations": {
                                "ru": "Паук плотно <w>опутал паутиной</w> свою добычу, прежде чем отступить в угол."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать сетью или запутывать, словно паутиной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покрывать сетью",
                                "target_lang_sense_clarification": "трещин, морщин"
                            },
                            {
                                "target_lang_word": "опутывать",
                                "target_lang_sense_clarification": "паутиной"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
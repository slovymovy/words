                            "trait_type": "form",
                            "comment": "imperative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола <w>geloven</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поверь",
                                "target_lang_sense_clarification": "совершенный вид"
                            },
                            {
                                "target_lang_word": "верь",
                                "target_lang_sense_clarification": "несовершенный вид"
                            }
                        ]

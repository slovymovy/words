    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0070d4e1-05a2-341b-a49a-249b3f306711",
                    "sense_definition": "The fibrous barb or shaft of a feather, particularly from a peacock or ostrich, used to create artificial flies for fishing.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "fishing_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The angler carefully wrapped the peacock <w>herl</w> around the hook to form the body of the fly.",
                            "target_lang_translations": {
                                "ru": "Рыболов осторожно обмотал павлинью <w>бородку</w> вокруг крючка, чтобы сформировать тело мушки."
                            }
                        },
                        {
                            "text": "He prefers using ostrich <w>herl</w> because it adds excellent movement to the lure in the water.",
                            "target_lang_translations": {
                                "ru": "Он предпочитает использовать страусиную <w>бородку</w>, так как она придает приманке отличное движение в воде."
                            }
                        }
                    ],
                    "synonyms": [
                        "barb",
                        "fiber"
                    ],
                    "common_phrases": [
                        "peacock herl",
                        "ostrich herl"
                    ],
                    "target_lang_definitions": {
                        "ru": "Волокнистая бородка или стержень пера, особенно павлиньего или страусиного, используемые для создания искусственных мушек для рыбалки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бородка пера",
                                "target_lang_sense_clarification": "часть пера"
                            },
                            {
                                "target_lang_word": "херл",
                                "target_lang_sense_clarification": "рыболовный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36d9a2e1-c39c-3d59-af44-e298e2cd8496",
                    "sense_definition": "An artificial fly used in angling that is constructed using feather barbs.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fishing_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She selected a small <w>herl</w> from her box to mimic the insects on the water's surface.",
                            "target_lang_translations": {
                                "ru": "Она выбрала маленькую <w>мушку</w> из своей коробки, чтобы имитировать насекомых на поверхности воды."
                            }
                        },
                        {
                            "text": "The trout rose quickly to snatch the floating <w>herl</w> drifting downstream.",
                            "target_lang_translations": {
                                "ru": "Форель быстро поднялась, чтобы схватить плывущую <w>мушку</w>, дрейфующую по течению."
                            }
                        }
                    ],
                    "synonyms": [
                        "fly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусственная мушка для рыбной ловли, изготовленная с использованием бородок пера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мушка",
                                "target_lang_sense_clarification": "рыболовная приманка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6fe4088-2a98-3ac8-8ff6-750ba9667ec1",
                    "sense_definition": "A single strand or filament of hair.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A single <w>herl</w> of hair fell across her forehead, distracting her from her work.",
                            "target_lang_translations": {
                                "ru": "Один <w>волосок</w> упал ей на лоб, отвлекая от работы."
                            }
                        },
                        {
                            "text": "The microscope revealed the rough texture of the damaged hair <w>herl</w>.",
                            "target_lang_translations": {
                                "ru": "Микроскоп выявил грубую текстуру поврежденного <w>волоска</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "strand",
                        "filament"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельная прядь или нить волоса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волосок",
                                "target_lang_sense_clarification": "единичный волос"
                            },
                            {
                                "target_lang_word": "нить",
                                "target_lang_sense_clarification": "волокно"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f910b36b-407a-32ee-800b-3dc27b2c8223",
                    "sense_definition": "Odbywający się z małą prędkością, trwający długo lub niewykonywany szybko.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "speed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <powolny> proces powrotu do zdrowia wymagał od pacjenta wiele cierpliwości.",
                            "target_lang_translations": {
                                "ru": "Этот <медленный> процесс выздоровления требовал от пациента много терпения."
                            }
                        },
                        {
                            "text": "Obserwowaliśmy <powolny> ruch statków handlowych płynących po szerokiej rzece.",
                            "target_lang_translations": {
                                "ru": "Мы наблюдали <медленное> движение торговых судов, плывущих по широкой реке."
                            }
                        }
                    ],
                    "synonyms": [
                        "wolny"
                    ],
                    "antonyms": [
                        "szybki",
                        "prędki",
                        "wartki"
                    ],
                    "common_phrases": [
                        "powolny proces",
                        "powolny ruch",
                        "powolny wzrost",
                        "powolny spadek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий с низкой скоростью, занимающий много времени или выполняемый небыстро."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "медленный",
                                "target_lang_sense_clarification": "о скорости, движении или процессе"
                            },
                            {
                                "target_lang_word": "неспешный",
                                "target_lang_sense_clarification": "совершаемый без спешки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4bf60b97-2c49-3027-a1ed-e2bd542732c1",
                    "sense_definition": "O człowieku lub zwierzęciu: poruszający się ociężale, działający bez energii i pośpiechu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój stary pies jest już bardzo <powolny> i większość dnia przesypia na kanapie.",
                            "target_lang_translations": {
                                "ru": "Мой старый пёс уже очень <медлительный> и большую часть дня спит на диване."
                            }
                        },
                        {
                            "text": "Był to wyjątkowo <powolny> człowiek, który nigdy się nie denerwował ani nie spieszył.",
                            "target_lang_translations": {
                                "ru": "Это был исключительно <медлительный> человек, который никогда не нервничал и не спешил."
                            }
                        }
                    ],
                    "synonyms": [
                        "ociężały",
                        "niemrawy",
                        "rozlazły"
                    ],
                    "antonyms": [
                        "szybki",
                        "chyży"
                    ],
                    "common_phrases": [
                        "powolny chód",
                        "powolny uczeń",
                        "powolny w ruchach"
                    ],
                    "target_lang_definitions": {
                        "ru": "О человеке или животном: двигающийся тяжело, действующий без энергии и спешки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "медлительный",
                                "target_lang_sense_clarification": "о характере или манере действий"
                            },
                            {
                                "target_lang_word": "нерасторопный",
                                "target_lang_sense_clarification": "медленный в действиях, неловкий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43fa52da-d0a2-3d41-b4dd-b9eee5ce08e8",
                    "sense_definition": "Uległy, posłuszny, chętnie poddający się czyjejś woli lub rozkazom.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "obedience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wierny sługa był zawsze <powolny> woli swojego surowego pana.",
                            "target_lang_translations": {
                                "ru": "Верный слуга был всегда <покорен> воле своего сурового господина."
                            }
                        },
                        {
                            "text": "Szukał żony cichej, skromnej i <powolnej> jego wszelkim rozkazom.",
                            "target_lang_translations": {
                                "ru": "Он искал жену тихую, скромную и <послушную> всем его приказам."
                            }
                        }
                    ],
                    "synonyms": [
                        "uległy",
                        "posłuszny"
                    ],
                    "common_phrases": [
                        "powolny rozkazom",
                        "powolny woli"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "współcześnie rzadko używane w tym znaczeniu"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Податливый, послушный, охотно подчиняющийся чьей-либо воле или приказам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покорный",
                                "target_lang_sense_clarification": "смиренно подчиняющийся"
                            },
                            {
                                "target_lang_word": "послушный",
                                "target_lang_sense_clarification": "исполняющий волю другого"
                            }
                        ]
                    }
                }
            ]
        },
        {

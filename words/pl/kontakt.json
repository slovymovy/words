                },
                {
                    "sense_id": "adb999d2-a464-316e-8169-8a61a5cf896f",
                    "sense_definition": "Soczewka korekcyjna zakładana bezpośrednio na gałkę oczną zamiast okularów.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "optics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na basen wolę zakładać <w>kontakty</w> zamiast zwykłych okularów.",
                            "target_lang_translations": {
                                "ru": "В бассейн я предпочитаю надевать <w>линзы</w> вместо обычных очков."
                            }
                        },
                        {
                            "text": "Muszę kupić nowy płyn do czyszczenia <w>kontaktów</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новую жидкость для очистки <w>линз</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "soczewka",
                        "soczewka kontaktowa",
                        "szkło kontaktowe"
                    ],
                    "common_phrases": [
                        "nosić kontakty",
                        "zakładać kontakty"
                    ],
                    "traits": [

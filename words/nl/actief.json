                {
                    "sense_id": "4e165316-36e4-3c0f-9c12-024dfac392c9",
                    "sense_definition": "Bezig met iets doen; vol energie en werklust, niet passief.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn grootvader is nog steeds erg <w>actief</w> voor zijn leeftijd.",
                            "target_lang_translations": {
                                "en": "My grandfather is still very <w>active</w> for his age.",
                                "ru": "Мой дедушка всё ещё очень <w>активен</w> для своего возраста."
                            }
                        },
                        {
                            "text": "Zij is een <w>actief</w> lid van de lokale sportvereniging.",
                            "target_lang_translations": {
                                "en": "She is an <w>active</w> member of the local sports club.",
                                "ru": "Она — <w>активный</w> член местного спортивного клуба."
                            }
                        }
                    ],
                    "antonyms": [
                        "passief",
                        "inactief",
                        "non-actief"
                    ],
                    "common_phrases": [
                        "actief zijn",
                        "actief deelnemen",
                        "actief blijven"
                    ],
                    "target_lang_definitions": {
                        "en": "Busy doing something; full of energy and zest for work, not passive.",
                        "ru": "Занятый делом; полный энергии и желания работать, не пассивный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "active",
                                "target_lang_sense_clarification": "energetic, engaged in action"
                            },
                            {
                                "target_lang_word": "energetic",
                                "target_lang_sense_clarification": "full of energy"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "активный",
                                "target_lang_sense_clarification": "энергичный, подвижный"
                            },
                            {
                                "target_lang_word": "деятельный",
                                "target_lang_sense_clarification": "трудолюбивый, проявляющий усердие"
                            }
                        ]
                    }
                }
            ]
        },
        {

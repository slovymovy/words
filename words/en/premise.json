            "senses": [
                {
                    "sense_id": "a86142be-fca9-3864-8bda-33bf7ad77ba4",
                    "sense_definition": "A building and the area of land that it is on.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "real_estate",
                    "name_type": "no",
                    "examples": [
                        {

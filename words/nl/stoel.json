        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7fcdeccd-f1c4-383a-a9c0-d377582c7a15",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>stoelen</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>stoel</w> mijn mening op feiten die ik heb gelezen.",
                            "target_lang_translations": {
                                "en": "I <w>base</w> my opinion on facts I have read.",
                                "ru": "Я <w>основываю</w> свое мнение на фактах, которые я прочитал."
                            }
                        },
                        {
                            "text": "Mijn overtuiging is dat ik <w>stoel</w> op jarenlange ervaring.",
                            "target_lang_translations": {
                                "en": "My conviction is that I <w>rely</w> on years of experience.",
                                "ru": "Мое убеждение заключается в том, что я <w>опираюсь</w> на многолетний опыт."
                            }

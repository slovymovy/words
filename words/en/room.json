            "pos": "noun",
            "senses": [
                {
                    "sense_id": "230137f4-c0fe-3041-b3ce-8420211d5d89",
                    "sense_definition": "A separate part of a building, enclosed by walls, a floor, and a ceiling.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_enclosure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She went to her <w>room</w> to read a book in peace.",
                            "target_lang_translations": {
                                "ru": "Она пошла в свою <w>комнату</w>, чтобы спокойно почитать книгу."
                            }
                        },
                        {
                            "text": "The hotel <w>room</w> was very clean and had a beautiful view of the ocean.",
                            "target_lang_translations": {
                                "ru": "<w>Номер</w> в отеле был очень чистым, и из него открывался прекрасный вид на океан."
                            }
                        }
                    ],
                    "synonyms": [
                        "chamber"
                    ],
                    "common_phrases": [
                        "living room",
                        "dining room",
                        "clean the room",
                        "leave the room",
                        "hotel room"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельное помещение в здании, ограниченное стенами, полом и потолком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комната",
                                "target_lang_sense_clarification": "жилое помещение"
                            },
                            {
                                "target_lang_word": "номер",
                                "target_lang_sense_clarification": "в гостинице"
                            },
                            {
                                "target_lang_word": "помещение",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85cb6b7f-68a4-3185-b7f5-183be51ad69d",
                    "sense_definition": "Empty space that is available to be used for a particular purpose.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please move your bag so there is enough <w>room</w> for everyone to sit.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, подвинь сумку, чтобы всем хватило <w>места</w> сесть."
                            }
                        },
                        {
                            "text": "This table takes up too much <w>room</w> in the small kitchen.",
                            "target_lang_translations": {
                                "ru": "Этот стол занимает слишком много <w>места</w> в маленькой кухне."
                            }
                        }
                    ],
                    "synonyms": [
                        "space",
                        "elbow room",
                        "legroom"
                    ],
                    "common_phrases": [
                        "enough room",
                        "make room",
                        "take up room",
                        "plenty of room",
                        "run out of room"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свободное пространство, которое можно использовать для определенной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "место",
                                "target_lang_sense_clarification": "свободное пространство"
                            },
                            {
                                "target_lang_word": "пространство",
                                "target_lang_sense_clarification": "объем, площадь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58a2f62c-e141-39f2-8d58-931cc9c69821",
                    "sense_definition": "A virtual space on the internet where people can communicate with each other.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_enclosure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He joined a chat <w>room</w> to discuss the latest video game strategies.",
                            "target_lang_translations": {
                                "ru": "Он зашел в чат-<w>комнату</w>, чтобы обсудить новейшие стратегии видеоигр."
                            }
                        },
                        {
                            "text": "The moderator had to remove a rude user from the digital <w>room</w>.",
                            "target_lang_translations": {
                                "ru": "Модератору пришлось удалить грубого пользователя из цифровой <w>комнаты</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chat room",
                        "online room"
                    ],
                    "target_lang_definitions": {
                        "ru": "Виртуальное пространство в интернете, где люди могут общаться друг с другом."

                {
                    "sense_id": "af39b07e-1f74-3d0a-89bd-c66383fde6c4",
                    "sense_definition": "A common component of Arabic names, signifying \"father of\".",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "arabic_name_part",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "The diplomat Nabil <w>Abu</w> Rudeineh issued a statement regarding the negotiations.",
                            "target_lang_translations": {
                                "ru": "Дипломат Набиль <w>Абу</w> Рудейна выступил с заявлением по поводу переговоров."
                            }
                        },
                        {
                            "text": "In many cultures, the title <w>Abu</w> is used to show respect to a father.",
                            "target_lang_translations": {
                                "ru": "Во многих культурах титул <w>Абу</w> используется для выражения уважения к отцу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Abu Dhabi",
                        "Abu Bakr"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространенный компонент арабских имен, означающий «отец»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Абу",
                                "target_lang_sense_clarification": "часть арабских имен"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e697d7da-df5d-3bdb-a71e-7e632c7afd38",

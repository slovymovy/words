    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b9f6b0d8-d533-342b-b0ba-dcc59a1a5597",
                    "sense_definition": "A group of related parts or things that work together as a whole.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The city has a very efficient public transport <w>system</w>.",
                            "target_lang_translations": {
                                "ru": "В городе очень эффективная <w>система</w> общественного транспорта."
                            }
                        },
                        {
                            "text": "We need to repair the heating <w>system</w> before winter comes.",
                            "target_lang_translations": {
                                "ru": "Нам нужно починить <w>систему</w> отопления до наступления зимы."
                            }
                        }
                    ],
                    "synonyms": [
                        "structure",
                        "organization",
                        "complex"
                    ],
                    "common_phrases": [
                        "transport system",
                        "heating system",
                        "security system",
                        "education system"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа связанных частей или объектов, работающих вместе как единое целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "система",
                                "target_lang_sense_clarification": "совокупность элементов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb2db715-9fa2-3d6f-b226-f190aebbd2cc",
                    "sense_definition": "A method or organized way of doing something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_method",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher uses a point <w>system</w> to encourage good behavior in class.",
                            "target_lang_translations": {
                                "ru": "Учитель использует балльную <w>систему</w> для поощрения хорошего поведения в классе."
                            }
                        },
                        {
                            "text": "She has her own <w>system</w> for organizing files on her computer.",
                            "target_lang_translations": {
                                "ru": "У неё своя <w>система</w> организации файлов на компьютере."
                            }
                        }
                    ],
                    "synonyms": [
                        "arrangement",
                        "organization"
                    ],
                    "common_phrases": [
                        "point system",
                        "metric system",
                        "system of government",
                        "scoring system"
                    ],
                    "target_lang_definitions": {
                        "ru": "Метод или организованный способ выполнения чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "система",
                                "target_lang_sense_clarification": "метод, способ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9660ed8-a5cd-3ac8-8bfa-3d27ab047326",
                    "sense_definition": "A set of hardware and software operating in a computer.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should update your operating <w>system</w> to the latest version.",
                            "target_lang_translations": {
                                "ru": "Вам следует обновить операционную <w>систему</w> до последней версии."
                            }
                        },
                        {
                            "text": "The entire computer <w>system</w> crashed during the power outage.",
                            "target_lang_translations": {
                                "ru": "Вся компьютерная <w>система</w> вышла из строя во время отключения электричества."
                            }
                        }
                    ],
                    "common_phrases": [
                        "operating system",
                        "system error",
                        "system administrator",
                        "computer system"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Комплекс аппаратного и программного обеспечения компьютера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "система",
                                "target_lang_sense_clarification": "компьютерная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef54c4ed-5e56-3784-83d3-5ee16832157c",
                    "sense_definition": "A set of organs in the body that perform a particular function.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Eating vegetables is good for your immune <w>system</w>.",
                            "target_lang_translations": {
                                "ru": "Употребление овощей полезно для вашей иммунной <w>системы</w>."
                            }
                        },
                        {
                            "text": "The nervous <w>system</w> transmits signals between the brain and the body.",
                            "target_lang_translations": {
                                "ru": "Нервная <w>система</w> передает сигналы между мозгом и телом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "immune system",
                        "digestive system",
                        "nervous system",
                        "respiratory system"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "anatomy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность органов тела, выполняющих определенную функцию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "система",
                                "target_lang_sense_clarification": "организма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "064a9ce9-5eec-3f82-89c2-e4156251baa3",
                    "sense_definition": "A group of celestial objects bound together by gravity.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There are eight planets in our solar <w>system</w>.",
                            "target_lang_translations": {
                                "ru": "В нашей Солнечной <w>системе</w> восемь планет."
                            }
                        },
                        {
                            "text": "Astronomers are studying a distant star <w>system</w> to find new planets.",
                            "target_lang_translations": {
                                "ru": "Астрономы изучают далекую звездную <w>систему</w> в поисках новых планет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "solar system",
                        "star system"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "astronomy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа небесных тел, связанных гравитацией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "система",
                                "target_lang_sense_clarification": "планетная или звездная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3266a891-e284-3a90-8b8a-92338fe59134",
                    "sense_definition": "The established government or social order, often seen as controlling or oppressive.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He felt that the <w>system</w> was designed to keep the rich in power.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал, что <w>система</w> создана для того, чтобы удерживать богатых у власти."
                            }
                        },
                        {
                            "text": "It is difficult to fight the <w>system</w> when you have no money or influence.",
                            "target_lang_translations": {
                                "ru": "Трудно бороться с <w>системой</w>, когда у тебя нет ни денег, ни влияния."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fight the system",
                        "beat the system",
                        "blame the system"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устоявшийся общественный или государственный строй, часто воспринимаемый как подавляющий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "система",
                                "target_lang_sense_clarification": "общественный строй"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5df18fcb-6b72-302a-9965-33d496312d43",
                    "sense_definition": "A set of equations involving the same variables to be solved together.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The students learned how to solve a linear <w>system</w> using matrices.",
                            "target_lang_translations": {
                                "ru": "Студенты учились решать линейную <w>систему</w> с помощью матриц."
                            }
                        },
                        {
                            "text": "We plotted the points on a Cartesian coordinate <w>system</w>.",
                            "target_lang_translations": {
                                "ru": "Мы нанесли точки на декартову <w>систему</w> координат."
                            }
                        }
                    ],
                    "common_phrases": [
                        "system of equations",
                        "coordinate system"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mathematics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор уравнений с общими переменными, которые решаются совместно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "система",
                                "target_lang_sense_clarification": "уравнений"
                            }
                        ]
                    }
                }
            ]
        }

            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9b3f7247-333b-35ae-9715-1e956e6834c3",
                    "sense_definition": "Zawierający w swoim składzie żelazo, bogaty w związki żelaza.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "composition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta woda jest <w>żelazista</w>, co widać po charakterystycznym rdzawym osadzie na dnie naczynia.",
                            "target_lang_translations": {
                                "ru": "Эта вода <w>железистая</w>, что видно по характерному ржавому осадку на дне сосуда."
                            }
                        },
                        {
                            "text": "Badania geologiczne wykazały, że gleba w tej dolinie jest wysoce <w>żelazista</w>.",
                            "target_lang_translations": {
                                "ru": "Геологические исследования показали, что почва в этой долине очень <w>железистая</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "żelazonośny"
                    ],
                    "common_phrases": [
                        "woda żelazista",
                        "źródło żelaziste",
                        "gleba żelazista"
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержащий в своём составе железо, богатый соединениями железа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "железистый",
                                "target_lang_sense_clarification": "содержащий железо (о воде, почве)"
                            },
                            {
                                "target_lang_word": "железосодержащий",
                                "target_lang_sense_clarification": "имеющий в составе железо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e14b13e-3d34-37c5-bc79-8e6b1bf4e917",
                    "sense_definition": "Mający smak lub zapach przypominający żelazo.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sensory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po przygryzieniu wargi poczułem w ustach nieprzyjemny, <w>żelazisty</w> posmak krwi.",
                            "target_lang_translations": {
                                "ru": "Прикусив губу, я почувствовал во рту неприятный <w>железистый</w> привкус крови."
                            }
                        },
                        {
                            "text": "Wino miało dziwny, nieco <w>żelazisty</w> aromat, który nie odpowiadał gościom.",
                            "target_lang_translations": {
                                "ru": "У вина был странный, слегка <w>железистый</w> аромат, который не понравился гостям."
                            }
                        }
                    ],
                    "synonyms": [
                        "metaliczny"
                    ],
                    "common_phrases": [
                        "żelazisty posmak",
                        "żelazisty zapach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий вкус или запах, напоминающий железо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "железистый",
                                "target_lang_sense_clarification": "о привкусе или запахе"
                            },
                            {
                                "target_lang_word": "металлический",
                                "target_lang_sense_clarification": "напоминающий металл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f17a569-0808-386b-8d15-8e9d64e19a5a",
                    "sense_definition": "O bakteriach: czerpiący energię życiową z procesu utleniania związków żelaza.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bakterie <w>żelaziste</w> mogą powodować zarastanie rur wodociągowych osadami.",
                            "target_lang_translations": {
                                "ru": "<w>Железистые</w> бактерии могут вызывать зарастание водопроводных труб осадком."
                            }
                        },
                        {
                            "text": "W próbkach wody ze studni wykryto liczne bakterie <w>żelaziste</w>.",
                            "target_lang_translations": {
                                "ru": "В пробах воды из колодца были обнаружены многочисленные <w>железистые</w> бактерии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bakterie żelaziste"
                    ],
                    "target_lang_definitions": {
                        "ru": "О бактериях: получающий энергию в процессе окисления соединений железа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "железистый",
                                "target_lang_sense_clarification": "о бактериях"
                            }
                        ]
                    }
                }
            ]
        }

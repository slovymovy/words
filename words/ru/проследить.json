    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "239debf1-6642-3bc9-8a66-e21b579339e4",
                    "sense_definition": "Проконтролировать выполнение чего-либо, позаботиться о том, чтобы что-то было сделано правильно или вовремя.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "control_supervision",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я лично <прослежу>, чтобы все документы были готовы к завтрашнему утру.",
                            "target_lang_translations": {
                                "en": "I will personally <see to it> that all documents are ready by tomorrow morning."
                            }
                        },
                        {
                            "text": "Родители попросили старшего сына <проследить> за порядком в доме, пока их не будет.",
                            "target_lang_translations": {
                                "en": "The parents asked their eldest son to <look after> the house while they were away."
                            }
                        }
                    ],
                    "synonyms": [
                        "проконтролировать",
                        "позаботиться",
                        "присмотреть"
                    ],
                    "common_phrases": [
                        "проследить за выполнением",
                        "проследить за тем",
                        "проследить за порядком"
                    ],
                    "target_lang_definitions": {
                        "en": "To ensure that something is done correctly or to supervise a process."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "see to",
                                "target_lang_sense_clarification": "take responsibility for ensuring something happens"
                            },
                            {
                                "target_lang_word": "make sure",
                                "target_lang_sense_clarification": "verify or ensure a result"
                            },
                            {
                                "target_lang_word": "supervise",
                                "target_lang_sense_clarification": "monitor the execution of a task"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "011c84d1-27c0-3e40-a768-6a6eeef874c6",
                    "sense_definition": "Проводить взглядом движущийся объект или наблюдать за чьими-либо действиями в течение некоторого времени.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "observation_tracking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она подошла к окну и <проследила> взглядом за отъезжающей машиной такси.",
                            "target_lang_translations": {
                                "en": "She walked to the window and <followed> the departing taxi with her eyes."
                            }
                        },
                        {
                            "text": "Мы решили <проследить>, куда направится эта странная собака.",
                            "target_lang_translations": {
                                "en": "We decided to <watch> where this strange dog would go."
                            }
                        }
                    ],
                    "synonyms": [
                        "пронаблюдать",
                        "проводить"
                    ],
                    "common_phrases": [
                        "проследить взглядом",
                        "проследить глазами",
                        "проследить за полётом"
                    ],
                    "target_lang_definitions": {
                        "en": "To follow a moving object with one's eyes or watch someone's movement."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "follow",
                                "target_lang_sense_clarification": "with one's eyes or gaze"
                            },
                            {
                                "target_lang_word": "watch",
                                "target_lang_sense_clarification": "observe movement or actions"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79048780-2d9d-35f4-927b-a6e12054e586",
                    "sense_definition": "Последовательно изучить ход развития чего-либо, установить причинно-следственные связи или историю возникновения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "investigation_analysis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этой книге автор пытается <проследить> историю развития космической отрасли.",
                            "target_lang_translations": {
                                "en": "In this book, the author attempts to <trace> the history of the space industry's development."
                            }
                        },
                        {
                            "text": "Врачам удалось <проследить> связь между новым лекарством и улучшением состояния пациентов.",
                            "target_lang_translations": {
                                "en": "Doctors managed to <trace> the link between the new drug and the improvement in patients' condition."
                            }
                        }
                    ],
                    "synonyms": [
                        "исследовать",
                        "установить",
                        "выяснить"
                    ],
                    "common_phrases": [
                        "проследить историю",
                        "проследить развитие",
                        "проследить связь",
                        "проследить динамику"
                    ],
                    "target_lang_definitions": {
                        "en": "To investigate the history, development, or cause-and-effect connections of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trace",
                                "target_lang_sense_clarification": "follow the history or origin of something"
                            },
                            {
                                "target_lang_word": "track",
                                "target_lang_sense_clarification": "monitor the progress or development"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a115d20c-1da1-3215-8e4e-5f710aa99226",
                    "sense_definition": "Тайно следуя за кем-либо, выяснить его местонахождение или действия; выследить.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "observation_tracking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Частный детектив смог <проследить> подозреваемого до его тайного укрытия.",
                            "target_lang_translations": {
                                "en": "The private detective was able to <track> the suspect to his secret hideout."
                            }
                        },
                        {
                            "text": "Им так и не удалось <проследить>, кто именно приносил эти загадочные письма.",
                            "target_lang_translations": {
                                "en": "They never managed to <track down> who exactly was bringing these mysterious letters."
                            }
                        }
                    ],
                    "synonyms": [
                        "выследить"
                    ],
                    "common_phrases": [
                        "проследить преступника",
                        "проследить маршрут"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies tracking down someone"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To secretly follow someone to discover their location or actions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "track down",
                                "target_lang_sense_clarification": "find by following evidence or movements"
                            },
                            {
                                "target_lang_word": "tail",
                                "target_lang_sense_clarification": "follow someone secretly"
                            }
                        ]
                    }
                }
            ]
        }
    ]

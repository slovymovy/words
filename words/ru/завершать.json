{
    "word_family": [
        "завершение",
        "завершённость",
        "завершиться"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f06a7548-7704-30c6-94ef-d6c5afe03968",
                    "sense_definition": "Доводить начатое дело, действие или процесс до конца.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "finish_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы планируем <завершать> этот сложный проект в следующем месяце.",
                            "target_lang_translations": {
                                "en": "We plan to <complete> this complex project next month."
                            }
                        },
                        {
                            "text": "Лектор начал <завершать> своё выступление, подводя итоги.",
                            "target_lang_translations": {
                                "en": "The lecturer began to <conclude> his speech, summarizing the results."
                            }
                        }
                    ],
                    "synonyms": [
                        "заканчивать",
                        "оканчивать",
                        "кончать"
                    ],
                    "antonyms": [
                        "начинать"
                    ],
                    "common_phrases": [
                        "завершать работу",
                        "завершать строительство",
                        "завершать карьеру",
                        "завершать процесс"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring a started task, action, or process to an end."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "complete",
                                "target_lang_sense_clarification": "to finish a task or project"
                            },
                            {
                                "target_lang_word": "finish",
                                "target_lang_sense_clarification": "general term for ending an action"
                            },
                            {
                                "target_lang_word": "conclude",
                                "target_lang_sense_clarification": "formal, to bring to an end"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3466721-daef-355e-9154-2b856e2b7420",
                    "sense_definition": "Делать что-либо цельным и совершенным, добавляя последнюю деталь.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "make_complete",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот изящный аксессуар будет удачно <завершать> твой вечерний образ.",
                            "target_lang_translations": {
                                "en": "This elegant accessory will successfully <complete> your evening look."
                            }
                        },
                        {
                            "text": "Художник решил <завершать> картину ярким штрихом в центре.",
                            "target_lang_translations": {
                                "en": "The artist decided to <finish> the painting with a bright stroke in the center."
                            }
                        }
                    ],
                    "synonyms": [
                        "заканчивать"
                    ],
                    "common_phrases": [
                        "завершать картину",
                        "завершать образ",
                        "завершать композицию"
                    ],
                    "target_lang_definitions": {
                        "en": "To make something whole and perfect by adding the last detail."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "complete",
                                "target_lang_sense_clarification": "to make whole"
                            },
                            {
                                "target_lang_word": "crown",
                                "target_lang_sense_clarification": "to add a perfect finishing touch"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94e259f3-17af-3397-966d-1cf3122566ac",
                    "sense_definition": "Находиться в конце чего-либо, составлять заключительную часть ряда или последовательности.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "be_the_end",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Выступление известного певца будет <завершать> праздничный концерт.",
                            "target_lang_translations": {
                                "en": "The famous singer's performance will <conclude> the festive concert."
                            }
                        },
                        {
                            "text": "Этот красивый старинный дом будет <завершать> нашу улицу.",
                            "target_lang_translations": {
                                "en": "This beautiful old house will <end> our street."
                            }
                        }
                    ],
                    "synonyms": [
                        "заканчивать"
                    ],
                    "antonyms": [
                        "начинать"
                    ],
                    "common_phrases": [
                        "завершать список",
                        "завершать ряд",
                        "завершать процессию"
                    ],
                    "target_lang_definitions": {
                        "en": "To be at the end of something or constitute the final part."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "conclude",
                                "target_lang_sense_clarification": "to be the last part of an event"
                            },
                            {
                                "target_lang_word": "end",
                                "target_lang_sense_clarification": "to be at the physical or temporal end"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d052093-110f-3a9c-ad42-cc01498de645",
                    "sense_definition": "Оформлять верхнюю часть здания или сооружения.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Архитектор задумал <завершать> башню высоким острым шпилем.",
                            "target_lang_translations": {
                                "en": "The architect planned to <crown> the tower with a tall sharp spire."
                            }
                        },
                        {
                            "text": "Строители начали <завершать> собор золотым куполом.",
                            "target_lang_translations": {
                                "en": "The builders began to <top> the cathedral with a golden dome."
                            }
                        }
                    ],
                    "common_phrases": [
                        "завершать куполом",
                        "завершать шпилем",
                        "завершать карнизом"
                    ],
                    "target_lang_definitions": {
                        "en": "To form the upper part of a building or structure."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crown",
                                "target_lang_sense_clarification": "to be at the top of"
                            },
                            {
                                "target_lang_word": "top",
                                "target_lang_sense_clarification": "to form the top of"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "ec155243-95fb-3fc9-a694-b076cc077398",
                    "sense_definition": "Niet slim of onverstandig; getuigend van weinig intelligentie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "intelligence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was erg <w>stom</w> van mij om de deur niet op slot te doen.",
                            "target_lang_translations": {
                                "en": "It was very <w>stupid</w> of me not to lock the door."
                            }
                        },
                        {
                            "text": "Hij is te <w>stom</w> om te begrijpen wat er aan de hand is.",
                            "target_lang_translations": {
                                "en": "He is too <w>stupid</w> to understand what is going on."
                            }
                        }
                    ],
                    "synonyms": [
                        "dom",
                        "dwaas",
                        "stompzinnig",
                        "oliedom"
                    ],
                    "antonyms": [
                        "slim"
                    ],
                    "common_phrases": [
                        "stom van me",
                        "doe niet zo stom"
                    ],
                    "target_lang_definitions": {
                        "en": "Not smart or unwise; showing little intelligence."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stupid",
                                "target_lang_sense_clarification": "lacking intelligence or common sense"
                            },
                            {
                                "target_lang_word": "dumb",
                                "target_lang_sense_clarification": "lacking intelligence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "710be1bb-c0ba-3875-bcf7-a3505f2aa65f",
                    "sense_definition": "Vervelend, irritant of saai; niet leuk.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "annoyance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik moet een <w>stomme</w> opdracht maken voor school waar ik geen zin in heb.",
                            "target_lang_translations": {
                                "en": "I have to do a <w>stupid</w> assignment for school that I don't feel like doing."
                            }
                        },
                        {
                            "text": "Wat een <w>stom</w> gedoe is dit allemaal, ik ga naar huis.",
                            "target_lang_translations": {
                                "en": "What a <w>stupid</w> hassle this all is, I'm going home."
                            }
                        }
                    ],
                    "synonyms": [
                        "afstompend",
                        "geestdodend"
                    ],
                    "common_phrases": [
                        "stom gedoe",
                        "stomme film"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used to express general dislike"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Annoying, irritating, or boring; not fun."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stupid",
                                "target_lang_sense_clarification": "annoying or unpleasant"
                            },
                            {
                                "target_lang_word": "lame",
                                "target_lang_sense_clarification": "uncool, boring, or disappointing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "456e45b6-c526-3947-9be0-28ace4112d1e",
                    "sense_definition": "Niet in staat om te spreken, door een fysieke beperking of tijdelijk door hevige emotie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "speech_ability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij was even <w>stom</w> van verbazing toen hij het onverwachte nieuws hoorde.",
                            "target_lang_translations": {
                                "en": "He was <w>speechless</w> with amazement for a moment when he heard the unexpected news."
                            }
                        },
                        {
                            "text": "In het verleden werden dove en <w>stomme</w> mensen vaak niet goed begrepen.",
                            "target_lang_translations": {
                                "en": "In the past, deaf and <w>mute</w> people were often not well understood."
                            }
                        }
                    ],
                    "synonyms": [
                        "sprakeloos"
                    ],
                    "common_phrases": [
                        "stom van verbazing",
                        "doof en stom"
                    ],
                    "target_lang_definitions": {
                        "en": "Unable to speak, due to a physical limitation or temporarily due to strong emotion."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mute",
                                "target_lang_sense_clarification": "physically unable to speak"
                            },
                            {
                                "target_lang_word": "speechless",
                                "target_lang_sense_clarification": "temporarily unable to speak due to emotion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ace603a-da24-38ae-bb6b-fd0789022522",
                    "sense_definition": "Zonder geluid of spraak; niet uitgesproken.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "soundless",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Charlie Chaplin is wereldberoemd geworden met zijn rollen in de <w>stomme</w> film.",
                            "target_lang_translations": {
                                "en": "Charlie Chaplin became world famous with his roles in <w>silent</w> film."
                            }
                        },
                        {
                            "text": "De letter 'n' aan het einde van dat woord is bijna <w>stom</w>.",
                            "target_lang_translations": {

            "senses": [
                {
                    "sense_id": "6ccbb41f-e3f3-3d19-a388-2671e9a1d6a3",
                    "sense_definition": "Монастырь; территория и здания, где живут и служат Богу монахи или монахини.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "monastery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Паломники со всей страны приезжали в эту святую <обитель>.",
                            "target_lang_translations": {
                                "en": "Pilgrims from all over the country came to this holy <monastery>."
                            }
                        },
                        {
                            "text": "Старая <обитель> была окружена высокими каменными стенами.",
                            "target_lang_translations": {
                                "en": "The old <cloister> was surrounded by high stone walls."
                            }
                        }
                    ],
                    "synonyms": [
                        "монастырь"
                    ],
                    "common_phrases": [
                        "святая обитель",
                        "тихая обитель",
                        "монашеская обитель"
                    ],
                    "target_lang_definitions": {
                        "en": "A monastery or convent; the territory and buildings where monks or nuns live and serve God."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "monastery",
                                "target_lang_sense_clarification": "general term for a community of monks"
                            },
                            {
                                "target_lang_word": "convent",
                                "target_lang_sense_clarification": "specifically for nuns"
                            },
                            {
                                "target_lang_word": "cloister",
                                "target_lang_sense_clarification": "place of religious seclusion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "66fd8844-e102-39cb-ae1d-1e767217e001",
                    "sense_definition": "Жилище, дом или место проживания (часто употребляется в торжественном или шутливом стиле).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "dwelling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Добро пожаловать в мою скромную <обитель>!",
                            "target_lang_translations": {
                                "en": "Welcome to my humble <abode>!"
                            }
                        },
                        {
                            "text": "Его лесная <обитель> была скрыта от посторонних глаз густыми деревьями.",
                            "target_lang_translations": {
                                "en": "His forest <dwelling> was hidden from prying eyes by thick trees."
                            }
                        }
                    ],
                    "synonyms": [
                        "жилище",
                        "кров",
                        "пристанище",
                        "обиталище",
                        "приют",
                        "угол"
                    ],
                    "common_phrases": [
                        "скромная обитель",
                        "лесная обитель",
                        "небесная обитель"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "used in high style or humorously"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A dwelling, house, or place of residence (often used in a solemn or humorous style)."
                    },

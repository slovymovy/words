    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ca935c8d-6e78-393c-8856-632caff8390d",
                    "sense_definition": "The part of an animal's body that sticks out from the back end, often flexible and movable.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The happy dog wagged its <w>tail</w> when I came home.",
                            "target_lang_translations": {
                                "ru": "Счастливая собака виляла <w>хвостом</w>, когда я пришел домой."
                            }
                        },
                        {
                            "text": "Monkeys use their long <w>tails</w> to help them balance in the trees.",
                            "target_lang_translations": {
                                "ru": "Обезьяны используют свои длинные <w>хвосты</w>, чтобы удерживать равновесие на деревьях."
                            }
                        }
                    ],
                    "synonyms": [
                        "appendage"
                    ],
                    "common_phrases": [
                        "wag its tail",
                        "between its legs"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть тела животного, выступающая сзади, часто гибкая и подвижная."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хвост",
                                "target_lang_sense_clarification": "часть тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8026796-bba5-38dc-bb06-23a3b487fdfa",
                    "sense_definition": "The rear structure of an aircraft, usually containing stabilizers and the rudder.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "body_part_extension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The airline's logo was painted clearly on the <w>tail</w> of the plane.",
                            "target_lang_translations": {
                                "ru": "Логотип авиакомпании был четко нарисован на <w>хвосте</w> самолета."
                            }
                        },
                        {
                            "text": "Smoke started coming from the <w>tail</w> shortly after takeoff.",
                            "target_lang_translations": {
                                "ru": "Вскоре после взлета из <w>хвоста</w> повалил дым."
                            }
                        }
                    ],
                    "synonyms": [
                        "empennage"
                    ],
                    "antonyms": [
                        "nose"
                    ],
                    "target_lang_definitions": {
                        "ru": "Задняя конструкция самолета, обычно содержащая стабилизаторы и руль направления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хвост",
                                "target_lang_sense_clarification": "самолета"
                            },
                            {
                                "target_lang_word": "хвостовое оперение",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5aea45cf-8f6f-3206-963e-3953ce6f030f",
                    "sense_definition": "The very end, last, or rear part of something.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "body_part_extension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We had to wait at the <w>tail</w> of the long queue for tickets.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось ждать в <w>хвосте</w> длинной очереди за билетами."
                            }
                        },
                        {
                            "text": "The <w>tail</w> of the storm brought heavy rain to the coast.",
                            "target_lang_translations": {
                                "ru": "<w>Хвост</w> шторма принес на побережье проливной дождь."
                            }
                        }
                    ],
                    "synonyms": [
                        "end",
                        "rear",
                        "back"
                    ],
                    "antonyms": [
                        "head",
                        "front"
                    ],
                    "common_phrases": [
                        "tail end"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самый конец или задняя часть чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хвост",
                                "target_lang_sense_clarification": "очереди, поезда"
                            },
                            {
                                "target_lang_word": "конец",
                                "target_lang_sense_clarification": "завершающая часть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "905d060e-89b1-3679-93f9-e4e3f15d2bd7",
                    "sense_definition": "The side of a coin that does not bear the image of a head; the reverse side.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "coin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He flipped the coin and it landed on <w>tails</w>.",
                            "target_lang_translations": {
                                "ru": "Он подбросил монету, и выпала <w>решка</w>."
                            }
                        },
                        {
                            "text": "You can call heads or <w>tails</w> to decide who goes first.",
                            "target_lang_translations": {
                                "ru": "Вы можете выбрать орла или <w>решку</w>, чтобы решить, кто ходит первым."
                            }
                        }
                    ],
                    "synonyms": [
                        "reverse"
                    ],
                    "antonyms": [
                        "heads"
                    ],
                    "common_phrases": [
                        "heads or tails"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сторона монеты, на которой нет изображения головы; обратная сторона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "решка",
                                "target_lang_sense_clarification": "сторона монеты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef901c5d-1e06-3e4c-82d7-1a4937ada0a2",
                    "sense_definition": "A formal evening coat for men with two long tapered sections at the back.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The groom wore a top hat and <w>tails</w> to the wedding.",
                            "target_lang_translations": {
                                "ru": "Жених был в цилиндре и <w>фраке</w> на свадьбе."
                            }
                        },
                        {
                            "text": "Musicians in the orchestra are often required to wear <w>tails</w>.",
                            "target_lang_translations": {
                                "ru": "Музыканты в оркестре часто обязаны носить <w>фраки</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "tailcoat"
                    ],
                    "common_phrases": [
                        "top hat and tails"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужской вечерний парадный костюм с пиджаком, имеющим длинные фалды сзади."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фрак",
                                "target_lang_sense_clarification": "одежда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d9d59bd-fb91-33a1-bfcd-31f9bd9e5742",
                    "sense_definition": "A person who secretly follows another to observe their actions.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "following",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The detective realized he had a <w>tail</w> and tried to lose him in the crowd.",
                            "target_lang_translations": {
                                "ru": "Детектив понял, что за ним <w>хвост</w>, и попытался скрыться от него в толпе."
                            }
                        },
                        {
                            "text": "They put a <w>tail</w> on the suspect to find out where he was hiding the money.",
                            "target_lang_translations": {
                                "ru": "Они приставили <w>хвост</w> к подозреваемому, чтобы выяснить, где он прячет деньги."
                            }
                        }
                    ],
                    "synonyms": [
                        "shadow"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который тайно следует за другим, чтобы наблюдать за его действиями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хвост",
                                "target_lang_sense_clarification": "шпион, преследователь"
                            },
                            {
                                "target_lang_word": "шпик",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "633918ce-0453-371f-8c33-65ae53e70424",
                    "sense_definition": "The luminous stream of gas and dust that extends from the head of a comet.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "astronomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The comet's <w>tail</w> was visible in the night sky for several weeks.",
                            "target_lang_translations": {
                                "ru": "<w>Хвост</w> кометы был виден в ночном небе в течение нескольких недель."
                            }
                        },
                        {
                            "text": "Solar winds cause the <w>tail</w> to point away from the sun.",
                            "target_lang_translations": {
                                "ru": "Солнечный ветер заставляет <w>хвост</w> отклоняться от Солнца."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Светящийся шлейф газа и пыли, тянущийся от головы кометы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хвост",
                                "target_lang_sense_clarification": "кометы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "694b1a4f-6cde-3ff6-8179-6cb78aea94b1",
                    "sense_definition": "Sexual intercourse or a person considered as a sexual object.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "slang_vulgar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He went to the bar hoping to find some <w>tail</w>.",
                            "target_lang_translations": {
                                "ru": "Он пошел в бар в надежде подцепить какую-нибудь <w>цыпочку</w>."
                            }
                        },
                        {
                            "text": "The guys were joking about chasing <w>tail</w> all weekend.",
                            "target_lang_translations": {
                                "ru": "Парни шутили о том, как будут бегать за <w>юбками</w> все выходные."
                            }
                        }
                    ],
                    "synonyms": [
                        "ass"
                    ],
                    "common_phrases": [
                        "chasing tail",
                        "piece of tail"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "vulgar"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сексуальный партнер или человек, рассматриваемый как сексуальный объект (грубо)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "юбка",
                                "target_lang_sense_clarification": "сленг, о женщине"
                            },
                            {
                                "target_lang_word": "телка",
                                "target_lang_sense_clarification": "грубый сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "84312a14-582c-3b62-95ac-522deb72a478",
                    "sense_definition": "A legal limitation that restricts inheritance to a specific line of heirs.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The estate was held in <w>tail</w>, meaning it could only pass to his male descendants.",
                            "target_lang_translations": {
                                "ru": "Поместье находилось во владении с <w>ограниченным правом наследования</w>, то есть могло переходить только к его потомкам мужского пола."
                            }
                        },
                        {
                            "text": "Laws regarding fee <w>tail</w> have been abolished in many jurisdictions.",
                            "target_lang_translations": {
                                "ru": "Законы, касающиеся <w>ограниченного права наследования</w>, были отменены во многих юрисдикциях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fee tail",
                        "estate tail"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "legal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Юридическое ограничение, закрепляющее наследование за определенной линией наследников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ограниченное право наследования",
                                "target_lang_sense_clarification": "юридический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7307e7e8-ec97-30ca-830c-6e0ebf0f4e6c",
                    "sense_definition": "To follow someone closely and secretly in order to observe them.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "following",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The private investigator was hired to <w>tail</w> the husband for a week.",
                            "target_lang_translations": {
                                "ru": "Частного детектива наняли <w>следить</w> за мужем в течение недели."
                            }
                        },
                        {
                            "text": "I had a feeling that a black car was <w>tailing</w> us on the highway.",
                            "target_lang_translations": {
                                "ru": "У меня было чувство, что черная машина <w>висела</w> у нас на <w>хвосте</w> на шоссе."
                            }
                        }
                    ],
                    "synonyms": [
                        "shadow",
                        "follow",
                        "track",
                        "stalk"
                    ],
                    "common_phrases": [
                        "tail a suspect"
                    ],
                    "target_lang_definitions": {
                        "ru": "Следовать за кем-то вплотную и тайно с целью наблюдения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следить",
                                "target_lang_sense_clarification": "наблюдать"
                            },
                            {
                                "target_lang_word": "висеть на хвосте",
                                "target_lang_sense_clarification": "идиома"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8b1cbd33-af2c-3391-9161-ef212ec15913",
                    "sense_definition": "Limited or restricted regarding the line of inheritance.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The property was granted as an estate <w>tail</w>.",
                            "target_lang_translations": {
                                "ru": "Собственность была передана как владение с <w>ограниченным наследованием</w>."
                            }
                        },
                        {
                            "text": "He tried to convert the <w>tail</w> interest into a fee simple.",
                            "target_lang_translations": {
                                "ru": "Он попытался преобразовать право с <w>ограниченным наследованием</w> в безусловное право собственности."
                            }
                        }
                    ],
                    "synonyms": [
                        "curtailed",
                        "limited"
                    ],
                    "common_phrases": [
                        "estate tail"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "legal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ограниченный в отношении линии наследования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ограниченный",
                                "target_lang_sense_clarification": "о праве наследования"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "94e08feb-6162-3c7e-9292-70957424cf9d",
                    "sense_definition": "A Chinese constellation located within the tail of Scorpius.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "astronomy",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "The <w>Tail</w> is one of the twenty-eight lunar mansions in Chinese astronomy.",
                            "target_lang_translations": {
                                "ru": "<w>Хвост</w> — одна из двадцати восьми лунных стоянок в китайской астрономии."
                            }
                        },
                        {
                            "text": "Ancient observers mapped the position of the <w>Tail</w> relative to the Azure Dragon.",
                            "target_lang_translations": {
                                "ru": "Древние наблюдатели наносили на карту положение <w>Хвоста</w> относительно Лазурного Дракона."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Китайское созвездие, расположенное в хвосте Скорпиона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хвост",
                                "target_lang_sense_clarification": "созвездие"
                            }
                        ]
                    }
                }
            ]
        }
    ]

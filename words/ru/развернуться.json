{
    "word_family": [
        "развернуть",
        "разворот",
        "развёртывание",
        "развёрнутый",
        "свернуть",
        "повернуть",
        "вернуть",
        "отвернуть"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bd6f497c-58b0-376a-ab0c-761c084bddf4",
                    "sense_definition": "Изменить направление движения или положение тела на противоположное или иное; сделать поворот.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он резко <развернулся> и пошёл в другую сторону.",
                            "target_lang_translations": {
                                "en": "He abruptly <turned around> and went the other way."
                            }
                        },
                        {
                            "text": "Машина <развернулась> на перекрёстке и уехала.",
                            "target_lang_translations": {
                                "en": "The car <made a U-turn> at the intersection and drove away."
                            }
                        }
                    ],
                    "synonyms": [
                        "повернуться",
                        "повернуть"
                    ],
                    "common_phrases": [
                        "развернуться на месте",
                        "развернуться назад",
                        "резко развернуться"
                    ],
                    "target_lang_definitions": {
                        "en": "To change direction of movement or body position to the opposite side."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "turn around",
                                "target_lang_sense_clarification": "general movement of a person or object"
                            },
                            {
                                "target_lang_word": "make a U-turn",
                                "target_lang_sense_clarification": "specifically for vehicles"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4fc36a7-a68d-3a23-9e0e-c20fd51e2250",
                    "sense_definition": "Раскрыться, распрямиться, стать плоским или широким (о чём-то свёрнутом, сложенном или упакованном).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Огромный парашют успешно <развернулся> в небе.",
                            "target_lang_translations": {
                                "en": "The huge parachute successfully <deployed> in the sky."
                            }
                        },
                        {
                            "text": "Старая карта <развернулась> на столе перед нами.",
                            "target_lang_translations": {
                                "en": "The old map <unfolded> on the table before us."
                            }
                        }
                    ],
                    "synonyms": [
                        "раскрыться",
                        "открыться",
                        "распаковаться"
                    ],
                    "antonyms": [
                        "стесняться"
                    ],
                    "common_phrases": [
                        "знамя развернулось",
                        "цветок развернулся",
                        "книга развернулась"
                    ],
                    "target_lang_definitions": {
                        "en": "To open up, straighten out, or expand from a folded or rolled state."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unfold",
                                "target_lang_sense_clarification": "for maps, paper, or general objects"
                            },
                            {
                                "target_lang_word": "deploy",
                                "target_lang_sense_clarification": "technical use, e.g., a parachute"
                            },
                            {
                                "target_lang_word": "unfurl",
                                "target_lang_sense_clarification": "for flags, sails, or banners"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4d244e4-b685-3856-a7cc-875878adbe4a",
                    "sense_definition": "Начать активно происходить, развиваться в широких масштабах (о событиях, действиях, процессах).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В городе <развернулось> масштабное строительство новых домов.",
                            "target_lang_translations": {
                                "en": "Large-scale construction of new houses <got under way> in the city."
                            }
                        },
                        {
                            "text": "Горячая дискуссия <развернулась> вокруг этой темы.",
                            "target_lang_translations": {
                                "en": "A heated discussion <developed> around this topic."
                            }
                        }
                    ],
                    "synonyms": [
                        "расшириться",
                        "разрастись"
                    ],
                    "common_phrases": [
                        "события развернулись",
                        "борьба развернулась",
                        "работа развернулась"
                    ],
                    "target_lang_definitions": {
                        "en": "To begin to happen actively or develop on a large scale (of events or processes)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unfold",
                                "target_lang_sense_clarification": "for events or stories"
                            },
                            {
                                "target_lang_word": "develop",
                                "target_lang_sense_clarification": "for discussions, situations, or activities"
                            },
                            {
                                "target_lang_word": "get under way",
                                "target_lang_sense_clarification": "to start happening or proceeding"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d14f596-906c-348d-88c5-14cd33a94dba",
                    "sense_definition": "Проявить свои способности, таланты или начать действовать свободно, с полным размахом.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "human_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его талант художника полностью <развернулся> только в зрелости.",
                            "target_lang_translations": {
                                "en": "His talent as an artist fully <blossomed> only in maturity."
                            }
                        },
                        {
                            "text": "На празднике гости <развернулись> и веселились до утра.",
                            "target_lang_translations": {
                                "en": "At the party, the guests <let loose> and had fun until morning."
                            }
                        }
                    ],
                    "synonyms": [
                        "показать себя",
                        "разойтись"
                    ],
                    "antonyms": [
                        "стесняться"
                    ],
                    "common_phrases": [
                        "душа развернулась",
                        "талант развернулся"
                    ],
                    "target_lang_definitions": {
                        "en": "To manifest one's abilities fully or to start acting freely and without restraint."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blossom",
                                "target_lang_sense_clarification": "metaphorical, regarding talent or potential"
                            },
                            {
                                "target_lang_word": "let loose",
                                "target_lang_sense_clarification": "colloquial, to behave freely or party hard"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d5ff054-d2fc-366b-863a-f009255c1700",
                    "sense_definition": "Расположиться боевым порядком, подготовиться к бою (о войсках).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Полк <развернулся> в боевой порядок на поле.",
                            "target_lang_translations": {
                                "en": "The regiment <deployed> into battle formation on the field."
                            }
                        },
                        {
                            "text": "Армия <развернулась> вдоль всей границы.",
                            "target_lang_translations": {
                                "en": "The army <spread out> along the entire border."
                            }
                        }
                    ],
                    "common_phrases": [
                        "фронт развернулся",
                        "строй развернулся"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "military"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To move into a battle formation or prepare for combat (military)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deploy",
                                "target_lang_sense_clarification": "military context"
                            },
                            {
                                "target_lang_word": "spread out",
                                "target_lang_sense_clarification": "to extend over a wider area"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4d0f241-06b1-3505-9595-1e2d8b810928",
                    "sense_definition": "Сильно размахнуться рукой или корпусом для удара.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Боксёр <развернулся> и нанёс сильный удар.",
                            "target_lang_translations": {
                                "en": "The boxer <swung around> and delivered a powerful blow."
                            }
                        },
                        {
                            "text": "Он <развернулся> всем телом, чтобы бросить мяч как можно дальше.",
                            "target_lang_translations": {
                                "en": "He <wound up> his whole body to throw the ball as far as possible."
                            }
                        }
                    ],
                    "synonyms": [
                        "размахнуться"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To rotate the body or arm strongly to prepare for a strike or throw."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "swing around",
                                "target_lang_sense_clarification": "turning body to generate force"
                            },
                            {
                                "target_lang_word": "wind up",
                                "target_lang_sense_clarification": "preparing to throw or punch"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8ac1433-4201-343b-830d-e95e1a943f30",
                    "sense_definition": "Ослабиться, перестать держаться крепко (о винте, гайке).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "От постоянной вибрации винт <развернулся>.",
                            "target_lang_translations": {
                                "en": "Due to constant vibration, the screw <came unscrewed>."
                            }
                        },
                        {
                            "text": "Гайка <развернулась> и упала на пол.",
                            "target_lang_translations": {
                                "en": "The nut <came loose> and fell to the floor."
                            }
                        }
                    ],
                    "synonyms": [
                        "развинтиться"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To become loose or unscrewed (of a nut, screw, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come unscrewed",
                                "target_lang_sense_clarification": "specific to threaded fasteners"
                            },
                            {
                                "target_lang_word": "come loose",
                                "target_lang_sense_clarification": "general loss of tightness"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
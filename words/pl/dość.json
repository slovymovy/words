                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "достаточно",
                                "target_lang_sense_clarification": "достаточное количество"
                            },
                            {
                                "target_lang_word": "хватит",
                                "target_lang_sense_clarification": "имеется в нужном количестве"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "28bb279b-bf5e-35b8-b8b7-0ec9368ce024",
                    "sense_definition": "Wykrzyknienie wyrażające żądanie natychmiastowego zakończenia jakiegoś działania; nakaz spokoju.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "command_stop",
                    "name_type": "no",
                    "examples": [

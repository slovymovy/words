    "entries": [
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "d9a32d9f-e969-3517-b32b-05e3f329343c",
                    "sense_definition": "Союз, указывающий на причину или основание для действия; соответствует по значению словам «так как», «потому что».",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили остаться дома, <w>поскольку</w> на улице шёл сильный дождь.",
                            "target_lang_translations": {
                                "en": "We decided to stay home <w>since</w> it was raining heavily outside."
                            }
                        },
                        {
                            "text": "<w>Поскольку</w> ты уже закончил работу, можешь идти домой.",
                            "target_lang_translations": {
                                "en": "<w>Since</w> you have already finished work, you can go home."
                            }
                        },
                        {
                            "text": "Этот вопрос меня интересует лишь постольку, <w>поскольку</w> он касается моей семьи.",
                            "target_lang_translations": {
                                "en": "This question interests me only insofar <w>as</w> it concerns my family."
                            }
                        }
                    ],
                    "synonyms": [
                        "так как",
                        "ибо"
                    ],
                    "common_phrases": [
                        "постольку-поскольку",
                        "постольку, поскольку"
                    ],
                    "target_lang_definitions": {
                        "en": "A conjunction indicating the reason or basis for an action; corresponds in meaning to \"since\" or \"because\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "since",
                                "target_lang_sense_clarification": "indicating a reason or cause"
                            },
                            {
                                "target_lang_word": "as",
                                "target_lang_sense_clarification": "used to indicate the reason for something"
                            },
                            {
                                "target_lang_word": "because",
                                "target_lang_sense_clarification": "for the reason that"
                            }
                        ]
                    }
                }
            ]
        }
    ]

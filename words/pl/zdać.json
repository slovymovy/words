        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1522b405-2f84-3b14-849a-fa027f986bfc",
                    "sense_definition": "Uzyskać pozytywną ocenę z egzaminu lub testu, co pozwala na zaliczenie przedmiotu lub zakończenie etapu nauki",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "education_pass",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę się dużo uczyć przez weekend, żeby <zdać> ten trudny egzamin z matematyki.",
                            "target_lang_translations": {
                                "ru": "Мне нужно много учиться в выходные, чтобы <сдать> этот трудный экзамен по математике."
                            }
                        },
                        {
                            "text": "Mój starszy brat bardzo się cieszył, gdy w końcu <zdał> egzamin na prawo jazdy.",
                            "target_lang_translations": {
                                "ru": "Мой старший брат был очень рад, когда наконец <сдал> экзамен на водительские права."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaliczyć"
                    ],
                    "antonyms": [
                        "oblać"
                    ],
                    "common_phrases": [
                        "zdać egzamin",
                        "zdać maturę",
                        "zdać za pierwszym razem",
                        "zdać do następnej klasy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получить положительную оценку на экзамене или тесте, что позволяет зачесть предмет или завершить этап обучения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдать",
                                "target_lang_sense_clarification": "пройти испытание, экзамен"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51189373-1d37-36a7-b24a-89e8b1ccc59d",
                    "sense_definition": "Oficjalnie przekazać coś, np. powierzone mienie, stanowisko lub raport, osobie uprawnionej",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "hand_over",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed wyprowadzką musimy posprzątać pokoje i oficjalnie <zdać> mieszkanie właścicielowi.",
                            "target_lang_translations": {
                                "ru": "Перед переездом мы должны убрать комнаты и официально <сдать> квартиру владельцу."
                            }
                        },
                        {
                            "text": "Żołnierz musiał natychmiast <zdać> relację swojemu dowódcy z przebiegu nocnego patrolu.",
                            "target_lang_translations": {
                                "ru": "Солдат должен был немедленно <доложить> своему командиру о ходе ночного патрулирования."
                            }
                        }
                    ],
                    "synonyms": [
                        "złożyć",
                        "zaliczyć",
                        "zaliczyć"
                    ],
                    "antonyms": [
                        "przyjąć",
                        "odebrać"
                    ],
                    "common_phrases": [
                        "zdać relację",
                        "zdać raport",
                        "zdać klucze",
                        "zdać sprzęt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально передать что-либо, например, вверенное имущество, должность или отчет, уполномоченному лицу"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдать",

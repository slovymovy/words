                    "sense_id": "e58e22a7-2dfa-3cb0-9b87-d8545756aa3a",
                    "sense_definition": "Iets naar een andere plaats of een ander tijdstip verplaatsen",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "move_shift",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik moet onze afspraak helaas naar volgende week <w>verzetten</w>.",
                            "target_lang_translations": {
                                "en": "Unfortunately, I have to <w>reschedule</w> our appointment to next week.",
                                "ru": "К сожалению, я должен <w>перенести</w> нашу встречу на следующую неделю."
                            }
                        },
                        {
                            "text": "In het weekend gaan we de klok weer een uur <w>verzetten</w>.",
                            "target_lang_translations": {
                                "en": "Over the weekend, we are going to <w>change</w> the clock by an hour again.",
                                "ru": "В выходные мы снова будем <w>переводить</w> часы на час."
                            }
                        },
                        {
                            "text": "Hij <w>verzette</w> zijn stoel om beter naar de televisie te kunnen kijken.",
                            "target_lang_translations": {
                                "en": "He <w>moved</w> his chair to be able to watch the television better.",
                                "ru": "Он <w>переставил</w> свой стул, чтобы лучше видеть телевизор."
                            }
                        }
                    ],
                    "synonyms": [
                        "verplaatsen",
                        "verschuiven"
                    ],
                    "antonyms": [
                        "laten staan"
                    ],
                    "common_phrases": [
                        "een afspraak verzetten",
                        "de klok verzetten",
                        "de zinnen verzetten"
                    ],
                    "target_lang_definitions": {
                        "en": "To move something to a different place or time",
                        "ru": "Перемещать что-либо в другое место или переносить на другое время"
                    },

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "99ac5a41-63e3-3c60-ae6b-95c437d32d2e",
                    "sense_definition": "The organ of hearing and balance located on the side of the head.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "hearing_organ",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She tucked a strand of hair behind her <w>ear</w>.",
                            "target_lang_translations": {
                                "ru": "Она заправила прядь волос за <w>ухо</w>."
                            }
                        },
                        {
                            "text": "The doctor examined his inner <w>ear</w> to find the cause of the dizziness.",
                            "target_lang_translations": {
                                "ru": "Врач осмотрел его внутреннее <w>ухо</w>, чтобы найти причину головокружения."
                            }
                        }
                    ],
                    "synonyms": [
                        "lug"
                    ],
                    "common_phrases": [
                        "inner ear",
                        "outer ear",
                        "left ear",
                        "right ear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Орган слуха и равновесия, расположенный по бокам головы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ухо",
                                "target_lang_sense_clarification": "орган"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "915602ea-66f0-3fdd-acde-6974eb035cc9",
                    "sense_definition": "The ability to recognize, appreciate, and reproduce sounds, especially music or language.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "hearing_ability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He has a good <w>ear</w> for foreign languages and picks up accents quickly.",
                            "target_lang_translations": {
                                "ru": "У него хороший <w>слух</w> на иностранные языки, и он быстро перенимает акценты."
                            }
                        },
                        {
                            "text": "She can play the piano by <w>ear</w> without needing to read sheet music.",
                            "target_lang_translations": {
                                "ru": "Она умеет играть на пианино на <w>слух</w>, не глядя в ноты."
                            }
                        }
                    ],
                    "common_phrases": [
                        "play by ear",
                        "have an ear for",
                        "good ear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность распознавать, оценивать и воспроизводить звуки, особенно музыку или речь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слух",
                                "target_lang_sense_clarification": "музыкальный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f02dc438-3474-3835-aa9d-bbddb561980a",
                    "sense_definition": "Willingness to listen or pay attention to someone.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am all <w>ears</w>, so please tell me exactly what happened.",
                            "target_lang_translations": {
                                "ru": "Я весь во <w>внимании</w>, так что, пожалуйста, расскажи мне точно, что случилось."
                            }
                        },
                        {
                            "text": "The manager turned a deaf <w>ear</w> to our complaints about the workload.",
                            "target_lang_translations": {
                                "ru": "Менеджер пропустил мимо <w>ушей</w> наши жалобы на рабочую нагрузку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "all ears",
                        "lend an ear",
                        "turn a deaf ear",
                        "give ear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовность выслушать кого-либо или уделить внимание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внимание",
                                "target_lang_sense_clarification": "готовность слушать"
                            },
                            {
                                "target_lang_word": "ухо",
                                "target_lang_sense_clarification": "в идиомах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "64bbb062-e75b-3aeb-8453-e786d01268a1",
                    "sense_definition": "The top part of a grain plant, such as corn or wheat, that contains the seeds.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "plant_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer harvested every <w>ear</w> of corn from the field.",
                            "target_lang_translations": {
                                "ru": "Фермер собрал каждый <w>початок</w> кукурузы с поля."
                            }
                        },
                        {
                            "text": "A golden <w>ear</w> of wheat swayed gently in the summer breeze.",
                            "target_lang_translations": {
                                "ru": "Золотистый <w>колос</w> пшеницы мягко покачивался на летнем ветру."
                            }
                        }
                    ],
                    "synonyms": [
                        "head",
                        "spike"
                    ],
                    "common_phrases": [
                        "ear of corn",
                        "ear of wheat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхняя часть зернового растения, содержащая семена."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колос",
                                "target_lang_sense_clarification": "пшеницы, ржи"
                            },
                            {
                                "target_lang_word": "початок",
                                "target_lang_sense_clarification": "кукурузы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12509da6-dc01-34e1-8c7d-a74d857d608d",
                    "sense_definition": "A handle or projecting part on an object that resembles an ear.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "object_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He lifted the heavy pot by grasping the <w>ears</w> on either side.",
                            "target_lang_translations": {
                                "ru": "Он поднял тяжелую кастрюлю, ухватившись за <w>ручки</w> с обеих сторон."
                            }
                        },
                        {
                            "text": "One <w>ear</w> of the ceramic pitcher had broken off years ago.",
                            "target_lang_translations": {
                                "ru": "Одно <w>ушко</w> керамического кувшина отломилось много лет назад."
                            }
                        }
                    ],
                    "synonyms": [
                        "lug",
                        "listener"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ручка или выступающая часть предмета, напоминающая ухо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ушко",
                                "target_lang_sense_clarification": "предмета"
                            },
                            {
                                "target_lang_word": "ручка",
                                "target_lang_sense_clarification": "кастрюли, кувшина"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "def3e0e8-15b4-3fc5-a7fa-955fba1b843b",
                    "sense_definition": "To grow or form ears (of a grain plant).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "plant_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The corn began to <w>ear</w> early this season due to the warm weather.",
                            "target_lang_translations": {
                                "ru": "Кукуруза начала <w>образовывать початки</w> в этом сезоне рано из-за теплой погоды."
                            }
                        },
                        {
                            "text": "Farmers worry when the wheat <w>ears</w> too late in the autumn.",
                            "target_lang_translations": {
                                "ru": "Фермеры беспокоятся, когда пшеница <w>колосится</w> слишком поздно осенью."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Образовывать колосья (о зерновых растениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колоситься",
                                "target_lang_sense_clarification": "о злаках"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
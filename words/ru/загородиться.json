            "senses": [
                {
                    "sense_id": "1e1c6814-b2b6-3bc5-9896-6d8f8319be60",
                    "sense_definition": "Заслонить себя чем-либо, чтобы спрятаться или защититься от кого-либо или чего-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "shielding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он успел <w>загородиться</w> рукой от яркого света фар.",
                            "target_lang_translations": {
                                "en": "He managed to <w>shield himself</w> with his hand from the bright headlights."
                            }
                        },
                        {
                            "text": "Девушка попыталась <w>загородиться</w> книгой от любопытных взглядов пассажиров.",
                            "target_lang_translations": {
                                "en": "The girl tried to <w>screen herself</w> with a book from the passengers' curious glances."
                            }
                        }
                    ],
                    "synonyms": [
                        "заслониться",
                        "прикрыться",
                        "закрыться"
                    ],
                    "antonyms": [
                        "открыться"
                    ],
                    "common_phrases": [
                        "загородиться рукой",
                        "загородиться от света",
                        "загородиться от удара"
                    ],
                    "target_lang_definitions": {
                        "en": "To shield or cover oneself with an object for protection or concealment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shield oneself",
                                "target_lang_sense_clarification": "protect from light, blows, or view"
                            },
                            {
                                "target_lang_word": "cover oneself",
                                "target_lang_sense_clarification": "physically block something"
                            },
                            {
                                "target_lang_word": "screen oneself",
                                "target_lang_sense_clarification": "hide behind something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6e2fe99-41e6-348a-a9a2-1fc6860051d8",
                    "sense_definition": "Отделить себя от окружающего пространства, устроив какую-либо преграду, забор или стену.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Соседи решили <w>загородиться</w> от нас высоким глухим забором.",
                            "target_lang_translations": {
                                "en": "The neighbors decided to <w>fence themselves off</w> from us with a high solid fence."
                            }
                        },
                        {
                            "text": "Мы хотим <w>загородиться</w> ширмой в углу комнаты, чтобы спокойно работать.",
                            "target_lang_translations": {
                                "en": "We want to <w>screen ourselves off</w> with a partition in the corner of the room to work in peace."
                            }
                        }
                    ],
                    "synonyms": [
                        "отгородиться",
                        "изолироваться"
                    ],
                    "common_phrases": [
                        "загородиться забором",
                        "загородиться стеной",
                        "загородиться ширмой"
                    ],
                    "target_lang_definitions": {
                        "en": "To separate oneself from the surroundings by erecting a barrier, fence, or wall."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fence oneself off",
                                "target_lang_sense_clarification": "using a fence"
                            },
                            {
                                "target_lang_word": "screen oneself off",
                                "target_lang_sense_clarification": "using a partition or screen"
                            },
                            {
                                "target_lang_word": "wall oneself off",
                                "target_lang_sense_clarification": "using a wall"
                            }
                        ]
                    }
                }
            ]
        }

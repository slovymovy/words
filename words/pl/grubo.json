{
    "word_family": [
        "gruby",
        "grubość",
        "grubas",
        "zgrubienie",
        "pogrubić",
        "grubiański"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "b80633d9-6bbf-3aca-a6db-25406689af23",
                    "sense_definition": "W sposób tworzący grubą warstwę lub z użyciem wielu warstw ubrań.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_layer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mama posmarowała mi chleb <grubo> masłem.",
                            "target_lang_translations": {
                                "ru": "Мама намазала мне хлеб <w>толстым слоем</w> масла."
                            }
                        },
                        {
                            "text": "Musisz się <grubo> ubrać, bo na zewnątrz jest mróz.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>тепло</w> одеться, потому что на улице мороз."
                            }
                        }
                    ],
                    "synonyms": [
                        "obficie"
                    ],
                    "common_phrases": [
                        "ubrać się grubo",
                        "posmarować grubo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образуя толстый слой или надевая много слоев одежды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "толстым слоем",
                                "target_lang_sense_clarification": "о нанесении вещества"
                            },
                            {
                                "target_lang_word": "тепло",
                                "target_lang_sense_clarification": "об одежде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09d6e728-046a-3e09-9e2c-6146889821f7",
                    "sense_definition": "W bardzo dużym stopniu; znacznie (często używane w odniesieniu do czasu, liczb lub pomyłek).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Impreza skończyła się <grubo> po północy.",
                            "target_lang_translations": {
                                "ru": "Вечеринка закончилась <w>далеко</w> за полночь."
                            }
                        },
                        {
                            "text": "Jeśli myślisz, że ci pożyczę pieniądze, to się <grubo> mylisz.",
                            "target_lang_translations": {
                                "ru": "Если ты думаешь, что я одолжу тебе денег, то ты <w>глубоко</w> ошибаешься."
                            }
                        }
                    ],
                    "synonyms": [
                        "bardzo",
                        "dużo",
                        "znacznie"
                    ],
                    "common_phrases": [
                        "grubo po",
                        "grubo się mylić",
                        "grubo przesadzić"
                    ],
                    "target_lang_definitions": {
                        "ru": "В очень большой степени; значительно (часто о времени, числах или ошибках)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сильно",
                                "target_lang_sense_clarification": "об ошибках или заблуждениях"
                            },
                            {
                                "target_lang_word": "глубоко",
                                "target_lang_sense_clarification": "об убежденности или ошибках"
                            },
                            {
                                "target_lang_word": "далеко",
                                "target_lang_sense_clarification": "о времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45437f2f-deb2-365b-9b1a-610eccc0599d",
                    "sense_definition": "Na duże, niedrobne kawałki.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_pieces",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do tej zupy warzywa powinny być pokrojone <grubo>.",
                            "target_lang_translations": {
                                "ru": "Для этого супа овощи должны быть нарезаны <w>крупно</w>."
                            }
                        },
                        {
                            "text": "Lubię, gdy kawa jest zmielona dość <grubo>.",
                            "target_lang_translations": {
                                "ru": "Я люблю, когда кофе помолот довольно <w>крупно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "niedrobno"
                    ],
                    "common_phrases": [
                        "pokroić grubo",
                        "zmielić grubo"
                    ],
                    "target_lang_definitions": {
                        "ru": "На крупные, не мелкие куски."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крупно",
                                "target_lang_sense_clarification": "о нарезке или помоле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc460e29-778a-346d-9508-b441d7133d7c",
                    "sense_definition": "W sposób świadczący o dużej tuszy; wyglądając na osobę otyłą.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym płaszczu wyglądasz trochę <grubo>.",
                            "target_lang_translations": {
                                "ru": "В этом пальто ты выглядишь немного <w>полновато</w>."
                            }
                        },
                        {
                            "text": "Na zdjęciu wyszedł bardzo <grubo> i niekorzystnie.",
                            "target_lang_translations": {
                                "ru": "На фото он получился очень <w>толстым</w> и неудачно."
                            }
                        }
                    ],
                    "synonyms": [
                        "otyle",
                        "tęgo",
                        "korpulentnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выглядя полным, тучным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полно",
                                "target_lang_sense_clarification": "о внешнем виде"
                            },
                            {
                                "target_lang_word": "толсто",
                                "target_lang_sense_clarification": "разговорное, о внешности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36851cf9-745d-3e79-804e-af2d28bd8a7c",
                    "sense_definition": "Niskim, basowym głosem lub tonem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pies sąsiada szczekał <grubo> i groźnie.",
                            "target_lang_translations": {
                                "ru": "Собака соседа лаяла <w>басом</w> и грозно."
                            }
                        },
                        {
                            "text": "Mężczyzna zaśmiał się <grubo>, co nas przestraszyło.",
                            "target_lang_translations": {
                                "ru": "Мужчина засмеялся <w>басом</w>, что нас напугало."
                            }
                        }
                    ],
                    "synonyms": [
                        "nisko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Низким, басовым голосом или тоном."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "басом",
                                "target_lang_sense_clarification": "о голосе"
                            },
                            {
                                "target_lang_word": "низко",
                                "target_lang_sense_clarification": "о тоне звука"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d153dbb3-7d08-3e7b-b08b-5774fa7523eb",
                    "sense_definition": "W sposób niekulturalny, nieprzyzwoity lub prostacki.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "manner_colloquial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tym żartem pojechałeś trochę za <grubo>.",
                            "target_lang_translations": {
                                "ru": "С этой шуткой ты перегнул, вышло слишком <w>жестко</w>."
                            }
                        },
                        {
                            "text": "Zachował się <grubo> wobec swoich rodziców.",
                            "target_lang_translations": {
                                "ru": "Он повел себя <w>по-хамски</w> по отношению к своим родителям."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieprzyzwoicie",
                        "prostacko",
                        "wulgarnie"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Некультурно, непристойно или вызывающе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жестко",
                                "target_lang_sense_clarification": "сленг, о шутках или критике"
                            },
                            {
                                "target_lang_word": "по-хамски",
                                "target_lang_sense_clarification": "о поведении"
                            },
                            {
                                "target_lang_word": "грубо",
                                "target_lang_sense_clarification": "о манерах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "aaaa5a33-0295-30e2-a59e-78b0c3ea9990",
                    "sense_definition": "Wykrzyknienie wyrażające zdziwienie, podziw lub komentarz do niezwykłej, szokującej sytuacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ale <grubo>! Nie wiedziałem, że potrafisz tak grać.",
                            "target_lang_translations": {
                                "ru": "Ну и <w>жесть</w>! Я не знал, что ты умеешь так играть."
                            }
                        },
                        {
                            "text": "<Grubo>, wygraliście ten mecz w ostatniej sekundzie.",
                            "target_lang_translations": {
                                "ru": "<w>Мощно</w>, вы выиграли этот матч на последней секунде."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieźle",
                        "no ładnie"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Восклицание, выражающее удивление или шок от необычной ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жесть",
                                "target_lang_sense_clarification": "сленг, о шокирующей ситуации"
                            },
                            {
                                "target_lang_word": "круто",
                                "target_lang_sense_clarification": "сленг, восхищение"
                            },
                            {
                                "target_lang_word": "сильно",
                                "target_lang_sense_clarification": "о впечатляющем событии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
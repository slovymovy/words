            "senses": [
                {
                    "sense_id": "741170b8-41c3-30c6-9bf5-effdd7d469ce",
                    "sense_definition": "Wydostać się skokiem z wnętrza czegoś na zewnątrz.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój kot zwinnie <w>wyskoczył</w> przez otwarte okno na ogród.",
                            "target_lang_translations": {
                                "ru": "Мой кот ловко <w>выпрыгнул</w> через открытое окно в сад."
                            }
                        },
                        {
                            "text": "Złodziej <w>wyskoczył</w> z pędzącego pociągu, by uniknąć aresztowania.",
                            "target_lang_translations": {
                                "ru": "Вор <w>выпрыгнул</w> из мчащегося поезда, чтобы избежать ареста."
                            }
                        }
                    ],
                    "synonyms": [
                        "wydostać się"
                    ],
                    "antonyms": [
                        "wskoczyć"
                    ],
                    "common_phrases": [
                        "wyskoczyć przez okno",
                        "wyskoczyć z łóżka",
                        "wyskoczyć z pociągu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выбраться прыжком изнутри наружу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выпрыгнуть",
                                "target_lang_sense_clarification": "совершить прыжок наружу"
                            },
                            {
                                "target_lang_word": "выскочить",
                                "target_lang_sense_clarification": "быстро выбежать или выпрыгнуть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0215a879-6991-3740-b797-9b2b960d9c44",
                    "sense_definition": "Udać się gdzieś na krótko, zwykle w pośpiechu lub w celach towarzyskich.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "social_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę tylko na chwilę <w>wyskoczyć</w> do sklepu po mleko.",
                            "target_lang_translations": {
                                "ru": "Мне нужно только на минутку <w>выскочить</w> в магазин за молоком."
                            }
                        },
                        {
                            "text": "W ten weekend chętnie <w>wyskoczymy</w> w góry, żeby odpocząć.",
                            "target_lang_translations": {
                                "ru": "В эти выходные мы с удовольствием <w>выберемся</w> в горы, чтобы отдохнуть."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyjść",
                        "wpaść"
                    ],
                    "common_phrases": [
                        "wyskoczyć na kawę",
                        "wyskoczyć do sklepu",
                        "wyskoczyć na miasto",
                        "wyskoczyć na piwo"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отлучиться куда-либо ненадолго, обычно в спешке или для встречи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выскочить",
                                "target_lang_sense_clarification": "разг. ненадолго выйти куда-то"
                            },
                            {
                                "target_lang_word": "сбегать",
                                "target_lang_sense_clarification": "быстро сходить куда-то и вернуться"
                            },
                            {
                                "target_lang_word": "выбраться",
                                "target_lang_sense_clarification": "найти время пойти куда-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83138d94-62e9-3cb7-9b5c-733116ce16ca",
                    "sense_definition": "Pojawić się nagle lub niespodziewanie (np. o zmianach na skórze lub komunikatach).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed ważnym spotkaniem na czole <w>wyskoczyła</w> mi wielka krosta.",
                            "target_lang_translations": {
                                "ru": "Перед важной встречей у меня на лбу <w>выскочил</w> огромный прыщ."
                            }
                        },
                        {
                            "text": "Podczas pracy na ekranie komputera nagle <w>wyskoczył</w> błąd systemu.",
                            "target_lang_translations": {
                                "ru": "Во время работы на экране компьютера внезапно <w>выскочила</w> системная ошибка."
                            }
                        }
                    ],
                    "synonyms": [
                        "pojawić się",
                        "wystąpić"
                    ],
                    "antonyms": [
                        "zniknąć"
                    ],
                    "common_phrases": [
                        "wyskoczył błąd",
                        "wyskoczyła krosta",
                        "wyskoczył pryszcz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Появиться внезапно или неожиданно (напр., о высыпаниях на коже или сообщениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выскочить",
                                "target_lang_sense_clarification": "появиться (о прыще, ошибке)"
                            },
                            {
                                "target_lang_word": "всплыть",
                                "target_lang_sense_clarification": "появиться на экране"
                            }
                        ]
                    }
                }
            ]
        }

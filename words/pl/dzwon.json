    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8ec3ea01-7135-331a-88a7-3570dcf6a913",
                    "sense_definition": "Duży, metalowy przedmiot w kształcie odwróconego kielicha, który wydaje donośny dźwięk po uderzeniu sercem lub młotkiem, używany często w kościołach lub jako sygnał.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "bell_object_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary <dzwon> na wieży kościelnej wybijał głośno południe.",
                            "target_lang_translations": {
                                "ru": "Старый <колокол> на церковной башне громко отбивал полдень."
                            }
                        },
                        {
                            "text": "Mieszkańcy ufundowali nowy <dzwon>, który zawisł w ratuszu.",
                            "target_lang_translations": {
                                "ru": "Жители пожертвовали средства на новый <колокол>, который повесили в ратуше."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bić w dzwony",
                        "dzwon kościelny",
                        "serce dzwonu",
                        "głośny jak dzwon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой металлический предмет в форме перевернутой чаши, который издает громкий звук при ударе языком или молотком, часто используется в церквях или в качестве сигнала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колокол",
                                "target_lang_sense_clarification": "предмет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c691c63b-b570-3e51-83e9-b8cd1e348933",
                    "sense_definition": "Dźwięk wytwarzany przez uderzanie w dzwon.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "bell_object_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z oddali słychać było ponury <dzwon> żegnający zmarłego.",
                            "target_lang_translations": {
                                "ru": "Вдали был слышен мрачный <звон>, провожающий покойного."
                            }
                        },
                        {
                            "text": "Radosny <dzwon> obwieścił wszystkim początek świąt.",
                            "target_lang_translations": {
                                "ru": "Радостный <звон> возвестил всем о начале праздников."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук, возникающий при ударе в колокол."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звон",
                                "target_lang_sense_clarification": "звук"
                            },
                            {
                                "target_lang_word": "колокольный звон",
                                "target_lang_sense_clarification": "звучание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f4abb67-6ac2-3efb-8aa1-01ff79ca6f88",
                    "sense_definition": "Kolizja drogowa lub wypadek samochodowy, zwykle skutkujący uszkodzeniem pojazdu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek zaliczył wczoraj solidnego <dzwona>, gdy wpadł w poślizg na moście.",
                            "target_lang_translations": {
                                "ru": "Марек вчера попал в серьезную <аварию>, когда его занесло на мосту."
                            }
                        },
                        {
                            "text": "To cud, że kierowca wyszedł z tego <dzwona> bez szwanku.",
                            "target_lang_translations": {
                                "ru": "Это чудо, что водитель вышел из этой <аварии> без царапины."
                            }
                        }
                    ],
                    "synonyms": [
                        "stłuczka",
                        "kraksa",
                        "kolizja"
                    ],
                    "common_phrases": [
                        "mieć dzwona",
                        "zaliczyć dzwona"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дорожно-транспортное происшествие или автомобильная авария, обычно приводящая к повреждению транспортного средства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "авария",
                                "target_lang_sense_clarification": "разг."
                            },
                            {
                                "target_lang_word": "столкновение",
                                "target_lang_sense_clarification": "ДТП"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1807d0f-8fb9-375e-8f3f-cbb2ef4cb7bb",
                    "sense_definition": "Forma gramatyczna (dopełniacz liczby mnogiej) rzeczownika <w>dzwono</w> (kawałek ryby).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wigilijną kolację usmażyliśmy kilka <dzwon> karpia.",
                            "target_lang_translations": {
                                "ru": "На рождественский ужин мы пожарили несколько <кусков> карпа."
                            }
                        },
                        {
                            "text": "W sklepie rybnym zabrakło już świeżych <dzwon> łososia.",
                            "target_lang_translations": {
                                "ru": "В рыбном магазине уже не осталось свежих <стейков> лосося."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural of 'dzwono'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грамматическая форма (родительный падеж множественного числа) существительного <w>dzwono</w> (кусок рыбы)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кусков",
                                "target_lang_sense_clarification": "рыбы"
                            },
                            {
                                "target_lang_word": "стейков",
                                "target_lang_sense_clarification": "рыбных"
                            }
                        ]
                    }
                }
            ]
        }
    ]

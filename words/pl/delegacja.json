    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d06d311d-6a6f-387a-b90e-19530c419297",
                    "sense_definition": "Służbowy wyjazd pracownika w celu wykonania zadań poza stałym miejscem pracy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "trip_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja mama pojechała w <w>delegację</w> do Łodzi na dwa dni.",
                            "target_lang_translations": {
                                "ru": "Моя мама поехала в <w>командировку</w> в Лодзь на два дня."
                            }
                        },
                        {
                            "text": "W przyszłym tygodniu czeka mnie długa <w>delegacja</w> zagraniczna.",
                            "target_lang_translations": {
                                "ru": "На следующей неделе меня ждет длительная заграничная <w>командировка</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "deputacja"
                    ],
                    "common_phrases": [
                        "jechać w delegację",
                        "być w delegacji",
                        "wrócić z delegacji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служебная поездка работника для выполнения задач вне постоянного места работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "командировка",
                                "target_lang_sense_clarification": "служебная поездка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1522808-eb67-38e6-bd6e-52ccda720b28",
                    "sense_definition": "Grupa osób oficjalnie reprezentująca jakąś instytucję, organizację lub państwo.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Delegacja</w> firmy uczestniczyła w ważnych rozmowach z władzami miasta.",
                            "target_lang_translations": {
                                "ru": "<w>Делегация</w> фирмы участвовала в важных переговорах с властями города."
                            }
                        },
                        {
                            "text": "Zagraniczna <w>delegacja</w> została powitana przez prezydenta na lotnisku.",
                            "target_lang_translations": {
                                "ru": "Иностранная <w>делегация</w> была встречена президентом в аэропорту."
                            }
                        }
                    ],
                    "synonyms": [
                        "przedstawicielstwo",
                        "deputacja"
                    ],
                    "common_phrases": [
                        "delegacja rządowa",
                        "polska delegacja",
                        "przyjąć delegację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа лиц, официально представляющая какую-либо организацию или государство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делегация",
                                "target_lang_sense_clarification": "группа представителей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ba3afcb-3606-3fe0-bc35-b666faf04e47",
                    "sense_definition": "Dokument formalny potwierdzający odbycie podróży służbowej i uprawniający do zwrotu kosztów.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "trip_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy może mi pani podstemplować <w>delegację</w> w sekretariacie?",
                            "target_lang_translations": {
                                "ru": "Не могли бы вы поставить печать на <w>командировочном удостоверении</w> в секретариате?"
                            }
                        },
                        {
                            "text": "Muszę wypełnić <w>delegację</w>, aby otrzymać zwrot kosztów podróży.",
                            "target_lang_translations": {
                                "ru": "Мне нужно заполнить <w>командировочное удостоверение</w>, чтобы получить возмещение расходов на проезд."
                            }
                        }
                    ],
                    "common_phrases": [
                        "podpisać delegację",
                        "rozliczyć delegację",
                        "wypełnić delegację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальный документ, подтверждающий служебную поездку и дающий право на возмещение расходов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "командировочное удостоверение",
                                "target_lang_sense_clarification": "документ"
                            },
                            {
                                "target_lang_word": "командировочный лист",
                                "target_lang_sense_clarification": "документ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ecffdc8-0f1a-39aa-85c3-81fee0d926c3",
                    "sense_definition": "Środki pieniężne wypłacane pracownikowi na pokrycie kosztów wyjazdu służbowego.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "trip_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Firma wypłaciła mi pieniądze za ostatnią <w>delegację</w> z dużym opóźnieniem.",
                            "target_lang_translations": {
                                "ru": "Фирма выплатила мне деньги за последнюю <w>командировку</w> с большим опозданием."
                            }
                        },
                        {
                            "text": "Koszty <w>delegacji</w> obejmują diety oraz zwrot za noclegi i przejazdy.",
                            "target_lang_translations": {
                                "ru": "Расходы на <w>командировку</w> включают суточные, а также возмещение за проживание и проезд."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zwrot za delegację",
                        "koszty delegacji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежные средства, выплачиваемые работнику на покрытие расходов во время служебной поездки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "командировочные",
                                "target_lang_sense_clarification": "денежные средства"
                            },
                            {
                                "target_lang_word": "суточные",
                                "target_lang_sense_clarification": "часть командировочных расходов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fdd13cad-31cc-391e-96c6-fd1d77eba308",
                    "sense_definition": "Przekazanie uprawnień lub kompetencji innemu organowi władzy lub urzędnikowi.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ustawa zawiera <w>delegację</w> dla ministra do wydania odpowiedniego rozporządzenia.",
                            "target_lang_translations": {
                                "ru": "Закон содержит <w>делегирование</w> полномочий министру для издания соответствующего постановления."
                            }
                        },
                        {
                            "text": "Nastąpiła <w>delegacja</w> zadań na niższy szczebel administracji samorządowej.",
                            "target_lang_translations": {
                                "ru": "Произошло <w>делегирование</w> задач на более низкий уровень местного самоуправления."
                            }
                        }
                    ],
                    "common_phrases": [
                        "delegacja ustawowa",
                        "delegacja uprawnień"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "urzędowy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Передача полномочий или компетенций другому органу власти или должностному лицу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делегирование",
                                "target_lang_sense_clarification": "передача полномочий"
                            },
                            {
                                "target_lang_word": "передача",
                                "target_lang_sense_clarification": "прав или обязанностей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2671dc9a-7b04-3dd6-b511-2809129911c5",
                    "sense_definition": "Mechanizm w programowaniu polegający na przekazaniu wykonania zadania innemu obiektowi.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym projekcie wykorzystujemy <w>delegację</w> zdarzeń do obsługi interfejsu.",
                            "target_lang_translations": {
                                "ru": "В этом проекте мы используем <w>делегирование</w> событий для обслуживания интерфейса."
                            }
                        },
                        {
                            "text": "<w>Delegacja</w> jest często stosowaną alternatywą dla dziedziczenia w programowaniu obiektowym.",
                            "target_lang_translations": {
                                "ru": "<w>Делегирование</w> является часто используемой альтернативой наследованию в объектно-ориентированном программировании."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wzorzec delegacji"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "informatyka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Механизм в программировании, заключающийся в передаче выполнения задачи другому объекту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делегирование",
                                "target_lang_sense_clarification": "шаблон проектирования"
                            }
                        ]
                    }
                }
            ]
        }
    ]

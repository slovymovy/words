    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fb968a3f-9046-34ab-a283-552782a5aacf",
                    "sense_definition": "Having no use or unable to be used effectively.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "functional_utility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This fork's prongs are bent, so it is <w>useless</w> now.",
                            "target_lang_translations": {
                                "ru": "Зубцы этой вилки погнуты, поэтому теперь она <w>бесполезна</w>."
                            }
                        },
                        {
                            "text": "The broken phone is completely <w>useless</w> without a battery.",
                            "target_lang_translations": {
                                "ru": "Сломанный телефон абсолютно <w>бесполезен</w> без аккумулятора."
                            }
                        }
                    ],
                    "synonyms": [
                        "unusable",
                        "unutilizable"
                    ],
                    "antonyms": [
                        "useful"
                    ],
                    "common_phrases": [
                        "useless object",
                        "completely useless",
                        "render useless"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непригодный для использования или неэффективный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бесполезный",
                                "target_lang_sense_clarification": "не приносящий пользы"
                            },
                            {
                                "target_lang_word": "непригодный",
                                "target_lang_sense_clarification": "невозможный для использования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a515abfd-0099-3a3b-a5a9-1c3684c8c0d3",
                    "sense_definition": "Producing no result or effect; pointless.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "functional_utility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I think it is <w>useless</w> to keep this discussion going.",
                            "target_lang_translations": {
                                "ru": "Я думаю, <w>бесполезно</w> продолжать это обсуждение."
                            }
                        },
                        {
                            "text": "It is <w>useless</w> to argue with him when he has made up his mind.",
                            "target_lang_translations": {
                                "ru": "<w>Бесполезно</w> спорить с ним, когда он уже всё решил."
                            }
                        }
                    ],
                    "synonyms": [
                        "good-for-nothing",
                        "unusable"
                    ],
                    "antonyms": [
                        "useful",
                        "skillful"
                    ],
                    "common_phrases": [
                        "useless to try",
                        "useless attempt",
                        "useless effort"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не приносящий результата или эффекта; бессмысленный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бесполезный",
                                "target_lang_sense_clarification": "тщетный, напрасный"
                            },
                            {
                                "target_lang_word": "бессмысленный",
                                "target_lang_sense_clarification": "лишенный смысла"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "604ec8ea-7fe2-36f4-a0a8-982a244e3e7c",
                    "sense_definition": "Lacking skill or competence in a particular activity.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "competence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My brother is <w>useless</w> at most computer games.",
                            "target_lang_translations": {
                                "ru": "Мой брат <w>бесполезен</w> в большинстве компьютерных игр."
                            }
                        },
                        {
                            "text": "I am completely <w>useless</w> at cooking dinner for guests.",
                            "target_lang_translations": {
                                "ru": "Я совершенно <w>бесполезна</w> в приготовлении ужина для гостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "unskillful"
                    ],
                    "antonyms": [
                        "skillful"
                    ],
                    "common_phrases": [
                        "useless at"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "mildly insulting"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Не обладающий навыками или компетентностью в определенной деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "никчёмный",
                                "target_lang_sense_clarification": "неумелый"
                            },
                            {
                                "target_lang_word": "бесполезный",
                                "target_lang_sense_clarification": "некомпетентный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45a58518-0798-312f-9a82-5601ff931b9c",
                    "sense_definition": "Lacking ability, purpose, or dependability as a person.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "competence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bill never mows the lawn or helps out; he is <w>useless</w>.",
                            "target_lang_translations": {
                                "ru": "Билл никогда не стрижет газон и не помогает; он <w>никчёмный</w>."
                            }
                        },
                        {
                            "text": "Don't ask him for help because he is <w>useless</w>.",
                            "target_lang_translations": {
                                "ru": "Не проси его о помощи, потому что он <w>бесполезный</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "good-for-nothing"
                    ],
                    "antonyms": [
                        "useful"
                    ],
                    "common_phrases": [
                        "useless person",
                        "totally useless"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often derogatory"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Неспособный, бесцельный или ненадежный (о человеке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "никчёмный",
                                "target_lang_sense_clarification": "никуда не годный (о человеке)"
                            },
                            {
                                "target_lang_word": "бесполезный",
                                "target_lang_sense_clarification": "ненадежный"
                            }
                        ]
                    }
                }
            ]
        }
    ]

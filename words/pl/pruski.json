    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "871f81ab-8130-300d-82de-3c3b4391b6a3",
                    "sense_definition": "związany z Prusami (historycznym państwem niemieckim lub krainą) lub ich mieszkańcami",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "state_region",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zabór <w>pruski</w> obejmował znaczną część zachodnich ziem dawnej Rzeczypospolitej.",
                            "target_lang_translations": {
                                "ru": "<w>Прусский</w> раздел охватывал значительную часть западных земель бывшей Речи Посполитой."
                            }
                        },
                        {
                            "text": "Charakterystyczny mur <w>pruski</w> nadaje starym budynkom wyjątkowy i rozpoznawalny wygląd.",
                            "target_lang_translations": {
                                "ru": "Характерный <w>прусский</w> фахверк придает старым зданиям уникальный и узнаваемый вид."
                            }
                        }
                    ],
                    "synonyms": [
                        "prusacki"
                    ],
                    "common_phrases": [
                        "mur pruski",
                        "błękit pruski",
                        "armia pruska",
                        "król pruski"
                    ],
                    "target_lang_definitions": {
                        "ru": "связанный с Пруссией (историческим немецким государством) или её жителями"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прусский",
                                "target_lang_sense_clarification": "относящийся к Пруссии"
                            },
                            {
                                "target_lang_word": "пруссацкий",
                                "target_lang_sense_clarification": "свойственный пруссакам (часто с негативным оттенком)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f38417b3-0b41-38d5-b9b7-11a940f5c9a4",
                    "sense_definition": "odnoszący się do Prusów, ludu bałtyjskiego zamieszkującego dawniej południowo-wschodnie wybrzeża Bałtyku",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "baltic_tribe",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dawne plemiona <w>pruskie</w> czciły siły natury w świętych gajach.",
                            "target_lang_translations": {
                                "ru": "Древние <w>прусские</w> племена поклонялись силам природы в священных рощах."
                            }
                        },
                        {
                            "text": "Archeolodzy odkryli w tym regionie cenne <w>pruskie</w> ozdoby z brązu.",
                            "target_lang_translations": {
                                "ru": "Археологи обнаружили в этом регионе ценные <w>прусские</w> бронзовые украшения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "plemiona pruskie",
                        "mitologia pruska"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "dotyczy ludu wymarłego lub historycznego"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "относящийся к пруссам, балтийскому народу, населявшему в прошлом юго-восточное побережье Балтики"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прусский",
                                "target_lang_sense_clarification": "относящийся к древним пруссам"
                            },
                            {
                                "target_lang_word": "древнепрусский",
                                "target_lang_sense_clarification": "исторический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "13872876-b55e-379f-ad31-dbb98e6c03f5",
                    "sense_definition": "wymarły język z grupy bałtyckiej, którym posługiwali się dawni Prusowie",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "language_name",
                    "examples": [
                        {
                            "text": "Lingwiści starają się odtworzyć <w>pruski</w> na podstawie zachowanych dokumentów i słowników.",
                            "target_lang_translations": {
                                "ru": "Лингвисты пытаются восстановить <w>прусский</w> язык на основе сохранившихся документов и словарей."
                            }
                        },
                        {
                            "text": "Chociaż <w>pruski</w> wymarł w XVII wieku, pozostawił ślady w nazwach geograficznych.",
                            "target_lang_translations": {
                                "ru": "Хотя <w>прусский</w> язык вымер в XVII веке, он оставил следы в географических названиях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "język pruski"
                    ],
                    "target_lang_definitions": {
                        "ru": "вымерший язык балтийской группы, на котором говорили древние пруссы"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прусский язык",
                                "target_lang_sense_clarification": "лингвистика"
                            },
                            {
                                "target_lang_word": "прусский",
                                "target_lang_sense_clarification": "разговорное сокращение"
                            }
                        ]
                    }
                }
            ]
        }

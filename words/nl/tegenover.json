            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "d36d5221-4568-3837-993e-7e3d1a519418",
                    "sense_definition": "Aan de overzijde van; aan de andere kant van een ruimte, straat of object.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "spatial_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Tegenover</w> de supermarkt staat een groot bankgebouw.",
                            "target_lang_translations": {
                                "en": "A large bank building stands <w>opposite</w> the supermarket.",
                                "ru": "<w>Напротив</w> супермаркета стоит большое здание банка."
                            }
                        },
                        {
                            "text": "Hij ging in de trein recht <w>tegenover</w> mij zitten.",
                            "target_lang_translations": {
                                "en": "He sat down right <w>opposite</w> me on the train.",
                                "ru": "Он сел в поезде прямо <w>напротив</w> меня."
                            }
                        }
                    ],
                    "synonyms": [
                        "aan de overkant van"
                    ],
                    "common_phrases": [
                        "recht tegenover",
                        "schuin tegenover",
                        "pal tegenover"
                    ],
                    "target_lang_definitions": {
                        "en": "Across from; on the other side of a space, street, or object.",
                        "ru": "На другой стороне от места, улицы или объекта; лицом к лицу."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "opposite",
                                "target_lang_sense_clarification": "spatial position"
                            },
                            {
                                "target_lang_word": "across from",
                                "target_lang_sense_clarification": "location relative to something else"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "напротив",
                                "target_lang_sense_clarification": "пространственное расположение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "baed3846-6886-38da-8579-bbb3f5ae385e",
                    "sense_definition": "In relatie tot; ten opzichte van iemand of iets (vaak gebruikt bij gevoelens, houding of verplichtingen).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "relational",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik voelde me een beetje opgelaten <w>tegenover</w> de medewerkers van het kantoor.",
                            "target_lang_translations": {
                                "en": "I felt a bit awkward <w>towards</w> the office employees.",
                                "ru": "Я чувствовал себя немного неловко <w>перед</w> сотрудниками офиса."
                            }
                        },
                        {
                            "text": "Je moet altijd eerlijk zijn <w>tegenover</w> je vrienden en familie.",
                            "target_lang_translations": {
                                "en": "You must always be honest <w>with</w> your friends and family.",
                                "ru": "Нужно всегда быть честным <w>по отношению к</w> своим друзьям и семье."
                            }
                        }
                    ],
                    "synonyms": [
                        "ten opzichte van",
                        "jegens"
                    ],
                    "common_phrases": [
                        "eerlijk tegenover",
                        "schuldig tegenover",
                        "verplichtingen tegenover"
                    ],
                    "target_lang_definitions": {
                        "en": "Towards; in relation to someone or something (often regarding feelings, attitude, or obligations).",
                        "ru": "По отношению к кому-либо или чему-либо; касательно чувств, поведения или обязательств."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "towards",
                                "target_lang_sense_clarification": "in relation to or directed at someone"
                            },
                            {
                                "target_lang_word": "with respect to",
                                "target_lang_sense_clarification": "regarding or concerning"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "по отношению к",
                                "target_lang_sense_clarification": "касательно кого-либо"
                            },
                            {
                                "target_lang_word": "перед",
                                "target_lang_sense_clarification": "в присутствии или в глазах кого-либо (о чувствах/долге)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "dc712938-8de1-3a23-9178-cbc18b240d4b",
                    "sense_definition": "Aan de overkant (gebruikt als onderdeel van een voornaamwoordelijk bijwoord in combinatie met er, hier of daar).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zie het station en het hotel staat er <w>tegenover</w>.",
                            "target_lang_translations": {
                                "en": "I see the station and the hotel stands <w>opposite</w> it.",
                                "ru": "Я вижу вокзал, а отель стоит <w>напротив</w>."
                            }
                        },
                        {
                            "text": "Het bankgebouw staat er schuin <w>tegenover</w>.",
                            "target_lang_translations": {
                                "en": "The bank building stands diagonally <w>opposite</w> it.",
                                "ru": "Здание банка стоит наискосок <w>напротив</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "aan de overkant"
                    ],
                    "common_phrases": [
                        "er tegenover",
                        "er recht tegenover"
                    ],
                    "target_lang_definitions": {
                        "en": "Opposite; on the other side (used as an adverb, often referring back to a previously mentioned location).",
                        "ru": "На противоположной стороне (используется как наречие)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "opposite",
                                "target_lang_sense_clarification": "adverbial use indicating position"
                            },
                            {
                                "target_lang_word": "across",
                                "target_lang_sense_clarification": "on the other side"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "напротив",
                                "target_lang_sense_clarification": "наречие места"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f794c8a5-3814-3f4f-9471-8aa610e6971e",
                    "sense_definition": "Als deel van een scheidbaar werkwoord (zoals tegenoverstellen), vaak met de betekenis van compensatie of contrast.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verbal_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij stelde daar wel iets positiefs <w>tegenover</w>.",
                            "target_lang_translations": {
                                "en": "He did set something positive <w>against</w> that.",
                                "ru": "Он выставил что-то положительное <w>в противовес</w> этому."
                            }
                        },
                        {
                            "text": "Wat staat daar eigenlijk <w>tegenover</w> als beloning?",
                            "target_lang_translations": {
                                "en": "What actually stands <w>in return</w> for that as a reward?",
                                "ru": "Что же полагается <w>взамен</w> в качестве награды?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "iets er tegenover stellen",
                        "daar staat tegenover"
                    ],
                    "target_lang_definitions": {
                        "en": "Against or in return; used to indicate contrast, compensation, or an offset.",
                        "ru": "В качестве компенсации, обмена или для создания контраста."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "against",
                                "target_lang_sense_clarification": "indicating contrast or counterbalance"
                            },
                            {
                                "target_lang_word": "in return",
                                "target_lang_sense_clarification": "indicating compensation or exchange"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "взамен",
                                "target_lang_sense_clarification": "в обмен на что-то"
                            },
                            {
                                "target_lang_word": "в противовес",
                                "target_lang_sense_clarification": "для контраста"
                            }
                        ]
                    }
                }
            ]
        }
    ]

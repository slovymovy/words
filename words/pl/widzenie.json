    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d54916b5-449d-33a9-bc70-ffea27df0dae",
                    "sense_definition": "Proces odbierania bodźców wzrokowych lub zdolność patrzenia; rzeczownik odczasownikowy od słowa <w>widzieć</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "senses_sight",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz stwierdził u pacjenta znaczne pogorszenie <w>widzenia</w> po zmroku.",
                            "target_lang_translations": {
                                "ru": "Врач констатировал у пациента значительное ухудшение <w>зрения</w> в сумерках."
                            }
                        },
                        {
                            "text": "Z mojego punktu <w>widzenia</w> ta decyzja jest niesłuszna.",
                            "target_lang_translations": {
                                "ru": "С моей точки <w>зрения</w> это решение неправильное."
                            }
                        }
                    ],
                    "synonyms": [
                        "wzrok"
                    ],
                    "common_phrases": [
                        "do widzenia",
                        "punkt widzenia",
                        "pole widzenia",
                        "kąt widzenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс восприятия зрительных стимулов или способность видеть; отглагольное существительное от слова «видеть»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зрение",
                                "target_lang_sense_clarification": "способность видеть"
                            },
                            {
                                "target_lang_word": "видение",
                                "target_lang_sense_clarification": "процесс восприятия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47f165b7-d59d-366d-af7c-5d8acc2ef6be",
                    "sense_definition": "Oficjalne spotkanie z osobą osadzoną w więzieniu lub areszcie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "senses_visit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tomek na <w>widzeniu</w> spotkał się ze swoją konkubiną.",
                            "target_lang_translations": {
                                "ru": "Томек на <w>свидании</w> встретился со своей сожительницей."
                            }
                        },
                        {
                            "text": "Rodzina czekała przed bramą zakładu karnego na <w>widzenie</w>.",
                            "target_lang_translations": {
                                "ru": "Семья ждала перед воротами тюрьмы <w>свидания</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "odwiedziny"
                    ],
                    "common_phrases": [
                        "sala widzeń",
                        "iść na widzenie",
                        "zgoda na widzenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальная встреча с человеком, находящимся в тюрьме или под арестом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свидание",
                                "target_lang_sense_clarification": "посещение заключённого"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1172b9d-a5ce-3040-8042-9b2c18b77a22",
                    "sense_definition": "Obraz powstający w wyobraźni, niemający odzwierciedlenia w rzeczywistości, często o charakterze mistycznym.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "senses_vision",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pustelnik opowiadał ludziom o swoim proroczym <w>widzeniu</w>.",
                            "target_lang_translations": {
                                "ru": "Отшельник рассказывал людям о своём пророческом <w>видении</w>."
                            }
                        },
                        {
                            "text": "W wysokiej gorączce chory miał dziwne i przerażające <w>widzenia</w>.",
                            "target_lang_translations": {
                                "ru": "В сильном жару у больного были странные и пугающие <w>видения</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wizja"
                    ],
                    "common_phrases": [
                        "mieć widzenia",
                        "doznać widzenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образ, возникающий в воображении, не имеющий отражения в реальности, часто мистического характера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "видение",
                                "target_lang_sense_clarification": "призрак, галлюцинация"
                            }
                        ]
                    }
                }
            ]
        }
    ]

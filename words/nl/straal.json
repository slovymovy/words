    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6afa8780-2f9f-3cb2-96d8-9147eb962bf8",
                    "sense_definition": "Een rechte, smalle bundel van elektromagnetische straling zoals licht, warmte of radio.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "beam_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zon kwam door de wolken en wierp een warme <w>straal</w> licht op de tafel.",
                            "target_lang_translations": {
                                "en": "The sun came through the clouds and cast a warm <w>ray</w> of light on the table.",
                                "ru": "Солнце вышло из-за туч и бросило теплый <w>луч</w> света на стол."
                            }
                        },
                        {
                            "text": "Een laser zendt een zeer geconcentreerde <w>straal</w> uit die ver kan reiken.",
                            "target_lang_translations": {
                                "en": "A laser emits a very concentrated <w>beam</w> that can reach far.",
                                "ru": "Лазер испускает очень концентрированный <w>луч</w>, который может достигать больших расстояний."
                            }
                        }
                    ],
                    "common_phrases": [
                        "straal van de zon",
                        "eerste straal"
                    ],
                    "target_lang_definitions": {
                        "en": "A straight, narrow bundle of electromagnetic radiation like light, heat, or radio.",
                        "ru": "Прямой, узкий пучок электромагнитного излучения, такого как свет, тепло или радиоволны."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ray",
                                "target_lang_sense_clarification": "single line of light or radiation"
                            },
                            {
                                "target_lang_word": "beam",
                                "target_lang_sense_clarification": "bundle of light or energy"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "луч",
                                "target_lang_sense_clarification": "света, солнца, лазера"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4b90644-06f2-3724-b423-12fc11f216f1",
                    "sense_definition": "Een smalle, krachtige stroom van vloeistof of gas die ergens uit spuit.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "beam_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij zette de tuinslang aan en een harde <w>straal</w> water kwam eruit.",
                            "target_lang_translations": {
                                "en": "He turned on the garden hose and a hard <w>jet</w> of water came out.",
                                "ru": "Он включил садовый шланг, и из него вырвалась сильная <w>струя</w> воды."
                            }
                        },
                        {
                            "text": "Bij een ernstige verwonding kan het bloed in een <w>straal</w> naar buiten komen.",
                            "target_lang_translations": {
                                "en": "With a serious injury, blood can come out in a <w>stream</w>.",
                                "ru": "При серьезном ранении кровь может бить <w>струей</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "straal water",
                        "straal bloed"
                    ],
                    "target_lang_definitions": {
                        "en": "A narrow, powerful stream of liquid or gas that spurts out of something.",
                        "ru": "Узкий, мощный поток жидкости или газа, вырывающийся откуда-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "jet",
                                "target_lang_sense_clarification": "forcible stream"
                            },
                            {
                                "target_lang_word": "stream",
                                "target_lang_sense_clarification": "flow of liquid"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "струя",
                                "target_lang_sense_clarification": "воды, жидкости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4d26932-2a77-3605-9898-15c7298069ff",
                    "sense_definition": "De afstand van het middelpunt tot de rand van een cirkel of bol; ook gebruikt om een bepaald bereik aan te duiden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "geometry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie zocht naar de verdachte in een <w>straal</w> van vijf kilometer rond de plaats delict.",
                            "target_lang_translations": {
                                "en": "The police searched for the suspect within a <w>radius</w> of five kilometers around the crime scene.",
                                "ru": "Полиция искала подозреваемого в <w>радиусе</w> пяти километров от места преступления."
                            }
                        },
                        {
                            "text": "Om de oppervlakte van de cirkel te berekenen, moet je de <w>straal</w> weten.",
                            "target_lang_translations": {
                                "en": "To calculate the area of the circle, you need to know the <w>radius</w>.",
                                "ru": "Чтобы вычислить площадь круга, нужно знать <w>радиус</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "radius"
                    ],
                    "common_phrases": [
                        "in een straal van"
                    ],
                    "target_lang_definitions": {
                        "en": "The distance from the center to the edge of a circle or sphere; also used to indicate a specific range.",
                        "ru": "Расстояние от центра до края круга или шара; также используется для обозначения определенной зоны действия."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "radius",
                                "target_lang_sense_clarification": "geometry or range"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "радиус",
                                "target_lang_sense_clarification": "геометрия, зона охвата"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28762b71-0dc4-36ad-bce2-cbe70c4584f4",
                    "sense_definition": "De angel van een insect.",
                    "learner_level": "C2",

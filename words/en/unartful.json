            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "00d5746f-c17d-3608-9ba7-c3cdd508e538",
                    "sense_definition": "Lacking skill or grace in execution; clumsy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His <w>unartful</w> attempt to repair the clock resulted in several broken springs.",
                            "target_lang_translations": {
                                "ru": "Его <w>неумелая</w> попытка починить часы привела к поломке нескольких пружин."
                            }
                        },
                        {
                            "text": "The politician made an <w>unartful</w> comment that offended many of his supporters.",
                            "target_lang_translations": {
                                "ru": "Политик сделал <w>неловкое</w> замечание, которое оскорбило многих его сторонников."
                            }
                        }
                    ],
                    "synonyms": [
                        "inartful"
                    ],
                    "antonyms": [
                        "skillful"
                    ],
                    "common_phrases": [
                        "unartful-attempt",
                        "unartful-construction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишённый мастерства или изящества в исполнении; неуклюжий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неумелый",
                                "target_lang_sense_clarification": "сделанный без должного навыка"
                            },
                            {
                                "target_lang_word": "неискусный",
                                "target_lang_sense_clarification": "лишенный тонкости или мастерства"
                            },
                            {
                                "target_lang_word": "неловкий",
                                "target_lang_sense_clarification": "неуклюжий, вызывающий смущение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "989c9467-e491-320c-bc26-d9b8e2e5d4b1",
                    "sense_definition": "Lacking artifice, cunning, or deceit; simple and sincere.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She spoke with an <w>unartful</w> honesty that immediately won our trust.",
                            "target_lang_translations": {
                                "ru": "Она говорила с <w>бесхитростной</w> честностью, которая сразу же завоевала наше доверие."
                            }
                        },
                        {
                            "text": "The child's <w>unartful</w> manner was refreshing in a room full of calculating adults.",
                            "target_lang_translations": {
                                "ru": "<w>Безыскусная</w> манера ребенка была глотком свежего воздуха в комнате, полной расчетливых взрослых."
                            }
                        }
                    ],
                    "synonyms": [
                        "inartful"
                    ],
                    "antonyms": [
                        "artful"
                    ],
                    "common_phrases": [
                        "unartful-sincerity",
                        "unartful-manner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишённый притворства, хитрости или обмана; простой и искренний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бесхитростный",
                                "target_lang_sense_clarification": "искренний, без скрытого умысла"
                            },
                            {
                                "target_lang_word": "безыскусный",
                                "target_lang_sense_clarification": "простой, естественный, без прикрас"
                            },
                            {
                                "target_lang_word": "простодушный",
                                "target_lang_sense_clarification": "наивный и открытый"
                            }
                        ]

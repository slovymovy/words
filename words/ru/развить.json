    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "48e423fd-49c2-3fbc-af28-2093b192d252",
                    "sense_definition": "Усовершенствовать, усилить, сделать более мощным, обширным или духовно зрелым",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "development_improvement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы хотим <развить> наш бизнес и открыть новые филиалы в других городах.",
                            "target_lang_translations": {
                                "en": "We want to <expand> our business and open new branches in other cities."
                            }
                        },
                        {
                            "text": "Спортсмен сумел <развить> огромную скорость на финишной прямой.",
                            "target_lang_translations": {
                                "en": "The athlete managed to <build up> huge speed on the home stretch."
                            }
                        }
                    ],
                    "synonyms": [
                        "улучшить",
                        "усовершенствовать",
                        "усилить"
                    ],
                    "antonyms": [
                        "ослабить",
                        "ухудшить",
                        "запустить"
                    ],
                    "common_phrases": [
                        "развить скорость",
                        "развить способности",
                        "развить успех",
                        "развить память"
                    ],
                    "target_lang_definitions": {
                        "en": "To improve, strengthen, make more powerful, extensive, or spiritually mature"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "develop",
                                "target_lang_sense_clarification": "general improvement or growth"
                            },
                            {
                                "target_lang_word": "expand",
                                "target_lang_sense_clarification": "business or scope"
                            },
                            {
                                "target_lang_word": "build up",
                                "target_lang_sense_clarification": "speed, strength, or momentum"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa251ae4-85cb-32ad-a360-6aa9a3513c8a",
                    "sense_definition": "Подробно изложить, расширить или углубить содержание мысли, темы или идеи",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "elaboration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Прошу вас <развить> эту интересную мысль подробнее в следующем абзаце.",
                            "target_lang_translations": {
                                "en": "I ask you to <elaborate on> this interesting thought in more detail in the next paragraph."
                            }
                        },
                        {
                            "text": "Автор планирует <развить> сюжетную линию главного героя во второй книге.",
                            "target_lang_translations": {
                                "en": "The author plans to <develop> the main character's storyline in the second book."
                            }
                        }
                    ],
                    "synonyms": [
                        "раскрыть",
                        "изложить",
                        "детализировать"
                    ],
                    "antonyms": [
                        "сократить",
                        "сжать",
                        "обобщить"
                    ],
                    "common_phrases": [
                        "развить мысль",
                        "развить тему",
                        "развить идею",
                        "развить сюжет"
                    ],
                    "target_lang_definitions": {
                        "en": "To set forth in detail, expand, or deepen the content of a thought, topic, or idea"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "elaborate on",
                                "target_lang_sense_clarification": "provide more details"
                            },
                            {
                                "target_lang_word": "develop",
                                "target_lang_sense_clarification": "an idea, plot, or theme"
                            },
                            {
                                "target_lang_word": "expand on",
                                "target_lang_sense_clarification": "add detail to a topic"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa7c4ade-9aa7-3a41-8e2b-1f9c119fa0fa",
                    "sense_definition": "Раскрутить, расплести или разъединить что-либо свитое или скрученное",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "untwisting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Девочка решила <развить> тугую косу и расчесать волосы перед сном.",
                            "target_lang_translations": {
                                "en": "The girl decided to <unbraid> the tight braid and comb her hair before bed."
                            }
                        },
                        {
                            "text": "Рабочим пришлось <развить> старый канат, чтобы использовать его отдельные нити.",
                            "target_lang_translations": {
                                "en": "The workers had to <untwist> the old rope to use its individual strands."
                            }
                        }
                    ],
                    "synonyms": [
                        "раскрутить",
                        "расплести",
                        "размотать"
                    ],
                    "antonyms": [
                        "свить",
                        "скрутить",
                        "завить"
                    ],
                    "common_phrases": [
                        "развить верёвку",
                        "развить косу",
                        "развить локон"
                    ],
                    "target_lang_definitions": {
                        "en": "To untwist, unbraid, or separate something twisted or curled"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "untwist",
                                "target_lang_sense_clarification": "rope or cord"
                            },
                            {
                                "target_lang_word": "unbraid",
                                "target_lang_sense_clarification": "hair or plaits"
                            },
                            {
                                "target_lang_word": "unwind",
                                "target_lang_sense_clarification": "something coiled"
                            }
                        ]
                    }
                }
            ]
        }
    ]

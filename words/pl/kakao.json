            "pos": "noun",
            "senses": [
                {
                    "sense_id": "13cb2b6d-cefb-3f42-9048-e813642f12cc",
                    "sense_definition": "Napój sporządzony z proszku kakaowego zalanego mlekiem lub wodą",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_drink",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na śniadanie dzieci często piją ciepłe <kakao> z mlekiem.",
                            "target_lang_translations": {
                                "ru": "На завтрак дети часто пьют теплое <w>какао</w> с молоком."
                            }
                        },
                        {
                            "text": "W zimowe wieczory lubię przygotować sobie gorące <kakao>.",
                            "target_lang_translations": {
                                "ru": "Зимними вечерами я люблю готовить себе горячее <w>какао</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kakałko"
                    ],
                    "common_phrases": [
                        "gorące kakao",
                        "kubek kakao",
                        "pić kakao",
                        "kakao na mleku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Напиток, приготовленный из порошка какао с молоком или водой"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "какао",
                                "target_lang_sense_clarification": "напиток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c53e0222-fb59-32c6-b86c-93bb79086369",
                    "sense_definition": "Ciemny proszek z nasion kakaowca używany do wyrobów cukierniczych i napojów",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "food_drink",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do upieczenia tego ciasta potrzebne jest gorzkie <kakao>.",
                            "target_lang_translations": {
                                "ru": "Для выпечки этого пирога нужно горькое <w>какао</w>."
                            }
                        },
                        {
                            "text": "Posypała wierzch deseru cienką warstwą ciemnego <kakao>.",
                            "target_lang_translations": {
                                "ru": "Она посыпала верх десерта тонким слоем темного <w>какао</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ciemne kakao",
                        "łyżeczka kakao",
                        "ciasto z kakao",
                        "naturalne kakao"
                    ],
                    "target_lang_definitions": {
                        "ru": "Темный порошок из семян какао, используемый для кондитерских изделий и напитков"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "какао",
                                "target_lang_sense_clarification": "порошок"
                            },
                            {
                                "target_lang_word": "какао-порошок",
                                "target_lang_sense_clarification": "кулинарный ингредиент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9731e73-c5e2-3a44-84fb-6ad42a8fb548",
                    "sense_definition": "Ziarno drzewa kakaowego lub (potocznie) sama ta roślina",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Plantacje <kakao> znajdują się głównie w strefie klimatu równikowego.",
                            "target_lang_translations": {
                                "ru": "Плантации <w>какао</w> находятся в основном в экваториальной климатической зоне."
                            }
                        },
                        {
                            "text": "Ceny surowego <kakao> na giełdach światowych znacznie wzrosły.",
                            "target_lang_translations": {
                                "ru": "Цены на сырое <w>какао</w> на мировых биржах значительно выросли."
                            }
                        }
                    ],
                    "synonyms": [
                        "kakaowiec"
                    ],
                    "common_phrases": [
                        "ziarno kakao",
                        "uprawa kakao",
                        "plantacja kakao"
                    ],
                    "target_lang_definitions": {

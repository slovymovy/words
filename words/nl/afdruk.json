                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "print",
                                "target_lang_sense_clarification": "a mark left by a person or object, e.g. a footprint"
                            },
                            {
                                "target_lang_word": "imprint",
                                "target_lang_sense_clarification": "a mark produced by pressure"
                            }
                        ]
                    }
                }
            ]

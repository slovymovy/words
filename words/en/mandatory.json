    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "dec3cc5c-920c-3a4e-b071-18f93923cb3c",
                    "sense_definition": "Required by a law, rule, or authority; obligatory.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Attendance at school is usually <w>mandatory</w> for children.",
                            "target_lang_translations": {
                                "ru": "Посещение школы, как правило, является <w>обязательным</w> для детей."
                            }
                        },
                        {
                            "text": "The company implemented <w>mandatory</w> safety training for all employees.",
                            "target_lang_translations": {
                                "ru": "Компания ввела <w>обязательный</w> инструктаж по технике безопасности для всех сотрудников."
                            }
                        }
                    ],
                    "synonyms": [
                        "compulsory",
                        "obligatory",
                        "required",
                        "binding",
                        "imperative",
                        "involuntary"
                    ],
                    "antonyms": [
                        "optional",
                        "elective"
                    ],
                    "common_phrases": [
                        "mandatory retirement",
                        "mandatory sentence",
                        "mandatory meeting",
                        "mandatory reporting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Требуемый законом, правилом или властью; обязательный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обязательный",
                                "target_lang_sense_clarification": "required by rule or law"
                            },
                            {
                                "target_lang_word": "принудительный",
                                "target_lang_sense_clarification": "involuntary, enforced"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c1c27ef8-a274-3048-8c3f-da4399bc82bc",
                    "sense_definition": "Relating to an official mandate or commission, particularly in a political or historical context.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>mandatory</w> power was responsible for the administration of the territory.",
                            "target_lang_translations": {
                                "ru": "<w>Мандатная</w> держава несла ответственность за управление территорией."
                            }
                        },
                        {
                            "text": "Scholars studied the legal framework of the <w>mandatory</w> system established after the war.",
                            "target_lang_translations": {
                                "ru": "Ученые изучали правовую основу <w>мандатной</w> системы, созданной после войны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mandatory power",
                        "mandatory administration"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к официальному мандату или поручению, особенно в политическом или историческом контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мандатный",
                                "target_lang_sense_clarification": "relating to a mandate"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cc8dc715-f969-32d3-b519-e6d9bcd41dc0",
                    "sense_definition": "A person, organization, or state that receives a mandate.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>mandatory</w> is legally bound to act in the best interests of the mandator.",
                            "target_lang_translations": {
                                "ru": "<w>Мандатарий</w> юридически обязан действовать в наилучших интересах мандатора."
                            }
                        },
                        {
                            "text": "Under the treaty, the state acted as a <w>mandatory</w> for the disputed region.",
                            "target_lang_translations": {
                                "ru": "Согласно договору, государство действовало в качестве <w>мандатария</w> для спорного региона."

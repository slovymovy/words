            "pos": "noun",
            "senses": [
                {
                    "sense_id": "84033364-4726-3de4-a0e8-161dc85ab2ce",
                    "sense_definition": "Osoba zajmująca się zawodowo lub artystycznie pisaniem utworów literackich.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znany <w>literat</w> spotkał się z czytelnikami w miejskiej bibliotece.",
                            "target_lang_translations": {
                                "ru": "Известный <w>литератор</w> встретился с читателями в городской библиотеке."
                            }
                        },
                        {
                            "text": "Związek <w>literatów</w> przyznał w tym roku nagrodę za debiut poetycki.",
                            "target_lang_translations": {
                                "ru": "Союз <w>литераторов</w> в этом году присудил премию за поэтический дебют."
                            }
                        }
                    ],
                    "synonyms": [
                        "pisarz",
                        "autor"
                    ],
                    "common_phrases": [
                        "związek literatów",
                        "wybitny literat",
                        "młody literat",
                        "środowisko literatów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, профессионально или художественно занимающийся написанием литературных произведений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "литератор",
                                "target_lang_sense_clarification": "профессиональный писатель"
                            },
                            {
                                "target_lang_word": "писатель",
                                "target_lang_sense_clarification": "автор художественных произведений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ef7ce99-fcab-38ad-b78e-0a13f16e7925",
                    "sense_definition": "Student wydziału filologicznego lub osoba studiująca historię literatury.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako pilny <w>literat</w>, spędzał całe dnie na analizie dawnych tekstów.",
                            "target_lang_translations": {
                                "ru": "Как прилежный <w>филолог</w>, он проводил целые дни за анализом старинных текстов."
                            }
                        },
                        {
                            "text": "Grupa <w>literatów</w> z uniwersytetu zorganizowała dyskusję o romantyzmie.",
                            "target_lang_translations": {
                                "ru": "Группа <w>филологов</w> из университета организовала дискуссию о романтизме."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "współcześnie rzadziej używane w tym znaczeniu"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Студент филологического факультета или человек, изучающий историю литературы (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "филолог",
                                "target_lang_sense_clarification": "специалист по языку и литературе"

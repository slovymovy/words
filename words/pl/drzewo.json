                {
                    "sense_id": "24f2d9e6-6ae7-395a-9b52-2632b102c400",
                    "sense_definition": "Duża, wieloletnia roślina o zdrewniałym pniu głównym, od którego odchodzą gałęzie i liście",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszym ogrodzie rośnie stare <w>drzewo</w>, które daje dużo cienia w upalne dni.",
                            "target_lang_translations": {
                                "ru": "В нашем саду растёт старое <w>дерево</w>, которое даёт много тени в жаркие дни."
                            }
                        },
                        {
                            "text": "To <w>drzewo</w> owocowe rodzi pyszne jabłka każdego roku.",
                            "target_lang_translations": {
                                "ru": "Это фруктовое <w>дерево</w> каждый год приносит вкусные яблоки."
                            }
                        }
                    ],
                    "antonyms": [
                        "krzew"
                    ],
                    "common_phrases": [
                        "drzewo owocowe",
                        "drzewo liściaste",
                        "drzewo iglaste",
                        "sadzić drzewo",
                        "korona drzewa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупное многолетнее растение с одревесневшим главным стволом, от которого отходят ветви и листья"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дерево",
                                "target_lang_sense_clarification": "растение"

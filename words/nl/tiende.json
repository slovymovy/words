                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e38c92f4-4221-376e-8bca-b54eaa751a81",
                    "sense_definition": "De tiende dag van de maand.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "date",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We krijgen ons salaris altijd op de <w>tiende</w> uitbetaald.",

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "основание",
                                "target_lang_sense_clarification": "причина, обоснование действия"
                            },
                            {
                                "target_lang_word": "титул",
                                "target_lang_sense_clarification": "юридическое право"
                            }
                        ]
                    }
                }
            ]

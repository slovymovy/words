                        "examples": [
                            {
                                "example_id": "61822ee9-8520-4840-9955-fd0ecc40e416",
                                "sense_id": "641c6d97-0189-302d-ab6a-b900a8a2dfca",
                                "text": "Eight POSs are defined in traditional English grammar: noun, verb, adjective, adverb, pronoun, preposition, conjunction, and interjection."
                            },
                            {
                                "example_id": "829c444a-d275-483f-8d0e-36f1ecb1b010",
                                "sense_id": "641c6d97-0189-302d-ab6a-b900a8a2dfca",
                                "text": "To include part-of-speech (POS) information, the corpus was tagged using the CLAWS tagger."

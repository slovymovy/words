            "senses": [
                {
                    "sense_id": "2f3cbf21-11ee-3fe0-b9d7-7bf3565e7d1c",
                    "sense_definition": "Met veel lichaamsvet; zwaarlijvig.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_size",
                    "name_type": "no",
                    "examples": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3b50ea55-526a-333b-bcb2-646511aa4289",
                    "sense_definition": "Simple past and past participle of <w>lie</w> (in the sense \"to give false information intentionally\").",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "deception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He looked me in the eye and <w>lied</w> about taking the money.",
                            "target_lang_translations": {
                                "ru": "Он посмотрел мне в глаза и <w>соврал</w> о том, что взял деньги."
                            }
                        },
                        {
                            "text": "She <w>lied</w> to her parents about where she was going last night.",
                            "target_lang_translations": {
                                "ru": "Она <w>солгала</w> родителям о том, куда ходила вчера вечером."
                            }
                        }
                    ],
                    "synonyms": [
                        "fibbed"
                    ],
                    "antonyms": [
                        "told the truth"
                    ],
                    "common_phrases": [
                        "lied about",
                        "lied to me",
                        "never lied"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense of lie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время глагола \"lie\" (лгать) в значении \"намеренно сообщать ложную информацию\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лгать",
                                "target_lang_sense_clarification": "говорить неправду"
                            },
                            {
                                "target_lang_word": "врать",
                                "target_lang_sense_clarification": "говорить неправду (разговорное)"
                            },
                            {
                                "target_lang_word": "обманывать",
                                "target_lang_sense_clarification": "вводить в заблуждение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1ff4d23e-030a-3ec3-8c1d-acdb8a28da2f",
                    "sense_definition": "An art song, usually sung solo in German and accompanied on the piano.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soprano performed a beautiful <w>lied</w> by Schubert during the recital.",
                            "target_lang_translations": {
                                "ru": "Сопрано исполнила прекрасную <w>песню</w> Шуберта во время концерта."
                            }
                        },
                        {
                            "text": "Students in the advanced music history class analyzed the structure of the German <w>lied</w>.",
                            "target_lang_translations": {
                                "ru": "Студенты продвинутого курса истории музыки анализировали структуру немецкой <w>песни</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "art song"
                    ],
                    "common_phrases": [
                        "German lied",
                        "Schubert lied"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жанр песни (обычно на немецком языке) для сольного исполнения в сопровождении фортепиано."
                    },
                    "translations": {
                        "ru": [
                            {

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c6a73b97-6371-3700-9587-7d81b7633469",
                    "sense_definition": "Aan de voorkant of op het voorste gedeelte, vaak gebruikt bij voertuigen zoals een fiets.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "position_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jan zat <w>voorop</w> op de tandem terwijl zijn broer stuurde.",
                            "target_lang_translations": {
                                "ru": "Ян сидел <w>спереди</w> на тандеме, пока его брат управлял.",
                                "en": "Jan sat <w>on the front</w> of the tandem while his brother steered."
                            }
                        },
                        {
                            "text": "Mijn dochtertje wil altijd graag <w>voorop</w> de fiets zitten.",
                            "target_lang_translations": {
                                "ru": "Моя дочка всегда хочет сидеть на велосипеде <w>спереди</w>.",
                                "en": "My little daughter always wants to sit <w>on the front</w> of the bike."
                            }
                        }
                    ],
                    "antonyms": [
                        "achterop"
                    ],
                    "common_phrases": [
                        "voorop de fiets",
                        "voorop zitten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спереди или на передней части, часто используется применительно к транспортным средствам, например, велосипеду.",
                        "en": "At the front or on the front part, often used with vehicles like a bicycle."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спереди",
                                "target_lang_sense_clarification": "на переднем сиденье или части"
                            },
                            {
                                "target_lang_word": "впереди",
                                "target_lang_sense_clarification": "в передней части"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "on the front",
                                "target_lang_sense_clarification": "seating position on a vehicle"
                            },
                            {
                                "target_lang_word": "at the front",
                                "target_lang_sense_clarification": "general location"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f215f787-b9e1-3636-b71e-7ff14025cef4",
                    "sense_definition": "Als belangrijkste beschouwd of leidend zijn in een ontwikkeling.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "figurative_priority",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij ons bedrijf staat de veiligheid van de werknemers altijd <w>voorop</w>.",
                            "target_lang_translations": {
                                "ru": "В нашей компании безопасность сотрудников всегда стоит <w>на первом месте</w>.",
                                "en": "At our company, employee safety always comes <w>first</w>."
                            }
                        },
                        {
                            "text": "Deze organisatie loopt <w>voorop</w> bij het gebruik van nieuwe technologieën.",
                            "target_lang_translations": {
                                "ru": "Эта организация идет <w>впереди</w> в использовании новых технологий.",
                                "en": "This organization is <w>at the forefront</w> of using new technologies."
                            }
                        }
                    ],
                    "common_phrases": [
                        "voorop staan",
                        "voorop lopen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Считаться самым важным или быть ведущим в каком-либо развитии.",
                        "en": "Considered most important or leading in a development."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на первом месте",
                                "target_lang_sense_clarification": "о приоритетах"
                            },
                            {
                                "target_lang_word": "впереди",
                                "target_lang_sense_clarification": "о лидерстве или прогрессе"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "first",
                                "target_lang_sense_clarification": "priority or importance"
                            },
                            {
                                "target_lang_word": "at the forefront",
                                "target_lang_sense_clarification": "leading position"
                            },
                            {
                                "target_lang_word": "paramount",
                                "target_lang_sense_clarification": "supreme importance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "351cc686-edcb-3425-93ca-4fae1140ed88",
                    "sense_definition": "Helemaal vooraan in een rij of groep tijdens een beweging.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "position_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gids liep <w>voorop</w> om de groep de weg te wijzen.",
                            "target_lang_translations": {
                                "ru": "Гид шел <w>впереди</w>, чтобы показывать группе дорогу.",
                                "en": "The guide walked <w>in front</w> to show the group the way."
                            }
                        },
                        {
                            "text": "Tijdens de wandeling mochten de kinderen <w>voorop</w> lopen.",
                            "target_lang_translations": {
                                "ru": "Во время прогулки детям разрешили идти <w>впереди</w>.",
                                "en": "During the walk, the children were allowed to walk <w>in front</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "achterop"
                    ],
                    "common_phrases": [
                        "voorop lopen",
                        "voorop rijden"
                    ],
                    "target_lang_definitions": {
                        "ru": "В самом начале ряда или группы во время движения.",
                        "en": "At the very front of a line or group during movement."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "впереди",
                                "target_lang_sense_clarification": "возглавляя движение"
                            },
                            {
                                "target_lang_word": "во главе",
                                "target_lang_sense_clarification": "во главе группы"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "in front",
                                "target_lang_sense_clarification": "position relative to others"
                            },
                            {
                                "target_lang_word": "ahead",
                                "target_lang_sense_clarification": "further forward"
                            },
                            {
                                "target_lang_word": "at the head",
                                "target_lang_sense_clarification": "leading a line"
                            }
                        ]
                    }
                }
            ]
        },
        {

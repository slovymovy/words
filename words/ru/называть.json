                    "common_phrases": [
                        "называть цену",
                        "называть дату",
                        "называть причину",
                        "называть вещи своими именами"
                    ],
                    "target_lang_definitions": {
                        "en": "To say, specify, or report a specific name, number, or fact"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "name",
                                "target_lang_sense_clarification": "to specify a price, date, or condition"
                            },
                            {
                                "target_lang_word": "state",
                                "target_lang_sense_clarification": "to formally declare or report information"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f560ad0-f056-361e-b64d-a11e6c38194d",
                    "sense_definition": "Давать имя или название кому-либо или чему-либо",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Родители решили <w>назвать</w> свою новорождённую дочь Викторией.",
                            "target_lang_translations": {
                                "en": "The parents decided to <w>name</w> their newborn daughter Victoria."
                            }
                        },
                        {
                            "text": "Новый корабль планируют <w>назвать</w> в честь знаменитого путешественника.",
                            "target_lang_translations": {
                                "en": "They plan to <w>name</w> the new ship after a famous traveler."
                            }
                        }
                    ],
                    "synonyms": [
                        "нарекать",
                        "крестить",
                        "окрестить"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "668f21c8-2135-398a-8d35-e3d870a7f45b",
                    "sense_definition": "Человек, носящий высший наследственный дворянский титул в Западной Европе.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "nobility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На торжественный прием прибыл <w>герцог</w> в сопровождении своей свиты.",
                            "target_lang_translations": {
                                "en": "The <w>duke</w> arrived at the ceremonial reception accompanied by his retinue."
                            }
                        },
                        {
                            "text": "Великий <w>герцог</w> правил этими землями мудро и справедливо.",
                            "target_lang_translations": {
                                "en": "The Grand <w>Duke</w> ruled these lands wisely and justly."
                            }
                        }
                    ],
                    "synonyms": [
                        "дюк"
                    ],
                    "common_phrases": [
                        "великий герцог",
                        "светлейший герцог"
                    ],
                    "target_lang_definitions": {
                        "en": "A man holding the highest hereditary noble title in Western Europe."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "duke",
                                "target_lang_sense_clarification": "nobleman holding the highest hereditary title"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4c2395b-9627-3d28-bb92-ad8c12043ff5",
                    "sense_definition": "Дворянский титул в некоторых европейских странах, обычно следующий по рангу после королевского.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "nobility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "За свои военные заслуги полководец получил почетный титул <w>герцога</w>.",
                            "target_lang_translations": {
                                "en": "For his military merits, the commander received the honorary title of <w>duke</w>."
                            }
                        },
                        {
                            "text": "История этого рода началась, когда король даровал предку титул <w>герцога</w>.",
                            "target_lang_translations": {
                                "en": "The history of this family began when the king granted the ancestor the title of <w>duke</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "титул герцога",
                        "корона герцога"
                    ],
                    "target_lang_definitions": {
                        "en": "A noble title in some European countries, usually ranking below royalty."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "duke",
                                "target_lang_sense_clarification": "title of nobility"
                            },
                            {
                                "target_lang_word": "dukedom",
                                "target_lang_sense_clarification": "the rank or title of a duke"
                            }
                        ]
                    }
                }
            ]
        },

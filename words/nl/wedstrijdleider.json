    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f847155d-96dd-3a04-ab38-574da73fb602",
                    "sense_definition": "Iemand die toeziet op een ordelijk verloop van een wedstrijd waarbij men zich houdt aan de spelregels.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>wedstrijdleider</w> besloot de race stil te leggen vanwege de zware regenval.",
                            "target_lang_translations": {
                                "ru": "<w>Директор гонки</w> решил остановить заезд из-за сильного дождя."
                            }
                        },
                        {
                            "text": "Na het incident moest de <w>wedstrijdleider</w> een lastige beslissing nemen over de straf.",
                            "target_lang_translations": {
                                "ru": "После инцидента <w>главному судье</w> пришлось принять непростое решение о наказании."
                            }
                        }
                    ],
                    "synonyms": [
                        "scheidsrechter",
                        "arbiter"
                    ],
                    "common_phrases": [
                        "beslissing van de wedstrijdleider",
                        "ingrijpen van de wedstrijdleider"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто следит за надлежащим ходом соревнования и соблюдением правил."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "главный судья",
                                "target_lang_sense_clarification": "лицо, принимающее решения по правилам"
                            },
                            {
                                "target_lang_word": "судья соревнований",
                                "target_lang_sense_clarification": "арбитр"
                            },
                            {
                                "target_lang_word": "директор гонки",
                                "target_lang_sense_clarification": "в контексте авто- или велогонок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aaa72f2d-ecd2-3496-9ddb-e901444ff68d",
                    "sense_definition": "De persoon die verantwoordelijk is voor de organisatie en het goede verloop van een wedstrijd of toernooi.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Alle deelnemers moeten zich voor aanvang van het toernooi melden bij de <w>wedstrijdleider</w>.",
                            "target_lang_translations": {
                                "ru": "Все участники должны отметиться у <w>директора турнира</w> перед началом соревнований."
                            }
                        },
                        {
                            "text": "De <w>wedstrijdleider</w> heeft het speelschema voor de komende week bekendgemaakt.",
                            "target_lang_translations": {
                                "ru": "<w>Руководитель соревнований</w> объявил расписание игр на предстоящую неделю."
                            }
                        }
                    ],
                    "synonyms": [
                        "referee",
                        "scheidsrechter",
                        "arbiter"
                    ],
                    "common_phrases": [
                        "melden bij de wedstrijdleider",
                        "taak van de wedstrijdleider"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, ответственное за организацию и проведение матча или турнира."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "директор турнира",
                                "target_lang_sense_clarification": "организатор"
                            },
                            {
                                "target_lang_word": "руководитель соревнований",
                                "target_lang_sense_clarification": "администратор мероприятия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

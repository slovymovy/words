{
    "word_family": [
        "wydobywać",
        "wydobycie",
        "wydobywczy",
        "wydobywca"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f65b3816-dc09-3a6d-a6cd-e7ff259d9416",
                    "sense_definition": "Wyciągnąć coś z wnętrza czegoś na zewnątrz lub pozyskać surowce naturalne z ziemi.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_extraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Górnicy każdego dnia ciężko pracują, aby <w>wydobyć</w> węgiel spod ziemi.",
                            "target_lang_translations": {
                                "ru": "Шахтеры каждый день тяжело работают, чтобы <w>добыть</w> уголь из-под земли."
                            }
                        },
                        {
                            "text": "Próbowałam <w>wydobyć</w> klucze z torebki, ale nie mogłam ich znaleźć.",
                            "target_lang_translations": {
                                "ru": "Я пыталась <w>достать</w> ключи из сумочки, но не могла их найти."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyciągnąć",
                        "wyjąć",
                        "pozyskać"
                    ],
                    "antonyms": [
                        "włożyć",
                        "schować",
                        "zakopać"
                    ],
                    "common_phrases": [
                        "wydobyć węgiel",
                        "wydobyć z kieszeni",
                        "wydobyć na wierzch",
                        "wydobyć z niepamięci"
                    ],
                    "target_lang_definitions": {
                        "ru": "Извлечь что-то изнутри наружу или получить природные ресурсы из земли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добыть",
                                "target_lang_sense_clarification": "полезные ископаемые"
                            },
                            {
                                "target_lang_word": "достать",
                                "target_lang_sense_clarification": "предмет из сумки, кармана"
                            },
                            {
                                "target_lang_word": "извлечь",
                                "target_lang_sense_clarification": "формально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1740ed45-bd81-31b1-a8b7-348185ecbc9d",
                    "sense_definition": "Sprawić, że coś ukrytego (np. głos, informacja, prawda) ujawni się lub stanie się słyszalne.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_extraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policjant robił wszystko, aby <w>wydobyć</w> prawdę od przesłuchiwanego świadka.",
                            "target_lang_translations": {
                                "ru": "Полицейский делал все, чтобы <w>вытянуть</w> правду из допрашиваемого свидетеля."
                            }
                        },
                        {
                            "text": "Była tak przerażona, że nie mogła z siebie <w>wydobyć</w> głosu.",
                            "target_lang_translations": {
                                "ru": "Она была так напугана, что не могла <w>выдавить</w> из себя ни звука."
                            }
                        }
                    ],
                    "synonyms": [
                        "ujawnić",
                        "uzyskać",
                        "wykrzesać"
                    ],
                    "antonyms": [
                        "zataić",
                        "ukryć",
                        "przemilczeć"
                    ],
                    "common_phrases": [
                        "wydobyć prawdę",
                        "wydobyć głos",
                        "wydobyć zeznania",
                        "wydobyć z siebie słowo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить что-то скрытое (голос, информацию, правду) проявиться или стать слышимым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вытянуть",
                                "target_lang_sense_clarification": "информацию, правду"
                            },
                            {
                                "target_lang_word": "добиться",
                                "target_lang_sense_clarification": "признания, правды"
                            },
                            {
                                "target_lang_word": "выдавить",
                                "target_lang_sense_clarification": "голос, слова"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "adc65c82-c8fe-3a49-899f-b1a7949bf40c",
                    "sense_definition": "Uwolnić się z trudnego położenia, miejsca lub sytuacji (używane z zaimkiem zwrotnym \"się\").",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_escape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wielu latach ciężkiej pracy firmie udało się w końcu <w>wydobyć</w> się z długów.",
                            "target_lang_translations": {
                                "ru": "После многих лет тяжелой работы компании наконец удалось <w>выбраться</w> из долгов."
                            }
                        },
                        {
                            "text": "Turysta nie potrafił samodzielnie <w>wydobyć</w> się ze szczeliny skalnej.",
                            "target_lang_translations": {
                                "ru": "Турист не мог самостоятельно <w>выбраться</w> из скальной расщелины."
                            }
                        }
                    ],
                    "synonyms": [
                        "wydostać się",
                        "uwolnić się",
                        "wyjść"
                    ],
                    "antonyms": [
                        "utknąć",
                        "ugrzęznąć"
                    ],
                    "common_phrases": [
                        "wydobyć się z kłopotów",
                        "wydobyć się z długów",
                        "wydobyć się na powierzchnię"
                    ],
                    "target_lang_definitions": {
                        "ru": "Освободиться из трудного положения, места или ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбраться",
                                "target_lang_sense_clarification": "из долгов, из ямы"
                            },
                            {
                                "target_lang_word": "вылезти",
                                "target_lang_sense_clarification": "физически"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "118ef27c-ac3c-35ef-9de2-83c315bb2b92",
                    "sense_definition": "De buitenste begrenzing, het vlak of de kant van een voorwerp, persoon of ruimte.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_side",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ene <w>zijde</w> van het papier is beschreven, de andere is nog leeg.",
                            "target_lang_translations": {
                                "en": "One <w>side</w> of the paper is written on, the other is still empty.",
                                "ru": "Одна <w>сторона</w> бумаги исписана, другая еще пуста."
                            }
                        },
                        {
                            "text": "Hij bleef de hele avond trouw aan haar <w>zijde</w> staan.",
                            "target_lang_translations": {
                                "en": "He remained faithfully at her <w>side</w> all evening.",
                                "ru": "Он весь вечер преданно стоял <w>рядом</w> с ней."
                            }
                        }
                    ],
                    "synonyms": [
                        "kant",
                        "zijkant",
                        "zij"
                    ],
                    "common_phrases": [
                        "aan de zijde",
                        "ter zijde"
                    ],
                    "target_lang_definitions": {
                        "en": "The outer boundary, surface, or edge of an object, person, or space.",
                        "ru": "Внешняя граница, поверхность или край предмета, человека или пространства."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "side",
                                "target_lang_sense_clarification": "surface or edge"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сторона",
                                "target_lang_sense_clarification": "поверхность или направление"
                            },
                            {
                                "target_lang_word": "бок",
                                "target_lang_sense_clarification": "боковая часть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6db7c52-f143-306e-887e-c86690c78b95",
                    "sense_definition": "Een specifieke groep of partij die betrokken is bij een meningsverschil, conflict of situatie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_side",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aan beide <w>zijden</w> van het conflict is er veel verdriet en verlies.",
                            "target_lang_translations": {
                                "en": "On both <w>sides</w> of the conflict, there is much grief and loss.",
                                "ru": "С обеих <w>сторон</w> конфликта много горя и потерь."
                            }
                        },
                        {
                            "text": "De president stond altijd aan de <w>zijde</w> van de zwakkeren in de samenleving.",
                            "target_lang_translations": {
                                "en": "The president always stood on the <w>side</w> of the weak in society.",
                                "ru": "Президент всегда был на <w>стороне</w> слабых слоев общества."
                            }
                        }
                    ],
                    "synonyms": [
                        "kant"
                    ],
                    "common_phrases": [
                        "aan iemands zijde staan"
                    ],
                    "target_lang_definitions": {
                        "en": "A specific group or party involved in a disagreement, conflict, or situation.",
                        "ru": "Конкретная группа или сторона, участвующая в споре, конфликте или ситуации."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "side",
                                "target_lang_sense_clarification": "party in a conflict"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сторона",
                                "target_lang_sense_clarification": "участник конфликта или спора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05ef3b22-434a-3b13-bda3-82d36b1f5265",
                    "sense_definition": "Een zeer zachte, glanzende stof gemaakt van de draden van de cocon van de zijderups.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze prachtige jurk is gemaakt van dure <w>zijde</w>.",
                            "target_lang_translations": {
                                "en": "This beautiful dress is made of expensive <w>silk</w>.",
                                "ru": "Это красивое платье сшито из дорогого <w>шёлка</w>."
                            }
                        },
                        {
                            "text": "Ze draagt graag kleding van zachte <w>zijde</w> omdat het fijn aanvoelt.",
                            "target_lang_translations": {
                                "en": "She likes wearing clothes of soft <w>silk</w> because it feels nice.",
                                "ru": "Ей нравится носить одежду из мягкого <w>шёлка</w>, потому что он приятен на ощупь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "van zijde",
                        "zuivere zijde"
                    ],
                    "target_lang_definitions": {
                        "en": "A very soft, shiny fabric made from the threads of the silkworm cocoon.",
                        "ru": "Очень мягкая, блестящая ткань, изготовленная из нитей кокона шелкопряда."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "silk",
                                "target_lang_sense_clarification": "fabric"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "шёлк",
                                "target_lang_sense_clarification": "ткань"
                            }
                        ]
                    }
                }
            ]
        },

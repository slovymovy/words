    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9d311fe0-f485-37ab-a8ad-9c376eb7fa3d",
                    "sense_definition": "Вносить плату, отдавать деньги за что-либо (товар, услугу, документ, проезд).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "payment_transaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вы можете <оплачивать> покупки как наличными, так и картой.",
                            "target_lang_translations": {
                                "en": "You can <pay for> purchases with both cash and card."
                            }
                        },
                        {
                            "text": "Пассажиры обязаны своевременно <оплачивать> проезд в общественном транспорте.",
                            "target_lang_translations": {
                                "en": "Passengers are required to <pay> the fare on public transport in a timely manner."
                            }
                        }
                    ],
                    "synonyms": [
                        "плати́ть",
                        "упла́чивать"
                    ],
                    "common_phrases": [
                        "оплачивать счета",
                        "оплачивать проезд",
                        "оплачивать покупки",
                        "оплачивать картой"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a payment or give money for something (goods, services, documents, fare)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pay",
                                "target_lang_sense_clarification": "bills, fare, fees"
                            },
                            {
                                "target_lang_word": "pay for",
                                "target_lang_sense_clarification": "goods, services, purchases"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "974aaf1a-ad57-354a-8002-ce3dae19dfdc",
                    "sense_definition": "Выдавать денежное вознаграждение за выполненную работу или труд.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "payment_remuneration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Компания обещала щедро <оплачивать> труд своих сотрудников.",
                            "target_lang_translations": {
                                "en": "The company promised to generously <pay for> the work of its employees."
                            }
                        },
                        {
                            "text": "Сверхурочную работу здесь <оплачивают> по двойному тарифу.",
                            "target_lang_translations": {
                                "en": "Overtime work here is <paid for> at a double rate."
                            }
                        }
                    ],
                    "synonyms": [
                        "плати́ть"
                    ],
                    "common_phrases": [
                        "хорошо оплачивать",
                        "достойно оплачивать",
                        "оплачивать труд",
                        "оплачивать работу"
                    ],
                    "target_lang_definitions": {
                        "en": "To provide monetary remuneration for work or labor performed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pay for",
                                "target_lang_sense_clarification": "work, labor"
                            },
                            {
                                "target_lang_word": "remunerate",
                                "target_lang_sense_clarification": "formal context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "030eabcb-be09-3495-8045-87eec91f4c1e",
                    "sense_definition": "Жертвовать чем-либо ценным (здоровьем, счастьем, жизнью) ради достижения какой-либо цели.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_sacrifice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солдатам часто приходилось <оплачивать> победу собственной кровью.",
                            "target_lang_translations": {
                                "en": "Soldiers often had to <pay for> victory with their own blood."
                            }
                        },
                        {
                            "text": "Он <оплачивал> свой карьерный успех потерей друзей и здоровья.",
                            "target_lang_translations": {
                                "en": "He <paid for> his career success with the loss of friends and health."
                            }
                        }
                    ],
                    "common_phrases": [
                        "оплачивать кровью",
                        "оплачивать ценой жизни",
                        "дорого оплачивать"
                    ],
                    "target_lang_definitions": {
                        "en": "To sacrifice something valuable (health, happiness, life) to achieve a goal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pay for",
                                "target_lang_sense_clarification": "figurative, with sacrifices"
                            }
                        ]
                    }
                }
            ]
        }
    ]

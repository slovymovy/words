    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "60cc34ab-7215-3d88-b285-14b1b25812c0",
                    "sense_definition": "Wystająca część twarzy człowieka lub pyska zwierzęcia, w której znajdują się nozdrza; narząd służący do oddychania i wąchania.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimne powietrze sprawiło, że mam czerwony <w>nos</w>.",
                            "target_lang_translations": {
                                "ru": "От холодного воздуха у меня красный <w>нос</w>."
                            }
                        },
                        {
                            "text": "Pies trącił mnie mokrym <w>nosem</w> w rękę, domagając się spaceru.",
                            "target_lang_translations": {
                                "ru": "Собака ткнула меня мокрым <w>носом</w> в руку, требуя прогулки."
                            }
                        }
                    ],
                    "synonyms": [
                        "kichawa",
                        "kichawa",
                        "węch",
                        "trąba",
                        "dziób"
                    ],
                    "common_phrases": [
                        "kręcić nosem",
                        "mieć muchy w nosie",
                        "zadzierać nosa",
                        "mieć nosa",
                        "wtykać nos"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выступающая часть лица человека или морды животного, в которой находятся ноздри; орган дыхания и обоняния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нос",
                                "target_lang_sense_clarification": "часть тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4de2957c-6234-3752-956a-060b444e12a7",
                    "sense_definition": "Zmysł węchu; zdolność wyczuwania i rozpoznawania zapachów.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sense_faculty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój pies ma doskonały <w>nos</w> i potrafi znaleźć ukryte przysmaki.",
                            "target_lang_translations": {
                                "ru": "У моей собаки отличный <w>нюх</w>, и она может найти спрятанные лакомства."
                            }
                        },
                        {
                            "text": "Po chorobie straciłem <w>nos</w> i nie czuję smaku potraw.",
                            "target_lang_translations": {
                                "ru": "После болезни я потерял <w>обоняние</w> и не чувствую вкуса еды."
                            }
                        }
                    ],
                    "synonyms": [
                        "węch"
                    ],
                    "common_phrases": [
                        "dobry nos",
                        "czuły nos"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство обоняния; способность чувствовать и распознавать запахи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обоняние",
                                "target_lang_sense_clarification": "чувство"
                            },
                            {
                                "target_lang_word": "нюх",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "чутьё",
                                "target_lang_sense_clarification": "способность чувствовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8081b8cd-14cd-3752-8c49-6271b1ce147a",
                    "sense_definition": "Intuicja, przeczucie lub zdolność przewidywania zdarzeń w jakiejś dziedzinie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "intuition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony detektyw miał <w>nosa</w> do rozwiązywania trudnych zagadek.",
                            "target_lang_translations": {
                                "ru": "У опытного детектива было <w>чутьё</w> на решение сложных загадок."
                            }
                        },
                        {
                            "text": "Mój <w>nos</w> podpowiada mi, że ta inwestycja będzie bardzo zyskowna.",
                            "target_lang_translations": {
                                "ru": "Моё <w>чутьё</w> подсказывает мне, что эта инвестиция будет очень прибыльной."
                            }
                        }
                    ],
                    "synonyms": [
                        "węch",
                        "perfumiarz"
                    ],
                    "common_phrases": [
                        "mieć nosa do interesów",
                        "mieć nosa do ludzi",
                        "nos mnie nie myli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Интуиция, предчувствие или способность предвидеть события в какой-либо области."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чутьё",
                                "target_lang_sense_clarification": "интуиция"
                            },
                            {
                                "target_lang_word": "нюх",
                                "target_lang_sense_clarification": "проницательность"
                            },
                            {
                                "target_lang_word": "интуиция",
                                "target_lang_sense_clarification": "предчувствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f905a62-13ae-3d5b-b602-9edcf1032b41",
                    "sense_definition": "Przednia, wystająca lub zwężona część jakiegoś przedmiotu (np. buta, samolotu, łodzi).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "object_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pilot delikatnie uniósł <w>nos</w> samolotu podczas startu.",
                            "target_lang_translations": {
                                "ru": "Пилот плавно поднял <w>нос</w> самолета во время взлета."
                            }
                        },
                        {
                            "text": "Potknąłem się i zdarłem skórę na <w>nosie</w> mojego nowego buta.",
                            "target_lang_translations": {
                                "ru": "Я споткнулся и содрал кожу на <w>носке</w> моего нового ботинка."
                            }
                        }
                    ],
                    "synonyms": [
                        "dziób"
                    ],
                    "common_phrases": [
                        "nos samolotu",
                        "nos buta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передняя, выступающая или суженная часть какого-либо предмета (например, ботинка, самолета, лодки)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нос",
                                "target_lang_sense_clarification": "самолета, судна"
                            },
                            {
                                "target_lang_word": "носок",
                                "target_lang_sense_clarification": "обуви"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "178d457f-92c5-32dc-8f71-1dc17fa90580",
                    "sense_definition": "Specjalista zajmujący się zawodowo tworzeniem kompozycji zapachowych i perfum.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "profession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słynny paryski <w>nos</w> pracował nad tą formułą przez dwa lata.",
                            "target_lang_translations": {
                                "ru": "Знаменитый парижский <w>«нос»</w> работал над этой формулой два года."
                            }
                        },
                        {
                            "text": "Aby zostać <w>nosem</w>, trzeba przejść wieloletnie i rygorystyczne szkolenie.",
                            "target_lang_translations": {
                                "ru": "Чтобы стать <w>«носом»</w>, нужно пройти многолетнее и строгое обучение."
                            }
                        }
                    ],
                    "synonyms": [
                        "perfumiarz",
                        "kompozytor zapachowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специалист, профессионально занимающийся созданием парфюмерных композиций."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "парфюмер",
                                "target_lang_sense_clarification": "профессия"
                            },
                            {
                                "target_lang_word": "«нос»",
                                "target_lang_sense_clarification": "профессиональный жаргон"
                            },
                            {
                                "target_lang_word": "нюхач",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8938db3-c6d3-33d7-98ac-d1c4929ea4ec",
                    "sense_definition": "Osoba nadużywająca alkoholu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "person_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miejscowy <w>nos</w> znowu przesiaduje pod sklepem od samego rana.",
                            "target_lang_translations": {
                                "ru": "Местный <w>пьяница</w> снова сидит у магазина с самого утра."
                            }
                        },
                        {
                            "text": "Wszyscy wiedzieli, że stary dozorca to <w>nos</w>, który rzadko bywa trzeźwy.",
                            "target_lang_translations": {
                                "ru": "Все знали, что старый дворник — <w>алкаш</w>, который редко бывает трезвым."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "określenie pejoratywne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, злоупотребляющий алкоголем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пьяница",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "алкоголик",
                                "target_lang_sense_clarification": "общее"
                            }
                        ]
                    }
                }
            ]
        }
    ]

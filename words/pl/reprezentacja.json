                {
                    "sense_id": "880a442b-b9a9-33c8-a3bf-cd891050edcd",
                    "sense_definition": "Drużyna sportowa składająca się z najlepszych zawodników, która reprezentuje kraj, klub lub organizację w zawodach międzynarodowych.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "grupa_ludzi",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polska <reprezentacja> w piłce nożnej wygrała wczorajszy mecz.",
                            "target_lang_translations": {
                                "ru": "Польская <сборная> по футболу выиграла вчерашний матч."
                            }
                        },
                        {
                            "text": "Mój brat marzy o tym, by kiedyś trafić do <reprezentacji> narodowej.",
                            "target_lang_translations": {
                                "ru": "Мой брат мечтает о том, чтобы когда-нибудь попасть в национальную <сборную>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kadra",
                        "drużyna narodowa"
                    ],
                    "common_phrases": [
                        "reprezentacja Polski",
                        "reprezentacja narodowa",
                        "kapitan reprezentacji",
                        "mecz reprezentacji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Спортивная команда, состоящая из лучших игроков, представляющая страну, клуб или организацию на международных соревнованиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сборная",
                                "target_lang_sense_clarification": "спортивная команда"
                            },
                            {
                                "target_lang_word": "национальная команда",
                                "target_lang_sense_clarification": "команда страны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6340cf7-0bfb-31b5-9c4b-7c2486c82192",
                    "sense_definition": "Grupa osób lub instytucja upoważniona do występowania w czyimś imieniu i dbania o czyjeś interesy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "grupa_ludzi",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza <reprezentacja> handlowa udała się na ważne negocjacje do Brukseli.",
                            "target_lang_translations": {
                                "ru": "Наше торговое <представительство> отправилось на важные переговоры в Брюссель."
                            }
                        },
                        {
                            "text": "Związek zawodowy zapewnia pracownikom <reprezentację> w rozmowach z zarządem.",
                            "target_lang_translations": {
                                "ru": "Профсоюз обеспечивает работникам <представительство> в переговорах с руководством."
                            }
                        }

    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "adca903a-14d7-369d-991f-e079bbf47bbb",
                    "sense_definition": "De handeling van het geven, leveren of beschikbaar stellen van iets aan iemand.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "actie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>verstrekking</w> van de nieuwe paspoorten liep enige vertraging op.",
                            "target_lang_translations": {
                                "en": "The <w>issuance</w> of the new passports was somewhat delayed.",
                                "ru": "<w>Выдача</w> новых паспортов произошла с некоторой задержкой."
                            }
                        },
                        {
                            "text": "Zij zijn verantwoordelijk voor de <w>verstrekking</w> van voedselpakketten in het rampgebied.",
                            "target_lang_translations": {
                                "en": "They are responsible for the <w>provision</w> of food parcels in the disaster area.",
                                "ru": "Они отвечают за <w>выдачу</w> продуктовых наборов в зоне бедствия."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitreiking",
                        "levering",
                        "verschaffing"
                    ],
                    "common_phrases": [
                        "verstrekking van gegevens",
                        "verstrekking van medicijnen",
                        "gratis verstrekking"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of giving, supplying, or making something available to someone.",
                        "ru": "Действие по передаче, поставке или предоставлению чего-либо кому-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "provision",
                                "target_lang_sense_clarification": "general act of providing"
                            },
                            {
                                "target_lang_word": "issuance",
                                "target_lang_sense_clarification": "of documents"
                            },
                            {
                                "target_lang_word": "supply",
                                "target_lang_sense_clarification": "of goods"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "предоставление",
                                "target_lang_sense_clarification": "действие по обеспечению чем-либо (услугами, данными)"
                            },
                            {
                                "target_lang_word": "выдача",
                                "target_lang_sense_clarification": "вручение конкретных предметов или документов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9721d55d-d761-3e55-9e98-e7af08df11de",
                    "sense_definition": "Datgene wat gegeven of geleverd is, vaak in de vorm van een voorziening, toelage of hulpmiddel.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze bril geldt als een medische <w>verstrekking</w> en wordt vergoed door de verzekering.",
                            "target_lang_translations": {
                                "en": "These glasses count as a medical <w>aid</w> and are reimbursed by the insurance.",
                                "ru": "Эти очки считаются медицинским <w>средством</w> и покрываются страховкой."
                            }
                        },
                        {
                            "text": "De gemeente beslist over de <w>verstrekking</w> die de bewoner mag ontvangen.",
                            "target_lang_translations": {
                                "en": "The municipality decides on the <w>benefit</w> that the resident may receive.",
                                "ru": "Муниципалитет принимает решение о <w>пособии</w>, которое может получить житель."
                            }
                        }
                    ],
                    "synonyms": [
                        "voorziening"
                    ],
                    "common_phrases": [
                        "medische verstrekking",
                        "eenmalige verstrekking"
                    ],
                    "target_lang_definitions": {
                        "en": "That which is given or supplied, often in the form of a provision, allowance, or aid.",
                        "ru": "То, что предоставлено или выдано, часто в виде пособия, льготы или вспомогательного средства."
                    },
                    "translations": {

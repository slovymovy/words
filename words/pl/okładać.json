{
    "word_family": [
        "okład",
        "okładka",
        "okładzina",
        "obkład",
        "obkładać"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6a9bb750-7b24-39e3-86f7-94d8a1856d6c",
                    "sense_definition": "Pokrywać powierzchnię czegoś warstwą innego materiału w celu ochrony, ozdoby lub leczenia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "covering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek starannie <okładał> młode drzewka słomą przed nadejściem zimy.",
                            "target_lang_translations": {
                                "ru": "Марек старательно <w>обкладывал</w> молодые деревца соломой перед наступлением зимы."
                            }
                        },
                        {
                            "text": "Musisz <okładać> stłuczone kolano lodem, żeby zmniejszyć opuchliznę.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>обкладывать</w> ушибленное колено льдом, чтобы уменьшить отек."
                            }
                        }
                    ],
                    "synonyms": [
                        "obkładać",
                        "pokrywać",
                        "osłaniać"
                    ],
                    "antonyms": [
                        "odsłaniać",
                        "odkrywać"
                    ],
                    "common_phrases": [
                        "okładać lodem",
                        "okładać książki",
                        "okładać słomą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать поверхность чего-либо слоем другого материала для защиты, украшения или лечения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обкладывать",
                                "target_lang_sense_clarification": "покрывать со всех сторон (напр. соломой, льдом)"
                            },
                            {
                                "target_lang_word": "оборачивать",
                                "target_lang_sense_clarification": "покрывать обложкой или бумагой (напр. книги)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06d77ba0-e4fd-3769-bef0-cb47abafafa5",
                    "sense_definition": "Uderzać kogoś wielokrotnie i mocno, używając do tego rąk lub jakiegoś przedmiotu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "violence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Napastnik zaczął <okładać> swoją ofiarę kijem bejsbolowym.",
                            "target_lang_translations": {
                                "ru": "Нападавший начал <w>избивать</w> свою жертву бейсбольной битой."
                            }
                        },
                        {
                            "text": "Rozwścieczony tłum chciał <okładać> złodzieja pięściami.",
                            "target_lang_translations": {
                                "ru": "Разъяренная толпа хотела <w>колотить</w> вора кулаками."
                            }
                        }
                    ],
                    "synonyms": [
                        "bić",
                        "uderzać",
                        "tłuc",
                        "walić",
                        "grzmocić"
                    ],
                    "antonyms": [
                        "głaskać"
                    ],
                    "common_phrases": [
                        "okładać pięściami",
                        "okładać kijem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наносить кому-либо множественные сильные удары руками или предметом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колотить",
                                "target_lang_sense_clarification": "сильно бить, наносить удары"
                            },
                            {
                                "target_lang_word": "избивать",
                                "target_lang_sense_clarification": "жестоко бить"
                            },
                            {
                                "target_lang_word": "охаживать",
                                "target_lang_sense_clarification": "разг. бить чем-либо (напр. палкой)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3bb4fdf3-db84-3fdd-a259-cf3bd5752584",
                    "sense_definition": "Nakładać na kogoś ciężary, przykre obowiązki lub kary, takie jak podatki czy klątwy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "imposing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rząd nie powinien <okładać> obywateli kolejnymi wysokimi podatkami.",
                            "target_lang_translations": {
                                "ru": "Правительство не должно <w>облагать</w> граждан очередными высокими налогами."
                            }
                        },
                        {
                            "text": "W legendach czarownice często potrafiły <okładać> ludzi strasznymi klątwami.",
                            "target_lang_translations": {
                                "ru": "В легендах ведьмы часто могли <w>накладывать</w> на людей страшные проклятия."
                            }
                        }
                    ],
                    "synonyms": [
                        "obarczać",
                        "obciążać"
                    ],
                    "antonyms": [
                        "zwalniać",
                        "odciążać"
                    ],
                    "common_phrases": [
                        "okładać podatkami",
                        "okładać klątwą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возлагать на кого-либо бремя, неприятные обязанности или наказания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "облагать",
                                "target_lang_sense_clarification": "налогами или пошлинами"
                            },
                            {
                                "target_lang_word": "накладывать",
                                "target_lang_sense_clarification": "проклятия, штрафы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
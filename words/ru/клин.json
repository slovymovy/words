    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a47652c1-5563-39d2-b44f-95d8dbaec200",
                    "sense_definition": "Предмет из твёрдого материала, заострённый с одной стороны и расширяющийся к другой, используемый для раскалывания чего-либо или закрепления деталей.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "tool_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он вставил деревянный <w>клин</w> в щель, чтобы дверь не закрывалась.",
                            "target_lang_translations": {
                                "en": "He inserted a wooden <w>wedge</w> into the gap so the door wouldn't close."
                            }
                        },
                        {
                            "text": "Чтобы расколоть толстое полено, нам понадобится железный <w>клин</w>.",
                            "target_lang_translations": {
                                "en": "To split the thick log, we will need an iron <w>wedge</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "вбить клин",
                        "клин клином вышибают",
                        "свет клином не сошёлся"
                    ],
                    "target_lang_definitions": {
                        "en": "An object made of hard material, pointed at one end and widening towards the other, used for splitting something or securing parts."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wedge",
                                "target_lang_sense_clarification": "tool for splitting or securing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f93962c6-a066-3503-a93d-770a4dd5a6cb",
                    "sense_definition": "Строй летящих перелётных птиц, имеющий форму острого угла.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "formation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В осеннем небе печально летел журавлиный <w>клин</w>.",
                            "target_lang_translations": {
                                "en": "A <w>wedge</w> of cranes flew sadly in the autumn sky."
                            }
                        },
                        {
                            "text": "Птицы выстроились в <w>клин</w> и взяли курс на юг.",
                            "target_lang_translations": {
                                "en": "The birds formed a <w>wedge</w> and set a course for the south."
                            }
                        }
                    ],
                    "synonyms": [
                        "косяк"
                    ],
                    "common_phrases": [
                        "журавлиный клин"
                    ],
                    "target_lang_definitions": {
                        "en": "A formation of flying migratory birds in the shape of an acute angle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wedge",
                                "target_lang_sense_clarification": "formation of birds"
                            },
                            {
                                "target_lang_word": "V-formation",
                                "target_lang_sense_clarification": "flight pattern"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c5dea86-413d-37a3-b9b2-dff5b2f23bea",
                    "sense_definition": "Треугольный кусок ткани, вшиваемый в одежду для её расширения или украшения.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Портниха вшила в юбку дополнительный <w>клин</w>, чтобы сделать её свободнее.",
                            "target_lang_translations": {
                                "en": "The dressmaker sewed an extra <w>gusset</w> into the skirt to make it looser."
                            }
                        },
                        {
                            "text": "Платье скроено так, что боковые <w>клинья</w> создают красивый силуэт.",
                            "target_lang_translations": {
                                "en": "The dress is cut so that the side <w>gores</w> create a beautiful silhouette."
                            }
                        }
                    ],
                    "common_phrases": [
                        "юбка из клиньев"
                    ],
                    "target_lang_definitions": {
                        "en": "A triangular piece of fabric sewn into clothing to widen or decorate it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gore",
                                "target_lang_sense_clarification": "triangular piece in a skirt or dress"
                            },
                            {
                                "target_lang_word": "gusset",
                                "target_lang_sense_clarification": "insert for widening or strengthening"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37a4a890-07ec-33ef-badd-92c10b94bf2c",
                    "sense_definition": "Боевой порядок войск (пехоты, конницы или танков) в форме треугольника, обращённого остриём к противнику для прорыва фронта.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Танковый <w>клин</w> прорвал оборону противника на узком участке фронта.",
                            "target_lang_translations": {
                                "en": "The tank <w>wedge</w> broke through the enemy defenses on a narrow sector of the front."
                            }
                        },
                        {
                            "text": "Рыцари построились в боевой <w>клин</w>, готовясь к атаке.",
                            "target_lang_translations": {
                                "en": "The knights formed a combat <w>wedge</w>, preparing for the attack."
                            }
                        }
                    ],
                    "synonyms": [
                        "свинья"
                    ],
                    "common_phrases": [
                        "танковый клин"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "military"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A combat formation of troops (infantry, cavalry, or tanks) in the shape of a triangle pointing towards the enemy to break the front."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wedge",
                                "target_lang_sense_clarification": "military formation"
                            },
                            {
                                "target_lang_word": "spearhead",
                                "target_lang_sense_clarification": "leading element of an attack"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1676a8aa-0ea6-3d99-973d-920467018a58",
                    "sense_definition": "Часть земельного угодья, выделяемая по какому-либо хозяйственному признаку или засеваемая одной культурой.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "agriculture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом году посевной <w>клин</w> пшеницы был значительно увеличен.",
                            "target_lang_translations": {
                                "en": "This year the wheat sowing <w>area</w> was significantly increased."
                            }
                        },
                        {
                            "text": "Озимый <w>клин</w> дал плохие всходы из-за ранних морозов.",
                            "target_lang_translations": {
                                "en": "The winter <w>crop area</w> produced poor shoots due to early frosts."
                            }
                        }
                    ],
                    "common_phrases": [
                        "посевной клин",
                        "пахотный клин"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "agricultural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A portion of agricultural land allocated by some economic characteristic or sown with a single crop."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "area",
                                "target_lang_sense_clarification": "cultivated land"
                            },
                            {
                                "target_lang_word": "field",
                                "target_lang_sense_clarification": "agricultural plot"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b727a80e-62ab-3e2d-bc91-ac596cb30d3a",
                    "sense_definition": "Техническая неисправность, при которой движущиеся части механизма застревают и перестают двигаться; в переносном смысле — состояние ступора.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за отсутствия масла двигатель поймал <w>клин</w>.",
                            "target_lang_translations": {
                                "en": "Due to a lack of oil, the engine <w>seized up</w>."
                            }
                        },
                        {
                            "text": "У меня в голове случился какой-то <w>клин</w>, и я забыл все слова.",
                            "target_lang_translations": {
                                "en": "I had a sort of mental <w>block</w>, and I forgot all the words."
                            }
                        }
                    ],
                    "synonyms": [
                        "сту́пор"
                    ],
                    "common_phrases": [
                        "поймать клин"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "technical or colloquial"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A technical malfunction where moving parts of a mechanism get stuck and stop moving; figuratively — a state of stupor."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "seizure",
                                "target_lang_sense_clarification": "mechanical failure"
                            },
                            {
                                "target_lang_word": "jam",
                                "target_lang_sense_clarification": "getting stuck"
                            },
                            {
                                "target_lang_word": "block",
                                "target_lang_sense_clarification": "mental stupor"
                            }
                        ]
                    }
                }
            ]
        },

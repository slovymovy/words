            "senses": [
                {
                    "sense_id": "39fa1e42-fa75-3012-8cca-99daaa32eaeb",
                    "sense_definition": "Een massa kleine luchtbellen die ontstaat op of in een vloeistof, bijvoorbeeld door beweging of gisting.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "foam_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er stond een dikke laag wit <w>schuim</w> op de golven van de zee.",
                            "target_lang_translations": {
                                "ru": "На морских волнах лежал толстый слой белой <w>пены</w>."
                            }
                        },
                        {
                            "text": "Klop de melk op tot er stevig <w>schuim</w> ontstaat voor de cappuccino.",
                            "target_lang_translations": {
                                "ru": "Взбивайте молоко, пока не образуется густая <w>пена</w> для капучино."
                            }
                        }
                    ],
                    "common_phrases": [
                        "schuim op de mond",
                        "schuim kloppen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Масса мелких пузырьков воздуха, образующаяся на поверхности или внутри жидкости, например, при взбалтывании или брожении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пена",
                                "target_lang_sense_clarification": "пузырьки на жидкости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a51990e-dcda-35dc-af1c-5cfd659a0e7b",
                    "sense_definition": "Een luchtig en zoet snoepje gemaakt van stijfgeklopt eiwit en suiker.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "candy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens het feest deelden ze zakjes met roze en witte <w>schuimpjes</w> uit.",
                            "target_lang_translations": {
                                "ru": "Во время праздника раздавали пакетики с розовым и белым <w>безе</w>."
                            }
                        },
                        {
                            "text": "De taart was versierd met zoet <w>schuim</w> dat in de mond smolt.",
                            "target_lang_translations": {
                                "ru": "Торт был украшен сладким <w>безе</w>, которое таяло во рту."
                            }

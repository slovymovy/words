            "senses": [
                {
                    "sense_id": "3cf079cf-e20d-3a75-8769-8eb35fb61f81",
                    "sense_definition": "To grind or crush a substance into a fine powder or pulp.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "grinding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pharmacist must <w>triturate</w> the solid ingredients before mixing them into the cream.",
                            "target_lang_translations": {
                                "ru": "Фармацевт должен <w>растереть</w> твердые ингредиенты перед тем, как смешать их с кремом."
                            }
                        },
                        {
                            "text": "You should <w>triturate</w> the dried herbs to release their aromatic oils.",
                            "target_lang_translations": {
                                "ru": "Вам следует <w>растереть</w> сушеные травы, чтобы высвободить их ароматические масла."
                            }
                        }
                    ],
                    "synonyms": [
                        "pulverize",
                        "grind",
                        "crush",
                        "comminute"
                    ],
                    "common_phrases": [
                        "triturate the mixture",
                        "triturate into powder"
                    ],
                    "target_lang_definitions": {
                        "ru": "Растирать или дробить вещество в мелкий порошок или кашицу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "растирать",

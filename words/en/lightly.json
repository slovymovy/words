    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "6b9d1ec2-2d9b-33d7-b607-a7904421c69d",
                    "sense_definition": "With little weight, pressure, or force; in a gentle manner.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She tapped him <w>lightly</w> on the shoulder to get his attention.",
                            "target_lang_translations": {
                                "ru": "Она <w>слегка</w> похлопала его по плечу, чтобы привлечь его внимание."
                            }
                        },
                        {
                            "text": "The bird landed <w>lightly</w> on the thin branch without breaking it.",
                            "target_lang_translations": {
                                "ru": "Птица <w>легко</w> приземлилась на тонкую ветку, не сломав её."
                            }
                        }
                    ],
                    "synonyms": [
                        "gently",
                        "softly",
                        "delicately"
                    ],
                    "antonyms": [
                        "heavily",
                        "forcefully",
                        "roughly"
                    ],
                    "common_phrases": [
                        "tread lightly",
                        "touch lightly",
                        "travel lightly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без должной серьезности, внимания или заботы; беспечно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "легкомысленно",
                                "target_lang_sense_clarification": "несерьезно, необдуманно"
                            },
                            {
                                "target_lang_word": "беспечно",
                                "target_lang_sense_clarification": "беззаботно"
                            },
                            {
                                "target_lang_word": "несерьезно",
                                "target_lang_sense_clarification": "шутливо, без ответственности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b9d1ec2-2d9b-33d7-b607-a7904421c69d",
                    "sense_definition": "To a small degree or amount; not thickly, deeply, or severely.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I prefer my toast <w>lightly</w> buttered rather than soaked in grease.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю тост, <w>слегка</w> смазанный маслом, а не пропитанный жиром."
                            }
                        },
                        {
                            "text": "It was raining <w>lightly</w> when we left the house, so we did not need an umbrella.",
                            "target_lang_translations": {
                                "ru": "Когда мы вышли из дома, дождь шел <w>слабо</w>, поэтому зонт нам не понадобился."
                            }
                        }
                    ],
                    "synonyms": [
                        "slightly",
                        "faintly",
                        "moderately",
                        "sparsely"
                    ],
                    "antonyms": [
                        "heavily",
                        "densely",
                        "intensely"
                    ],
                    "common_phrases": [
                        "lightly salted",
                        "lightly seasoned",

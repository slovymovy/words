{
    "word_family": [
        "verrassing",
        "verrassend"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8e5a52fc-b8d2-33a6-a896-38a205a22c16",
                    "sense_definition": "Iemand onverwachts blij maken, meestal met een cadeau of een leuke gebeurtenis.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil je graag <w>verrassen</w> met dit cadeau.",
                            "target_lang_translations": {
                                "en": "I would like to <w>surprise</w> you with this gift.",
                                "ru": "Я хочу <w>удивить</w> тебя этим подарком."
                            }
                        },
                        {
                            "text": "Mijn ouders <w>verrasten</w> mij met een groot feest voor mijn verjaardag.",
                            "target_lang_translations": {
                                "en": "My parents <w>surprised</w> me with a big party for my birthday.",
                                "ru": "Мои родители <w>устроили</w> мне <w>сюрприз</w> в виде большой вечеринки на день рождения."
                            }
                        }
                    ],
                    "synonyms": [
                        "verblijden"
                    ],
                    "antonyms": [
                        "teleurstellen"
                    ],
                    "common_phrases": [
                        "iemand verrassen met",
                        "laat je verrassen"
                    ],
                    "target_lang_definitions": {
                        "en": "To make someone unexpectedly happy, usually with a gift or a pleasant event.",
                        "ru": "Сделать кому-то неожиданный приятный сюрприз, обычно с помощью подарка или радостного события."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "surprise",
                                "target_lang_sense_clarification": "to gladden with something unexpected"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сделать сюрприз",
                                "target_lang_sense_clarification": "устроить неожиданное приятное событие"
                            },
                            {
                                "target_lang_word": "удивить",
                                "target_lang_sense_clarification": "вызвать приятное удивление"
                            },
                            {
                                "target_lang_word": "порадовать",
                                "target_lang_sense_clarification": "доставить радость неожиданностью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abf888ba-9b48-3447-80f4-6bf20e6a0b62",
                    "sense_definition": "Iets onverwachts doen of laten zien waardoor iemand verbaasd of overrompeld is; iemand onvoorbereid aantreffen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De inbreker werd <w>verrast</w> door de politie en direct gearresteerd.",
                            "target_lang_translations": {
                                "en": "The burglar was <w>surprised</w> by the police and immediately arrested.",
                                "ru": "Грабитель был <w>застигнут врасплох</w> полицией и немедленно арестован."
                            }
                        },
                        {
                            "text": "Het adembenemende uitzicht boven op de berg <w>verraste</w> ons volledig.",
                            "target_lang_translations": {
                                "en": "The breathtaking view at the top of the mountain completely <w>surprised</w> us.",
                                "ru": "Захватывающий вид на вершине горы полностью нас <w>поразил</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "overrompelen",
                        "confronteren"
                    ],
                    "common_phrases": [
                        "aangenaam verrast zijn",
                        "verrast worden door"
                    ],
                    "target_lang_definitions": {
                        "en": "To do something unexpected that amazes or overwhelms someone; to catch someone unprepared.",
                        "ru": "Совершить неожиданное действие, вызывающее удивление или замешательство; застать кого-то неподготовленным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "surprise",
                                "target_lang_sense_clarification": "to catch unawares or astonish"
                            },
                            {
                                "target_lang_word": "catch off guard",
                                "target_lang_sense_clarification": "to find someone unprepared"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "застать врасплох",
                                "target_lang_sense_clarification": "застать неожиданно, неподготовленным"
                            },
                            {
                                "target_lang_word": "поразить",
                                "target_lang_sense_clarification": "сильно удивить, ошеломить"
                            },
                            {
                                "target_lang_word": "ошеломить",
                                "target_lang_sense_clarification": "привести в сильное замешательство"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is always <w>complaining</w> about the cold weather.",
                            "target_lang_translations": {
                                "ru": "Она вечно <w>жалуется</w> на холодную погоду."
                            }
                        },
                        {
                            "text": "Stop <w>complaining</w> and help me clean this mess.",
                            "target_lang_translations": {
                                "ru": "Перестань <w>жаловаться</w> и помоги мне убрать этот беспорядок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stop complaining",
                        "complaining about",
                        "complaining to"
                    ],

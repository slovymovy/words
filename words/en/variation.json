                    ],
                    "target_lang_definitions": {
                        "ru": "Отклонение небесного тела от его средней орбиты или движения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отклонение",
                                "target_lang_sense_clarification": "девиация"
                            },
                            {
                                "target_lang_word": "вариация",
                                "target_lang_sense_clarification": "астрономический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

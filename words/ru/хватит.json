                        "всё хватит",
                        "хватит болтать"
                    ],
                    "target_lang_definitions": {
                        "en": "Expresses a demand or request to stop an action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "enough",
                                "target_lang_sense_clarification": "command to stop"
                            },
                            {
                                "target_lang_word": "stop it",
                                "target_lang_sense_clarification": "imperative command"
                            },
                            {
                                "target_lang_word": "that's enough",

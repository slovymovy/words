    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "10549fdd-a52a-3c40-b12f-f9e6acdb2d26",
                    "sense_definition": "W sposób otwarty, serdeczny i życzliwy wobec przybywających gości, chętnie ich przyjmując.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarze przywitali nas bardzo <w>gościnnie</w> i od razu zaprosili do stołu.",
                            "target_lang_translations": {
                                "ru": "Хозяева встретили нас очень <w>гостеприимно</w> и сразу пригласили к столу."
                            }
                        },
                        {
                            "text": "Mimo niespodziewanej wizyty, zostaliśmy potraktowani niezwykle <w>gościnnie</w>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на неожиданный визит, нас приняли чрезвычайно <w>радушно</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "serdecznie",
                        "życzliwie"
                    ],
                    "antonyms": [
                        "niegościnnie"
                    ],
                    "common_phrases": [
                        "przyjąć gościnnie",
                        "witać gościnnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Открыто, сердечно и доброжелательно по отношению к гостям, радушно принимая их."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гостеприимно",
                                "target_lang_sense_clarification": "с готовностью принять и угостить"
                            },
                            {
                                "target_lang_word": "радушно",
                                "target_lang_sense_clarification": "сердечно, приветливо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ef0950d-33ba-3e66-9963-0bacec702b9e",
                    "sense_definition": "W roli zaproszonego gościa, biorąc udział w wydarzeniu artystycznym lub projekcie, nie będąc jego stałym uczestnikiem.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znany aktor wystąpił <w>gościnnie</w> w jednym z odcinków popularnego serialu.",
                            "target_lang_translations": {
                                "ru": "Известный актер снялся <w>в качестве приглашенного гостя</w> в одной из серий популярного сериала."
                            }
                        },
                        {
                            "text": "Zespół rockowy zaprosił skrzypaczkę, aby zagrała <w>gościnnie</w> na ich nowej płycie.",
                            "target_lang_translations": {
                                "ru": "Рок-группа пригласила скрипачку, чтобы она сыграла <w>в качестве гостя</w> на их новом альбоме."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wystąpić gościnnie",
                        "zagrać gościnnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "В качестве приглашенного участника в художественном проекте или мероприятии, не являясь его постоянным членом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в качестве гостя",
                                "target_lang_sense_clarification": "о выступлении приглашенного артиста"
                            },
                            {
                                "target_lang_word": "на правах гостя",
                                "target_lang_sense_clarification": "участие по приглашению"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "642be09f-4cd8-3454-83af-48663f351f25",
                    "sense_definition": "Относящийся к механике как науке, а также к устройству и работе механизмов.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "science_and_devices",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старые <w>механические</w> часы всё ещё точно показывают время.",
                            "target_lang_translations": {
                                "en": "The old <w>mechanical</w> watch still keeps accurate time."
                            }
                        },
                        {
                            "text": "При ударе автомобиль получил серьёзные <w>механические</w> повреждения.",
                            "target_lang_translations": {
                                "en": "Upon impact, the car sustained serious <w>mechanical</w> damage."
                            }
                        }
                    ],
                    "common_phrases": [
                        "механические часы",
                        "механическая энергия",
                        "механическое повреждение",
                        "механические свойства"
                    ],
                    "target_lang_definitions": {
                        "en": "Relating to mechanics as a science, as well as to the structure and operation of mechanisms."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mechanical",
                                "target_lang_sense_clarification": "relating to machines or physics"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b90e5e1f-972d-34ca-b6dd-1762e0717ea4",
                    "sense_definition": "Осуществляемый с помощью машин и механизмов, а не вручную.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "production_method",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На заводе используется <w>механическая</w> обработка деталей для повышения точности.",
                            "target_lang_translations": {
                                "en": "The factory uses <w>mechanical</w> processing of parts to improve precision."
                            }
                        },
                        {
                            "text": "Тяжёлый <w>механический</w> труд постепенно заменяется роботами.",
                            "target_lang_translations": {
                                "en": "Heavy <w>mechanical</w> labor is gradually being replaced by robots."
                            }
                        }
                    ],
                    "antonyms": [
                        "ручной"
                    ],
                    "common_phrases": [
                        "механическая обработка",
                        "механический цех",
                        "механический способ",
                        "механическая дойка"
                    ],
                    "target_lang_definitions": {
                        "en": "Carried out with the help of machines and mechanisms, rather than manually."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mechanical",
                                "target_lang_sense_clarification": "done by a machine"
                            },
                            {
                                "target_lang_word": "machine",
                                "target_lang_sense_clarification": "attributive use, e.g. machine processing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57b8994a-62c7-39c7-97d2-484015067a22",
                    "sense_definition": "Совершаемый автоматически, без участия сознания; непроизвольный.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ученикам не поможет простое <w>механическое</w> запоминание правил без понимания.",
                            "target_lang_translations": {
                                "en": "Simple <w>rote</w> memorization of rules without understanding will not help students."
                            }
                        },
                        {
                            "text": "Он сделал <w>механический</w> жест рукой, словно отгоняя назойливую муху.",
                            "target_lang_translations": {
                                "en": "He made a <w>mechanical</w> gesture with his hand, as if swatting away a pesky fly."
                            }
                        }
                    ],
                    "synonyms": [
                        "машинальный"
                    ],
                    "antonyms": [
                        "осознанный"
                    ],
                    "common_phrases": [
                        "механическое запоминание",
                        "механическое движение",
                        "механический жест",
                        "механическая привычка"
                    ],
                    "target_lang_definitions": {
                        "en": "Performed automatically, without conscious awareness; involuntary."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mechanical",
                                "target_lang_sense_clarification": "lacking thought or spontaneity"
                            },
                            {
                                "target_lang_word": "automatic",
                                "target_lang_sense_clarification": "done without thinking"
                            },
                            {
                                "target_lang_word": "rote",
                                "target_lang_sense_clarification": "learned or done by repetition"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3463e3ea-d42d-386e-aaf6-bbd1babb92d5",
                    "sense_definition": "Present participle and gerund of <w>search</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>searching</w> for my keys in the living room.",
                            "target_lang_translations": {
                                "ru": "Я <w>ищу</w> свои ключи в гостиной."
                            }
                        },
                        {
                            "text": "They were <w>searching</w> the woods for the missing dog.",
                            "target_lang_translations": {
                                "ru": "Они <w>обыскивали</w> лес в поисках пропавшей собаки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола search (искать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искать",
                                "target_lang_sense_clarification": "пытаться найти кого-либо или что-либо"
                            },
                            {
                                "target_lang_word": "обыскивать",
                                "target_lang_sense_clarification": "осматривать место или человека с целью найти что-то спрятанное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0fe77672-1408-3e53-ab0f-d8fbb5564053",
                    "sense_definition": "The act of looking carefully for something or trying to find answers.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her constant <w>searching</w> for the truth led her to study philosophy.",
                            "target_lang_translations": {
                                "ru": "Её постоянный <w>поиск</w> истины привел её к изучению философии."
                            }
                        },
                        {
                            "text": "After much soul <w>searching</w>, he decided to change his career path.",
                            "target_lang_translations": {
                                "ru": "После долгих душевных <w>исканий</w> он решил сменить карьеру."
                            }
                        }
                    ],
                    "synonyms": [
                        "hunt",
                        "quest",
                        "search"
                    ],
                    "common_phrases": [
                        "soul searching",
                        "searching for answers"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, заключающееся во внимательном поиске чего-либо или попытке найти ответы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поиск",
                                "target_lang_sense_clarification": "процесс отыскания чего-либо"
                            },
                            {
                                "target_lang_word": "искание",
                                "target_lang_sense_clarification": "стремление найти что-то (часто духовное или абстрактное)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7495e9ad-261e-3d02-9807-3db882b2e1ea",
                    "sense_definition": "Done in a very thorough, critical, and detailed way to find the truth.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "thoroughness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The journalist asked several <w>searching</w> questions that made the politician uncomfortable.",
                            "target_lang_translations": {
                                "ru": "Журналист задал несколько <w>дотошных</w> вопросов, которые заставили политика чувствовать себя неловко."
                            }
                        },
                        {
                            "text": "The committee conducted a <w>searching</w> inquiry into the cause of the accident.",
                            "target_lang_translations": {
                                "ru": "Комитет провел <w>тщательное</w> расследование причин аварии."
                            }
                        }
                    ],
                    "synonyms": [
                        "thorough",
                        "rigorous",
                        "exhaustive"
                    ],
                    "antonyms": [
                        "superficial"
                    ],
                    "common_phrases": [
                        "searching questions",
                        "searching inquiry",
                        "searching examination"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделанный очень тщательно, критически и подробно с целью выяснения истины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тщательный",
                                "target_lang_sense_clarification": "подробный, глубокий (об исследовании)"
                            },
                            {
                                "target_lang_word": "дотошный",
                                "target_lang_sense_clarification": "въедливый, направленный на выяснение деталей (о вопросах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df2de5ae-9249-3fc0-8658-0f9d6602e431",
                    "sense_definition": "Examining someone closely and keenly, often making them feel that their thoughts are being read.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "thoroughness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She gave him a <w>searching</w> look to see if he was telling the truth.",
                            "target_lang_translations": {
                                "ru": "Она бросила на него <w>испытующий</w> взгляд, чтобы понять, говорит ли он правду."
                            }
                        },
                        {
                            "text": "His <w>searching</w> gaze seemed to look right through me.",
                            "target_lang_translations": {
                                "ru": "Его <w>проницательный</w> взгляд, казалось, видел меня насквозь."
                            }
                        }
                    ],
                    "synonyms": [
                        "penetrating",
                        "piercing",
                        "keen",
                        "observant"
                    ],
                    "antonyms": [
                        "blank",
                        "uninterested"
                    ],
                    "common_phrases": [
                        "searching look",
                        "searching gaze",
                        "searching glance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внимательно и проницательно изучающий кого-либо, часто заставляющий чувствовать, что мысли читаются."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "испытующий",
                                "target_lang_sense_clarification": "проверяющий, оценивающий (о взгляде)"
                            },
                            {
                                "target_lang_word": "проницательный",
                                "target_lang_sense_clarification": "острый, замечающий скрытое"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
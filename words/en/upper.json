                },
                {
                    "sense_id": "a9c6aaec-0634-3f16-923d-0d10d590178b",
                    "sense_definition": "Situated on higher ground, further inland, or towards the north.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The explorers decided to travel by boat to the <w>upper</w> reaches of the river.",

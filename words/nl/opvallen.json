{
    "word_family": [
        "opvallend",
        "opvallendheid"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f8949f1d-934e-3f9f-b685-afc17ba5ffb6",
                    "sense_definition": "De aandacht trekken of duidelijk zichtbaar zijn, vaak door af te wijken van het gewone",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "stand_out",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met haar felgekleurde kleding <valt> zij altijd <op> in de menigte.",
                            "target_lang_translations": {
                                "en": "With her brightly colored clothes she always <w>stands</w> <w>out</w> in the crowd."
                            }
                        },
                        {
                            "text": "Hij probeerde tijdens de vergadering niet <op> te <vallen> door stil te blijven.",
                            "target_lang_translations": {
                                "en": "He tried not to <w>stand</w> <w>out</w> during the meeting by staying quiet."
                            }
                        }
                    ],
                    "synonyms": [
                        "in het oog springen",
                        "de aandacht trekken"
                    ],
                    "antonyms": [
                        "wegvallen"
                    ],
                    "common_phrases": [
                        "in positieve zin opvallen",
                        "niet opvallen"
                    ],
                    "target_lang_definitions": {
                        "en": "To attract attention or be clearly visible, often by deviating from the ordinary"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stand out",
                                "target_lang_sense_clarification": "to be easily seen or noticeable due to being different"
                            },
                            {
                                "target_lang_word": "catch the eye",
                                "target_lang_sense_clarification": "to attract attention visually"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04cf6922-6968-31ae-9cbc-7d3c0b5be7c8",
                    "sense_definition": "Waargenomen worden door iemand; tot het bewustzijn van iemand doordringen",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "notice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <is> mij <opgevallen> dat de prijzen in de supermarkt zijn gestegen.",
                            "target_lang_translations": {
                                "en": "I <w>noticed</w> that the prices in the supermarket have risen."
                            }
                        },
                        {
                            "text": "<Viel> het jou ook <op> dat hij vandaag een nieuwe bril droeg?",
                            "target_lang_translations": {
                                "en": "Did you also <w>notice</w> that he was wearing new glasses today?"
                            }
                        }
                    ],
                    "synonyms": [
                        "opgemerkt worden"
                    ],
                    "antonyms": [
                        "ontgaan"
                    ],
                    "common_phrases": [
                        "het viel me op",
                        "het valt op dat"
                    ],
                    "target_lang_definitions": {
                        "en": "To be perceived by someone; to come to someone's realization"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "notice",
                                "target_lang_sense_clarification": "to become aware of something"
                            },
                            {
                                "target_lang_word": "strike",
                                "target_lang_sense_clarification": "to cause someone to realize or feel something (e.g., it struck me)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
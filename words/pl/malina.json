                },
                {
                    "sense_id": "cc6751d9-2b1b-30d0-a382-93f5b6869291",
                    "sense_definition": "Nazwisko używane w Polsce.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Malina</w> jest naszym nowym sąsiadem z naprzeciwka.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Малина</w> — наш новый сосед напротив."
                            }
                        },
                        {
                            "text": "Nie znam nikogo, kto nosiłby nazwisko <w>Malina</w>.",
                            "target_lang_translations": {

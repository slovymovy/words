    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c76f612b-b554-37cd-be89-e79a380e8fbe",
                    "sense_definition": "To accept or tolerate something unpleasant, difficult, or annoying.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "endure_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I cannot <w>bear</w> the thought of him leaving so soon.",
                            "target_lang_translations": {
                                "ru": "Я не могу <w>вынести</w> мысль о том, что он уезжает так скоро."
                            }
                        },
                        {
                            "text": "Please <w>bear</w> with me while I try to fix this error.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>потерпите</w>, пока я пытаюсь исправить эту ошибку."
                            }
                        },
                        {
                            "text": "The pain was almost too much to <w>bear</w>.",
                            "target_lang_translations": {
                                "ru": "Боль было почти невозможно <w>терпеть</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "tolerate",
                        "stand",
                        "endure",
                        "abide",
                        "stomach"
                    ],
                    "common_phrases": [
                        "can't bear",
                        "bear with",
                        "hard to bear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать или терпеть что-то неприятное, трудное или раздражающее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "терпеть",
                                "target_lang_sense_clarification": "стойко переносить страдания или неудобства"
                            },
                            {
                                "target_lang_word": "выносить",
                                "target_lang_sense_clarification": "быть в состоянии терпеть (часто в отрицательных предложениях)"
                            },
                            {
                                "target_lang_word": "переносить",
                                "target_lang_sense_clarification": "переживать трудности или боль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef5df737-1b54-3ea5-89cd-d75c312943aa",
                    "sense_definition": "To support the weight of something or someone.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "carry_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old bridge can no longer <w>bear</w> heavy traffic.",
                            "target_lang_translations": {
                                "ru": "Старый мост больше не может <w>выдерживать</w> интенсивное движение."
                            }
                        },
                        {
                            "text": "The ice is too thin to <w>bear</w> your weight.",
                            "target_lang_translations": {
                                "ru": "Лёд слишком тонок, чтобы <w>выдержать</w> твой вес."
                            }
                        }
                    ],
                    "synonyms": [
                        "support",
                        "sustain",
                        "hold up"
                    ],
                    "common_phrases": [
                        "bear the weight",
                        "bear the load"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поддерживать вес чего-либо или кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выдерживать",
                                "target_lang_sense_clarification": "не ломаться и не падать под тяжестью"
                            },
                            {
                                "target_lang_word": "держать",
                                "target_lang_sense_clarification": "служить опорой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a617db2e-2d76-3977-8d83-a724f1463340",
                    "sense_definition": "To have or display a visible mark, feature, or characteristic.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "display_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>bears</w> a striking resemblance to his grandfather.",
                            "target_lang_translations": {
                                "ru": "Он <w>имеет</w> поразительное сходство со своим дедом."
                            }
                        },
                        {
                            "text": "The document <w>bore</w> the signature of the president.",
                            "target_lang_translations": {
                                "ru": "Документ <w>содержал</w> подпись президента."
                            }
                        }
                    ],
                    "synonyms": [
                        "display",
                        "exhibit",
                        "show",
                        "have"
                    ],
                    "common_phrases": [
                        "bear a resemblance",
                        "bear the name",
                        "bear a signature"
                    ],
                    "target_lang_definitions": {

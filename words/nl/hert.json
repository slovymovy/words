            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5bd7d6f9-a378-3d22-b9a2-e961450533c5",
                    "sense_definition": "Een herkauwend zoogdier uit de familie Cervidae, waarvan het mannetje meestal een gewei draagt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens onze wandeling in het bos zagen we een <w>hert</w> tussen de bomen.",
                            "target_lang_translations": {
                                "ru": "Во время нашей прогулки по лесу мы увидели <w>оленя</w> среди деревьев."
                            }
                        },
                        {
                            "text": "Het <w>hert</w> rende snel weg toen het ons hoorde aankomen.",
                            "target_lang_translations": {
                                "ru": "<w>Олень</w> быстро убежал, когда услышал, что мы приближаемся."
                            }
                        }
                    ],
                    "common_phrases": [
                        "schuw hert",
                        "wild hert"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жвачное млекопитающее из семейства оленевых, самец которого обычно носит рога."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "олень",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1aed7b66-5e18-384e-a16d-2afae6dab1e7",
                    "sense_definition": "Het vlees van een hert, gegeten als wildgerecht.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met Kerstmis staat er vaak <w>hert</w> met rodekool op het menu.",
                            "target_lang_translations": {
                                "ru": "На Рождество в меню часто бывает <w>оленина</w> с краснокочанной капустой."
                            }
                        },
                        {
                            "text": "Ik heb nog nooit <w>hert</w> gegeten, maar het schijnt erg lekker te zijn.",
                            "target_lang_translations": {
                                "ru": "Я никогда не ел <w>оленину</w>, но говорят, что это очень вкусно."
                            }
                        }
                    ],
                    "synonyms": [
                        "hertenvlees"
                    ],
                    "common_phrases": [
                        "hert eten",
                        "biefstuk van hert"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мясо оленя, употребляемое в пищу как дичь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оленина",
                                "target_lang_sense_clarification": "мясо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6fecc5bd-8736-39b0-9663-617f28043c10",
                    "sense_definition": "Een verouderde of dialectische vorm van <w>hart</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In oude gedichten lees je soms dat iemand pijn in zijn <w>hert</w> heeft.",
                            "target_lang_translations": {
                                "ru": "В старых стихах иногда можно прочитать, что у кого-то болит <w>сердце</w>."
                            }
                        },
                        {
                            "text": "Mijn <w>hert</w> verlangt naar rust en vrede.",
                            "target_lang_translations": {
                                "ru": "Мое <w>сердце</w> жаждет покоя и мира."
                            }
                        }
                    ],
                    "traits": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "513d375e-008e-35dc-b1a0-9413884a1398",
                    "sense_definition": "Хирургически удалить повреждённую ткань, часть органа или инородное тело.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "medical_removal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хирург принял решение <иссечь> повреждённые края раны для лучшего заживления.",
                            "target_lang_translations": {
                                "en": "The surgeon decided to <w>excise</w> the damaged edges of the wound for better healing."
                            }
                        },
                        {
                            "text": "Врачам пришлось полностью <иссечь> опухоль, чтобы предотвратить её рост.",
                            "target_lang_translations": {
                                "en": "The doctors had to completely <w>excise</w> the tumor to prevent its growth."
                            }
                        }
                    ],
                    "synonyms": [
                        "вы́резать",
                        "удали́ть"
                    ],
                    "common_phrases": [
                        "иссечь рану",
                        "иссечь рубец",
                        "иссечь ткань"
                    ],
                    "target_lang_definitions": {
                        "en": "To surgically remove damaged tissue, part of an organ, or a foreign body."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "excise",
                                "target_lang_sense_clarification": "medical context"
                            },
                            {
                                "target_lang_word": "cut out",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "resect",
                                "target_lang_sense_clarification": "medical, usually regarding organs"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf9f5c38-f107-3978-86c5-bc280ddbd97d",
                    "sense_definition": "Покрыть поверхность множеством порезов, трещин или глубоких линий (часто о морщинах или шрамах).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "surface_damage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Глубокие морщины успели <иссечь> его лицо за долгие годы тяжёлой работы.",
                            "target_lang_translations": {
                                "en": "Deep wrinkles managed to <w>furrow</w> his face over long years of hard work."
                            }
                        },
                        {
                            "text": "Осколки разбитого стекла могут сильно <иссечь> руки, если не соблюдать осторожность.",
                            "target_lang_translations": {
                                "en": "Shards of broken glass can severely <w>lacerate</w> your hands if you are not careful."
                            }
                        }
                    ],
                    "synonyms": [
                        "пересе́чь"
                    ],
                    "common_phrases": [
                        "иссечь лицо морщинами",
                        "иссечь поверхность"
                    ],
                    "target_lang_definitions": {
                        "en": "To cover a surface with many cuts, cracks, or deep lines (often regarding wrinkles or scars)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "furrow",
                                "target_lang_sense_clarification": "regarding wrinkles or deep lines"
                            },
                            {
                                "target_lang_word": "lacerate",
                                "target_lang_sense_clarification": "to cut skin or flesh deeply or roughly"
                            },
                            {
                                "target_lang_word": "scar",
                                "target_lang_sense_clarification": "to mark with scars"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65bfa1ff-2f92-3f9c-a382-b89a4bfd8089",
                    "sense_definition": "Вырубить или высечь форму, изображение из твёрдого материала (камня, мрамора).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "carving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Скульптор задумал <иссечь> величественную статую из цельной глыбы гранита.",
                            "target_lang_translations": {
                                "en": "The sculptor planned to <w>hew</w> a majestic statue from a single block of granite."
                            }
                        },
                        {
                            "text": "Древние мастера умели искусно <иссечь> узоры на твёрдом камне.",
                            "target_lang_translations": {
                                "en": "Ancient masters knew how to skillfully <w>carve</w> patterns on hard stone."
                            }
                        }
                    ],
                    "synonyms": [
                        "вы́рубить",
                        "вы́сечь"
                    ],
                    "common_phrases": [
                        "иссечь из камня",
                        "иссечь из мрамора"
                    ],
                    "target_lang_definitions": {
                        "en": "To hew or carve a shape or image out of hard material (stone, marble)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hew",
                                "target_lang_sense_clarification": "to shape stone or wood with sharp blows"
                            },
                            {
                                "target_lang_word": "carve",
                                "target_lang_sense_clarification": "general term for shaping material"
                            },
                            {
                                "target_lang_word": "chisel out",
                                "target_lang_sense_clarification": "using a chisel"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f771e5cc-2daf-37c4-baea-cd1069349933",
                    "sense_definition": "Lekka torba służąca do przenoszenia zakupów, wykonana z tworzywa sztucznego lub plecionki.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "container_objects",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sklepie poprosiłem o foliową <w>siatkę</w>, żeby spakować owoce.",
                            "target_lang_translations": {
                                "ru": "В магазине я попросил полиэтиленовый <w>пакет</w>, чтобы упаковать фрукты."
                            }
                        },
                        {
                            "text": "Ta <w>siatka</w> na zakupy jest bardzo wytrzymała i ekologiczna.",
                            "target_lang_translations": {
                                "ru": "Эта <w>сетка</w> для покупок очень прочная и экологичная."
                            }
                        }
                    ],
                    "synonyms": [
                        "reklamówka",
                        "torba"
                    ],
                    "common_phrases": [
                        "siatka na zakupy",
                        "foliowa siatka",
                        "pełna siatka",
                        "nosić w siatce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкая сумка для переноски покупок, сделанная из пластика или плетенки."
                    },
                    "translations": {

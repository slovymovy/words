{
    "word_family": [
        "brygadzista",
        "brygadier",
        "brygadowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "beb2461b-74ab-31e3-88a0-cf82231966c5",
                    "sense_definition": "Zorganizowany zespół ludzi wykonujący określoną pracę albo zadanie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W lesie <w>brygada</w> pracowników leśnych zajmowała się wyrębem drzew.",
                            "target_lang_translations": {
                                "ru": "В лесу <w>бригада</w> лесных рабочих занималась вырубкой деревьев."
                            }
                        },
                        {
                            "text": "Nasza <w>brygada</w> skończyła remont dachu przed terminem.",
                            "target_lang_translations": {
                                "ru": "Наша <w>бригада</w> закончила ремонт крыши досрочно."
                            }
                        }
                    ],
                    "synonyms": [
                        "zespół",
                        "ekipa",
                        "drużyna",
                        "grupa"
                    ],
                    "common_phrases": [
                        "brygada remontowa",
                        "brygada budowlana",
                        "brygada pracy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организованная группа людей, выполняющая определенную работу или задачу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бригада",
                                "target_lang_sense_clarification": "рабочая группа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a4821c7d-4f81-3503-b641-31e939ffbfbb",
                    "sense_definition": "Związek taktyczny złożony z kilku pułków lub batalionów, będący częścią dywizji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "military_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na początku wojny każdej <w>brygadzie</w> podlegała bateria artyleryjska.",
                            "target_lang_translations": {
                                "ru": "В начале войны каждой <w>бригаде</w> подчинялась артиллерийская батарея."
                            }
                        },
                        {
                            "text": "Żołnierze z tej <w>brygady</w> zostali wysłani na misję pokojową.",
                            "target_lang_translations": {
                                "ru": "Солдаты из этой <w>бригады</w> были отправлены с миротворческой миссией."
                            }
                        }
                    ],
                    "synonyms": [
                        "jednostka",
                        "oddział"
                    ],
                    "common_phrases": [
                        "generał brygady",
                        "brygada pancerna",
                        "brygada piechoty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тактическое соединение, состоящее из нескольких полков или батальонов, являющееся частью дивизии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бригада",
                                "target_lang_sense_clarification": "воинское соединение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a260db45-6784-3ca4-bcc2-25126976e294",
                    "sense_definition": "Samodzielna jednostka organizacyjna marynarki wojennej, składająca się z przynajmniej dwóch dywizjonów.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Brygada</w> okrętów podwodnych wypłynęła z portu o świcie.",
                            "target_lang_translations": {
                                "ru": "<w>Бригада</w> подводных лодок вышла из порта на рассвете."
                            }
                        },
                        {
                            "text": "Dowódca <w>brygady</w> wydał rozkaz do manewrów na morzu.",
                            "target_lang_translations": {
                                "ru": "Командир <w>бригады</w> отдал приказ о маневрах в море."
                            }
                        }
                    ],
                    "common_phrases": [
                        "brygada okrętów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самостоятельная организационная единица военно-морского флота, состоящая как минимум из двух дивизионов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бригада",
                                "target_lang_sense_clarification": "соединение кораблей"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "podległość",
        "podległy",
        "niepodległość",
        "niepodległy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "89117ff9-3be4-3a34-8bf9-1020248ca460",
                    "sense_definition": "Znajdować się pod czyjąś władzą, kontrolą lub kierownictwem; być służbowo zależnym od kogoś.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "hierarchy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszej firmie każdy pracownik <w>podlega</w> bezpośrednio kierownikowi działu.",
                            "target_lang_translations": {
                                "ru": "В нашей фирме каждый сотрудник <w>подчиняется</w> непосредственно руководителю отдела."
                            }
                        },
                        {
                            "text": "Żołnierze w armii bezwzględnie <w>podlegają</w> rozkazom swoich dowódców.",
                            "target_lang_translations": {
                                "ru": "Солдаты в армии беспрекословно <w>подчиняются</w> приказам своих командиров."
                            }
                        }
                    ],
                    "synonyms": [
                        "zależeć"
                    ],
                    "antonyms": [
                        "rządzić",
                        "kierować"
                    ],
                    "common_phrases": [
                        "podlegać dyrektorowi",
                        "podlegać szefowi",
                        "podlegać ministerstwu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находиться под чьей-либо властью, контролем или руководством; быть в служебной зависимости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подчиняться",
                                "target_lang_sense_clarification": "слушаться приказов, быть зависимым по службе"
                            },
                            {
                                "target_lang_word": "находиться в подчинении",
                                "target_lang_sense_clarification": "быть в зависимости от начальника"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e68abb81-f080-3e7f-a3b3-839f2b3656f1",
                    "sense_definition": "Być objętym działaniem określonych praw, przepisów, kar lub procesów; musieć się do nich stosować.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "rules_and_processes",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Towar zakupiony na wyprzedaży nie <w>podlega</w> zwrotowi ani wymianie.",
                            "target_lang_translations": {
                                "ru": "Товар, купленный на распродаже, не <w>подлежит</w> возврату или обмену."
                            }
                        },
                        {
                            "text": "Wszystkie dochody z pracy za granicą <w>podlegają</w> opodatkowaniu w kraju.",
                            "target_lang_translations": {
                                "ru": "Все доходы от работы за границей <w>подлежат</w> налогообложению в стране."
                            }
                        },
                        {
                            "text": "Ta sprawa jest oczywista i nie <w>podlega</w> żadnej dyskusji.",
                            "target_lang_translations": {
                                "ru": "Это дело очевидно и не <w>подлежит</w> никакому обсуждению."
                            }
                        }
                    ],
                    "common_phrases": [
                        "podlegać karze",
                        "podlegać opodatkowaniu",
                        "podlegać zwrotowi",
                        "podlegać dyskusji",
                        "podlegać zmianie",
                        "podlegać prawu"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подпадать под действие определенных законов, правил или процедур; быть обязательным для исполнения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подлежать",
                                "target_lang_sense_clarification": "быть предметом чего-либо (обсуждения, возврата, налогообложения)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b500d0a-ddf3-30ce-a1d5-70f6efc39abd",
                    "sense_definition": "Dawać się opanować silnym uczuciom, nastrojom lub zewnętrznym wpływom.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie powinieneś tak łatwo <w>podlegać</w> presji otoczenia i zmieniać zdania.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует так легко <w>поддаваться</w> давлению окружения и менять свое мнение."
                            }
                        },
                        {
                            "text": "W trudnych chwilach człowiek często <w>podlega</w> skrajnym emocjom.",
                            "target_lang_translations": {
                                "ru": "В трудные моменты человек часто <w>поддается</w> крайним эмоциям."
                            }
                        }
                    ],
                    "synonyms": [
                        "ulegać",
                        "poddawać się"
                    ],
                    "antonyms": [
                        "opierać się"
                    ],
                    "common_phrases": [
                        "podlegać emocjom",
                        "podlegać nastrojom",
                        "podlegać sugestii",
                        "podlegać wpływom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Позволять сильным чувствам, настроениям или внешнему влиянию овладеть собой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поддаваться",
                                "target_lang_sense_clarification": "уступать давлению, влиянию или эмоциям"
                            },
                            {
                                "target_lang_word": "подвергаться",
                                "target_lang_sense_clarification": "испытывать на себе воздействие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
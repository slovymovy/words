{
    "word_family": [
        "possible",
        "possibility",
        "impossible",
        "impossibly"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c65307b6-9054-37b9-8c68-0cced087ff09",
                    "sense_definition": "Used to state that something might be true or might happen, but is not certain.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is <w>possibly</w> the best movie I have ever seen.",
                            "target_lang_translations": {
                                "ru": "Это, <w>возможно</w>, лучший фильм, который я когда-либо видел."
                            }
                        },
                        {
                            "text": "<w>Possibly</w>, we will go to the beach tomorrow if the weather is nice.",
                            "target_lang_translations": {
                                "ru": "<w>Возможно</w>, завтра мы пойдем на пляж, если погода будет хорошей."
                            }
                        }
                    ],
                    "synonyms": [
                        "potentially#Adverb"
                    ],
                    "antonyms": [
                        "certainly#Adverb",
                        "inevitably#Adverb",
                        "undoubtedly#Adverb",
                        "no doubt#Adverb"
                    ],
                    "common_phrases": [
                        "quite possibly",
                        "very possibly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется, чтобы заявить, что что-то может быть правдой или может случиться, но полной уверенности в этом нет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возможно",
                                "target_lang_sense_clarification": "вероятность"
                            },
                            {
                                "target_lang_word": "может быть",
                                "target_lang_sense_clarification": "неуверенность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4eaec1af-82ad-3185-83bc-bcc7a2bbeb20",
                    "sense_definition": "Used with 'can' or 'could' to emphasize that something is achievable, or in negative sentences to say something cannot happen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I cannot <w>possibly</w> finish all this work by noon.",
                            "target_lang_translations": {
                                "ru": "Я <w>никак</w> не могу закончить всю эту работу к полудню."
                            }
                        },
                        {
                            "text": "How could he <w>possibly</w> know the answer to that question?",
                            "target_lang_translations": {
                                "ru": "Откуда он <w>вообще</w> мог знать ответ на этот вопрос?"
                            }
                        }
                    ],
                    "antonyms": [
                        "impossibly#Adverb"
                    ],
                    "common_phrases": [
                        "could possibly",
                        "cannot possibly",
                        "could not possibly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется с модальными глаголами для усиления утверждения о возможности чего-либо или для подчеркивания невозможности в отрицательных предложениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возможно",
                                "target_lang_sense_clarification": "теоретически"
                            },
                            {
                                "target_lang_word": "никак",
                                "target_lang_sense_clarification": "усиление в отрицаниях"
                            },
                            {
                                "target_lang_word": "только",
                                "target_lang_sense_clarification": "усиление в вопросах"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
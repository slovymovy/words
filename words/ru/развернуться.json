            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bd6f497c-58b0-376a-ab0c-761c084bddf4",
                    "sense_definition": "Изменить направление движения или положение тела на противоположное или иное; сделать поворот.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он резко <развернулся> и пошёл в другую сторону."
                        },
                        {
                            "text": "Машина <развернулась> на перекрёстке и уехала."
                        }
                    ],
                    "synonyms": [
                        "повернуться",
                        "повернуть"
                    ],
                    "common_phrases": [
                        "развернуться на месте",
                        "развернуться назад",
                        "резко развернуться"
                    ]
                },
                {
                    "sense_id": "f4fc36a7-a68d-3a23-9e0e-c20fd51e2250",
                    "sense_definition": "Раскрыться, распрямиться, стать плоским или широким (о чём-то свёрнутом, сложенном или упакованном).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Огромный парашют успешно <развернулся> в небе."
                        },
                        {
                            "text": "Старая карта <развернулась> на столе перед нами."
                        }
                    ],
                    "synonyms": [
                        "раскрыться",
                        "открыться",
                        "распаковаться"
                    ],
                    "antonyms": [
                        "стесняться"
                    ],
                    "common_phrases": [
                        "знамя развернулось",
                        "цветок развернулся",
                        "книга развернулась"
                    ]
                },
                {
                    "sense_id": "a4d244e4-b685-3856-a7cc-875878adbe4a",
                    "sense_definition": "Начать активно происходить, развиваться в широких масштабах (о событиях, действиях, процессах).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В городе <развернулось> масштабное строительство новых домов."
                        },
                        {
                            "text": "Горячая дискуссия <развернулась> вокруг этой темы."
                        }
                    ],
                    "synonyms": [
                        "расшириться",
                        "разрастись"
                    ],
                    "common_phrases": [
                        "события развернулись",
                        "борьба развернулась",
                        "работа развернулась"
                    ]
                },
                {
                    "sense_id": "3d14f596-906c-348d-88c5-14cd33a94dba",
                    "sense_definition": "Проявить свои способности, таланты или начать действовать свободно, с полным размахом.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "human_behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его талант художника полностью <развернулся> только в зрелости."
                        },
                        {
                            "text": "На празднике гости <развернулись> и веселились до утра."
                        }
                    ],
                    "synonyms": [
                        "показать себя",
                        "разойтись"
                    ],
                    "antonyms": [
                        "стесняться"
                    ],
                    "common_phrases": [

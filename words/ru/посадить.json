                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "stain_mark",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я случайно <посадил> жирное пятно на новую рубашку.",
                            "target_lang_translations": {
                                "en": "I accidentally <got> a grease stain on my new shirt."
                            }
                        },
                        {
                            "text": "Ребёнок бегал босиком и <посадил> занозу.",
                            "target_lang_translations": {
                                "en": "The child was running barefoot and <got> a splinter."
                            }
                        }
                    ],
                    "common_phrases": [
                        "посадить пятно",
                        "посадить синяк",
                        "посадить занозу"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "11e23621-6350-396c-9ee7-846233533353",
                    "sense_definition": "Zmiana kierunku ruchu w bok lub miejsce, w którym następuje ta zmiana.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca wykonał gwałtowny <w>skręt</w> w prawo, aby uniknąć zderzenia.",
                            "target_lang_translations": {
                                "ru": "Водитель сделал резкий <w>поворот</w> направо, чтобы избежать столкновения."
                            }
                        },
                        {
                            "text": "Na tym skrzyżowaniu obowiązuje zakaz <w>skrętu</w> w lewo.",
                            "target_lang_translations": {
                                "ru": "На этом перекрестке запрещен <w>поворот</w> налево."
                            }
                        }
                    ],
                    "common_phrases": [
                        "skręt w lewo",
                        "skręt w prawo",
                        "zakaz skrętu",
                        "ostry skręt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменение направления движения в сторону или место, где происходит это изменение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поворот",
                                "target_lang_sense_clarification": "изменение направления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3dc91e5-5c55-3177-81e0-5dfb3a642389",
                    "sense_definition": "Papieros zrobiony ręcznie z tytoniu i bibułki.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "smoking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek woli palić własne <w>skręty</w> niż kupować gotowe papierosy.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка предпочитает курить свои <w>самокрутки</w>, а не покупать готовые сигареты."
                            }
                        },
                        {
                            "text": "Siedział na ławce i powoli robił sobie <w>skręta</w>.",
                            "target_lang_translations": {
                                "ru": "Он сидел на скамейке и медленно делал себе <w>самокрутку</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "palić skręta",
                        "robić skręta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сигарета, скрученная вручную из табака и папиросной бумаги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "самокрутка",
                                "target_lang_sense_clarification": "самодельная папироса"
                            },
                            {
                                "target_lang_word": "цигарка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd7d8f7c-a56a-3824-8b91-87e040b5bdbd",
                    "sense_definition": "Ręcznie zrobiony papieros zawierający marihuanę.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "smoking_drugs",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na imprezie ktoś poczęstował go <w>skrętem</w>, ale odmówił.",
                            "target_lang_translations": {
                                "ru": "На вечеринке кто-то угостил его <w>косяком</w>, но он отказался."
                            }
                        },
                        {
                            "text": "Policja znalazła u zatrzymanego jednego <w>skręta</w>.",
                            "target_lang_translations": {
                                "ru": "Полиция нашла у задержанного один <w>косяк</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "blant",
                        "joint",
                        "gibon"
                    ],
                    "common_phrases": [
                        "palić skręta"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common term for a joint"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Скрученная вручную сигарета с марихуаной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "косяк",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ee38b96-134b-3542-8d20-2b8e70c0bf12",
                    "sense_definition": "Kształt powstały przez owinięcie czegoś wokół osi lub naturalne ułożenie się czegoś w spiralę (np. włosów).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej włosy mają naturalny, bardzo ładny <w>skręt</w>.",
                            "target_lang_translations": {
                                "ru": "У ее волос естественный, очень красивый <w>завиток</w>."
                            }
                        },
                        {
                            "text": "Lina miała mocny <w>skręt</w>, przez co trudno było ją rozplątać.",
                            "target_lang_translations": {
                                "ru": "У веревки была сильная <w>скрутка</w>, из-за чего ее было трудно распутать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "skręt włosów",
                        "skręt liny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма, образованная обматыванием чего-либо вокруг оси или естественным укладыванием чего-либо в спираль (например, волос)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "завиток",
                                "target_lang_sense_clarification": "волос"
                            },
                            {
                                "target_lang_word": "скрутка",
                                "target_lang_sense_clarification": "веревки, провода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ccdadf1-6b65-3b6e-89e4-cbe178aeb0ae",
                    "sense_definition": "Niebezpieczne dla zdrowia obrócenie się narządu wewnętrznego wokół własnej osi.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Skręt</w> żołądka u dużych psów wymaga natychmiastowej interwencji chirurga.",
                            "target_lang_translations": {
                                "ru": "<w>Заворот</w> желудка у крупных собак требует немедленного хирургического вмешательства."
                            }
                        },
                        {
                            "text": "Lekarze podejrzewali u pacjenta <w>skręt</w> jelit.",
                            "target_lang_translations": {
                                "ru": "Врачи подозревали у пациента <w>заворот</w> кишок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "skręt kiszek",
                        "skręt żołądka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Опасный для здоровья поворот внутреннего органа вокруг собственной оси."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заворот",
                                "target_lang_sense_clarification": "медицинское"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a270c26-0aa3-3f66-b11d-f2ec850cd2d5",
                    "sense_definition": "Bardzo złe samopoczucie fizyczne i psychiczne występujące po odstawieniu narkotyków.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "medical_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Narkoman bał się bolesnego <w>skręta</w>, dlatego nie chciał iść na odwyk.",
                            "target_lang_translations": {
                                "ru": "Наркоман боялся болезненной <w>ломки</w>, поэтому не хотел идти на реабилитацию."
                            }
                        },
                        {
                            "text": "Janek miał strasznego <w>skręta</w>, gdy próbował rzucić heroinę.",
                            "target_lang_translations": {
                                "ru": "У Янека была ужасная <w>ломка</w>, когда он пытался бросить героин."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mieć skręta"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "drug withdrawal symptoms"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень плохое физическое и психическое самочувствие, возникающее после прекращения приема наркотиков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ломка",
                                "target_lang_sense_clarification": "абстинентный синдром"
                            }
                        ]
                    }
                }
            ]
        },
        {

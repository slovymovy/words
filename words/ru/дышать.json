    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0cb76713-5265-34da-9b4f-540025a45701",
                    "sense_definition": "Втягивать и выпускать воздух лёгкими (или другими органами) для получения кислорода; совершать дыхание.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "biological_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Человек не может жить под водой, потому что ему нужно <w>дышать</w> воздухом.",
                            "target_lang_translations": {
                                "en": "A human cannot live underwater because they need to <w>breathe</w> air."
                            }
                        },
                        {
                            "text": "После быстрого бега спортсмен тяжело <w>дышал</w> и не мог говорить.",
                            "target_lang_translations": {
                                "en": "After a fast run, the athlete was <w>breathing</w> heavily and could not speak."
                            }
                        }
                    ],
                    "antonyms": [
                        "задыхаться"
                    ],
                    "common_phrases": [
                        "тяжело дышать",
                        "дышать носом",
                        "дышать ртом",
                        "дышать свежим воздухом"
                    ],
                    "target_lang_definitions": {
                        "en": "To inhale and exhale air using the lungs to obtain oxygen; to perform the act of breathing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "breathe",
                                "target_lang_sense_clarification": "biological process"
                            },
                            {
                                "target_lang_word": "respire",
                                "target_lang_sense_clarification": "technical/biological term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3921dd94-c353-368f-92ba-452c9424d291",
                    "sense_definition": "Пропускать воздух через поры, волокна или отверстия (о материале, ткани, коже).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "permeability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Летом лучше носить одежду из хлопка, так как эта ткань хорошо <w>дышит</w>.",
                            "target_lang_translations": {
                                "en": "In summer, it is better to wear cotton clothing, as this fabric <w>breathes</w> well."
                            }
                        },
                        {
                            "text": "В этой тесной синтетической куртке тело совсем не <w>дышит</w>.",
                            "target_lang_translations": {
                                "en": "In this tight synthetic jacket, the body does not <w>breathe</w> at all."
                            }
                        }
                    ],
                    "synonyms": [
                        "пропускать воздух"
                    ],
                    "common_phrases": [
                        "кожа дышит",
                        "ткань дышит",
                        "материал дышит"
                    ],
                    "target_lang_definitions": {
                        "en": "To allow air to pass through pores or fibers (referring to material, fabric, or skin)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "breathe",
                                "target_lang_sense_clarification": "permeability of material"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "421bd96b-0d2e-36b1-a7d4-a23cbc2ddc7e",
                    "sense_definition": "Выпускать струю тёплого воздуха ртом на что-либо (обычно чтобы согреть или увлажнить).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "exhaling_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Девочка забыла перчатки дома и теперь <w>дышала</w> на озябшие руки.",
                            "target_lang_translations": {
                                "en": "The girl forgot her gloves at home and was now <w>breathing</w> on her frozen hands."
                            }
                        },
                        {
                            "text": "Он <w>дышал</w> на холодное стекло, чтобы нарисовать на нём смешную рожицу.",
                            "target_lang_translations": {
                                "en": "He <w>breathed</w> on the cold glass to draw a funny face on it."
                            }
                        }
                    ],
                    "common_phrases": [
                        "дышать на руки",
                        "дышать на стекло",
                        "дышать в ладони"
                    ],
                    "target_lang_definitions": {
                        "en": "To exhale warm air onto something (usually to warm it up or create moisture/fog)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "breathe",
                                "target_lang_sense_clarification": "exhale onto something"
                            },
                            {
                                "target_lang_word": "blow",
                                "target_lang_sense_clarification": "exhale gently"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41a29c0f-aef0-30b4-a6ab-5eed570b9bd0",
                    "sense_definition": "Быть живым, существовать.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "existence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я буду любить и помнить тебя до тех пор, пока я <w>дышу</w>.",
                            "target_lang_translations": {
                                "en": "I will love and remember you as long as I <w>breathe</w>."
                            }
                        },
                        {
                            "text": "Раненый солдат лежал неподвижно, но врачи увидели, что он ещё <w>дышит</w>.",
                            "target_lang_translations": {
                                "en": "The wounded soldier lay motionless, but the doctors saw that he was still <w>breathing</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "жить",
                        "существовать"
                    ],
                    "antonyms": [
                        "умереть"
                    ],
                    "common_phrases": [
                        "пока я дышу",
                        "еле дышит"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in high style or dramatic contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be alive; to exist."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "breathe",
                                "target_lang_sense_clarification": "to be alive"
                            },
                            {
                                "target_lang_word": "live",
                                "target_lang_sense_clarification": "synonym for existing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95e8c9f7-4263-3c73-b9ed-17c179c8d9bc",
                    "sense_definition": "Явно выражать собой какое-либо сильное чувство, состояние или качество (о лице, виде, тексте).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_expression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его лицо <w>дышало</w> спокойствием и уверенностью в победе.",
                            "target_lang_translations": {
                                "en": "His face <w>radiated</w> calm and confidence in victory."
                            }
                        },
                        {
                            "text": "Каждая строчка её последнего письма <w>дышала</w> любовью и нежностью.",
                            "target_lang_translations": {
                                "en": "Every line of her last letter <w>breathed</w> love and tenderness."
                            }
                        }
                    ],
                    "synonyms": [
                        "излучать"
                    ],
                    "common_phrases": [
                        "дышать злобой",
                        "дышать любовью",
                        "дышать добротой"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary style"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To clearly express or radiate a strong feeling, state, or quality (of a face, appearance, or text)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "radiate",
                                "target_lang_sense_clarification": "emit a feeling or quality"
                            },
                            {
                                "target_lang_word": "exude",
                                "target_lang_sense_clarification": "display an emotion strongly"
                            },
                            {
                                "target_lang_word": "breathe",
                                "target_lang_sense_clarification": "literary/metaphorical"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1433a6e-87b4-3001-ba3b-c82fa9a259df",
                    "sense_definition": "Быть полностью поглощённым чем-либо, жить какими-либо интересами.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "devotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она безумно любит своего сына и буквально <w>дышит</w> им.",
                            "target_lang_translations": {
                                "en": "She loves her son madly and literally <w>lives and breathes</w> for him."
                            }
                        },
                        {
                            "text": "Мы хотели узнать, чем <w>дышит</w> современная молодёжь и какие у неё интересы.",
                            "target_lang_translations": {
                                "en": "We wanted to find out what modern youth <w>lives by</w> and what their interests are."
                            }
                        }
                    ],
                    "common_phrases": [
                        "чем он дышит",
                        "дышать только им"
                    ],
                    "target_lang_definitions": {
                        "en": "To be completely absorbed in something; to live for specific interests."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "live and breathe",
                                "target_lang_sense_clarification": "be devoted to"
                            },
                            {
                                "target_lang_word": "live for",
                                "target_lang_sense_clarification": "focus one's life on"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35951f41-9c28-32dc-8591-a4ff01e417a4",
                    "sense_definition": "Издавать запах, веять теплом или прохладой (о природе, времени года).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nature_wafting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Летний вечер <w>дышал</w> прохладой и ароматом скошенной травы.",
                            "target_lang_translations": {
                                "en": "The summer evening <w>breathed</w> coolness and the aroma of cut grass."
                            }
                        },
                        {
                            "text": "Весенний сад <w>дышал</w> свежестью после короткого дождя.",
                            "target_lang_translations": {
                                "en": "The spring garden <w>breathed</w> freshness after a short rain."
                            }
                        }
                    ],
                    "synonyms": [
                        "веять"
                    ],
                    "common_phrases": [
                        "дышать прохладой",
                        "небо осенью дышало"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "poetic/literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To emit a scent or radiate warmth/coolness (referring to nature or seasons)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "breathe",
                                "target_lang_sense_clarification": "emanate (poetic)"
                            },
                            {
                                "target_lang_word": "emanate",
                                "target_lang_sense_clarification": "give out (smell/feeling)"
                            }
                        ]
                    }
                }
            ]
        }

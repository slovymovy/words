    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e5df7f29-09f5-30f6-b822-e18837d2d7de",
                    "sense_definition": "Poruszać się w wodzie, na jej powierzchni lub w powietrzu (o ludziach, zwierzętach, statkach, chmurach)",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten duży statek pasażerski <w>płynie</w> do portu w Gdańsku.",
                            "target_lang_translations": {
                                "ru": "Этот большой пассажирский корабль <w>плывет</w> в порт Гданьска."
                            }
                        },
                        {
                            "text": "Białe chmury wolno <w>płyną</w> po błękitnym niebie.",
                            "target_lang_translations": {
                                "ru": "Белые облака медленно <w>плывут</w> по голубому небу."
                            }
                        },
                        {
                            "text": "Musimy <w>płynąć</w> z prądem rzeki, żeby oszczędzać siły.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>плыть</w> по течению реки, чтобы беречь силы."
                            }
                        }
                    ],
                    "synonyms": [
                        "żeglować"
                    ],
                    "antonyms": [
                        "iść"
                    ],
                    "common_phrases": [
                        "płynąć statkiem",
                        "płynąć z prądem",
                        "płynąć pod prąd",
                        "płynąć wpław"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передвигаться в воде, по её поверхности или в воздухе (о людях, животных, кораблях, облаках)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плыть",
                                "target_lang_sense_clarification": "о движении в воде или воздухе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c982ff76-2519-3e7c-9372-96963254a9ad",
                    "sense_definition": "Przemieszczać się ciągłym strumieniem w określonym kierunku (o cieczach, rzekach)",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "motion_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wisła <w>płynie</w> przez całą Polskę aż do Morza Bałtyckiego.",
                            "target_lang_translations": {
                                "ru": "Висла <w>течет</w> через всю Польшу до самого Балтийского моря."
                            }
                        },
                        {
                            "text": "Z małej rany na palcu <w>płynęła</w> krew.",
                            "target_lang_translations": {
                                "ru": "Из маленькой раны на пальце <w>текла</w> кровь."
                            }
                        }
                    ],
                    "synonyms": [
                        "spływać"
                    ],
                    "common_phrases": [
                        "rzeka płynie",
                        "krew płynie",
                        "łzy płyną",
                        "woda płynie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаться непрерывным потоком в определённом направлении (о жидкостях, реках)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "течь",
                                "target_lang_sense_clarification": "о жидкости, реках"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffafa8a1-a6c5-30e5-8bb8-32606017d6de",
                    "sense_definition": "Mijać, przemijać (o czasie)",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "motion_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czas na wakacjach <w>płynie</w> znacznie szybciej niż w pracy.",
                            "target_lang_translations": {
                                "ru": "Время в отпуске <w>течет</w> гораздо быстрее, чем на работе."
                            }
                        },
                        {
                            "text": "Godziny <w>płynęły</w>, a my wciąż rozmawialiśmy o przyszłości.",
                            "target_lang_translations": {
                                "ru": "Часы <w>шли</w>, а мы всё разговаривали о будущем."
                            }
                        }
                    ],
                    "synonyms": [
                        "mijać",
                        "upływać",
                        "przemijać",
                        "biec"
                    ],
                    "common_phrases": [
                        "czas płynie",
                        "życie płynie",
                        "godziny płyną",
                        "lata płyną"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проходить, миновать (о времени)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "течь",
                                "target_lang_sense_clarification": "проходить (о времени)"
                            },
                            {
                                "target_lang_word": "проходить",
                                "target_lang_sense_clarification": "миновать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2a5d986-4356-3da1-9929-986cec532aba",
                    "sense_definition": "Rozprzestrzeniać się w przestrzeni, dobiegać skądś (o dźwiękach, zapachach, prądzie)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "motion_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z otwartego okna <w>płynęła</w> piękna muzyka fortepianowa.",
                            "target_lang_translations": {
                                "ru": "Из открытого окна <w>лилась</w> прекрасная фортепианная музыка."
                            }
                        },
                        {
                            "text": "Przez ten gruby kabel <w>płynie</w> prąd o wysokim napięciu.",
                            "target_lang_translations": {
                                "ru": "По этому толстому кабелю <w>течет</w> ток высокого напряжения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "muzyka płynie",
                        "prąd płynie",
                        "słowa płyną"
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространяться в пространстве, доноситься откуда-то (о звуках, запахах, электричестве)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доноситься",
                                "target_lang_sense_clarification": "о звуках, запахах"
                            },
                            {
                                "target_lang_word": "литься",
                                "target_lang_sense_clarification": "о музыке, свете"
                            },
                            {
                                "target_lang_word": "течь",
                                "target_lang_sense_clarification": "об электрическом токе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90b2a038-7bb5-3000-bb62-a831142180ec",
                    "sense_definition": "Wynikać z czegoś, być czegoś skutkiem",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "logical_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z tej trudnej sytuacji <w>płyną</w> dla nas bardzo ważne wnioski.",
                            "target_lang_translations": {
                                "ru": "Из этой сложной ситуации <w>вытекают</w> для нас очень важные выводы."
                            }
                        },
                        {
                            "text": "Wszelkie korzyści <w>płynące</w> z tej umowy są opisane w dokumencie.",
                            "target_lang_translations": {
                                "ru": "Все выгоды, <w>вытекающие</w> из этого договора, описаны в документе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "korzyści płynące z",
                        "wnioski płynące z",
                        "nauka płynąca z"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть следствием чего-либо, логически следовать из фактов"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вытекать",
                                "target_lang_sense_clarification": "быть следствием"
                            },
                            {
                                "target_lang_word": "следовать",
                                "target_lang_sense_clarification": "логически"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e399580d-0871-323b-9555-405a248a1444",
                    "sense_definition": "Mówić nie na temat lub tracić wątek wypowiedzi; improwizować w sposób niepewny",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "colloquial_speech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Student nie znał odpowiedzi na pytanie i zaczął strasznie <w>płynąć</w>.",
                            "target_lang_translations": {
                                "ru": "Студент не знал ответа на вопрос и начал страшно <w>плавать</w>."
                            }
                        },
                        {
                            "text": "Kiedy zapomniał tekstu piosenki, musiał <w>płynąć</w>, żeby uratować występ.",
                            "target_lang_translations": {
                                "ru": "Когда он забыл текст песни, ему пришлось <w>импровизировать</w>, чтобы спасти выступление."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in school or exam context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить не по теме, терять нить разговора или неуверенно импровизировать"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плавать",
                                "target_lang_sense_clarification": "разг. быть неуверенным в знаниях"
                            },
                            {
                                "target_lang_word": "лить воду",
                                "target_lang_sense_clarification": "разг. говорить пустое"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
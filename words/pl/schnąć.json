            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f53da9f3-ca0a-3405-a961-f7a0823b202e",
                    "sense_definition": "Stawać się suchym, tracić wilgoć pod wpływem powietrza lub ciepła.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_drying",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mokre ubrania powieszone na sznurku w ogrodzie szybko <schną> na wietrze.",
                            "target_lang_translations": {
                                "ru": "Мокрая одежда, развешанная на веревке в саду, быстро <сохнет> на ветру."
                            }
                        },
                        {
                            "text": "Musimy poczekać, aż farba na ścianach całkowicie <wyschnie>, zanim wstawimy meble.",
                            "target_lang_translations": {
                                "ru": "Мы должны подождать, пока краска на стенах полностью <высохнет>, прежде чем ставить мебель."
                            }
                        }
                    ],
                    "synonyms": [
                        "wysychać",
                        "suszyć się"
                    ],
                    "antonyms": [
                        "moknąć"
                    ],
                    "common_phrases": [
                        "pranie schnie",
                        "farba schnie",
                        "szybko schnąć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться сухим, терять влагу под воздействием воздуха или тепла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сохнуть",
                                "target_lang_sense_clarification": "становиться сухим"
                            },
                            {
                                "target_lang_word": "высыхать",
                                "target_lang_sense_clarification": "полностью терять влагу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e1c31ec-6034-3d1e-979d-435eeb9227d5",
                    "sense_definition": "O roślinach: tracić życie, świeżość i zielony kolor, zazwyczaj z powodu braku wody.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "biological_withering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeśli nie będziesz regularnie podlewać kwiatów, zaczną <schnąć> i opadać.",
                            "target_lang_translations": {
                                "ru": "Если ты не будешь регулярно поливать цветы, они начнут <сохнуть> и опадать."
                            }
                        },
                        {
                            "text": "Podczas upalnego lata trawa w parku zaczęła <schnąć> i żółknąć.",
                            "target_lang_translations": {
                                "ru": "Во время жаркого лета трава в парке начала <сохнуть> и желтеть."
                            }
                        }
                    ],
                    "synonyms": [
                        "więdnąć",
                        "usychać"
                    ],
                    "antonyms": [
                        "rozkwitać"
                    ],
                    "common_phrases": [
                        "kwiaty schną",
                        "drzewa schną"
                    ],
                    "target_lang_definitions": {
                        "ru": "О растениях: терять жизнь, свежесть и зеленый цвет, обычно из-за недостатка воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сохнуть",
                                "target_lang_sense_clarification": "о растениях"
                            },
                            {
                                "target_lang_word": "засыхать",
                                "target_lang_sense_clarification": "погибать от засухи"
                            },
                            {
                                "target_lang_word": "вянуть",
                                "target_lang_sense_clarification": "увядать, терять свежесть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18c96916-3670-37af-8ff2-1861ba38f2a7",
                    "sense_definition": "Bardzo szybko chudnąć i tracić siły witalne, często na skutek choroby lub zmartwień.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "human_health",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Od kiedy zachorował, zaczął <schnąć> w oczach i wszyscy się o niego martwią.",
                            "target_lang_translations": {
                                "ru": "С тех пор как он заболел, он начал <сохнуть> на глазах, и все за него беспокоятся."
                            }
                        },
                        {
                            "text": "Biedne dziecko <schnie> z głodu, potrzebuje natychmiastowej pomocy.",
                            "target_lang_translations": {
                                "ru": "Бедный ребенок <сохнет> от голода, ему нужна немедленная помощь."
                            }
                        }
                    ],
                    "synonyms": [
                        "mizernieć",
                        "usychać"
                    ],
                    "antonyms": [
                        "tyć"
                    ],
                    "common_phrases": [
                        "schnąć w oczach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень быстро худеть и терять жизненные силы, часто вследствие болезни или переживаний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сохнуть",
                                "target_lang_sense_clarification": "худеть, истощаться"
                            },
                            {
                                "target_lang_word": "чахнуть",
                                "target_lang_sense_clarification": "угасать от болезни"
                            },
                            {
                                "target_lang_word": "иссыхать",
                                "target_lang_sense_clarification": "становиться очень худым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa0ce5a5-3fa6-3f18-bd04-17fda58bb675",
                    "sense_definition": "Cierpieć psychicznie i tracić chęć do życia z powodu silnych emocji, zwłaszcza tęsknoty lub nieszczęśliwej miłości.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emotional_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bohater romantycznej powieści <schnie> z tęsknoty za swoją ukochaną, która wyjechała.",
                            "target_lang_translations": {
                                "ru": "Герой романтического романа <сохнет> от тоски по своей возлюбленной, которая уехала."
                            }
                        },
                        {
                            "text": "Nie możesz tak <schnąć> z żalu po stracie, musisz spróbować żyć dalej.",
                            "target_lang_translations": {
                                "ru": "Ты не можешь так <сохнуть> от горя после утраты, ты должен попытаться жить дальше."
                            }
                        }
                    ],
                    "synonyms": [
                        "usychać"
                    ],
                    "common_phrases": [
                        "schnąć z tęsknoty",
                        "schnąć z miłości"
                    ],
                    "target_lang_definitions": {
                        "ru": "Страдать душевно и терять интерес к жизни из-за сильных эмоций, особенно тоски или несчастной любви."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сохнуть",
                                "target_lang_sense_clarification": "страдать от любви или тоски"
                            },
                            {
                                "target_lang_word": "изнывать",
                                "target_lang_sense_clarification": "мучиться душевно"
                            }
                        ]
                    }
                }
            ]
        }
    ]

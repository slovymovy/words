    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3e415663-8bb6-39fa-bf2f-d12b0a7d9aab",
                    "sense_definition": "To display or allow something to be seen by others.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "display_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>show</w> me your passport before you enter.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>покажите</w> мне ваш паспорт перед входом."
                            }
                        },
                        {
                            "text": "He wanted to <w>show</w> us the new car he bought yesterday.",
                            "target_lang_translations": {
                                "ru": "Он хотел <w>показать</w> нам новую машину, которую купил вчера."
                            }
                        }
                    ],
                    "synonyms": [
                        "display",
                        "exhibit",
                        "reveal",
                        "show off",
                        "point out"
                    ],
                    "antonyms": [
                        "hide",
                        "conceal",
                        "cover up"
                    ],
                    "common_phrases": [
                        "show me",
                        "show off",
                        "show around"
                    ],
                    "target_lang_definitions": {
                        "ru": "Показывать или давать возможность увидеть что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пока́зывать",
                                "target_lang_sense_clarification": "несов. вид"
                            },
                            {
                                "target_lang_word": "показа́ть",
                                "target_lang_sense_clarification": "сов. вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56e87631-eb66-36b8-a6ab-64c2f12e0a94",
                    "sense_definition": "To indicate that a fact is true; to demonstrate or prove something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "prove_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The results of the study <w>show</w> a clear improvement in health.",
                            "target_lang_translations": {
                                "ru": "Результаты исследования <w>показывают</w> явное улучшение здоровья."
                            }
                        },
                        {
                            "text": "Can you <w>show</w> that this theory is actually correct?",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>доказать</w>, что эта теория действительно верна?"
                            }
                        }
                    ],
                    "synonyms": [
                        "demonstrate",
                        "prove",
                        "indicate"
                    ],
                    "antonyms": [
                        "disprove",
                        "refute"
                    ],
                    "common_phrases": [
                        "studies show",
                        "show that",
                        "show how"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывать на истинность факта; демонстрировать или доказывать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пока́зывать",
                                "target_lang_sense_clarification": "свидетельствовать"
                            },
                            {
                                "target_lang_word": "дока́зывать",
                                "target_lang_sense_clarification": "подтверждать фактами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b91842f-15d4-349d-a34a-afacd338dcd1",
                    "sense_definition": "To guide or escort someone to a specific place.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "guide_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The receptionist will <w>show</w> you to the meeting room now.",
                            "target_lang_translations": {
                                "ru": "Администратор сейчас <w>проводит</w> вас в переговорную."
                            }
                        },
                        {
                            "text": "It is late, so let me <w>show</w> you out.",
                            "target_lang_translations": {
                                "ru": "Уже поздно, позвольте мне вас <w>проводить</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "show someone out",
                        "show someone in",
                        "show the way"
                    ],
                    "target_lang_definitions": {
                        "ru": "Провожать или сопровождать кого-либо в определенное место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "провожа́ть",
                                "target_lang_sense_clarification": "сопровождать"
                            },
                            {
                                "target_lang_word": "проводи́ть",
                                "target_lang_sense_clarification": "указать путь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d39a89dd-e3bb-32ae-8708-abb908369d30",
                    "sense_definition": "To be visible or apparent; to be seen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "visible_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The stain on your shirt will not <w>show</w> if you wear a jacket.",
                            "target_lang_translations": {
                                "ru": "Пятна на рубашке не <w>будет видно</w>, если вы наденете пиджак."
                            }
                        },
                        {
                            "text": "Her disappointment started to <w>show</w> despite her smile.",
                            "target_lang_translations": {
                                "ru": "Её разочарование начало <w>проявляться</w>, несмотря на улыбку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "it shows",
                        "starting to show"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть видимым или заметным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть ви́дным",
                                "target_lang_sense_clarification": "быть заметным"
                            },
                            {
                                "target_lang_word": "видне́ться",
                                "target_lang_sense_clarification": "показываться"
                            },
                            {
                                "target_lang_word": "проявля́ться",
                                "target_lang_sense_clarification": "становиться явным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b13e8dc6-7aa3-340f-9b44-52c0df958c77",
                    "sense_definition": "To put in an appearance; to arrive.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "arrive_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We waited for an hour, but he never <w>showed</w>.",
                            "target_lang_translations": {
                                "ru": "Мы прождали час, но он так и не <w>появился</w>."
                            }
                        },
                        {
                            "text": "She finally <w>showed</w> up at the party just before midnight.",
                            "target_lang_translations": {
                                "ru": "Она наконец <w>появилась</w> на вечеринке незадолго до полуночи."
                            }
                        }
                    ],
                    "synonyms": [
                        "arrive",
                        "show up"
                    ],
                    "common_phrases": [
                        "show up",
                        "never showed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Появляться; приходить куда-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "появля́ться",
                                "target_lang_sense_clarification": "приходить"
                            },
                            {
                                "target_lang_word": "объявля́ться",
                                "target_lang_sense_clarification": "разг. приходить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "662bdc66-a0d0-3b7d-8698-a2f373751a3a",
                    "sense_definition": "To bestow or confer something, such as mercy or respect.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "bestow_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The king refused to <w>show</w> mercy to the rebels.",
                            "target_lang_translations": {
                                "ru": "Король отказался <w>проявить</w> милосердие к мятежникам."
                            }
                        },
                        {
                            "text": "You must <w>show</w> respect to your elders at all times.",
                            "target_lang_translations": {
                                "ru": "Вы должны всегда <w>проявлять</w> уважение к старшим."
                            }
                        }
                    ],
                    "common_phrases": [
                        "show mercy",
                        "show respect",
                        "show favor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проявлять (чувство, отношение) или оказывать (милость, уважение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проявля́ть",
                                "target_lang_sense_clarification": "демонстрировать чувство"
                            },
                            {
                                "target_lang_word": "ока́зывать",
                                "target_lang_sense_clarification": "предоставлять (честь, уважение)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03e9a358-fd98-36df-a210-4f4e0c3c2354",
                    "sense_definition": "To have a visible enlarged belly due to pregnancy.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "pregnancy_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is six months pregnant and is really starting to <w>show</w>.",
                            "target_lang_translations": {
                                "ru": "Она на шестом месяце беременности, и живот уже действительно <w>заметен</w>."
                            }
                        },
                        {
                            "text": "Even though she was due soon, she was barely <w>showing</w>.",
                            "target_lang_translations": {
                                "ru": "Хотя ей скоро рожать, живот был едва <w>заметен</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь заметный живот (о беременности)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть заме́тным",
                                "target_lang_sense_clarification": "о животе при беременности"
                            }
                        ]
                    }
                }
            ]
        },
        {

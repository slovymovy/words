    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "00224648-3ec7-3956-a0d8-7e4c72814c80",
                    "sense_definition": "Lekka kurtka chroniąca przed wiatrem i deszczem, często wykonana z materiałów syntetycznych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wycieczkę w góry przyda ci się <w>wiatrówka</w>, bo pogoda jest zmienna.",
                            "target_lang_translations": {
                                "ru": "Для похода в горы тебе пригодится <w>ветровка</w>, так как погода переменчива."
                            }
                        },
                        {
                            "text": "Moja nowa <w>wiatrówka</w> jest bardzo lekka i zajmuje mało miejsca w plecaku.",
                            "target_lang_translations": {
                                "ru": "Моя новая <w>ветровка</w> очень легкая и занимает мало места в рюкзаке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "lekka wiatrówka",
                        "wiatrówka z kapturem",
                        "nosić wiatrówkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкая куртка, защищающая от ветра и дождя, часто изготовленная из синтетических материалов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ветровка",
                                "target_lang_sense_clarification": "куртка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9e98951-4f4e-31ca-a327-fd40e4064769",
                    "sense_definition": "Broń pneumatyczna, z której strzela się śrutem za pomocą sprężonego powietrza, używana głównie w celach sportowych i rekreacyjnych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "weapon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek uczył mnie strzelać do puszek, używając starej <w>wiatrówki</w>.",
                            "target_lang_translations": {
                                "ru": "Моя дедушка учил меня стрелять по банкам из старой <w>воздушки</w>."
                            }
                        },
                        {
                            "text": "Do posiadania tej <w>wiatrówki</w> nie jest wymagane pozwolenie na broń.",
                            "target_lang_translations": {
                                "ru": "Для владения этой <w>пневматической винтовкой</w> не требуется разрешение на оружие."
                            }
                        }
                    ],
                    "common_phrases": [
                        "strzelać z wiatrówki",
                        "śrut do wiatrówki",
                        "celna wiatrówka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пневматическое оружие, стреляющее дробью с помощью сжатого воздуха, используемое в основном в спортивных и развлекательных целях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пневматическая винтовка",
                                "target_lang_sense_clarification": "оружие"
                            },
                            {
                                "target_lang_word": "воздушка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97c2d6b8-13d2-32cd-a6e1-733a7f6ee8f1",
                    "sense_definition": "Potoczne określenie ospy wietrznej, choroby zakaźnej objawiającej się swędzącą wysypką.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "disease",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Większość dzieci przechodzi <w>wiatrówkę</w> w wieku przedszkolnym.",
                            "target_lang_translations": {
                                "ru": "Большинство детей переносит <w>ветрянку</w> в дошкольном возрасте."
                            }
                        },
                        {
                            "text": "Kiedy zachorowałem na <w>wiatrówkę</w>, musiałem zostać w domu przez dwa tygodnie.",
                            "target_lang_translations": {
                                "ru": "Когда я заболел <w>ветрянкой</w>, мне пришлось остаться дома на две недели."
                            }
                        }
                    ],
                    "synonyms": [
                        "ospa wietrzna",
                        "ospa"
                    ],
                    "common_phrases": [
                        "zachorować na wiatrówkę",
                        "zarazić się wiatrówką",
                        "blizny po wiatrówce"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common name for the disease"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название ветряной оспы, инфекционного заболевания, проявляющегося зудящей сыпью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ветрянка",
                                "target_lang_sense_clarification": "болезнь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffea8150-ad72-3131-8c2e-1489907f6db6",
                    "sense_definition": "Elektrownia wiatrowa lub zespół turbin wiatrowych wytwarzających energię elektryczną.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "energy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszej gminie powstała nowa <w>wiatrówka</w>, która zasila okoliczne wsie.",
                            "target_lang_translations": {
                                "ru": "В нашей общине появилась новая <w>ветроэлектростанция</w>, которая питает окрестные деревни."
                            }
                        },
                        {
                            "text": "Mieszkańcy protestowali przeciwko postawieniu głośnej <w>wiatrówki</w> blisko ich domów.",
                            "target_lang_translations": {
                                "ru": "Жители протестовали против установки шумного <w>ветряка</w> рядом с их домами."
                            }
                        }
                    ],
                    "synonyms": [
                        "elektrownia wiatrowa",
                        "farma wiatrowa"
                    ],
                    "common_phrases": [
                        "budowa wiatrówki",
                        "energia z wiatrówki"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ветряная электростанция или группа ветряных турбин, вырабатывающих электроэнергию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ветряк",
                                "target_lang_sense_clarification": "турбина"
                            },
                            {
                                "target_lang_word": "ветроэлектростанция",
                                "target_lang_sense_clarification": "электростанция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c87d74d7-4814-3190-a9a9-f5168f94093e",
                    "sense_definition": "Deska lub listwa wykończeniowa mocowana na szczytowej krawędzi dachu, chroniąca pokrycie przed podwiewaniem.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dekarz przymocował <w>wiatrówkę</w>, aby zabezpieczyć dach przed silnym wiatrem.",
                            "target_lang_translations": {
                                "ru": "Кровельщик прикрепил <w>ветровую доску</w>, чтобы защитить крышу от сильного ветра."
                            }
                        },
                        {
                            "text": "Musimy wymienić zniszczoną <w>wiatrówkę</w> przy kominie, bo zaczyna gnić.",
                            "target_lang_translations": {
                                "ru": "Нам нужно заменить гнилую <w>ветровую доску</w> у дымохода."
                            }
                        }
                    ],
                    "synonyms": [
                        "wiatrownica"
                    ],
                    "common_phrases": [
                        "montaż wiatrówki",
                        "drewniana wiatrówka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доска или отделочная планка, закрепляемая на торцевом краю крыши для защиты покрытия от ветра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ветровая доска",
                                "target_lang_sense_clarification": "строительство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "025fcd3f-aff1-3465-bdf5-46a5f32df22a",
                    "sense_definition": "Lekki, letni but, zazwyczaj z wycięciami, noszony dawniej w cieplejsze dni.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starszy pan spacerował po parku, mając na nogach wygodne <w>wiatrówki</w>.",
                            "target_lang_translations": {
                                "ru": "Пожилой господин гулял по парку в удобных <w>летних туфлях</w>."
                            }
                        },
                        {
                            "text": "Na lato kupiłem sobie przewiewne <w>wiatrówki</w>, żeby stopa się nie pociła.",
                            "target_lang_translations": {
                                "ru": "На лето я купил себе дышащие <w>летние туфли</w>, чтобы нога не потела."
                            }
                        }
                    ],
                    "common_phrases": [
                        "skórzane wiatrówki",
                        "letnie wiatrówki"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкие летние туфли, обычно с вырезами, которые носили раньше в теплую погоду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "летние туфли",
                                "target_lang_sense_clarification": "обувь"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
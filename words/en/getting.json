                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что было получено, добыто или заработано; прибыль; доход."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добыча",
                                "target_lang_sense_clarification": "то, что добыто, часто нечестным путем или на охоте"
                            },
                            {
                                "target_lang_word": "выручка",

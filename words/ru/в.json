{
    "word_family": [],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "b38b6d9f-62af-3e4d-9914-34045d690481",
                    "sense_definition": "Указывает на нахождение предмета или лица внутри чего-либо, в пределах какого-либо пространства (с предложным падежом).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой телефон лежит <в> правом кармане куртки.",
                            "target_lang_translations": {
                                "en": "My phone is <in> the right pocket of the jacket."
                            }
                        },
                        {
                            "text": "Сейчас дети находятся <в> школе на уроках.",
                            "target_lang_translations": {
                                "en": "The children are currently <at> school in class."
                            }
                        }
                    ],
                    "synonyms": [
                        "внутри",
                        "посреди"
                    ],
                    "antonyms": [
                        "вне"
                    ],
                    "common_phrases": [
                        "жить в городе",
                        "работать в офисе",
                        "быть в комнате",
                        "лежать в коробке"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates the location of an object or person inside something, within the limits of some space."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "in",
                                "target_lang_sense_clarification": "Used for enclosed spaces, containers, cities, or countries"
                            },
                            {
                                "target_lang_word": "at",
                                "target_lang_sense_clarification": "Used for locations, institutions, or events"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23556ded-fdd5-38e7-b25a-619dae551718",
                    "sense_definition": "Указывает на направление действия или движения внутрь чего-либо (с винительным падежом).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы завтра поедем <в> центр города за покупками.",
                            "target_lang_translations": {
                                "en": "Tomorrow we will go <to> the city center for shopping."
                            }
                        },
                        {
                            "text": "Положите, пожалуйста, все документы <в> эту папку.",
                            "target_lang_translations": {
                                "en": "Please put all the documents <into> this folder."
                            }
                        }
                    ],
                    "synonyms": [
                        "внутрь"
                    ],
                    "antonyms": [
                        "из"
                    ],
                    "common_phrases": [
                        "идти в школу",
                        "войти в дом",
                        "положить в сумку",
                        "убрать в ящик"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates the direction of action or movement into something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to",
                                "target_lang_sense_clarification": "Indicates movement towards a destination"
                            },
                            {
                                "target_lang_word": "into",
                                "target_lang_sense_clarification": "Indicates movement entering the inside of something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19e90f68-8e03-374e-a9ba-c5902d4f258b",
                    "sense_definition": "Указывает на время совершения действия: час, день недели, месяц или год.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наш поезд отправляется <в> семь часов утра.",
                            "target_lang_translations": {
                                "en": "Our train departs <at> seven o'clock in the morning."
                            }
                        },
                        {
                            "text": "Мы планируем поехать в отпуск <в> июле.",
                            "target_lang_translations": {
                                "en": "We plan to go on vacation <in> July."
                            }
                        }
                    ],
                    "common_phrases": [
                        "в пять часов",
                        "в среду",
                        "в прошлом году",
                        "в этот момент"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates the time of action: hour, day of the week, month, or year."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "at",
                                "target_lang_sense_clarification": "Used for specific clock times"
                            },
                            {
                                "target_lang_word": "in",
                                "target_lang_sense_clarification": "Used for months, years, or seasons"
                            },
                            {
                                "target_lang_word": "on",
                                "target_lang_sense_clarification": "Used for days of the week or specific dates"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4436598-faf2-3bd2-8045-cadf3a7f54b8",
                    "sense_definition": "Указывает на единицу измерения времени при обозначении частоты действия (значение «за» или «на каждый»).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врачи рекомендуют чистить зубы два раза <в> день.",
                            "target_lang_translations": {
                                "en": "Doctors recommend brushing teeth twice <a> day."
                            }
                        },
                        {
                            "text": "Я хожу в спортивный зал три раза <в> неделю.",
                            "target_lang_translations": {
                                "en": "I go to the gym three times <a> week."
                            }
                        }
                    ],
                    "common_phrases": [
                        "раз в год",
                        "дважды в день",
                        "три раза в неделю"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates a unit of time measurement when denoting frequency of action (meaning \"per\" or \"for every\")."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "a",
                                "target_lang_sense_clarification": "Used in frequency expressions (e.g., once a day)"
                            },
                            {
                                "target_lang_word": "per",
                                "target_lang_sense_clarification": "Used to express rates or ratios"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "222938a4-8c40-3488-9850-e4aeabcd2fcc",
                    "sense_definition": "Указывает на участие в какой-либо игре, спорте или сфере деятельности.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "activity_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой брат очень хорошо играет <в> большой теннис.",
                            "target_lang_translations": {
                                "en": "My brother plays tennis very well."
                            }
                        },
                        {
                            "text": "Дети весь вечер играли <в> прятки во дворе.",
                            "target_lang_translations": {
                                "en": "The children played hide-and-seek in the yard all evening."
                            }
                        }
                    ],
                    "common_phrases": [
                        "играть в футбол",
                        "играть в шахматы",
                        "верить в бога",
                        "играть в карты"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates participation in a game, sport, or sphere of activity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "(no preposition)",
                                "target_lang_sense_clarification": "Usually omitted in English when playing games or sports"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4f9617a-1e97-3ff0-951c-d40a3d0f00ca",
                    "sense_definition": "Указывает на одежду, в которую кто-либо одет, или на внешние признаки предмета (цвет, узор).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "attribute_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На праздник пришла красивая девочка <в> розовом платье.",
                            "target_lang_translations": {
                                "en": "A beautiful girl <in> a pink dress came to the party."
                            }
                        },
                        {
                            "text": "Мне очень нравится эта ткань <в> мелкий горошек.",
                            "target_lang_translations": {
                                "en": "I really like this fabric <with> small polka dots."
                            }
                        }
                    ],
                    "common_phrases": [
                        "женщина в красном",
                        "рубашка в клетку",
                        "одет в костюм",
                        "люди в черном"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates clothing someone is wearing, or external features of an object (color, pattern)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "in",
                                "target_lang_sense_clarification": "Used to describe what someone is wearing"
                            },
                            {
                                "target_lang_word": "with",
                                "target_lang_sense_clarification": "Used to describe patterns or features"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5dcc524f-c617-3450-9871-b4fffaddba68",
                    "sense_definition": "Указывает на объект, через который направлено действие или взгляд.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "location_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она задумчиво смотрела <в> окно на проходящих людей.",
                            "target_lang_translations": {
                                "en": "She looked thoughtfully <out> the window at the passing people."
                            }
                        },
                        {
                            "text": "Свет проникал <в> щель под дверью.",
                            "target_lang_translations": {
                                "en": "Light penetrated <through> the crack under the door."
                            }
                        }
                    ],
                    "synonyms": [
                        "через",
                        "сквозь"
                    ],
                    "common_phrases": [
                        "смотреть в окно",
                        "глядеть в бинокль",
                        "говорить в микрофон"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates an object through which an action or gaze is directed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "out",
                                "target_lang_sense_clarification": "Used when looking from inside to outside"
                            },
                            {
                                "target_lang_word": "through",
                                "target_lang_sense_clarification": "Used when movement or vision passes from one side to the other"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eee3aac7-db32-31d2-8a8a-b9b9589f4935",
                    "sense_definition": "Указывает на результат превращения, изменения состояния или статуса.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "activity_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ваза упала со стола и разбилась <в> мелкие осколки.",
                            "target_lang_translations": {
                                "en": "The vase fell from the table and smashed <into> small fragments."
                            }
                        },
                        {
                            "text": "Он решил пойти <в> летчики, когда вырастет.",
                            "target_lang_translations": {
                                "en": "He decided to become a pilot when he grows up."
                            }
                        }
                    ],
                    "common_phrases": [
                        "превратиться в лёд",
                        "разбить вдребезги",
                        "вступить в партию",
                        "годится в сыновья"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates the result of transformation, change of state or status."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "into",
                                "target_lang_sense_clarification": "Indicates a change of form or state"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "478f0187-4383-364c-8358-8021675e2008",
                    "sense_definition": "Указывает на цель, причину или образ совершения действия.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "purpose_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он сказал это <в> шутку, чтобы разрядить обстановку.",
                            "target_lang_translations": {
                                "en": "He said it <as> a joke to defuse the situation."
                            }
                        },
                        {
                            "text": "Мы привели этот факт <в> доказательство нашей теории.",
                            "target_lang_translations": {
                                "en": "We cited this fact <as> proof of our theory."
                            }
                        }
                    ],
                    "synonyms": [
                        "с целью",
                        "ради",
                        "для"
                    ],
                    "common_phrases": [
                        "сказать в шутку",
                        "в знак дружбы",
                        "в наказание",
                        "в отместку"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates the purpose, reason, or manner of an action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "as",
                                "target_lang_sense_clarification": "Used to indicate function or role"
                            },
                            {
                                "target_lang_word": "in",
                                "target_lang_sense_clarification": "Used in phrases of manner (e.g., in jest)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d6d058d9-2f37-3c0a-84c3-5f7274fe5ba2",
                    "sense_definition": "Указывает на сходство с кем-либо (обычно о внешности или характере).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "attribute_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сын пошёл <в> отца не только внешностью, но и характером.",
                            "target_lang_translations": {
                                "en": "The son takes <after> his father not only in appearance but also in character."
                            }
                        },
                        {
                            "text": "У неё глаза <в> бабушку, такие же ярко-голубые.",
                            "target_lang_translations": {
                                "en": "She has eyes <like> her grandmother's, just as bright blue."
                            }
                        }
                    ],
                    "common_phrases": [
                        "весь в отца",
                        "характером в мать",
                        "лицом в деда"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates resemblance to someone (usually appearance or character)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "after",
                                "target_lang_sense_clarification": "Used in the phrasal verb \"take after\""
                            },
                            {
                                "target_lang_word": "like",
                                "target_lang_sense_clarification": "Indicates similarity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09902ff1-b70b-3fe9-89b1-2131f06f8bbc",
                    "sense_definition": "Используется в устойчивых выражениях для обозначения точного соответствия или идентичности.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "attribute_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Они живут душа <в> душу уже много лет.",
                            "target_lang_translations": {
                                "en": "They have lived in perfect harmony for many years."
                            }
                        },
                        {
                            "text": "Копия картины получилась один <в> один как оригинал.",
                            "target_lang_translations": {
                                "en": "The copy of the painting turned out exactly like the original."
                            }
                        }
                    ],
                    "common_phrases": [
                        "один в один",
                        "слово в слово",
                        "душа в душу",
                        "тютелька в тютельку"
                    ],
                    "target_lang_definitions": {
                        "en": "Used in set expressions to denote exact correspondence or identity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "for",
                                "target_lang_sense_clarification": "Used in idioms like \"word for word\""
                            },
                            {
                                "target_lang_word": "to",
                                "target_lang_sense_clarification": "Used in idioms of comparison"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d0371db2-aef5-3ef5-bf74-8079d15ed46c",
                    "sense_definition": "Обозначение единицы измерения электрического напряжения (вольт).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "measurement_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Напряжение в нашей электрической сети составляет двести двадцать <В>.",
                            "target_lang_translations": {
                                "en": "The voltage in our electrical network is two hundred and twenty <V>."
                            }
                        },
                        {
                            "text": "Батарейка имеет напряжение полтора <В>.",
                            "target_lang_translations": {
                                "en": "The battery has a voltage of one and a half <V>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "символ"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Designation of the unit of measurement of electrical voltage (volt)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "V",
                                "target_lang_sense_clarification": "Symbol for volt"
                            },
                            {
                                "target_lang_word": "volt",
                                "target_lang_sense_clarification": "Unit of electrical potential"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
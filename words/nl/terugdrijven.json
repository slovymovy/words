    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "17b4c92b-1977-3827-9c38-ca0f21ef4f40",
                    "sense_definition": "Mensen of dieren dwingen om terug te gaan naar de plaats waar ze vandaan kwamen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "force_back",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie moest de agressieve menigte met waterkanonnen <w>terugdrijven</w>.",
                            "target_lang_translations": {
                                "ru": "Полиции пришлось <w>оттеснить</w> агрессивную толпу водометами."
                            }
                        },
                        {
                            "text": "De koeien werden door de boer <w>teruggedreven</w> naar hun schuilplaats.",
                            "target_lang_translations": {
                                "ru": "Коровы были <w>загнаны обратно</w> фермером в укрытие."
                            }
                        }
                    ],
                    "synonyms": [
                        "terugdringen",
                        "verdrijven"
                    ],
                    "common_phrases": [
                        "de vijand terugdrijven",
                        "de menigte terugdrijven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять людей или животных вернуться туда, откуда они пришли; оттеснять силой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оттеснять",
                                "target_lang_sense_clarification": "силой заставлять отступить (о толпе, войсках)"
                            },
                            {
                                "target_lang_word": "отгонять",
                                "target_lang_sense_clarification": "заставлять уйти назад (о животных, враге)"
                            },
                            {
                                "target_lang_word": "загонять обратно",
                                "target_lang_sense_clarification": "возвращать внутрь или в укрытие (о животных)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3999e82-cf48-3fc3-adef-4ba024f82de2",
                    "sense_definition": "Een hoeveelheid, aantal of percentage aanzienlijk verminderen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "reduce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De overheid wil de werkloosheid de komende jaren verder <w>terugdrijven</w>.",
                            "target_lang_translations": {
                                "ru": "Правительство хочет в ближайшие годы еще больше <w>сократить</w> безработицу."
                            }
                        },
                        {
                            "text": "We moeten het aantal sigaretten dit jaar met ruim tien procent kunnen <w>terugdrijven</w>.",
                            "target_lang_translations": {
                                "ru": "Мы должны суметь <w>снизить</w> потребление сигарет в этом году более чем на десять процентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "terugdringen",
                        "verminderen",
                        "reduceren",
                        "beperken"
                    ],
                    "antonyms": [
                        "verhogen",
                        "vermeerderen"
                    ],
                    "common_phrases": [
                        "de kosten terugdrijven",
                        "het tekort terugdrijven",
                        "de uitstoot terugdrijven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Значительно уменьшать количество, число или процент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сокращать",
                                "target_lang_sense_clarification": "уменьшать объем или количество"
                            },
                            {
                                "target_lang_word": "снижать",
                                "target_lang_sense_clarification": "понижать уровень или показатели"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d001149b-55e9-3ae2-94c0-59636403138a",
                    "sense_definition": "Door wind of stroming langzaam terugkeren naar de oorspronkelijke positie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "drift_back",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het vlot is door de veranderde wind weer naar de kust <w>teruggedreven</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за перемены ветра плот снова <w>прибило</w> к берегу."
                            }
                        },
                        {
                            "text": "Op het uitgaande tij was het stuurloze schip naar open zee <w>teruggedreven</w>.",
                            "target_lang_translations": {
                                "ru": "Во время отлива неуправляемый корабль <w>унесло обратно</w> в открытое море."
                            }
                        }
                    ],
                    "antonyms": [
                        "wegdrijven"
                    ],
                    "common_phrases": [
                        "naar zee terugdrijven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Медленно возвращаться в исходное положение под действием ветра или течения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приплывать обратно",
                                "target_lang_sense_clarification": "возвращаться по воде (о плавучих объектах)"
                            },
                            {
                                "target_lang_word": "сносить обратно",
                                "target_lang_sense_clarification": "быть унесенным назад течением или ветром"
                            }
                        ]
                    }
                }
            ]
        }
    ]

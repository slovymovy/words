    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1eaea1ea-2780-3c0e-b982-3530c6fbc09c",
                    "sense_definition": "The department in a company responsible for calculating salaries, wages, and tax deductions.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "dept_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please submit your timesheets to <w>payroll</w> before the end of the week.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, сдайте табели учёта рабочего времени в <w>отдел расчёта зарплаты</w> до конца недели."
                            }
                        },
                        {
                            "text": "She works in <w>payroll</w> and handles all the monthly salary transfers.",
                            "target_lang_translations": {
                                "ru": "Она работает в <w>бухгалтерии</w> и занимается всеми ежемесячными перечислениями зарплаты."
                            }
                        }
                    ],
                    "common_phrases": [
                        "payroll department",
                        "process payroll",
                        "payroll tax"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдел в компании, отвечающий за начисление зарплат, окладов и налоговых вычетов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отдел расчёта заработной платы",
                                "target_lang_sense_clarification": "подразделение компании"
                            },
                            {
                                "target_lang_word": "бухгалтерия",
                                "target_lang_sense_clarification": "общее название финансового отдела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a89fd554-373d-36d7-87dc-3d763d8706c0",
                    "sense_definition": "A list of employees who receive salary or wages, together with the amounts due to each.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "list_money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company has over five hundred staff members on its <w>payroll</w>.",
                            "target_lang_translations": {
                                "ru": "В <w>штате</w> компании более пятисот сотрудников."
                            }
                        },
                        {
                            "text": "The manager checked the <w>payroll</w> to ensure the new recruit was listed.",
                            "target_lang_translations": {
                                "ru": "Менеджер проверил <w>платёжную ведомость</w>, чтобы убедиться, что новый сотрудник внесён в список."
                            }
                        }
                    ],
                    "synonyms": [
                        "paysheet"
                    ],
                    "common_phrases": [
                        "on the payroll",
                        "payroll list"
                    ],
                    "target_lang_definitions": {
                        "ru": "Список сотрудников, получающих зарплату или оклад, с указанием причитающихся сумм."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "платёжная ведомость",
                                "target_lang_sense_clarification": "документ со списком выплат"
                            },
                            {
                                "target_lang_word": "штат",
                                "target_lang_sense_clarification": "список сотрудников (в контексте 'on the payroll')"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b91cbd5-5faf-3fec-999b-4f98d4cb0650",
                    "sense_definition": "The total sum of money paid to employees by a company.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "list_money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The football club has a massive <w>payroll</w> due to its star players.",
                            "target_lang_translations": {
                                "ru": "У футбольного клуба огромный <w>фонд заработной платы</w> из-за звёздных игроков."
                            }
                        },
                        {
                            "text": "Management is trying to reduce the annual <w>payroll</w> to save money.",
                            "target_lang_translations": {
                                "ru": "Руководство пытается сократить годовой <w>фонд оплаты труда</w>, чтобы сэкономить деньги."
                            }
                        }
                    ],
                    "common_phrases": [
                        "annual payroll",
                        "payroll costs",
                        "meet the payroll"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общая сумма денег, выплачиваемая компанией сотрудникам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фонд заработной платы",
                                "target_lang_sense_clarification": "финансовый термин"
                            },
                            {
                                "target_lang_word": "расходы на персонал",
                                "target_lang_sense_clarification": "затраты компании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1bf5fcef-b69e-3ded-aaa0-a583ea05385e",
                    "sense_definition": "Bribes paid to people to secure their influence or silence.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The investigation revealed that several police officers were on the mob's <w>payroll</w>.",
                            "target_lang_translations": {
                                "ru": "Расследование показало, что несколько полицейских были <w>на содержании</w> у мафии."
                            }
                        },
                        {
                            "text": "It was rumored that the corrupt politician was on the company's <w>payroll</w>.",
                            "target_lang_translations": {
                                "ru": "Ходили слухи, что коррумпированный политик был <w>на зарплате</w> у этой компании."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on the payroll"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to imply corruption"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Взятки, выплачиваемые людям для обеспечения их влияния или молчания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на содержании",
                                "target_lang_sense_clarification": "получающий регулярные выплаты (неодобр.)"
                            },
                            {
                                "target_lang_word": "на зарплате",
                                "target_lang_sense_clarification": "ироничное использование"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e5cd37e8-9bd5-37da-8d38-917f6c81cae6",
                    "sense_definition": "To place an employee on a payroll system.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The agency will <w>payroll</w> the temporary workers for the duration of the project.",
                            "target_lang_translations": {
                                "ru": "Агентство будет <w>начислять зарплату</w> временным работникам в течение проекта."
                            }
                        },
                        {
                            "text": "They decided to <w>payroll</w> the new hires through a third-party service.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>оплачивать труд</w> новых сотрудников через сторонний сервис."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "business jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Включить сотрудника в систему начисления заработной платы или выплачивать её."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начислять зарплату",
                                "target_lang_sense_clarification": "производить выплаты"
                            },
                            {
                                "target_lang_word": "оплачивать труд",
                                "target_lang_sense_clarification": "финансировать работу"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    }
                },
                {
                    "sense_id": "4ea21006-4353-3dd9-81b2-2fc46dd5475c",
                    "sense_definition": "Używany, gdy coś jest dzielone lub rozdzielane dla kilku odbiorców.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Matka sprawiedliwie podzieliła cukierki <między> swoje dzieci.",
                            "target_lang_translations": {
                                "ru": "Мать справедливо разделила конфеты <w>между</w> своими детьми."
                            }
                        },
                        {
                            "text": "Zysk firmy został rozdzielony <między> wszystkich akcjonariuszy.",
                            "target_lang_translations": {
                                "ru": "Прибыль компании была распределена <w>между</w> всеми акционерами."
                            }
                        }
                    ],
                    "synonyms": [
                        "pomiędzy"
                    ],
                    "common_phrases": [
                        "podzielić między siebie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется при разделении или распределении чего-либо между несколькими получателями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "между",
                                "target_lang_sense_clarification": "при распределении"

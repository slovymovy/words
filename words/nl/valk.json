            "senses": [
                {
                    "sense_id": "e6a34ccb-00bc-3fbf-86e7-d0ee1382efc3",
                    "sense_definition": "Een roofvogel uit het geslacht Falco, gekenmerkt door lange, spitse vleugels en een haakvormige snavel.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>valk</w> dook met grote snelheid naar beneden om een muis te vangen.",
                            "target_lang_translations": {
                                "en": "The <w>falcon</w> dived down at high speed to catch a mouse."
                            }
                        },
                        {
                            "text": "Tijdens onze wandeling zagen we een <w>valk</w> hoog in de lucht zweven.",
                            "target_lang_translations": {
                                "en": "During our walk, we saw a <w>falcon</w> soaring high in the sky."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een snelle valk",
                        "de jagende valk"
                    ],
                    "target_lang_definitions": {
                        "en": "A bird of prey of the genus Falco, characterized by long, pointed wings and a hooked beak."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "falcon",
                                "target_lang_sense_clarification": "bird of prey"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0191c66-4ba7-3d6d-b04a-b309a3032f46",
                    "sense_definition": "Een type open zeilboot met een vaste kiel, veel gebruikt voor recreatie en zeillessen in Nederland.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben het hele weekend heerlijk gezeild in een <w>valk</w> op de Friese meren.",
                            "target_lang_translations": {
                                "en": "We sailed wonderfully all weekend in a <w>Valk</w> on the Frisian lakes."
                            }
                        },
                        {
                            "text": "Voor de beginnerscursus gebruiken ze meestal een stabiele <w>valk</w> omdat deze niet snel omslaat.",
                            "target_lang_translations": {
                                "en": "For the beginner's course, they usually use a stable <w>Valk</w> because it doesn't capsize easily."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zeilen in een valk",
                        "een open valk"
                    ],
                    "target_lang_definitions": {
                        "en": "A type of open sailboat with a fixed keel, widely used for recreation and sailing lessons in the Netherlands."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Valk",
                                "target_lang_sense_clarification": "sailboat class"
                            }

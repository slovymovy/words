            "senses": [
                {
                    "sense_id": "a89d7b1c-9c2d-30d8-9b40-1cdb202dbc39",
                    "sense_definition": "Niet gesloten; zo dat er doorgang mogelijk is of de inhoud bereikbaar is.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De deur van de winkel is al <w>open</w>.",

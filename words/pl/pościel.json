            "senses": [
                {
                    "sense_id": "848c1b71-fd11-3f41-b990-d25f781b83b9",
                    "sense_definition": "Forma rozkazująca 2. osoby liczby pojedynczej czasownika <w>pościelić</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "null",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jacku, <pościel> swoje łóżko, zanim wyjdziesz z domu.",
                            "target_lang_translations": {
                                "ru": "Яцек, <постели> свою кровать, прежде чем выйдешь из дома."
                            }
                        },
                        {
                            "text": "Proszę, <pościel> starannie, żeby w pokoju był porządek.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <застели> аккуратно, чтобы в комнате был порядок."
                            }

                {
                    "sense_id": "c0cbbb02-4c6a-30ac-8307-988d006f8f32",
                    "sense_definition": "Znajdujący się osobiście w danym miejscu i czasie; biorący w czymś udział.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel sprawdził listę, aby zobaczyć, kto jest dziś <w>obecny</w>.",
                            "target_lang_translations": {
                                "ru": "Учитель проверил список, чтобы посмотреть, кто сегодня <w>присутствует</w>."
                            }
                        },
                        {
                            "text": "Wszyscy <w>obecni</w> na sali wstali, gdy zaczęto grać hymn narodowy.",
                            "target_lang_translations": {
                                "ru": "Все <w>присутствующие</w> в зале встали, когда заиграл национальный гимн."
                            }
                        }
                    ],
                    "antonyms": [
                        "nieobecny"
                    ],
                    "common_phrases": [
                        "być obecnym",
                        "lista obecnych",
                        "wszyscy obecni",
                        "obecny na zebraniu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся лично в данном месте и в данное время; принимающий в чем-либо участие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "присутствующий",
                                "target_lang_sense_clarification": "находящийся в определенном месте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3048e4b3-c6c6-33a7-838a-093c26138398",
                    "sense_definition": "Dziejący się teraz, dotyczący chwili bieżącej; taki, który istnieje w tym momencie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "current_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W <w>obecnej</w> sytuacji nie możemy podjąć żadnej innej decyzji.",
                            "target_lang_translations": {
                                "ru": "В <w>нынешней</w> ситуации мы не можем принять никакого другого решения."
                            }
                        },
                        {
                            "text": "Mój <w>obecny</w> adres zamieszkania różni się od tego w dowodzie.",
                            "target_lang_translations": {
                                "ru": "Мой <w>текущий</w> адрес проживания отличается от указанного в удостоверении личности."
                            }
                        }
                    ],
                    "synonyms": [
                        "aktualny",
                        "bieżący",
                        "teraźniejszy",
                        "dzisiejszy",
                        "współczesny"
                    ],
                    "antonyms": [
                        "nieaktualny",
                        "dawny"
                    ],
                    "common_phrases": [
                        "w obecnej chwili",
                        "obecny stan",
                        "obecny rok",
                        "obecna sytuacja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий сейчас, относящийся к текущему моменту; существующий в данный момент."

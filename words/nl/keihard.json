            "senses": [
                {
                    "sense_id": "32d9dcc0-698e-3d40-8a46-8eeb34818555",
                    "sense_definition": "Met een zeer hoog volume; erg luid.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de <w>keiharde</w> muziek konden we elkaar niet verstaan.",
                            "target_lang_translations": {
                                "ru": "Из-за <w>оглушительной</w> музыки мы не слышали друг друга."
                            }
                        },
                        {
                            "text": "Er klonk een <w>keiharde</w> knal uit de keuken.",
                            "target_lang_translations": {
                                "ru": "Из кухни раздался <w>оглушительный</w> хлопок."
                            }
                        }
                    ],
                    "synonyms": [
                        "oorverdovend",
                        "luid"
                    ],
                    "antonyms": [
                        "zacht",
                        "stil"
                    ],
                    "common_phrases": [
                        "keiharde muziek",
                        "keiharde knal",
                        "keihard geluid"
                    ],
                    "target_lang_definitions": {
                        "ru": "С очень высокой громкостью; очень громкий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оглушительный",
                                "target_lang_sense_clarification": "настолько громкий, что закладывает уши"
                            },
                            {
                                "target_lang_word": "громкий",
                                "target_lang_sense_clarification": "общий термин для высокого уровня звука"
                            }
                        ]

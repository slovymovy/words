    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dec8afb3-fbc3-3856-9c73-58c6ceb35ec0",
                    "sense_definition": "Поместить, вложить что-либо внутрь чего-либо, часто с усилием, поспешно или небрежно.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "insertion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он небрежно <w>всунул</w> руки в карманы брюк и пошёл дальше.",
                            "target_lang_translations": {
                                "en": "He carelessly <w>shoved</w> his hands into his trouser pockets and walked on."
                            }
                        },
                        {
                            "text": "Я попытался <w>всунуть</w> толстую книгу на переполненную полку.",
                            "target_lang_translations": {
                                "en": "I tried to <w>squeeze</w> the thick book onto the overcrowded shelf."
                            }
                        }
                    ],
                    "synonyms": [
                        "засунуть",
                        "вложить",
                        "поместить"
                    ],
                    "antonyms": [
                        "вынуть",
                        "вытащить"
                    ],
                    "common_phrases": [
                        "всунуть руку в карман",
                        "всунуть ключ в замок",
                        "всунуть ногу в ботинок"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "implies a casual or forceful action compared to 'вложить'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To put something inside something else, often hastily, carelessly, or with effort."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shove",
                                "target_lang_sense_clarification": "to put something somewhere roughly or carelessly"
                            },
                            {
                                "target_lang_word": "stick",
                                "target_lang_sense_clarification": "to put something somewhere casually"
                            },
                            {
                                "target_lang_word": "squeeze",
                                "target_lang_sense_clarification": "to force something into a tight space"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92486099-e7c2-312d-a52e-80fc7f9f3777",
                    "sense_definition": "Быстро или незаметно дать, вручить что-либо кому-либо (обычно прямо в руки или в карман).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "giving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Прохожий молча <w>всунул</w> мне в руку рекламную листовку.",
                            "target_lang_translations": {
                                "en": "A passerby silently <w>thrust</w> a flyer into my hand."
                            }
                        },
                        {
                            "text": "Бабушка успела <w>всунуть</w> внуку немного денег перед отъездом.",
                            "target_lang_translations": {
                                "en": "Grandma managed to <w>slip</w> her grandson some money before leaving."
                            }
                        }
                    ],
                    "synonyms": [
                        "сунуть",
                        "вручить"
                    ],
                    "antonyms": [
                        "забрать",
                        "отнять"
                    ],
                    "common_phrases": [
                        "всунуть записку",
                        "всунуть взятку",
                        "всунуть деньги"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To give or hand something to someone quickly, abruptly, or surreptitiously."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slip",
                                "target_lang_sense_clarification": "to give something quickly or secretly"
                            },
                            {
                                "target_lang_word": "thrust",
                                "target_lang_sense_clarification": "to put something into someone's hand abruptly"
                            },
                            {
                                "target_lang_word": "shove",
                                "target_lang_sense_clarification": "to give something roughly or unceremoniously"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
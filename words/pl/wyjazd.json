                {
                    "sense_id": "58879a20-f227-3b8f-ba0a-c0747cbe8093",
                    "sense_definition": "Podróż w określonym celu, wycieczka lub wyprawa w inne miejsce.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W maju planuję <w>wyjazd</w> do Włoch, żeby zobaczyć Rzym.",
                            "target_lang_translations": {
                                "ru": "В мае я планирую <w>поездку</w> в Италию, чтобы увидеть Рим."
                            }
                        },
                        {
                            "text": "Nasz wspólny <w>wyjazd</w> na wakacje był wspaniałą przygodą.",
                            "target_lang_translations": {
                                "ru": "Наша совместная <w>поездка</w> в отпуск была чудесным приключением."
                            }
                        }
                    ],
                    "synonyms": [
                        "podróż",
                        "wycieczka",
                        "wyprawa"
                    ],
                    "antonyms": [
                        "powrót"
                    ],
                    "common_phrases": [
                        "wyjazd służbowy",
                        "wyjazd na wakacje",
                        "wyjazd za granicę",
                        "wyjazd integracyjny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Путешествие с определенной целью, поездка или экскурсия в другое место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поездка",
                                "target_lang_sense_clarification": "путешествие, перемещение куда-либо"
                            },
                            {
                                "target_lang_word": "выезд",
                                "target_lang_sense_clarification": "организованное посещение какого-либо места"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "834a3fb2-a920-32f4-a78f-c6f54532e93b",
                    "sense_definition": "Czynność opuszczania jakiegoś miejsca, zazwyczaj za pomocą pojazdu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szybki <w>wyjazd</w> z miasta pozwolił nam uniknąć korków.",
                            "target_lang_translations": {
                                "ru": "Быстрый <w>выезд</w> из города позволил нам избежать пробок."
                            }
                        },
                        {
                            "text": "Częste <w>wyjazdy</w> rodziców były trudne dla dzieci.",
                            "target_lang_translations": {
                                "ru": "Частые <w>отъезды</w> родителей были тяжелы для детей."
                            }
                        }
                    ],
                    "synonyms": [
                        "odjazd"
                    ],
                    "antonyms": [
                        "przyjazd",
                        "wjazd",
                        "powrót"
                    ],
                    "common_phrases": [
                        "godzina wyjazdu",
                        "dzień wyjazdu",
                        "szybki wyjazd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие покидания какого-либо места, обычно на транспортном средстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отъезд",
                                "target_lang_sense_clarification": "отправление в путь, начало путешествия"
                            },
                            {
                                "target_lang_word": "выезд",
                                "target_lang_sense_clarification": "процесс покидания места"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3a4eda00-ca7b-3084-bb17-247379e22315",
                    "sense_definition": "Miejsce, przez które można wyjechać pojazdem z terenu lub budynku.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jakiś nieuważny kierowca zablokował <w>wyjazd</w> z parkingu.",
                            "target_lang_translations": {
                                "ru": "Какой-то невнимательный водитель заблокировал <w>выезд</w> с парковки."
                            }
                        },
                        {
                            "text": "Główny <w>wyjazd</w> z osiedla jest w remoncie.",
                            "target_lang_translations": {
                                "ru": "Главный <w>выезд</w> из жилого комплекса находится на ремонте."
                            }
                        }
                    ],
                    "antonyms": [
                        "wjazd"
                    ],
                    "common_phrases": [
                        "wyjazd z garażu",
                        "wyjazd z parkingu",
                        "wyjazd z autostrady",
                        "wyjazd awaryjny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место, через которое можно выехать на транспортном средстве с территории или из здания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выезд",
                                "target_lang_sense_clarification": "место для проезда транспорта наружу"
                            }
                        ]
                    }
                }
            ]
        }

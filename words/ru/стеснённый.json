    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3b0b0b60-41b3-3be5-863b-279fb4a4a3ce",
                    "sense_definition": "Испытывающий недостаток в средствах или удобствах; трудный, тяжёлый (о жизненных или финансовых условиях).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "financial_difficulty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Семья жила в крайне <w>стеснённых</w> обстоятельствах и экономила на всём.",
                            "target_lang_translations": {
                                "en": "The family lived in extremely <w>straitened</w> circumstances and saved on everything."
                            }
                        },
                        {
                            "text": "Из-за потери работы он оказался в <w>стеснённом</w> материальном положении.",
                            "target_lang_translations": {
                                "en": "Due to losing his job, he found himself in a <w>tight</w> financial situation."
                            }
                        }
                    ],
                    "common_phrases": [
                        "стеснённые обстоятельства",
                        "стеснённое положение",
                        "стеснённые средства"
                    ],
                    "target_lang_definitions": {
                        "en": "Experiencing a lack of funds or comforts; difficult (regarding living or financial conditions)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "straitened",
                                "target_lang_sense_clarification": "specifically regarding circumstances or means"
                            },
                            {
                                "target_lang_word": "tight",
                                "target_lang_sense_clarification": "informal, regarding money or budget"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b9d7d2d-e008-34b2-809c-aaacba4e5f90",
                    "sense_definition": "Чувствующий неловкость, смущение или отсутствие свободы в поведении.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotional_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я чувствовал себя <w>стеснённым</w> в присутствии строгого начальника.",
                            "target_lang_translations": {
                                "en": "I felt <w>ill at ease</w> in the presence of the strict boss."
                            }
                        },
                        {
                            "text": "Её движения были <w>стеснёнными</w>, и она боялась поднять глаза.",
                            "target_lang_translations": {
                                "en": "Her movements were <w>constrained</w>, and she was afraid to look up."
                            }
                        }
                    ],
                    "synonyms": [
                        "смущённый"
                    ],
                    "common_phrases": [
                        "чувствовать себя стеснённым",
                        "стеснённый вид",
                        "стеснённая улыбка"
                    ],
                    "target_lang_definitions": {
                        "en": "Feeling awkwardness, embarrassment, or lack of freedom in behavior."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "constrained",
                                "target_lang_sense_clarification": "forced, unnatural, or restricted in manner"
                            },
                            {
                                "target_lang_word": "ill at ease",
                                "target_lang_sense_clarification": "feeling uncomfortable or awkward"
                            },
                            {
                                "target_lang_word": "inhibited",
                                "target_lang_sense_clarification": "psychologically unable to act freely"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f37d856-2233-3b31-9843-2408402f7377",
                    "sense_definition": "Ограниченный в пространстве, очень тесный, плотно заполненный.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы работали в <w>стеснённых</w> условиях маленького офиса.",
                            "target_lang_translations": {
                                "en": "We worked in the <w>cramped</w> conditions of a small office."
                            }
                        },
                        {
                            "text": "В этом <w>стеснённом</w> пространстве было трудно развернуться.",
                            "target_lang_translations": {
                                "en": "It was hard to turn around in this <w>confined</w> space."
                            }
                        }
                    ],
                    "synonyms": [
                        "стиснутый"
                    ],
                    "common_phrases": [
                        "стеснённые условия",
                        "стеснённое пространство"
                    ],
                    "target_lang_definitions": {
                        "en": "Limited in space, very tight, densely filled."
                    },
                    "translations": {

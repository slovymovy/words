    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8cf133ae-ac31-3c49-ba24-2c34586f4392",
                    "sense_definition": "Een opening of ruimte die uitzicht biedt op iets wat erachter ligt, vaak in een tuin, landschap of architectuur.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "literal_view",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De architect creëerde een mooie <w>doorkijk</w> naar de achterliggende tuin.",
                            "target_lang_translations": {
                                "en": "The architect created a beautiful <w>vista</w> to the garden behind.",
                                "ru": "Архитектор создал красивый <w>вид</w> на сад позади дома."
                            }
                        },
                        {
                            "text": "Via de <w>doorkijk</w> in de heg konden we de rivier zien stromen.",
                            "target_lang_translations": {
                                "en": "Through the <w>opening</w> in the hedge, we could see the river flowing.",
                                "ru": "Через <w>просвет</w> в живой изгороди мы видели, как течет река."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitzicht",
                        "blikveld"
                    ],
                    "common_phrases": [
                        "een mooie doorkijk",
                        "doorkijk naar"
                    ],
                    "target_lang_definitions": {
                        "en": "An opening or space offering a view of something lying behind, often in a garden, landscape, or architecture.",
                        "ru": "Отверстие или пространство, открывающее вид на что-то позади, часто в саду, ландшафте или архитектуре."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "vista",
                                "target_lang_sense_clarification": "architectural or garden view"
                            },
                            {
                                "target_lang_word": "view through",
                                "target_lang_sense_clarification": "literal meaning"
                            },
                            {
                                "target_lang_word": "opening",
                                "target_lang_sense_clarification": "physical gap providing a view"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "просвет",
                                "target_lang_sense_clarification": "отверстие для обзора"
                            },
                            {
                                "target_lang_word": "вид",
                                "target_lang_sense_clarification": "открывающаяся панорама"
                            },
                            {
                                "target_lang_word": "перспектива",
                                "target_lang_sense_clarification": "вид вдаль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b3d1700-d842-316d-b7de-9f475f492057",
                    "sense_definition": "Een inzicht in of vooruitblik op een toekomstige situatie of ontwikkeling.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_view",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit rapport geeft een heldere <w>doorkijk</w> naar de economische verwachtingen.",
                            "target_lang_translations": {
                                "en": "This report provides a clear <w>outlook</w> on the economic expectations.",
                                "ru": "Этот отчет дает ясный <w>прогноз</w> экономических ожиданий."
                            }
                        },
                        {
                            "text": "De directeur gaf tijdens de vergadering een strategische <w>doorkijk</w> naar volgend jaar.",
                            "target_lang_translations": {
                                "en": "During the meeting, the director gave a strategic <w>projection</w> for next year.",
                                "ru": "Директор представил на собрании стратегический <w>обзор</w> на следующий год."
                            }
                        }
                    ],
                    "synonyms": [
                        "vooruitblik",
                        "perspectief",
                        "inzicht"
                    ],
                    "common_phrases": [
                        "een doorkijk geven",
                        "doorkijk naar de toekomst"
                    ],
                    "target_lang_definitions": {
                        "en": "An insight into or look ahead at a future situation or development.",
                        "ru": "Понимание или прогноз будущей ситуации или развития событий."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "outlook",
                                "target_lang_sense_clarification": "future perspective"
                            },
                            {
                                "target_lang_word": "projection",
                                "target_lang_sense_clarification": "forecast or estimate"
                            },
                            {
                                "target_lang_word": "preview",
                                "target_lang_sense_clarification": "look ahead"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "прогноз",
                                "target_lang_sense_clarification": "предсказание будущего"
                            },
                            {
                                "target_lang_word": "обзор",
                                "target_lang_sense_clarification": "анализ ситуации"
                            },
                            {
                                "target_lang_word": "перспектива",
                                "target_lang_sense_clarification": "взгляд в будущее"
                            }
                        ]
                    }
                }
            ]
        },
        {

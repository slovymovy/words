                        "ru": [
                            {
                                "target_lang_word": "действовать",
                                "target_lang_sense_clarification": "проявлять активность, предпринимать меры"
                            },
                            {
                                "target_lang_word": "поступать",
                                "target_lang_sense_clarification": "совершать поступок определенным образом"
                            }
                        ]
                    }
                },
                {

                {
                    "sense_id": "72adfbac-0d95-3075-882e-b27be7aa7934",
                    "sense_definition": "Met een geringe afstand van de grond tot de top; niet hoog.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_height",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De temperatuur is vandaag erg <w>laag</w> voor de tijd van het jaar.",
                            "target_lang_translations": {
                                "ru": "Температура сегодня очень <w>низкая</w> для этого времени года."
                            }
                        },
                        {
                            "text": "In dit oude huis zijn de plafonds erg <w>laag</w>.",
                            "target_lang_translations": {
                                "ru": "В этом старом доме потолки очень <w>низкие</w>."
                            }
                        },
                        {
                            "text": "Hij draagt liever <w>lage</w> schoenen dan laarzen.",
                            "target_lang_translations": {
                                "ru": "Он предпочитает носить <w>низкие</w> ботинки, а не сапоги."
                            }

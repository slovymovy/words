    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ffe1d2d3-aeee-3514-a972-706ce163f0b1",
                    "sense_definition": "Een hard, massief stuk materiaal, vaak met hoeken, zoals hout, steen of metaal.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij gooide een zwaar <w>blok</w> hout op het vuur.",
                            "target_lang_translations": {
                                "ru": "Он бросил тяжелое <w>полено</w> в огонь."
                            }
                        },
                        {
                            "text": "Het beeld was gehouwen uit een enkel <w>blok</w> marmer.",
                            "target_lang_translations": {
                                "ru": "Статуя была высечена из цельной <w>глыбы</w> мрамора."
                            }
                        }
                    ],
                    "synonyms": [
                        "balk"
                    ],
                    "common_phrases": [
                        "een blok aan het been",
                        "voor het blok zetten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердый, массивный кусок материала, часто угловатый, например, дерева, камня или металла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глыба",
                                "target_lang_sense_clarification": "большой кусок камня или льда"
                            },
                            {
                                "target_lang_word": "полено",
                                "target_lang_sense_clarification": "кусок дерева для топки"
                            },
                            {
                                "target_lang_word": "блок",
                                "target_lang_sense_clarification": "строительный или конструктивный элемент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8562e106-8c94-3afd-98f8-36a40aa082e5",
                    "sense_definition": "Een groep huizen of gebouwen die door straten wordt omsloten.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "grouping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik loop even een rondje om het <w>blok</w> met de hond.",
                            "target_lang_translations": {
                                "ru": "Я пройдусь вокруг <w>квартала</w> с собакой."
                            }
                        },
                        {
                            "text": "Er wordt een nieuw <w>blok</w> woningen gebouwd in onze straat.",
                            "target_lang_translations": {
                                "ru": "На нашей улице строится новый <w>квартал</w> жилых домов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een blok om",
                        "huizenblok"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа домов или зданий, ограниченная улицами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "квартал",
                                "target_lang_sense_clarification": "часть города"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a495104-5359-3158-b0d4-f236273b5370",
                    "sense_definition": "Een aaneengesloten periode tijd die gereserveerd is voor een specifieke activiteit.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "grouping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De televisie zendt vanavond een <w>blok</w> met oude films uit.",
                            "target_lang_translations": {
                                "ru": "Сегодня вечером по телевизору показывают <w>блок</w> старых фильмов."
                            }
                        },
                        {
                            "text": "We hebben een <w>blok</w> van twee uur ingepland voor de vergadering.",
                            "target_lang_translations": {
                                "ru": "Мы запланировали двухчасовой <w>блок</w> для собрания."
                            }
                        }
                    ],
                    "common_phrases": [
                        "blok reclame",
                        "blok tijd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Непрерывный период времени, отведенный для определенной деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блок",
                                "target_lang_sense_clarification": "часть программы или расписания"
                            },
                            {
                                "target_lang_word": "промежуток",
                                "target_lang_sense_clarification": "отрезок времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5685ec73-c92e-34ed-a5cd-ba70a30db365",
                    "sense_definition": "Een verbond van personen, partijen of landen die samenwerken voor dezelfde belangen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "grouping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oppositiepartijen vormden één <w>blok</w> tegen het nieuwe beleid.",
                            "target_lang_translations": {
                                "ru": "Оппозиционные партии сформировали единый <w>блок</w> против новой политики."
                            }
                        },
                        {
                            "text": "De landen in het oostelijke <w>blok</w> werkten nauw samen.",
                            "target_lang_translations": {
                                "ru": "Страны восточного <w>блока</w> тесно сотрудничали."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een blok vormen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз людей, партий или стран, сотрудничающих ради общих интересов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блок",
                                "target_lang_sense_clarification": "политический союз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f436780a-c7a4-3c95-a6db-05c9cc9f84a9",
                    "sense_definition": "De periode waarin studenten intensief studeren ter voorbereiding op hun examens.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "study",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de <w>blok</w> is de bibliotheek elke dag vol.",
                            "target_lang_translations": {
                                "ru": "Во время <w>сессии</w> библиотека полна каждый день."
                            }
                        },
                        {
                            "text": "Hij heeft geen tijd om af te spreken omdat hij in de <w>blok</w> zit.",
                            "target_lang_translations": {
                                "ru": "У него нет времени встретиться, потому что у него <w>сессия</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in de blok"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Belgium"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Период, когда студенты интенсивно учатся, готовясь к экзаменам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сессия",
                                "target_lang_sense_clarification": "период экзаменов и подготовки"
                            },
                            {
                                "target_lang_word": "подготовка к экзаменам",
                                "target_lang_sense_clarification": "процесс учебы"
                            }
                        ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5f077efa-2a18-3c39-a618-b200b1647bc8",
                    "sense_definition": "Clipping of <w>Democrat</w>.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "political_clipping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>Dems</w> are hoping to win the upcoming election.",
                            "target_lang_translations": {
                                "ru": "<w>Демократы</w> надеются победить на предстоящих выборах."
                            }
                        },
                        {
                            "text": "He has been a loyal <w>Dem</w> for over thirty years.",
                            "target_lang_translations": {
                                "ru": "Он был преданным <w>демократом</w> более тридцати лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "Democrat"
                    ],
                    "common_phrases": [
                        "moderate Dems",
                        "senior Dems",
                        "registered Dem"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "political context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное сокращение от слова \"демократ\" (член Демократической партии США)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "демократ",
                                "target_lang_sense_clarification": "член партии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9b83605-557f-38f0-81bd-996af9cd7b96",
                    "sense_definition": "Initialism of digital elevation model.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "acronym_dem",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scientists used high-resolution <w>DEMs</w> to map the flood risk areas.",
                            "target_lang_translations": {
                                "ru": "Ученые использовали <w>ЦМР</w> высокого разрешения для картирования зон риска наводнений."
                            }
                        },
                        {
                            "text": "We need to update the <w>DEMs</w> before starting the construction project.",
                            "target_lang_translations": {
                                "ru": "Нам нужно обновить <w>ЦМР</w> перед началом строительного проекта."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура от \"цифровая модель рельефа\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ЦМР",
                                "target_lang_sense_clarification": "картография"
                            },
                            {
                                "target_lang_word": "цифровая модель рельефа",
                                "target_lang_sense_clarification": "полная форма"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "c87d760f-5e60-345e-a5f3-38756094239a",
                    "sense_definition": "Nonstandard form of <w>them</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dialectal_pronoun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I told <w>dem</w> that we would be arriving late.",
                            "target_lang_translations": {
                                "ru": "Я сказал <w>им</w>, что мы приедем поздно."
                            }
                        },
                        {
                            "text": "Go tell <w>dem</w> to stop making so much noise.",
                            "target_lang_translations": {
                                "ru": "Пойди скажи <w>им</w>, чтобы перестали так шуметь."
                            }
                        }
                    ],
                    "synonyms": [
                        "them"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "AAVE / Caribbean"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорная или диалектная форма местоимения \"them\" (они/их/им)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "им",
                                "target_lang_sense_clarification": "дательный падеж"
                            },
                            {
                                "target_lang_word": "их",
                                "target_lang_sense_clarification": "родительный/винительный падеж"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "5456f1fc-295d-3d4f-a0ac-05bea6268c34",
                    "sense_definition": "Nonstandard form of <w>them</w>, in the sense of \"those\".",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dialectal_determiner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "What are <w>dem</w> things doing over there?",
                            "target_lang_translations": {
                                "ru": "Что <w>те</w> штуки там делают?"
                            }
                        },
                        {
                            "text": "Look at <w>dem</w> shoes he is wearing.",
                            "target_lang_translations": {
                                "ru": "Посмотри на <w>те</w> туфли, что на нем."
                            }
                        }
                    ],
                    "synonyms": [
                        "those"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "AAVE / Caribbean"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорная форма \"them\" в значении указательного местоимения \"those\" (те)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "те",
                                "target_lang_sense_clarification": "указательное местоимение"
                            },
                            {
                                "target_lang_word": "эти",
                                "target_lang_sense_clarification": "близкое расположение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3cdd4c78-1d17-399c-b73b-661aa44cac0f",
                    "sense_definition": "A particle used after a noun to indicate a group or plural number.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "dialectal_plural_marker",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am going out to meet the man <w>dem</w> later tonight.",
                            "target_lang_translations": {
                                "ru": "Я собираюсь встретиться с <w>ребятами</w> позже вечером."
                            }
                        },
                        {
                            "text": "Love and tenderness is what the woman <w>dem</w> want.",
                            "target_lang_translations": {
                                "ru": "Любовь и нежность — это то, чего хотят <w>женщины</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "man dem",
                        "woman dem"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Caribbean / MLE"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Частица, используемая после существительного для указания на группу людей или множественное число (в карибских диалектах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ребята",
                                "target_lang_sense_clarification": "в значении 'компания' (man dem)"
                            },
                            {
                                "target_lang_word": "группа",
                                "target_lang_sense_clarification": "коллектив"
                            }
                        ]
                    }
                }
            ]
        },

{
    "word_family": [
        "wpłynąć",
        "wpływ",
        "wpływowy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a31ad54a-9a61-3262-abf3-827ba90cf30a",
                    "sense_definition": "O rzekach i strumieniach: kończyć swój bieg, wpadając do innego zbiornika wodnego, takiego jak morze, jezioro lub inna rzeka.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "water_flow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta duża rzeka <w>wpływa</w> bezpośrednio do Morza Bałtyckiego.",
                            "target_lang_translations": {
                                "ru": "Эта большая река <w>впадает</w> непосредственно в Балтийское море."
                            }
                        },
                        {
                            "text": "Widzieliśmy miejsce, gdzie górski potok <w>wpływa</w> do jeziora.",
                            "target_lang_translations": {
                                "ru": "Мы видели место, где горный поток <w>впадает</w> в озеро."
                            }
                        }
                    ],
                    "synonyms": [
                        "uchodzić"
                    ],
                    "antonyms": [
                        "wypływać"
                    ],
                    "common_phrases": [
                        "wpływać do morza",
                        "wpływać do jeziora",
                        "rzeka wpływa"
                    ],
                    "target_lang_definitions": {
                        "ru": "О реках и ручьях: заканчивать свое течение, впадая в другой водоем (море, озеро или другую реку)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "впадать",
                                "target_lang_sense_clarification": "о реках и ручьях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5fe6fdb0-c2c7-3693-bdcc-6e1e5fb13119",
                    "sense_definition": "Wywierać wpływ na kogoś lub na coś; sprawiać, że ktoś zmienia zachowanie lub że zachodzą pewne zjawiska.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stres w pracy negatywnie <w>wpływa</w> na jego zdrowie i samopoczucie.",
                            "target_lang_translations": {
                                "ru": "Стресс на работе негативно <w>влияет</w> на его здоровье и самочувствие."
                            }
                        },
                        {
                            "text": "Twoja opinia bardzo <w>wpływa</w> na moje ostateczne decyzje.",
                            "target_lang_translations": {
                                "ru": "Твое мнение сильно <w>влияет</w> на мои окончательные решения."
                            }
                        }
                    ],
                    "synonyms": [
                        "oddziaływać"
                    ],
                    "common_phrases": [
                        "wpływać na decyzję",
                        "korzystnie wpływać",
                        "negatywnie wpływać",
                        "wpływać na rozwój"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оказывать влияние на кого-то или что-то; заставлять кого-то менять поведение или вызывать определенные явления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "влиять",
                                "target_lang_sense_clarification": "оказывать воздействие"
                            },
                            {
                                "target_lang_word": "воздействовать",
                                "target_lang_sense_clarification": "производить действие на кого-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d5f23b7-75ef-39d7-887f-d04b602ea55f",
                    "sense_definition": "Dostawać się do wnętrza czegoś lub na dany obszar, płynąc w wodzie lub unosząc się w powietrzu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_entry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Małe ryby często <w>wpływają</w> między skały, aby ukryć się przed drapieżnikami.",
                            "target_lang_translations": {
                                "ru": "Маленькие рыбы часто <w>заплывают</w> между камнями, чтобы спрятаться от хищников."
                            }
                        },
                        {
                            "text": "Zimne powietrze <w>wpływa</w> do pokoju przez otwarte okno.",
                            "target_lang_translations": {
                                "ru": "Холодный воздух <w>проникает</w> в комнату через открытое окно."
                            }
                        }
                    ],
                    "antonyms": [
                        "wypływać"
                    ],
                    "common_phrases": [
                        "wpływać do środka",
                        "wpływać na głęboką wodę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Попадать внутрь чего-либо или на определенную территорию, плывя в воде или перемещаясь по воздуху."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заплывать",
                                "target_lang_sense_clarification": "попадать куда-либо вплавь"
                            },
                            {
                                "target_lang_word": "проникать",
                                "target_lang_sense_clarification": "попадать внутрь (о воздухе, воде)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3b899a3-6546-3508-86ab-beef0dfa5eab",
                    "sense_definition": "Wchodzić statkiem, łodzią lub innym obiektem pływającym do portu lub przystani.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_entry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielki statek pasażerski powoli <w>wpływa</w> do portu w Gdyni.",
                            "target_lang_translations": {
                                "ru": "Большой пассажирский лайнер медленно <w>входит</w> в порт Гдыни."
                            }
                        },
                        {
                            "text": "Jachty zazwyczaj <w>wpływają</w> do mariny przed zachodem słońca.",
                            "target_lang_translations": {
                                "ru": "Яхты обычно <w>заходят</w> в марину перед закатом."
                            }
                        }
                    ],
                    "synonyms": [
                        "zawijać"
                    ],
                    "antonyms": [
                        "wypływać"
                    ],
                    "common_phrases": [
                        "wpływać do portu",
                        "wpływać do przystani"
                    ],
                    "target_lang_definitions": {
                        "ru": "Входить в порт или гавань на судне, лодке или другом плавучем средстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "входить",
                                "target_lang_sense_clarification": "о судах (в порт)"
                            },
                            {
                                "target_lang_word": "заходить",
                                "target_lang_sense_clarification": "прибывать (о морском транспорте)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "769d104c-4c00-308c-85a4-0b5d0ad10c24",
                    "sense_definition": "O pieniądzach, dokumentach lub pismach: docierać do adresata, urzędu lub na konto bankowe.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_arrival",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wypłata zawsze <w>wpływa</w> na moje konto dziesiątego dnia miesiąca.",
                            "target_lang_translations": {
                                "ru": "Зарплата всегда <w>поступает</w> на мой счет десятого числа месяца."
                            }
                        },
                        {
                            "text": "Do biura codziennie <w>wpływają</w> nowe podania o pracę od kandydatów.",
                            "target_lang_translations": {
                                "ru": "В офис ежедневно <w>поступают</w> новые заявления о приеме на работу от кандидатов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wpływać na konto",
                        "wpływać do urzędu",
                        "wniosek wpływa"
                    ],
                    "target_lang_definitions": {
                        "ru": "О деньгах, документах или письмах: приходить к адресату, в учреждение или на банковский счет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поступать",
                                "target_lang_sense_clarification": "приходить (о деньгах, заявках)"
                            },
                            {
                                "target_lang_word": "приходить",
                                "target_lang_sense_clarification": "доставляться адресату"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
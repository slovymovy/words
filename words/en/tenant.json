{
    "word_family": [
        "tenancy",
        "subtenant",
        "tenantable",
        "tenantry"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3a92754f-30be-348e-9655-dfd6e98c616b",
                    "sense_definition": "A person who pays rent to use land, a building, or a room that belongs to someone else.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "lease_holder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>tenant</w> signed a new lease for the apartment yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера <w>арендатор</w> подписал новый договор аренды квартиры."
                            }
                        },
                        {
                            "text": "If the <w>tenant</w> fails to pay rent, the landlord may start eviction proceedings.",
                            "target_lang_translations": {
                                "ru": "Если <w>съёмщик</w> не платит арендную плату, арендодатель может начать процедуру выселения."
                            }
                        }
                    ],
                    "synonyms": [
                        "renter",
                        "lessee",
                        "leaseholder"
                    ],
                    "antonyms": [
                        "landlord"
                    ],
                    "common_phrases": [
                        "sitting tenant",
                        "tenant farmer",
                        "model tenant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который платит арендную плату за использование земли, здания или комнаты, принадлежащих кому-то другому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "арендатор",
                                "target_lang_sense_clarification": "официальное или общее название"
                            },
                            {
                                "target_lang_word": "съёмщик",
                                "target_lang_sense_clarification": "разговорное, обычно о жилье"
                            },
                            {
                                "target_lang_word": "квартиросъёмщик",
                                "target_lang_sense_clarification": "официальное, только для квартир"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5d41267-9e51-3a4f-89ca-986660d43e24",
                    "sense_definition": "A person, animal, or group that lives in or occupies a specific place.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "occupant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Owls are the noisy <w>tenants</w> of the old barn loft.",
                            "target_lang_translations": {
                                "ru": "Совы — шумные <w>обитатели</w> чердака старого сарая."
                            }
                        },
                        {
                            "text": "The previous <w>tenant</w> of this office left a lot of paperwork behind.",
                            "target_lang_translations": {
                                "ru": "Предыдущий <w>арендатор</w> этого офиса оставил после себя много бумаг."
                            }
                        }
                    ],
                    "synonyms": [
                        "occupant",
                        "dweller"
                    ],
                    "common_phrases": [
                        "forest tenants"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, животное или группа, живущие в определенном месте или занимающие его."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обитатель",
                                "target_lang_sense_clarification": "о людях или животных"
                            },
                            {
                                "target_lang_word": "жилец",
                                "target_lang_sense_clarification": "обычно о людях в доме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d123625-fd3a-3a0f-805d-14e2a9d1e28e",
                    "sense_definition": "A specific customer or group of users serviced by a single instance of a software application.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cloud platform isolates data so that one <w>tenant</w> cannot access another's information.",
                            "target_lang_translations": {
                                "ru": "Облачная платформа изолирует данные так, что один <w>арендатор</w> не может получить доступ к информации другого."
                            }
                        },
                        {
                            "text": "In a multi-<w>tenant</w> architecture, resources are shared efficiently among clients.",
                            "target_lang_translations": {
                                "ru": "В архитектуре с несколькими <w>арендаторами</w> ресурсы эффективно распределяются между клиентами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "multi-tenant",
                        "single-tenant"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный клиент или группа пользователей, обслуживаемых одним экземпляром программного приложения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "арендатор",
                                "target_lang_sense_clarification": "в облачных вычислениях"
                            },
                            {
                                "target_lang_word": "клиент",
                                "target_lang_sense_clarification": "пользователь сервиса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa9ccb62-9034-3a89-a3f4-ee46ba7f1283",
                    "sense_definition": "A person who holds land or property under feudal law or specific legal title.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "lease_holder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Under the feudal system, the <w>tenant</w> owed military service to the lord.",
                            "target_lang_translations": {
                                "ru": "При феодальном строе <w>держатель</w> земли был обязан нести военную службу сюзерену."
                            }
                        },
                        {
                            "text": "The legal rights of a <w>tenant</w> in fee simple are extensive.",
                            "target_lang_translations": {
                                "ru": "Законные права <w>владельца</w> безусловного права собственности весьма обширны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tenant in chief"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical or legal context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, владеющее землей или собственностью по феодальному праву или определенному правовому титулу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "землевладелец",
                                "target_lang_sense_clarification": "владелец земли"
                            },
                            {
                                "target_lang_word": "держатель",
                                "target_lang_sense_clarification": "феодальный термин"
                            },
                            {
                                "target_lang_word": "владелец",
                                "target_lang_sense_clarification": "юридический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8ea37f56-5679-32bf-b793-6570607f7c2f",
                    "sense_definition": "To occupy or live in a place as a tenant.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old cottage is currently <w>tenanted</w> by a young couple from the city.",
                            "target_lang_translations": {
                                "ru": "Этот старый коттедж сейчас <w>арендуется</w> молодой парой из города."
                            }
                        },
                        {
                            "text": "Many species of birds <w>tenant</w> these woods during the spring.",
                            "target_lang_translations": {
                                "ru": "Многие виды птиц <w>обитают</w> в этих лесах весной."
                            }
                        }
                    ],
                    "synonyms": [
                        "occupant",
                        "dweller",
                        "lodge"
                    ],
                    "common_phrases": [
                        "tenanted property"
                    ],
                    "target_lang_definitions": {
                        "ru": "Занимать место или жить в нем на правах арендатора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "арендовать",
                                "target_lang_sense_clarification": "брать в аренду"
                            },
                            {
                                "target_lang_word": "снимать",
                                "target_lang_sense_clarification": "разговорное, о жилье"
                            },
                            {
                                "target_lang_word": "обитать",
                                "target_lang_sense_clarification": "населять (о животных)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "a9916abc-efb1-3785-a171-92b9888e9b48",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Tenant</w> arrived late for the meeting.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Тенант</w> опоздал на встречу."
                            }
                        },
                        {
                            "text": "We looked up the name <w>Tenant</w> in the local directory.",
                            "target_lang_translations": {
                                "ru": "Мы искали фамилию <w>Тенант</w> в местном справочнике."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Тенант",
                                "target_lang_sense_clarification": "транслитерация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
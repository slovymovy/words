    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9d93c4ab-7318-385b-ad18-81d4a7c01a0d",
                    "sense_definition": "Раскладывать вещи по местам, наводить порядок.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "organization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После долгой поездки мне нужно <разбирать> чемодан.",
                            "target_lang_translations": {
                                "en": "After a long trip, I need to <w>unpack</w> my suitcase."
                            }
                        },
                        {
                            "text": "Мама попросила детей <разбирать> игрушки в комнате.",
                            "target_lang_translations": {
                                "en": "Mom asked the children to <w>sort out</w> the toys in the room."
                            }
                        }
                    ],
                    "synonyms": [
                        "упорядочивать",
                        "приводить в порядок"
                    ],
                    "common_phrases": [
                        "разбирать вещи",
                        "разбирать чемодан",
                        "разбирать бумаги",
                        "разбирать шкаф"
                    ],
                    "target_lang_definitions": {
                        "en": "To put things in their places, to tidy up or organize."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unpack",
                                "target_lang_sense_clarification": "a suitcase, bags, or boxes"
                            },
                            {
                                "target_lang_word": "sort out",
                                "target_lang_sense_clarification": "things, papers, or clutter"
                            },
                            {
                                "target_lang_word": "tidy up",
                                "target_lang_sense_clarification": "a room or mess"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c08a1b9c-18fd-36b9-a418-741ab5d61557",
                    "sense_definition": "Разделять сложный объект на составные части.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Механик начал <разбирать> двигатель машины для ремонта.",
                            "target_lang_translations": {
                                "en": "The mechanic started to <w>disassemble</w> the car engine for repairs."
                            }
                        },
                        {
                            "text": "Маленький ребёнок любит ломать и <разбирать> свои игрушки.",
                            "target_lang_translations": {
                                "en": "The small child loves to break and <w>take apart</w> his toys."

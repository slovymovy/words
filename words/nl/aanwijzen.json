            "senses": [
                {
                    "sense_id": "4263e7c4-47d9-3f2d-bc83-3b6875961360",
                    "sense_definition": "Met de hand of vinger laten zien waar iemand of iets is.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun je op de kaart <w>aanwijzen</w> waar we nu zijn?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>показать</w> на карте, где мы сейчас находимся?",
                                "en": "Can you <w>point out</w> on the map where we are now?"
                            }
                        },
                        {
                            "text": "De leraar <w>wees</w> het juiste antwoord op het bord <w>aan</w>.",
                            "target_lang_translations": {
                                "ru": "Учитель <w>указал</w> на правильный ответ на доске.",
                                "en": "The teacher <w>pointed out</w> the correct answer on the board."
                            }
                        }
                    ],
                    "synonyms": [
                        "aantonen",
                        "aantreffen",
                        "vaststellen"
                    ],
                    "common_phrases": [
                        "de weg aanwijzen",
                        "op de kaart aanwijzen",
                        "met de vinger aanwijzen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Показывать рукой или пальцем местоположение кого-либо или чего-либо.",
                        "en": "To show where someone or something is using your hand or finger."

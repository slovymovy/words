            "senses": [
                {
                    "sense_id": "7708f8f7-d258-359b-b047-15ee22d93451",
                    "sense_definition": "De toestand waarin men steun of hulp nodig heeft en niet zonder iemand of iets kan functioneren.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude vrouw vindt haar <w>afhankelijkheid</w> van haar kinderen storend.",
                            "target_lang_translations": {
                                "ru": "Старую женщину тяготит её <w>зависимость</w> от детей."
                            }
                        },
                        {
                            "text": "Hij vindt zijn <w>afhankelijkheid</w> van alcohol en tabak niet prettig.",
                            "target_lang_translations": {
                                "ru": "Ему неприятна его <w>зависимость</w> от алкоголя и табака."
                            }
                        }
                    ],
                    "synonyms": [
                        "onzelfstandigheid",
                        "gebondenheid"
                    ],
                    "antonyms": [
                        "onafhankelijkheid"
                    ],
                    "common_phrases": [
                        "financiële afhankelijkheid",
                        "wederzijdse afhankelijkheid",
                        "grote afhankelijkheid"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние, при котором человек нуждается в поддержке или помощи и не может функционировать без кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зависимость",
                                "target_lang_sense_clarification": "потребность в ком-либо или чём-либо"
                            },
                            {
                                "target_lang_word": "несамостоятельность",
                                "target_lang_sense_clarification": "неспособность действовать без помощи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae55bc6a-d1c7-3146-a401-3ebd90d79284",
                    "sense_definition": "Het feit dat iets wordt bepaald of beïnvloed door omstandigheden of andere factoren.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de <w>afhankelijkheid</w> van het weer kun je schilderwerk niet altijd vooraf plannen.",
                            "target_lang_translations": {
                                "ru": "Из-за <w>зависимости</w> от погоды малярные работы не всегда можно спланировать заранее."
                            }
                        },
                        {
                            "text": "De economische <w>afhankelijkheid</w> van de export maakt het land kwetsbaar voor crisissen.",
                            "target_lang_translations": {
                                "ru": "Экономическая <w>зависимость</w> от экспорта делает страну уязвимой перед кризисами."
                            }
                        }
                    ],
                    "antonyms": [
                        "onafhankelijkheid"
                    ],
                    "common_phrases": [
                        "afhankelijkheid van grondstoffen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Факт того, что что-то определяется или находится под влиянием обстоятельств или других факторов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зависимость",
                                "target_lang_sense_clarification": "обусловленность внешними факторами"
                            }
                        ]
                    }
                }
            ]
        }

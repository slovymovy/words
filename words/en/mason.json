    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "99831c3e-7570-3398-b4aa-ed310d570537",
                    "sense_definition": "A person whose occupation is to build with stone, brick, or concrete.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "worker",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The skilled <w>mason</w> repaired the crumbling wall in just a few hours.",
                            "target_lang_translations": {
                                "ru": "Опытный <w>каменщик</w> починил разрушающуюся стену всего за несколько часов."
                            }
                        },
                        {
                            "text": "They hired a master <w>mason</w> to build a beautiful stone fireplace in the living room.",
                            "target_lang_translations": {
                                "ru": "Они наняли мастера-<w>каменщика</w>, чтобы построить красивый каменный камин в гостиной."
                            }
                        }
                    ],
                    "synonyms": [
                        "bricklayer",
                        "stonemason",
                        "builder"
                    ],
                    "common_phrases": [
                        "master mason",
                        "stone mason"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, чья профессия — строить из камня, кирпича или бетона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "каменщик",
                                "target_lang_sense_clarification": "строитель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09bd4d21-ad01-38f9-a7dd-e64e3207dd87",
                    "sense_definition": "A member of the international fraternity of Freemasons.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fraternity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My grandfather was a proud <w>mason</w> and attended meetings at the local lodge every week.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка был гордым <w>масоном</w> и посещал собрания в местной ложе каждую неделю."
                            }
                        },
                        {
                            "text": "Many historical figures were known to be <w>masons</w> during their lifetimes.",
                            "target_lang_translations": {
                                "ru": "Известно, что многие исторические личности были <w>масонами</w> при жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "Freemason"
                    ],
                    "common_phrases": [
                        "become a mason"
                    ],
                    "target_lang_definitions": {
                        "ru": "Член международного братства масонов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "масон",
                                "target_lang_sense_clarification": "член братства"
                            },
                            {
                                "target_lang_word": "вольный каменщик",
                                "target_lang_sense_clarification": "устаревшее/торжественное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "b5c54287-ae50-36a3-9f44-a89faeba11b7",
                    "sense_definition": "A surname originating as an occupation for a stonemason.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "name_surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Mason</w> teaches mathematics at the local high school.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Мейсон</w> преподает математику в местной средней школе."
                            }
                        },
                        {
                            "text": "I have an appointment with Dr. <w>Mason</w> at the clinic tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "У меня назначен прием у доктора <w>Мейсона</w> в клинике завтра утром."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "occupational origin"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия, происходящая от названия профессии каменщика."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мейсон",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c81c6c6-2f7e-3f1d-99a6-3036b57923f2",
                    "sense_definition": "A male given name.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "name_given",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "<w>Mason</w> is playing soccer with his friends in the park.",
                            "target_lang_translations": {
                                "ru": "<w>Мейсон</w> играет в футбол со своими друзьями в парке."
                            }
                        },
                        {
                            "text": "They decided to name their new baby boy <w>Mason</w>.",
                            "target_lang_translations": {
                                "ru": "Они решили назвать своего новорожденного сына <w>Мейсон</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мейсон",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1f53e9f-dcfb-3aa2-a839-b56db73c571f",
                    "sense_definition": "The name of various cities, towns, and communities in the United States.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "name_place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We stopped in the town of <w>Mason</w> to get some gas and food.",
                            "target_lang_translations": {
                                "ru": "Мы остановились в городе <w>Мейсон</w>, чтобы заправиться и поесть."
                            }
                        },
                        {
                            "text": "The family moved to <w>Mason</w> last year to enjoy a quieter life.",
                            "target_lang_translations": {
                                "ru": "Семья переехала в <w>Мейсон</w> в прошлом году, чтобы насладиться более спокойной жизнью."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Название различных городов и населенных пунктов в США."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мейсон",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "02174df2-c9b1-3c3a-a667-9644e30b726c",
                    "sense_definition": "To construct, strengthen, or enclose something using stone or brickwork.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The workers had to <w>mason</w> up the old tunnel entrance for safety reasons.",
                            "target_lang_translations": {
                                "ru": "Рабочим пришлось <w>замуровать</w> вход в старый туннель из соображений безопасности."
                            }
                        },
                        {
                            "text": "They plan to <w>mason</w> the sides of the well to prevent it from collapsing.",
                            "target_lang_translations": {
                                "ru": "Они планируют <w>обложить камнем</w> стенки колодца, чтобы предотвратить его обрушение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mason up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Строить, укреплять или огораживать что-либо с помощью каменной или кирпичной кладки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обкладывать камнем",
                                "target_lang_sense_clarification": "облицовывать"
                            },
                            {
                                "target_lang_word": "замуровывать",
                                "target_lang_sense_clarification": "закрывать кладкой"
                            }
                        ]
                    }
                }
            ]
        }
    ]

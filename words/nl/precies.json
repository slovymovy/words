{
    "word_family": [
        "precisie",
        "preciseren",
        "preciesheid"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "d050f61c-83b4-3876-a3ac-93516ffe1b0c",
                    "sense_definition": "Helemaal juist; zonder afwijking of verschil.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "exactness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat is <w>precies</w> wat ik bedoelde toen ik het uitlegde.",
                            "target_lang_translations": {
                                "en": "That is <w>exactly</w> what I meant when I explained it.",
                                "ru": "Это <w>именно</w> то, что я имел в виду, когда объяснял."
                            }
                        },
                        {
                            "text": "Ik weet niet <w>precies</w> hoe laat de film begint.",
                            "target_lang_translations": {
                                "en": "I don't know <w>exactly</w> what time the movie starts.",
                                "ru": "Я не знаю <w>точно</w>, во сколько начинается фильм."
                            }
                        }
                    ],
                    "common_phrases": [
                        "precies hetzelfde",
                        "niet precies weten",
                        "precies zoals"
                    ],
                    "target_lang_definitions": {
                        "en": "Completely correct; without deviation or difference.",
                        "ru": "Совершенно верно; без отклонений или различий."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exactly",
                                "target_lang_sense_clarification": "completely accurate or correct"
                            },
                            {
                                "target_lang_word": "precisely",
                                "target_lang_sense_clarification": "with exactness"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "точно",
                                "target_lang_sense_clarification": "наречие"
                            },
                            {
                                "target_lang_word": "именно",
                                "target_lang_sense_clarification": "усилительная частица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac901889-b072-3d10-bbb9-677e54ef3abd",
                    "sense_definition": "Stipt; op het exacte moment, niet vroeger of later.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De trein vertrok <w>precies</w> op tijd van het station.",
                            "target_lang_translations": {
                                "en": "The train left the station <w>exactly</w> on time.",
                                "ru": "Поезд отошел от станции <w>точно</w> по расписанию."
                            }
                        },
                        {
                            "text": "We moeten <w>precies</w> om acht uur bij de ingang zijn.",
                            "target_lang_translations": {
                                "en": "We have to be at the entrance at eight o'clock <w>sharp</w>.",
                                "ru": "Мы должны быть у входа <w>ровно</w> в восемь часов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "precies op tijd",
                        "precies om"
                    ],
                    "target_lang_definitions": {
                        "en": "Punctual; at the exact moment, not earlier or later.",
                        "ru": "Пунктуально; в точный момент, не раньше и не позже."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exactly",
                                "target_lang_sense_clarification": "at a specific time"
                            },
                            {
                                "target_lang_word": "sharp",
                                "target_lang_sense_clarification": "precisely at a stated time"
                            },
                            {
                                "target_lang_word": "on the dot",
                                "target_lang_sense_clarification": "at the exact time"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ровно",
                                "target_lang_sense_clarification": "о времени"
                            },
                            {
                                "target_lang_word": "точно",
                                "target_lang_sense_clarification": "пунктуально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c2a91c0-e7a7-3338-8d2b-ca4532722670",
                    "sense_definition": "Uitroep om aan te geven dat je het volledig eens bent met wat iemand zegt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "\"Bedoel je dat we beter moeten luisteren?\" \"Ja, <w>precies</w>!\"",
                            "target_lang_translations": {
                                "en": "\"Do you mean we should listen better?\" \"Yes, <w>exactly</w>!\"",
                                "ru": "\"Ты имеешь в виду, что нам нужно лучше слушать?\" \"Да, <w>именно</w>!\""
                            }
                        },
                        {
                            "text": "<w>Precies</w>, dat is de reden waarom ik niet mee wilde gaan.",
                            "target_lang_translations": {
                                "en": "<w>Precisely</w>, that is the reason why I didn't want to go along.",
                                "ru": "<w>Именно</w>, это причина, по которой я не хотел идти."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ja precies",
                        "nou precies"
                    ],
                    "target_lang_definitions": {
                        "en": "Exclamation to indicate that you completely agree with what someone says.",
                        "ru": "Восклицание, выражающее полное согласие с собеседником."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exactly",
                                "target_lang_sense_clarification": "expression of agreement"
                            },
                            {
                                "target_lang_word": "precisely",
                                "target_lang_sense_clarification": "confirmation of correctness"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "именно",
                                "target_lang_sense_clarification": "согласие"
                            },
                            {
                                "target_lang_word": "точно",
                                "target_lang_sense_clarification": "подтверждение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5ede104e-991d-3bfe-ae05-0ed2c4420691",
                    "sense_definition": "Zoals het lijkt; in schijn.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "semblance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is hier <w>precies</w> een feestje met al die ballonnen.",
                            "target_lang_translations": {
                                "en": "It looks <w>just like</w> a party here with all those balloons.",
                                "ru": "Здесь <w>как будто</w> вечеринка со всеми этими шарами."
                            }
                        },
                        {
                            "text": "Je doet <w>precies</w> of je mij niet hoort als ik roep.",
                            "target_lang_translations": {
                                "en": "You act <w>just as if</w> you don't hear me when I call.",
                                "ru": "Ты делаешь вид, <w>словно</w> не слышишь меня, когда я зову."
                            }
                        }
                    ],
                    "synonyms": [
                        "net alsof",
                        "blijkbaar",
                        "schijnbaar"
                    ],
                    "common_phrases": [
                        "het lijkt precies",
                        "precies of"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in België (Vlaanderen)"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "As it appears; in appearance (often used in Flemish).",
                        "ru": "Как кажется; по видимости (региональное значение)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "just like",
                                "target_lang_sense_clarification": "resembling"
                            },
                            {
                                "target_lang_word": "as if",
                                "target_lang_sense_clarification": "as though"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "как будто",
                                "target_lang_sense_clarification": "сравнение"
                            },
                            {
                                "target_lang_word": "словно",
                                "target_lang_sense_clarification": "сравнение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0bb3b47e-a5ab-3237-801a-276b46d9509e",
                    "sense_definition": "Zonder fouten en met veel details; heel nauwkeurig.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "accuracy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun je mij de <w>precieze</w> datum van je verjaardag geven?",
                            "target_lang_translations": {
                                "en": "Can you give me the <w>exact</w> date of your birthday?",
                                "ru": "Можешь назвать мне <w>точную</w> дату своего дня рождения?"
                            }
                        },
                        {
                            "text": "De getuige gaf een heel <w>precieze</w> beschrijving van de dader.",
                            "target_lang_translations": {
                                "en": "The witness gave a very <w>precise</w> description of the perpetrator.",
                                "ru": "Свидетель дал очень <w>точное</w> описание преступника."
                            }
                        }
                    ],
                    "synonyms": [
                        "loepzuiver"
                    ],
                    "common_phrases": [
                        "het precieze aantal",
                        "een precieze weergave"
                    ],
                    "target_lang_definitions": {
                        "en": "Without errors and with many details; very accurate.",
                        "ru": "Без ошибок и с множеством деталей; очень точный."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "precise",
                                "target_lang_sense_clarification": "exact and detailed"
                            },
                            {
                                "target_lang_word": "exact",
                                "target_lang_sense_clarification": "correct in every detail"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "точный",
                                "target_lang_sense_clarification": "безошибочный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cf2e1e1-cf21-3b60-aa08-010da4dcca01",
                    "sense_definition": "Iemand die heel nauwkeurig werkt, erg op de regels let of veel aandacht voor details heeft.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "personality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn leraar is erg <w>precies</w> als het gaat om grammatica.",
                            "target_lang_translations": {
                                "en": "My teacher is very <w>strict</w> when it comes to grammar.",
                                "ru": "Мой учитель очень <w>педантичен</w> в вопросах грамматики."
                            }
                        },
                        {
                            "text": "Zij is een <w>precieze</w> vrouw die haar huis altijd keurig op orde heeft.",
                            "target_lang_translations": {
                                "en": "She is a <w>meticulous</w> woman who always has her house neatly in order.",
                                "ru": "Она <w>аккуратная</w> женщина, у которой в доме всегда идеальный порядок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "erg precies zijn",
                        "een precieze werker"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who works very accurately, pays close attention to rules, or has great attention to detail.",
                        "ru": "Человек, который работает очень тщательно или уделяет много внимания деталям."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "meticulous",
                                "target_lang_sense_clarification": "showing great attention to detail"
                            },
                            {
                                "target_lang_word": "precise",
                                "target_lang_sense_clarification": "careful and exact in conduct"
                            },
                            {
                                "target_lang_word": "fussy",
                                "target_lang_sense_clarification": "hard to please; fastidious"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "педантичный",
                                "target_lang_sense_clarification": "строгий к деталям"
                            },
                            {
                                "target_lang_word": "аккуратный",
                                "target_lang_sense_clarification": "тщательный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    }
                },
                {
                    "sense_id": "9c53bc83-3cc3-315c-a7f2-b7ed9d081108",
                    "sense_definition": "Officially or in agreement with established procedures and rules.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company <w>formally</w> announced the merger early this morning.",
                            "target_lang_translations": {
                                "ru": "Компания <w>официально</w> объявила о слиянии сегодня рано утром."
                            }
                        },
                        {
                            "text": "She <w>formally</w> filed a complaint regarding the safety violations.",
                            "target_lang_translations": {
                                "ru": "Она <w>официально</w> подала жалобу на нарушения техники безопасности."
                            }
                        }
                    ],
                    "antonyms": [
                        "informally"
                    ],
                    "common_phrases": [
                        "formally announce",
                        "formally request",
                        "formally charge"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально, в соответствии с установленными процедурами и правилами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "официально",
                                "target_lang_sense_clarification": "публично, от лица организации"
                            },
                            {
                                "target_lang_word": "формально",
                                "target_lang_sense_clarification": "с соблюдением всех формальностей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e31697c0-741d-3efd-ae81-69035fe0be37",
                    "sense_definition": "In a way that adheres strictly to rules, forms, or definitions, often in academic or technical contexts.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The professor <w>formally</w> proved the theorem on the blackboard.",
                            "target_lang_translations": {
                                "ru": "Профессор <w>формально</w> доказал теорему на доске."
                            }
                        },
                        {
                            "text": "We need to <w>formally</w> define the parameters before starting the experiment.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>формально</w> определить параметры перед началом эксперимента."
                            }
                        }
                    ],
                    "antonyms": [
                        "informally"
                    ],
                    "common_phrases": [
                        "formally proved",
                        "formally defined"
                    ],
                    "target_lang_definitions": {
                        "ru": "Строго придерживаясь правил, форм или определений, часто в научном или техническом контексте."

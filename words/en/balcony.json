            "senses": [
                {
                    "sense_id": "7e3e4bfe-c180-36fa-8625-7ec146e4b543",
                    "sense_definition": "An accessible structure extending from a building, especially outside a window.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We sat on the <w>balcony</w> to enjoy the fresh air and the view of the street.",
                            "target_lang_translations": {
                                "ru": "Мы сидели на <w>балконе</w>, наслаждаясь свежим воздухом и видом на улицу."
                            }
                        },
                        {
                            "text": "My hotel room has a private <w>balcony</w> where I can drink my morning coffee.",
                            "target_lang_translations": {
                                "ru": "В моем номере отеля есть собственный <w>балкон</w>, где я могу пить утренний кофе."
                            }
                        }
                    ],
                    "synonyms": [
                        "terrace",
                        "veranda"
                    ],
                    "common_phrases": [
                        "on the balcony",
                        "private balcony",
                        "balcony view"
                    ],
                    "target_lang_definitions": {
                        "ru": "Площадка с перилами, выступающая из стены здания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "балкон",
                                "target_lang_sense_clarification": "часть здания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6febfc5c-8c47-3c34-8d6a-b91dd77b8522",
                    "sense_definition": "An accessible structure overlooking a stage or the like.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tickets for the <w>balcony</w> were much cheaper than those for the front rows.",
                            "target_lang_translations": {
                                "ru": "Билеты на <w>балкон</w> были намного дешевле, чем билеты в первые ряды."
                            }
                        },
                        {
                            "text": "From our seats in the <w>balcony</w>, we had a great view of the entire orchestra.",
                            "target_lang_translations": {
                                "ru": "С наших мест на <w>балконе</w> открывался отличный вид на весь оркестр."
                            }
                        }
                    ],
                    "synonyms": [
                        "gallery",
                        "mezzanine",
                        "circle"
                    ],
                    "antonyms": [
                        "stalls"
                    ],
                    "common_phrases": [
                        "balcony seat",
                        "in the balcony"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхний ярус в зрительном зале театра или концертного зала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "балкон",
                                "target_lang_sense_clarification": "в театре"

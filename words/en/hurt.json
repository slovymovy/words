    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f53fd8db-3d9d-345c-b871-8571a3f083b1",
                    "sense_definition": "To cause physical pain or injury to a person or animal.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_injury",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Be careful with that sharp knife or you might <w>hurt</w> yourself.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен с этим острым ножом, иначе можешь <w>пора́ниться</w>."
                            }
                        },
                        {
                            "text": "The boy promised that he would never <w>hurt</w> his little brother again.",
                            "target_lang_translations": {
                                "ru": "Мальчик пообещал, что больше никогда не <w>сде́лает бо́льно</w> своему младшему брату."
                            }
                        }
                    ],
                    "synonyms": [
                        "injure",
                        "wound",
                        "harm"
                    ],
                    "antonyms": [
                        "heal",
                        "soothe"
                    ],
                    "common_phrases": [
                        "hurt yourself",
                        "hurt someone badly",
                        "get hurt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причинять физическую боль или наносить травму человеку или животному."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "причини́ть боль",
                                "target_lang_sense_clarification": "сделать больно"
                            },
                            {
                                "target_lang_word": "ра́нить",
                                "target_lang_sense_clarification": "нанести рану"
                            },
                            {
                                "target_lang_word": "пора́нить",
                                "target_lang_sense_clarification": "нанести легкую травму, порезать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c31cece-c83d-388d-abfd-17b0a48eeeb2",
                    "sense_definition": "To feel painful; to be a source of pain.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My head starts to <w>hurt</w> whenever I read without my glasses.",
                            "target_lang_translations": {
                                "ru": "У меня начинает <w>боле́ть</w> голова всякий раз, когда я читаю без очков."
                            }
                        },
                        {
                            "text": "Does your stomach still <w>hurt</w> after eating that spicy food?",
                            "target_lang_translations": {
                                "ru": "У тебя всё ещё <w>боли́т</w> живот после той острой еды?"
                            }
                        }
                    ],
                    "synonyms": [
                        "ache",
                        "smart",
                        "throb"
                    ],
                    "antonyms": [
                        "soothe",
                        "relieve"
                    ],
                    "common_phrases": [
                        "it hurts",
                        "hurt a lot",
                        "hurt like hell"
                    ],
                    "target_lang_definitions": {
                        "ru": "Болеть, испытывать боль; быть источником боли (о частях тела)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "боле́ть",
                                "target_lang_sense_clarification": "испытывать боль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3212eb9-cb7c-3605-9e45-c5864189db5d",
                    "sense_definition": "To cause someone to feel emotional pain, distress, or sadness.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotional_pain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It really <w>hurt</w> her feelings when you forgot to call her on her birthday.",
                            "target_lang_translations": {
                                "ru": "Это сильно <w>заде́ло</w> её чувства, когда ты забыл позвонить ей в день рождения."
                            }
                        },
                        {
                            "text": "He was deeply <w>hurt</w> by the rude comments his colleagues made.",
                            "target_lang_translations": {
                                "ru": "Его глубоко <w>оби́дели</w> грубые замечания коллег."
                            }
                        }
                    ],
                    "synonyms": [
                        "upset",
                        "offend",
                        "grieve",
                        "sadden"
                    ],
                    "antonyms": [
                        "comfort",
                        "soothe",
                        "alleviate"
                    ],
                    "common_phrases": [
                        "hurt someone's feelings",
                        "deeply hurt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причинять кому-либо душевную боль, расстраивать или обижать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оби́деть",
                                "target_lang_sense_clarification": "оскорбить, расстроить"
                            },
                            {
                                "target_lang_word": "ра́нить",
                                "target_lang_sense_clarification": "причинить душевную боль"
                            },
                            {
                                "target_lang_word": "заде́ть",
                                "target_lang_sense_clarification": "затронуть чувства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7826811c-e6c1-32df-b0c1-f49d26ff1ddc",
                    "sense_definition": "To have a bad effect on something; to damage, impair, or hinder progress.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_harm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The recent scandal will likely <w>hurt</w> the politician's chances of winning the election.",
                            "target_lang_translations": {
                                "ru": "Недавний скандал, вероятно, <w>навреди́т</w> шансам политика на победу в выборах."
                            }
                        },
                        {
                            "text": "It wouldn't <w>hurt</w> to check the weather forecast before we leave for the trip.",
                            "target_lang_translations": {
                                "ru": "Не <w>помеша́ло</w> бы проверить прогноз погоды перед тем, как мы отправимся в поездку."
                            }
                        }
                    ],
                    "synonyms": [
                        "damage",
                        "impair",
                        "undermine",
                        "hinder"
                    ],
                    "antonyms": [
                        "heal",
                        "relieve"
                    ],
                    "common_phrases": [
                        "hurt the economy",
                        "wouldn't hurt to",
                        "hurt your chances"
                    ],
                    "target_lang_definitions": {
                        "ru": "Негативно сказываться на чем-либо; вредить или мешать развитию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "навреди́ть",
                                "target_lang_sense_clarification": "нанести вред"
                            },
                            {
                                "target_lang_word": "помеша́ть",
                                "target_lang_sense_clarification": "создать препятствие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d050848e-12ae-3e1d-8ab5-29ad415d2c1b",
                    "sense_definition": "A physical injury or wound that causes pain.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_injury",
                    "name_type": "no",
                    "examples": [
                        {

{
    "word_family": [
        "nadreński",
        "Nadrenia",
        "renowy"
    ],
    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "cb10398b-0ef9-3800-b2b3-610a0b27e09d",
                    "sense_definition": "Jedna z najdłuższych rzek w Europie, przepływająca między innymi przez Szwajcarię, Niemcy, Francję i Holandię.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "river_rhine",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "<w>Ren</w> jest jedną z najważniejszych rzek transportowych w Europie.",
                            "target_lang_translations": {
                                "ru": "<w>Рейн</w> является одной из важнейших транспортных рек в Европе."
                            }
                        },
                        {
                            "text": "Wiele zabytkowych zamków znajduje się nad <w>Renem</w>.",
                            "target_lang_translations": {
                                "ru": "Многие старинные замки расположены на <w>Рейне</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rzeka Ren",
                        "dolina Renu",
                        "nad Renem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из самых длинных рек в Европе, протекающая, в частности, через Швейцарию, Германию, Францию и Нидерланды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Рейн",
                                "target_lang_sense_clarification": "река"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6f97d3b9-9c82-396b-ab88-ac04928dee0d",
                    "sense_definition": "Ssak z rodziny jeleniowatych zamieszkujący strefę arktyczną i subarktyczną; renifer.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "animal_reindeer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez ośnieżoną drogę przeszło stado dzikich <w>renów</w>.",
                            "target_lang_translations": {
                                "ru": "Через заснеженную дорогу прошло стадо диких <w>северных оленей</w>."
                            }
                        },
                        {
                            "text": "Mieszkańcy północy od wieków hodują <w>reny</w>.",
                            "target_lang_translations": {
                                "ru": "Жители севера веками разводят <w>северных оленей</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "renifer",
                        "karibu",
                        "renifer tundrowy"
                    ],
                    "common_phrases": [
                        "stado renów",
                        "hodowla renów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Млекопитающее из семейства оленевых, обитающее в арктической и субарктической зонах; северный олень."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "северный олень",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5952916b-b4f4-3a73-9d52-205c0eb28fc4",
                    "sense_definition": "Pierwiastek chemiczny o symbolu Re i liczbie atomowej 75; twardy, srebrzystobiały metal.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "element_rhenium",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jakie właściwości chemiczne i fizyczne ma <w>ren</w>?",
                            "target_lang_translations": {
                                "ru": "Какими химическими и физическими свойствами обладает <w>рений</w>?"
                            }
                        },
                        {
                            "text": "W przemyśle lotniczym często wykorzystuje się stopy zawierające <w>ren</w>.",
                            "target_lang_translations": {
                                "ru": "В авиационной промышленности часто используются сплавы, содержащие <w>рений</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Re"
                    ],
                    "target_lang_definitions": {
                        "ru": "Химический элемент с символом Re и атомным номером 75; твердый серебристо-белый металл."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рений",
                                "target_lang_sense_clarification": "химический элемент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88429a48-b687-3369-a577-d9e807077fb4",
                    "sense_definition": "W konfucjanizmie: cnota humanitarności, życzliwości i miłości do drugiego człowieka.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "philosophy_ren",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dla konfucjanistów <w>ren</w> jest najważniejszą cnotą moralną.",
                            "target_lang_translations": {
                                "ru": "Для конфуцианцев <w>жэнь</w> является важнейшей моральной добродетелью."
                            }
                        },
                        {
                            "text": "Pojęcie <w>ren</w> jest trudne do jednoznacznego przetłumaczenia na języki europejskie.",
                            "target_lang_translations": {
                                "ru": "Понятие <w>жэнь</w> трудно однозначно перевести на европейские языки."
                            }
                        }
                    ],
                    "synonyms": [
                        "humanitarność",
                        "żen"
                    ],
                    "target_lang_definitions": {
                        "ru": "В конфуцианстве: добродетель гуманности, доброжелательности и любви к другому человеку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жэнь",
                                "target_lang_sense_clarification": "философия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "b6950cce-7a60-3b30-987c-2952565972ec",
                    "sense_definition": "Vaststellen wat er gaat gebeuren.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "decision_making",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ouders <w>beslisten</w> dat hun kind naar die bepaalde school ging.",
                            "target_lang_translations": {
                                "en": "The parents <w>decided</w> that their child would go to that particular school.",
                                "ru": "Родители <w>решили</w>, что их ребенок пойдет именно в эту школу."
                            }
                        },
                        {
                            "text": "De directeur moet morgen <w>beslissen</w> of het project doorgaat.",
                            "target_lang_translations": {
                                "en": "The director must <w>decide</w> tomorrow whether the project will go ahead.",
                                "ru": "Директор должен завтра <w>решить</w>, будет ли продолжен проект."
                            }
                        }
                    ],
                    "synonyms": [
                        "besluiten"
                    ],
                    "common_phrases": [
                        "snel beslissen",
                        "zelf beslissen",
                        "erover beslissen"
                    ],
                    "target_lang_definitions": {
                        "en": "To establish or make a judgment about what is going to happen.",
                        "ru": "Определять, что произойдет; принимать решение."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "decide",
                                "target_lang_sense_clarification": "to make a choice or judgment"
                            },
                            {
                                "target_lang_word": "make a decision",
                                "target_lang_sense_clarification": "to reach a conclusion"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "решать",
                                "target_lang_sense_clarification": "принимать решение"
                            },
                            {
                                "target_lang_word": "принимать решение",
                                "target_lang_sense_clarification": "делать вывод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76344a88-fe23-3bd8-97b9-db37b3883479",
                    "sense_definition": "Kiezen tussen verschillende mogelijkheden of de doorslag geven.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "decision_making",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze spannende wedstrijd zal <w>beslissen</w> wie de winnaar van het toernooi is.",
                            "target_lang_translations": {
                                "en": "This exciting match will <w>decide</w> who the winner of the tournament is.",
                                "ru": "Этот захватывающий матч <w>решит</w>, кто станет победителем турнира."
                            }
                        },
                        {
                            "text": "Ik vind het erg moeilijk om te <w>beslissen</w> welke kleur ik wil.",
                            "target_lang_translations": {
                                "en": "I find it very difficult to <w>decide</w> which color I want.",
                                "ru": "Мне очень трудно <w>решить</w>, какой цвет я хочу."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitmaken"
                    ],
                    "common_phrases": [
                        "de wedstrijd beslissen",
                        "het lot beslissen",
                        "moeilijk te beslissen"
                    ],
                    "target_lang_definitions": {
                        "en": "To choose between different possibilities or to determine the outcome of something.",
                        "ru": "Выбирать из нескольких вариантов или определять исход события."
                    },

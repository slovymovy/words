                {
                    "sense_id": "fddd53b9-fed1-3914-a4ed-4230dc4471db",
                    "sense_definition": "Een staaf van brandbaar materiaal, meestal was of vet, met een lont in het midden die licht en warmte geeft als hij wordt aangestoken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "light_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zullen we vanavond gezellig de <w>kaarsen</w> aansteken in de woonkamer?",
                            "target_lang_translations": {
                                "en": "Shall we light the <w>candles</w> in the living room tonight for a cozy atmosphere?"
                            }
                        },
                        {
                            "text": "Hij blies de <w>kaars</w> uit voordat hij naar bed ging.",
                            "target_lang_translations": {
                                "en": "He blew out the <w>candle</w> before he went to bed."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kaarsen aansteken",
                        "kaarsen uitblazen",
                        "brandende kaars"
                    ],
                    "target_lang_definitions": {
                        "en": "A stick of flammable material, usually wax or fat, with a wick in the middle that gives light and heat when lit."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "candle",
                                "target_lang_sense_clarification": "light source"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77c6020e-e547-3ef9-9993-739a1cc3f1d8",
                    "sense_definition": "Een verouderde eenheid om lichtsterkte aan te duiden, tegenwoordig vervangen door de candela.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De lichtsterkte van deze antieke lamp is gelijk aan dertig <w>kaarsen</w>.",
                            "target_lang_translations": {
                                "en": "The luminous intensity of this antique lamp is equal to thirty <w>candles</w>."
                            }
                        },
                        {
                            "text": "In oude technische handleidingen wordt de intensiteit van lichtbronnen nog in <w>kaarsen</w> uitgedrukt.",
                            "target_lang_translations": {
                                "en": "In old technical manuals, the intensity of light sources is still expressed in <w>candles</w>."
                            }
                        }
                    ],

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c448315b-a591-3ed7-a389-067a09e2aed5",
                    "sense_definition": "Skupić uwagę na dźwiękach, mowie lub muzyce przez pewien czas; zapoznać się z czymś za pomocą słuchu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chcę chwilę <posłuchać> muzyki, żeby się zrelaksować po pracy.",
                            "target_lang_translations": {
                                "ru": "Я хочу немного <w>послушать</w> музыку, чтобы расслабиться после работы."
                            }
                        },
                        {
                            "text": "Usiądź proszę i <posłuchaj> uważnie tego, co mam ci do powiedzenia.",
                            "target_lang_translations": {
                                "ru": "Сядь, пожалуйста, и внимательно <w>послушай</w> то, что я хочу тебе сказать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "posłuchać muzyki",
                        "posłuchać radia",
                        "posłuchać wiadomości",
                        "posłuchać piosenki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сосредоточить внимание на звуках, речи или музыке в течение некоторого времени; ознакомиться с чем-либо на слух."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "послушать",
                                "target_lang_sense_clarification": "восприятие звуков"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "264db848-0f1e-3f5f-9214-0ad4465cbf33",
                    "sense_definition": "Poddać się czyjejś decyzji, wypełnić czyjeś polecenie lub postąpić zgodnie z czyjąś radą.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "obedience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko postanowiło <posłuchać> rodziców i wrócić wcześnie do domu.",
                            "target_lang_translations": {
                                "ru": "Ребенок решил <w>послушаться</w> родителей и вернуться домой пораньше."
                            }
                        },
                        {
                            "text": "Powinieneś <posłuchać> rady lekarza i brać te leki regularnie.",
                            "target_lang_translations": {
                                "ru": "Тебе следует <w>послушать</w> совета врача и принимать эти лекарства регулярно."
                            }
                        }
                    ],
                    "synonyms": [
                        "usłuchać",
                        "posłuchać się"
                    ],
                    "antonyms": [
                        "sprzeciwić się",
                        "zlekceważyć",
                        "zignorować"
                    ],
                    "common_phrases": [
                        "posłuchać rady",
                        "posłuchać rodziców",
                        "posłuchać rozkazu",
                        "posłuchać głosu rozsądku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подчиниться чьему-либо решению, выполнить чье-то указание или поступить согласно чьему-то совету."
                    },
                    "translations": {

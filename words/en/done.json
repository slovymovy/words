                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "adj_cooked",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Check if the potatoes are <w>done</w> before you turn off the stove.",
                            "target_lang_translations": {
                                "ru": "Проверь, <w>готов</w> ли картофель, прежде чем выключать плиту."
                            }
                        },
                        {
                            "text": "I like my steak well <w>done</w>, not rare.",
                            "target_lang_translations": {
                                "ru": "Я люблю хорошо <w>прожаренный</w> стейк, а не с кровью."
                            }
                        }
                    ],
                    "synonyms": [
                        "concluded",
                        "finished"
                    ],
                    "antonyms": [
                        "raw",
                        "undercooked"
                    ],
                    "common_phrases": [
                        "well done",
                        "medium done"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью приготовленный и готовый к употреблению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "готовый",
                                "target_lang_sense_clarification": "о еде"
                            },
                            {
                                "target_lang_word": "прожаренный",
                                "target_lang_sense_clarification": "о мясе"
                            }
                        ]

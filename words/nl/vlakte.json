                {
                    "sense_id": "6ac3520a-c822-3b6a-9176-449a0e97e078",
                    "sense_definition": "Een uitgestrekt gebied met weinig of geen hoogteverschillen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij Denver eindigt de <w>vlakte</w> en begint het hoge gebergte.",
                            "target_lang_translations": {
                                "en": "Near Denver the <w>plain</w> ends and the high mountains begin.",
                                "ru": "У Денвера заканчивается <w>равнина</w> и начинаются высокие горы."
                            }
                        },
                        {
                            "text": "We keken uit over een dorre <w>vlakte</w> waar bijna niets groeide.",
                            "target_lang_translations": {
                                "en": "We looked out over a barren <w>plain</w> where almost nothing grew.",
                                "ru": "Мы смотрели на сухую <w>равнину</w>, где почти ничего не росло."
                            }
                        },
                        {
                            "text": "In de verte zagen we dieren grazen op de open <w>vlakte</w>.",
                            "target_lang_translations": {
                                "en": "In the distance we saw animals grazing on the open <w>plain</w>.",
                                "ru": "Вдали мы видели животных, пасущихся на открытой <w>равнине</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "laagland",
                        "veld"
                    ],
                    "antonyms": [
                        "gebergte",
                        "heuvel"
                    ],
                    "common_phrases": [
                        "open vlakte",
                        "uitgestrekte vlakte",
                        "kale vlakte",
                        "dorre vlakte"
                    ],
                    "target_lang_definitions": {
                        "en": "An extensive area of land with little or no variation in elevation.",
                        "ru": "Обширная территория с небольшими перепадами высот или без них."
                    },

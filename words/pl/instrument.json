                },
                {
                    "sense_id": "7fa16467-151b-390a-bad6-12c8ebce6339",
                    "sense_definition": "Precyzyjne urządzenie techniczne służące do wykonywania pomiarów, obserwacji lub sterowania procesami.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "technical_tools",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To przedsiębiorstwo zajmuje się produkcją precyzyjnych <w>instrumentów</w> pomiarowych.",
                            "target_lang_translations": {
                                "ru": "Это предприятие занимается производством точных измерительных <w>приборов</w>."
                            }
                        },
                        {
                            "text": "Pilot musi ufać <w>instrumentom</w> pokładowym podczas lotu we mgle.",
                            "target_lang_translations": {
                                "ru": "Пилот должен доверять бортовым <w>приборам</w> во время полета в тумане."
                            }
                        }
                    ],
                    "synonyms": [
                        "narzędzie chirurgiczne"

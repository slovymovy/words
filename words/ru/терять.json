{
    "word_family": [
        "потеря",
        "утеря",
        "теряться",
        "растерянность",
        "потерянный"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "328234c8-5bd2-311d-a10c-062d9b9af215",
                    "sense_definition": "Лишаться чего-либо по небрежности, забывчивости или случайно",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "loss_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я постоянно <w>теряю</w> свои ключи от квартиры.",
                            "target_lang_translations": {
                                "en": "I constantly <w>lose</w> my apartment keys."
                            }
                        },
                        {
                            "text": "Туристы часто <w>теряют</w> багаж при пересадке на другой рейс.",
                            "target_lang_translations": {
                                "en": "Tourists often <w>lose</w> their luggage when transferring to another flight."
                            }
                        }
                    ],
                    "synonyms": [
                        "лишаться"
                    ],
                    "antonyms": [
                        "находить"
                    ],
                    "common_phrases": [
                        "терять ключи",
                        "терять вещи",
                        "часто терять"
                    ],
                    "target_lang_definitions": {
                        "en": "To be deprived of something due to negligence, forgetfulness, or accident"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lose",
                                "target_lang_sense_clarification": "to cease to have possession of something physically"
                            },
                            {
                                "target_lang_word": "misplace",
                                "target_lang_sense_clarification": "to lose temporarily by forgetting where one put it"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f4a2891-50ff-3a1b-9995-e7013ba104ba",
                    "sense_definition": "Переставать обладать чем-либо, утрачивать свойства, связи или контроль над чем-либо",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "loss_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "При сильном волнении он начинает <w>терять</w> дар речи.",
                            "target_lang_translations": {
                                "en": "When very agitated, he begins to <w>lose</w> the power of speech."
                            }
                        },
                        {
                            "text": "Мы не должны <w>терять</w> надежду даже в самой сложной ситуации.",
                            "target_lang_translations": {
                                "en": "We must not <w>lose</w> hope even in the most difficult situation."
                            }
                        }
                    ],
                    "synonyms": [
                        "утрачивать",
                        "лишаться"
                    ],
                    "antonyms": [
                        "обретать"
                    ],
                    "common_phrases": [
                        "терять сознание",
                        "терять надежду",
                        "терять контроль",
                        "терять друзей",
                        "терять вес"
                    ],
                    "target_lang_definitions": {
                        "en": "To cease to possess something, or to lose properties, connections, or control"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lose",
                                "target_lang_sense_clarification": "to be deprived of abstract qualities, hope, or control"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "64976384-d0cc-3fd6-b8d7-cb0ce7e21727",
                    "sense_definition": "Бесцельно или напрасно расходовать время или ресурсы",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "waste",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не стоит <w>терять</w> время на пустые разговоры.",
                            "target_lang_translations": {
                                "en": "It is not worth <w>wasting</w> time on empty conversations."
                            }
                        },
                        {
                            "text": "Из-за плохой организации компания продолжает <w>терять</w> деньги.",
                            "target_lang_translations": {
                                "en": "Due to poor organization, the company continues to <w>lose</w> money."
                            }
                        }
                    ],
                    "synonyms": [
                        "тратить",
                        "растрачивать",
                        "расходовать"
                    ],
                    "common_phrases": [
                        "терять время",
                        "терять время зря",
                        "терять даром"
                    ],
                    "target_lang_definitions": {
                        "en": "To spend time or resources aimlessly or in vain"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "waste",
                                "target_lang_sense_clarification": "to use time or resources inefficiently"
                            },
                            {
                                "target_lang_word": "lose",
                                "target_lang_sense_clarification": "to suffer the loss of money or time"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a3c70f25-fd51-380c-b034-3f17c6f2d2af",
                    "sense_definition": "De zorg, bescherming of het toezicht dat iemand over iets of iemand anders heeft.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "protection_care",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De tante nam het jonge weeskind liefdevol onder haar <w>hoede</w>.",
                            "target_lang_translations": {
                                "ru": "Тетя с любовью взяла юную сироту под свою <w>опеку</w>.",
                                "en": "The aunt lovingly took the young orphan under her <w>care</w>."
                            }
                        },
                        {
                            "text": "Het beheer van het natuurpark valt onder de <w>hoede</w> van de overheid.",
                            "target_lang_translations": {
                                "ru": "Управление природным парком находится под <w>покровительством</w> государства.",
                                "en": "The management of the nature park falls under the <w>protection</w> of the government."
                            }
                        }
                    ],
                    "synonyms": [
                        "bescherming",
                        "zorg",
                        "toezicht",
                        "bewaring"
                    ],
                    "antonyms": [
                        "verwaarlozing"
                    ],
                    "common_phrases": [
                        "onder iemands hoede",
                        "onder zijn hoede nemen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Забота, защита или надзор, осуществляемые кем-либо над кем-то или чем-то.",
                        "en": "The care, protection, or supervision that someone has over something or someone else."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опека",
                                "target_lang_sense_clarification": "забота, попечение"
                            },
                            {
                                "target_lang_word": "покровительство",
                                "target_lang_sense_clarification": "защита, поддержка"
                            },
                            {
                                "target_lang_word": "присмотр",
                                "target_lang_sense_clarification": "надзор, наблюдение"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "care",
                                "target_lang_sense_clarification": "supervision or responsibility for welfare"
                            },
                            {
                                "target_lang_word": "protection",
                                "target_lang_sense_clarification": "the act of keeping safe"
                            },
                            {
                                "target_lang_word": "custody",
                                "target_lang_sense_clarification": "formal protective care or guardianship"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6823c4ab-91fb-3cb0-b4b3-c10264781b26",
                    "sense_definition": "Een toestand van waakzaamheid en oplettendheid om mogelijk gevaar te voorkomen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "alertness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet altijd op je <w>hoede</w> zijn als je alleen door een donker bos loopt.",
                            "target_lang_translations": {
                                "ru": "Нужно всегда быть <w>начеку</w>, когда идешь один через темный лес.",
                                "en": "You must always be on your <w>guard</w> when walking alone through a dark forest."
                            }
                        },
                        {
                            "text": "De soldaten waren voortdurend op hun <w>hoede</w> voor een onverwachte aanval.",
                            "target_lang_translations": {
                                "ru": "Солдаты были постоянно <w>настороже</w> из-за возможной неожиданной атаки.",
                                "en": "The soldiers were constantly on their <w>guard</w> against an unexpected attack."
                            }
                        }
                    ],
                    "synonyms": [
                        "waakzaamheid",
                        "oplettendheid"
                    ],
                    "antonyms": [
                        "onoplettendheid"
                    ],
                    "common_phrases": [
                        "op zijn hoede zijn",
                        "op je hoede zijn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние бдительности и внимательности для предотвращения возможной опасности.",
                        "en": "A state of vigilance and attentiveness to prevent possible danger."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начеку",
                                "target_lang_sense_clarification": "в выражении \"быть начеку\""
                            },
                            {
                                "target_lang_word": "настороже",
                                "target_lang_sense_clarification": "в состоянии готовности к опасности"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "guard",
                                "target_lang_sense_clarification": "in the phrase \"on one's guard\""
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "88b11122-af42-340f-976b-7ddf235c2572",
                    "sense_definition": "Enkelvoudige aanvoegende wijs van <w>hoeden</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moge God u <w>hoede</w> op uw lange en gevaarlijke reis.",
                            "target_lang_translations": {
                                "ru": "Да <w>хранит</w> вас Бог в вашем долгом и опасном путешествии.",
                                "en": "May God <w>protect</w> you on your long and dangerous journey."
                            }
                        },
                        {
                            "text": "Men <w>hoede</w> zich voor goedkope namaakproducten die snel kapotgaan.",
                            "target_lang_translations": {
                                "ru": "Следует <w>остерегаться</w> дешевых подделок, которые быстро ломаются.",
                                "en": "One should <w>beware</w> of cheap fake products that break quickly."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "aanvoegende wijs"
                        },
                        {
                            "trait_type": "archaic",
                            "comment": "voornamelijk in vaste uitdrukkingen of plechtige taal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма сослагательного наклонения глагола \"hoeden\" (беречь, охранять).",
                        "en": "Form of the subjunctive mood of the verb \"hoeden\" (to protect, to guard)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хранить",
                                "target_lang_sense_clarification": "беречь, защищать (в высоком стиле)"
                            },
                            {
                                "target_lang_word": "остерегаться",
                                "target_lang_sense_clarification": "быть осторожным (в возвратной форме)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "protect",
                                "target_lang_sense_clarification": "to keep safe from harm"
                            },
                            {
                                "target_lang_word": "beware",
                                "target_lang_sense_clarification": "used reflexively; to be cautious of"
                            },
                            {
                                "target_lang_word": "guard",
                                "target_lang_sense_clarification": "to watch over or shield"
                            }
                        ]
                    }
                }
            ]
        }
    ]

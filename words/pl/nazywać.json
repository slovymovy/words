    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a3807680-285a-3da0-acb4-c8bc9ce9363f",
                    "sense_definition": "Mieć na imię, nosić określone nazwisko lub nazwę; być znanym pod danym określeniem (używane z zaimkiem zwrotnym „się”)",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "identity_naming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzień dobry, <w>nazywam się</w> Piotr Kowalski i jestem nowym pracownikiem.",
                            "target_lang_translations": {
                                "ru": "Добрый день, <w>меня зовут</w> Петр Ковальский, и я новый сотрудник."
                            }
                        },
                        {
                            "text": "To piękne, historyczne miasto <w>nazywa się</w> Kraków.",
                            "target_lang_translations": {
                                "ru": "Этот красивый исторический город <w>называется</w> Краков."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwać się",
                        "mieć na imię",
                        "nosić imię"
                    ],
                    "common_phrases": [
                        "jak się nazywasz",
                        "nazywać się po mężu",
                        "godnie się nazywać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Носить имя, фамилию или название; быть известным под определенным именем (употребляется с возвратным местоимением «się»)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "называться",
                                "target_lang_sense_clarification": "носить имя или название (о предметах, местах)"
                            },
                            {
                                "target_lang_word": "звать",
                                "target_lang_sense_clarification": "в конструкции «меня зовут» (о людях)"
                            },
                            {
                                "target_lang_word": "зваться",
                                "target_lang_sense_clarification": "именоваться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9d5bcfde-883f-396d-87d0-7572d22d7ac0",
                    "sense_definition": "Nadawać komuś lub czemuś konkretną nazwę, imię lub tytuł",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "giving_names",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rodzice postanowili <w>nazywać</w> swoją córkę zdrobnieniem Zosia.",
                            "target_lang_translations": {
                                "ru": "Родители решили <w>называть</w> свою дочь уменьшительным именем Зося."
                            }
                        },
                        {
                            "text": "Mieszkańcy zaczęli <w>nazywać</w> ten park zielonymi płucami miasta.",
                            "target_lang_translations": {
                                "ru": "Жители начали <w>называть</w> этот парк зелеными легкими города."
                            }
                        }
                    ],
                    "synonyms": [
                        "mianować",
                        "określać"
                    ],
                    "common_phrases": [
                        "nazywać rzeczy po imieniu",
                        "nazywać kogoś imieniem",
                        "nazywać na cześć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать кому-то или чему-то конкретное название, имя или титул"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "называть",
                                "target_lang_sense_clarification": "давать имя или название"
                            },
                            {
                                "target_lang_word": "нарекать",
                                "target_lang_sense_clarification": "давать имя (торжественно или устар.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40443bc8-41cb-3a09-bf96-168457fc85fe",
                    "sense_definition": "Używać określonego słowa lub wyrażenia, aby kogoś lub coś opisać, sklasyfikować lub zidentyfikować",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "describing_categorizing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi przyjaciele często <w>nazywają</w> mnie szczęściarzem, ponieważ wygrywam w loterii.",
                            "target_lang_translations": {
                                "ru": "Мои друзья часто <w>называют</w> меня счастливчиком, так как я выигрываю в лотерею."
                            }
                        },
                        {
                            "text": "Czy można <w>nazywać</w> sztuką coś, co zostało stworzone przez maszynę?",
                            "target_lang_translations": {
                                "ru": "Можно ли <w>называть</w> искусством то, что было создано машиной?"
                            }
                        }
                    ],
                    "synonyms": [
                        "określać",
                        "zwać"
                    ],
                    "common_phrases": [
                        "tak to można nazwać",
                        "niesłusznie kogoś nazywać",
                        "nazywać kogoś przyjacielem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать определенное слово или выражение, чтобы описать, классифицировать или идентифицировать кого-то или что-то"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "называть",
                                "target_lang_sense_clarification": "характеризовать, описывать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
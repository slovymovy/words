    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "945e7d82-ccc7-3b34-b237-b936388861b5",
                    "sense_definition": "Designed to turn around a central point, or having parts that turn around in this way.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The machine uses a <w>rotary</w> blade to cut the grass evenly.",
                            "target_lang_translations": {
                                "ru": "В этой машине используется <w>роторное</w> лезвие, чтобы ровно стричь траву."
                            }
                        },
                        {
                            "text": "My grandmother still has an old <w>rotary</w> phone in her hallway.",
                            "target_lang_translations": {
                                "ru": "У моей бабушки в коридоре до сих пор стоит старый <w>дисковый</w> телефон."
                            }
                        }
                    ],
                    "synonyms": [
                        "rotating",
                        "revolving",
                        "spinning"
                    ],
                    "antonyms": [
                        "fixed",
                        "stationary"
                    ],
                    "common_phrases": [
                        "rotary engine",
                        "rotary dial",
                        "rotary motion",
                        "rotary club"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предназначенный для вращения вокруг центральной точки или имеющий вращающиеся части."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "роторный",
                                "target_lang_sense_clarification": "технический термин; относящийся к механизмам"
                            },
                            {
                                "target_lang_word": "вращающийся",
                                "target_lang_sense_clarification": "общий термин для движущихся частей"
                            },
                            {
                                "target_lang_word": "дисковый",
                                "target_lang_sense_clarification": "в контексте телефона с наборным диском"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "823c06a5-6629-379a-ae83-7e28f26277ae",
                    "sense_definition": "A road junction where traffic moves in one direction around a central island to reach different exits.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should slow down before you enter the <w>rotary</w>.",
                            "target_lang_translations": {
                                "ru": "Вам следует снизить скорость перед въездом на <w>круговую развязку</w>."
                            }
                        },
                        {
                            "text": "Take the second exit off the <w>rotary</w> to get to the city center.",
                            "target_lang_translations": {
                                "ru": "Сверните на втором съезде с <w>кольца</w>, чтобы попасть в центр города."
                            }
                        }
                    ],
                    "synonyms": [
                        "roundabout",
                        "traffic circle"
                    ],
                    "common_phrases": [
                        "enter the rotary",
                        "exit the rotary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дорожная развязка, где движение транспорта осуществляется в одном направлении вокруг центрального острова."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "круговая развязка",
                                "target_lang_sense_clarification": "официальный термин"
                            },
                            {
                                "target_lang_word": "кольцо",
                                "target_lang_sense_clarification": "разговорное название кругового движения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e824a71-2cff-3a6c-adae-0bb7eea8446c",
                    "sense_definition": "A local branch of an international organization of business and professional people dedicated to community service.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "He attends the local <w>Rotary</w> meeting every Tuesday afternoon.",
                            "target_lang_translations": {
                                "ru": "Он посещает собрание местного клуба <w>Ротари</w> каждый вторник днем."
                            }
                        },
                        {
                            "text": "The <w>Rotary</w> raised a significant amount of money for the new hospital wing.",
                            "target_lang_translations": {
                                "ru": "Клуб <w>Ротари</w> собрал значительную сумму денег для нового крыла больницы."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Местное отделение международной организации деловых и профессиональных людей, занимающихся общественной деятельностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ротари",
                                "target_lang_sense_clarification": "название организации (Ротари-клуб)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
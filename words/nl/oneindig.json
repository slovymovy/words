            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6229a0ab-a845-3db2-9df2-e3bd70deb099",
                    "sense_definition": "Zonder einde of grenzen in ruimte, tijd of aantal.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "limitless",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het heelal is volgens veel wetenschappers <w>oneindig</w> groot en dijt nog steeds uit.",
                            "target_lang_translations": {
                                "ru": "По мнению многих ученых, Вселенная <w>бесконечно</w> огромна и продолжает расширяться.",
                                "en": "According to many scientists, the universe is <w>infinitely</w> large and is still expanding."
                            }
                        },
                        {
                            "text": "Er lijken <w>oneindig</w> veel mogelijkheden te zijn om dit probleem op te lossen.",
                            "target_lang_translations": {
                                "ru": "Кажется, существует <w>бесконечно</w> много возможностей для решения этой проблемы.",
                                "en": "There seem to be <w>infinitely</w> many possibilities to solve this problem."
                            }
                        }
                    ],
                    "synonyms": [
                        "eindeloos",
                        "onbegrensd",
                        "onbeperkt",
                        "grenzeloos"
                    ],
                    "antonyms": [
                        "eindig",
                        "begrensd",
                        "beperkt"
                    ],
                    "common_phrases": [
                        "oneindige ruimte",
                        "oneindig veel",
                        "in het oneindige"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не имеющий конца или границ в пространстве, времени или количестве.",
                        "en": "Having no end or limits in space, time, or number."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бесконечный",
                                "target_lang_sense_clarification": "не имеющий конца"
                            },
                            {
                                "target_lang_word": "безграничный",
                                "target_lang_sense_clarification": "не имеющий пределов"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "infinite",
                                "target_lang_sense_clarification": "without limits"
                            },
                            {
                                "target_lang_word": "endless",
                                "target_lang_sense_clarification": "without end"
                            },
                            {
                                "target_lang_word": "limitless",
                                "target_lang_sense_clarification": "without boundaries"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba69fdd1-74a4-34e8-89ed-27aaf1e26bfa",
                    "sense_definition": "Heel erg groot, langdurig of intens; in zeer hoge mate.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het wachten op de uitslag leek voor mijn gevoel <w>oneindig</w> lang te duren.",
                            "target_lang_translations": {
                                "ru": "Ожидание результата, по моим ощущениям, длилось <w>бесконечно</w> долго.",
                                "en": "To me, waiting for the result seemed to take <w>infinitely</w> long."
                            }
                        },
                        {
                            "text": "Ik ben mijn ouders <w>oneindig</w> dankbaar voor alle steun die ze mij hebben gegeven.",
                            "target_lang_translations": {
                                "ru": "Я <w>бесконечно</w> благодарен своим родителям за всю ту поддержку, которую они мне оказали.",
                                "en": "I am <w>infinitely</w> grateful to my parents for all the support they have given me."
                            }
                        }
                    ],
                    "synonyms": [
                        "eindeloos"
                    ],
                    "common_phrases": [
                        "oneindig dankbaar",
                        "oneindig lang",
                        "oneindig gelukkig"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень большой, длительный или интенсивный; в очень высокой степени.",
                        "en": "Very large, long-lasting, or intense; to a very high degree."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бесконечный",

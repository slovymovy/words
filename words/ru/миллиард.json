                    "synonyms": [
                        "ярд",
                        "арбуз",
                        "лимард"
                    ],
                    "common_phrases": [
                        "заработать миллиард",
                        "золотой миллиард"
                    ],
                    "target_lang_definitions": {
                        "en": "A huge amount of something; often used to denote a large sum of money."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "billion",
                                "target_lang_sense_clarification": "large sum of money or quantity"

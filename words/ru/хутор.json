    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e6dcbe37-1e54-3b51-b4ee-0950c2c514da",
                    "sense_definition": "Небольшое селение, деревня (обычно на юге России или на Украине).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Летом мы всей семьёй часто ездили отдыхать на бабушкин <w>хутор</w>.",
                            "target_lang_translations": {
                                "en": "In the summer, our whole family often went to stay at grandmother's <w>farmstead</w>."
                            }
                        },
                        {
                            "text": "На этом <w>хуторе</w> все соседи знали друг друга и жили очень дружно.",
                            "target_lang_translations": {
                                "en": "In this <w>hamlet</w>, all the neighbors knew each other and lived very harmoniously."
                            }
                        }
                    ],
                    "synonyms": [
                        "мыза",
                        "усадьба",
                        "фольварк"
                    ],
                    "common_phrases": [
                        "жить на хуторе",
                        "дальний хутор",
                        "поехать на хутор"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "юг России, Украина"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A small settlement or village, typically found in Southern Russia or Ukraine."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hamlet",
                                "target_lang_sense_clarification": "a small settlement, smaller than a village"
                            },
                            {
                                "target_lang_word": "farmstead",
                                "target_lang_sense_clarification": "a farm with its buildings and surrounding land"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09b1449e-e105-3a89-bf72-33406b2b44b5",
                    "sense_definition": "Обособленное хозяйство с земельным участком и усадьбой владельца.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Фермер купил большой участок земли и основал там свой собственный <w>хутор</w>.",
                            "target_lang_translations": {
                                "en": "The farmer bought a large plot of land and founded his own <w>homestead</w> there."
                            }
                        },
                        {
                            "text": "Старый <w>хутор</w> стоял вдали от большой дороги, окружённый густым лесом.",
                            "target_lang_translations": {
                                "en": "The old <w>isolated farm</w> stood far from the main road, surrounded by dense forest."
                            }
                        }
                    ],
                    "synonyms": [
                        "усадьба",
                        "мыза",
                        "фольварк"
                    ],
                    "common_phrases": [
                        "вести хозяйство",
                        "заброшенный хутор"
                    ],
                    "target_lang_definitions": {
                        "en": "An isolated farm or estate with a plot of land and the owner's house."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "homestead",
                                "target_lang_sense_clarification": "a house with surrounding land and buildings"
                            },
                            {
                                "target_lang_word": "isolated farm",
                                "target_lang_sense_clarification": "a farm located away from a village or town"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05570650-6a3a-3ddf-9417-8448100f7b20",
                    "sense_definition": "Жители такого селения (собирательно).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Весь <w>хутор</w> собрался на главной площади, чтобы обсудить последние новости.",
                            "target_lang_translations": {
                                "en": "The whole <w>village</w> gathered in the main square to discuss the latest news."
                            }
                        },
                        {
                            "text": "Когда приехали артисты из города, <w>хутор</w> встретил их радостными криками.",
                            "target_lang_translations": {
                                "en": "When the artists arrived from the city, the <w>community</w> welcomed them with joyful shouts."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The inhabitants of such a settlement considered as a group."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "village",
                                "target_lang_sense_clarification": "referring to the people living in the settlement"
                            },
                            {
                                "target_lang_word": "community",
                                "target_lang_sense_clarification": "the collective group of residents"
                            }
                        ]
                    }
                }
            ]
        }
    ]

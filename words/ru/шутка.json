    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d1a05ecf-7de3-3bac-a1a0-4f2e16d06a7c",
                    "sense_definition": "Несерьёзное высказывание или действие, имеющее целью вызвать смех или развлечь.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "humor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Твоя новая <w>шутка</w> заставила всех громко смеяться.",
                            "target_lang_translations": {
                                "en": "Your new <w>joke</w> made everyone laugh loudly."
                            }
                        },
                        {
                            "text": "Мой брат слишком серьёзный и совсем не умеет понимать <w>шутки</w>.",
                            "target_lang_translations": {
                                "en": "My brother is too serious and doesn't know how to take <w>jokes</w> at all."
                            }
                        }
                    ],
                    "synonyms": [
                        "остро́та"
                    ],
                    "common_phrases": [
                        "злая шутка",
                        "глупая шутка",
                        "понимать шутки",
                        "кроме шуток",
                        "шутки в сторону"
                    ],
                    "target_lang_definitions": {
                        "en": "A non-serious statement or action intended to cause laughter or entertain."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "joke",
                                "target_lang_sense_clarification": "something said or done for amusement"
                            },
                            {
                                "target_lang_word": "jest",
                                "target_lang_sense_clarification": "a witty remark or act"
                            },
                            {
                                "target_lang_word": "prank",
                                "target_lang_sense_clarification": "a practical joke or mischievous act"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0fc522f-b11c-3f05-8700-d5910840b765",
                    "sense_definition": "Что-то незначительное, лёгкое, не требующее больших усилий (часто используется в отрицаниях для подчеркивания сложности).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "insignificance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Управлять такой огромной компанией — это вам не <w>шутка</w>.",
                            "target_lang_translations": {
                                "en": "Managing such a huge company is no <w>joke</w>."
                            }
                        },
                        {
                            "text": "<w>Шутка</w> ли, пройти пешком через весь лес за один день!",
                            "target_lang_translations": {
                                "en": "It's no <w>joke</w> to walk through the entire forest in one day!"
                            }
                        }
                    ],
                    "synonyms": [
                        "пустяк",
                        "безделица",
                        "ерунда"
                    ],
                    "common_phrases": [
                        "это не шутка",
                        "шутка ли"
                    ],
                    "target_lang_definitions": {
                        "en": "Something insignificant, easy, or not requiring much effort (often used in negations to emphasize difficulty)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "joke",
                                "target_lang_sense_clarification": "something trivial or very easy"
                            },
                            {
                                "target_lang_word": "laughing matter",
                                "target_lang_sense_clarification": "something not to be treated lightly (used in negations)"
                            },
                            {
                                "target_lang_word": "trifle",
                                "target_lang_sense_clarification": "a thing of little value or importance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e079779b-deec-35d8-aa9f-30e52c77201f",
                    "sense_definition": "Небольшая комическая пьеса или лёгкое, игривое музыкальное произведение.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "art",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Оркестр виртуозно исполнил известную музыкальную <w>шутку</w> Баха.",
                            "target_lang_translations": {
                                "en": "The orchestra virtuously performed Bach's famous musical <w>joke</w>."
                            }
                        },
                        {
                            "text": "В театре сегодня показывают старинную одноактную <w>шутку</w>.",
                            "target_lang_translations": {
                                "en": "Today the theater is showing an old one-act <w>joke</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "скерцо"
                    ],
                    "common_phrases": [
                        "музыкальная шутка"
                    ],
                    "target_lang_definitions": {
                        "en": "A small comic play or a light, playful musical piece."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scherzo",
                                "target_lang_sense_clarification": "a vigorous, light, or playful musical composition"
                            },
                            {
                                "target_lang_word": "joke",
                                "target_lang_sense_clarification": "a light or humorous artistic piece"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "b6683dd1-cabb-3038-a588-b38fb6fbebb0",
                    "sense_definition": "Возглас, который используется, чтобы показать, что сказанное ранее не следует воспринимать всерьёз.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "humor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я выиграл миллион в лотерею... <w>шутка</w>, к сожалению!",
                            "target_lang_translations": {
                                "en": "I won a million in the lottery... <w>just kidding</w>, unfortunately!"
                            }
                        },
                        {
                            "text": "Мы завтра переезжаем в другую страну, <w>шутка</w>, мы остаёмся здесь.",
                            "target_lang_translations": {
                                "en": "We are moving to another country tomorrow, <w>just kidding</w>, we are staying here."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation used to show that what was said earlier should not be taken seriously."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "just kidding",
                                "target_lang_sense_clarification": "phrase indicating the previous statement was not serious"
                            },
                            {
                                "target_lang_word": "joke",
                                "target_lang_sense_clarification": "short for 'it was a joke'"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
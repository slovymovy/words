    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ed715648-3531-3597-9946-4bdf5ffc4d03",
                    "sense_definition": "W sposób działający samoczynnie, za pomocą mechanizmu lub oprogramowania, bez bezpośredniego udziału człowieka.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "machine_operation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Brama wjazdowa otwiera się <w>automatycznie</w>, gdy czujnik wykryje samochód.",
                            "target_lang_translations": {
                                "ru": "Въездные ворота открываются <w>автоматически</w>, когда датчик обнаруживает автомобиль."
                            }
                        },
                        {
                            "text": "System operacyjny <w>automatycznie</w> pobiera i instaluje najnowsze aktualizacje bezpieczeństwa.",
                            "target_lang_translations": {
                                "ru": "Операционная система <w>автоматически</w> скачивает и устанавливает последние обновления безопасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "samoczynnie"
                    ],
                    "antonyms": [
                        "ręcznie"
                    ],
                    "common_phrases": [
                        "sterowany automatycznie",
                        "wyłączać się automatycznie",
                        "działać automatycznie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действуя самостоятельно, с помощью механизма или программного обеспечения, без прямого участия человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "автоматически",
                                "target_lang_sense_clarification": "о работе устройств или программ"
                            },
                            {
                                "target_lang_word": "самостоятельно",
                                "target_lang_sense_clarification": "без участия человека"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "398c6e4f-f554-3c53-9739-e467b8816c63",
                    "sense_definition": "W sposób nieświadomy, wykonany pod wpływem odruchu lub nawyku, bez głębszego zastanowienia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "human_reflex",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy usłyszałem swoje imię, <w>automatycznie</w> odwróciłem głowę w stronę głosu.",
                            "target_lang_translations": {
                                "ru": "Когда я услышал свое имя, я <w>машинально</w> повернул голову в сторону голоса."
                            }
                        },
                        {
                            "text": "Kierowca <w>automatycznie</w> nacisnął hamulec na widok czerwonego światła.",
                            "target_lang_translations": {
                                "ru": "Водитель <w>автоматически</w> нажал на тормоз при виде красного света."
                            }
                        }
                    ],
                    "synonyms": [
                        "odruchowo",
                        "bezwiednie",
                        "mechanicznie",
                        "machinalnie",
                        "mimowolnie"
                    ],
                    "common_phrases": [
                        "reagować automatycznie",
                        "odpowiedzieć automatycznie",
                        "robić coś automatycznie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неосознанно, под влиянием рефлекса или привычки, не задумываясь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "машинально",
                                "target_lang_sense_clarification": "по привычке, бессознательно"
                            },
                            {
                                "target_lang_word": "автоматически",
                                "target_lang_sense_clarification": "не задумываясь"
                            },
                            {
                                "target_lang_word": "рефлекторно",
                                "target_lang_sense_clarification": "как физическая реакция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26c59bdc-937c-3518-a2ea-bfe37cd4b968",
                    "sense_definition": "W sposób wynikający bezpośrednio z przepisów, logiki lub wcześniejszych zdarzeń, jako ich nieuchronne następstwo.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "logical_consequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po upływie terminu ważności umowa przedłuża się <w>automatycznie</w> na kolejny rok.",
                            "target_lang_translations": {
                                "ru": "После истечения срока действия договор продлевается <w>автоматически</w> на следующий год."
                            }
                        },
                        {
                            "text": "Zwycięstwo w tym meczu <w>automatycznie</w> zapewnia drużynie awans do finału.",
                            "target_lang_translations": {
                                "ru": "Победа в этом матче <w>автоматически</w> обеспечивает команде выход в финал."
                            }
                        }
                    ],
                    "synonyms": [
                        "z automatu",
                        "konsekwentnie"
                    ],
                    "common_phrases": [
                        "przechodzić automatycznie",
                        "przysługiwać automatycznie",
                        "oznaczać automatycznie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Как прямое следствие правил, логики или предыдущих событий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "автоматически",
                                "target_lang_sense_clarification": "как следствие"
                            },
                            {
                                "target_lang_word": "автоматом",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        }
    ]

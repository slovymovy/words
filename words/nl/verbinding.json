                {
                    "sense_id": "3fe295fc-6530-39f9-97e5-f81516b301e8",
                    "sense_definition": "Het contact via telefoon, internet of een ander communicatiemiddel.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "link_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sorry, de <w>verbinding</w> is erg slecht, ik hoor je niet goed.",
                            "target_lang_translations": {
                                "en": "Sorry, the <w>connection</w> is very bad, I can't hear you well.",
                                "ru": "Извини, <w>связь</w> очень плохая, я тебя плохо слышу."
                            }
                        },
                        {
                            "text": "Heb jij hier wel een goede <w>verbinding</w> met het internet?",
                            "target_lang_translations": {
                                "en": "Do you have a good internet <w>connection</w> here?",
                                "ru": "У тебя здесь хорошая <w>связь</w> с интернетом?"
                            }
                        }
                    ],
                    "synonyms": [
                        "lijn",
                        "connectie"
                    ],
                    "common_phrases": [
                        "slechte verbinding",
                        "verbinding verbreken",
                        "geen verbinding hebben"
                    ],
                    "target_lang_definitions": {

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7969c991-30b1-3cf3-86d2-a4227ff53140",
                    "sense_definition": "Iets vastmaken met een touw, koord of band",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>bond</w> de boot met een touw vast aan de paal.",
                            "target_lang_translations": {
                                "en": "He <w>tied</w> the boat to the pole with a rope.",
                                "ru": "Он <w>привязал</w> лодку веревкой к столбу."
                            }
                        },
                        {
                            "text": "Ik <w>bind</w> mijn veters altijd dubbel vast zodat ze niet losgaan.",
                            "target_lang_translations": {
                                "en": "I always <w>tie</w> my shoelaces double so they don't come loose.",
                                "ru": "Я всегда <w>завязываю</w> шнурки на двойной узел, чтобы они не развязались."
                            }
                        }
                    ],
                    "synonyms": [
                        "vastmaken",
                        "vastbinden",
                        "knopen"
                    ],
                    "antonyms": [
                        "losmaken",
                        "losbinden"
                    ],
                    "common_phrases": [
                        "vast binden",
                        "touw binden"
                    ],
                    "target_lang_definitions": {
                        "en": "To fasten something with a rope, cord, or band",
                        "ru": "Прикреплять что-либо с помощью веревки, шнура или ленты"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tie",
                                "target_lang_sense_clarification": "general fastening with a knot"
                            },
                            {
                                "target_lang_word": "bind",
                                "target_lang_sense_clarification": "to fasten tightly or restrain"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "привязывать",
                                "target_lang_sense_clarification": "крепить к чему-либо"
                            },
                            {
                                "target_lang_word": "связывать",
                                "target_lang_sense_clarification": "соединять вместе"
                            },
                            {
                                "target_lang_word": "завязывать",
                                "target_lang_sense_clarification": "делать узел"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40511ede-6128-3e80-8370-5dcba89cf1ee",
                    "sense_definition": "Iets om een ander voorwerp of lichaamsdeel heen wikkelen en vastmaken",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>bond</w> een sjaal om zijn nek tegen de kou.",
                            "target_lang_translations": {
                                "en": "He <w>tied</w> a scarf around his neck against the cold.",
                                "ru": "Он <w>повязал</w> шарф вокруг шеи, чтобы защититься от холода."
                            }
                        },
                        {
                            "text": "De verpleegkundige <w>bond</w> voorzichtig een verband om de zere arm.",
                            "target_lang_translations": {
                                "en": "The nurse carefully <w>bound</w> a bandage around the sore arm.",
                                "ru": "Медсестра осторожно <w>обвязала</w> бинт вокруг больной руки."
                            }
                        }
                    ],
                    "synonyms": [
                        "ombinden"
                    ],
                    "common_phrases": [
                        "om binden"
                    ],
                    "target_lang_definitions": {
                        "en": "To wrap something around another object or body part and fasten it",
                        "ru": "Обматывать и закреплять что-либо вокруг другого предмета или части тела"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tie",
                                "target_lang_sense_clarification": "to fasten around, e.g., a scarf"
                            },
                            {
                                "target_lang_word": "bind",
                                "target_lang_sense_clarification": "to wrap tightly, e.g., a bandage"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "повязывать",
                                "target_lang_sense_clarification": "надевать, завязывая концы (шарф, платок)"
                            },
                            {
                                "target_lang_word": "обвязывать",
                                "target_lang_sense_clarification": "обматывать вокруг чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29375b73-f24c-3184-8982-59466f6b7066",
                    "sense_definition": "Zorgen dat iemand verplicht is iets te doen of emotioneel verbonden is",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door het contract waren beide bedrijven <w>gebonden</w> aan de afspraken.",
                            "target_lang_translations": {
                                "en": "Due to the contract, both companies were <w>bound</w> to the agreements.",
                                "ru": "Из-за контракта обе компании были <w>связаны</w> договоренностями."
                            }
                        },
                        {
                            "text": "Zij probeerde haar zoon te veel aan zich te <w>binden</w>.",
                            "target_lang_translations": {
                                "en": "She tried to <w>bind</w> her son to her too much.",
                                "ru": "Она пыталась слишком сильно <w>привязать</w> сына к себе."
                            }
                        }
                    ],
                    "synonyms": [
                        "verplichten",
                        "verbinden"
                    ],
                    "antonyms": [
                        "vrijlaten"
                    ],
                    "common_phrases": [
                        "zich binden aan",
                        "gebonden zijn aan"
                    ],
                    "target_lang_definitions": {
                        "en": "To ensure someone is obliged to do something or is emotionally connected",
                        "ru": "Обязывать кого-либо сделать что-то или создавать эмоциональную связь"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bind",
                                "target_lang_sense_clarification": "to oblige legally or morally"
                            },
                            {
                                "target_lang_word": "tie",
                                "target_lang_sense_clarification": "to connect or link emotionally"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "связывать",
                                "target_lang_sense_clarification": "обязательствами или договором"
                            },
                            {
                                "target_lang_word": "привязывать",
                                "target_lang_sense_clarification": "эмоционально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "22b9a00d-6c23-3b97-9cb9-b59d99784a41",
                    "sense_definition": "Een vloeistof, zoals saus of soep, dikker maken",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "culinary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft de soep <w>gebonden</w> door er bloem aan toe te voegen.",
                            "target_lang_translations": {
                                "en": "He <w>thickened</w> the soup by adding flour.",
                                "ru": "Он <w>загустил</w> суп, добавив в него муку."
                            }
                        },
                        {
                            "text": "Je kunt maizena gebruiken om de jus mooi te <w>binden</w>.",
                            "target_lang_translations": {
                                "en": "You can use cornstarch to nicely <w>thicken</w> the gravy.",
                                "ru": "Вы можете использовать кукурузный крахмал, чтобы хорошо <w>загустить</w> подливку."
                            }
                        }
                    ],
                    "synonyms": [
                        "indikken"
                    ],
                    "antonyms": [
                        "verdunnen"
                    ],
                    "common_phrases": [
                        "saus binden",
                        "soep binden"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a liquid, such as sauce or soup, thicker",
                        "ru": "Делать жидкость, например соус или суп, более густой"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thicken",
                                "target_lang_sense_clarification": "to make more viscous"
                            },
                            {
                                "target_lang_word": "bind",
                                "target_lang_sense_clarification": "culinary term for combining ingredients"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "загущать",
                                "target_lang_sense_clarification": "кулинария"
                            }
                        ]
                    }
                }
            ]
        }
    ]

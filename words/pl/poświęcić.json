                    "sense_id": "e089cd0b-f1fc-327c-9274-227f80ee5a9d",
                    "sense_definition": "Przeznaczyć czas, uwagę lub środki na realizację określonego celu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "devote_resources",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <poświęcić> więcej czasu na naukę języka polskiego.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <посвятить> больше времени изучению польского языка."
                            }
                        },
                        {
                            "text": "Nauczyciel <poświęcił> uczniowi dużo uwagi, aby wytłumaczyć trudny temat.",
                            "target_lang_translations": {
                                "ru": "Учитель <уделил> ученику много внимания, чтобы объяснить трудную тему."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeznaczyć"
                    ],
                    "antonyms": [
                        "zmarnować",
                        "stracić"
                    ],
                    "common_phrases": [

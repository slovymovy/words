                {
                    "sense_id": "e4889cc3-652b-3c02-af89-da170a1b8279",
                    "sense_definition": "Substancja, surowiec lub tworzywo, z którego wytwarza się przedmioty lub buduje obiekty.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Drewno to naturalny <w>materiał</w>, z którego robi się meble.",
                            "target_lang_translations": {
                                "ru": "Дерево — это натуральный <w>материал</w>, из которого делают мебель."
                            }
                        },
                        {
                            "text": "Ten <w>materiał</w> budowlany jest bardzo wytrzymały na mróz.",
                            "target_lang_translations": {
                                "ru": "Этот строительный <w>материал</w> очень устойчив к морозу."
                            }
                        }
                    ],
                    "synonyms": [
                        "surowiec",
                        "tworzywo",
                        "substancja",
                        "budulec"
                    ],
                    "common_phrases": [
                        "materiał budowlany",
                        "wytrzymały materiał",
                        "materiał wybuchowy",
                        "materiał izolacyjny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещество, сырье или материал, из которого изготавливают предметы или строят объекты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "материал",
                                "target_lang_sense_clarification": "вещество"
                            },
                            {
                                "target_lang_word": "сырьё",
                                "target_lang_sense_clarification": "необработанный материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "641ec8fe-f00e-3b13-ac2c-ce2ac6f90af7",
                    "sense_definition": "Wyrób włókienniczy, z którego szyje się ubrania lub inne rzeczy; tkanina.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłam piękny <w>materiał</w> w kwiaty na letnią sukienkę.",
                            "target_lang_translations": {
                                "ru": "Я купила красивую <w>ткань</w> в цветочек на летнее платье."
                            }
                        },
                        {
                            "text": "Ten <w>materiał</w> jest bardzo delikatny i trzeba go prać ręcznie.",
                            "target_lang_translations": {
                                "ru": "Эта <w>ткань</w> очень деликатная, и её нужно стирать вручную."
                            }
                        }
                    ],
                    "synonyms": [
                        "tkanina",
                        "gw. (Górny Śląsk) sztof"
                    ],
                    "common_phrases": [
                        "materiał w kratkę",
                        "sztuczny materiał",
                        "uszyć z materiału",
                        "sklep z materiałami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Текстильное изделие, из которого шьют одежду или другие вещи; ткань."
                    },
                    "translations": {

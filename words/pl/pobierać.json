                {
                    "sense_id": "197523b6-bf3a-3dd1-af65-651e970da8d3",
                    "sense_definition": "Brać fragment materiału, płynu lub tkanki w celu przeprowadzenia badań laboratoryjnych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "medical_scientific",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pielęgniarka potrafi bezboleśnie <w>pobierać</w> krew nawet małym dzieciom.",
                            "target_lang_translations": {
                                "ru": "Медсестра умеет безболезненно <w>брать</w> кровь даже у маленьких детей."
                            }
                        },
                        {
                            "text": "Naukowcy pojechali na Antarktydę, aby <w>pobierać</w> próbki lodu do analizy.",
                            "target_lang_translations": {
                                "ru": "Ученые поехали в Антарктиду, чтобы <w>брать</w> образцы льда для анализа."
                            }
                        }
                    ],

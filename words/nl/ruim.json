{
    "word_family": [
        "ruimen",
        "ruimte",
        "ruimtelijk",
        "ruimschoots",
        "opruimen",
        "inruimen",
        "ontruimen"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a4387ced-34af-31c0-9952-8a90ec8d5e7a",
                    "sense_definition": "Groot van opzet; met veel beschikbare plaats om zich heen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij wonen in een <w>ruim</w> huis met een grote tuin.",
                            "target_lang_translations": {
                                "ru": "Они живут в <w>просторном</w> доме с большим садом."
                            }
                        },
                        {
                            "text": "Deze broek zit erg <w>ruim</w> rond mijn middel.",
                            "target_lang_translations": {
                                "ru": "Эти брюки сидят очень <w>свободно</w> на моей талии."
                            }
                        }
                    ],
                    "antonyms": [
                        "krap",
                        "nauw",
                        "eng"
                    ],
                    "common_phrases": [
                        "ruim huis",
                        "ruime kamer",
                        "ruim jasje"
                    ],
                    "target_lang_definitions": {
                        "ru": "Просторный, занимающий много места; (об одежде) свободный, не тесный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просторный",
                                "target_lang_sense_clarification": "о помещении или пространстве"
                            },
                            {
                                "target_lang_word": "свободный",
                                "target_lang_sense_clarification": "об одежде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a82f334-127a-3ced-a03f-cca8c5da8fe7",
                    "sense_definition": "Meer dan voldoende; in grote mate of hoeveelheid aanwezig.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar heeft een <w>ruime</w> ervaring in het onderwijs.",
                            "target_lang_translations": {
                                "ru": "У учителя <w>обширный</w> опыт работы в сфере образования."
                            }
                        },
                        {
                            "text": "Het voorstel werd met een <w>ruime</w> meerderheid aangenomen.",
                            "target_lang_translations": {
                                "ru": "Предложение было принято <w>значительным</w> большинством голосов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ruime meerderheid",
                        "ruime ervaring",
                        "ruim bemeten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обильный, значительный; имеющийся в большом количестве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обширный",
                                "target_lang_sense_clarification": "об опыте или знаниях"
                            },
                            {
                                "target_lang_word": "значительный",
                                "target_lang_sense_clarification": "о количестве или большинстве"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "d48b1ba0-1ddc-3b34-91de-4f1b828d4c01",
                    "sense_definition": "Iets meer dan een genoemde hoeveelheid, aantal of tijdstip.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quantity_adverb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het duurt <w>ruim</w> een uur om naar de stad te rijden.",
                            "target_lang_translations": {
                                "ru": "Поездка в город занимает <w>более</w> часа."
                            }
                        },
                        {
                            "text": "Er waren <w>ruim</w> honderd mensen aanwezig op het feest.",
                            "target_lang_translations": {
                                "ru": "На вечеринке присутствовало <w>более</w> ста человек."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ruim voldoende",
                        "ruim op tijd",
                        "ruim van tevoren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Немного больше указанного количества или времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "более",
                                "target_lang_sense_clarification": "больше чем"
                            },
                            {
                                "target_lang_word": "с лишним",
                                "target_lang_sense_clarification": "чуть больше"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1f28f858-7bbe-3633-86bb-14834512e967",
                    "sense_definition": "Vervoeging van <w>ruimen</w>: eerste persoon enkelvoud tegenwoordige tijd.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>ruim</w> de tafel af na het avondeten.",
                            "target_lang_translations": {
                                "ru": "Я <w>убираю</w> со стола после ужина."
                            }
                        },
                        {
                            "text": "Elke zaterdag <w>ruim</w> ik mijn kamer grondig op.",
                            "target_lang_translations": {
                                "ru": "Каждую субботу я тщательно <w>убираю</w> свою комнату."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма глагола <w>ruimen</w>: 1-е лицо единственного числа (я убираю)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убираю",
                                "target_lang_sense_clarification": "навожу порядок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8f7d567-fc63-3dfa-b887-7768711cfc8c",
                    "sense_definition": "Vervoeging van <w>ruimen</w>: gebiedende wijs.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Ruim</w> je speelgoed op voordat je naar bed gaat!",
                            "target_lang_translations": {
                                "ru": "<w>Убери</w> свои игрушки, прежде чем идти спать!"
                            }
                        },
                        {
                            "text": "<w>Ruim</w> die rommel onmiddellijk op.",
                            "target_lang_translations": {
                                "ru": "Немедленно <w>убери</w> этот беспорядок."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма глагола <w>ruimen</w>: повелительное наклонение (убери)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убери",
                                "target_lang_sense_clarification": "приказ навести порядок"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "16708a31-464a-3b54-a31e-f0504223643e",
                    "sense_definition": "De opslagruimte onder het dek van een schip of in een vliegtuig.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "cargo_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zware koffers werden in het <w>ruim</w> van het vliegtuig geladen.",
                            "target_lang_translations": {
                                "ru": "Тяжелые чемоданы погрузили в <w>грузовой отсек</w> самолета."
                            }
                        },
                        {
                            "text": "Er ontstond rookontwikkeling in het <w>ruim</w> van het vrachtschip.",
                            "target_lang_translations": {
                                "ru": "В <w>трюме</w> грузового судна возникло задымление."
                            }
                        }
                    ],
                    "synonyms": [
                        "scheepsruim"
                    ],
                    "common_phrases": [
                        "in het ruim",
                        "het ruim laden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Грузовое помещение на корабле или в самолете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трюм",
                                "target_lang_sense_clarification": "на корабле"
                            },
                            {
                                "target_lang_word": "грузовой отсек",
                                "target_lang_sense_clarification": "в самолете"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
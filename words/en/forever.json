                    "frequency": "Middle",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have not seen him in <w>forever</w>.",
                            "target_lang_translations": {
                                "ru": "Я не видел его <w>целую вечность</w>."
                            }
                        },
                        {
                            "text": "It took me <w>forever</w> to finish reading that book.",
                            "target_lang_translations": {
                                "ru": "У меня ушла <w>вечность</w> на то, чтобы дочитать эту книгу."
                            }
                        }
                    ],
                    "synonyms": [
                        "eternally"
                    ],
                    "common_phrases": [
                        "in forever",
                        "took forever"
                    ],
                    "traits": [

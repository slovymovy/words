    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "97fbad73-e102-30e6-9999-7737b3acb9eb",
                    "sense_definition": "Een levend organisme, zoals een mens of een dier.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Elk levend <w>wezen</w> heeft behoefte aan water en voedsel.",
                            "target_lang_translations": {
                                "ru": "Каждое живое <w>существо</w> нуждается в воде и еде."
                            }
                        },
                        {
                            "text": "In het donkere bos zag hij plotseling een vreemd <w>wezen</w> staan.",
                            "target_lang_translations": {
                                "ru": "В темном лесу он внезапно увидел странное <w>существо</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "schepsel",
                        "creatuur"
                    ],
                    "common_phrases": [
                        "menselijk wezen",
                        "levend wezen",
                        "vreemd wezen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Живой организм, такой как человек или животное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "существо",
                                "target_lang_sense_clarification": "живой организм"
                            },
                            {
                                "target_lang_word": "создание",
                                "target_lang_sense_clarification": "творение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f7695b7-f728-38ac-9bb4-862d5d6e862f",
                    "sense_definition": "De diepste kern of de ware aard van iets of iemand.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "essence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In <w>wezen</w> is hij een heel verlegen jongen, ook al doet hij stoer.",
                            "target_lang_translations": {
                                "ru": "В <w>сущности</w>, он очень застенчивый мальчик, хотя и ведет себя круто."
                            }
                        },
                        {
                            "text": "De kunstenaar probeerde het <w>wezen</w> van de natuur in zijn schilderij te vangen.",
                            "target_lang_translations": {
                                "ru": "Художник пытался запечатлеть <w>сущность</w> природы в своей картине."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in wezen",
                        "het diepste wezen",
                        "van wezen zijn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Глубинная суть или истинная природа чего-либо или кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сущность",
                                "target_lang_sense_clarification": "внутренняя природа"
                            },
                            {
                                "target_lang_word": "суть",
                                "target_lang_sense_clarification": "самое главное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c698adb-9bf9-30e1-9a91-f6a4f6ff27a3",
                    "sense_definition": "Een georganiseerd maatschappelijk stelsel of sector.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er zijn grote veranderingen nodig in het hele ambtelijk <w>wezen</w>.",
                            "target_lang_translations": {
                                "ru": "Необходимы большие перемены во всей бюрократической <w>системе</w>."
                            }
                        },
                        {
                            "text": "Het verenigingsleven is een belangrijk onderdeel van het maatschappelijk <w>wezen</w>.",
                            "target_lang_translations": {
                                "ru": "Клубная жизнь является важной частью общественного <w>устройства</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "maatschappelijk wezen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организованная общественная система или сектор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "система",
                                "target_lang_sense_clarification": "устройство, структура"
                            },
                            {
                                "target_lang_word": "сфера",
                                "target_lang_sense_clarification": "область деятельности"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "76af4549-348c-3842-8257-a75196ca62ea",
                    "sense_definition": "Een informele variant van het werkwoord <w>zijn</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "to_be",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je hoeft echt niet bang te <w>wezen</w> voor die kleine hond.",
                            "target_lang_translations": {
                                "ru": "Тебе правда не нужно <w>бояться</w> этой маленькой собаки."
                            }
                        },
                        {
                            "text": "Het kan soms lastig <w>wezen</w> om de juiste keuze te maken.",
                            "target_lang_translations": {
                                "ru": "Иногда может <w>быть</w> трудно сделать правильный выбор."
                            }
                        }
                    ],
                    "synonyms": [
                        "zijn"
                    ],
                    "common_phrases": [
                        "er wezen",
                        "weg wezen"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in speech instead of 'zijn'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорный вариант глагола 'zijn' (быть)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "быть",
                                "target_lang_sense_clarification": "существовать, находиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e9d62b9-3f81-392d-bc80-9479267d2282",
                    "sense_definition": "Hulpwerkwoord dat aangeeft dat men ergens anders is geweest om een handeling te verrichten en weer terug is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "auxiliary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij zijn gisteren gezellig in de stad <w>wezen</w> winkelen.",
                            "target_lang_translations": {
                                "ru": "Вчера мы приятно <w>ходили</w> по магазинам в городе."
                            }
                        },
                        {
                            "text": "Hij is even bij zijn zieke oma <w>wezen</w> kijken.",
                            "target_lang_translations": {
                                "ru": "Он ненадолго <w>заходил</w> проведать свою больную бабушку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wezen kijken",
                        "wezen sporten",
                        "wezen eten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вспомогательный глагол, указывающий на то, что кто-то ходил или ездил куда-то совершить действие и вернулся."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ходить",
                                "target_lang_sense_clarification": "сходить и вернуться"
                            },
                            {
                                "target_lang_word": "ездить",
                                "target_lang_sense_clarification": "съездить и вернуться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d49cd2c0-bd10-36b1-8614-90f12c352359",
                    "sense_definition": "De gebiedende wijs meervoud van <w>zijn</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "imperative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Wezen</w> jullie niet bang voor wat de toekomst brengt.",
                            "target_lang_translations": {
                                "ru": "Не <w>будьте</w> напуганы тем, что несет будущее."
                            }
                        },
                        {
                            "text": "<w>Wezen</w> allen welkom in ons bescheiden huis.",
                            "target_lang_translations": {
                                "ru": "<w>Будьте</w> все желанны в нашем скромном доме."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern Dutch usually uses 'wees' for both singular and plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение множественного числа глагола 'zijn'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "будьте",
                                "target_lang_sense_clarification": "форма повелительного наклонения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8dfaa555-2539-302d-a408-bd646a3ceea8",
                    "sense_definition": "De verleden tijd meervoud van <w>wijzen</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "point",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De borden langs de weg <w>wezen</w> ons de juiste richting.",
                            "target_lang_translations": {
                                "ru": "Знаки вдоль дороги <w>указывали</w> нам правильное направление."
                            }
                        },
                        {
                            "text": "Zij <w>wezen</w> allemaal naar de man die de taart had laten vallen.",
                            "target_lang_translations": {
                                "ru": "Они все <w>показывали</w> на мужчину, который уронил торт."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время множественного числа глагола 'wijzen' (указывать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "указывали",
                                "target_lang_sense_clarification": "показывали направление"
                            },
                            {
                                "target_lang_word": "показывали",
                                "target_lang_sense_clarification": "указывали пальцем"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [

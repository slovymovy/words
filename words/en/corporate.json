                    "synonyms": [
                        "soulless",
                        "sterile",
                        "impersonal",
                        "bland"
                    ],
                    "antonyms": [
                        "distinctive",
                        "unique"
                    ],
                    "common_phrases": [
                        "corporate look",
                        "corporate feel"
                    ],
                    "target_lang_definitions": {

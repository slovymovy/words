    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "7e016ad1-ba5e-3190-9095-74fa8191fd64",
                    "sense_definition": "Необычным образом, странно; иначе, чем обычно.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "unusual_manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В доме было <w>непривычно</w> тихо для этого времени суток.",
                            "target_lang_translations": {
                                "en": "It was <w>unusually</w> quiet in the house for this time of day."
                            }
                        },
                        {
                            "text": "Её голос звучал <w>непривычно</w> резко и громко.",
                            "target_lang_translations": {
                                "en": "Her voice sounded <w>unusually</w> harsh and loud."
                            }
                        }
                    ],
                    "synonyms": [
                        "необы́чно",
                        "необыкнове́нно"
                    ],
                    "antonyms": [
                        "привы́чно"
                    ],
                    "common_phrases": [
                        "непривычно тихо",
                        "звучать непривычно",
                        "выглядеть непривычно"
                    ],
                    "target_lang_definitions": {
                        "en": "In a manner different from what is usual or expected; strangely."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unusually",
                                "target_lang_sense_clarification": "indicating a higher degree or different manner than normal"
                            },
                            {
                                "target_lang_word": "strangely",
                                "target_lang_sense_clarification": "in a way that is surprising or odd"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9daa7885-74e3-365f-a02c-80690b86d734",
                    "sense_definition": "Обозначает состояние неловкости, новизны или отсутствие привычки, испытываемое кем-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "state_of_feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне <w>непривычно</w> просыпаться так рано утром.",
                            "target_lang_translations": {
                                "en": "It feels <w>strange</w> for me to wake up so early in the morning."
                            }
                        },
                        {
                            "text": "На новом месте работы ему было <w>непривычно</w> и одиноко.",
                            "target_lang_translations": {
                                "en": "At his new job, he felt <w>unfamiliar</w> and lonely."
                            }
                        }
                    ],
                    "synonyms": [
                        "необы́чно"
                    ],
                    "antonyms": [
                        "привы́чно"
                    ],
                    "common_phrases": [
                        "мне непривычно",
                        "было непривычно",
                        "с непривычки"
                    ],
                    "target_lang_definitions": {
                        "en": "Describes a feeling of strangeness, awkwardness, or lack of familiarity with a situation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "strange",
                                "target_lang_sense_clarification": "feeling unfamiliar or odd (e.g., it feels strange to me)"
                            },
                            {
                                "target_lang_word": "unusual",
                                "target_lang_sense_clarification": "not habitual or customary"
                            },
                            {
                                "target_lang_word": "unfamiliar",
                                "target_lang_sense_clarification": "lacking knowledge or experience of something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4f37c1e-128c-3266-b6f4-11c770435457",
                    "sense_definition": "Не имея навыка или привычки к чему-либо; неумело.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "lack_of_skill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Девушка <w>непривычно</w> держала в руках тяжёлый инструмент.",
                            "target_lang_translations": {
                                "en": "The girl held the heavy tool <w>awkwardly</w> in her hands."
                            }
                        },
                        {
                            "text": "Он <w>непривычно</w> поклонился гостям и быстро вышел.",
                            "target_lang_translations": {
                                "en": "He bowed <w>clumsily</w> to the guests and quickly left."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "In an awkward or unskillful manner due to a lack of practice or habit."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "awkwardly",
                                "target_lang_sense_clarification": "lacking grace or skill"
                            },
                            {
                                "target_lang_word": "clumsily",
                                "target_lang_sense_clarification": "without physical coordination or habit"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {

                    "synonyms": [
                        "kot domowy",
                        "dachowiec",
                        "sierściuch",
                        "futrzak"
                    ],
                    "common_phrases": [
                        "kupować kota w worku",
                        "odwracać kota ogonem",
                        "pierwsze koty za płoty",
                        "bawić się w kotka i myszkę",
                        "dostać kota"

                    "common_phrases": [
                        "ручная полка",
                        "полка огорода"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of removing weeds from a garden or field."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "weeding",
                                "target_lang_sense_clarification": "agriculture"
                            }

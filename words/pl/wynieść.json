    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "272b22dc-fd72-3ad0-93ca-349cee74d2a5",
                    "sense_definition": "Zabrać coś lub kogoś z wnętrza pomieszczenia lub obiektu na zewnątrz, trzymając to w rękach.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <wynieść> śmieci, zanim przyjadą goście.",
                            "target_lang_translations": {
                                "ru": "Ты должен <вынести> мусор до приезда гостей."
                            }
                        },
                        {
                            "text": "Ratownicy zdołali bezpiecznie <wynieść> rannych z płonącego budynku.",
                            "target_lang_translations": {
                                "ru": "Спасателям удалось благополучно <вынести> раненых из горящего здания."
                            }
                        }
                    ],
                    "antonyms": [
                        "przynieść"
                    ],
                    "common_phrases": [
                        "wynieść śmieci",
                        "wynieść meble",
                        "wynieść na noszach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переместить что-то или кого-то изнутри помещения или объекта наружу, держа в руках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вынести",
                                "target_lang_sense_clarification": "переместить наружу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5b19952-70e7-32b4-bdcc-5dcb707e4743",
                    "sense_definition": "Osiągnąć określoną wartość liczbową, sumę lub wysokość (o kosztach, wymiarach, wynikach).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "value_amount",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Całkowity koszt naprawy samochodu może <wynieść> nawet dwa tysiące złotych.",
                            "target_lang_translations": {
                                "ru": "Общая стоимость ремонта автомобиля может <составить> даже две тысячи злотых."
                            }
                        },
                        {
                            "text": "Przewidujemy, że zysk netto firmy w tym roku <wyniesie> milion euro.",
                            "target_lang_translations": {
                                "ru": "Мы прогнозируем, что чистая прибыль компании в этом году <составит> миллион евро."
                            }
                        }
                    ],
                    "common_phrases": [
                        "koszty wyniosą",
                        "suma wyniesie",
                        "wynieść łącznie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достигнуть определенного количества, суммы или уровня (о расходах, размерах, результатах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "составить",
                                "target_lang_sense_clarification": "о сумме или количестве"
                            },
                            {
                                "target_lang_word": "достигнуть",
                                "target_lang_sense_clarification": "о значении или уровне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90cf26c5-1e4c-3593-aa21-61e7c3515143",
                    "sense_definition": "Zyskać coś (np. wiedzę, doświadczenie, wrażenie) w wyniku przebywania gdzieś lub uczestniczenia w czymś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_acquisition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam nadzieję, że z tego trudnego projektu uda ci się <wynieść> cenną lekcję.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, что из этого трудного проекта тебе удастся <извлечь> ценный урок."
                            }
                        },
                        {
                            "text": "Ze szkoły należy <wynieść> nie tylko wiedzę książkową, ale i umiejętności społeczne.",
                            "target_lang_translations": {
                                "ru": "Из школы нужно <вынести> не только книжные знания, но и социальные навыки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wynieść lekcję",
                        "wynieść doświadczenie",
                        "wynieść korzyści"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получить или усвоить что-то (знания, опыт, впечатления) в результате пребывания где-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "извлечь",
                                "target_lang_sense_clarification": "урок или пользу"
                            },
                            {
                                "target_lang_word": "вынести",
                                "target_lang_sense_clarification": "опыт или впечатление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9b48685-4f02-3d07-8152-6349c1d44563",
                    "sense_definition": "Opuścić miejsce zamieszkania lub pobytu; wyprowadzić się (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po kłótni z właścicielem mieszkania postanowili natychmiast <wynieść> się z tego lokalu.",
                            "target_lang_translations": {
                                "ru": "После ссоры с владельцем квартиры они решили немедленно <съехать> из этого помещения."
                            }
                        },
                        {
                            "text": "Kiedy wreszcie zamierzasz <wynieść> się od rodziców i zamieszkać na swoim?",
                            "target_lang_translations": {
                                "ru": "Когда ты наконец собираешься <съехать> от родителей и начать жить самостоятельно?"
                            }
                        }
                    ],
                    "synonyms": [
                        "wyprowadzić się",
                        "zmyć się"
                    ],
                    "common_phrases": [
                        "wynieść się z domu",
                        "wynieść się z miasta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покинуть место жительства; переехать или уйти (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "съехать",
                                "target_lang_sense_clarification": "переехать из жилья"
                            },
                            {
                                "target_lang_word": "убраться",
                                "target_lang_sense_clarification": "разг. уйти прочь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d16383aa-83d8-3e09-aee8-c08997493e5d",
                    "sense_definition": "Przywłaszczyć sobie cudzą własność; ukraść coś.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "theft",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pracownik ochrony zauważył, jak ktoś próbuje <wynieść> drogi sprzęt ze sklepu.",
                            "target_lang_translations": {
                                "ru": "Охранник заметил, как кто-то пытается <вынести> дорогое оборудование из магазина."
                            }
                        },
                        {
                            "text": "Nie możesz tak po prostu <wynieść> firmowych narzędzi do prywatnego użytku.",
                            "target_lang_translations": {
                                "ru": "Ты не можешь так просто <вынести> инструменты компании для личного пользования."
                            }
                        }
                    ],
                    "synonyms": [
                        "ukraść",
                        "zwinąć",
                        "zwędzić"
                    ],
                    "common_phrases": [
                        "wynieść coś pod pazuchą",
                        "wynieść z zakładu"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal contexts regarding theft"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тайком забрать чужую собственность; украсть (часто с места работы или из магазина)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вынести",
                                "target_lang_sense_clarification": "украсть, выкрасть"
                            },
                            {
                                "target_lang_word": "стащить",
                                "target_lang_sense_clarification": "разг. украсть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5202ace-f930-3081-b6a7-be4d19c7f051",
                    "sense_definition": "Sprawić, że ktoś awansuje, zyska wyższą pozycję, znaczenie lub zostanie uhonorowany.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "elevation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego bohaterskie czyny mogą go <wynieść> na szczyty popularności w całym kraju.",
                            "target_lang_translations": {
                                "ru": "Его героические поступки могут <вознести> его на вершины популярности во всей стране."
                            }
                        },
                        {
                            "text": "Decyzja papieża pozwoliła <wynieść> zmarłego zakonnika na ołtarze.",
                            "target_lang_translations": {
                                "ru": "Решение Папы позволило <возвести> умершего монаха на алтарь (причислить к лику блаженных)."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wynieść na ołtarze",
                        "wynieść na tron",
                        "wynieść na szczyty"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "often used in literary or formal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Способствовать повышению статуса, положения или славы кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вознести",
                                "target_lang_sense_clarification": "прославить, поднять на вершину"
                            },
                            {
                                "target_lang_word": "возвести",
                                "target_lang_sense_clarification": "в ранг или статус"
                            }
                        ]
                    }
                }
            ]
        }
    ]

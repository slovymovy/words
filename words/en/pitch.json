                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поле",
                                "target_lang_sense_clarification": "спортивная площадка"
                            },
                            {
                                "target_lang_word": "площадка",
                                "target_lang_sense_clarification": "место для игры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c890b25-10b2-3a7f-a84c-854d0a1d9c8f",
                    "sense_definition": "The degree of highness or lowness of a tone or sound.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "sound_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her voice rose to a high <w>pitch</w> of excitement when she heard the news.",
                            "target_lang_translations": {
                                "ru": "Её голос поднялся до высокого <w>тона</w> от волнения, когда она услышала новости."
                            }
                        },
                        {
                            "text": "Dogs can hear sounds at a much higher <w>pitch</w> than humans can.",
                            "target_lang_translations": {
                                "ru": "Собаки могут слышать звуки гораздо большей <w>высоты</w>, чем люди."
                            }
                        }
                    ],
                    "common_phrases": [
                        "high pitch",
                        "low pitch",
                        "perfect pitch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Степень высоты тона или звука."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высота",
                                "target_lang_sense_clarification": "звука или голоса"
                            },
                            {
                                "target_lang_word": "тон",
                                "target_lang_sense_clarification": "звуковой оттенок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9f5b1c4-205e-319a-a23a-e08608a2a051",
                    "sense_definition": "A speech or act that attempts to persuade someone to buy or do something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "persuasion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The salesman made a convincing <w>pitch</w> for the new vacuum cleaner.",
                            "target_lang_translations": {
                                "ru": "Продавец сделал убедительную <w>презентацию</w> нового пылесоса."
                            }
                        },
                        {
                            "text": "We listened to their sales <w>pitch</w> but decided not to invest in the company.",
                            "target_lang_translations": {
                                "ru": "Мы выслушали их торговое <w>предложение</w>, но решили не инвестировать в компанию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sales pitch",
                        "make a pitch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Речь или действие, направленные на то, чтобы убедить кого-то купить что-то или сделать что-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "презентация",
                                "target_lang_sense_clarification": "товара или идеи"
                            },
                            {
                                "target_lang_word": "питч",
                                "target_lang_sense_clarification": "короткая презентация (бизнес-сленг)"
                            },
                            {
                                "target_lang_word": "рекламная речь",
                                "target_lang_sense_clarification": "убеждение покупателя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abb3f8bb-d416-35ee-af49-a95b88ec0dcc",
                    "sense_definition": "The act of throwing the ball towards the batter in baseball.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "baseball_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>pitch</w> was too high, and the batter let it pass.",
                            "target_lang_translations": {
                                "ru": "<w>Подача</w> была слишком высокой, и отбивающий пропустил её."
                            }
                        },
                        {
                            "text": "He threw a fast <w>pitch</w> that struck the player out immediately.",
                            "target_lang_translations": {
                                "ru": "Он сделал быструю <w>подачу</w>, которая тут же вывела игрока из игры."
                            }
                        }
                    ],
                    "synonyms": [
                        "throw",
                        "toss"
                    ],
                    "common_phrases": [
                        "wild pitch",
                        "first pitch"
                    ],
                    "target_lang_definitions": {

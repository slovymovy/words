    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "56ae816c-81ee-379c-bb5e-2fde2108e497",
                    "sense_definition": "Not connected to or supplied with electric power.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many remote villages in the mountains remain <w>unelectrified</w> due to the difficult terrain.",
                            "target_lang_translations": {
                                "ru": "Многие отдаленные деревни в горах остаются <w>неэлектрифицированными</w> из-за сложного рельефа."
                            }
                        },
                        {
                            "text": "The band decided to play an <w>unelectrified</w> set using only acoustic instruments.",
                            "target_lang_translations": {
                                "ru": "Группа решила сыграть <w>акустический</w> сет, используя только акустические инструменты."
                            }
                        }
                    ],
                    "synonyms": [
                        "non-electrified",
                        "nonelectrified"
                    ],
                    "antonyms": [
                        "electrified"
                    ],
                    "common_phrases": [
                        "unelectrified railway",
                        "unelectrified village",
                        "unelectrified fence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не подключенный к электросети или не снабжаемый электроэнергией."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неэлектрифицированный",
                                "target_lang_sense_clarification": "о населенных пунктах, железных дорогах или оборудовании"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "65dcf0d8-eefb-3166-bcc7-73480fc0e6ed",
                    "sense_definition": "The simple past and past participle of <w>unelectrify</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The engineers <w>unelectrified</w> the section of the track before starting the repairs.",
                            "target_lang_translations": {
                                "ru": "Инженеры <w>обесточили</w> участок пути перед началом ремонтных работ."
                            }
                        },
                        {
                            "text": "Once the device was <w>unelectrified</w>, it was safe to touch the internal components.",
                            "target_lang_translations": {
                                "ru": "Как только устройство было <w>обесточено</w>, прикасаться к внутренним компонентам стало безопасно."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола unelectrify (лишать электричества, обесточивать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обесточенный",
                                "target_lang_sense_clarification": "отключенный от источника питания (причастие)"
                            },
                            {
                                "target_lang_word": "обесточил",
                                "target_lang_sense_clarification": "отключил электричество (глагол)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0ee5c156-b1b2-3322-bd92-fe7f36d2a3a2",
                    "sense_definition": "Część zdania, która informuje o czynności, stanie lub cechach podmiotu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym prostym zdaniu <w>orzeczenie</w> znajduje się na samym końcu.",
                            "target_lang_translations": {
                                "ru": "В этом простом предложении <w>сказуемое</w> находится в самом конце."
                            }
                        },
                        {
                            "text": "Nauczyciel poprosił uczniów, aby podkreślili podmiot i <w>orzeczenie</w>.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил учеников подчеркнуть подлежащее и <w>сказуемое</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "predykat"
                    ],
                    "common_phrases": [
                        "podmiot i orzeczenie",
                        "związek zgody z orzeczeniem",
                        "orzeczenie imienne",
                        "orzeczenie czasownikowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Главный член предложения, который сообщает о действии, состоянии или признаках подлежащего."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сказуемое",
                                "target_lang_sense_clarification": "грамматика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be10deb2-f060-3430-a297-6f069232504a",
                    "sense_definition": "Oficjalna decyzja wydana przez sąd lub inny organ państwowy, rozstrzygająca konkretną sprawę.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "legal_decision",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd wydał <w>orzeczenie</w> korzystne dla powoda.",
                            "target_lang_translations": {
                                "ru": "Суд вынес <w>решение</w> в пользу истца."
                            }
                        },
                        {
                            "text": "To <w>orzeczenie</w> jest prawomocne i nie przysługuje od niego odwołanie.",
                            "target_lang_translations": {
                                "ru": "Это <w>решение</w> вступило в законную силу и не подлежит обжалованию."
                            }
                        }
                    ],
                    "synonyms": [
                        "werdykt",
                        "rozstrzygnięcie",
                        "decyzja",
                        "osąd",
                        "opinia"
                    ],
                    "common_phrases": [
                        "wydać orzeczenie",
                        "prawomocne orzeczenie",
                        "orzeczenie sądu",
                        "uchylić orzeczenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное решение, принятое судом или иным государственным органом по конкретному делу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "решение",
                                "target_lang_sense_clarification": "суда"
                            },
                            {
                                "target_lang_word": "постановление",
                                "target_lang_sense_clarification": "официальный акт"
                            },
                            {
                                "target_lang_word": "приговор",
                                "target_lang_sense_clarification": "по уголовному делу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a8d97cc-0014-31c9-82e5-a1e990d67107",
                    "sense_definition": "Dokument wydany przez specjalistę lub instytucję, potwierdzający stan zdrowia, stan techniczny lub inne fakty.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "expert_opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do podjęcia pracy potrzebne jest aktualne <w>orzeczenie</w> lekarskie.",
                            "target_lang_translations": {
                                "ru": "Для трудоустройства необходимо актуальное медицинское <w>заключение</w>."
                            }
                        },
                        {
                            "text": "Komisja wydała <w>orzeczenie</w> o stopniu niepełnosprawności pacjenta.",
                            "target_lang_translations": {
                                "ru": "Комиссия выдала <w>заключение</w> о степени инвалидности пациента."
                            }
                        }
                    ],
                    "synonyms": [
                        "ekspertyza",
                        "opinia",
                        "decyzja"
                    ],
                    "common_phrases": [
                        "orzeczenie lekarskie",
                        "orzeczenie o niepełnosprawności",
                        "orzeczenie techniczne",
                        "uzyskać orzeczenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Документ, выданный специалистом или учреждением, подтверждающий состояние здоровья, техническое состояние или иные факты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заключение",
                                "target_lang_sense_clarification": "экспертное или медицинское"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "860da2d9-f2f9-3c9c-b3d5-05674a92cb2f",
                    "sense_definition": "Rzeczownik odczasownikowy od <w>orzec</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Samo <w>orzeczenie</w> winy zajęło sędziemu zaledwie kilka minut.",
                            "target_lang_translations": {
                                "ru": "Само <w>вынесение</w> решения о виновности заняло у судьи всего несколько минут."
                            }
                        },
                        {
                            "text": "Czekamy na oficjalne <w>orzeczenie</w> werdyktu przez przewodniczącego komisji.",
                            "target_lang_translations": {
                                "ru": "Мы ждем официального <w>оглашения</w> вердикта председателем комиссии."
                            }
                        }
                    ],

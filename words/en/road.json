    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8c41f4af-c8cd-3bea-a004-73de7db83b8c",
                    "sense_definition": "A hard surface built for vehicles, people, and animals to travel on between places.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_way",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Be careful when you cross the <w>road</w> during rush hour.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен, когда переходишь <w>дорогу</w> в час пик."
                            }
                        },
                        {
                            "text": "We drove along a narrow country <w>road</w> surrounded by fields.",
                            "target_lang_translations": {
                                "ru": "Мы ехали по узкой проселочной <w>дороге</w>, окруженной полями."
                            }
                        }
                    ],
                    "synonyms": [
                        "street"
                    ],
                    "common_phrases": [
                        "main road",
                        "country road",
                        "cross the road",
                        "down the road",
                        "dirt road"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердое покрытие, построенное для передвижения транспортных средств, людей и животных между пунктами назначения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дорога",
                                "target_lang_sense_clarification": "полоса земли для передвижения"
                            },
                            {
                                "target_lang_word": "шоссе",
                                "target_lang_sense_clarification": "дорога с твердым покрытием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6599bc8d-37b9-343e-a1f8-89c01e8f7a84",
                    "sense_definition": "The use of roads for travelling, especially by car, truck, or bus, rather than by other means.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "travel_method",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is often cheaper to transport heavy goods by <w>road</w> than by air.",
                            "target_lang_translations": {
                                "ru": "Часто дешевле перевозить тяжелые грузы по <w>дороге</w> (автотранспортом), чем по воздуху."
                            }
                        },
                        {
                            "text": "The band has been on the <w>road</w> for three months touring the country.",
                            "target_lang_translations": {
                                "ru": "Группа находится в <w>дороге</w> (в разъездах) уже три месяца, гастролируя по стране."
                            }
                        }
                    ],
                    "common_phrases": [
                        "by road",
                        "on the road"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использование дорог для путешествий, особенно на автомобиле, грузовике или автобусе (в отличие от других видов транспорта)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дорога",
                                "target_lang_sense_clarification": "путешествие, поездка"
                            },
                            {
                                "target_lang_word": "автотранспорт",
                                "target_lang_sense_clarification": "перевозка по автомобильным дорогам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d230542-e308-38ae-971e-dd4dfe89e87e",
                    "sense_definition": "A path or course of action taken in life or to achieve a specific goal.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_way",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hard work and dedication are the only sure <w>road</w> to success.",
                            "target_lang_translations": {
                                "ru": "Упорный труд и самоотверженность — единственный верный <w>путь</w> к успеху."
                            }
                        },
                        {
                            "text": "The country is finally on the <w>road</w> to recovery after the economic crisis.",
                            "target_lang_translations": {
                                "ru": "Страна наконец-то на <w>пути</w> к восстановлению после экономического кризиса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "road to success",
                        "road to recovery",
                        "end of the road"
                    ],
                    "target_lang_definitions": {
                        "ru": "Путь или курс действий в жизни для достижения определенной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путь",
                                "target_lang_sense_clarification": "направление деятельности, развитие чего-либо"
                            },
                            {
                                "target_lang_word": "дорога",
                                "target_lang_sense_clarification": "направление, путь следования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "971ff44b-a459-3d8f-a98b-02a0ec7070b8",
                    "sense_definition": "A railroad or a single railway track.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "railroad",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old <w>road</w> was built in the nineteenth century to transport coal.",
                            "target_lang_translations": {
                                "ru": "Старая железная <w>дорога</w> была построена в девятнадцатом веке для перевозки угля."
                            }
                        },
                        {
                            "text": "Workers spent the day repairing the damaged <w>road</w> after the storm.",
                            "target_lang_translations": {
                                "ru": "Рабочие провели день, ремонтируя поврежденный <w>путь</w> после шторма."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "US"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "railroad industry context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Железная дорога или отдельный железнодорожный путь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "железная дорога",
                                "target_lang_sense_clarification": "система железнодорожного сообщения"
                            },
                            {
                                "target_lang_word": "путь",
                                "target_lang_sense_clarification": "железнодорожная колея"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7c88fad-ba5a-3141-824d-233ef37134fa",
                    "sense_definition": "A partly sheltered area of water near a shore where ships can safely anchor.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "nautical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The captain decided to anchor in the <w>road</w> until the storm passed.",
                            "target_lang_translations": {
                                "ru": "Капитан решил встать на якорь на <w>рейде</w>, пока не пройдет шторм."
                            }
                        },
                        {
                            "text": "Several merchant ships were waiting in the <w>road</w> for permission to dock.",
                            "target_lang_translations": {
                                "ru": "Несколько торговых судов ожидали на <w>рейде</w> разрешения на швартовку."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "nautical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Частично защищенная акватория у берега, где корабли могут безопасно стоять на якоре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рейд",
                                "target_lang_sense_clarification": "место для якорной стоянки судов"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "801d228f-e4e8-3f9a-8071-cc5ddae8aa8c",
                    "sense_definition": "Taking place at the venue of the opposing team or competitor.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team has struggled to win any <w>road</w> games this season.",
                            "target_lang_translations": {
                                "ru": "В этом сезоне команда с трудом выигрывает <w>выездные</w> матчи."
                            }
                        },
                        {
                            "text": "They are preparing for a long <w>road</w> trip to play three different teams.",
                            "target_lang_translations": {
                                "ru": "Они готовятся к долгому <w>выездному</w> турне, чтобы сыграть с тремя разными командами."
                            }
                        }
                    ],
                    "synonyms": [
                        "away"
                    ],
                    "common_phrases": [
                        "road game",
                        "road trip"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Проходящий на площадке соперника (о спортивных соревнованиях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выездной",
                                "target_lang_sense_clarification": "гостевой матч или игра"
                            },
                            {
                                "target_lang_word": "гостевой",
                                "target_lang_sense_clarification": "на чужом поле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68fb1071-6d64-33f5-b0f0-4a1320d004ab",
                    "sense_definition": "Relating to bicycles designed for use on paved surfaces rather than rough terrain.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "cycling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He bought a new <w>road</w> bike to train for the upcoming race.",
                            "target_lang_translations": {
                                "ru": "Он купил новый <w>шоссейный</w> велосипед, чтобы тренироваться к предстоящей гонке."
                            }
                        },
                        {
                            "text": "You need smooth <w>road</w> tires to go fast on the asphalt.",
                            "target_lang_translations": {
                                "ru": "Вам нужны гладкие <w>шоссейные</w> шины, чтобы быстро ехать по асфальту."
                            }
                        }
                    ],
                    "common_phrases": [
                        "road bike",
                        "road race"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к велосипедам, предназначенным для езды по асфальтированным поверхностям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шоссейный",
                                "target_lang_sense_clarification": "предназначенный для скоростной езды по асфальту"
                            },
                            {
                                "target_lang_word": "дорожный",
                                "target_lang_sense_clarification": "предназначенный для дорог"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
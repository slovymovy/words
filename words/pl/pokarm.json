                {
                    "sense_id": "c61520f6-b4f8-349b-86c4-eba4d942c903",
                    "sense_definition": "Substancja, którą organizmy żywe jedzą i przyswajają, aby uzyskać energię oraz składniki potrzebne do życia i wzrostu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "food_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W trudnych warunkach zimowych zwierzęta mają problem ze zdobyciem <w>pokarmu</w>.",
                            "target_lang_translations": {
                                "ru": "В суровых зимних условиях у животных возникают проблемы с добычей <w>пищи</w>."
                            }
                        },
                        {
                            "text": "Ten <w>pokarm</w> jest bogaty w białko i niezbędne witaminy.",
                            "target_lang_translations": {
                                "ru": "Эта <w>пища</w> богата белком и необходимыми витаминами."
                            }
                        }
                    ],
                    "synonyms": [
                        "pożywienie",
                        "jedzenie",
                        "żywność"
                    ],
                    "common_phrases": [
                        "zdobywać pokarm",
                        "przyjmować pokarm",
                        "zdrowy pokarm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вещество, которое живые организмы едят и усваивают для получения энергии, а также компонентов, необходимых для жизни и роста."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пища",
                                "target_lang_sense_clarification": "общее понятие"
                            },
                            {
                                "target_lang_word": "питание",
                                "target_lang_sense_clarification": "процесс или рацион"
                            },
                            {
                                "target_lang_word": "пропитание",
                                "target_lang_sense_clarification": "средства к существованию, еда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "572f5db4-96f5-3dcf-bd24-e681ade5f8f9",
                    "sense_definition": "Gotowe pożywienie przygotowane specjalnie dla zwierząt hodowlanych lub domowych.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "food_animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę iść do sklepu zoologicznego, aby kupić <w>pokarm</w> dla rybek.",
                            "target_lang_translations": {
                                "ru": "Мне нужно пойти в зоомагазин, чтобы купить <w>корм</w> для рыбок."
                            }
                        },
                        {
                            "text": "Mój pies nie chce jeść tego suchego <w>pokarmu</w>.",
                            "target_lang_translations": {
                                "ru": "Моя собака не хочет есть этот сухой <w>корм</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "karma",
                        "pasza"
                    ],
                    "common_phrases": [

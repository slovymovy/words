            "senses": [
                {
                    "sense_id": "7ed8293f-93b3-3772-8a5e-0dab28d528ab",
                    "sense_definition": "Провести некоторое время, находясь где-либо или присутствуя в каком-то месте.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <побыть> в деревне ещё пару дней, чтобы отдохнуть.",
                            "target_lang_translations": {
                                "en": "We decided to <stay> in the village for a couple more days to rest."
                            }
                        },
                        {
                            "text": "Ты можешь <побыть> здесь, пока я быстро схожу в магазин?",
                            "target_lang_translations": {
                                "en": "Can you <stay> here while I quickly go to the store?"
                            }
                        }
                    ],
                    "synonyms": [
                        "пробыть"
                    ],
                    "common_phrases": [
                        "побыть дома",
                        "побыть в гостях",
                        "побыть с семьёй",
                        "побыть на море"
                    ],
                    "target_lang_definitions": {
                        "en": "To spend some time being somewhere or present in a place."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stay",
                                "target_lang_sense_clarification": "remain in a place for a short time"
                            },
                            {
                                "target_lang_word": "spend some time",
                                "target_lang_sense_clarification": "at a location"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "401756e7-8f6f-3d63-815f-0fdd5f1a2823",
                    "sense_definition": "Провести некоторое время в каком-либо душевном или физическом состоянии, а также в какой-либо роли.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После тяжёлого дня мне просто нужно <побыть> одному и подумать.",
                            "target_lang_translations": {
                                "en": "After a hard day, I just need to <be> alone and think."
                            }
                        },
                        {
                            "text": "Иногда очень полезно <побыть> ребёнком и забыть о взрослых проблемах.",
                            "target_lang_translations": {
                                "en": "Sometimes it is very useful to <be> a child and forget about adult problems."
                            }
                        }
                    ],
                    "common_phrases": [
                        "побыть одному",
                        "побыть в тишине",
                        "побыть собой"
                    ],
                    "target_lang_definitions": {
                        "en": "To spend some time in a certain mental or physical state, or in a certain role."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be",
                                "target_lang_sense_clarification": "in a state or role for a while"
                            },
                            {
                                "target_lang_word": "stay",
                                "target_lang_sense_clarification": "remain in a condition"
                            }
                        ]
                    }
                }
            ]
        }

                {
                    "sense_id": "d90edf53-038f-3a88-b88c-d025c607c706",
                    "sense_definition": "Een vogel uit de familie Columbidae, vaak grijs of wit, die een koerend geluid maakt en veel in steden voorkomt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "bird",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De witte <w>duif</w> is wereldwijd een bekend symbool van vrede.",
                            "target_lang_translations": {
                                "en": "The white <w>dove</w> is a well-known symbol of peace worldwide.",
                                "ru": "Белый <w>голубь</w> — это всемирно известный символ мира."
                            }
                        },
                        {
                            "text": "Er zat een <w>duif</w> op de rand van het balkon te koeren.",
                            "target_lang_translations": {
                                "en": "There was a <w>pigeon</w> cooing on the edge of the balcony.",
                                "ru": "На краю балкона сидел <w>голубь</w> и ворковал."
                            }
                        }
                    ],
                    "common_phrases": [
                        "witte duif",
                        "tamme duif",
                        "grijze duif"
                    ],
                    "target_lang_definitions": {
                        "en": "A bird of the family Columbidae, often grey or white, that makes a cooing sound and is common in cities.",
                        "ru": "Птица из семейства голубиных, часто серая или белая, издающая воркующие звуки и широко распространенная в городах."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pigeon",
                                "target_lang_sense_clarification": "general term, especially for the common grey city bird"
                            },
                            {
                                "target_lang_word": "dove",
                                "target_lang_sense_clarification": "often used for white varieties or smaller species"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "голубь",
                                "target_lang_sense_clarification": "птица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17c2e05e-af1c-3d12-b8c3-548ff9880051",
                    "sense_definition": "Een vrouwelijke duif.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "bird",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kweker zette de <w>duif</w> apart zodat ze rustig haar eieren kon leggen.",
                            "target_lang_translations": {
                                "en": "The breeder set the <w>hen</w> apart so she could lay her eggs in peace.",
                                "ru": "Заводчик отсадил <w>голубку</w> отдельно, чтобы она могла спокойно отложить яйца."
                            }
                        },
                        {
                            "text": "Je kunt aan het gedrag zien of het een doffer of een <w>duif</w> is.",
                            "target_lang_translations": {
                                "en": "You can tell from the behavior whether it is a cock or a <w>hen</w>.",
                                "ru": "По поведению можно понять, самец это или <w>голубка</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "duivin",
                        "vrouwtjesduif"
                    ],
                    "antonyms": [
                        "doffer",
                        "duiver",
                        "mannetjesduif"
                    ],
                    "target_lang_definitions": {

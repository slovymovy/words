                    "common_phrases": [
                        "forensic society",
                        "forensic competition"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к искусству формальных дебатов или публичной аргументации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "риторический",
                                "target_lang_sense_clarification": "относящийся к ораторскому искусству"
                            },
                            {
                                "target_lang_word": "дискуссионный",

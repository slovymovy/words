{
    "word_family": [
        "trek",
        "trekken",
        "aantrekkelijk",
        "trekker",
        "aftrek",
        "vertrek"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fd51708c-2faa-323d-a200-603b7a16af70",
                    "sense_definition": "Een keer inhaleren van rook, meestal bij het roken van een sigaret.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "smoking_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij nam een diep <w>trekje</w> van zijn sigaret en blies de rook uit.",
                            "target_lang_translations": {
                                "en": "He took a deep <w>drag</w> on his cigarette and blew out the smoke.",
                                "ru": "Он сделал глубокую <w>затяжку</w> сигаретой и выдохнул дым."
                            }
                        },
                        {
                            "text": "Mag ik misschien een <w>trekje</w> van jouw sigaar proberen?",
                            "target_lang_translations": {
                                "en": "May I perhaps try a <w>puff</w> of your cigar?",
                                "ru": "Можно мне сделать одну <w>затяжку</w> твоей сигарой?"
                            }
                        }
                    ],
                    "synonyms": [
                        "haal",
                        "hijs"
                    ],
                    "common_phrases": [
                        "een trekje nemen",
                        "een trekje van een sigaret"
                    ],
                    "target_lang_definitions": {
                        "en": "An act of inhaling smoke, usually when smoking a cigarette.",
                        "ru": "Однократное вдыхание дыма, обычно при курении сигареты."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "puff",
                                "target_lang_sense_clarification": "short inhalation of smoke"
                            },
                            {
                                "target_lang_word": "drag",
                                "target_lang_sense_clarification": "deeper inhalation of smoke"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "затяжка",
                                "target_lang_sense_clarification": "вдох дыма при курении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe0b398b-2c5a-3aeb-abe1-d2172bc54b30",
                    "sense_definition": "Een kenmerkende eigenschap in iemands gedrag of uiterlijk, vaak lijkend op dat van een familielid.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "character_trait",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij heeft precies dezelfde <w>trekjes</w> als haar moeder als ze lacht.",
                            "target_lang_translations": {
                                "en": "She has exactly the same <w>mannerisms</w> as her mother when she laughs.",
                                "ru": "У неё точно такие же <w>черточки</w>, как у матери, когда она смеётся."
                            }
                        },
                        {
                            "text": "Dat ongeduldige gedrag is een vervelend <w>trekje</w> van hem.",
                            "target_lang_translations": {
                                "en": "That impatient behavior is an annoying <w>trait</w> of his.",
                                "ru": "Это нетерпеливое поведение — его неприятная <w>черта</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "eigenschap",
                        "kenmerk",
                        "karaktertrek"
                    ],
                    "common_phrases": [
                        "trekjes vertonen",
                        "trekjes van iemand hebben"
                    ],
                    "target_lang_definitions": {
                        "en": "A characteristic feature in someone's behavior or appearance, often resembling that of a family member.",
                        "ru": "Характерная особенность поведения или внешности, часто напоминающая родственника."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trait",
                                "target_lang_sense_clarification": "characteristic feature"
                            },
                            {
                                "target_lang_word": "mannerism",
                                "target_lang_sense_clarification": "distinctive behavioral quirk"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "черта",
                                "target_lang_sense_clarification": "особенность характера или внешности"
                            },
                            {
                                "target_lang_word": "черточка",
                                "target_lang_sense_clarification": "небольшая особенность (уменьш.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "493ec93e-dd34-3c2c-a7ce-2ab4fda3708b",
                    "sense_definition": "Verkleinwoord van <w>trek</w>; een korte, kleine beweging waarbij men iets naar zich toe haalt.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met een voorzichtig <w>trekje</w> aan het touw controleerde hij of de knoop vastzat.",
                            "target_lang_translations": {
                                "en": "With a careful <w>tug</w> on the rope, he checked if the knot was tight.",
                                "ru": "Осторожным <w>рывком</w> за веревку он проверил, держит ли узел."
                            }
                        },
                        {
                            "text": "Geef even een <w>trekje</w> aan de deur, want hij klemt een beetje.",
                            "target_lang_translations": {
                                "en": "Just give the door a <w>tug</w>, because it sticks a little.",
                                "ru": "Сделай небольшой <w>рывок</w> дверью, так как её немного заело."
                            }
                        }
                    ],
                    "synonyms": [
                        "rukje"
                    ],
                    "antonyms": [
                        "duwtje"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Diminutive of 'pull'; a short, small movement of pulling something towards oneself.",
                        "ru": "Короткое, легкое движение, при котором что-то тянут к себе; уменьшительное от слова «тяга»."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tug",
                                "target_lang_sense_clarification": "short, sharp pull"
                            },
                            {
                                "target_lang_word": "little pull",
                                "target_lang_sense_clarification": "literal translation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "рывок",
                                "target_lang_sense_clarification": "резкое движение на себя"
                            },
                            {
                                "target_lang_word": "подергивание",
                                "target_lang_sense_clarification": "легкое тянущее движение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
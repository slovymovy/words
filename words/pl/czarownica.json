            "senses": [
                {
                    "sense_id": "a3213211-6a0a-3897-8c17-d4bd33b36341",
                    "sense_definition": "W wierzeniach ludowych: kobieta umiejąca czarować, rzucać czary, zaklęcia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rzuciwszy urok <w>czarownica</w> rechocząc odleciała na miotle.",
                            "target_lang_translations": {
                                "ru": "Наложив заклятие, <w>ведьма</w>, хохоча, улетела на метле."
                            }
                        },
                        {
                            "text": "Najkrótsza noc w roku to tradycyjne święto <w>czarownic</w>.",
                            "target_lang_translations": {
                                "ru": "Самая короткая ночь в году — это традиционный праздник <w>ведьм</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wiedźma",
                        "baba jaga"
                    ],
                    "common_phrases": [
                        "sabat czarownic",
                        "polowanie na czarownice",
                        "latać na miotle"
                    ],
                    "target_lang_definitions": {
                        "ru": "В народных поверьях: женщина, умеющая колдовать, накладывать чары и заклятия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ведьма",
                                "target_lang_sense_clarification": "мифологическое существо"
                            },
                            {
                                "target_lang_word": "колдунья",
                                "target_lang_sense_clarification": "женщина, занимающаяся магией"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bdd7f1e2-32bc-3721-b204-78e67238e916",
                    "sense_definition": "Kobieta stara, brzydka, złośliwa.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta wredna <w>czarownica</w> znowu zrobiła nam awanturę o hałas.",
                            "target_lang_translations": {
                                "ru": "Эта злобная <w>ведьма</w> снова устроила нам скандал из-за шума."
                            }
                        },
                        {
                            "text": "Nikt nie lubi tej starej <w>czarownicy</w> z parteru, bo na wszystkich krzyczy.",
                            "target_lang_translations": {
                                "ru": "Никто не любит эту старую <w>ведьму</w> с первого этажа, потому что она на всех кричит."
                            }
                        }
                    ],
                    "synonyms": [
                        "jędza",
                        "zołza",
                        "heksa",
                        "brzydula"
                    ],
                    "common_phrases": [
                        "stara czarownica"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старая, некрасивая, злобная женщина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ведьма",
                                "target_lang_sense_clarification": "злая, сварливая женщина"
                            },
                            {
                                "target_lang_word": "карга",
                                "target_lang_sense_clarification": "старая, безобразная женщина"
                            }
                        ]
                    }
                }
            ]

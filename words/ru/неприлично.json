    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "47ca94b7-3efb-3b40-a36c-9ff428f81f66",
                    "sense_definition": "Оценочная характеристика ситуации или чьих-либо действий как предосудительных, не соответствующих правилам приличия.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Перебивать старших во время разговора — это <неприлично>.",
                            "target_lang_translations": {
                                "en": "Interrupting elders during a conversation is <w>impolite</w>."
                            }
                        },
                        {
                            "text": "Было бы <неприлично> уйти с праздника, не попрощавшись с хозяевами.",
                            "target_lang_translations": {
                                "en": "It would be <w>rude</w> to leave the party without saying goodbye to the hosts."
                            }
                        }
                    ],
                    "synonyms": [
                        "неподобающе",
                        "непристойно"
                    ],
                    "antonyms": [
                        "прилично"
                    ],
                    "common_phrases": [
                        "это неприлично",
                        "очень неприлично",
                        "поступать неприлично"
                    ],
                    "target_lang_definitions": {
                        "en": "Characterized as improper, impolite, or contrary to accepted standards of behavior."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "impolite",
                                "target_lang_sense_clarification": "lacking good manners"
                            },
                            {
                                "target_lang_word": "rude",
                                "target_lang_sense_clarification": "ill-mannered or offensive"
                            },
                            {
                                "target_lang_word": "improper",
                                "target_lang_sense_clarification": "not in accordance with accepted standards"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd6a7aa4-91e2-3081-9f65-86518ec9ebfe",
                    "sense_definition": "Не соответствуя принятым правилам приличия; неподобающим образом.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Гость вёл себя <неприлично> и громко спорил с официантами.",
                            "target_lang_translations": {
                                "en": "The guest behaved <w>improperly</w> and argued loudly with the waiters."
                            }
                        },
                        {
                            "text": "Она считала, что он одет слишком <неприлично> для посещения театра.",
                            "target_lang_translations": {
                                "en": "She thought that he was dressed too <w>indecently</w> for a visit to the theater."
                            }
                        }
                    ],
                    "synonyms": [
                        "неподобающе",
                        "непристойно"
                    ],
                    "antonyms": [
                        "прилично"
                    ],
                    "common_phrases": [
                        "вести себя неприлично",
                        "выглядеть неприлично",
                        "выражаться неприлично"
                    ],
                    "target_lang_definitions": {
                        "en": "In a manner that violates accepted standards of decency or propriety."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "indecently",

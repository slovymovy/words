    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "aeb4fdc7-df20-300b-8ee7-71b8af098e19",
                    "sense_definition": "Zich met kracht afzetten van de grond om even los te komen en door de lucht te bewegen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen <w>springen</w> vrolijk op het grote bed.",
                            "target_lang_translations": {
                                "en": "The children are <w>jumping</w> happily on the big bed."
                            }
                        },
                        {
                            "text": "Hij <w>sprong</w> over de brede sloot zonder nat te worden.",
                            "target_lang_translations": {
                                "en": "He <w>jumped</w> over the wide ditch without getting wet."
                            }
                        },
                        {
                            "text": "Tijdens de gymles moesten we over een bok <w>springen</w>.",
                            "target_lang_translations": {
                                "en": "During gym class, we had to <w>jump</w> over a buck."
                            }
                        }
                    ],
                    "synonyms": [
                        "huppen"
                    ],
                    "common_phrases": [
                        "touwtje springen",
                        "in het oog springen",
                        "een gat in de lucht springen",
                        "uit de band springen"
                    ],
                    "target_lang_definitions": {
                        "en": "To push oneself off the ground into the air using one's legs."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "jump",
                                "target_lang_sense_clarification": "general movement"
                            },
                            {
                                "target_lang_word": "leap",
                                "target_lang_sense_clarification": "over a distance or obstacle"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7a639ac-f11f-356e-95b5-113a68045774",
                    "sense_definition": "Plotseling kapotgaan, barsten of uit elkaar vallen, vaak door druk, hitte of kou.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "breaking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de extreme hitte is het glas van het raam <w>gesprongen</w>.",
                            "target_lang_translations": {
                                "en": "Due to the extreme heat, the window glass <w>cracked</w>."
                            }
                        },
                        {
                            "text": "Mijn lippen zijn <w>gesprongen</w> door de koude wind.",
                            "target_lang_translations": {
                                "en": "My lips have <w>cracked</w> due to the cold wind."
                            }
                        }
                    ],
                    "synonyms": [
                        "barsten",
                        "knappen",
                        "uiteenspatten"
                    ],
                    "common_phrases": [
                        "op springen staan"
                    ],
                    "target_lang_definitions": {
                        "en": "To break, crack, or burst suddenly, often due to pressure or temperature."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crack",
                                "target_lang_sense_clarification": "glass, skin, etc."
                            },
                            {
                                "target_lang_word": "burst",
                                "target_lang_sense_clarification": "explode or break open"
                            },
                            {
                                "target_lang_word": "shatter",
                                "target_lang_sense_clarification": "break into pieces"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1e1072a-8dff-368f-8430-193fe62fd4ad",
                    "sense_definition": "Plotseling en onverwacht tevoorschijn komen (specifiek gezegd van tranen).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De tranen <w>sprongen</w> hem in de ogen toen hij het slechte nieuws hoorde.",
                            "target_lang_translations": {
                                "en": "Tears <w>sprang</w> to his eyes when he heard the bad news."
                            }
                        },
                        {
                            "text": "Bij het snijden van de uien <w>sprongen</w> de tranen direct in mijn ogen.",
                            "target_lang_translations": {
                                "en": "When cutting the onions, tears immediately <w>sprang</w> to my eyes."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de tranen springen in de ogen"
                    ],
                    "target_lang_definitions": {
                        "en": "To appear suddenly and unexpectedly (specifically of tears)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "spring",
                                "target_lang_sense_clarification": "appear suddenly"
                            },
                            {
                                "target_lang_word": "well up",
                                "target_lang_sense_clarification": "rise to the surface"
                            }
                        ]
                    }
                }
            ]
        }
    ]

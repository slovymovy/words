            "senses": [
                {
                    "sense_id": "e970f075-55b6-368a-84f7-7a79d9e4eeab",
                    "sense_definition": "Związany z <w>koniem</w>, dotyczący go lub do niego należący.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "relational",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wiejskiej drodze słychać było głośny stukot <w>końskich</w> kopyt.",
                            "target_lang_translations": {
                                "ru": "На деревенской дороге был слышен громкий стук <w>конских</w> копыт."
                            }
                        },
                        {
                            "text": "Ten stary wóz <w>koński</w> jest teraz eksponatem w muzeum.",
                            "target_lang_translations": {
                                "ru": "Эта старая <w>конная</w> повозка теперь является экспонатом в музее."
                            }
                        }
                    ],
                    "common_phrases": [
                        "koński ogon",
                        "końskie kopyto",
                        "koński targ",
                        "końskie mięso"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с лошадью, относящийся к ней или ей принадлежащий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конский",
                                "target_lang_sense_clarification": "относящийся к коню"
                            },
                            {
                                "target_lang_word": "лошадиный",
                                "target_lang_sense_clarification": "принадлежащий лошади"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d57dc57-b80d-33b7-9db6-fb8f3e28a365",
                    "sense_definition": "Mający cechy typowe dla <w>konia</w>; w przenośni oznaczający coś bardzo dużego, silnego lub intensywnego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek ma <w>końskie</w> zdrowie i mimo wieku wciąż ciężko pracuje.",
                            "target_lang_translations": {
                                "ru": "У моего дедушки <w>лошадиное</w> здоровье, и, несмотря на возраст, он всё ещё тяжело работает."
                            }
                        },
                        {
                            "text": "Lekarz musiał przepisać pacjentowi <w>końską</w> dawkę antybiotyku.",
                            "target_lang_translations": {
                                "ru": "Врач был вынужден выписать пациенту <w>конскую</w> дозу антибиотика."
                            }
                        }
                    ],
                    "common_phrases": [
                        "końskie zdrowie",
                        "końska dawka",
                        "koński śmiech",
                        "końska twarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий черты, типичные для лошади; в переносном смысле — очень большой, сильный или интенсивный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лошадиный",

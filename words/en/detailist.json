                            {
                                "target_lang_word": "подробный",
                                "target_lang_sense_clarification": "описывающий что-либо во всех деталях"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c0c0cf94-a0b3-396e-963f-97e13195a28a",

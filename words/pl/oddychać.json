{
    "word_family": [
        "oddech",
        "wdech",
        "wydech",
        "tchnienie",
        "natchnienie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "318e0f45-4497-3b33-9474-0df230c79391",
                    "sense_definition": "Wciągać powietrze do płuc i wypuszczać je w celu dostarczenia tlenu do organizmu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physiological_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz głęboko <w>oddychać</w>, żeby się uspokoić.",
                            "target_lang_translations": {
                                "ru": "Ты должен глубоко <w>дышать</w>, чтобы успокоиться."
                            }
                        },
                        {
                            "text": "Lubię wyjść rano do ogrodu i <w>oddychać</w> świeżym powietrzem.",
                            "target_lang_translations": {
                                "ru": "Я люблю выйти утром в сад и <w>дышать</w> свежим воздухом."
                            }
                        }
                    ],
                    "synonyms": [
                        "reg. śl. dychać"
                    ],
                    "antonyms": [
                        "dusić się",
                        "wstrzymywać oddech"
                    ],
                    "common_phrases": [
                        "głęboko oddychać",
                        "ciężko oddychać",
                        "oddychać z ulgą",
                        "oddychać świeżym powietrzem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вдыхать воздух в легкие и выдыхать его для насыщения организма кислородом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дышать",
                                "target_lang_sense_clarification": "физиологический процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "440c4f29-67b3-35bb-927f-ef520210d1a2",
                    "sense_definition": "Żyć; wykazywać podstawowe oznaki życia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "state_of_being",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Poszkodowany w wypadku nadal <w>oddycha</w>, więc jest nadzieja.",
                            "target_lang_translations": {
                                "ru": "Пострадавший в аварии всё ещё <w>дышит</w>, так что есть надежда."
                            }
                        },
                        {
                            "text": "Dopóki człowiek <w>oddycha</w>, może zmienić swój los.",
                            "target_lang_translations": {
                                "ru": "Пока человек <w>дышит</w>, он может изменить свою судьбу."
                            }
                        }
                    ],
                    "synonyms": [
                        "reg. śl. dychać"
                    ],
                    "antonyms": [
                        "umierać"
                    ],
                    "common_phrases": [
                        "ledwo oddychać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жить; проявлять основные признаки жизни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дышать",
                                "target_lang_sense_clarification": "жить, существовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5297e79d-5497-3970-9d30-21e6c10c6482",
                    "sense_definition": "Odpoczywać po dużym wysiłku fizycznym, aby uspokoić rytm oddechu.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "rest",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po szybkim biegu muszę chwilę <w>oddychać</w>, zanim cokolwiek powiem.",
                            "target_lang_translations": {
                                "ru": "После быстрого бега мне нужно немного <w>отдышаться</w>, прежде чем я смогу что-то сказать."
                            }
                        },
                        {
                            "text": "Daj mi <w>oddychać</w>, jestem bardzo zmęczony po tej wspinaczce.",
                            "target_lang_translations": {
                                "ru": "Дай мне <w>отдышаться</w>, я очень устал после этого подъема."
                            }
                        }
                    ],
                    "synonyms": [
                        "reg. śl. dychać"
                    ],
                    "common_phrases": [
                        "daj mi oddychać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдыхать после физической нагрузки, чтобы восстановить дыхание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отдышаться",
                                "target_lang_sense_clarification": "восстановить дыхание"
                            },
                            {
                                "target_lang_word": "перевести дух",
                                "target_lang_sense_clarification": "сделать передышку"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
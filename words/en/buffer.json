                            {
                                "target_lang_word": "более накачанный",
                                "target_lang_sense_clarification": "о теле"
                            },
                            {
                                "target_lang_word": "более мускулистый",
                                "target_lang_sense_clarification": "о теле"
                            }
                        ]
                    }

                    "sense_id": "0df11fa9-6662-3cda-ae58-806d16c051c2",
                    "sense_definition": "Nummer acht in een volgorde of rij.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "ordinal_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is de <w>achtste</w> keer dat ik je probeer te bellen.",
                            "target_lang_translations": {

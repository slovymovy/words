    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "86de2e8a-9408-3610-a2ca-02a161f4f31a",
                    "sense_definition": "Pojazd szynowy posiadający własny napęd, przeznaczony do ciągnięcia lub pchania wagonów po torach.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport_vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zawsze marzyłem, że wsiądę do <w>lokomotywy</w> i pociągnę za głośny gwizdek.",
                            "target_lang_translations": {
                                "ru": "Я всегда мечтал, что сяду в <w>локомотив</w> и дам громкий гудок."
                            }
                        },
                        {
                            "text": "Ciężka <w>lokomotywa</w> powoli ruszyła ze stacji, ciągnąc za sobą długi sznur wagonów.",
                            "target_lang_translations": {
                                "ru": "Тяжелый <w>локомотив</w> медленно тронулся со станции, таща за собой длинную вереницу вагонов."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciuchcia",
                        "elektrowóz",
                        "parowóz"
                    ],
                    "antonyms": [
                        "wagon"
                    ],
                    "common_phrases": [
                        "lokomotywa parowa",
                        "lokomotywa spalinowa",
                        "prowadzić lokomotywę",
                        "stara lokomotywa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рельсовое транспортное средство с собственным двигателем, предназначенное для тяги или толкания вагонов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "локомотив",
                                "target_lang_sense_clarification": "железнодорожное транспортное средство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aff5e2d6-197b-359d-aefb-404cf9d748c1",
                    "sense_definition": "Osoba, organizacja lub zjawisko będące główną siłą napędową działań, od której zależy sukces całego przedsięwzięcia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_leader",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znany polityk ma być <w>lokomotywą</w> listy wyborczej w naszym województwie.",
                            "target_lang_translations": {
                                "ru": "Известный политик должен стать <w>локомотивом</w> избирательного списка в нашем воеводстве."
                            }
                        },
                        {
                            "text": "Eksport nowoczesnych technologii stał się główną <w>lokomotywą</w> rozwoju gospodarczego kraju.",
                            "target_lang_translations": {
                                "ru": "Экспорт современных технологий стал главным <w>локомотивом</w> экономического развития страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "parowóz",
                        "ciuchcia"
                    ],
                    "common_phrases": [
                        "lokomotywa wyborcza",
                        "lokomotywa gospodarcza",
                        "lokomotywa postępu",
                        "być lokomotywą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, организация или явление, выступающее главной движущей силой, от которой зависит успех всего предприятия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "локомотив",
                                "target_lang_sense_clarification": "движущая сила (переносное значение)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65106ff7-54a4-3a8d-96e3-a69b4a0d0b2d",
                    "sense_definition": "Uczennica lub dziewczyna, która pali bardzo dużo papierosów.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kaśka to straszna <w>lokomotywa</w>, bo biega na papierosa podczas każdej przerwie.",
                            "target_lang_translations": {
                                "ru": "Каська — страшный <w>паровоз</w>, потому что бегает на перекур на каждой перемене."
                            }
                        },
                        {
                            "text": "Nauczyciele znowu przyłapali tę szkolną <w>lokomotywę</w> z dymkiem za salą gimnastyczną.",
                            "target_lang_translations": {
                                "ru": "Учителя снова поймали этот школьный <w>паровоз</w> с сигаретой за спортзалом."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "szkolny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ученица или девушка, которая очень много курит."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паровоз",
                                "target_lang_sense_clarification": "о человеке, который много курит (разговорное)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

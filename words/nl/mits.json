        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "c18fb4c9-d13a-369c-b823-101fbe704a1f",
                    "sense_definition": "Op voorwaarde dat; alleen in het geval dat.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je mag vanavond uitgaan, <w>mits</w> je eerst je huiswerk afmaakt.",
                            "target_lang_translations": {
                                "en": "You may go out tonight, <w>provided that</w> you finish your homework first."
                            }
                        },
                        {
                            "text": "De verzekering keert het bedrag uit, <w>mits</w> de schade binnen 24 uur is gemeld.",
                            "target_lang_translations": {
                                "en": "The insurance pays out the amount, <w>provided that</w> the damage is reported within 24 hours."
                            }
                        }
                    ],
                    "antonyms": [
                        "tenzij"
                    ],
                    "target_lang_definitions": {
                        "en": "On the condition that; provided that."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "provided that",
                                "target_lang_sense_clarification": "formal condition"
                            },
                            {
                                "target_lang_word": "on condition that",
                                "target_lang_sense_clarification": "formal"
                            },
                            {
                                "target_lang_word": "if",
                                "target_lang_sense_clarification": "general condition"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e389f923-e4ac-3497-8f6c-95ed8e75605b",
                    "sense_definition": "Aangezien; omdat.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "reason",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij kon niet aanwezig zijn, <w>mits</w> hij ziek was.",
                            "target_lang_translations": {
                                "en": "He could not be present, <w>since</w> he was ill."
                            }
                        },
                        {
                            "text": "De vergadering werd uitgesteld, <w>mits</w> er te weinig deelnemers waren.",
                            "target_lang_translations": {
                                "en": "The meeting was postponed, <w>because</w> there were too few participants."
                            }
                        }
                    ],
                    "synonyms": [
                        "vermits"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "wordt in modern Nederlands vaak als onjuist beschouwd"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Since; because (archaic or non-standard usage)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "since",
                                "target_lang_sense_clarification": "reason"
                            },
                            {
                                "target_lang_word": "because",
                                "target_lang_sense_clarification": "reason"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "97d29db7-f60c-38cb-923a-e527fa13cdbf",
                    "sense_definition": "Op voorwaarde van; met als vereiste.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Toegang tot het evenement is mogelijk <w>mits</w> betaling aan de deur.",
                            "target_lang_translations": {

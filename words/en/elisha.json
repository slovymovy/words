                {
                    "sense_id": "f88925d5-78f5-349a-a6c4-dbfb00656536",
                    "sense_definition": "A prophet in the Hebrew Bible who was the disciple and successor of Elijah.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "biblical_figure",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "The prophet <w>Elisha</w> performed many miracles throughout his life.",
                            "target_lang_translations": {
                                "ru": "Пророк <w>Елисей</w> совершил много чудес за свою жизнь."
                            }
                        },
                        {
                            "text": "In the scriptures, Elijah passed his mantle to <w>Elisha</w>.",
                            "target_lang_translations": {
                                "ru": "В Писании Илия передал свою милоть <w>Елисею</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "prophet Elisha"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пророк в еврейской Библии, ученик и преемник Илии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Елисей",
                                "target_lang_sense_clarification": "библейский пророк"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67dd3a19-3bd9-3fcd-9715-e9363f6eaefa",
                    "sense_definition": "A male given name of Hebrew origin.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "male_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My uncle <w>Elisha</w> is known for his quiet and thoughtful nature.",
                            "target_lang_translations": {
                                "ru": "Мой дядя <w>Элайша</w> известен своим тихим и вдумчивым характером."
                            }
                        },
                        {
                            "text": "<w>Elisha</w> Otis was the inventor of the safety elevator.",
                            "target_lang_translations": {

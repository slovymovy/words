            "senses": [
                {
                    "sense_id": "1db9831b-699f-3b46-8f51-9ed5c8ef8858",
                    "sense_definition": "Klarowna zupa na wywarze z kości i mięsa, zazwyczaj z dodatkiem warzyw.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "soup_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gdy wróciliśmy do domu, mama podała nam gorący <w>rosół</w>.",
                            "target_lang_translations": {
                                "ru": "Когда мы вернулись домой, мама подала нам горячий <w>бульон</w>."
                            }
                        },
                        {
                            "text": "W niedzielę na obiad tradycyjnie jemy <w>rosół</w> z makaronem.",
                            "target_lang_translations": {
                                "ru": "В воскресенье на обед мы традиционно едим <w>бульон</w> с лапшой."
                            }
                        }
                    ],
                    "synonyms": [
                        "bulion"
                    ],
                    "common_phrases": [
                        "rosół z kury",
                        "rosół z makaronem",
                        "niedzielny rosół",
                        "gorący rosół"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прозрачный суп на мясном бульоне, обычно из птицы с добавлением овощей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бульон",
                                "target_lang_sense_clarification": "суп"
                            },
                            {
                                "target_lang_word": "куриный бульон",
                                "target_lang_sense_clarification": "разновидность супа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e466308-0eed-3c6d-856c-0245789b3c53",
                    "sense_definition": "Marynata do mięsa lub bryndzy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodyni przygotowała <w>rosół</w> do zamarynowania świeżego mięsa.",
                            "target_lang_translations": {
                                "ru": "Хозяйка приготовила <w>рассол</w> для маринования свежего мяса."
                            }
                        },
                        {
                            "text": "Ten <w>rosół</w> nada bryndzy odpowiedni, ostry smak.",
                            "target_lang_translations": {
                                "ru": "Этот <w>рассол</w> придаст брынзе соответствующий острый вкус."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Bukowina"
                        }
                    ],
                    "target_lang_definitions": {

                    "sense_definition": "Een voertuig dat speciaal is uitgerust om zieken en gewonden naar een ziekenhuis te vervoeren.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>ambulance</w> kwam snel ter plaatse na het ernstige ongeluk.",
                            "target_lang_translations": {
                                "en": "The <w>ambulance</w> arrived quickly at the scene after the serious accident."
                            }
                        },
                        {
                            "text": "Je moet direct een <w>ambulance</w> bellen als iemand in levensgevaar is.",
                            "target_lang_translations": {
                                "en": "You must call an <w>ambulance</w> immediately if someone is in life-threatening danger."
                            }
                        }
                    ],
                    "synonyms": [
                        "ziekenwagen",
                        "ziekenauto"
                    ],
                    "common_phrases": [
                        "een ambulance bellen",
                        "met de ambulance",
                        "de ambulance komt"
                    ],
                    "target_lang_definitions": {
                        "en": "A vehicle specially equipped to transport the sick and injured to a hospital."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ambulance",
                                "target_lang_sense_clarification": "emergency vehicle"
                            }
                        ]

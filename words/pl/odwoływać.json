                            },
                            {
                                "target_lang_word": "отзывать",
                                "target_lang_sense_clarification": "посла, представителя или чиновника"
                            },
                            {
                                "target_lang_word": "увольнять",
                                "target_lang_sense_clarification": "с работы"
                            }

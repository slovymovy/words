                            {
                                "target_lang_word": "упустить",
                                "target_lang_sense_clarification": "не учесть, потерять из виду"
                            },
                            {
                                "target_lang_word": "обойти",
                                "target_lang_sense_clarification": "обделить вниманием, игнорировать"

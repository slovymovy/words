                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "reflexive with 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально заявлять об уходе с должности (с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подавать в отставку",
                                "target_lang_sense_clarification": "увольняться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eee9ecd6-4343-3e9b-affe-d63d884e7912",
                    "sense_definition": "Zgłosić sprawę przeciwko komuś do sądu.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "transfer_info",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie chcę cię <podawać> do sądu, ale nie dajesz mi wyboru.",
                            "target_lang_translations": {
                                "ru": "Я не хочу <подавать> на тебя в суд, но ты не оставляешь мне выбора."
                            }
                        },
                        {
                            "text": "Ludzie coraz częściej <podają> szpitale do sądu za błędy lekarskie.",
                            "target_lang_translations": {
                                "ru": "Люди всё чаще <подают> на больницы в суд за врачебные ошибки."
                            }
                        }
                    ],
                    "synonyms": [
                        "pozywać",
                        "skarżyć"
                    ],
                    "common_phrases": [
                        "podać do sądu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обращаться с иском против кого-то в суд."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подавать в суд",
                                "target_lang_sense_clarification": "судиться"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3e8c54c1-ad14-37fe-abb0-09eaebd9e2f6",
                    "sense_definition": "Een document waarmee iemand kan laten zien hoe hij als uniek persoon door een autoriteit is geregistreerd.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "identification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet een geldig <w>ID</w> laten zien als je alcohol wilt kopen.",
                            "target_lang_translations": {
                                "ru": "Вы должны предъявить действительное <w>удостоверение личности</w>, если хотите купить алкоголь.",
                                "en": "You must show a valid <w>ID</w> if you want to buy alcohol."
                            }
                        },
                        {
                            "text": "Ben je je <w>ID</w> vergeten mee te nemen naar het gemeentehuis?",
                            "target_lang_translations": {
                                "ru": "Ты забыл взять с собой <w>удостоверение личности</w> в мэрию?",
                                "en": "Did you forget to take your <w>ID</w> to the town hall?"
                            }
                        }
                    ],
                    "synonyms": [
                        "identiteitsbewijs",
                        "identiteitskaart",
                        "ID-kaart",
                        "persoonsbewijs"
                    ],
                    "common_phrases": [
                        "geldig ID",
                        "ID tonen",
                        "geen ID"
                    ],
                    "target_lang_definitions": {
                        "ru": "Документ, выданный официальными органами, с помощью которого можно подтвердить свою личность.",
                        "en": "A document issued by an authority used to prove a person's identity."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удостоверение личности",
                                "target_lang_sense_clarification": "документ"
                            },
                            {
                                "target_lang_word": "паспорт",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "ID",
                                "target_lang_sense_clarification": "document"
                            },
                            {
                                "target_lang_word": "ID card",
                                "target_lang_sense_clarification": "physical card"
                            },
                            {
                                "target_lang_word": "identification",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "527ceac8-c5f7-3548-a4d4-10e3b497829e",
                    "sense_definition": "Een ondubbelzinnige aanduiding van iemand als uniek persoon door een autoriteit.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "identification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie controleerde het <w>ID</w> van de verdachte in het systeem.",
                            "target_lang_translations": {
                                "ru": "Полиция проверила <w>идентификационные данные</w> подозреваемого в системе.",
                                "en": "The police checked the suspect's <w>ID</w> in the system."
                            }
                        },
                        {
                            "text": "Zonder het juiste <w>ID</w> krijg je geen toegang tot deze gegevens.",
                            "target_lang_translations": {
                                "ru": "Без правильного <w>идентификатора</w> вы не получите доступ к этим данным.",
                                "en": "Without the correct <w>ID</w>, you won't get access to this data."
                            }
                        }
                    ],
                    "synonyms": [
                        "identiteit"
                    ],
                    "common_phrases": [
                        "vals ID",
                        "ID controleren"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уникальное обозначение или данные, идентифицирующие личность в системе.",
                        "en": "A unique designation or code used to identify someone or something within a system."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "идентификатор",
                                "target_lang_sense_clarification": "код/обозначение"
                            },
                            {
                                "target_lang_word": "идентификационные данные",
                                "target_lang_sense_clarification": "сведения"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "ID",
                                "target_lang_sense_clarification": "system code/identity"
                            },
                            {
                                "target_lang_word": "identifier",
                                "target_lang_sense_clarification": "technical"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53fc6ec4-05d8-3b5d-ae21-eb9851ae9224",
                    "sense_definition": "Het meest oorspronkelijke deel van iemands onderbewustzijn.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Volgens de theorie wordt het <w>id</w> gedreven door onze basisbehoeften.",
                            "target_lang_translations": {
                                "ru": "Согласно теории, <w>Оно</w> движимо нашими базовыми потребностями.",
                                "en": "According to the theory, the <w>id</w> is driven by our basic needs."
                            }
                        },
                        {
                            "text": "Er is een constant conflict tussen het ego en het <w>id</w>.",
                            "target_lang_translations": {
                                "ru": "Существует постоянный конфликт между Эго и <w>Оно</w>.",
                                "en": "There is a constant conflict between the ego and the <w>id</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het ego en het id"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бессознательная часть психики, являющаяся источником инстинктивных влечений (в психоанализе).",
                        "en": "The unconscious part of the psyche that is the source of instinctive impulses (in psychoanalysis)."
                    },
                    "translations": {

                {
                    "sense_id": "f224f038-7dfc-39a8-8af8-d85fa31b4431",
                    "sense_definition": "Initialism of <w>accounts</w> <w>payable</w>: money owed by a business to its suppliers; also, the department responsible for paying these debts.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "business_finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>AP</w> department processes all invoices at the end of the week.",
                            "target_lang_translations": {
                                "ru": "Отдел <w>расчетов с поставщиками</w> обрабатывает все счета-фактуры в конце недели."
                            }
                        },
                        {
                            "text": "Please send the bill directly to <w>AP</w> for approval.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, отправьте счет напрямую в <w>отдел расчетов с поставщиками</w> для утверждения."
                            }
                        }
                    ],
                    "antonyms": [
                        "AR"
                    ],
                    "common_phrases": [
                        "AP department",
                        "AP clerk",
                        "AP ledger"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "initialism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от accounts payable: деньги, которые компания должна своим поставщикам; также отдел, ответственный за оплату этих долгов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кредиторская задолженность",
                                "target_lang_sense_clarification": "финансы"
                            },
                            {
                                "target_lang_word": "отдел расчетов с поставщиками",
                                "target_lang_sense_clarification": "подразделение компании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f04f457d-a005-3532-bcc1-9dc6ed91516d",
                    "sense_definition": "Initialism of <w>access</w> <w>point</w>: a device that allows wireless devices to connect to a wired network.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The signal is weak because the <w>AP</w> is located in the basement.",
                            "target_lang_translations": {
                                "ru": "Сигнал слабый, потому что <w>точка доступа</w> находится в подвале."
                            }
                        },
                        {
                            "text": "You need to configure the wireless <w>AP</w> before users can connect to the internet.",
                            "target_lang_translations": {
                                "ru": "Вам нужно настроить беспроводную <w>точку доступа</w>, прежде чем пользователи смогут подключиться к интернету."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wireless AP",
                        "configure the AP"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "initialism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от access point: устройство, позволяющее беспроводным устройствам подключаться к проводной сети."

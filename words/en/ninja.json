{
    "word_family": [
        "ninjutsu"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b02d96f5-f96a-3348-8815-88b5dfa90672",
                    "sense_definition": "A person trained in ninjutsu, especially a covert agent or assassin in feudal Japan.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "historical_ninja",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>ninja</w> moved silently across the roof to infiltrate the castle.",
                            "target_lang_translations": {
                                "ru": "<w>Ниндзя</w> бесшумно двигался по крыше, чтобы проникнуть в замок."
                            }
                        },
                        {
                            "text": "Legends say a <w>ninja</w> could disappear in a puff of smoke.",
                            "target_lang_translations": {
                                "ru": "Легенды гласят, что <w>ниндзя</w> мог исчезнуть в клубах дыма."
                            }
                        }
                    ],
                    "synonyms": [
                        "shinobi",
                        "assassin",
                        "spy"
                    ],
                    "common_phrases": [
                        "ninja warrior",
                        "ninja sword",
                        "ninja costume"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "historical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, обученный ниндзюцу, особенно тайный агент или наемный убийца в феодальной Японии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ниндзя",
                                "target_lang_sense_clarification": "исторический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "792dc79f-a329-318a-9653-81e3f1f488ca",
                    "sense_definition": "A person who is exceptionally skillful or effective in a particular field.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "skillful_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is a coding <w>ninja</w> who can solve any software problem.",
                            "target_lang_translations": {
                                "ru": "Она <w>виртуоз</w> программирования, способный решить любую проблему с софтом."
                            }
                        },
                        {
                            "text": "We need a marketing <w>ninja</w> to boost our sales figures.",
                            "target_lang_translations": {
                                "ru": "Нам нужен <w>ниндзя</w> маркетинга, чтобы поднять показатели продаж."
                            }
                        }
                    ],
                    "synonyms": [
                        "magician",
                        "superspy"
                    ],
                    "common_phrases": [
                        "coding ninja",
                        "kitchen ninja"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который исключительно искусен или эффективен в определенной области."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "виртуоз",
                                "target_lang_sense_clarification": "мастер своего дела"
                            },
                            {
                                "target_lang_word": "ниндзя",
                                "target_lang_sense_clarification": "метафорическое использование"
                            },
                            {
                                "target_lang_word": "гуру",
                                "target_lang_sense_clarification": "эксперт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e29fbe25-9e58-3578-8b5e-50963b02c266",
                    "sense_definition": "A friendly term of address for a person.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "slang_address",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hey <w>ninja</w>, are you coming to the party tonight?",
                            "target_lang_translations": {
                                "ru": "Эй, <w>братан</w>, ты идешь сегодня на вечеринку?"
                            }
                        },
                        {
                            "text": "I trust my <w>ninja</w> to always have my back.",
                            "target_lang_translations": {
                                "ru": "Я верю, что мой <w>братан</w> всегда прикроет мне спину."
                            }
                        }
                    ],
                    "synonyms": [
                        "man as a friendly term of address"
                    ],
                    "common_phrases": [
                        "my ninja"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "AAVE / euphemism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Дружеское обращение к человеку (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "братан",
                                "target_lang_sense_clarification": "дружеское обращение"
                            },
                            {
                                "target_lang_word": "дружище",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "чувак",
                                "target_lang_sense_clarification": "молодежный сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98053fb9-2c9b-3819-ada6-dfd4ff49ccc0",
                    "sense_definition": "A member of an anti-government rebel group active in the Republic of the Congo.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "rebel_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>Ninjas</w> were known for their opposition to the central government.",
                            "target_lang_translations": {
                                "ru": "Ополченцы <w>«Ниндзя»</w> были известны своей оппозицией центральному правительству."
                            }
                        },
                        {
                            "text": "Peacekeepers monitored the region where the <w>Ninjas</w> operated.",
                            "target_lang_translations": {
                                "ru": "Миротворцы наблюдали за регионом, где действовали <w>«Ниндзя»</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Republic of the Congo"
                        },
                        {
                            "trait_type": "form",
                            "comment": "usually capitalized"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Член антиправительственной повстанческой группы, действующей в Республике Конго."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "«Ниндзя»",
                                "target_lang_sense_clarification": "название группировки"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ceacfdd2-3b97-35ed-87fd-cf71ba96769e",
                    "sense_definition": "Of or related to ninjas or their characteristics, such as stealth or agility.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "adj_related",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He showed off his <w>ninja</w> moves on the dance floor.",
                            "target_lang_translations": {
                                "ru": "Он продемонстрировал свои движения <w>в стиле ниндзя</w> на танцполе."
                            }
                        },
                        {
                            "text": "The cat has <w>ninja</w> reflexes when catching flies.",
                            "target_lang_translations": {
                                "ru": "У кота реакция <w>как у ниндзя</w>, когда он ловит мух."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ninja skills",
                        "ninja moves"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к ниндзя или их характеристикам, таким как скрытность или ловкость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в стиле ниндзя",
                                "target_lang_sense_clarification": "о характеристиках"
                            },
                            {
                                "target_lang_word": "ниндзя",
                                "target_lang_sense_clarification": "несклоняемое прилагательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "722c9be7-3432-3d77-af81-1df34bf6d281",
                    "sense_definition": "Describing a loan or borrower characterized by having no income, no job, and no assets.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "adj_financial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The financial crisis was partly caused by the proliferation of <w>ninja</w> loans.",
                            "target_lang_translations": {
                                "ru": "Финансовый кризис был частично вызван распространением кредитов типа <w>«ниндзя»</w>."
                            }
                        },
                        {
                            "text": "Banks have stopped offering <w>ninja</w> mortgages to high-risk applicants.",
                            "target_lang_translations": {
                                "ru": "Банки перестали предлагать ипотеку <w>«ниндзя»</w> заявителям с высоким риском."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ninja loan",
                        "ninja mortgage"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "acronym"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "financial context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Описание кредита или заемщика, характеризующегося отсутствием дохода, работы и активов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ниндзя",
                                "target_lang_sense_clarification": "банковский сленг"
                            },
                            {
                                "target_lang_word": "необеспеченный",
                                "target_lang_sense_clarification": "о кредите"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f5205fd0-2afe-3cf9-ab58-b68f184ff6d8",
                    "sense_definition": "To move with great speed, stealth, and agility.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "action_stealth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>ninjaed</w> across the room so he wouldn't wake the baby.",
                            "target_lang_translations": {
                                "ru": "Он <w>прокрался</w> через комнату, чтобы не разбудить ребенка."
                            }
                        },
                        {
                            "text": "I managed to <w>ninja</w> my way into the front row of the concert.",
                            "target_lang_translations": {
                                "ru": "Мне удалось <w>просочиться</w> в первый ряд концерта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ninja around"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться с большой скоростью, скрытностью и ловкостью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прокрасться",
                                "target_lang_sense_clarification": "двигаться скрытно"
                            },
                            {
                                "target_lang_word": "проскользнуть",
                                "target_lang_sense_clarification": "пройти незамеченным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47bd8099-100f-3945-930b-afa3cde95bce",
                    "sense_definition": "To post a reply to a forum thread immediately before someone else, rendering their post redundant.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "action_internet",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I tried to answer the question, but another user <w>ninja'd</w> me.",
                            "target_lang_translations": {
                                "ru": "Я пытался ответить на вопрос, но другой пользователь <w>опередил</w> меня."
                            }
                        },
                        {
                            "text": "He got <w>ninjaed</w> by a faster typist who posted the same link.",
                            "target_lang_translations": {
                                "ru": "Его <w>опередил</w> более быстрый машинист, опубликовавший ту же ссылку."
                            }
                        }
                    ],
                    "synonyms": [
                        "preempt"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправить ответ в ветке форума непосредственно перед кем-то другим, делая его сообщение излишним."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опередить",
                                "target_lang_sense_clarification": "в обсуждении"
                            },
                            {
                                "target_lang_word": "перехватить",
                                "target_lang_sense_clarification": "инициативу в ответе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "045f6a33-4c35-3832-b180-59407ca8b1a7",
                    "sense_definition": "To claim an item in a video game through abuse of game mechanics or unfair timing.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "action_gaming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The player was kicked from the group after he <w>ninja'd</w> the rare loot.",
                            "target_lang_translations": {
                                "ru": "Игрока выгнали из группы после того, как он <w>своровал</w> редкую добычу."
                            }
                        },
                        {
                            "text": "Don't invite him; he is known to <w>ninja</w> items from bosses.",
                            "target_lang_translations": {
                                "ru": "Не приглашай его; известно, что он <w>крысит</w> предметы с боссов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ninja loot"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "gaming"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Забрать предмет в видеоигре, злоупотребляя механикой игры или несправедливым таймингом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "своровать",
                                "target_lang_sense_clarification": "об игровом предмете"
                            },
                            {
                                "target_lang_word": "скрысить",
                                "target_lang_sense_clarification": "игровой сленг"
                            },
                            {
                                "target_lang_word": "заниндзить",
                                "target_lang_sense_clarification": "игровой сленг"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
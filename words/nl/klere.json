        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3364fc0a-aada-3c60-a852-dc4b3fef5128",
                    "sense_definition": "Een platte benaming voor de ziekte cholera, tegenwoordig voornamelijk gebruikt in verwensingen of als krachtterm.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "disease_curse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als hij niet wil luisteren, kan hij wat mij betreft de <w>klere</w> krijgen.",
                            "target_lang_translations": {
                                "en": "If he doesn't want to listen, he can <w>drop dead</w> as far as I'm concerned.",
                                "ru": "Если он не хочет слушать, то, как по мне, пусть катится к <w>черту</w>."
                            }
                        },
                        {
                            "text": "Ik schrok me echt de <w>klere</w> toen die auto plotseling remde.",
                            "target_lang_translations": {
                                "en": "I was <w>scared to death</w> when that car suddenly braked.",
                                "ru": "Я перепугался <w>до смерти</w>, когда та машина внезапно затормозила."
                            }
                        }
                    ],
                    "synonyms": [
                        "cholera",
                        "tering"
                    ],
                    "common_phrases": [
                        "krijg de klere",
                        "zich de klere werken",
                        "zich de klere schrikken"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A vulgar term for the disease cholera, nowadays mainly used in curses or as an expletive.",
                        "ru": "Грубое сленговое название болезни холера, используемое преимущественно в проклятиях или для выражения сильных эмоций."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cholera",
                                "target_lang_sense_clarification": "literal meaning, archaic"
                            },
                            {
                                "target_lang_word": "the plague",
                                "target_lang_sense_clarification": "idiomatic equivalent in curses"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "холера",
                                "target_lang_sense_clarification": "буквальное значение, также используется как ругательство"
                            },
                            {
                                "target_lang_word": "зараза",
                                "target_lang_sense_clarification": "в значении проклятия или болезни"
                            },
                            {
                                "target_lang_word": "черт",
                                "target_lang_sense_clarification": "в устойчивых выражениях (пошел к черту)"
                            }
                        ]
                    }
                }
            ]
        },
        {

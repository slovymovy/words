    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0b03975d-9df6-36eb-b39c-5f938a48812a",
                    "sense_definition": "Dotyczący minerałów, zawierający minerały lub z nich złożony.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano piję szklankę gazowanej wody <w>mineralnej</w>.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я выпиваю стакан газированной <w>минеральной</w> воды."
                            }
                        },
                        {
                            "text": "Warzywa i owoce dostarczają organizmowi niezbędnych soli <w>mineralnych</w>.",
                            "target_lang_translations": {
                                "ru": "Овощи и фрукты обеспечивают организм необходимыми <w>минеральными</w> солями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "woda mineralna",
                        "sole mineralne",
                        "składniki mineralne",
                        "surowce mineralne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к минералам, содержащий минералы или состоящий из них."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "минеральный",
                                "target_lang_sense_clarification": "относящийся к минералам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c64cb773-91f4-3b43-88fa-fee2d84b1e22",
                    "sense_definition": "Taki, który nie jest organiczny; powstały w procesach geologicznych lub chemicznych bez udziału organizmów żywych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do konserwacji maszyn często wykorzystuje się olej <w>mineralny</w>.",
                            "target_lang_translations": {
                                "ru": "Для консервации машин часто используется <w>минеральное</w> масло."
                            }
                        },
                        {
                            "text": "Współczesne rolnictwo powszechnie stosuje sztuczne nawozy <w>mineralne</w>.",
                            "target_lang_translations": {
                                "ru": "Современное сельское хозяйство широко применяет искусственные <w>минеральные</w> удобрения."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieorganiczny"
                    ],
                    "antonyms": [
                        "organiczny"
                    ],
                    "common_phrases": [
                        "olej mineralny",
                        "nawóz mineralny",
                        "wełna mineralna",
                        "chemia mineralna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неорганический; образовавшийся в результате геологических или химических процессов без участия живых организмов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "минеральный",
                                "target_lang_sense_clarification": "неорганический"
                            }
                        ]
                    }
                }
            ]
        }
    ]

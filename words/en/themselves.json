                {
                    "sense_id": "29db788a-2854-3949-9732-ca316c787d39",
                    "sense_definition": "The reflexive form of <w>they</w> referring to a group of people, animals, or things previously mentioned.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children dressed <w>themselves</w> before going to school.",
                            "target_lang_translations": {
                                "ru": "Дети оделись <w>сами</w> перед тем, как пойти в школу."
                            }
                        },
                        {
                            "text": "They decided to see the new house for <w>themselves</w>.",
                            "target_lang_translations": {
                                "ru": "Они решили осмотреть новый дом <w>сами</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "by themselves",
                        "for themselves",
                        "among themselves",
                        "to themselves"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возвратная форма местоимения they (они), относящаяся к группе ранее упомянутых людей, животных или предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "себя",
                                "target_lang_sense_clarification": "возвратное местоимение"
                            },
                            {
                                "target_lang_word": "сами",
                                "target_lang_sense_clarification": "усилительное значение (сделали сами)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5516b038-ed3d-3a9a-b92c-7f0ea0e6cf6e",
                    "sense_definition": "The reflexive form of <w>they</w> referring to a single person, used when gender is unknown or not specified.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If anyone calls, tell them to leave a message for <w>themselves</w>.",
                            "target_lang_translations": {
                                "ru": "Если кто-нибудь позвонит, скажи им оставить сообщение для <w>себя</w>."
                            }
                        },
                        {
                            "text": "Every student must do the homework by <w>themselves</w>.",
                            "target_lang_translations": {
                                "ru": "Каждый студент должен выполнить домашнее задание <w>сам</w>."
                            }

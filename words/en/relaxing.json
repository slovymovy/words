            "senses": [
                {
                    "sense_id": "aee0cda6-b21e-3e51-8907-8b5c2752939f",
                    "sense_definition": "The process by which something is relaxed.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor monitored the periodic <w>relaxings</w> of the patient's muscles.",
                            "target_lang_translations": {
                                "ru": "Врач наблюдал за периодическими <w>расслаблениями</w> мышц пациента."
                            }
                        },
                        {
                            "text": "Sudden <w>relaxings</w> of the strict rules led to confusion among the staff.",
                            "target_lang_translations": {
                                "ru": "Внезапные <w>ослабления</w> строгих правил привели к замешательству среди персонала."
                            }
                        }
                    ],
                    "synonyms": [

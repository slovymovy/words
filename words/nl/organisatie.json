    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ab677ad9-9aae-3d3e-afa6-7efa831cde3c",
                    "sense_definition": "Een groep mensen of instanties die samenwerken aan een gemeenschappelijk doel, zoals een bedrijf, vereniging of stichting.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_institution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze <w>organisatie</w> zet zich in voor de bescherming van het milieu.",
                            "target_lang_translations": {
                                "en": "This <w>organization</w> is committed to protecting the environment.",
                                "ru": "Эта <w>организация</w> занимается защитой окружающей среды."
                            }
                        },
                        {
                            "text": "Hij werkt al jaren voor een grote internationale <w>organisatie</w>.",
                            "target_lang_translations": {
                                "en": "He has been working for a large international <w>organization</w> for years.",
                                "ru": "Он уже много лет работает в крупной международной <w>организации</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "instelling",
                        "instantie",
                        "vereniging"
                    ],
                    "common_phrases": [
                        "internationale organisatie",
                        "non-profit organisatie",
                        "overkoepelende organisatie"
                    ],
                    "target_lang_definitions": {
                        "en": "A group of people or institutions working together for a common purpose, such as a company, association, or foundation.",
                        "ru": "Группа людей или учреждений, работающих вместе для достижения общей цели, например, компания, ассоциация или фонд."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "organization",
                                "target_lang_sense_clarification": "group with a purpose"
                            },
                            {
                                "target_lang_word": "institution",
                                "target_lang_sense_clarification": "established organization"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "организация",
                                "target_lang_sense_clarification": "учреждение, объединение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cdbcb97-3071-3d6d-be66-6b7b4d85978d",
                    "sense_definition": "De handeling van het regelen, plannen en voorbereiden van iets.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_action_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was een hele <w>organisatie</w> om het feest op tijd voor te bereiden.",
                            "target_lang_translations": {
                                "en": "It took a lot of <w>organization</w> to prepare the party on time.",
                                "ru": "Потребовалась серьезная <w>организация</w>, чтобы подготовить праздник вовремя."
                            }
                        },
                        {
                            "text": "De <w>organisatie</w> van het evenement verliep vlekkeloos dankzij het goede team.",
                            "target_lang_translations": {
                                "en": "The <w>organization</w> of the event went flawlessly thanks to the good team.",
                                "ru": "<w>Организация</w> мероприятия прошла безупречно благодаря хорошей команде."
                            }
                        }
                    ],
                    "synonyms": [
                        "planning",
                        "regeling",
                        "voorbereiding"
                    ],
                    "antonyms": [
                        "chaos",
                        "desorganisatie"
                    ],
                    "common_phrases": [
                        "een hele organisatie",
                        "strakke organisatie"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of arranging, planning, and preparing something.",
                        "ru": "Действие по регулированию, планированию и подготовке чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "organization",
                                "target_lang_sense_clarification": "act of organizing"
                            },
                            {
                                "target_lang_word": "planning",
                                "target_lang_sense_clarification": "process of making plans"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "организация",
                                "target_lang_sense_clarification": "процесс, планирование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7d1162e-762e-316c-9b6e-fc8dd4069ec6",
                    "sense_definition": "De manier waarop iets is opgebouwd, ingericht of gestructureerd.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_action_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De interne <w>organisatie</w> van het bedrijf is onlangs volledig veranderd.",
                            "target_lang_translations": {
                                "en": "The internal <w>organization</w> of the company has recently changed completely.",
                                "ru": "Внутренняя <w>организация</w> компании недавно полностью изменилась."
                            }
                        },
                        {
                            "text": "Zonder een duidelijke <w>organisatie</w> ontstaan er snel misverstanden op de werkvloer.",
                            "target_lang_translations": {
                                "en": "Without clear <w>organization</w>, misunderstandings quickly arise in the workplace.",
                                "ru": "Без четкой <w>организации</w> на рабочем месте быстро возникают недоразумения."
                            }
                        }
                    ],
                    "synonyms": [
                        "structuur",
                        "opbouw",
                        "inrichting"
                    ],
                    "antonyms": [
                        "chaos",
                        "desorganisatie"
                    ],
                    "common_phrases": [
                        "interne organisatie",
                        "goede organisatie"
                    ],
                    "target_lang_definitions": {
                        "en": "The way in which something is built, arranged, or structured.",
                        "ru": "Способ, которым что-либо построено, устроено или структурировано."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "organization",
                                "target_lang_sense_clarification": "structure or arrangement"
                            },
                            {
                                "target_lang_word": "structure",
                                "target_lang_sense_clarification": "arrangement of parts"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "организация",
                                "target_lang_sense_clarification": "структура, устройство"
                            }
                        ]
                    }
                }
            ]
        }
    ]

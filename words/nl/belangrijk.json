                            },
                            {
                                "target_lang_word": "significant",
                                "target_lang_sense_clarification": "noteworthy or having a particular meaning"
                            },
                            {
                                "target_lang_word": "key",
                                "target_lang_sense_clarification": "crucial or essential"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "важный",
                                "target_lang_sense_clarification": "имеющий большое значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9b51c59-b1aa-3cb7-ad74-0f1498b3921a",
                    "sense_definition": "Groot in aantal, omvang of mate.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een <w>belangrijk</w> deel van de studenten is geslaagd voor het examen.",
                            "target_lang_translations": {
                                "en": "A <w>significant</w> portion of the students passed the exam.",
                                "ru": "<w>Значительная</w> часть студентов сдала экзамен."
                            }
                        },
                        {
                            "text": "De kosten vormen een <w>belangrijke</w> post op de begroting van het bedrijf.",
                            "target_lang_translations": {
                                "en": "The costs constitute a <w>major</w> item in the company's budget.",
                                "ru": "Расходы составляют <w>существенную</w> статью в бюджете компании."
                            }
                        }
                    ],
                    "synonyms": [

                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The tourists visited a famous <w>domed</w> cathedral in the city center.",
                            "target_lang_translations": {
                                "ru": "Туристы посетили знаменитый <w>купольный</w> собор в центре города."
                            }
                        },
                        {
                            "text": "The new sports complex features a massive <w>domed</w> roof.",
                            "target_lang_translations": {
                                "ru": "Новый спортивный комплекс отличается огромной <w>купольной</w> крышей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "domed roof",
                        "domed ceiling",
                        "domed stadium",
                        "domed building"
                    ],

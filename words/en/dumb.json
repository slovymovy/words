            "senses": [
                {
                    "sense_id": "b72386cf-b8c7-3996-924d-cdbd8e969b2f",
                    "sense_definition": "Lacking intelligence or good judgment.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "lack_of_intelligence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't be <w>dumb</w>; wear a helmet when you ride your bike.",
                            "target_lang_translations": {
                                "ru": "Не будь <w>глу́пым</w>; надевай шлем, когда катаешься на велосипеде."
                            }
                        },
                        {
                            "text": "He felt <w>dumb</w> after forgetting his own phone number.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал себя <w>глу́пым</w>, забыв свой собственный номер телефона."
                            }
                        }

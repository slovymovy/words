                {
                    "sense_id": "f0437961-f4b9-306b-838b-800310b13776",
                    "sense_definition": "Een situatie met veel activiteit, beweging van mensen of verkeer.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "bustle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de <w>drukte</w> in de stad kon ik niet snel lopen.",
                            "target_lang_translations": {
                                "en": "Because of the <w>crowds</w> in the city, I could not walk fast.",
                                "ru": "Из-за <w>суматохи</w> в городе я не мог быстро идти."
                            }
                        },
                        {
                            "text": "Het is altijd een gezellige <w>drukte</w> op de zaterdagmarkt.",
                            "target_lang_translations": {
                                "en": "It is always a pleasant <w>bustle</w> at the Saturday market.",
                                "ru": "На субботнем рынке всегда приятное <w>оживление</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "hectiek"
                    ],
                    "common_phrases": [
                        "grote drukte",
                        "gezellige drukte",
                        "drukte van belang"
                    ],
                    "target_lang_definitions": {
                        "en": "A situation with a lot of activity, movement of people, or traffic.",
                        "ru": "Ситуация, характеризующаяся большой активностью, скоплением людей или интенсивным движением."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bustle",
                                "target_lang_sense_clarification": "lively activity or movement"
                            },
                            {
                                "target_lang_word": "crowds",
                                "target_lang_sense_clarification": "large number of people"
                            },
                            {
                                "target_lang_word": "traffic",
                                "target_lang_sense_clarification": "congestion of vehicles"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "суматоха",
                                "target_lang_sense_clarification": "беспорядочное движение, суета"
                            },
                            {
                                "target_lang_word": "оживление",
                                "target_lang_sense_clarification": "активность, многолюдность (часто в позитивном контексте)"
                            },
                            {
                                "target_lang_word": "суета",
                                "target_lang_sense_clarification": "хлопоты, торопливое движение"
                            },
                            {
                                "target_lang_word": "толчея",
                                "target_lang_sense_clarification": "теснота от скопления людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "907d22c6-a344-345f-b634-7a51e783a8ef",
                    "sense_definition": "De toestand van veel te doen hebben of weinig tijd hebben.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "busyness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de <w>drukte</w> op mijn werk ben ik erg moe.",
                            "target_lang_translations": {
                                "en": "Due to the <w>busyness</w> at my work, I am very tired.",
                                "ru": "Я очень устал из-за <w>загруженности</w> на работе."
                            }
                        },
                        {
                            "text": "Vergeet in alle <w>drukte</w> niet om af en toe te ontspannen.",
                            "target_lang_translations": {
                                "en": "Do not forget to relax occasionally amidst all the <w>rush</w>.",
                                "ru": "Не забывай время от времени расслабляться в этой <w>суете</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in alle drukte",
                        "de dagelijkse drukte"
                    ],
                    "target_lang_definitions": {
                        "en": "The state of having a lot to do or having little time.",
                        "ru": "Состояние, когда у человека много дел или мало времени; загруженность."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "busyness",
                                "target_lang_sense_clarification": "state of being busy"
                            },
                            {
                                "target_lang_word": "rush",
                                "target_lang_sense_clarification": "hurried activity or pressure"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "загруженность",
                                "target_lang_sense_clarification": "большое количество работы или дел"
                            },
                            {
                                "target_lang_word": "занятость",
                                "target_lang_sense_clarification": "отсутствие свободного времени"
                            },
                            {
                                "target_lang_word": "спешка",
                                "target_lang_sense_clarification": "необходимость делать всё быстро"
                            }
                        ]
                    }
                }
            ]
        },

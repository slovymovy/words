                    "sense_id": "639024c1-b153-3f0d-bee9-88d6cae3d4f5",
                    "sense_definition": "An entry posted on the microblogging service Twitter.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "social_media_post",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I sent a <w>tweet</w> to my followers about the news.",
                            "target_lang_translations": {
                                "ru": "Я отправил <w>твит</w> своим подписчикам с этой новостью."
                            }
                        },
                        {
                            "text": "His funny <w>tweet</w> went viral within a few minutes.",
                            "target_lang_translations": {
                                "ru": "Его смешной <w>твит</w> стал вирусным за несколько минут."
                            }

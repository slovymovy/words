    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6b817a6b-6117-3ae6-b8ed-cc7925c6ad61",
                    "sense_definition": "A small, round spot or mark.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_mark",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wore a white dress with a black <w>dot</w> pattern.",
                            "target_lang_translations": {
                                "ru": "Она была в белом платье в черный <w>горошек</w>."
                            }
                        },
                        {
                            "text": "From the airplane, the cars looked like a tiny <w>dot</w> on the road.",
                            "target_lang_translations": {
                                "ru": "С самолета машины казались крошечной <w>точкой</w> на дороге."
                            }
                        }
                    ],
                    "synonyms": [
                        "spot",
                        "speck",
                        "fleck",
                        "point"
                    ],
                    "common_phrases": [
                        "polka dot",
                        "blue dot",
                        "small dot"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленькое круглое пятно или отметка."
                    },
                    "translations": {
                        "ru": [
                            {

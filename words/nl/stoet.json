    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a2529354-02cb-311b-a700-2f512e988df5",
                    "sense_definition": "Een groep mensen, dieren of voertuigen die zich in een rij voortbeweegt, vaak als onderdeel van een ceremonie of viering.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "procession_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De lange <w>stoet</w> trok langzaam door de versierde straten van het dorp.",
                            "target_lang_translations": {
                                "en": "The long <w>procession</w> moved slowly through the decorated streets of the village.",
                                "ru": "Длинная <w>процессия</w> медленно двигалась по украшенным улицам деревни."
                            }
                        },
                        {
                            "text": "We moesten even wachten voor het stoplicht omdat er een <w>stoet</w> voorbijkwam.",
                            "target_lang_translations": {
                                "en": "We had to wait at the traffic light because a <w>procession</w> was passing by.",
                                "ru": "Нам пришлось немного подождать на светофоре, так как мимо проезжал <w>кортеж</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "optocht",
                        "defilé"
                    ],
                    "common_phrases": [
                        "in een stoet",
                        "een lange stoet"
                    ],
                    "target_lang_definitions": {
                        "en": "A group of people, animals, or vehicles moving forward in a line, often as part of a ceremony or celebration.",
                        "ru": "Группа людей, животных или транспортных средств, движущихся друг за другом, часто как часть церемонии или празднования."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "procession",
                                "target_lang_sense_clarification": "general movement in a line"
                            },
                            {
                                "target_lang_word": "parade",
                                "target_lang_sense_clarification": "celebratory context"
                            },
                            {
                                "target_lang_word": "cortege",
                                "target_lang_sense_clarification": "funeral context"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "процессия",
                                "target_lang_sense_clarification": "торжественное шествие"
                            },
                            {
                                "target_lang_word": "кортеж",
                                "target_lang_sense_clarification": "вереница автомобилей, сопровождение"
                            },
                            {
                                "target_lang_word": "шествие",
                                "target_lang_sense_clarification": "организованное движение группы людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b70c6c0-4e75-34e1-952f-0a68bc0f0fbe",
                    "sense_definition": "Een grote hoeveelheid of reeks van personen of dingen die elkaar opvolgen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "procession_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er kwam een hele <w>stoet</w> aan deskundigen aan het woord tijdens de vergadering.",
                            "target_lang_translations": {
                                "en": "A whole <w>procession</w> of experts spoke during the meeting.",
                                "ru": "На собрании выступила целая <w>вереница</w> экспертов."
                            }
                        },
                        {
                            "text": "Na de wedstrijd volgde een <w>stoet</w> van analyses en nabeschouwingen op televisie.",
                            "target_lang_translations": {
                                "en": "After the match, a <w>stream</w> of analyses and reviews followed on television.",
                                "ru": "После матча по телевидению последовала <w>череда</w> анализов и обзоров."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een hele stoet"
                    ],
                    "target_lang_definitions": {
                        "en": "A large quantity or series of persons or things following one another.",
                        "ru": "Большое количество или череда людей или предметов, следующих друг за другом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stream",
                                "target_lang_sense_clarification": "continuous series"
                            },
                            {
                                "target_lang_word": "procession",
                                "target_lang_sense_clarification": "figurative series"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вереница",
                                "target_lang_sense_clarification": "длинный ряд движущихся объектов"
                            },
                            {
                                "target_lang_word": "череда",
                                "target_lang_sense_clarification": "непрерывная последовательность событий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1527cfa1-f8f3-3202-9535-e9f4af77d20d",

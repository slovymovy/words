                        {
                            "text": "Potatoes are cheap and they <w>fill</w> the stomach quickly.",
                            "target_lang_translations": {
                                "ru": "Картошка дешёвая и быстро <w>наполняет</w> желудок."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fill the stomach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снабжать едой до состояния сытости."

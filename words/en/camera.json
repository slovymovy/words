                    "sense_id": "43bb3a97-dd2a-38a3-a1c3-8c7e1b7e5923",
                    "sense_definition": "A device for taking still or moving pictures or photographs.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I used my new digital <w>camera</w> to take photos of the sunset.",
                            "target_lang_translations": {
                                "ru": "Я использовал свою новую цифровую <w>камеру</w>, чтобы сфотографировать закат."
                            }
                        },
                        {
                            "text": "Please look at the <w>camera</w> and smile for the picture.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, посмотрите в <w>камеру</w> и улыбнитесь для снимка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "digital camera",
                        "security camera",
                        "on camera",
                        "camera lens"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство для съемки неподвижных или движущихся изображений (фотографий или видео)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "камера",
                                "target_lang_sense_clarification": "общее название"
                            },
                            {
                                "target_lang_word": "фотоаппарат",
                                "target_lang_sense_clarification": "для фотосъемки"
                            },
                            {
                                "target_lang_word": "видеокамера",
                                "target_lang_sense_clarification": "для видеосъемки"
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7397f510-069b-3c8d-9c50-3384cdcffe1a",
                    "sense_definition": "A heavy metal object attached to a chain or rope and dropped from a boat into the water to prevent the boat from moving.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "nautical_tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The captain ordered the crew to drop <w>anchor</w> in the calm bay.",
                            "target_lang_translations": {
                                "ru": "Капитан приказал команде бросить <w>якорь</w> в спокойной бухте."
                            }
                        },
                        {
                            "text": "We waited at <w>anchor</w> until the storm passed.",
                            "target_lang_translations": {
                                "ru": "Мы стояли на <w>якоре</w>, пока шторм не прошел."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drop anchor",
                        "weigh anchor",
                        "at anchor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тяжелый металлический предмет, прикрепленный к цепи или веревке, который сбрасывают с лодки в воду, чтобы предотвратить движение лодки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "якорь",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a379f29c-f522-3f8e-8c81-fe9699938db3",

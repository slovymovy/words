    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0df55a47-660c-3615-844a-162caa3677bc",
                    "sense_definition": "Mający liczne sęki, nierówny i guzowaty (zwykle o drewnie lub roślinach).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Płot wokół domu był zrobiony z grubych, <w>sękatych</w> żerdzi.",
                            "target_lang_translations": {
                                "ru": "Забор вокруг дома был сделан из толстых, <w>суковатых</w> жердей."
                            }
                        },
                        {
                            "text": "W lesie znaleźliśmy stary, <w>sękaty</w> dąb, który wyglądał na bardzo wiekowy.",
                            "target_lang_translations": {
                                "ru": "В лесу мы нашли старый, <w>суковатый</w> дуб, который выглядел очень древним."
                            }
                        }
                    ],
                    "synonyms": [
                        "sęczasty",
                        "sękowaty"
                    ],
                    "antonyms": [
                        "gładki",
                        "bezsęczny"
                    ],
                    "common_phrases": [
                        "sękaty kij",
                        "sękaty pień",
                        "sękata deska"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий много сучков, неровный и бугристый (обычно о древесине или растениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "суковатый",
                                "target_lang_sense_clarification": "о дереве или досках"
                            },
                            {
                                "target_lang_word": "узловатый",
                                "target_lang_sense_clarification": "имеющий выступы или узлы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e213c1e-44fc-33fd-a971-eecde7e55aa4",
                    "sense_definition": "Mający zgrubienia, kościsty i powykręcany (zwykle o dłoniach lub palcach).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Starzec oparł swoje spracowane, <w>sękate</w> dłonie na drewnianym stole.",
                            "target_lang_translations": {
                                "ru": "Старик положил свои натруженные, <w>узловатые</w> ладони на деревянный стол."
                            }
                        },
                        {
                            "text": "Jej <w>sękate</w> palce z trudem utrzymywały delikatną filiżankę herbaty.",
                            "target_lang_translations": {
                                "ru": "Её <w>узловатые</w> пальцы с трудом удерживали хрупкую чашку чая."
                            }
                        }
                    ],
                    "synonyms": [
                        "chropowaty",
                        "zacięty"
                    ],
                    "antonyms": [
                        "gładki",
                        "delikatny"
                    ],
                    "common_phrases": [
                        "sękate dłonie",
                        "sękate palce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий утолщения, костистый и искривленный (обычно о руках или пальцах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "узловатый",
                                "target_lang_sense_clarification": "о пальцах или суставах"
                            },
                            {
                                "target_lang_word": "суковатый",
                                "target_lang_sense_clarification": "переносное значение; грубый, неровный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5757829c-ccde-341c-abe9-ecb354c25a4d",
                    "sense_definition": "Cechujący się uporem, nieustępliwością lub będący trudnym do rozwiązania.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "character_trait",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miał bardzo <w>sękaty</w> charakter i trudno było go do czegokolwiek przekonać.",
                            "target_lang_translations": {
                                "ru": "У него был очень <w>неуживчивый</w> характер, и его трудно было в чём-либо убедить."
                            }
                        },
                        {
                            "text": "To niezwykle <w>sękaty</w> problem, nad którym głowili się najlepsi eksperci.",
                            "target_lang_translations": {
                                "ru": "Это чрезвычайно <w>заковыристый</w> вопрос, над которым ломали голову лучшие эксперты."
                            }
                        }
                    ],
                    "synonyms": [
                        "zacięty"
                    ],
                    "antonyms": [
                        "ugodowy",
                        "łatwy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отличающийся упрямством, неуступчивостью или трудный для разрешения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неуживчивый",
                                "target_lang_sense_clarification": "о характере"
                            },
                            {
                                "target_lang_word": "заковыристый",
                                "target_lang_sense_clarification": "о сложной проблеме"
                            },
                            {
                                "target_lang_word": "трудный",
                                "target_lang_sense_clarification": "общий смысл"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "81f6658d-85e3-34b7-932c-03212ba8ca1b",
                    "sense_definition": "A device in an engine that increases power by forcing more air into the combustion chamber; short for turbocharger.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "mechanical_engine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mechanic installed a new <w>turbo</w> to boost the car's acceleration.",
                            "target_lang_translations": {
                                "ru": "Механик установил новую <w>турбину</w>, чтобы увеличить ускорение автомобиля."
                            }
                        },
                        {
                            "text": "Engines equipped with a <w>turbo</w> are generally more efficient than standard ones.",
                            "target_lang_translations": {
                                "ru": "Двигатели, оснащенные <w>турбонаддувом</w>, обычно более эффективны, чем стандартные."
                            }
                        }
                    ],
                    "synonyms": [
                        "turbocharger"
                    ],
                    "common_phrases": [
                        "twin turbo",
                        "turbo diesel"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "clipping of turbocharger"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство в двигателе, которое увеличивает мощность за счет нагнетания большего количества воздуха в камеру сгорания; сокращение от турбокомпрессор или турбонаддув."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "турбина",
                                "target_lang_sense_clarification": "разговорное название турбокомпрессора"
                            },
                            {
                                "target_lang_word": "турбонаддув",
                                "target_lang_sense_clarification": "система наддува двигателя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2bb34d0-e005-398b-a009-55e3e7f32a56",
                    "sense_definition": "A type of jet engine used in aircraft; short for turbojet.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mechanical_engine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The vintage aircraft was powered by an early <w>turbo</w> design.",
                            "target_lang_translations": {
                                "ru": "Этот старинный самолет приводился в движение ранней моделью <w>турбореактивного двигателя</w>."
                            }
                        },
                        {
                            "text": "Engineers studied the efficiency of the <w>turbo</w> at high altitudes.",
                            "target_lang_translations": {
                                "ru": "Инженеры изучали эффективность <w>турбореактивного двигателя</w> на больших высотах."
                            }
                        }
                    ],
                    "synonyms": [
                        "turbojet"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "clipping of turbojet"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тип реактивного двигателя, используемого в самолетах; сокращение от турбореактивный двигатель."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "турбореактивный двигатель",
                                "target_lang_sense_clarification": "тип авиационного двигателя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51328b66-d297-3f75-8f3d-d6f10a5d4656",
                    "sense_definition": "A feature on a video game controller that simulates rapid, repeated button presses automatically.",

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "49e19e3c-fc54-34af-87aa-271f88a555d4",
                    "sense_definition": "Stworzyć w umyśle obraz kogoś, czegoś lub jakiejś sytuacji; pomyśleć o czymś, co nie jest obecnie widoczne.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "mental_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie potrafię sobie <w>wyobrazić</w> życia bez dostępu do internetu.",
                            "target_lang_translations": {
                                "ru": "Я не могу <w>представить</w> себе жизнь без доступа к интернету."
                            }
                        },
                        {
                            "text": "Spróbuj sobie <w>wyobrazić</w>, jak pięknie będzie wyglądał ten dom po remoncie.",
                            "target_lang_translations": {
                                "ru": "Попробуй <w>представить</w> себе, как красиво будет выглядеть этот дом после ремонта."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyimaginować"
                    ],
                    "common_phrases": [
                        "wyobrazić sobie coś",
                        "trudno sobie wyobrazić",
                        "możesz sobie wyobrazić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создать в воображении образ кого-либо, чего-либо или какой-либо ситуации; подумать о чем-то, что в данный момент не видно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "представить",
                                "target_lang_sense_clarification": "создать мысленный образ"
                            },
                            {
                                "target_lang_word": "вообразить",
                                "target_lang_sense_clarification": "нафантазировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ebd5d3c-7739-3160-a8c1-6ad066a11be5",
                    "sense_definition": "Zdać sobie sprawę z czegoś, w pełni pojąć lub zrozumieć istotę jakiejś sytuacji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie mogę sobie <w>wyobrazić</w>, jak on zdołał naprawić ten silnik bez narzędzi.",
                            "target_lang_translations": {
                                "ru": "Я не могу <w>представить</w>, как он сумел починить этот двигатель без инструментов."
                            }
                        },
                        {
                            "text": "Musisz sobie <w>wyobrazić</w>, jak wielki stres towarzyszył jej podczas egzaminu.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>представить</w> себе, какой сильный стресс она испытывала во время экзамена."
                            }
                        }
                    ],
                    "synonyms": [
                        "zrozumieć",
                        "pojąć"
                    ],
                    "common_phrases": [
                        "nie sposób wyobrazić",
                        "nie móc wyobrazić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Осознать что-либо, в полной мере постичь или понять суть какой-либо ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "представить",
                                "target_lang_sense_clarification": "осознать масштаб или сложность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a098f34b-d7c3-3cf6-bcba-81e11fc884f9",
                    "sense_definition": "Przedstawić kogoś lub coś w dziele sztuki, na przykład na obrazie lub rzeźbie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "artistic_depiction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na swoim najnowszym obrazie malarz <w>wyobraził</w> scenę z mitologii greckiej.",
                            "target_lang_translations": {
                                "ru": "На своей новейшей картине художник <w>изобразил</w> сцену из греческой мифологии."
                            }
                        },
                        {
                            "text": "Ta gotycka rzeźba <w>wyobraża</w> króla siedzącego na tronie z mieczem w dłoni.",
                            "target_lang_translations": {
                                "ru": "Эта готическая скульптура <w>изображает</w> короля, сидящего на троне с мечом в руке."
                            }
                        }
                    ],
                    "synonyms": [
                        "przedstawić",
                        "zobrazować"
                    ],
                    "common_phrases": [
                        "wyobrazić na płótnie",
                        "artysta wyobraził"
                    ],
                    "traits": [
                        {

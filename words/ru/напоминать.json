    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e8e79b9f-cc75-3f53-9a23-0c9fe41dfc9c",
                    "sense_definition": "Обращаться к чьей-либо памяти, заставлять вспомнить о ком-либо или чём-либо",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не надо об этом <напоминать> по десять раз, мы всё помним.",
                            "target_lang_translations": {
                                "en": "There is no need to <remind> us about this ten times, we remember everything."
                            }
                        },
                        {
                            "text": "Я постоянно <напоминаю> сыну убирать в своей комнате.",
                            "target_lang_translations": {
                                "en": "I constantly <remind> my son to clean his room."
                            }
                        }
                    ],
                    "common_phrases": [
                        "напоминать о встрече",
                        "напоминать о себе",
                        "лишний раз напоминать"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause someone to remember someone or something; to bring to someone's attention"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "remind",
                                "target_lang_sense_clarification": "cause to remember"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24e66d22-6da2-3cc9-a408-5464b7ce6eff",
                    "sense_definition": "Быть похожим на кого-либо или что-либо, иметь сходство",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот маленький домик <напоминал> старинный замок.",
                            "target_lang_translations": {
                                "en": "This little house <resembled> an old castle."
                            }
                        },
                        {
                            "text": "Его поведение <напоминает> поступки маленького ребёнка.",
                            "target_lang_translations": {
                                "en": "His behavior <resembles> the actions of a small child."
                            }
                        }
                    ],
                    "synonyms": [
                        "походить",
                        "быть похожим"
                    ],
                    "common_phrases": [
                        "сильно напоминать",
                        "смутно напоминать",
                        "отдалённо напоминать"
                    ],
                    "target_lang_definitions": {
                        "en": "To be similar to someone or something; to have a resemblance"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "resemble",
                                "target_lang_sense_clarification": "be similar to"
                            },
                            {
                                "target_lang_word": "look like",
                                "target_lang_sense_clarification": "have the appearance of"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
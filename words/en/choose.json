    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8dfc5c55-b476-3738-bc35-a1e8ff6d2c40",
                    "sense_definition": "To pick out or select from a number of alternatives.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "selection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>chose</w> the red shirt because it matches my shoes."
                        },
                        {
                            "text": "Please <w>choose</w> a partner for the next activity."
                        }
                    ],
                    "synonyms": [
                        "pick",
                        "select",
                        "opt",
                        "single out"
                    ],
                    "antonyms": [
                        "refuse"
                    ],
                    "common_phrases": [
                        "choose between",
                        "choose from",
                        "choose carefully",
                        "hard to choose"
                    ]
                },
                {
                    "sense_id": "b6d6c610-aab0-37c6-a8fe-3cd117f81fce",
                    "sense_definition": "To decide to do something or act in a certain way.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "decision",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>chose</w> to stay home instead of going to the party."
                        },
                        {
                            "text": "We <w>choose</w> to believe that everything will work out."
                        }
                    ],
                    "synonyms": [
                        "decide",
                        "determine",
                        "settle"
                    ],
                    "antonyms": [
                        "compel",
                        "constrain"
                    ],
                    "common_phrases": [
                        "choose to ignore",
                        "choose to believe",
                        "choose not to"
                    ]
                },
                {
                    "sense_id": "67c6db85-c134-3a1d-98ff-80990fd07b57",
                    "sense_definition": "To select someone for a position or office, often by voting.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "selection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee <w>chose</w> a new chairman yesterday."
                        },
                        {
                            "text": "They will <w>choose</w> their representative in the upcoming election."
                        }
                    ],
                    "synonyms": [
                        "elect",
                        "appoint",
                        "vote",
                        "designate"
                    ],
                    "common_phrases": [
                        "chosen for office",
                        "choose a leader"
                    ]
                },
                {
                    "sense_id": "981eac3b-41f8-3769-aef0-6cc16778b5d0",
                    "sense_definition": "To want, prefer, or desire something.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "preference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You may do as you <w>choose</w> in this matter."
                        },
                        {
                            "text": "I do not <w>choose</w> to discuss my private life with strangers."
                        }
                    ],
                    "synonyms": [
                        "desire",
                        "prefer",
                        "favor",
                        "crave",
                        "embrace"
                    ],
                    "common_phrases": [
                        "as you choose",
                        "do as you choose"
                    ]
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "ff132410-1e25-3b02-9ca3-98feba17a5f4",
                    "sense_definition": "Used in mathematics to indicate the binomial coefficient, representing the number of ways to pick a subset of items.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "math_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The formula for the calculation is read as n <w>choose</w> k."
                        },
                        {
                            "text": "Five <w>choose</w> two equals ten distinct combinations."
                        }
                    ],
                    "common_phrases": [
                        "n choose k"
                    ]
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a7e86a6a-6762-3b94-9cee-0969854f97a9",
                    "sense_definition": "The act of choosing or the power to make a selection.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "noun_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He had no <w>choose</w> but to accept the offer."
                        },
                        {
                            "text": "It was a difficult <w>choose</w> to make at the time."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Standard English uses 'choice' as the noun; 'choose' as a noun is non-standard or dialectal."
                        }
                    ]

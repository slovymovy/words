{
    "word_family": [
        "использование",
        "пользователь",
        "польза",
        "воспользоваться",
        "пользоваться"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "68687f98-7a37-3531-ae30-a7a7a3557743",
                    "sense_definition": "Употреблять что-либо для какого-либо дела, применять средство или инструмент для достижения цели.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "general_use",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы планируем <использовать> этот материал для строительства нового загородного дома.",
                            "target_lang_translations": {
                                "en": "We plan to <use> this material for the construction of a new country house."
                            }
                        },
                        {
                            "text": "Студенты могут <использовать> словарь во время перевода сложного текста.",
                            "target_lang_translations": {
                                "en": "Students can <use> a dictionary while translating a complex text."
                            }
                        }
                    ],
                    "synonyms": [
                        "применять",
                        "употреблять",
                        "пользоваться"
                    ],
                    "common_phrases": [
                        "использовать возможность",
                        "использовать метод",
                        "широко использовать",
                        "использовать информацию"
                    ],
                    "target_lang_definitions": {
                        "en": "To employ something for a specific task; to apply a means or tool to achieve a goal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "use",
                                "target_lang_sense_clarification": "to employ for a purpose"
                            },
                            {
                                "target_lang_word": "utilize",
                                "target_lang_sense_clarification": "to make practical use of"
                            },
                            {
                                "target_lang_word": "apply",
                                "target_lang_sense_clarification": "to put into practice"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8aaed28-150c-3f2b-ad3a-a0e051694c4d",
                    "sense_definition": "Извлекать личную выгоду из кого-либо или чего-либо, часто манипулируя или действуя в своих интересах.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "exploitation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он привык <использовать> людей для достижения своих корыстных целей.",
                            "target_lang_translations": {
                                "en": "He is accustomed to <using> people to achieve his selfish goals."
                            }
                        },
                        {
                            "text": "Хитрый политик решил <использовать> сложившуюся ситуацию в своих интересах.",
                            "target_lang_translations": {
                                "en": "The cunning politician decided to <use> the current situation to his advantage."
                            }
                        }
                    ],
                    "synonyms": [
                        "эксплуатировать"
                    ],
                    "common_phrases": [
                        "использовать в своих интересах",
                        "использовать чужой труд",
                        "использовать момент",
                        "использовать слабости"
                    ],
                    "target_lang_definitions": {
                        "en": "To extract personal benefit from someone or something, often by manipulating or acting in one's own interest."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "use",
                                "target_lang_sense_clarification": "to manipulate or take advantage of"
                            },
                            {
                                "target_lang_word": "exploit",
                                "target_lang_sense_clarification": "to take unfair advantage of"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
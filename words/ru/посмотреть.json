    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "66a4a4bc-6108-3c80-8be3-09e8d86a4e1b",
                    "sense_definition": "Направить взгляд на кого-либо или что-либо, чтобы увидеть.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "visual_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я хочу <посмотреть> на эту красивую картину поближе.",
                            "target_lang_translations": {
                                "en": "I want to <take a look> at this beautiful painting more closely."
                            }
                        },
                        {
                            "text": "Учитель попросил ученика <посмотреть> на доску и прочитать задание.",
                            "target_lang_translations": {
                                "en": "The teacher asked the student to <look> at the board and read the assignment."
                            }
                        },
                        {
                            "text": "Не забудь <посмотреть> по сторонам, прежде чем переходить улицу.",
                            "target_lang_translations": {
                                "en": "Don't forget to <look> both ways before crossing the street."
                            }
                        }
                    ],
                    "synonyms": [
                        "взглянуть"
                    ],
                    "antonyms": [
                        "отвернуться"
                    ],
                    "common_phrases": [
                        "посмотреть в глаза",
                        "посмотреть на часы",
                        "посмотреть в окно",
                        "посмотреть по сторонам",
                        "посмотреть вверх"
                    ],
                    "target_lang_definitions": {
                        "en": "To direct one's gaze at someone or something in order to see."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "look",
                                "target_lang_sense_clarification": "to direct one's eyes"
                            },
                            {
                                "target_lang_word": "take a look",
                                "target_lang_sense_clarification": "to examine briefly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce43528b-5632-391c-bf1d-6fb4f831d217",
                    "sense_definition": "Провести время, наблюдая за чем-либо (фильмом, спектаклем, передачей), чтобы ознакомиться с содержанием.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "consume_media",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В выходные мы планируем <посмотреть> новый фильм в кинотеатре.",
                            "target_lang_translations": {
                                "en": "Over the weekend we plan to <watch> a new movie at the cinema."
                            }
                        },
                        {
                            "text": "Вечером после работы я люблю <посмотреть> новости по телевизору.",
                            "target_lang_translations": {
                                "en": "In the evening after work, I like to <watch> the news on TV."
                            }
                        },
                        {
                            "text": "Друзья собрались вместе, чтобы <посмотреть> футбольный матч.",
                            "target_lang_translations": {
                                "en": "Friends gathered together to <watch> a football match."
                            }
                        }
                    ],
                    "common_phrases": [
                        "посмотреть фильм",
                        "посмотреть телевизор",
                        "посмотреть новости",
                        "посмотреть спектакль",
                        "посмотреть сериал"
                    ],
                    "target_lang_definitions": {
                        "en": "To spend time observing something (a movie, play, broadcast) to engage with the content."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "watch",
                                "target_lang_sense_clarification": "to view media or entertainment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "130252fa-d7ff-3cfe-83e1-84eb5b92310d",
                    "sense_definition": "Найти информацию или проверить наличие чего-либо.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "search_check",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Если ты не знаешь это слово, нужно <посмотреть> его в словаре.",
                            "target_lang_translations": {
                                "en": "If you don't know this word, you need to <look> it <up> in the dictionary."
                            }
                        },
                        {
                            "text": "Пожалуйста, <посмотри> в интернете, когда открывается музей.",
                            "target_lang_translations": {
                                "en": "Please <check> on the internet when the museum opens."
                            }
                        },
                        {
                            "text": "Мама попросила сына <посмотреть>, есть ли в холодильнике молоко.",
                            "target_lang_translations": {
                                "en": "Mom asked her son to <see> if there is milk in the fridge."
                            }
                        }
                    ],
                    "synonyms": [
                        "взглянуть"
                    ],
                    "common_phrases": [
                        "посмотреть в словаре",
                        "посмотреть в интернете",
                        "посмотреть расписание",
                        "посмотреть документы"
                    ],
                    "target_lang_definitions": {
                        "en": "To find information or verify the presence of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "check",
                                "target_lang_sense_clarification": "to verify or confirm"
                            },
                            {
                                "target_lang_word": "look up",
                                "target_lang_sense_clarification": "to search for information"
                            },
                            {
                                "target_lang_word": "see",
                                "target_lang_sense_clarification": "to determine a fact"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
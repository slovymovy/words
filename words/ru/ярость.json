    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "701ceda9-f380-3ca4-8156-31a1d6dcb14c",
                    "sense_definition": "Сильный, неукротимый гнев; состояние крайнего раздражения или бешенства.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отец пришёл в <w>ярость</w>, когда узнал о случившемся.",
                            "target_lang_translations": {
                                "en": "Father flew into a <w>rage</w> when he found out what happened."
                            }
                        },
                        {
                            "text": "Она с трудом сдерживала <w>ярость</w>, слушая его оправдания.",
                            "target_lang_translations": {
                                "en": "She barely held back her <w>fury</w> while listening to his excuses."
                            }
                        }
                    ],
                    "synonyms": [
                        "гнев",
                        "бешенство",
                        "неистовство",
                        "злость"
                    ],
                    "antonyms": [
                        "спокойствие",
                        "умиротворение",
                        "невозмутимость"
                    ],
                    "common_phrases": [
                        "впасть в ярость",
                        "прийти в ярость",
                        "довести до ярости",
                        "слепая ярость",
                        "в пылу ярости"
                    ],
                    "target_lang_definitions": {
                        "en": "Strong, uncontrollable anger; a state of extreme irritation or frenzy."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rage",
                                "target_lang_sense_clarification": "violent, uncontrolled anger"
                            },
                            {
                                "target_lang_word": "fury",
                                "target_lang_sense_clarification": "wild or violent anger"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c37f1bc1-4094-3467-a9d8-d734868c2707",
                    "sense_definition": "Необычайная сила, напряжение, интенсивность проявления чего-либо (часто о природных стихиях, звуках или действиях).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "intensity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Путников испугала внезапная <w>ярость</w> зимней бури.",
                            "target_lang_translations": {
                                "en": "The travelers were frightened by the sudden <w>fury</w> of the winter storm."
                            }
                        },
                        {
                            "text": "Ветер дул с такой <w>яростью</w>, что ломал деревья.",
                            "target_lang_translations": {
                                "en": "The wind blew with such <w>violence</w> that it broke trees."
                            }
                        }
                    ],
                    "synonyms": [
                        "сила",
                        "мощь",
                        "напор",
                        "буйство"
                    ],
                    "antonyms": [
                        "затишье",
                        "покой",
                        "слабость"
                    ],
                    "common_phrases": [
                        "ярость стихии",
                        "ярость ветра",
                        "ярость атаки",
                        "с яростью"
                    ],
                    "target_lang_definitions": {
                        "en": "Extraordinary force, tension, or intensity (often regarding natural elements, sounds, or actions)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fury",
                                "target_lang_sense_clarification": "intense force of elements"
                            },
                            {
                                "target_lang_word": "violence",
                                "target_lang_sense_clarification": "great physical force or intensity"
                            }
                        ]
                    }
                }
            ]
        }
    ]

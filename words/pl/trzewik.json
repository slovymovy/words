    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ce6d676f-70e2-3461-be4d-e5e28dced0e7",
                    "sense_definition": "Rodzaj obuwia z cholewką sięgającą kostki, zwykle sznurowanego i na twardej podeszwie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "footwear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Miała na nogach czerwone <w>trzewiki</w> z klamrami.",
                            "target_lang_translations": {
                                "ru": "На ногах у нее были красные <w>ботинки</w> с пряжками."
                            }
                        },
                        {
                            "text": "Do pracy w ogrodzie dziadek zawsze zakładał stare, skórzane <w>trzewiki</w>.",
                            "target_lang_translations": {
                                "ru": "Для работы в саду дедушка всегда надевал старые кожаные <w>ботинки</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Śląsk Cieszyński) strzewik"
                    ],
                    "common_phrases": [
                        "skórzane trzewiki",
                        "sznurowane trzewiki",
                        "robocze trzewiki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид обуви с голенищем, доходящим до щиколотки, обычно на шнуровке и жесткой подошве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ботинок",
                                "target_lang_sense_clarification": "обувь, закрывающая лодыжку"
                            },
                            {
                                "target_lang_word": "башмак",
                                "target_lang_sense_clarification": "разговорное или устаревшее название обуви"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a22614b-43ca-33b0-aa7f-0d4dc1f71a5e",
                    "sense_definition": "Element zbroi płytowej osłaniający stopę.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "armor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rycerz założył ciężki <w>trzewik</w>, aby chronić stopę podczas walki.",
                            "target_lang_translations": {
                                "ru": "Рыцарь надел тяжелый <w>латный ботинок</w>, чтобы защитить стопу во время боя."
                            }
                        },
                        {
                            "text": "W muzealnej gablocie lśnił stalowy <w>trzewik</w> należący do średniowiecznej zbroi.",
                            "target_lang_translations": {
                                "ru": "В музейной витрине блестел стальной <w>латный ботинок</w>, принадлежавший средневековым доспехам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stalowy trzewik",
                        "płytowy trzewik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элемент латных доспехов, защищающий стопу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "латный ботинок",
                                "target_lang_sense_clarification": "часть доспеха"
                            },
                            {
                                "target_lang_word": "сабатон",
                                "target_lang_sense_clarification": "исторический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "d133af4e-6ef5-3fb4-9f05-97f1791ae1f6",
                    "sense_definition": "Het niveau of de hoogte ter hoogte van de schouders van een persoon.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op sommige plaatsen in de stad staat het water tot <w>schouderhoogte</w>.",
                            "target_lang_translations": {
                                "ru": "В некоторых местах города вода доходит до <w>уровня плеч</w>."
                            }
                        },
                        {
                            "text": "Tijdens de oefening moet je de gewichten langzaam tot <w>schouderhoogte</w> brengen.",
                            "target_lang_translations": {
                                "ru": "Во время упражнения нужно медленно поднимать веса до <w>уровня плеч</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op schouderhoogte",
                        "tot schouderhoogte"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уровень или высота, соответствующая положению плеч человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уровень плеч",
                                "target_lang_sense_clarification": "высота"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d6c22d9-2f47-38d5-b4eb-e43e68024140",
                    "sense_definition": "De afstand van de grond tot de bovenkant van de schouders bij een dier dat op vier poten loopt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>schouderhoogte</w> van dit hondenras is ongeveer zestig centimeter.",
                            "target_lang_translations": {
                                "ru": "<w>Высота в холке</w> этой породы собак составляет около шестидесяти сантиметров."
                            }
                        },
                        {
                            "text": "Bij de keuring wordt de <w>schouderhoogte</w> van het paard nauwkeurig opgemeten.",
                            "target_lang_translations": {
                                "ru": "На осмотре точно измеряется <w>высота в холке</w> лошади."
                            }
                        }
                    ],
                    "synonyms": [
                        "schofthoogte",
                        "stokmaat"
                    ],
                    "common_phrases": [
                        "gemiddelde schouderhoogte"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расстояние от земли до верхней точки плеча (холки) у четвероногого животного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высота в холке",
                                "target_lang_sense_clarification": "у животных"
                            }
                        ]
                    }
                }
            ]
        }
    ]

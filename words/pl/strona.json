                {
                    "sense_id": "194cf9c6-c25c-3c2e-b0a9-ef055de0ca9b",
                    "sense_definition": "Jedna z dwóch powierzchni kartki papieru w książce, zeszycie, gazecie lub dokumencie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "paper_content",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę otworzyć podręcznik na dziesiątej <w>stronie</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, откройте учебник на десятой <w>странице</w>."
                            }
                        },
                        {
                            "text": "Przeczytałem już ponad sto <w>stron</w> tej powieści.",
                            "target_lang_translations": {
                                "ru": "Я прочитал уже более ста <w>страниц</w> этого романа."
                            }
                        }
                    ],
                    "synonyms": [
                        "stronica",
                        "kartka",
                        "s.",
                        "str."
                    ],
                    "common_phrases": [
                        "numer strony",
                        "tytułowa strona",
                        "przewrócić stronę",
                        "żółte strony"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из двух сторон листа бумаги в книге, тетради, газете или документе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страница",
                                "target_lang_sense_clarification": "лист в книге или документе"

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "422002dc-25a3-3353-9c42-8c3787d8a51e",
                    "sense_definition": "The borders or limits of a specific area or space.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "limits_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The prisoners were not allowed to leave the <w>confines</w> of the camp.",
                            "target_lang_translations": {
                                "ru": "Заключенным не разрешалось покидать <w>пределы</w> лагеря."
                            }
                        },
                        {
                            "text": "She felt safe and secure within the <w>confines</w> of her own home.",
                            "target_lang_translations": {
                                "ru": "Она чувствовала себя в безопасности в <w>стенах</w> своего собственного дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "boundaries",
                        "borders",
                        "limits"
                    ],
                    "common_phrases": [
                        "within the confines",
                        "narrow confines"
                    ],
                    "target_lang_definitions": {
                        "ru": "Границы или пределы определенной территории или пространства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пределы",
                                "target_lang_sense_clarification": "границы пространства"
                            },
                            {
                                "target_lang_word": "границы",
                                "target_lang_sense_clarification": "физические рубежи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc47718d-89f1-348d-ac0c-afd0854fb9ff",
                    "sense_definition": "Restrictions or limitations that restrain someone or something.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "limits_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He wanted to escape the <w>confines</w> of his boring daily routine.",
                            "target_lang_translations": {
                                "ru": "Он хотел вырваться из <w>рамок</w> своей скучной повседневной рутины."
                            }
                        },
                        {
                            "text": "Art should not always be limited by the <w>confines</w> of tradition.",
                            "target_lang_translations": {
                                "ru": "Искусство не всегда должно ограничиваться <w>рамками</w> традиций."
                            }
                        }
                    ],
                    "synonyms": [
                        "restrictions",
                        "constraints",
                        "limitations"
                    ],
                    "common_phrases": [
                        "escape the confines",
                        "social confines"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ограничения или рамки, которые сдерживают кого-то или что-то (часто в переносном смысле)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рамки",
                                "target_lang_sense_clarification": "абстрактные ограничения"
                            },
                            {
                                "target_lang_word": "оковы",
                                "target_lang_sense_clarification": "то, что сковывает свободу (перен.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca6372d4-d5bb-349f-a7ea-0178d9a8afae",
                    "sense_definition": "The scope or range of a particular subject or field.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "limits_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This specific discussion falls outside the <w>confines</w> of our current topic.",
                            "target_lang_translations": {
                                "ru": "Эта конкретная дискуссия выходит за <w>рамки</w> нашей текущей темы."
                            }
                        },
                        {
                            "text": "The research was kept strictly within the <w>confines</w> of the original theory.",
                            "target_lang_translations": {
                                "ru": "Исследование проводилось строго в <w>рамках</w> первоначальной теории."
                            }
                        }
                    ],
                    "synonyms": [
                        "scope",
                        "range",
                        "bounds"
                    ],
                    "common_phrases": [
                        "beyond the confines",
                        "outside the confines"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сфера охвата или пределы определенной темы или области знаний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рамки",
                                "target_lang_sense_clarification": "контекст или сфера"
                            },
                            {
                                "target_lang_word": "границы",
                                "target_lang_sense_clarification": "пределы допустимого или изучаемого"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "977337bc-8f65-3d8c-8bd5-690e8d348fe0",
                    "sense_definition": "Third-person singular simple present indicative of <w>confine</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The severe illness <w>confines</w> him to his bed for most of the day.",
                            "target_lang_translations": {
                                "ru": "Тяжелая болезнь <w>приковывает</w> его к постели большую часть дня."
                            }
                        },
                        {
                            "text": "She usually <w>confines</w> her remarks to the most important facts.",
                            "target_lang_translations": {
                                "ru": "Она обычно <w>ограничивает</w> свои замечания наиболее важными фактами."
                            }
                        }
                    ],
                    "synonyms": [
                        "restricts",
                        "limits",
                        "imprisons"
                    ],
                    "antonyms": [
                        "frees",
                        "releases",
                        "liberates"
                    ],
                    "common_phrases": [
                        "confines to",
                        "strictly confines"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb inflection"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма глагола confine (ограничивать) в 3-м лице единственного числа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ограничивает",
                                "target_lang_sense_clarification": "ставит в рамки"
                            },
                            {
                                "target_lang_word": "приковывает",
                                "target_lang_sense_clarification": "лишает возможности двигаться (напр. болезнь)"
                            }
                        ]
                    }
                }
            ]
        }

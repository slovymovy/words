            "pos": "noun",
            "senses": [
                {
                    "sense_id": "efff1bba-d8ea-300c-82f9-063a0f8d134b",
                    "sense_definition": "Wynagrodzenie pieniężne otrzymywane za jeden przepracowany dzień.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "money",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Andrzej przepuścił całą <w>dniówkę</w>, grając na automatach.",
                            "target_lang_translations": {
                                "ru": "Анджей спустил весь <w>дневной заработок</w>, играя на автоматах."
                            }
                        },
                        {
                            "text": "Szef obiecał, że wypłaci mi zaległą <w>dniówkę</w> jeszcze w tym tygodniu.",
                            "target_lang_translations": {
                                "ru": "Шеф обещал, что выплатит мне задержанный <w>дневной заработок</w> еще на этой неделе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cała dniówka",
                        "stracić dniówkę",
                        "wysokość dniówki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежное вознаграждение за один отработанный день."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дневной заработок",
                                "target_lang_sense_clarification": "сумма денег"
                            },
                            {
                                "target_lang_word": "поденная оплата",
                                "target_lang_sense_clarification": "форма оплаты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6735f330-f071-3426-83a6-b516eec515c8",
                    "sense_definition": "Jeden dzień spędzony na pracy zarobkowej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam już za sobą cztery <w>dniówki</w> w tej pracy i na razie nie narzekam.",
                            "target_lang_translations": {
                                "ru": "У меня уже позади четыре <w>смены</w> на этой работе, и пока я не жалуюсь."
                            }
                        },
                        {
                            "text": "Muszę wziąć wolne w piątek, więc przepadnie mi jedna <w>dniówka</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно взять выходной в пятницу, так что у меня пропадет один <w>рабочий день</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dzień roboczy"
                    ],
                    "common_phrases": [
                        "odrobić dniówkę",
                        "ciężka dniówka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Один день, проведенный на оплачиваемой работе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рабочий день",
                                "target_lang_sense_clarification": "единица времени"
                            },
                            {
                                "target_lang_word": "смена",
                                "target_lang_sense_clarification": "период работы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "356655d0-7f79-3ea0-940f-3a9549d413cf",
                    "sense_definition": "System zatrudnienia lub płacy, w którym rozliczenie następuje za każdy dzień osobno.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zenek nie ma stałej pracy, pracuje na <w>dniówki</w> po różnych budowach.",
                            "target_lang_translations": {
                                "ru": "У Зенека нет постоянной работы, он работает <w>поденно</w> на разных стройках."
                            }
                        },
                        {
                            "text": "W sezonie letnim wielu studentów zatrudnia się na <w>dniówki</w> przy zbiorach owoców.",
                            "target_lang_translations": {
                                "ru": "В летний сезон многие студенты нанимаются на <w>поденную работу</w> на сбор фруктов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pracować na dniówki",
                        "płatne dniówki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система найма или оплаты, при которой расчет производится за каждый день отдельно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поденная работа",
                                "target_lang_sense_clarification": "тип занятости"
                            },
                            {
                                "target_lang_word": "поденщина",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        }
    ]

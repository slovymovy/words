            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0ab966a8-3508-3e0e-81d4-a19dc00c7cc4",
                    "sense_definition": "Sprawiać, że ktoś lub coś znajdzie się wewnątrz jakiegoś pomieszczenia lub obiektu; wchodzić z kimś do środka.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz uprzejmie <wprowadzał> gości do salonu, wskazując im miejsca.",
                            "target_lang_translations": {
                                "ru": "Хозяин вежливо <вводил> гостей в гостиную, указывая им места."
                            }
                        },
                        {
                            "text": "Nie wolno <wprowadzać> psów do tego sklepu spożywczego.",
                            "target_lang_translations": {
                                "ru": "Нельзя <вводить> собак в этот продуктовый магазин."
                            }
                        }
                    ],
                    "synonyms": [
                        "wnosić"
                    ],
                    "antonyms": [
                        "wyprowadzać"
                    ],
                    "common_phrases": [
                        "wprowadzać gości",
                        "wprowadzać samochód",
                        "wprowadzać psa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять или помогать кому-то войти внутрь помещения; вести кого-то или что-то внутрь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вводить",
                                "target_lang_sense_clarification": "помогать войти"
                            },
                            {
                                "target_lang_word": "заводить",
                                "target_lang_sense_clarification": "вести внутрь (часто о животных)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd33c672-59a7-354e-8b5e-9dac18499f8d",
                    "sense_definition": "Zamieszkiwać w nowym miejscu; zajmować nowe mieszkanie lub dom (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "living_arrangement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi nowi sąsiedzi zaczęli <wprowadzać> się do bloku wczoraj wieczorem.",
                            "target_lang_translations": {
                                "ru": "Мои новые соседи начали <въезжать> в дом вчера вечером."
                            }
                        },
                        {
                            "text": "Kiedy zamierzasz <wprowadzać> się do swojego nowego pokoju w akademiku?",
                            "target_lang_translations": {
                                "ru": "Когда ты собираешься <вселяться> в свою новую комнату в общежитии?"
                            }
                        }
                    ],
                    "antonyms": [
                        "wyprowadzać się"
                    ],
                    "common_phrases": [
                        "wprowadzać się do mieszkania",
                        "wprowadzać się do domu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поселяться в новом месте жительства; занимать новую квартиру или дом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вселяться",
                                "target_lang_sense_clarification": "занимать жилье"
                            },
                            {
                                "target_lang_word": "въезжать",
                                "target_lang_sense_clarification": "переезжать в новое место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7fc7ede-32ab-39f2-ab77-6ab91fa04a93",
                    "sense_definition": "Zaczynać stosować coś nowego, na przykład prawo, system, modę lub produkt; udostępniać coś do powszechnego użytku.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "implementation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rząd planuje <wprowadzać> nowe przepisy podatkowe od przyszłego roku.",
                            "target_lang_translations": {
                                "ru": "Правительство планирует <вводить> новые налоговые правила со следующего года."
                            }
                        },
                        {
                            "text": "Firma chce <wprowadzić> na rynek innowacyjny model telefonu komórkowego.",
                            "target_lang_translations": {
                                "ru": "Компания хочет <вывести> на рынок инновационную модель мобильного телефона."
                            }
                        }
                    ],
                    "synonyms": [
                        "wnosić",
                        "wtajemniczać"
                    ],
                    "antonyms": [
                        "wyprowadzać"
                    ],
                    "common_phrases": [
                        "wprowadzać zmiany",
                        "wprowadzać w życie",
                        "wprowadzać na rynek",
                        "wprowadzać zasady"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начинать применять что-то новое (закон, систему) или выпускать продукт."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вводить",
                                "target_lang_sense_clarification": "законы, правила"
                            },
                            {
                                "target_lang_word": "внедрять",
                                "target_lang_sense_clarification": "технологии, системы"
                            },
                            {
                                "target_lang_word": "выводить",
                                "target_lang_sense_clarification": "на рынок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c81ab873-caa5-3959-b93b-ff1456f51aa8",
                    "sense_definition": "Zapisywać dane, tekst lub informacje w systemie komputerowym, rejestrze lub dokumencie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "data_input",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz poprawnie <wprowadzić> hasło, aby odblokować swój komputer.",
                            "target_lang_translations": {
                                "ru": "Ты должен правильно <ввести> пароль, чтобы разблокировать свой компьютер."
                            }
                        },
                        {
                            "text": "Sekretarka codziennie musi <wprowadzać> setki faktur do systemu księgowego.",
                            "target_lang_translations": {
                                "ru": "Секретарь должна ежедневно <вводить> сотни счетов в бухгалтерскую систему."
                            }

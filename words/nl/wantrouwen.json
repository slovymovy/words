    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6aa901c2-2479-3221-883c-611e50f840ac",
                    "sense_definition": "Geen vertrouwen hebben in iemand of iets; vermoeden dat iemand oneerlijk is of slechte bedoelingen heeft.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De burgers <w>wantrouwen</w> de beloften die de politici tijdens de verkiezingen hebben gedaan.",
                            "target_lang_translations": {
                                "ru": "Граждане <w>не доверяют</w> обещаниям, которые политики дали во время выборов."
                            }
                        },
                        {
                            "text": "<w>Wantrouw</w> e-mails met vreemde bijlagen van afzenders die je niet kent.",
                            "target_lang_translations": {
                                "ru": "<w>Не доверяй</w> электронным письмам со странными вложениями от отправителей, которых ты не знаешь."
                            }
                        }
                    ],
                    "antonyms": [
                        "vertrouwen"
                    ],
                    "common_phrases": [
                        "iemand wantrouwen",
                        "de overheid wantrouwen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не доверять кому-либо или чему-либо; подозревать кого-то в нечестности или плохих намерениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не доверять",
                                "target_lang_sense_clarification": "испытывать отсутствие доверия"
                            },
                            {
                                "target_lang_word": "относиться с недоверием",
                                "target_lang_sense_clarification": "проявлять осторожность и сомнение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "943360cc-7a4c-3abd-8773-345998343b1e",
                    "sense_definition": "Een gevoel van onzekerheid over de eerlijkheid, oprechtheid of betrouwbaarheid van iemand of iets.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er heerst een groot <w>wantrouwen</w> tussen de twee partijen na de mislukte onderhandelingen.",
                            "target_lang_translations": {
                                "ru": "После провалившихся переговоров между двумя сторонами царит сильное <w>недоверие</w>."
                            }
                        },
                        {
                            "text": "Zijn <w>wantrouwen</w> bleek uiteindelijk ongegrond te zijn toen de waarheid aan het licht kwam.",
                            "target_lang_translations": {
                                "ru": "Его <w>недоверие</w> в конечном итоге оказалось необоснованным, когда правда вышла наружу."
                            }
                        }
                    ],
                    "synonyms": [
                        "achterdocht",
                        "argwaan"
                    ],
                    "antonyms": [
                        "vertrouwen"
                    ],
                    "common_phrases": [
                        "gezond wantrouwen",
                        "diep wantrouwen",
                        "wederzijds wantrouwen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство неуверенности в честности, искренности или надежности кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "недоверие",
                                "target_lang_sense_clarification": "отсутствие доверия"
                            },
                            {
                                "target_lang_word": "подозрительность",
                                "target_lang_sense_clarification": "склонность видеть в чем-то обман"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
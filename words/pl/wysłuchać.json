            "senses": [
                {
                    "sense_id": "0550b47f-7a51-35c6-ba23-4587d1eaa043",
                    "sense_definition": "Pozwolić komuś wypowiedzieć się do końca i zapoznać się z jego problemami lub racjami",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę, nie przerywaj mi teraz i <wysłuchaj> mnie do końca.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не перебивай меня сейчас и <выслушай> меня до конца."
                            }
                        },
                        {
                            "text": "Mój przyjaciel cierpliwie <wysłuchał> moich problemów i udzielił mi wsparcia.",
                            "target_lang_translations": {
                                "ru": "Мой друг терпеливо <выслушал> рассказ о моих проблемах и поддержал меня."
                            }
                        }
                    ],
                    "antonyms": [
                        "zignorować",
                        "zlekceważyć"
                    ],
                    "common_phrases": [
                        "wysłuchać rady",
                        "wysłuchać prośby",
                        "wysłuchać zwierzeń",
                        "wysłuchać do końca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Позволить кому-то высказаться до конца, вникнуть в его проблемы или доводы"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выслушать",
                                "target_lang_sense_clarification": "дать кому-то высказаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "de8dd51c-2062-3411-96f8-2b7a584e2393",
                    "sense_definition": "Zapoznać się z treścią czegoś słuchając tego od początku do końca",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj wieczorem z przyjemnością <wysłuchałem> ciekawej audycji radiowej.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером я с удовольствием <прослушал> интересную радиопередачу."
                            }
                        },
                        {
                            "text": "Wszyscy zgromadzeni na placu w ciszy <wysłuchali> hymnu narodowego.",
                            "target_lang_translations": {
                                "ru": "Все собравшиеся на площади в тишине <выслушали> национальный гимн."
                            }
                        }
                    ],
                    "synonyms": [
                        "przesłuchać"
                    ],
                    "common_phrases": [
                        "wysłuchać audycji",
                        "wysłuchać wykładu",
                        "wysłuchać koncertu",
                        "wysłuchać wiadomości"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ознакомиться с содержанием чего-либо, слушая это от начала до конца"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прослушать",
                                "target_lang_sense_clarification": "ознакомиться с аудиоматериалом"
                            },
                            {
                                "target_lang_word": "выслушать",
                                "target_lang_sense_clarification": "внимательно прослушать до конца"
                            }
                        ]
                    }
                }
            ]
        }
    ]

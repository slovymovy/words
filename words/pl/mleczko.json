{
    "word_family": [
        "mleko",
        "mleczny",
        "mlecz",
        "mleczarnia",
        "mleczarz",
        "mleczak"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0183a655-f7e3-3840-8942-0bf5102ece74",
                    "sense_definition": "Zdrobnienie od <w>mleko</w>; biały płyn spożywczy, często w kontekście żywienia dzieci lub jako dodatek do kawy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food_drink",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Małe dziecko wypiło całe ciepłe <mleczko> ze swojej butelki.",
                            "target_lang_translations": {
                                "ru": "Маленький ребенок выпил все теплое <молочко> из своей бутылочки."
                            }
                        },
                        {
                            "text": "Czy chciałbyś dolać sobie odrobinę <mleczka> do czarnej kawy?",
                            "target_lang_translations": {
                                "ru": "Хотел бы ты добавить немного <молочка> в черный кофе?"
                            }
                        }
                    ],
                    "synonyms": [
                        "gw. (Śląsk Cieszyński) mlycziczko"
                    ],
                    "common_phrases": [
                        "ciepłe mleczko",
                        "pić mleczko",
                        "mleczko do kawy"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительно-ласкательная форма от <w>mleko</w>; белый питательный напиток, часто в контексте детского питания или как добавка к кофе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молочко",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56407556-62bd-3d40-afd3-c699d57ac899",
                    "sense_definition": "Biały, gęsty płyn kosmetyczny używany do czyszczenia, demakijażu lub nawilżania skóry.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cosmetics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie wieczorem używam łagodnego <mleczka> do zmywania makijażu.",
                            "target_lang_translations": {
                                "ru": "Каждый вечер я использую мягкое <молочко> для снятия макияжа."
                            }
                        },
                        {
                            "text": "To nawilżające <mleczko> do ciała bardzo szybko się wchłania.",
                            "target_lang_translations": {
                                "ru": "Это увлажняющее <молочко> для тела очень быстро впитывается."
                            }
                        },
                        {
                            "text": "Kupiłam nowe <mleczko> kosmetyczne, które doskonale oczyszcza twarz.",
                            "target_lang_translations": {
                                "ru": "Я купила новое косметическое <молочко>, которое отлично очищает лицо."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mleczko do demakijażu",
                        "mleczko do ciała",
                        "mleczko kosmetyczne",
                        "mleczko nawilżające"
                    ],
                    "target_lang_definitions": {
                        "ru": "Белая густая косметическая жидкость, используемая для очищения, снятия макияжа или увлажнения кожи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молочко",
                                "target_lang_sense_clarification": "косметическое средство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "080e3199-6e4b-3cae-bb8b-deeb24f0871d",
                    "sense_definition": "Biały, lepki sok wydzielany przez niektóre grzyby lub rośliny po ich uszkodzeniu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "biology_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po zerwaniu łodygi mniszka lekarskiego na dłoni zostało mi białe <mleczko>.",
                            "target_lang_translations": {
                                "ru": "После того как я сорвал стебель одуванчика, на ладони осталось белое <молочко>."
                            }
                        },
                        {
                            "text": "Niektóre gatunki grzybów wydzielają piekące <mleczko> po przekrojeniu.",
                            "target_lang_translations": {
                                "ru": "Некоторые виды грибов выделяют жгучее <молочко> на срезе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "białe mleczko",
                        "mleczko kauczukowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Белый липкий сок, выделяемый некоторыми грибами или растениями при их повреждении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "млечный сок",
                                "target_lang_sense_clarification": "ботанический термин"
                            },
                            {
                                "target_lang_word": "молочко",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "868958c2-f5ec-33ea-a0ce-6d2e4a78dc3a",
                    "sense_definition": "Wydzielina z wola niektórych ptaków, np. gołębi, służąca do karmienia młodych.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "biology_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gołębie karmią swoje pisklęta pożywnym <mleczkiem> produkowanym w wolu.",
                            "target_lang_translations": {
                                "ru": "Голуби кормят своих птенцов питательным <молочком>, вырабатываемым в зобе."
                            }
                        },
                        {
                            "text": "To specyficzne ptasie <mleczko> zawiera dużo białka i tłuszczu dla młodych.",
                            "target_lang_translations": {
                                "ru": "Это специфическое птичье <молочко> содержит много белка и жира для птенцов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ptasie mleczko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Секрет зоба некоторых птиц, например голубей, служащий для выкармливания птенцов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "птичье молочко",
                                "target_lang_sense_clarification": "питательная жидкость птиц"
                            },
                            {
                                "target_lang_word": "зобное молочко",
                                "target_lang_sense_clarification": "биологический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "2f162934-5ac6-35b0-b12f-b1e8723bb909",
                    "sense_definition": "Polskie nazwisko (męskie lub żeńskie).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "names",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Andrzej <Mleczko> jest jednym z najbardziej znanych polskich rysowników.",
                            "target_lang_translations": {
                                "ru": "Анджей <Млечко> — один из самых известных польских художников-карикатуристов."
                            }
                        },
                        {
                            "text": "Wczoraj w urzędzie rozmawiałem z panią <Mleczko> o moich dokumentach.",
                            "target_lang_translations": {
                                "ru": "Вчера в ведомстве я разговаривал с госпожой <Млечко> о моих документах."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская фамилия (мужская или женская)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Млечко",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
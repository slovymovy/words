{
    "word_family": [
        "melisowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "220388af-1bc1-3a0a-af57-596db0189f91",
                    "sense_definition": "Napar sporządzony z liści rośliny o tej samej nazwie, pity zazwyczaj w celach uspokajających lub dla smaku.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "beverage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed snem zawsze piję gorącą <w>melisę</w>, żeby się wyciszyć.",
                            "target_lang_translations": {
                                "ru": "Перед сном я всегда пью горячую <w>мелиссу</w>, чтобы успокоиться."
                            }
                        },
                        {
                            "text": "Jeśli się denerwujesz, zaparzę ci <w>melisę</w> z dodatkiem miodu.",
                            "target_lang_translations": {
                                "ru": "Если ты нервничаешь, я заварю тебе <w>мелиссу</w> с медом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pić melisę",
                        "parzyć melisę",
                        "szklanka melisy",
                        "melisa na uspokojenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Настой из листьев одноименного растения, который пьют для успокоения или ради вкуса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мелисса",
                                "target_lang_sense_clarification": "напиток"
                            },
                            {
                                "target_lang_word": "чай с мелиссой",
                                "target_lang_sense_clarification": "напиток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f48a99e-3436-3e69-b2b8-52178f2f22e1",
                    "sense_definition": "Roślina zielna o liściach pachnących cytryną, uprawiana jako zioło lecznicze i przyprawowe.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W moim ogrodzie rośnie bujna <w>melisa</w>, która pięknie pachnie.",
                            "target_lang_translations": {
                                "ru": "В моем саду растет пышная <w>мелисса</w>, которая прекрасно пахнет."
                            }
                        },
                        {
                            "text": "Moja babcia zbiera <w>melisę</w> latem, aby ususzyć ją na zimowe herbatki.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка собирает <w>мелиссу</w> летом, чтобы насушить её для зимнего чая."
                            }
                        }
                    ],
                    "synonyms": [
                        "melisa lekarska",
                        "cytrynowe ziele",
                        "rojownik"
                    ],
                    "common_phrases": [
                        "uprawa melisy",
                        "liście melisy",
                        "świeża melisa",
                        "zapach melisy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Травянистое растение с запахом лимона, выращиваемое как лекарственное и пряное средство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мелисса",
                                "target_lang_sense_clarification": "растение"
                            },
                            {
                                "target_lang_word": "лимонная мята",
                                "target_lang_sense_clarification": "растение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "329a8f08-a669-3eb8-bb54-a16d45b54758",
                    "sense_definition": "Świeże lub suszone liście tej rośliny używane jako aromatyczny dodatek do potraw, deserów i napojów.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "spice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do sałatki owocowej warto dodać posiekaną <w>melisę</w> dla orzeźwienia.",
                            "target_lang_translations": {
                                "ru": "Во фруктовый салат стоит добавить нарезанную <w>мелиссу</w> для свежести."
                            }
                        },
                        {
                            "text": "Kucharz udekorował deser listkiem świeżej <w>melisy</w>.",
                            "target_lang_translations": {
                                "ru": "Повар украсил десерт листиком свежей <w>мелиссы</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dodać melisy",
                        "szczypta melisy",
                        "posiekana melisa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свежие или сушеные листья этого растения, используемые как ароматная добавка к блюдам и напиткам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мелисса",
                                "target_lang_sense_clarification": "приправа"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
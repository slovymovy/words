    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "65409a34-15b8-306f-b808-b38dc21e2211",
                    "sense_definition": "Consisting of a number more than one (especially, more than two) but not very many.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "quantity_small",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have visited that museum <w>several</w> times in the past year.",
                            "target_lang_translations": {
                                "ru": "Я посетил этот музей <w>несколько</w> раз за последний год."
                            }
                        },
                        {
                            "text": "<w>Several</w> of the students stayed after class to ask questions.",
                            "target_lang_translations": {
                                "ru": "<w>Несколько</w> студентов остались после занятий, чтобы задать вопросы."
                            }
                        },
                        {
                            "text": "We have <w>several</w> options to choose from for our holiday.",
                            "target_lang_translations": {
                                "ru": "У нас есть <w>несколько</w> вариантов на выбор для нашего отпуска."
                            }
                        }
                    ],
                    "synonyms": [
                        "a few#Determiner",
                        "a couple of"
                    ],
                    "antonyms": [
                        "many#Determiner",
                        "numerous#Determiner",
                        "countless#Determiner"
                    ],
                    "common_phrases": [
                        "several times",
                        "several years",
                        "several days",
                        "several people"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обозначает количество больше одного (обычно больше двух), но не очень много."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "несколько",
                                "target_lang_sense_clarification": "неопределенное небольшое число"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "041d1c6e-fcb3-3c31-af77-f68f224fca88",
                    "sense_definition": "Separate, distinct, or particular to a specific individual.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "distinctness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After the conference, the delegates went their <w>several</w> ways.",
                            "target_lang_translations": {
                                "ru": "После конференции делегаты разошлись в <w>разные</w> стороны."
                            }
                        },
                        {
                            "text": "Each <w>several</w> man in the regiment received a specific order.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>отдельный</w> человек в полку получил конкретный приказ."
                            }
                        }
                    ],
                    "synonyms": [
                        "a few#Determiner",
                        "a couple of"
                    ],
                    "antonyms": [
                        "many#Determiner",
                        "numerous#Determiner"
                    ],
                    "common_phrases": [
                        "several ways"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "mostly found in older literature or specific idioms like 'several ways'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный, обособленный или свойственный конкретному лицу (устаревшее или книжное значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разный",

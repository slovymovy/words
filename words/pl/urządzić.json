    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8efc3d15-f7e7-3376-bc20-9dc615d5f400",
                    "sense_definition": "Wyposażyć pomieszczenie lub budynek w meble, sprzęty i elementy dekoracyjne, nadając mu funkcjonalność i styl.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "furnishing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowożeńcy postanowili nowocześnie <urządzić> swoje pierwsze wspólne mieszkanie w centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Молодожены решили современно <обустроить> свою первую совместную квартиру в центре города."
                            }
                        },
                        {
                            "text": "Musimy <urządzić> pokój dla dziecka, zanim przyjdzie ono na świat.",
                            "target_lang_translations": {
                                "ru": "Мы должны <обустроить> детскую комнату до того, как ребенок появится на свет."
                            }
                        }
                    ],
                    "synonyms": [
                        "umeblować",
                        "wyposażyć"
                    ],
                    "common_phrases": [
                        "urządzić mieszkanie",
                        "przytulnie urządzić pokój",
                        "urządzić dom",
                        "nowocześnie urządzić wnętrze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оборудовать помещение мебелью, техникой и элементами декора, делая его функциональным и уютным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обустро́ить",
                                "target_lang_sense_clarification": "создать необходимые условия, привести в порядок"
                            },
                            {
                                "target_lang_word": "обста́вить",
                                "target_lang_sense_clarification": "снабдить мебелью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35a6ccdb-b226-3a97-aa55-21496e678dfd",
                    "sense_definition": "Zorganizować przyjęcie, uroczystość, spotkanie towarzyskie lub inne wydarzenie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "organizing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój znajomy planuje <urządzić> wielką imprezę urodzinową w przyszły weekend.",
                            "target_lang_translations": {
                                "ru": "Мой знакомый планирует <устроить> большую вечеринку в честь дня рождения в следующие выходные."
                            }
                        },
                        {
                            "text": "Rodzice chcą <urządzić> uroczysty obiad z okazji rocznicy ich ślubu.",
                            "target_lang_translations": {
                                "ru": "Родители хотят <устроить> торжественный обед по случаю годовщины их свадьбы."
                            }
                        }
                    ],
                    "synonyms": [
                        "zorganizować"
                    ],
                    "common_phrases": [
                        "urządzić przyjęcie",
                        "urządzić wesele",
                        "urządzić imprezę",
                        "urządzić awanturę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организовать и провести мероприятие, праздник или встречу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устро́ить",
                                "target_lang_sense_clarification": "организовать (вечеринку, прием)"
                            },
                            {
                                "target_lang_word": "организова́ть",
                                "target_lang_sense_clarification": "подготовить и осуществить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43be206f-9d6f-31e0-bd05-6d73d66b6623",
                    "sense_definition": "Zagospodarować się w nowym miejscu, stworzyć sobie odpowiednie warunki do życia lub pracy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "settling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jestem bardzo zadowolony z tego, jak wygodnie się tu <urządziliśmy>.",
                            "target_lang_translations": {
                                "ru": "Я очень доволен тем, как удобно мы здесь <устроились>."
                            }
                        },
                        {
                            "text": "Zajęło mi kilka tygodni, aby <urządzić> się w nowym biurze i zacząć normalnie pracować.",
                            "target_lang_translations": {
                                "ru": "Мне потребовалось несколько недель, чтобы <обустроиться> в новом офисе и начать нормально работать."
                            }
                        }
                    ],
                    "synonyms": [
                        "zagospodarować się",
                        "zainstalować się"
                    ],
                    "common_phrases": [
                        "urządzić się w domu",
                        "wygodnie się urządzić",
                        "dobrze się urządzić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обосноваться на новом месте, создать себе условия для жизни или работы (обычно используется с возвратным местоимением się)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "устро́иться",
                                "target_lang_sense_clarification": "расположиться, найти свое место"
                            },
                            {
                                "target_lang_word": "обустро́иться",
                                "target_lang_sense_clarification": "наладить быт на новом месте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8f875d7-2301-3b7a-8472-78609ccca208",
                    "sense_definition": "Sprawić komuś kłopot lub postawić w trudnej, niekorzystnej sytuacji (często używane ironicznie).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "trouble",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ale mnie <urządziłeś>, teraz muszę przepisywać cały ten raport od początku!",
                            "target_lang_translations": {
                                "ru": "Ну и <подставил> же ты меня, теперь мне придется переписывать весь этот отчет с самого начала!"
                            }
                        },
                        {
                            "text": "Pożyczyłem mu samochód na jeden dzień, a on go rozbił i nieźle mnie <urządził>.",
                            "target_lang_translations": {
                                "ru": "Я одолжил ему машину на один день, а он ее разбил и здорово меня <подставил>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nieźle kogoś urządzić",
                        "ale mnie urządziłeś"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to express frustration when someone causes trouble"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поставить кого-либо в неприятное, затруднительное положение (часто иронично)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подста́вить",
                                "target_lang_sense_clarification": "поставить в невыгодное положение (разг.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

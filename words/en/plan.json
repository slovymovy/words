                {
                    "sense_id": "bc7abcdc-b8fb-3e30-b2ed-9cde7b9ed1fc",
                    "sense_definition": "To make detailed arrangements for a future event or project.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "arrangement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They are busy <w>planning</w> their wedding for next spring.",
                            "target_lang_translations": {
                                "ru": "Они заняты <w>планированием</w> своей свадьбы на следующую весну."
                            }
                        },
                        {
                            "text": "You should <w>plan</w> ahead to avoid any problems later.",
                            "target_lang_translations": {
                                "ru": "Тебе следует <w>планировать</w> всё заранее, чтобы избежать проблем в будущем."
                            }
                        }
                    ],
                    "synonyms": [
                        "design",
                        "scheme",
                        "project"
                    ],
                    "common_phrases": [
                        "plan ahead",
                        "plan a trip",
                        "plan a party",
                        "plan for the future"
                    ],
                    "target_lang_definitions": {
                        "ru": "Детально подготавливать или организовывать будущее событие."

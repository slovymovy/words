            "senses": [
                {
                    "sense_id": "61ca387c-25fb-3178-963d-e691d4b2d661",
                    "sense_definition": "Druga dynastia cesarska Chin, panująca po dynastii Qin i przed Epoką Trzech Królestw.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "history_china",
                    "name_type": "historical_name",
                    "examples": [
                        {
                            "text": "Dynastia <w>Han</w> jest często uważana za złoty wiek w historii chińskiej cywilizacji.",
                            "target_lang_translations": {
                                "ru": "Династия <w>Хань</w> часто считается золотым веком в истории китайской цивилизации."
                            }
                        },
                        {
                            "text": "Wraz z rządami dynastii <w>Han</w> zapanowała atmosfera tolerancji dla różnych doktryn.",
                            "target_lang_translations": {
                                "ru": "С правлением династии <w>Хань</w> воцарилась атмосфера терпимости к различным учениям."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dynastia Han",
                        "epoka Han",
                        "cesarz Han"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вторая императорская династия Китая, правившая после династии Цинь и перед Эпохой Троецарствия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хань",
                                "target_lang_sense_clarification": "китайская династия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "fa20b247-6fb0-3ee1-b0b0-1f856f32fdb8",
                    "sense_definition": "Słowo używane w gwarze Śląska Cieszyńskiego oznaczające miejsce oddalone od mówiącego; tam, tam daleko.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "location_deixis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz iść <w>han</w>, żeby zobaczyć ten stary młyn nad rzeką.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно пойти <w>туда</w>, чтобы увидеть ту старую мельницу у реки."
                            }
                        },
                        {
                            "text": "Zostaw te ciężkie worki <w>han</w> pod ścianą i chodź do nas.",
                            "target_lang_translations": {
                                "ru": "Оставь эти тяжелые мешки <w>там</w> под стеной и иди к нам."
                            }
                        }
                    ],
                    "synonyms": [
                        "tam"
                    ],
                    "antonyms": [
                        "haw"
                    ],
                    "common_phrases": [
                        "iść han",
                        "patrzeć han"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Śląsk Cieszyński"
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": "gwara"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое в говоре Тешинской Силезии, обозначающее место, удаленное от говорящего; там, вон там."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "там",
                                "target_lang_sense_clarification": "наречие места (диалектизм)"
                            },
                            {
                                "target_lang_word": "вон там",
                                "target_lang_sense_clarification": "указание на удаленное место"
                            }
                        ]
                    }
                }
            ]
        }
    ]

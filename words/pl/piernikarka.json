            "senses": [
                {
                    "sense_id": "46fdae8c-9695-37e2-a2d1-ec19929994d3",
                    "sense_definition": "Kobieta zajmująca się zawodowo wypiekiem lub sprzedażą pierników; żeńska forma słowa <w>piernikarz</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "professions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczona <w>piernikarka</w> potrafiła ręcznie ozdobić setki ciastek dziennie.",
                            "target_lang_translations": {
                                "ru": "Опытная <w>пряничница</w> могла вручную украсить сотни пряников в день."
                            }
                        },
                        {
                            "text": "Na starym rynku stanęła rzeźba przedstawiająca <w>piernikarkę</w> z koszem pełnym słodkości.",
                            "target_lang_translations": {
                                "ru": "На старой рыночной площади установили скульптуру, изображающую <w>пряничницу</w> с корзиной, полной сладостей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "toruńska piernikarka",
                        "mistrzyni piernikarka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, профессионально занимающаяся выпечкой или продажей пряников; женская форма слова piernikarz."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пряничница",
                                "target_lang_sense_clarification": "профессия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "324ac72b-4c7d-3375-8039-3a9c6e531cea",
                    "sense_definition": "Żona <w>piernikarza</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "family_roles",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mistrz cechu przybył na uroczystość, a towarzyszyła mu jego <w>piernikarka</w>.",
                            "target_lang_translations": {
                                "ru": "Мастер цеха прибыл на торжество, а сопровождала его <w>жена пряничника</w>."
                            }
                        },
                        {
                            "text": "Młoda <w>piernikarka</w> dbała o dom, podczas gdy jej mąż pracował w warsztacie.",
                            "target_lang_translations": {
                                "ru": "Молодая <w>жена пряничника</w> заботилась о доме, пока её муж работал в мастерской."
                            }
                        }
                    ],
                    "synonyms": [
                        "piernikarzowa"
                    ],

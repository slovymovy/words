    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "56288dae-ba36-33d3-80a1-ef200e3bbda8",
                    "sense_definition": "Money or something equivalent given to someone to make up for loss, damage, or suffering.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "reparation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The victims received financial <w>compensation</w> for the accident.",
                            "target_lang_translations": {
                                "ru": "Пострадавшие получили денежную <w>компенсацию</w> за несчастный случай."
                            }
                        },
                        {
                            "text": "He filed a claim for <w>compensation</w> after losing his luggage at the airport.",
                            "target_lang_translations": {
                                "ru": "Он подал заявление на <w>компенсацию</w> после потери багажа в аэропорту."
                            }
                        }
                    ],
                    "synonyms": [
                        "restitution",
                        "amends",
                        "recompense"
                    ],
                    "common_phrases": [
                        "financial compensation",
                        "claim compensation",
                        "receive compensation",
                        "demand compensation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деньги или их эквивалент, выплачиваемые кому-либо для возмещения убытков, ущерба или страданий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "компенсация",
                                "target_lang_sense_clarification": "денежная выплата за ущерб"
                            },
                            {
                                "target_lang_word": "возмещение",
                                "target_lang_sense_clarification": "покрытие убытков"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df6d9c09-8c2a-30ea-a4cc-32558a684ce0",
                    "sense_definition": "Payment and benefits provided to an employee for their work or service.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "payment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new job offers a very generous <w>compensation</w> package including health insurance.",
                            "target_lang_translations": {
                                "ru": "На новой работе предлагают очень щедрый <w>компенсационный</w> пакет, включающий медицинскую страховку."
                            }
                        },
                        {
                            "text": "Executive <w>compensation</w> is often linked to the overall performance of the company.",
                            "target_lang_translations": {
                                "ru": "<w>Вознаграждение</w> руководителей часто связано с общими показателями деятельности компании."
                            }

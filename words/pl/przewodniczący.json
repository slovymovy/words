            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2bc7f09e-e5cd-36b0-a78f-94b6e06b6352",
                    "sense_definition": "Osoba stojąca na czele organizacji, zarządu, komitetu, partii lub innego ciała kolegialnego, kierująca jego pracami.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "leader_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy <przewodniczący> partii przedstawił swój program wyborczy na konferencji.",
                            "target_lang_translations": {
                                "ru": "Новый <председатель> партии представил свою предвыборную программу на конференции."
                            }
                        },
                        {
                            "text": "Jako <przewodniczący> rady nadzorczej, jest odpowiedzialny za kluczowe decyzje.",
                            "target_lang_translations": {
                                "ru": "Как <председатель> наблюдательного совета, он несет ответственность за ключевые решения."
                            }
                        }
                    ],
                    "synonyms": [
                        "prezes",
                        "szef",
                        "kierownik"
                    ],
                    "antonyms": [
                        "członek",
                        "podwładny"
                    ],
                    "common_phrases": [
                        "przewodniczący rady",
                        "przewodniczący zarządu",
                        "przewodniczący komisji",
                        "panie przewodniczący"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, стоящее во главе организации, правления, комитета, партии или иного коллегиального органа и руководящее его работой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "председатель",
                                "target_lang_sense_clarification": "глава организации, совета или комитета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a06d8d59-de63-3a53-ab3f-c2555ef07d23",
                    "sense_definition": "Osoba, która w danym momencie prowadzi zebranie, obrady lub dyskusję, dbając o ich porządek.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "meeting_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Przewodniczący> poprosił zgromadzonych o zajęcie miejsc i ciszę.",
                            "target_lang_translations": {
                                "ru": "<Председатель> попросил собравшихся занять места и соблюдать тишину."
                            }
                        },
                        {
                            "text": "Jeśli chcesz zabrać głos, musisz zgłosić się do <przewodniczącego> zebrania.",
                            "target_lang_translations": {
                                "ru": "Если вы хотите взять слово, нужно обратиться к <председателю> собрания."
                            }
                        }
                    ],
                    "synonyms": [
                        "prowadzący",
                        "moderator",
                        "gospodarz"
                    ],
                    "antonyms": [
                        "uczestnik"
                    ],
                    "common_phrases": [
                        "przewodniczący zebrania",
                        "przewodniczący obrad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который в данный момент ведет собрание, заседание или дискуссию, следя за порядком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "председатель",
                                "target_lang_sense_clarification": "ведущий собрания"
                            },
                            {
                                "target_lang_word": "председательствующий",
                                "target_lang_sense_clarification": "лицо, ведущее заседание"
                            }
                        ]
                    }
                }
            ]
        },

                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "kick out",
                                "target_lang_sense_clarification": "colloquial"
                            },
                            {
                                "target_lang_word": "fire",
                                "target_lang_sense_clarification": "from a job"
                            },
                            {
                                "target_lang_word": "expel",
                                "target_lang_sense_clarification": "from school or university"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ec61856-830a-3ee3-8315-588c59009144",
                    "sense_definition": "С трудом добиваться чего-либо, заставлять отдать что-либо (чаще всего деньги).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "forced_extraction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кредиторы пытаются <вышибать> старые долги любыми способами.",
                            "target_lang_translations": {
                                "en": "Creditors try to <w>squeeze out</w> old debts by any means."
                            }

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "da237512-58d5-3c2b-acbb-284676ccc2d0",
                    "sense_definition": "Of high quality and from a past era, often used to describe clothing, cars, or items that are old-fashioned but stylish.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "retro_style",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She loves to shop for <w>vintage</w> dresses at the local market.",
                            "target_lang_translations": {
                                "ru": "Она любит покупать <w>винтажные</w> платья на местном рынке."
                            }
                        },
                        {
                            "text": "He spent years restoring a <w>vintage</w> car from the 1960s.",
                            "target_lang_translations": {
                                "ru": "Он потратил годы на восстановление <w>старинного</w> автомобиля 1960-х годов."
                            }
                        }
                    ],
                    "synonyms": [
                        "retro",
                        "classic",
                        "old-school",
                        "antique"
                    ],
                    "antonyms": [
                        "modern",
                        "contemporary",
                        "new"
                    ],
                    "common_phrases": [
                        "vintage clothing",
                        "vintage car",
                        "vintage style",
                        "vintage look"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокого качества и из прошлой эпохи; часто используется для описания одежды, автомобилей или предметов, которые старомодны, но стильны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "винтажный",
                                "target_lang_sense_clarification": "о стиле, одежде или предметах старины"
                            },
                            {
                                "target_lang_word": "старинный",
                                "target_lang_sense_clarification": "древний, антикварный"
                            },
                            {
                                "target_lang_word": "классический",
                                "target_lang_sense_clarification": "имеющий непреходящую ценность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a97a90a0-7cf7-36a4-abc8-fe3f6d9d914d",
                    "sense_definition": "Typical of the best work or characteristic style of a particular person or thing.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "characteristic_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The director returned to his roots with a <w>vintage</w> performance that thrilled the audience.",
                            "target_lang_translations": {
                                "ru": "Режиссер вернулся к истокам с <w>классической</w> постановкой, которая привела публику в восторг."
                            }
                        },
                        {
                            "text": "It was <w>vintage</w> behavior for him to arrive late and charm everyone immediately.",
                            "target_lang_translations": {
                                "ru": "Опоздать и сразу всех очаровать — это было <w>типичное</w> для него поведение."
                            }
                        }
                    ],
                    "synonyms": [
                        "classic",
                        "characteristic",
                        "quintessential",
                        "typical"
                    ],
                    "antonyms": [
                        "atypical",
                        "uncharacteristic"
                    ],
                    "common_phrases": [
                        "vintage performance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Типичный для лучших работ или характерного стиля конкретного человека или вещи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "классический",
                                "target_lang_sense_clarification": "типичный, образцовый для кого-либо"
                            },
                            {
                                "target_lang_word": "типичный",
                                "target_lang_sense_clarification": "характерный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4de09e12-f90a-3adc-b7f3-8c192dd08863",
                    "sense_definition": "Relating to the production of wine in a specific year.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "wine_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>vintage</w> year is clearly marked on the label of the bottle.",
                            "target_lang_translations": {
                                "ru": "<w>Год урожая</w> четко указан на этикетке бутылки."
                            }
                        },
                        {
                            "text": "They discussed the <w>vintage</w> chart to decide which wine to order.",
                            "target_lang_translations": {
                                "ru": "Они обсуждали карту <w>урожаев</w>, чтобы решить, какое вино заказать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "vintage wine",
                        "vintage year",
                        "vintage port"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к производству вина в определенном году."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "марочный",
                                "target_lang_sense_clarification": "о вине высокого качества"
                            },
                            {
                                "target_lang_word": "выдержанный",
                                "target_lang_sense_clarification": "о вине с выдержкой"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f1a3071f-d749-323e-8c24-1a59e16db4b2",
                    "sense_definition": "A wine of high quality produced in a specific year and region, or the year itself.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "wine_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This bottle is a rare <w>vintage</w> from 1995.",
                            "target_lang_translations": {
                                "ru": "Эта бутылка — редкое <w>вино урожая</w> 1995 года."
                            }
                        },
                        {
                            "text": "Collectors often pay high prices for a fine <w>vintage</w>.",
                            "target_lang_translations": {
                                "ru": "Коллекционеры часто платят высокие цены за хорошее <w>марочное вино</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "year",
                        "crop"
                    ],
                    "common_phrases": [
                        "fine vintage",
                        "recent vintage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вино высокого качества, произведенное в определенный год и в определенном регионе, или сам год урожая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вино урожая",
                                "target_lang_sense_clarification": "вино конкретного года"
                            },
                            {
                                "target_lang_word": "марочное вино",
                                "target_lang_sense_clarification": "высококачественное вино"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aec8ad11-1004-34fa-8e58-78033a357475",
                    "sense_definition": "The harvesting of grapes or the yield of wine from a vineyard during a single season.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "harvest_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmers were busy preparing for the upcoming <w>vintage</w>.",
                            "target_lang_translations": {
                                "ru": "Фермеры были заняты подготовкой к предстоящему <w>сбору винограда</w>."
                            }
                        },
                        {
                            "text": "Bad weather significantly reduced the <w>vintage</w> this autumn.",
                            "target_lang_translations": {
                                "ru": "Плохая погода значительно сократила <w>урожай</w> этой осенью."
                            }
                        }
                    ],
                    "synonyms": [
                        "harvest",
                        "yield"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сбор винограда или урожай вина с виноградника в течение одного сезона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбор винограда",
                                "target_lang_sense_clarification": "процесс сбора"
                            },
                            {
                                "target_lang_word": "урожай",
                                "target_lang_sense_clarification": "количество собранного продукта"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4b51117b-32a9-3c9d-84e5-747f10e1d48a",
                    "sense_definition": "To harvest grapes or make wine from them.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "harvest_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>vintaged</w> the grapes early this year due to the heat.",
                            "target_lang_translations": {
                                "ru": "В этом году они <w>собрали</w> виноград рано из-за жары."
                            }
                        },
                        {
                            "text": "The estate has <w>vintaged</w> excellent wines for over a century.",
                            "target_lang_translations": {
                                "ru": "Это поместье <w>производило</w> отличные вина на протяжении более века."
                            }
                        }
                    ],
                    "synonyms": [
                        "harvest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собирать виноград или делать из него вино."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собирать",
                                "target_lang_sense_clarification": "урожай винограда"

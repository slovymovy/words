{
    "word_family": [
        "unwelcome",
        "welcoming",
        "welcomeness",
        "welcomer"
    ],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "e3a97569-1c73-3e4f-af3a-f35145c61d9c",
                    "sense_definition": "Used to greet someone in a friendly way when they arrive at a place.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "greeting_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Welcome</w> to our new home!",
                            "target_lang_translations": {
                                "ru": "<w>Добро пожаловать</w> в наш новый дом!"
                            }
                        },
                        {
                            "text": "Hello everyone, and <w>welcome</w> back to the show.",
                            "target_lang_translations": {
                                "ru": "Всем привет и <w>добро пожаловать</w> снова на шоу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "welcome to",
                        "welcome back",
                        "welcome home",
                        "welcome aboard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для дружеского приветствия кого-либо по прибытии в какое-либо место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "добро пожаловать",
                                "target_lang_sense_clarification": "стандартное приветствие"
                            },
                            {
                                "target_lang_word": "приветствуем",
                                "target_lang_sense_clarification": "выражение приветствия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "12492a6a-c7f2-3d0f-a541-b2dfeeca3f69",
                    "sense_definition": "Used as a polite response when someone thanks you.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "greeting_interaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Thank you for helping me. — You are <w>welcome</w>.",
                            "target_lang_translations": {
                                "ru": "Спасибо за помощь. — <w>Пожалуйста</w>."
                            }
                        },
                        {
                            "text": "You're <w>welcome</w> for the ride.",
                            "target_lang_translations": {
                                "ru": "За поездку — <w>не за что</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "you are welcome",
                        "you're welcome"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется как вежливый ответ, когда кто-то благодарит вас."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пожалуйста",
                                "target_lang_sense_clarification": "ответ на спасибо"
                            },
                            {
                                "target_lang_word": "не за что",
                                "target_lang_sense_clarification": "разговорный ответ на благодарность"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "79185b6e-ee1e-3c75-a25a-597cfd3c5c10",
                    "sense_definition": "To greet someone in a friendly and polite manner when they arrive.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "action_greet",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The host stood at the door to <w>welcome</w> the guests.",
                            "target_lang_translations": {
                                "ru": "Хозяин стоял у двери, чтобы <w>приветствовать</w> гостей."
                            }
                        },
                        {
                            "text": "We were warmly <w>welcomed</w> by the local community.",
                            "target_lang_translations": {
                                "ru": "Нас тепло <w>встретила</w> местная община."
                            }
                        }
                    ],
                    "synonyms": [
                        "greet",
                        "receive"
                    ],
                    "common_phrases": [
                        "welcome someone",
                        "welcome back"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приветствовать кого-либо в дружеской и вежливой манере по прибытии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приветствовать",
                                "target_lang_sense_clarification": "здороваться"
                            },
                            {
                                "target_lang_word": "встречать",
                                "target_lang_sense_clarification": "принимать гостей"
                            },
                            {
                                "target_lang_word": "радушно принимать",
                                "target_lang_sense_clarification": "оказывать прием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f83c7c8-c9db-3dce-87d0-c1c8ea9d9d7a",
                    "sense_definition": "To accept or receive something, such as an idea or decision, with pleasure.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_accept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee <w>welcomes</w> suggestions for improvement.",
                            "target_lang_translations": {
                                "ru": "Комитет <w>приветствует</w> предложения по улучшению."
                            }
                        },
                        {
                            "text": "She <w>welcomed</w> the opportunity to work on the new project.",
                            "target_lang_translations": {
                                "ru": "Она <w>приветствовала</w> возможность поработать над новым проектом."
                            }
                        }
                    ],
                    "synonyms": [
                        "embrace",
                        "accept"
                    ],
                    "antonyms": [
                        "reject"
                    ],
                    "common_phrases": [
                        "welcome the opportunity",
                        "welcome the chance",
                        "welcome suggestions"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать или получать что-либо, например идею или решение, с удовольствием; одобрять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приветствовать",
                                "target_lang_sense_clarification": "одобрять, поддерживать"
                            },
                            {
                                "target_lang_word": "одобрять",
                                "target_lang_sense_clarification": "относиться положительно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d7921fea-3bcf-310a-98c2-ba5e29b47aa6",
                    "sense_definition": "Received with pleasure or giving cause for happiness and satisfaction.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "quality_pleasing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cool breeze was a <w>welcome</w> relief after the heat.",
                            "target_lang_translations": {
                                "ru": "Прохладный ветерок стал <w>желанным</w> облегчением после жары."
                            }
                        },
                        {
                            "text": "She was always a <w>welcome</w> visitor at our house.",
                            "target_lang_translations": {
                                "ru": "Она всегда была <w>желанным</w> гостем в нашем доме."
                            }
                        }
                    ],
                    "synonyms": [
                        "appreciated",
                        "desirable",
                        "pleasing"
                    ],
                    "antonyms": [
                        "unwelcome"
                    ],
                    "common_phrases": [
                        "welcome change",
                        "welcome news",
                        "welcome sight",
                        "make someone feel welcome"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принятый с удовольствием или доставляющий радость и удовлетворение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "желанный",
                                "target_lang_sense_clarification": "долгожданный, приятный"
                            },
                            {
                                "target_lang_word": "приятный",
                                "target_lang_sense_clarification": "доставляющий удовольствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "23858f30-60cc-3789-a899-350d7b73f418",
                    "sense_definition": "Allowed or freely permitted to do, have, or use something.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "quality_permitted",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You are <w>welcome</w> to use my car whenever you need it.",
                            "target_lang_translations": {
                                "ru": "Вы <w>можете</w> пользоваться моей машиной, когда вам это нужно."
                            }
                        },
                        {
                            "text": "Anyone is <w>welcome</w> to join the club.",
                            "target_lang_translations": {
                                "ru": "Любой <w>может</w> вступить в клуб."
                            }
                        }
                    ],
                    "common_phrases": [
                        "welcome to",
                        "more than welcome"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий разрешение или право свободно делать, иметь или использовать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "можете",
                                "target_lang_sense_clarification": "в значении 'имеете право', 'разрешено'"
                            },
                            {
                                "target_lang_word": "разрешено",
                                "target_lang_sense_clarification": "позволено"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bb2c4e74-266b-3a4d-bf05-a5c413b50e1d",
                    "sense_definition": "The act of greeting someone or the kind reception given to a person upon arrival.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_reception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They gave us a very warm <w>welcome</w> when we arrived.",
                            "target_lang_translations": {
                                "ru": "Они оказали нам очень тёплый <w>приём</w>, когда мы приехали."
                            }
                        },
                        {
                            "text": "The hero received a noisy <w>welcome</w> from the crowd.",
                            "target_lang_translations": {
                                "ru": "Герой получил шумное <w>приветствие</w> от толпы."
                            }
                        }
                    ],
                    "synonyms": [
                        "reception",
                        "greeting"
                    ],
                    "common_phrases": [
                        "warm welcome",
                        "give a welcome",
                        "overstay one's welcome",
                        "wear out one's welcome"
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт приветствия кого-либо или любезный приём, оказанный человеку по прибытии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приём",
                                "target_lang_sense_clarification": "встреча гостей"
                            },
                            {
                                "target_lang_word": "встреча",
                                "target_lang_sense_clarification": "собрание, прием"
                            },
                            {
                                "target_lang_word": "приветствие",
                                "target_lang_sense_clarification": "слова приветствия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "80371b99-c090-396f-a5f8-5ecc780e99ca",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "name_surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Welcome</w> has been appointed as the new director.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Уэлком</w> был назначен новым директором."
                            }
                        },
                        {
                            "text": "I met a family named <w>Welcome</w> during my trip.",
                            "target_lang_translations": {
                                "ru": "Я встретил семью по фамилии <w>Уэлком</w> во время поездки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "A family name."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Уэлком",
                                "target_lang_sense_clarification": "транслитерация фамилии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
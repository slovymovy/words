                {
                    "sense_id": "0ab34f56-2d90-3410-ae5a-a2238acd2955",
                    "sense_definition": "A state of peaceful existence, agreement, and cooperation between people or groups.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "agreement_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The different communities in the city lived in <w>harmony</w> for many decades.",
                            "target_lang_translations": {
                                "ru": "Различные общины города жили в <w>гармонии</w> на протяжении многих десятилетий."
                            }
                        },
                        {
                            "text": "We must strive to maintain social <w>harmony</w> despite our differences.",
                            "target_lang_translations": {
                                "ru": "Мы должны стремиться поддерживать социальную <w>гармонию</w>, несмотря на наши различия."
                            }
                        }
                    ],
                    "synonyms": [
                        "H/H",
                        "H/Hr"
                    ],
                    "antonyms": [
                        "conflict",
                        "discord",
                        "friction",
                        "disagreement"
                    ],
                    "common_phrases": [
                        "live in harmony",
                        "perfect harmony",
                        "social harmony",
                        "in harmony with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние мирного сосуществования, согласия и сотрудничества между людьми или группами."
                    },
                    "translations": {
                        "ru": [
                            {

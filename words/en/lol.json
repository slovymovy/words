    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "25f23c03-07f3-3c24-bd05-6794a86a59aa",
                    "sense_definition": "Used in electronic communication to express laughter or strong amusement; an abbreviation of \"laughing out loud\".",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "laughter_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That video is so funny, <w>lol</w>!",
                            "target_lang_translations": {
                                "ru": "Это видео такое смешное, <w>лол</w>!"
                            }
                        },
                        {
                            "text": "I can't believe you actually said that, <w>lol</w>.",
                            "target_lang_translations": {
                                "ru": "Не верится, что ты реально это сказал, <w>лол</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "haha",
                        "lmao",
                        "rofl"
                    ],
                    "common_phrases": [
                        "lol jk",
                        "lol no"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в электронном общении для выражения смеха или сильного веселья; аббревиатура от \"laughing out loud\" (смеюсь вслух)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лол",
                                "target_lang_sense_clarification": "прямое заимствование"
                            },
                            {
                                "target_lang_word": "ржу",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "ахаха",
                                "target_lang_sense_clarification": "междометие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afb93e50-9d0d-3017-b262-9b653881c7a2",
                    "sense_definition": "Used to indicate light-heartedness, irony, or to soften the tone of a statement, implying that the message is not serious.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "laughter_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have no idea what I'm doing <w>lol</w>.",
                            "target_lang_translations": {
                                "ru": "Я понятия не имею, что делаю, <w>лол</w>."
                            }
                        },
                        {
                            "text": "It's not a big deal, <w>lol</w>, don't worry about it.",
                            "target_lang_translations": {
                                "ru": "Это не так уж важно, <w>лол</w>, не парься."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "internet slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для придания оттенка беззаботности, иронии или для смягчения тона высказывания, подразумевая, что сообщение не является серьезным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лол",
                                "target_lang_sense_clarification": "иронично"
                            },
                            {
                                "target_lang_word": "хех",
                                "target_lang_sense_clarification": "усмешка"
                            },
                            {
                                "target_lang_word": ")))",
                                "target_lang_sense_clarification": "текстовый смайлик"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d46b27ad-41eb-3951-a4c3-ee62a41971f8",

{
    "word_family": [
        "taki",
        "także"
    ],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "72dd5d54-888d-32d9-9421-eb6d7e8a2c7e",
                    "sense_definition": "Słowo używane do potwierdzenia, wyrażenia zgody lub udzielenia twierdzącej odpowiedzi na pytanie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "affirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy masz ochotę na spacer? – <w>Tak</w>, chętnie pójdę.",
                            "target_lang_translations": {
                                "ru": "Хочешь прогуляться? – <w>Да</w>, с удовольствием пойду."
                            }
                        },
                        {
                            "text": "Zapytałem go o zdanie i bez wahania odpowiedział <w>tak</w>.",
                            "target_lang_translations": {
                                "ru": "Я спросил его мнение, и он без колебаний ответил <w>да</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "owszem",
                        "naturalnie",
                        "oczywiście",
                        "no",
                        "dokładnie",
                        "racja",
                        "rzeczywiście",
                        "właśnie",
                        "fakt"
                    ],
                    "antonyms": [
                        "nie"
                    ],
                    "common_phrases": [
                        "no tak",
                        "chyba tak",
                        "o tak",
                        "tak czy nie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое для подтверждения, выражения согласия или утвердительного ответа на вопрос."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "выражение согласия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "562afc0f-6ff4-3164-93df-de60743d04e8",
                    "sense_definition": "W ten sposób; wskazując na metodę lub sposób wykonania jakiejś czynności.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dlaczego na mnie <w>tak</w> patrzysz?",
                            "target_lang_translations": {
                                "ru": "Почему ты на меня <w>так</w> смотришь?"
                            }
                        },
                        {
                            "text": "Musisz to zrobić <w>tak</w>, żeby konstrukcja się nie przewróciła.",
                            "target_lang_translations": {
                                "ru": "Ты должен сделать это <w>так</w>, чтобы конструкция не упала."
                            }
                        }
                    ],
                    "antonyms": [
                        "inaczej",
                        "odwrotnie"
                    ],
                    "common_phrases": [
                        "tak samo",
                        "tak jak",
                        "i tak dalej",
                        "tak czy owak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Таким образом; указывая на метод или способ выполнения какого-либо действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "так",
                                "target_lang_sense_clarification": "таким образом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "272a2492-ab64-37f1-82c0-3e18346399c9",
                    "sense_definition": "W bardzo dużym stopniu; słowo używane do podkreślenia natężenia cechy lub stanu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jestem <w>tak</w> zmęczony, że marzę tylko o łóżku.",
                            "target_lang_translations": {
                                "ru": "Я <w>так</w> устал, что мечтаю только о кровати."
                            }
                        },
                        {
                            "text": "To był <w>tak</w> piękny widok, że zaparło mi dech.",
                            "target_lang_translations": {
                                "ru": "Это был <w>такой</w> красивый вид, что у меня перехватило дыхание."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tak bardzo",
                        "tak dużo",
                        "tak długo"
                    ],
                    "target_lang_definitions": {
                        "ru": "В очень большой степени; слово, используемое для подчеркивания интенсивности признака или состояния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "так",
                                "target_lang_sense_clarification": "настолько / в такой степени"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5d8474ac-a408-30b2-8d3d-e7b653a804b8",
                    "sense_definition": "Wyrażenie zgody, akceptacji lub poparcia dla czegoś.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "affirmation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czekamy na twoje ostateczne <w>tak</w> w tej sprawie.",
                            "target_lang_translations": {
                                "ru": "Мы ждем твоего окончательного <w>да</w> по этому вопросу."
                            }
                        },
                        {
                            "text": "W głosowaniu oddano pięć głosów na <w>tak</w>.",
                            "target_lang_translations": {
                                "ru": "В голосовании было отдано пять голосов «<w>да</w>»."
                            }
                        }
                    ],
                    "common_phrases": [
                        "być na tak",
                        "powiedzieć tak"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражение согласия, принятия или поддержки чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "утвердительный ответ"
                            }
                        ]
                    }
                }
            ]
        }
    ]

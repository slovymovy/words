    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d6dfb6f9-0532-3232-aa69-d2dcbe69ae0e",
                    "sense_definition": "Grubościenne, duże naczynie z uchem, przeznaczone głównie do picia piwa.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Panie barman, ten <w>kufel</w> jest brudny!",
                            "target_lang_translations": {
                                "ru": "Бармен, эта <w>кружка</w> грязная!"
                            }
                        },
                        {
                            "text": "Kelner postawił na stole ciężki <w>kufel</w> wypełniony zimnym napojem.",
                            "target_lang_translations": {
                                "ru": "Официант поставил на стол тяжёлую <w>кружку</w>, наполненную холодным напитком."
                            }
                        }
                    ],
                    "synonyms": [
                        "szklanica"
                    ],
                    "common_phrases": [
                        "kufel do piwa",
                        "ciężki kufel",
                        "rozbić kufel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой толстостенный сосуд с ручкой, предназначенный главным образом для питья пива."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кружка",
                                "target_lang_sense_clarification": "сосуд"
                            },
                            {
                                "target_lang_word": "пивная кружка",
                                "target_lang_sense_clarification": "тип посуды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8750f836-f57c-3326-8450-2292a0c8091b",
                    "sense_definition": "Ilość płynu, zazwyczaj piwa, która mieści się w naczyniu zwanym kuflem.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj wypiłem w pubie dwa <w>kufle</w> piwa.",
                            "target_lang_translations": {
                                "ru": "Вчера я выпил в пабе две <w>кружки</w> пива."
                            }
                        },
                        {
                            "text": "Zamówiliśmy jeszcze jeden <w>kufel</w> dla spóźnionego kolegi.",
                            "target_lang_translations": {
                                "ru": "Мы заказали ещё одну <w>кружку</w> для опаздывающего коллеги."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wypić kufel",
                        "zamówić kufel",
                        "kolejny kufel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество жидкости, обычно пива, которое помещается в кружку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кружка",
                                "target_lang_sense_clarification": "порция"
                            }
                        ]
                    }
                }
            ]
        }
    ]

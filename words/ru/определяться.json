    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4c5a5c46-d2e0-3e51-917f-c29fbe0f6fdb",
                    "sense_definition": "Принимать окончательное решение, делать выбор в пользу чего-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "decision_making",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы наконец <определились> с местом проведения летнего отпуска.",
                            "target_lang_translations": {
                                "en": "We finally <decided on> the location for our summer vacation."
                            }
                        },
                        {
                            "text": "Тебе нужно скорее <определиться>, какую профессию ты хочешь выбрать.",
                            "target_lang_translations": {
                                "en": "You need to <make up your mind> soon about which profession you want to choose."
                            }
                        }
                    ],
                    "synonyms": [
                        "решать",
                        "выбирать"
                    ],
                    "antonyms": [
                        "колебаться",
                        "сомневаться"
                    ],
                    "common_phrases": [
                        "определиться с выбором",
                        "определиться с датой",
                        "пока не определился"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a final decision or choice about something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "decide on",
                                "target_lang_sense_clarification": "to choose something specific after thinking"
                            },
                            {
                                "target_lang_word": "make up one's mind",
                                "target_lang_sense_clarification": "to reach a decision"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06f0469f-bf67-3b57-9eff-6724d01edfc8",
                    "sense_definition": "Быть устанавливаемым, вычисляемым или распознаваемым (страдательный залог к глаголу <w>определять</w>).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "passive_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Стоимость этого товара <определяется> спросом и предложением на рынке.",
                            "target_lang_translations": {
                                "en": "The cost of this product <is determined> by supply and demand in the market."
                            }
                        },
                        {
                            "text": "Победитель конкурса <определяется> по результатам зрительского голосования.",
                            "target_lang_translations": {
                                "en": "The winner of the contest <is determined> by the results of the audience vote."
                            }
                        }
                    ],
                    "synonyms": [
                        "устанавливаться",
                        "вычисляться",
                        "зависеть"
                    ],
                    "common_phrases": [
                        "определяется законом",
                        "определяется правилами",
                        "цена определяется"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "passive voice"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be established, calculated, or characterized by something (passive meaning)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be determined",
                                "target_lang_sense_clarification": "to be established or decided by factors"
                            },
                            {
                                "target_lang_word": "be defined",
                                "target_lang_sense_clarification": "to be characterized or described"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6dd90f6-2359-39e1-8542-40ef849c5739",
                    "sense_definition": "Становиться ясным, отчётливым; приобретать законченную форму или характер.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "becoming_clear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "К вечеру погода <определилась>, ветер стих и выглянуло солнце.",
                            "target_lang_translations": {
                                "en": "By evening the weather <settled>, the wind died down, and the sun came out."
                            }
                        },
                        {
                            "text": "Её талант к рисованию <определился> ещё в раннем детстве.",
                            "target_lang_translations": {
                                "en": "Her talent for drawing <became apparent> in early childhood."
                            }
                        }
                    ],
                    "synonyms": [
                        "проясняться",
                        "формироваться",
                        "вырисовываться",
                        "складываться"
                    ],
                    "antonyms": [
                        "размываться"
                    ],
                    "common_phrases": [
                        "погода определилась",
                        "характер определился"
                    ],
                    "target_lang_definitions": {
                        "en": "To become clear, distinct, or acquire a finished form."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take shape",
                                "target_lang_sense_clarification": "to become distinct or developed"
                            },
                            {
                                "target_lang_word": "become clear",
                                "target_lang_sense_clarification": "to stop being vague"
                            },
                            {
                                "target_lang_word": "settle",
                                "target_lang_sense_clarification": "regarding weather or a situation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49fd13e4-1281-3710-b996-6ae3cdc9c1e5",
                    "sense_definition": "Вычислять своё местонахождение, ориентироваться в пространстве.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "navigation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Опытные моряки умеют <определяться> в открытом море по звёздам.",
                            "target_lang_translations": {
                                "en": "Experienced sailors know how to <determine their position> in the open sea by the stars."
                            }
                        },
                        {
                            "text": "Туристы остановились на перекрёстке, чтобы <определиться> по карте.",
                            "target_lang_translations": {
                                "en": "The tourists stopped at the intersection to <get their bearings> using the map."
                            }
                        }
                    ],
                    "synonyms": [
                        "ориентироваться"
                    ],
                    "antonyms": [
                        "теряться",
                        "заблудиться"
                    ],
                    "common_phrases": [
                        "определяться на местности",
                        "определяться по карте",
                        "определяться по звёздам"
                    ],
                    "target_lang_definitions": {
                        "en": "To calculate one's location or orient oneself in space."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get one's bearings",
                                "target_lang_sense_clarification": "to figure out one's position relative to surroundings"
                            },
                            {
                                "target_lang_word": "determine one's position",
                                "target_lang_sense_clarification": "navigation context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e136035c-d50a-312f-b987-011f610ec22d",
                    "sense_definition": "Устраиваться на работу, поступать на службу или учебу.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "employment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой брат удачно <определился> на службу в крупную торговую компанию.",
                            "target_lang_translations": {
                                "en": "My brother successfully <got a job> at a large trading company."
                            }
                        },
                        {
                            "text": "После окончания школы она решила <определиться> в медицинский колледж.",
                            "target_lang_translations": {
                                "en": "After finishing school, she decided to <enroll> in medical college."
                            }
                        }
                    ],
                    "synonyms": [
                        "устраиваться",
                        "поступать"
                    ],
                    "antonyms": [
                        "увольняться"
                    ],
                    "common_phrases": [
                        "определиться на службу",
                        "определиться на работу"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in older texts or simple speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To find employment or enroll in an institution (colloquial)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get a job",
                                "target_lang_sense_clarification": "employment context"
                            },
                            {
                                "target_lang_word": "enroll",
                                "target_lang_sense_clarification": "education context"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
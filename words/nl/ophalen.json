{
    "word_family": [
        "ophaalbrug",
        "ophaling",
        "ophaaldienst"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ab9f9cf9-931f-34af-bd47-e457dffaec4a",
                    "sense_definition": "Iemand of iets ergens gaan halen om mee te nemen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "collect_fetch",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik moet de kinderen van school <w>ophalen</w>.",
                            "target_lang_translations": {
                                "ru": "Я должен <w>забрать</w> детей из школы."
                            }
                        },
                        {
                            "text": "Je kunt je bestelling morgen in de winkel <w>ophalen</w>.",
                            "target_lang_translations": {
                                "ru": "Вы можете <w>забрать</w> свой заказ завтра в магазине."
                            }
                        }
                    ],
                    "antonyms": [
                        "brengen",
                        "wegbrengen"
                    ],
                    "common_phrases": [
                        "iemand ophalen",
                        "geld ophalen",
                        "vuilnis ophalen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Забрать кого-то или что-то из определенного места, чтобы взять с собой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "забирать",
                                "target_lang_sense_clarification": "взять кого-то или что-то с собой"
                            },
                            {
                                "target_lang_word": "встречать",
                                "target_lang_sense_clarification": "прибывающих на вокзал или в аэропорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41159a29-3e98-3e79-a89b-b16e8721a002",
                    "sense_definition": "Iets naar boven trekken of hijsen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "raise_lift",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>haalde</w> zijn schouders <w>op</w> toen ik hem een vraag stelde.",
                            "target_lang_translations": {
                                "ru": "Он <w>пожал</w> плечами, когда я задал ему вопрос."
                            }
                        },
                        {
                            "text": "De vissers <w>haalden</w> de zware netten <w>op</w>.",
                            "target_lang_translations": {
                                "ru": "Рыбаки <w>поднимали</w> тяжелые сети."
                            }
                        }
                    ],
                    "synonyms": [
                        "optrekken"
                    ],
                    "antonyms": [
                        "neerlaten",
                        "laten zakken"
                    ],
                    "common_phrases": [
                        "schouders ophalen",
                        "neus ophalen",
                        "anker ophalen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поднимать или тянуть что-то вверх."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поднимать",
                                "target_lang_sense_clarification": "перемещать вверх (мост, сети)"
                            },
                            {
                                "target_lang_word": "пожимать",
                                "target_lang_sense_clarification": "только в выражении \"пожимать плечами\""
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffca3acd-832a-32bf-a412-84bf4d0200f7",
                    "sense_definition": "Herinneringen of verhalen uit het verleden weer naar boven brengen en bespreken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "recall_memory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens het diner <w>haalden</w> we herinneringen <w>op</w> aan onze studententijd.",
                            "target_lang_translations": {
                                "ru": "За ужином мы <w>вспоминали</w> наши студенческие годы."
                            }
                        },
                        {
                            "text": "Opa vindt het fijn om oude verhalen <w>op</w> te <w>halen</w>.",
                            "target_lang_translations": {
                                "ru": "Дедушка любит <w>вспоминать</w> старые истории."
                            }
                        }
                    ],
                    "common_phrases": [
                        "herinneringen ophalen",
                        "oude koeien uit de sloot halen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Воскрешать в памяти воспоминания или истории из прошлого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вспоминать",
                                "target_lang_sense_clarification": "освежать в памяти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d74088a3-492b-3863-8c55-27f9690a15c0",
                    "sense_definition": "Digitale gegevens of bestanden van een netwerk naar een computer halen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "digital_transfer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn telefoon kan de nieuwe e-mails niet automatisch <w>ophalen</w>.",
                            "target_lang_translations": {
                                "ru": "Мой телефон не может автоматически <w>получать</w> новые письма."
                            }
                        },
                        {
                            "text": "Het programma moet eerst de laatste updates <w>ophalen</w>.",
                            "target_lang_translations": {
                                "ru": "Программа должна сначала <w>загрузить</w> последние обновления."
                            }
                        }
                    ],
                    "synonyms": [
                        "downloaden"
                    ],
                    "antonyms": [
                        "uploaden",
                        "versturen"
                    ],
                    "common_phrases": [
                        "data ophalen",
                        "mail ophalen"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Получать цифровые данные или файлы из сети на компьютер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скачивать",
                                "target_lang_sense_clarification": "загружать файлы"
                            },
                            {
                                "target_lang_word": "получать",
                                "target_lang_sense_clarification": "о почте или данных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8dfc7540-8929-37c5-87ae-7b7702aed174",
                    "sense_definition": "Een slecht resultaat of cijfer verbeteren.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "improve_grade",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft hard gestudeerd en zijn cijfer voor wiskunde <w>opgehaald</w>.",
                            "target_lang_translations": {
                                "ru": "Он усердно учился и <w>исправил</w> свою оценку по математике."
                            }
                        },
                        {
                            "text": "Het is lastig om die achterstand nog <w>op</w> te <w>halen</w>.",
                            "target_lang_translations": {
                                "ru": "Сложно <w>наверстать</w> это отставание."
                            }
                        }
                    ],
                    "synonyms": [
                        "verbeteren"
                    ],
                    "common_phrases": [
                        "een cijfer ophalen",
                        "schade ophalen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Улучшить плохой результат или оценку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исправлять",
                                "target_lang_sense_clarification": "улучшать оценку"
                            },
                            {
                                "target_lang_word": "наверстывать",
                                "target_lang_sense_clarification": "ликвидировать отставание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
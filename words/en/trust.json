    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ef8e950f-619c-3399-ba4c-1815c43a685c",
                    "sense_definition": "To believe that someone is good and honest and will not harm you, or that something is safe and reliable.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "belief_confidence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can <w>trust</w> him to do the job well because he is very experienced."
                        },
                        {
                            "text": "I do not <w>trust</w> this old ladder; it looks like it might break."
                        }
                    ],
                    "synonyms": [
                        "belief",
                        "faith",
                        "confidence"
                    ],
                    "antonyms": [
                        "distrust",
                        "mistrust"
                    ],
                    "common_phrases": [
                        "trust me",
                        "trust your instincts",
                        "trust in God"
                    ]
                },
                {
                    "sense_id": "c7ac5b8c-ce4d-300a-aec3-269967c3b192",
                    "sense_definition": "To hope and expect that something is true or correct.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hope_expect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>trust</w> that you had a pleasant journey here despite the rain."
                        },
                        {
                            "text": "We <w>trust</w> this arrangement will be satisfactory for everyone involved."
                        }
                    ],
                    "synonyms": [
                        "expectation",
                        "hope"
                    ],
                    "common_phrases": [
                        "trust that",
                        "trust you are well"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ]
                },
                {
                    "sense_id": "10d9209a-6bff-3545-b821-69b40dd3a84a",
                    "sense_definition": "To give something to someone else to look after because you believe they will be careful with it.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "entrust",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I would never <w>trust</w> him with my car keys because he drives too fast."
                        },
                        {
                            "text": "She <w>trusts</w> her assistant with all the financial details of the company."
                        }
                    ],
                    "common_phrases": [
                        "trust someone with",
                        "trust with my life"
                    ]
                },
                {
                    "sense_id": "0b281006-f722-3ecb-bcf0-853b74ac10ab",
                    "sense_definition": "To rely on something, such as luck or memory, without being certain of the result.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "reliance_chance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He decided to <w>trust</w> to luck rather than preparing properly for the exam."
                        },
                        {
                            "text": "Without a map, we had to <w>trust</w> to our sense of direction."
                        }
                    ],
                    "common_phrases": [
                        "trust to luck",
                        "trust to memory"
                    ]
                }
            ]
        },

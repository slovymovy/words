    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "21210b5b-3b7d-3058-8745-411ce3abade3",
                    "sense_definition": "Suitable or fit to be relied on; worthy of dependence, reliance, or trust.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is a very <w>reliable</w> friend who always helps when needed.",
                            "target_lang_translations": {
                                "ru": "Она очень <w>надёжная</w> подруга, которая всегда помогает, когда это нужно."
                            }
                        },
                        {
                            "text": "We need to find a <w>reliable</w> source of information for our research project.",
                            "target_lang_translations": {
                                "ru": "Нам нужно найти <w>достоверный</w> источник информации для нашего исследовательского проекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "dependable",
                        "trustworthy"
                    ],
                    "antonyms": [
                        "unreliable"
                    ],
                    "common_phrases": [
                        "reliable source",
                        "reliable information",
                        "reliable service",
                        "reliable car"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пригодный для того, чтобы на него положиться; заслуживающий доверия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надёжный",
                                "target_lang_sense_clarification": "о людях, технике, услугах"
                            },
                            {
                                "target_lang_word": "достоверный",
                                "target_lang_sense_clarification": "об информации или источниках"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a6e058f-de45-3b1c-aa64-813cc76a557f",
                    "sense_definition": "Describing a data transmission method that guarantees the delivery of packets to the destination.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The engineer chose a <w>reliable</w> protocol to ensure all files were transferred correctly.",
                            "target_lang_translations": {
                                "ru": "Инженер выбрал <w>надёжный</w> протокол, чтобы гарантировать правильную передачу всех файлов."
                            }
                        },
                        {
                            "text": "For streaming video, a fast connection is often preferred over a strictly <w>reliable</w> one.",
                            "target_lang_translations": {
                                "ru": "Для потокового видео быстрое соединение часто предпочтительнее строго <w>надёжного</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "secure"
                    ],
                    "antonyms": [
                        "unreliable"
                    ],
                    "common_phrases": [
                        "reliable connection",
                        "reliable protocol",
                        "reliable delivery"
                    ],
                    "target_lang_definitions": {
                        "ru": "Описывает метод передачи данных, гарантирующий доставку пакетов по назначению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надёжный",
                                "target_lang_sense_clarification": "в контексте технических протоколов и связи"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1da5099a-33c6-38b9-8194-a68a02205210",
                    "sense_definition": "A person or thing that is trustworthy or dependable.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is one of the old <w>reliables</w> in the department who knows how everything works.",
                            "target_lang_translations": {
                                "ru": "Он один из старых <w>надёжных сотрудников</w> в отделе, кто знает, как всё работает."
                            }
                        },
                        {
                            "text": "I decided to take my old <w>reliable</w> to the campsite instead of the new truck.",
                            "target_lang_translations": {
                                "ru": "Я решил взять в кемпинг свою <w>старую проверенную машину</w> вместо нового грузовика."
                            }
                        }
                    ],
                    "common_phrases": [
                        "old reliable"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in the phrase 'old reliable'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или предмет, который заслуживает доверия или на которого можно положиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надёжный человек",
                                "target_lang_sense_clarification": "о людях"
                            },
                            {
                                "target_lang_word": "проверенная вещь",
                                "target_lang_sense_clarification": "о предметах (разговорное)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
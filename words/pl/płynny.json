            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7a504cc4-ece3-3b4f-b4d1-b404994ed2db",
                    "sense_definition": "Będący w stanie ciekłym, mogący swobodnie płynąć jak woda; przeciwieństwo stanu stałego i gazowego.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "state_of_matter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Woda w temperaturze pokojowej ma stan <w>płynny</w>.",
                            "target_lang_translations": {
                                "ru": "Вода при комнатной температуре имеет <w>жидкое</w> состояние."
                            }
                        },
                        {
                            "text": "Musisz wlać <w>płynny</w> detergent do odpowiedniej przegródki w pralce.",
                            "target_lang_translations": {
                                "ru": "Ты должен налить <w>жидкое</w> моющее средство в соответствующий отсек стиральной машины."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciekły"
                    ],
                    "antonyms": [
                        "stały"
                    ],
                    "common_phrases": [
                        "stan płynny",
                        "płynny miód",
                        "płynny metal",
                        "płynne złoto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся в жидком состоянии, способный свободно течь, как вода; противоположность твердому и газообразному."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жидкий",
                                "target_lang_sense_clarification": "о физическом состоянии вещества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5e7f35e-de54-3286-8b29-0233b295f15e",
                    "sense_definition": "Odznaczający się swobodą, poprawnością i brakiem wahania w mowie lub piśmie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fluency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej <w>płynny</w> angielski pozwolił jej na łatwe znalezienie pracy w Londynie.",
                            "target_lang_translations": {
                                "ru": "Её <w>свободный</w> английский позволил ей легко найти работу в Лондоне."
                            }
                        },
                        {
                            "text": "Po latach nauki osiągnął <w>płynny</w> styl pisania.",
                            "target_lang_translations": {
                                "ru": "После многих лет обучения он выработал <w>плавный</w> стиль письма."
                            }
                        }
                    ],
                    "synonyms": [
                        "biegły",
                        "swobodny",
                        "składny"
                    ],
                    "antonyms": [
                        "łamany"
                    ],
                    "common_phrases": [
                        "płynny angielski",
                        "płynna mowa",
                        "płynne czytanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отличающийся свободой, правильностью и отсутствием запинок в речи или на письме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беглый",
                                "target_lang_sense_clarification": "о речи или чтении"
                            },
                            {
                                "target_lang_word": "свободный",
                                "target_lang_sense_clarification": "о владении языком"
                            },
                            {
                                "target_lang_word": "плавный",
                                "target_lang_sense_clarification": "о стиле речи или письма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fad2df33-ba84-36c9-af4d-32ac0f02614b",
                    "sense_definition": "Taki, który odbywa się gładko, bez przerw, zahamowań i gwałtownych zmian.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "smoothness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tancerka wykonywała na scenie bardzo <w>płynne</w> ruchy.",
                            "target_lang_translations": {
                                "ru": "Танцовщица совершала на сцене очень <w>плавные</w> движения."
                            }
                        },
                        {
                            "text": "Dzięki nowej autostradzie ruch samochodowy jest teraz <w>płynny</w>.",
                            "target_lang_translations": {
                                "ru": "Благодаря новой автостраде движение автомобилей теперь <w>беспрепятственное</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "harmonijny",
                        "swobodny"
                    ],
                    "antonyms": [
                        "chaotyczny"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9cefeef8-3a95-338c-811f-6d8b6aa04516",
                    "sense_definition": "Occurring once every quarter year (three months).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company publishes a <w>quarterly</w> report to update investors on its financial status.",
                            "target_lang_translations": {
                                "ru": "Компания публикует <w>квартальный</w> отчет, чтобы информировать инвесторов о своем финансовом состоянии."
                            }
                        },
                        {
                            "text": "We scheduled a <w>quarterly</w> meeting to discuss our progress over the last three months.",
                            "target_lang_translations": {
                                "ru": "Мы запланировали <w>ежеквартальное</w> собрание, чтобы обсудить наш прогресс за последние три месяца."
                            }
                        }
                    ],
                    "synonyms": [
                        "three-monthly"
                    ],
                    "common_phrases": [
                        "quarterly report",
                        "quarterly earnings",
                        "quarterly meeting",
                        "quarterly review"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий раз в квартал (каждые три месяца)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ежекварта́льный",
                                "target_lang_sense_clarification": "повторяющийся каждый квартал"
                            },
                            {
                                "target_lang_word": "кварта́льный",
                                "target_lang_sense_clarification": "относящийся к кварталу (отчетный период)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "efec621c-441f-3ec2-b998-863db9f7a840",
                    "sense_definition": "Divided into four parts crosswise, typically referring to a coat of arms.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "heraldry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient shield was <w>quarterly</w> gold and red, representing the two united families.",
                            "target_lang_translations": {
                                "ru": "Древний щит был <w>четырёхчастным</w>, золотым с красным, представляя две объединившиеся семьи."
                            }
                        },
                        {
                            "text": "He designed a <w>quarterly</w> flag that combined symbols from four different regions.",
                            "target_lang_translations": {
                                "ru": "Он разработал <w>четырёхчастный</w> флаг, объединяющий символы четырех разных регионов."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разделённый на четыре части (обычно о гербе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "четырёхчастный",
                                "target_lang_sense_clarification": "геральдический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "eba91ccb-c748-30fb-bab3-11643ee9af75",
                    "sense_definition": "Once every quarter year (three months).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee meets <w>quarterly</w> to review the budget and plan for the next season.",
                            "target_lang_translations": {
                                "ru": "Комитет собирается <w>ежеквартально</w> для пересмотра бюджета и планирования на следующий сезон."
                            }
                        },
                        {
                            "text": "Interest on the savings account is calculated daily but paid <w>quarterly</w>.",
                            "target_lang_translations": {
                                "ru": "Проценты по сберегательному счету начисляются ежедневно, но выплачиваются <w>поквартально</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "meet quarterly",
                        "paid quarterly",
                        "updated quarterly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Раз в квартал (каждые три месяца)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ежекварта́льно",
                                "target_lang_sense_clarification": "регулярно каждый квартал"
                            },
                            {
                                "target_lang_word": "покварта́льно",
                                "target_lang_sense_clarification": "исчисляясь или производясь по кварталам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc3f26d0-7957-3ed0-a3ab-327276fed768",
                    "sense_definition": "In the four quarters of a shield, or in two diagonally opposite quarters.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "heraldry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The symbols were arranged <w>quarterly</w> to show the alliance between the two noble houses.",
                            "target_lang_translations": {
                                "ru": "Символы были расположены <w>по четвертям</w>, чтобы показать союз двух благородных домов."
                            }
                        },
                        {
                            "text": "The coat of arms is divided <w>quarterly</w>, featuring a lion and a unicorn.",
                            "target_lang_translations": {
                                "ru": "Герб разделён <w>на четыре части</w>, изображая льва и единорога."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В четырёх частях щита или по диагонали."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по четвертям",
                                "target_lang_sense_clarification": "расположение элементов"
                            },
                            {
                                "target_lang_word": "на четыре части",
                                "target_lang_sense_clarification": "о делении щита"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "424f9259-c8cb-3ca7-a026-07bd8846d3ba",
                    "sense_definition": "A periodical publication that appears four times per year.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {

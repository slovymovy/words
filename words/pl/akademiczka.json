            "senses": [
                {
                    "sense_id": "38c72a2e-7e7b-3210-893b-cce2e3ae3395",
                    "sense_definition": "Studentka wyższej uczelni.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "student",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młoda <w>akademiczka</w> spędzała całe dnie w bibliotece uniwersyteckiej.",
                            "target_lang_translations": {
                                "ru": "Молодая <w>студентка</w> проводила целые дни в университетской библиотеке."
                            }
                        },
                        {
                            "text": "Jako pilna <w>akademiczka</w> zawsze oddawała wszystkie prace w terminie.",
                            "target_lang_translations": {
                                "ru": "Будучи прилежной <w>студенткой</w>, она всегда сдавала все работы в срок."
                            }
                        }
                    ],
                    "synonyms": [
                        "studentka"
                    ],
                    "common_phrases": [
                        "pilna akademiczka",
                        "młoda akademiczka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Студентка высшего учебного заведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "студентка",
                                "target_lang_sense_clarification": "учащаяся вуза"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fdb4d19b-0f1b-3b82-bc12-0bf0bb5188a3",
                    "sense_definition": "Członkini akademii nauk lub sztuk pięknych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "member",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znana <w>akademiczka</w> wygłosiła wykład na temat fizyki kwantowej.",
                            "target_lang_translations": {
                                "ru": "Известный <w>академик</w> прочитала лекцию о квантовой физике."
                            }
                        },
                        {
                            "text": "Została przyjęta do grona uczonych jako nowa <w>akademiczka</w>.",
                            "target_lang_translations": {
                                "ru": "Она была принята в круг учёных как новый <w>академик</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "honorowa akademiczka",
                        "tytuł akademiczki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Член академии наук или искусств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "академик",
                                "target_lang_sense_clarification": "звание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0ab0330e-8b37-3534-8714-7eec67d4191e",
                    "sense_definition": "Artystka tworząca w stylu akademizmu lub zwolenniczka tradycyjnych reguł w sztuce.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "art",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej obrazy były krytykowane przez awangardę, ponieważ była zatwardziałą <w>akademiczką</w>.",
                            "target_lang_translations": {
                                "ru": "Её картины критиковались авангардом, поскольку она была убеждённой <w>академисткой</w>."
                            }
                        },
                        {
                            "text": "Jako <w>akademiczka</w> dbała o idealne proporcje w swoich rzeźbach.",
                            "target_lang_translations": {
                                "ru": "Как <w>академистка</w>, она заботилась об идеальных пропорциях в своих скульптурах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "typowa akademiczka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Художница, творящая в стиле академизма, или сторонница традиционных правил в искусстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "академистка",
                                "target_lang_sense_clarification": "последовательница академизма"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
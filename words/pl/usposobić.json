    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "88163e74-8202-3aa7-aae7-7c977d8f43b0",
                    "sense_definition": "Sprawić, że ktoś nabierze określonego stosunku do kogoś lub czegoś, albo wywołać u kogoś pewien nastrój.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego uprzejme słowa natychmiast <usposobiły> mnie do niego przychylnie.",
                            "target_lang_translations": {
                                "ru": "Его вежливые слова немедленно <w>расположили</w> меня к нему."
                            }
                        },
                        {
                            "text": "Ta spokojna muzyka <usposobiła> wszystkich gości melancholijnie.",
                            "target_lang_translations": {
                                "ru": "Эта спокойная музыка <w>настроила</w> всех гостей на меланхолический лад."
                            }
                        }
                    ],
                    "synonyms": [
                        "nastawić",
                        "nastroić"
                    ],
                    "antonyms": [
                        "zrazić"
                    ],
                    "common_phrases": [
                        "usposobić przychylnie",
                        "usposobić wrogo",
                        "usposobić optymistycznie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вызвать у кого-то определенное отношение к кому-либо или чему-либо, либо создать у кого-то определенное настроение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "настроить",
                                "target_lang_sense_clarification": "придать определенное направление чувствам или мыслям"
                            },
                            {
                                "target_lang_word": "расположить",
                                "target_lang_sense_clarification": "вызвать симпатию или благоприятное отношение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "658c408c-e9ee-381a-870d-5ad48e69a16f",
                    "sense_definition": "Nabrać określonego nastawienia do otoczenia lub wprawić się w pewien nastrój (używane z zaimkiem zwrotnym \"się\").",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz pozytywnie <usposobić> się do nadchodzących zmian w firmie.",
                            "target_lang_translations": {
                                "ru": "Ты должен позитивно <w>настроиться</w> на грядущие изменения в фирме."
                            }
                        },
                        {
                            "text": "Trudno <usposobić> się życzliwie do kogoś, kto nas oszukał.",
                            "target_lang_translations": {
                                "ru": "Трудно доброжелательно <w>настроиться</w> по отношению к тому, кто нас обманул."
                            }
                        }
                    ],
                    "synonyms": [
                        "nastawić się"
                    ],
                    "antonyms": [
                        "zrazić się"
                    ],
                    "common_phrases": [
                        "usposobić się pozytywnie",
                        "usposobić się negatywnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принять определенное отношение к окружающему или привести себя в определенное настроение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "настроиться",
                                "target_lang_sense_clarification": "внутренне подготовиться, принять определенную установку"
                            }
                        ]
                    }
                }
            ]
        }
    ]

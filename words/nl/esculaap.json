        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "164dccfa-a097-32e8-9f14-5c9d39bc7f2d",
                    "sense_definition": "Het symbool van de geneeskunde, bestaande uit een staf waar een slang omheen kronkelt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "symbol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op zijn visitekaartje is een <w>esculaap</w> geprint.",
                            "target_lang_translations": {
                                "en": "A <w>staff of Aesculapius</w> is printed on his business card.",
                                "ru": "На его визитке напечатан <w>посох Асклепия</w>."
                            }
                        },
                        {
                            "text": "Je ziet vaak een afbeelding van een <w>esculaap</w> op de ruit van een apotheek.",
                            "target_lang_translations": {
                                "en": "You often see an image of a <w>Rod of Asclepius</w> on the window of a pharmacy.",
                                "ru": "На витрине аптеки часто можно увидеть изображение <w>посоха Асклепия</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "asclepiusstaf"
                    ],
                    "target_lang_definitions": {
                        "en": "The symbol of medicine, consisting of a staff with a snake coiled around it.",
                        "ru": "Символ медицины, представляющий собой посох, обвитый змеей."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "staff of Aesculapius",
                                "target_lang_sense_clarification": "symbol of medicine"
                            },
                            {
                                "target_lang_word": "Rod of Asclepius",
                                "target_lang_sense_clarification": "symbol of medicine"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "посох Асклепия",
                                "target_lang_sense_clarification": "символ медицины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d4d75c1-1e29-3423-bd1d-7d124f9821d3",
                    "sense_definition": "Een arts of geneesheer.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude <w>esculaap</w> schreef de patiënt een medicijn voor.",
                            "target_lang_translations": {
                                "en": "The old <w>physician</w> prescribed a medicine for the patient.",
                                "ru": "Старый <w>эскулап</w> выписал пациенту лекарство."
                            }
                        },
                        {
                            "text": "Hij besloot geneeskunde te studeren om later als <w>esculaap</w> aan het werk te gaan.",
                            "target_lang_translations": {
                                "en": "He decided to study medicine to work as a <w>doctor</w> later.",
                                "ru": "Он решил изучать медицину, чтобы в будущем работать <w>эскулапом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "arts",
                        "dokter",
                        "geneesheer"
                    ],
                    "target_lang_definitions": {
                        "en": "A doctor or physician.",
                        "ru": "Врач или доктор (часто используется в ироническом или устаревшем смысле)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "physician",
                                "target_lang_sense_clarification": "medical professional (formal/archaic)"
                            },
                            {
                                "target_lang_word": "doctor",
                                "target_lang_sense_clarification": "medical professional"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "эскулап",
                                "target_lang_sense_clarification": "врач (иронично)"
                            },
                            {
                                "target_lang_word": "врачеватель",
                                "target_lang_sense_clarification": "лекарь (устар.)"
                            }
                        ]
                    }
                }
            ]
        },
        {

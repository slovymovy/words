{
    "word_family": [
        "demonisch",
        "demoniseren",
        "demonisme"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "51a3fba7-bb92-3955-b7d5-df8d3a193df7",
                    "sense_definition": "Een boze geest, gevallen engel of ander bovennatuurlijk wezen dat vaak geassocieerd wordt met het kwaad.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "supernatural_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de horrorfilm nam een angstaanjagende <w>demon</w> bezit van het oude huis.",
                            "target_lang_translations": {
                                "ru": "В фильме ужасов пугающий <w>демон</w> завладел старым домом."
                            }
                        },
                        {
                            "text": "Hij moest lang vechten tegen zijn innerlijke <w>demon</w> om zijn verslaving te overwinnen.",
                            "target_lang_translations": {
                                "ru": "Ему пришлось долго бороться со своим внутренним <w>демоном</w>, чтобы победить зависимость."
                            }
                        }
                    ],
                    "synonyms": [
                        "demoon",
                        "duivel"
                    ],
                    "common_phrases": [
                        "innerlijke demon",
                        "demon uitdrijven",
                        "bezeten door een demon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Злой дух, падший ангел или сверхъестественное существо, часто ассоциируемое со злом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "демон",
                                "target_lang_sense_clarification": "злой дух"
                            },
                            {
                                "target_lang_word": "бес",
                                "target_lang_sense_clarification": "зловредный дух"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6bf9a34-440b-327d-b91e-073b66c58588",
                    "sense_definition": "Een beschermgeest of lagere godheid in de klassieke mythologie, vaak gezien als een bemiddelaar tussen goden en mensen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "supernatural_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Socrates beweerde dat een persoonlijke <w>demon</w> hem waarschuwde voor verkeerde beslissingen.",
                            "target_lang_translations": {
                                "ru": "Сократ утверждал, что личный <w>демон</w> предостерегал его от неверных решений."
                            }
                        },
                        {
                            "text": "In de Romeinse oudheid werd de <w>demon</w> vereerd als een beschermer van de familie.",
                            "target_lang_translations": {
                                "ru": "В римской древности <w>демон</w> почитался как покровитель семьи."
                            }
                        }
                    ],
                    "synonyms": [
                        "demoon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дух-хранитель или низшее божество в классической мифологии, посредник между богами и людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "демон",
                                "target_lang_sense_clarification": "в античной мифологии"
                            },
                            {
                                "target_lang_word": "даймон",
                                "target_lang_sense_clarification": "дух-покровитель"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
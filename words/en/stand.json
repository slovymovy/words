                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "киоск",
                                "target_lang_sense_clarification": "торговая точка"
                            },
                            {
                                "target_lang_word": "ларёк",
                                "target_lang_sense_clarification": "палатка"
                            },
                            {
                                "target_lang_word": "прилавок",
                                "target_lang_sense_clarification": "торговое место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0639896-109c-3d53-b4e1-e1e2356db86c",
                    "sense_definition": "A frame, rack, or piece of furniture used to hold or support something upright.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please put your wet umbrella in the <w>stand</w> by the door.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, поставь свой мокрый зонт в <w>подставку</w> у двери."
                            }
                        },
                        {
                            "text": "The musician placed his sheet music on the <w>stand</w>.",
                            "target_lang_translations": {
                                "ru": "Музыкант положил ноты на <w>пюпитр</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rack",
                        "holder",
                        "support"
                    ],
                    "common_phrases": [
                        "music stand",
                        "umbrella stand",
                        "nightstand"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рама, вешалка или предмет мебели, используемый для удержания или поддержки чего-либо вертикально."
                    },
                    "translations": {

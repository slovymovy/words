    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "22d97956-a533-36cf-ae4c-5660e0f78809",
                    "sense_definition": "Состояние, при котором отсутствует грязь, пыль или мусор; внешняя опрятность.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В больнице должна быть идеальная <w>чистота</w>.",
                            "target_lang_translations": {
                                "en": "There must be ideal <w>cleanliness</w> in the hospital."
                            }
                        },
                        {
                            "text": "Мы стараемся поддерживать <w>чистоту</w> и порядок в доме.",
                            "target_lang_translations": {
                                "en": "We try to maintain <w>cleanliness</w> and order in the house."
                            }
                        }
                    ],
                    "synonyms": [
                        "опрятность"
                    ],
                    "antonyms": [
                        "грязь",
                        "загрязнённость"
                    ],
                    "common_phrases": [
                        "идеальная чистота",
                        "соблюдать чистоту",
                        "наводить чистоту",
                        "поддерживать чистоту"
                    ],
                    "target_lang_definitions": {
                        "en": "The state of being free from dirt, dust, or trash; external neatness."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cleanliness",
                                "target_lang_sense_clarification": "general state of being clean and hygienic"
                            },
                            {
                                "target_lang_word": "tidiness",
                                "target_lang_sense_clarification": "orderliness and neatness"
                            },
                            {
                                "target_lang_word": "cleanness",
                                "target_lang_sense_clarification": "physical state of being free from dirt"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "426d7440-ef0a-394a-ba7f-3245ac7f99b5",
                    "sense_definition": "Отсутствие посторонних примесей в составе чего-либо; прозрачность и незамутнённость.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Экологи проверяют <w>чистоту</w> воды в горных реках.",
                            "target_lang_translations": {
                                "en": "Ecologists check the <w>purity</w> of the water in mountain rivers."
                            }
                        },
                        {
                            "text": "Для успешного опыта важна высокая <w>чистота</w> эксперимента.",
                            "target_lang_translations": {
                                "en": "High <w>purity</w> of the experiment is important for a successful trial."
                            }
                        }
                    ],
                    "common_phrases": [
                        "чистота воды",
                        "чистота эксперимента",
                        "чистота воздуха",
                        "экологическая чистота"
                    ],
                    "target_lang_definitions": {
                        "en": "The absence of foreign impurities in the composition of something; transparency."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "purity",
                                "target_lang_sense_clarification": "freedom from adulteration or contamination (water, air, metals)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dba75029-8d9a-3793-a5fc-41c9e9602657",
                    "sense_definition": "Нравственная безупречность, честность, отсутствие дурных помыслов.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его отличала удивительная душевная <w>чистота</w> и доброта.",
                            "target_lang_translations": {
                                "en": "He was distinguished by amazing spiritual <w>purity</w> and kindness."
                            }
                        },
                        {
                            "text": "В этом деле важна <w>чистота</w> намерений, а не только результат.",
                            "target_lang_translations": {
                                "en": "In this matter, <w>purity</w> of intentions is important, not just the result."
                            }
                        }
                    ],
                    "common_phrases": [
                        "душевная чистота",
                        "нравственная чистота",
                        "чистота помыслов",
                        "чистота сердца"
                    ],
                    "target_lang_definitions": {
                        "en": "Moral impeccability, honesty, absence of bad thoughts."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "purity",
                                "target_lang_sense_clarification": "moral or spiritual innocence"
                            },
                            {
                                "target_lang_word": "cleanness",
                                "target_lang_sense_clarification": "moral innocence (less common)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae5b2071-2b9e-399b-ba78-d34beff079ec",
                    "sense_definition": "Чёткость и ясность звучания, произношения или очертаний; отсутствие искажений.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Музыкант поразил слушателей <w>чистотой</w> звука своей скрипки.",
                            "target_lang_translations": {
                                "en": "The musician amazed the listeners with the <w>purity</w> of his violin's sound."
                            }
                        },
                        {
                            "text": "Учитель обращал внимание на <w>чистоту</w> речи учеников.",
                            "target_lang_translations": {
                                "en": "The teacher paid attention to the <w>purity</w> of the students' speech."
                            }
                        }
                    ],
                    "common_phrases": [
                        "чистота звука",
                        "чистота речи",
                        "чистота линий",
                        "чистота голоса"
                    ],
                    "target_lang_definitions": {
                        "en": "Distinctness and clarity of sound, pronunciation, or outlines."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "purity",
                                "target_lang_sense_clarification": "flawlessness of sound or tone"
                            },
                            {
                                "target_lang_word": "clarity",
                                "target_lang_sense_clarification": "clearness of speech or lines"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
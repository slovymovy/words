    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3b4a7012-2409-3132-8109-fccb7d214957",
                    "sense_definition": "Действие по значению глагола <w>добавлять</w>; процесс присоединения чего-либо к чему-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Автоматическое <добавление> новых контактов в список сэкономило мне время.",
                            "target_lang_translations": {
                                "en": "The automatic <addition> of new contacts to the list saved me time."
                            }
                        },
                        {
                            "text": "Постепенное <добавление> муки поможет сделать тесто мягким и эластичным.",
                            "target_lang_translations": {
                                "en": "Gradual <addition> of flour will help make the dough soft and elastic."
                            }
                        }
                    ],
                    "synonyms": [
                        "прибавление"
                    ],
                    "antonyms": [
                        "изъятие",
                        "вычитание"
                    ],
                    "common_phrases": [
                        "добавление файлов",
                        "добавление в друзья",
                        "добавление сахара"
                    ],
                    "target_lang_definitions": {
                        "en": "The action of adding; the process of joining something to something else."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "addition",
                                "target_lang_sense_clarification": "the act or process"
                            },
                            {
                                "target_lang_word": "adding",
                                "target_lang_sense_clarification": "process"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f386f5b7-594d-3c96-b7ad-2062e75af88e",
                    "sense_definition": "То, что было добавлено к чему-либо; дополнение или приложение.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это <добавление> к контракту уточняет условия оплаты труда.",
                            "target_lang_translations": {
                                "en": "This <addendum> to the contract clarifies the payment terms."
                            }
                        },
                        {
                            "text": "В конце книги автор сделал небольшое <добавление> с пояснениями.",
                            "target_lang_translations": {
                                "en": "At the end of the book, the author made a small <addition> with explanations."
                            }
                        }
                    ],
                    "synonyms": [
                        "добавка",
                        "прибавка",
                        "прибавление"
                    ],
                    "common_phrases": [
                        "важное добавление",
                        "небольшое добавление",
                        "приятное добавление"
                    ],
                    "target_lang_definitions": {
                        "en": "Something that has been added to something else; a supplement or appendix."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "addition",
                                "target_lang_sense_clarification": "thing added"
                            },
                            {
                                "target_lang_word": "supplement",
                                "target_lang_sense_clarification": "something added to complete or enhance"
                            },
                            {
                                "target_lang_word": "addendum",
                                "target_lang_sense_clarification": "documentary addition"
                            }
                        ]
                    }
                }
            ]
        }
    ]

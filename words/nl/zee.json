                {
                    "sense_id": "a639504e-7102-30dc-82cc-6957d29775ea",
                    "sense_definition": "Een uitgestrekt oppervlak zout water dat het grootste deel van de aarde bedekt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "water_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij gaan op vakantie naar de kust en liggen de hele dag aan <w>zee</w>.",
                            "target_lang_translations": {
                                "en": "We are going on vacation to the coast and will lie by the <w>sea</w> all day.",
                                "ru": "Мы едем в отпуск на побережье и весь день лежим у <w>моря</w>."
                            }
                        },
                        {
                            "text": "Ze zeilden over de open <w>zee</w> om nieuwe landen te ontdekken.",
                            "target_lang_translations": {
                                "en": "They sailed across the open <w>sea</w> to discover new lands.",
                                "ru": "Они плыли по открытому <w>морю</w>, чтобы открыть новые земли."
                            }
                        }
                    ],
                    "antonyms": [
                        "land"
                    ],
                    "common_phrases": [
                        "aan zee",
                        "op zee",
                        "volle zee",
                        "kalme zee"
                    ],
                    "target_lang_definitions": {
                        "en": "A vast expanse of salt water that covers most of the earth.",
                        "ru": "Обширное пространство соленой воды, покрывающее большую часть Земли."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sea",
                                "target_lang_sense_clarification": "large body of salt water"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "море",
                                "target_lang_sense_clarification": "водоем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98bd4c1f-bc84-3777-ad39-e8bfe12adc14",
                    "sense_definition": "Een zeer grote hoeveelheid of massa.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nu hij met pensioen is, heeft hij opeens een <w>zee</w> van tijd.",
                            "target_lang_translations": {
                                "en": "Now that he is retired, he suddenly has a <w>sea</w> of time.",
                                "ru": "Теперь, когда он на пенсии, у него вдруг появилось <w>море</w> времени."
                            }
                        },
                        {
                            "text": "Vanaf het balkon keken we uit over een <w>zee</w> van kleurrijke bloemen.",
                            "target_lang_translations": {
                                "en": "From the balcony, we looked out over a <w>sea</w> of colorful flowers.",
                                "ru": "С балкона мы смотрели на <w>море</w> разноцветных цветов."
                            }
                        }
                    ],
                    "synonyms": [
                        "massa",
                        "overvloed",
                        "menigte"
                    ],
                    "common_phrases": [
                        "een zee van tijd",
                        "een zee van mensen",
                        "een zee van licht"
                    ],
                    "target_lang_definitions": {

                {
                    "sense_id": "f3872805-b3ea-379d-9c05-c1572ce55923",
                    "sense_definition": "Osoba zajmująca się merytoryczną opieką nad zbiorami muzealnymi lub organizacją i koncepcją wystaw artystycznych",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "culture_art",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znany <w>kurator</w> oprowadził dziennikarzy po nowej wystawie sztuki nowoczesnej."
                        },
                        {
                            "text": "Jako <w>kurator</w> tego muzeum, dba on o właściwą konserwację eksponatów."
                        }
                    ],
                    "common_phrases": [
                        "kurator wystawy",
                        "kurator sztuki",
                        "kurator muzeum"
                    ]
                },
                {
                    "sense_id": "b6658c43-7d9d-3e4e-974e-54050f3f75c4",
                    "sense_definition": "Urzędnik państwowy sprawujący nadzór pedagogiczny nad szkołami i placówkami oświatowymi w danym województwie",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "education_admin",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Małopolski <w>kurator</w> oświaty podjął decyzję o zamknięciu szkół z powodu mrozów."
                        },
                        {
                            "text": "Dyrektor szkoły został wezwany na spotkanie przez <w>kuratora</w>."
                        }
                    ],
                    "common_phrases": [

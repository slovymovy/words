            "senses": [
                {
                    "sense_id": "4f872f12-a482-3737-a2ec-20656d61c61a",
                    "sense_definition": "Ясно, явственно, хорошо различимо слухом или зрением.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <чётко> вижу надпись на доске.",
                            "target_lang_translations": {
                                "en": "I can <clearly> see the writing on the board."
                            }
                        },
                        {
                            "text": "Студент <чётко> ответил на вопрос экзаменатора.",
                            "target_lang_translations": {
                                "en": "The student answered the examiner's question <clearly>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ясно",
                        "отчётливо"
                    ],
                    "antonyms": [
                        "расплывчато"
                    ],
                    "common_phrases": [
                        "чётко видеть",
                        "чётко понимать",
                        "чётко помнить",
                        "говорить чётко"
                    ],
                    "target_lang_definitions": {
                        "en": "Clearly, distinctly, easily distinguishable by hearing or sight."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "clearly",
                                "target_lang_sense_clarification": "in a clear manner; without ambiguity"
                            },
                            {
                                "target_lang_word": "distinctly",
                                "target_lang_sense_clarification": "sharply; with clear outlines or sound"
                            }
                        ]
                    }
                },
                {

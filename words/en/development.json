    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "47abd0ce-ffb2-37d4-ae78-2754d2dacda4",
                    "sense_definition": "The process of growing, changing, or becoming more advanced.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "process_growth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Education plays a vital role in the <w>development</w> of a country.",
                            "target_lang_translations": {
                                "ru": "Образование играет жизненно важную роль в <w>развитии</w> страны."
                            }
                        },
                        {
                            "text": "The <w>development</w> of the new software took over two years to complete.",
                            "target_lang_translations": {
                                "ru": "<w>Разработка</w> нового программного обеспечения заняла более двух лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "growth",
                        "advancement",
                        "progress",
                        "evolution",
                        "expansion"
                    ],
                    "antonyms": [
                        "decline",
                        "stagnation",
                        "regression"
                    ],
                    "common_phrases": [
                        "economic development",
                        "professional development",
                        "sustainable development",
                        "child development"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс роста, изменения или перехода на более высокий уровень развития."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "развитие",
                                "target_lang_sense_clarification": "процесс роста или совершенствования"
                            },
                            {
                                "target_lang_word": "совершенствование",
                                "target_lang_sense_clarification": "улучшение качеств"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2b2b00c-7d9a-3af2-bc6f-bad5b2bbf119",
                    "sense_definition": "The process of creating new products, ideas, or systems, or improving existing ones.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "process_growth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company invests a significant amount of money in research and <w>development</w>.",
                            "target_lang_translations": {
                                "ru": "Компания вкладывает значительные средства в исследования и <w>разработки</w>."
                            }
                        },
                        {
                            "text": "She works in the product <w>development</w> department of a large tech firm.",
                            "target_lang_translations": {
                                "ru": "Она работает в отделе <w>разработки</w> продукции крупной технологической фирмы."
                            }
                        }
                    ],
                    "synonyms": [
                        "innovation",
                        "design"
                    ],
                    "common_phrases": [
                        "research and development",
                        "product development",
                        "software development"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс создания новых продуктов, идей или систем, либо улучшение существующих."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разработка",
                                "target_lang_sense_clarification": "создание продуктов или технологий"
                            },
                            {
                                "target_lang_word": "создание",
                                "target_lang_sense_clarification": "процесс творения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c9b9e36-fea5-3111-b943-9ae6ddc22ddf",
                    "sense_definition": "A recent event, occurrence, or piece of news that changes a situation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "event_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are waiting for further <w>developments</w> before making a decision.",
                            "target_lang_translations": {
                                "ru": "Мы ждем дальнейшего <w>развития событий</w>, прежде чем принимать решение."
                            }
                        },
                        {
                            "text": "The news channel reported the latest <w>developments</w> in the peace negotiations.",
                            "target_lang_translations": {
                                "ru": "Новостной канал сообщил о последних <w>событиях</w> в мирных переговорах."
                            }
                        }
                    ],
                    "synonyms": [
                        "event",
                        "incident",
                        "occurrence",
                        "change"
                    ],
                    "common_phrases": [
                        "latest developments",
                        "recent developments",
                        "new development",
                        "further developments"
                    ],
                    "target_lang_definitions": {
                        "ru": "Недавнее событие, происшествие или новость, меняющие текущую ситуацию."

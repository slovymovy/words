{
    "word_family": [
        "pozostałość",
        "pozostały",
        "pozostawić",
        "zostać",
        "zostawać"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fa6aaa68-f396-3288-8d19-cab6e220969b",
                    "sense_definition": "Przebywać nadal w tym samym miejscu, nie odchodzić ani nie wyjeżdżać.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas burzy najlepiej <w>pozostawać</w> w bezpiecznym domu.",
                            "target_lang_translations": {
                                "ru": "Во время грозы лучше всего <w>оставаться</w> в безопасном доме."
                            }
                        },
                        {
                            "text": "Nasz pies zawsze <w>pozostaje</w> na miejscu, gdy wydamy mu taką komendę.",
                            "target_lang_translations": {
                                "ru": "Наш пес всегда <w>остается</w> на месте, когда мы даем ему такую команду."
                            }
                        }
                    ],
                    "synonyms": [
                        "zostawać"
                    ],
                    "antonyms": [
                        "odchodzić",
                        "wyjeżdżać",
                        "opuszczać"
                    ],
                    "common_phrases": [
                        "pozostawać w domu",
                        "pozostawać na miejscu",
                        "pozostawać w tyle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продолжать находиться в том же месте, не уходить и не уезжать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставаться",
                                "target_lang_sense_clarification": "находиться где-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5fd7d4b-38b6-3352-96f6-0bf6778b9cab",
                    "sense_definition": "Utrzymywać się ciągle w tym samym stanie, nie zmieniać swoich cech, poglądów lub sytuacji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo upływu lat, oni nadal <w>pozostają</w> bliskimi przyjaciółmi.",
                            "target_lang_translations": {
                                "ru": "Несмотря на прошедшие годы, они по-прежнему <w>остаются</w> близкими друзьями."
                            }
                        },
                        {
                            "text": "Proszę <w>pozostawać</w> w kontakcie telefonicznym, gdyby coś się wydarzyło.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>оставайтесь</w> на телефонной связи на случай, если что-то случится."
                            }
                        }
                    ],
                    "synonyms": [
                        "zostawać",
                        "przyzostawać się"
                    ],
                    "antonyms": [
                        "zmieniać się"
                    ],
                    "common_phrases": [
                        "pozostawać w kontakcie",
                        "pozostawać bez zmian",
                        "pozostawać pod wrażeniem",
                        "pozostawać do dyspozycji",
                        "pozostawać przy swoim"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сохраняться в прежнем состоянии, не менять своих качеств, взглядов или положения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставаться",
                                "target_lang_sense_clarification": "сохранять состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ddcd5a36-359e-38b7-86f9-840fd415fee8",
                    "sense_definition": "Być tym, co zostało po usunięciu, zużyciu lub zniknięciu reszty; być jedyną dostępną opcją.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "residue",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej trudnej sytuacji <w>pozostaje</w> nam tylko cierpliwie czekać na wyniki.",
                            "target_lang_translations": {
                                "ru": "В этой сложной ситуации нам <w>остается</w> только терпеливо ждать результатов."
                            }
                        },
                        {
                            "text": "Po wielkiej uczcie na stole <w>pozostawały</w> jedynie puste talerze i okruchy.",
                            "target_lang_translations": {
                                "ru": "После большого пира на столе <w>оставались</w> лишь пустые тарелки и крошки."
                            }
                        }
                    ],
                    "synonyms": [
                        "zostawać",
                        "ostawać"
                    ],
                    "antonyms": [
                        "znikać",
                        "kończyć się"
                    ],
                    "common_phrases": [
                        "nic nie pozostaje",
                        "pozostaje tylko jedno",
                        "pozostaje do zrobienia",
                        "pozostaje kwestią czasu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть тем, что сохранилось после исчезновения остального; быть единственным возможным вариантом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставаться",
                                "target_lang_sense_clarification": "быть в остатке"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
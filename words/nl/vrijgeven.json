            "senses": [
                {
                    "sense_id": "ea31934f-7e8c-3dd4-b2bf-d871e25db651",
                    "sense_definition": "Beschikbaar stellen of toegankelijk maken voor gebruik (zoals wegen, gebouwen of software)",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "availability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie heeft de weg na het ongeluk weer <w>vrijgegeven</w>.",
                            "target_lang_translations": {
                                "ru": "Полиция снова <w>открыла</w> дорогу после аварии.",
                                "en": "The police <w>reopened</w> the road after the accident."
                            }
                        },
                        {
                            "text": "Het softwarebedrijf zal de nieuwe update volgende week officieel <w>vrijgeven</w>.",
                            "target_lang_translations": {
                                "ru": "Компания-разработчик программного обеспечения официально <w>выпустит</w> новое обновление на следующей неделе.",
                                "en": "The software company will officially <w>release</w> the new update next week."
                            }
                        }
                    ],
                    "synonyms": [
                        "openstellen",
                        "beschikbaar stellen"
                    ],
                    "antonyms": [
                        "afsluiten",
                        "blokkeren"
                    ],
                    "common_phrases": [
                        "de weg vrijgeven",
                        "voor gebruik vrijgeven",
                        "officieel vrijgeven"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "separable verb"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать доступным для использования или открыть (например, дороги, здания или программное обеспечение)",
                        "en": "To make available or accessible for use (such as roads, buildings, or software)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открыть",
                                "target_lang_sense_clarification": "дорогу, территорию"
                            },
                            {
                                "target_lang_word": "выпустить",
                                "target_lang_sense_clarification": "программное обеспечение, продукт"
                            },
                            {
                                "target_lang_word": "разблокировать",
                                "target_lang_sense_clarification": "снять блокировку"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "release",
                                "target_lang_sense_clarification": "software, products"
                            },
                            {
                                "target_lang_word": "open",
                                "target_lang_sense_clarification": "roads, buildings"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1bbb225-cfc5-31ab-96d0-e6ff79bd761b",
                    "sense_definition": "Openbaar maken van informatie, documenten of namen die eerder geheim of afgeschermd waren",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "disclosure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De minister moest de vertrouwelijke documenten onder druk <w>vrijgeven</w>.",
                            "target_lang_translations": {
                                "ru": "Министру пришлось <w>обнародовать</w> конфиденциальные документы под давлением.",
                                "en": "The minister had to <w>release</w> the confidential documents under pressure."
                            }
                        },
                        {
                            "text": "De politie wil de identiteit van het slachtoffer nog niet <w>vrijgeven</w>.",
                            "target_lang_translations": {
                                "ru": "Полиция пока не хочет <w>раскрывать</w> личность жертвы.",
                                "en": "The police do not want to <w>release</w> the victim's identity yet."
                            }
                        }
                    ],
                    "synonyms": [
                        "openbaar maken",
                        "publiceren",
                        "onthullen"
                    ],
                    "antonyms": [
                        "geheimhouden",
                        "verzwijgen"
                    ],
                    "common_phrases": [
                        "documenten vrijgeven",
                        "informatie vrijgeven",
                        "namen vrijgeven"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "separable verb"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать общедоступной ранее скрытую информацию, документы или имена",
                        "en": "To make public previously hidden information, documents, or names"
                    },
                    "translations": {

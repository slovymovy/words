            "senses": [
                {
                    "sense_id": "f9795b6d-53d8-3b2c-ad73-0659d43c5d32",
                    "sense_definition": "Zajmujący niższą pozycję w hierarchii w stosunku do innych lub mniej ważny.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "hierarchy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej dużej korporacji zajmuje on raczej <w>podrzędne</w> stanowisko.",
                            "target_lang_translations": {
                                "ru": "В этой крупной корпорации он занимает довольно <w>второстепенную</w> должность."
                            }
                        },
                        {
                            "text": "To jest dla nas sprawa <w>podrzędna</w>, więc zajmiemy się nią w przyszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "Для нас это дело <w>второстепенное</w>, поэтому мы займёмся им на следующей неделе."
                            }
                        }
                    ],
                    "antonyms": [
                        "nadrzędny"
                    ],
                    "common_phrases": [
                        "rola podrzędna",
                        "stanowisko podrzędne",
                        "znaczenie podrzędne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Занимающий более низкое положение в иерархии или менее важный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "второстепенный",
                                "target_lang_sense_clarification": "не основной, менее важный"
                            },
                            {
                                "target_lang_word": "подчинённый",
                                "target_lang_sense_clarification": "занимающий низшее служебное положение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d48bd7a-7a39-3e94-9d52-4209cfb6f157",
                    "sense_definition": "W gramatyce: taki, który wynika z innego elementu zdania i jest od niego zależny.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hierarchy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel wyjaśnił nam dzisiaj, jak rozpoznać zdanie <w>podrzędne</w>.",
                            "target_lang_translations": {
                                "ru": "Учитель объяснил нам сегодня, как распознать <w>придаточное</w> предложение."
                            }
                        },
                        {
                            "text": "W tym przykładzie przecinek oddziela zdanie nadrzędne od <w>podrzędnego</w>.",
                            "target_lang_translations": {
                                "ru": "В этом примере запятая отделяет главное предложение от <w>придаточного</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "nadrzędny"
                    ],
                    "common_phrases": [
                        "zdanie podrzędne",
                        "zdanie złożone podrzędnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "В грамматике: зависящий от другого элемента предложения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "придаточный",
                                "target_lang_sense_clarification": "о предложении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57340c3a-52b0-38b6-9268-25adeb3a2eda",
                    "sense_definition": "Taki, który odznacza się niską jakością lub jest gorszy od innych tego typu.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas podróży musieliśmy nocować w <w>podrzędnym</w> hotelu na obrzeżach miasta.",
                            "target_lang_translations": {
                                "ru": "Во время путешествия нам пришлось ночевать в <w>захудалом</w> отеле на окраине города."
                            }
                        },
                        {
                            "text": "Nie warto inwestować pieniędzy w materiały <w>podrzędnej</w> jakości.",
                            "target_lang_translations": {
                                "ru": "Не стоит вкладывать деньги в материалы <w>низкого</w> качества."
                            }
                        }
                    ],
                    "synonyms": [
                        "pośledni"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "9831f5bb-537f-319b-91c5-ef02e44e64ec",
                    "sense_definition": "Een informele manier om iemand aan te spreken, vergelijkbaar met 'man' of 'gast'.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "address",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hé <w>joh</w>, heb je zin om vanavond iets te gaan drinken?",
                            "target_lang_translations": {
                                "en": "Hey <w>man</w>, do you feel like going for a drink tonight?",
                                "ru": "Эй, <w>дружище</w>, не хочешь сегодня вечером что-нибудь выпить?"
                            }
                        },
                        {
                            "text": "Ik heb je al in geen tijden meer gezien, <w>joh</w>!",
                            "target_lang_translations": {
                                "en": "I haven't seen you in ages, <w>man</w>!",
                                "ru": "Я тебя сто лет не видел, <w>старик</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "man",
                        "gast",
                        "kerel"
                    ],
                    "common_phrases": [
                        "hé joh"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An informal way to address someone, similar to 'man' or 'dude'.",
                        "ru": "Неформальный способ обращения к кому-либо, аналогичный словам «старик» или «приятель»."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "man",
                                "target_lang_sense_clarification": "informal address"
                            },
                            {
                                "target_lang_word": "dude",
                                "target_lang_sense_clarification": "informal address"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "дружище",
                                "target_lang_sense_clarification": "дружеское обращение"
                            },
                            {
                                "target_lang_word": "старик",
                                "target_lang_sense_clarification": "разговорное обращение к другу"
                            },
                            {
                                "target_lang_word": "приятель",
                                "target_lang_sense_clarification": "обращение к знакомому"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7838243-a546-3133-95b2-6501649dd818",
                    "sense_definition": "Een uitroep om nadruk, verbazing, ongeloof of protest te versterken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nee <w>joh</w>, dat meen je toch niet serieus?",
                            "target_lang_translations": {
                                "en": "No <w>way</w>, you can't be serious?",
                                "ru": "Да <w>ладно</w>, ты же это не серьезно?"
                            }
                        },
                        {
                            "text": "Hou toch op <w>joh</w>, daar geloof ik helemaal niets van.",
                            "target_lang_translations": {
                                "en": "Give it a rest <w>man</w>, I don't believe a word of that.",
                                "ru": "Да перестань <w>ты</w>, я этому совсем не верю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nee joh",
                        "kom op joh",
                        "hou op joh",
                        "echt niet joh"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation to reinforce emphasis, surprise, disbelief, or protest.",
                        "ru": "Восклицание для усиления сказанного, выражения удивления, недоверия или протеста."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "man",
                                "target_lang_sense_clarification": "used for emphasis or disbelief"
                            },
                            {
                                "target_lang_word": "no way",
                                "target_lang_sense_clarification": "expression of disbelief"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "да ладно",
                                "target_lang_sense_clarification": "выражение недоверия"
                            },
                            {
                                "target_lang_word": "да ну",
                                "target_lang_sense_clarification": "выражение удивления"
                            },
                            {
                                "target_lang_word": "же",
                                "target_lang_sense_clarification": "усилительная частица"
                            }
                        ]
                    }
                }
            ]
        }

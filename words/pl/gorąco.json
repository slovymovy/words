{
    "word_family": [
        "gorący",
        "gorączka",
        "gorączkować",
        "gorączkowy"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "0e6b5dbc-6bc6-3ce9-ae5e-795d6351a4fc",
                    "sense_definition": "W sposób charakteryzujący się wysoką temperaturą; bardzo ciepło.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "temperature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Latem w tym regionie jest zawsze bardzo <w>gorąco</w>.",
                            "target_lang_translations": {
                                "ru": "Летом в этом регионе всегда очень <w>жарко</w>."
                            }
                        },
                        {
                            "text": "Otwórz okno, bo zrobiło się tutaj strasznie <w>gorąco</w>.",
                            "target_lang_translations": {
                                "ru": "Открой окно, потому что здесь стало ужасно <w>жарко</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "zimno"
                    ],
                    "common_phrases": [
                        "jest mi gorąco",
                        "robi się gorąco"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наречие или предикатив, описывающие высокую температуру воздуха или ощущение тепла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жарко",
                                "target_lang_sense_clarification": "о температуре воздуха или ощущении тепла"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98275048-560e-3bdc-b768-a8be78056977",
                    "sense_definition": "W sposób pełen emocji, entuzjazmu lub serdeczności.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Publiczność <w>gorąco</w> oklaskiwała występ młodych artystów.",
                            "target_lang_translations": {
                                "ru": "Публика <w>горячо</w> аплодировала выступлению молодых артистов."
                            }
                        },
                        {
                            "text": "<w>Gorąco</w> zachęcam wszystkich do wzięcia udziału w dyskusji.",
                            "target_lang_translations": {
                                "ru": "<w>Горячо</w> призываю всех принять участие в дискуссии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gorąco polecam",
                        "gorąco pozdrawiam",
                        "gorąco wierzyć"
                    ],
                    "target_lang_definitions": {
                        "ru": "С большим чувством, энтузиазмом или сердечностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горячо",
                                "target_lang_sense_clarification": "с энтузиазмом, страстно"
                            },
                            {
                                "target_lang_word": "жарко",
                                "target_lang_sense_clarification": "пылко, страстно (например, спорить)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5a7a400-2e31-35cb-a516-48b20c07b3bc",
                    "sense_definition": "W sposób zagrażający bezpieczeństwu lub wywołujący napięcie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "danger",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy policja przyjechała na miejsce, zrobiło się naprawdę <w>gorąco</w>.",
                            "target_lang_translations": {
                                "ru": "Когда полиция прибыла на место, стало действительно <w>жарко</w>."
                            }
                        },
                        {
                            "text": "Wokół skorumpowanego urzędnika zaczyna robić się <w>gorąco</w>.",
                            "target_lang_translations": {
                                "ru": "Вокруг коррумпированного чиновника начинает становиться <w>жарко</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "niebezpiecznie"
                    ],
                    "common_phrases": [
                        "robi się gorąco"
                    ],
                    "target_lang_definitions": {
                        "ru": "О ситуации, которая становится опасной, рискованной или напряженной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жарко",
                                "target_lang_sense_clarification": "об опасной или напряженной ситуации"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8a02b15d-8f8a-34dc-8e3d-60db7d3b6b34",
                    "sense_definition": "Bardzo wysoka temperatura powietrza lub otoczenia.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "temperature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nieznośne <w>gorąco</w> sprawiło, że wszyscy szukali cienia.",
                            "target_lang_translations": {
                                "ru": "Невыносимая <w>жара</w> заставила всех искать тень."
                            }
                        },
                        {
                            "text": "Przez panujące na zewnątrz <w>gorąco</w> nie wychodziliśmy z domu.",
                            "target_lang_translations": {
                                "ru": "Из-за стоящей на улице <w>жары</w> мы не выходили из дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciepło",
                        "skwar",
                        "upał",
                        "spiekota"
                    ],
                    "antonyms": [
                        "zimno"
                    ],
                    "common_phrases": [
                        "fala gorąca",
                        "lejące się gorąco"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень высокая температура воздуха или окружающей среды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жара",
                                "target_lang_sense_clarification": "высокая температура воздуха"
                            },
                            {
                                "target_lang_word": "зной",
                                "target_lang_sense_clarification": "сильная жара, духота"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
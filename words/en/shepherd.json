                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "tending_and_guiding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher <w>shepherded</w> the noisy students into the museum hall.",
                            "target_lang_translations": {
                                "ru": "Учитель <w>сопроводил</w> шумных учеников в зал музея."
                            }
                        },
                        {
                            "text": "She successfully <w>shepherded</w> the complex bill through the legislative process.",
                            "target_lang_translations": {
                                "ru": "Она успешно <w>провела</w> сложный законопроект через законодательный процесс."
                            }
                        }
                    ],
                    "common_phrases": [
                        "shepherd into",
                        "shepherd through"
                    ],
                    "target_lang_definitions": {
                        "ru": "Присматривать, направлять или руководить людьми или процессами, часто обеспечивая движение в нужном направлении."
                    },
                    "translations": {
                        "ru": [

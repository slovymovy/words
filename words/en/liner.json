    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c69a4453-252d-3ae7-8686-50ff412cbb6e",
                    "sense_definition": "A large ship designed to carry passengers on regular long-distance voyages.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "transport_ship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The famous ocean <w>liner</w> crossed the Atlantic in less than a week.",
                            "target_lang_translations": {
                                "ru": "Знаменитый океанский <w>лайнер</w> пересек Атлантику менее чем за неделю."
                            }
                        },
                        {
                            "text": "They spent their holiday on a luxury <w>liner</w> cruising around the Mediterranean.",
                            "target_lang_translations": {
                                "ru": "Они провели отпуск на роскошном <w>лайнере</w>, путешествуя по Средиземному морю."
                            }
                        }
                    ],
                    "synonyms": [
                        "ocean liner",
                        "cruise ship",
                        "vessel"
                    ],
                    "common_phrases": [
                        "ocean liner",
                        "luxury liner",
                        "cruise liner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое судно, предназначенное для перевозки пассажиров на дальние расстояния."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лайнер",
                                "target_lang_sense_clarification": "судно"
                            },
                            {
                                "target_lang_word": "океанский лайнер",
                                "target_lang_sense_clarification": "полное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0fd53941-9f12-3687-9f48-67f93144ef66",
                    "sense_definition": "A removable bag, layer, or cover placed inside a container or garment for protection or cleanliness.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "covering_lining",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please put a fresh plastic <w>liner</w> in the kitchen bin before you throw that away.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, вставь новый пластиковый <w>мешок</w> в мусорное ведро, прежде чем выбрасывать это."
                            }
                        },
                        {
                            "text": "My winter coat has a detachable wool <w>liner</w> that makes it very warm.",
                            "target_lang_translations": {
                                "ru": "У моего зимнего пальто есть отстегивающаяся шерстяная <w>подстёжка</w>, которая делает его очень теплым."
                            }
                        }
                    ],
                    "synonyms": [
                        "lining",
                        "insert",
                        "bag"
                    ],
                    "common_phrases": [
                        "bin liner",
                        "trash can liner",
                        "coat liner",
                        "helmet liner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Съемный мешок, слой или покрытие, помещаемое внутрь контейнера или одежды для защиты или чистоты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вкладыш",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "мешок",
                                "target_lang_sense_clarification": "для мусора"
                            },
                            {
                                "target_lang_word": "подстёжка",
                                "target_lang_sense_clarification": "для одежды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "751705e4-c3a7-30bc-8e82-e243c23a2ce8",
                    "sense_definition": "A short joke, remark, or text consisting of a single line or sentence (most commonly used in the compound 'one-liner').",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "text_speech",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The comedian is famous for his witty one-<w>liners</w> that make everyone laugh instantly.",
                            "target_lang_translations": {
                                "ru": "Комик известен своими остроумными <w>шутками</w>, которые мгновенно заставляют всех смеяться."
                            }
                        },
                        {
                            "text": "She delivered a perfect one-<w>liner</w> to end the argument.",
                            "target_lang_translations": {
                                "ru": "Она выдала идеальную <w>остроту</w>, чтобы положить конец спору."
                            }
                        }
                    ],
                    "synonyms": [
                        "quip",
                        "joke"
                    ],
                    "common_phrases": [
                        "one-liner",
                        "witty one-liner"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткая шутка, замечание или текст, состоящий из одной строки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "острота",
                                "target_lang_sense_clarification": "шутка"
                            },
                            {
                                "target_lang_word": "реплика",
                                "target_lang_sense_clarification": "высказывание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "586cb97c-fa38-34fc-b721-b34d36698699",
                    "sense_definition": "The booklet or paper insert containing information about a music album, usually found inside the case.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "media_text",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I enjoy reading the <w>liner</w> notes to learn who played the instruments on each track.",
                            "target_lang_translations": {
                                "ru": "Мне нравится читать <w>вкладыш</w> с аннотациями, чтобы узнать, кто играл на инструментах в каждом треке."
                            }
                        },
                        {
                            "text": "The reissue of the album includes expanded <w>liner</w> notes with rare photos.",
                            "target_lang_translations": {
                                "ru": "Переиздание альбома включает расширенный <w>буклет</w> с редкими фотографиями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "liner notes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Буклет или бумажный вкладыш с информацией о музыкальном альбоме, обычно находящийся внутри футляра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вкладыш",
                                "target_lang_sense_clarification": "в упаковке альбома"
                            },
                            {
                                "target_lang_word": "буклет",
                                "target_lang_sense_clarification": "книжечка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef7b2e7e-9b0b-3151-bf24-25329a695b57",
                    "sense_definition": "A ball hit hard and low in the air that travels in a relatively straight line.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sports_baseball",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The batter hit a sharp <w>liner</w> straight into the glove of the shortstop.",
                            "target_lang_translations": {
                                "ru": "Бэттер отбил резкий <w>лайнер</w> прямо в перчатку шорт-стопа."
                            }
                        },
                        {
                            "text": "It was a screaming <w>liner</w> that barely cleared the infield.",
                            "target_lang_translations": {
                                "ru": "Это был свистящий <w>лайнер</w>, который едва перелетел инфилд."
                            }
                        }
                    ],
                    "synonyms": [
                        "line drive"
                    ],
                    "antonyms": [
                        "pop-up",
                        "fly ball"
                    ],
                    "common_phrases": [
                        "hit a liner"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mostly US"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "baseball context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильный и низкий удар в бейсболе, при котором мяч летит по прямой линии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лайнер",
                                "target_lang_sense_clarification": "бейсбол"
                            },
                            {
                                "target_lang_word": "прямой удар",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                }
            ]
        },
        {

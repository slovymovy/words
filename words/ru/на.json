                    ],
                    "common_phrases": [
                        "на нервах",
                        "на эмоциях",
                        "на радостях"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "часто используется в разговорной речи для описания эмоционального состояния"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates a state or emotion determining a person's behavior."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "on",
                                "target_lang_sense_clarification": "idiomatic emotional state (e.g., on edge)"
                            },
                            {
                                "target_lang_word": "in",
                                "target_lang_sense_clarification": "in a state of emotion"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6405a559-ec0c-34e2-8f4d-5fc76d2a2c6f",
                    "sense_definition": "Указывает на объект нападения или агрессивного действия (с винительным падежом).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "direction_aggression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Армия двинулась <w>на</w> врага ранним утром.",
                            "target_lang_translations": {
                                "en": "The army moved <w>against</w> the enemy early in the morning."

                            {
                                "target_lang_word": "глупость",
                                "target_lang_sense_clarification": "свойство личности, недостаток ума"
                            },
                            {
                                "target_lang_word": "тупость",
                                "target_lang_sense_clarification": "разг., бестолковость, ограниченность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d548616e-b335-328e-b034-e4dcdeff84f5",
                    "sense_definition": "A specific action, mistake, or idea that is <w>stupid</w>.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "stupidity_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jimmy's <w>stupidity</w> cost him his car.",

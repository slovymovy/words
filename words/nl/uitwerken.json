            "pos": "verb",
            "senses": [
                {
                    "sense_id": "56f3e20e-0f92-3e42-aca2-d4854b3f1a22",
                    "sense_definition": "Iets dat al in grote lijnen bestaat nauwkeuriger maken, verder ontwikkelen of in detail beschrijven.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "elaborate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De architect gaat het voorlopige ontwerp nu technisch <w>uitwerken</w>.",
                            "target_lang_translations": {
                                "ru": "Архитектор теперь собирается технически <w>проработать</w> предварительный проект.",
                                "en": "The architect is now going to technically <w>work out</w> the preliminary design."
                            }
                        },
                        {
                            "text": "De ambtenaren moeten de plannen van het kabinet concreet <w>uitwerken</w>.",
                            "target_lang_translations": {
                                "ru": "Чиновники должны конкретно <w>разработать</w> планы кабинета министров.",
                                "en": "The civil servants must concretely <w>flesh out</w> the cabinet's plans."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontwikkelen",
                        "detailleren",
                        "concretiseren"
                    ],
                    "antonyms": [
                        "schetsen"
                    ],
                    "common_phrases": [
                        "een plan uitwerken",
                        "een idee uitwerken",
                        "nader uitwerken"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "scheidbaar werkwoord"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-то, что уже существует в общих чертах, более точным, развивать дальше или описывать в деталях.",
                        "en": "To make something that already exists in outline more accurate, develop it further, or describe it in detail."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разрабатывать",
                                "target_lang_sense_clarification": "развивать план или идею"
                            },
                            {
                                "target_lang_word": "прорабатывать",
                                "target_lang_sense_clarification": "тщательно продумывать детали"
                            },
                            {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "850fd4b6-066f-3450-b104-f77ba74ac338",
                    "sense_definition": "Poruszać się rytmicznie w takt muzyki, wykonując odpowiednie kroki i figury.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy goście weselni będą <w>tańczyć</w> do samego rana.",
                            "target_lang_translations": {
                                "ru": "Все свадебные гости будут <w>танцевать</w> до самого утра."
                            }
                        },
                        {
                            "text": "Moja siostra bardzo ładnie <w>tańczy</w> i chodzi na zajęcia baletu.",
                            "target_lang_translations": {
                                "ru": "Моя сестра очень красиво <w>танцует</w> и ходит на занятия балетом."
                            }
                        }
                    ],
                    "synonyms": [
                        "tańcować",
                        "pląsać"
                    ],
                    "common_phrases": [
                        "tańczyć w parze",
                        "tańczyć walca",
                        "lubić tańczyć",
                        "iść tańczyć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться ритмично в такт музыке, выполняя соответствующие шаги и фигуры."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "танцевать",
                                "target_lang_sense_clarification": "основное значение"
                            },
                            {
                                "target_lang_word": "плясать",
                                "target_lang_sense_clarification": "энергично или исполнять народные танцы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62fbe144-9cdb-3d3a-a7b2-1e845a30c344",
                    "sense_definition": "Poruszać się szybko, chaotycznie lub migotać (zwykle o światłach, cieniach lub drobnych elementach).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cienie drzew <w>tańczyły</w> na ścianie w świetle księżyca.",
                            "target_lang_translations": {
                                "ru": "Тени деревьев <w>танцевали</w> на стене в лунном свете."
                            }
                        },
                        {
                            "text": "Przed zmęczonymi oczami zaczęły mi <w>tańczyć</w> czarne plamki.",
                            "target_lang_translations": {
                                "ru": "Перед усталыми глазами у меня начали <w>плясать</w> черные пятна."
                            }
                        }
                    ],
                    "common_phrases": [
                        "płomienie tańczą",
                        "cienie tańczą",
                        "iskry tańczą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро и хаотично двигаться или мерцать (обычно о свете, тенях или мелких частицах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "танцевать",
                                "target_lang_sense_clarification": "о тенях, бликах, пламени"
                            },
                            {
                                "target_lang_word": "плясать",
                                "target_lang_sense_clarification": "об огне, искрах, пятнах перед глазами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e05a3c2-bbeb-36e7-9b5d-1da15756276e",
                    "sense_definition": "Chwiać się lub poruszać nieskoordynowanym ruchem, tracąc stabilność (często o pojazdach lub przedmiotach).",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas burzy mała łódka niebezpiecznie <w>tańczyła</w> na wysokich falach.",
                            "target_lang_translations": {
                                "ru": "Во время шторма маленькая лодка опасно <w>танцевала</w> на высоких волнах."
                            }
                        },
                        {
                            "text": "Samochód wpadł w poślizg i <w>tańczył</w> na oblodzonej jezdni.",
                            "target_lang_translations": {
                                "ru": "Машину занесло, и она <w>танцевала</w> на обледенелой дороге."
                            }
                        }
                    ],
                    "synonyms": [
                        "chwiać się"
                    ],
                    "common_phrases": [
                        "auto tańczy na drodze",
                        "statek tańczy na falach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качаться или совершать беспорядочные движения, теряя устойчивость (часто о транспорте или предметах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "танцевать",
                                "target_lang_sense_clarification": "переносное значение: о неустойчивом движении"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
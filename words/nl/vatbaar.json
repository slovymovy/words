                    "synonyms": [
                        "begrijpelijk",
                        "bevattelijk"
                    ],
                    "antonyms": [
                        "onvatbaar"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "wordt in modern Nederlands vaker als 'begrijpelijk' of 'te bevatten' uitgedrukt"
                        }
                    ],
                    "target_lang_definitions": {

{
    "word_family": [
        "вход",
        "вхождение",
        "входной"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "100568ef-c949-3e8a-8e61-b924e823aba5",
                    "sense_definition": "Переместиться внутрь помещения, здания или огороженного пространства, ступив туда.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement_in",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Можно мне <войти> в комнату?",
                            "target_lang_translations": {
                                "en": "May I <w>enter</w> the room?"
                            }
                        },
                        {
                            "text": "Гости начали <войти> в зал, когда заиграла музыка.",
                            "target_lang_translations": {
                                "en": "The guests started to <w>enter</w> the hall when the music began to play."
                            }
                        },
                        {
                            "text": "Он постучал в дверь, прежде чем <войти>.",
                            "target_lang_translations": {
                                "en": "He knocked on the door before <w>coming in</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "зайти",
                        "пройти",
                        "ступить",
                        "проникнуть"
                    ],
                    "antonyms": [
                        "выйти"
                    ],
                    "common_phrases": [
                        "войти в дом",
                        "войти в комнату",
                        "можно войти",
                        "войти внутрь"
                    ],
                    "target_lang_definitions": {
                        "en": "To move inside a room, building, or enclosed space."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "enter",
                                "target_lang_sense_clarification": "general term for going into a place"
                            },
                            {
                                "target_lang_word": "come in",
                                "target_lang_sense_clarification": "usually when the speaker is inside"
                            },
                            {
                                "target_lang_word": "go in",
                                "target_lang_sense_clarification": "usually when the speaker is outside"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1620a1b4-9d88-3f3b-b035-b94b0eb10878",
                    "sense_definition": "Получить доступ к компьютерной системе, сайту или приложению (авторизоваться).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "digital_access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вам нужно <войти> в систему, чтобы увидеть сообщения.",
                            "target_lang_translations": {
                                "en": "You need to <w>log in</w> to the system to see the messages."
                            }
                        },
                        {
                            "text": "Я не могу <войти> в свой аккаунт, так как забыл пароль.",
                            "target_lang_translations": {
                                "en": "I cannot <w>log into</w> my account because I forgot the password."
                            }
                        }
                    ],
                    "synonyms": [
                        "зайти"
                    ],
                    "antonyms": [
                        "выйти"
                    ],
                    "common_phrases": [
                        "войти в систему",
                        "войти в аккаунт",
                        "войти на сайт",
                        "войти через пароль"
                    ],
                    "target_lang_definitions": {
                        "en": "To gain access to a computer system, website, or application."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "log in",
                                "target_lang_sense_clarification": "to access a digital account"
                            },
                            {
                                "target_lang_word": "sign in",
                                "target_lang_sense_clarification": "synonym for log in"
                            },
                            {
                                "target_lang_word": "enter",
                                "target_lang_sense_clarification": "to access a system (less common)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b76ec32-3a31-35de-adf0-c6ada1ea0cb1",
                    "sense_definition": "Стать частью чего-либо, быть включённым в состав или группу.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "inclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это великое событие навсегда <войдёт> в историю.",
                            "target_lang_translations": {
                                "en": "This great event will <w>go down</w> in history forever."
                            }
                        },
                        {
                            "text": "Новый сотрудник должен <войти> в состав нашей команды завтра.",
                            "target_lang_translations": {
                                "en": "The new employee is due to <w>join</w> our team tomorrow."
                            }
                        }
                    ],
                    "synonyms": [
                        "вступить"
                    ],
                    "antonyms": [
                        "выйти"
                    ],
                    "common_phrases": [
                        "войти в историю",
                        "войти в состав",
                        "войти в моду",
                        "войти в список"
                    ],
                    "target_lang_definitions": {
                        "en": "To become part of something; to be included in a group or list."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "enter",
                                "target_lang_sense_clarification": "to become part of history, a list, etc."
                            },
                            {
                                "target_lang_word": "join",
                                "target_lang_sense_clarification": "to become a member of a group or team"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c79a564e-9e96-37b5-b3c2-be229cbcb5ee",
                    "sense_definition": "Начать находиться в каком-либо состоянии, привыкнуть к чему-либо или вникнуть в суть дела.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "state_condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Прошу вас <войти> в моё положение и помочь мне.",
                            "target_lang_translations": {
                                "en": "I ask you to <w>understand</w> my situation and help me."
                            }
                        },
                        {
                            "text": "После отпуска трудно сразу <войти> в рабочий ритм.",
                            "target_lang_translations": {
                                "en": "After a vacation, it is hard to immediately <w>get into</w> the work rhythm."
                            }
                        }
                    ],
                    "common_phrases": [
                        "войти в положение",
                        "войти в курс дела",
                        "войти в роль",
                        "войти в привычку",
                        "войти в силу"
                    ],
                    "target_lang_definitions": {
                        "en": "To start being in a state or condition; to empathize or get used to something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get into",
                                "target_lang_sense_clarification": "to become involved in or accustomed to a rhythm/habit"
                            },
                            {
                                "target_lang_word": "understand",
                                "target_lang_sense_clarification": "specifically for 'entering someone's position' (empathize)"
                            },
                            {
                                "target_lang_word": "enter",
                                "target_lang_sense_clarification": "to enter a state or situation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cea10e39-2dcd-31c8-8db7-875bdf99d968",
                    "sense_definition": "Поместиться внутри чего-либо (об объёме или количестве).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "capacity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этот маленький лифт может <войти> только три человека.",
                            "target_lang_translations": {
                                "en": "Only three people can <w>fit</w> in this small elevator."
                            }
                        },
                        {
                            "text": "В банку <войдёт> ещё немного воды.",
                            "target_lang_translations": {
                                "en": "A little more water will <w>fit</w> into the jar."
                            }
                        }
                    ],
                    "synonyms": [
                        "уместиться",
                        "поместиться",
                        "влезть"
                    ],
                    "common_phrases": [
                        "войти целиком",
                        "с трудом войти"
                    ],
                    "target_lang_definitions": {
                        "en": "To fit inside something (referring to volume or capacity)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fit",
                                "target_lang_sense_clarification": "to be of the right size to go inside"
                            },
                            {
                                "target_lang_word": "go in",
                                "target_lang_sense_clarification": "to be able to be inserted or contained"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
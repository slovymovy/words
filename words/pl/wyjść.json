{
    "word_family": [
        "wyjście",
        "wychodzić",
        "wychodzenie",
        "powychodzić"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c4b05d25-6b6c-3fd3-9349-63af2fee2cbc",
                    "sense_definition": "Opuścić pomieszczenie, budynek lub inne miejsce, udając się na zewnątrz.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę już <wyjść> z domu, żeby zdążyć na autobus do pracy.",
                            "target_lang_translations": {
                                "ru": "Мне нужно уже <выйти> из дома, чтобы успеть на автобус до работы."
                            }
                        },
                        {
                            "text": "Poczekaj chwilę, zaraz <wyjdę> przed klatkę i otworzę ci drzwi.",
                            "target_lang_translations": {
                                "ru": "Подожди минуту, я сейчас <выйду> к подъезду и открою тебе дверь."
                            }
                        }
                    ],
                    "synonyms": [
                        "opuścić"
                    ],
                    "antonyms": [
                        "wejść"
                    ],
                    "common_phrases": [
                        "wyjść z domu",
                        "wyjść na zewnątrz",
                        "wyjść z pokoju"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покинуть помещение, здание или другое место, выходя наружу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выйти",
                                "target_lang_sense_clarification": "покинуть место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7bf09cb-16ee-3c10-8465-45d66c84af60",
                    "sense_definition": "Udać się gdzieś w celach towarzyskich lub rozrywkowych (np. do kina, restauracji).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "social_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ten piątek planujemy <wyjść> ze znajomymi do nowej włoskiej restauracji.",
                            "target_lang_translations": {
                                "ru": "В эту пятницу мы планируем <сходить> с друзьями в новый итальянский ресторан."
                            }
                        },
                        {
                            "text": "Dawno nigdzie nie byliśmy, może chcesz <wyjść> do kina w weekend?",
                            "target_lang_translations": {
                                "ru": "Мы давно нигде не были, может, хочешь <сходить> в кино в выходные?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyjść na miasto",
                        "wyjść ze znajomymi",
                        "wyjść do kina"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пойти куда-либо с целью общения или развлечения (например, в кино, ресторан)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбраться",
                                "target_lang_sense_clarification": "пойти развлечься"
                            },
                            {
                                "target_lang_word": "сходить",
                                "target_lang_sense_clarification": "посетить место"
                            },
                            {
                                "target_lang_word": "выйти",
                                "target_lang_sense_clarification": "в свет / в люди"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b3dd38aa-3d2e-3a87-a530-3604229cd915",
                    "sense_definition": "Wziąć ślub (używane w odniesieniu do kobiety).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "marriage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja starsza siostra zamierza <wyjść> za mąż w przyszłym roku.",
                            "target_lang_translations": {
                                "ru": "Моя старшая сестра собирается <выйти> замуж в следующем году."
                            }
                        },
                        {
                            "text": "Ona postanowiła <wyjść> za swojego wieloletniego partnera z czasów studiów.",
                            "target_lang_translations": {
                                "ru": "Она решила <выйти> за своего давнего партнера со времен учебы."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyjść za mąż"
                    ],
                    "common_phrases": [
                        "wyjść za mąż",
                        "wyjść za niego"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вступить в брак (используется по отношению к женщине)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выйти",
                                "target_lang_sense_clarification": "замуж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1188ff6-9e31-3ced-ae34-068d5b7bd8fd",
                    "sense_definition": "Powieść się; zakończyć się sukcesem lub zgodnie z zamierzonym planem.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "result_outcome",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bardzo się starałam upiec to ciasto, ale niestety mi nie <wyszło>.",
                            "target_lang_translations": {
                                "ru": "Я очень старалась испечь этот пирог, но, к сожалению, у меня не <получилось>."
                            }
                        },
                        {
                            "text": "Nie martw się, jestem pewien, że wszystko <wyjdzie> na dobre.",
                            "target_lang_translations": {
                                "ru": "Не волнуйся, я уверен, что всё <обернется> к лучшему."
                            }
                        }
                    ],
                    "synonyms": [
                        "zagrać"
                    ],
                    "common_phrases": [
                        "wyjść na dobre",
                        "nic z tego nie wyszło",
                        "dobrze wyjść"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удаться; завершиться успешно или согласно плану."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "получиться",
                                "target_lang_sense_clarification": "удаться"
                            },
                            {
                                "target_lang_word": "выйти",
                                "target_lang_sense_clarification": "обернуться чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7774d068-245d-3538-b4e7-81e2d5703c2b",
                    "sense_definition": "Zostać uwiecznionym na zdjęciu lub obrazie w określony sposób.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na tym zdjęciu ze ślubu <wyszła> naprawdę przepięknie i naturalnie.",
                            "target_lang_translations": {
                                "ru": "На этой свадебной фотографии она <получилась> действительно прекрасно и естественно."
                            }
                        },
                        {
                            "text": "Zawsze zamykam oczy i źle <wychodzę> na zdjęciach do dokumentów.",
                            "target_lang_translations": {
                                "ru": "Я всегда закрываю глаза и плохо <получаюсь> на фотографиях для документов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dobrze wyjść na zdjęciu",
                        "źle wyjść"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получиться на фотографии или картине определенным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "получиться",
                                "target_lang_sense_clarification": "на фото"
                            },
                            {
                                "target_lang_word": "выйти",
                                "target_lang_sense_clarification": "на снимке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82a4defa-e020-302f-baaf-bab5499164ba",
                    "sense_definition": "Zostać opublikowanym, wydanym drukiem lub udostępnionym publicznie (o książce, filmie, grze).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "publication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowa płyta mojego ulubionego zespołu rockowego <wyjdzie> w przyszłym miesiącu.",
                            "target_lang_translations": {
                                "ru": "Новый альбом моей любимой рок-группы <выйдет> в следующем месяце."
                            }
                        },
                        {
                            "text": "Ten artykuł <wyszedł> wczoraj w lokalnej gazecie i wywołał dyskusję.",
                            "target_lang_translations": {
                                "ru": "Эта статья <вышла> вчера в местной газете и вызвала дискуссию."
                            }
                        }
                    ],
                    "synonyms": [
                        "ukazać się"
                    ],
                    "common_phrases": [
                        "książka wyszła",
                        "film niedługo wyjdzie",
                        "gra wyszła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть опубликованным, изданным или выпущенным в свет (о книге, фильме, игре)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выйти",
                                "target_lang_sense_clarification": "быть изданным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c6c05be-73a1-3ba7-bb1f-58c30e38cec2",
                    "sense_definition": "Być wynikiem jakiegoś działania, procesu lub obliczeń matematycznych.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "result_outcome",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z moich szybkich obliczeń <wyszło>, że musimy dopłacić jeszcze sto złotych.",
                            "target_lang_translations": {
                                "ru": "Из моих быстрых подсчетов <вышло>, что мы должны доплатить еще сто злотых."
                            }
                        },
                        {
                            "text": "Nauczyciel matematyki zapytał ucznia, jaki wynik mu <wyszedł> w ostatnim zadaniu.",
                            "target_lang_translations": {
                                "ru": "Учитель математики спросил ученика, какой результат у него <получился> в последнем задании."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyszło mi",
                        "ile ci wyszło",
                        "drogo wyszło"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть результатом действия, процесса или математических вычислений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "получиться",
                                "target_lang_sense_clarification": "результат"
                            },
                            {
                                "target_lang_word": "выйти",
                                "target_lang_sense_clarification": "в итоге"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "722d19dc-f3d2-3ca7-9b50-e4506ec6857c",
                    "sense_definition": "Stać się jawnym; zostać ujawnionym (o tajemnicach, faktach, kłamstwach).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "revelation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wielu latach prawda o tamtych wydarzeniach w końcu <wyszła> na jaw.",
                            "target_lang_translations": {
                                "ru": "После многих лет правда о тех событиях наконец <вышла> наружу."
                            }
                        },
                        {
                            "text": "Kłamstwo ma krótkie nogi i prędzej czy później wszystko <wyjdzie>.",
                            "target_lang_translations": {
                                "ru": "У лжи короткие ноги, и рано или поздно всё <откроется>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyjść na jaw",
                        "wyjść w praniu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать явным; открыться (о тайне, фактах, лжи)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открыться",
                                "target_lang_sense_clarification": "стать известным"
                            },
                            {
                                "target_lang_word": "всплыть",
                                "target_lang_sense_clarification": "наружу"
                            },
                            {
                                "target_lang_word": "выйти",
                                "target_lang_sense_clarification": "наружу / на свет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d2bb84d-7592-35ba-9a33-4f4129fa69ab",
                    "sense_definition": "Zostać odebranym lub ocenionym przez innych w określony sposób (często negatywny lub korzystny).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie odzywał się przez całe spotkanie, żeby nie <wyjść> na ignoranta.",
                            "target_lang_translations": {
                                "ru": "Он не разговаривал всю встречу, чтобы не <сойти> за невежду."
                            }
                        },
                        {
                            "text": "Kupił taniej i sprzedał drożej, więc bardzo dobrze na tym interesie <wyszedł>.",
                            "target_lang_translations": {
                                "ru": "Он купил дешевле и продал дороже, так что очень хорошо на этом деле <заработал>."
                            }
                        }
                    ],
                    "synonyms": [
                        "zyskać"
                    ],
                    "common_phrases": [
                        "wyjść na głupka",
                        "dobrze na tym wyjść",
                        "źle na tym wyjść"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть воспринятым или оцененным другими определенным образом; оказаться в выигрыше или проигрыше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выглядеть",
                                "target_lang_sense_clarification": "казаться кем-то"
                            },
                            {
                                "target_lang_word": "сойти",
                                "target_lang_sense_clarification": "за кого-то"
                            },
                            {
                                "target_lang_word": "оказаться",
                                "target_lang_sense_clarification": "в плюсе/минусе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9609b2b6-34b4-3a57-a67c-f8ad983ee47c",
                    "sense_definition": "Poradzić sobie z trudną sytuacją; uwolnić się od problemów lub choroby.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "recovery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzięki pomocy rodziny udało mu się <wyjść> z długów w krótkim czasie.",
                            "target_lang_translations": {
                                "ru": "Благодаря помощи семьи ему удалось <выбраться> из долгов за короткое время."
                            }
                        },
                        {
                            "text": "Pacjent powoli <wychodzi> z choroby i czuje się coraz lepiej.",
                            "target_lang_translations": {
                                "ru": "Пациент медленно <оправляется> от болезни и чувствует себя всё лучше."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyjść z kłopotów",
                        "wyjść z choroby",
                        "wyjść z długów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Справиться с трудной ситуацией; избавиться от проблем или болезни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выбраться",
                                "target_lang_sense_clarification": "из проблем"
                            },
                            {
                                "target_lang_word": "выйти",
                                "target_lang_sense_clarification": "из положения/состояния"
                            },
                            {
                                "target_lang_word": "оправиться",
                                "target_lang_sense_clarification": "от болезни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffced1f0-e4f8-3dcc-9ca3-15f25855f0fc",
                    "sense_definition": "Zrezygnować z członkostwa w jakiejś organizacji lub grupie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "membership",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wielu latach sporów postanowił ostatecznie <wyjść> z partii politycznej.",
                            "target_lang_translations": {
                                "ru": "После многих лет споров он решил окончательно <выйти> из политической партии."
                            }
                        },
                        {
                            "text": "Wielka Brytania zdecydowała się <wyjść> z Unii Europejskiej.",
                            "target_lang_translations": {
                                "ru": "Великобритания решила <выйти> из Европейского Союза."
                            }
                        }
                    ],
                    "synonyms": [
                        "wypaść"
                    ],
                    "antonyms": [
                        "wejść"
                    ],
                    "common_phrases": [
                        "wyjść z grupy",
                        "wyjść z roli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прекратить членство в какой-либо организации или группе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выйти",
                                "target_lang_sense_clarification": "из состава"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c25bbb20-8d71-3097-beb5-9a2a922e0e1b",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <w>wyjście</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym budynku nie ma żadnych awaryjnych <wyjść> na wypadek pożaru.",
                            "target_lang_translations": {
                                "ru": "В этом здании нет никаких аварийных <выходов> на случай пожара."
                            }
                        },
                        {
                            "text": "Szukaliśmy innych <wyjść> z tej trudnej sytuacji, ale nic nie znaleźliśmy.",
                            "target_lang_translations": {
                                "ru": "Мы искали другие <выходы> из этой сложной ситуации, но ничего не нашли."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural of wyjście"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа существительного 'wyjście' (выход)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выходов",
                                "target_lang_sense_clarification": "существительное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "frequency": "High",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Can you wait a <w>sec</w> while I grab my coat?",
                            "target_lang_translations": {
                                "ru": "Можешь подождать <w>секунду</w>, пока я возьму пальто?"
                            }
                        },
                        {
                            "text": "I will be with you in just a <w>sec</w>.",
                            "target_lang_translations": {
                                "ru": "Я буду с вами буквально через <w>секунду</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "moment",
                        "jiffy",
                        "bit",
                        "second"
                    ],
                    "common_phrases": [

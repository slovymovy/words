                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>beest</w> de hele avond door op de dansvloer.",
                            "target_lang_translations": {

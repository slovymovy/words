{
    "word_family": [
        "onderkruipen",
        "onderkruipsel"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "decef634-5d79-3750-9033-bbf8c590bce6",
                    "sense_definition": "Iemand die werkt op een plaats waar gestaakt wordt en zo de staking ondermijnt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De stakers schreeuwden naar de <w>onderkruiper</w> toen hij de fabriekspoort passeerde.",
                            "target_lang_translations": {
                                "en": "The strikers shouted at the <w>scab</w> as he passed the factory gate.",
                                "ru": "Бастующие кричали на <w>штрейкбрехера</w>, когда он проходил через заводские ворота."
                            }
                        },
                        {
                            "text": "Geen enkele <w>onderkruiper</w> durfde zich te laten zien tijdens het protest.",
                            "target_lang_translations": {
                                "en": "Not a single <w>strikebreaker</w> dared to show his face during the protest.",
                                "ru": "Ни один <w>штрейкбрехер</w> не осмелился показаться во время протеста."
                            }
                        }
                    ],
                    "synonyms": [
                        "stakingsbreker"
                    ],
                    "common_phrases": [
                        "vuile onderkruiper"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who works during a strike, thereby undermining the strike action.",
                        "ru": "Человек, который работает во время забастовки, тем самым подрывая её."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scab",
                                "target_lang_sense_clarification": "derogatory term for a strikebreaker"
                            },
                            {
                                "target_lang_word": "strikebreaker",
                                "target_lang_sense_clarification": "worker who continues working during a strike"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "штрейкбрехер",
                                "target_lang_sense_clarification": "работник, не поддерживающий забастовку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ed01eb6-2543-3c72-a4ac-c5e18a8da246",
                    "sense_definition": "Een achterbaks en kruiperig persoon die probeert bij anderen in de gunst te komen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niemand vertrouwt die <w>onderkruiper</w> omdat hij alles doorvertelt aan de baas.",
                            "target_lang_translations": {
                                "en": "No one trusts that <w>sneak</w> because he tells the boss everything.",
                                "ru": "Никто не доверяет этому <w>подхалиму</w>, потому что он всё докладывает начальнику."
                            }
                        },
                        {
                            "text": "Hij gedraagt zich als een echte <w>onderkruiper</w> om promotie te maken.",
                            "target_lang_translations": {
                                "en": "He behaves like a real <w>creep</w> to get a promotion.",
                                "ru": "Он ведёт себя как настоящий <w>подлиза</w>, чтобы получить повышение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "achterbakse onderkruiper"
                    ],
                    "target_lang_definitions": {
                        "en": "A sneaky and groveling person who tries to curry favor with others.",
                        "ru": "Скрытный и раболепный человек, пытающийся добиться расположения других."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sneak",
                                "target_lang_sense_clarification": "deceitful or underhanded person"
                            },
                            {
                                "target_lang_word": "creep",
                                "target_lang_sense_clarification": "obsequious or servile person"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "подхалим",
                                "target_lang_sense_clarification": "льстивый человек"
                            },
                            {
                                "target_lang_word": "проныра",
                                "target_lang_sense_clarification": "хитрый, проникающий всюду человек"
                            },
                            {
                                "target_lang_word": "подлиза",
                                "target_lang_sense_clarification": "угодливый человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89ef5e9d-b490-3667-9297-b92e436865a6",
                    "sense_definition": "Iemand die opvallend klein van stuk is.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat kleine <w>onderkruiper</w> denkt dat hij de hele wereld aankan.",
                            "target_lang_translations": {
                                "en": "That little <w>shrimp</w> thinks he can take on the whole world.",
                                "ru": "Этот мелкий <w>недомерок</w> думает, что ему всё по плечу."
                            }
                        },
                        {
                            "text": "Ze noemden hem spottend een <w>onderkruiper</w> vanwege zijn lengte.",
                            "target_lang_translations": {
                                "en": "They mockingly called him a <w>runt</w> because of his height.",
                                "ru": "Они насмешливо называли его <w>коротышкой</w> из-за его роста."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kleine onderkruiper"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who is remarkably small in stature.",
                        "ru": "Человек очень маленького роста."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shrimp",
                                "target_lang_sense_clarification": "small or puny person"
                            },
                            {
                                "target_lang_word": "runt",
                                "target_lang_sense_clarification": "undersized person"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "коротышка",
                                "target_lang_sense_clarification": "низкорослый человек"
                            },
                            {
                                "target_lang_word": "недомерок",
                                "target_lang_sense_clarification": "пренебрежительно о низком человеке"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
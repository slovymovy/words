            "senses": [
                {
                    "sense_id": "4b5525db-6dd2-36ef-8d99-2018e6b55f57",
                    "sense_definition": "Совокупность людей, объединённых исторически обусловленными социальными формами совместной жизни и деятельности.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_people_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Современное <w>общество</w> зависит от технологий.",
                            "target_lang_translations": {
                                "en": "Modern <w>society</w> depends on technology."
                            }
                        },
                        {
                            "text": "Каждый человек должен приносить пользу <w>обществу</w>.",
                            "target_lang_translations": {
                                "en": "Every person should be of benefit to <w>society</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "общественность"
                    ],
                    "antonyms": [
                        "индивидуум"
                    ],
                    "common_phrases": [
                        "современное общество",
                        "человеческое общество",
                        "развитие общества"
                    ],
                    "target_lang_definitions": {
                        "en": "A collection of people united by historically determined social forms of joint life and activity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "society",
                                "target_lang_sense_clarification": "community of people"
                            },
                            {
                                "target_lang_word": "community",
                                "target_lang_sense_clarification": "general public"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9787f0f-1721-31cc-a0f7-b29a51e6f623",
                    "sense_definition": "Добровольное объединение людей для какой-либо общей цели (научной, благотворительной, спортивной и т.п.).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_organization",
                    "name_type": "organization_name",
                    "examples": [
                        {
                            "text": "Русское географическое <w>общество</w> организовало новую экспедицию.",
                            "target_lang_translations": {
                                "en": "The Russian Geographical <w>Society</w> organized a new expedition."
                            }
                        },
                        {
                            "text": "Она вступила в <w>общество</w> защиты животных.",
                            "target_lang_translations": {
                                "en": "She joined the <w>society</w> for the protection of animals."
                            }
                        }
                    ],
                    "synonyms": [
                        "организация",
                        "союз"
                    ],
                    "common_phrases": [
                        "тайное общество",
                        "научное общество",
                        "спортивное общество"
                    ],
                    "target_lang_definitions": {
                        "en": "A voluntary union of people for a common goal (scientific, charitable, sports, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "society",
                                "target_lang_sense_clarification": "organization"
                            },
                            {
                                "target_lang_word": "association",
                                "target_lang_sense_clarification": "organization"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b18889f4-6c05-316e-a3e2-afc8af3cf902",
                    "sense_definition": "Группа людей, проводящих время вместе; чьё-либо присутствие.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_presence",
                    "name_type": "no",
                    "examples": [

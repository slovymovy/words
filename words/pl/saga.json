{
    "word_family": [
        "sagowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6662250e-4bfb-30ac-b5ec-32d49889e830",
                    "sense_definition": "Utwór literacki lub filmowy o charakterze epickim, opisujący historię rodziny lub rodu, obejmujący zazwyczaj kilka pokoleń.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "epic_story",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta wieloczęściowa <saga> opowiada o losach arystokratycznego rodu na tle przemian historycznych.",
                            "target_lang_translations": {
                                "ru": "Эта многочастная <w>сага</w> рассказывает о судьбе аристократического рода на фоне исторических перемен."
                            }
                        },
                        {
                            "text": "W księgarniach pojawiła się nowa <saga> rodzinna, która szybko stała się bestsellerem.",
                            "target_lang_translations": {
                                "ru": "В книжных магазинах появилась новая семейная <w>сага</w>, которая быстро стала бестселлером."
                            }
                        }
                    ],
                    "synonyms": [
                        "epopeja",
                        "kronika"
                    ],
                    "common_phrases": [
                        "saga rodzinna",
                        "saga rodu",
                        "wielotomowa saga"
                    ],
                    "target_lang_definitions": {
                        "ru": "Литературное или кинематографическое произведение эпического характера, описывающее историю семьи или рода, обычно охватывающее несколько поколений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сага",
                                "target_lang_sense_clarification": "семейная история"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0643cb5e-9605-329c-be4a-12779663bc49",
                    "sense_definition": "Staroskandynawski utwór epicki pisany prozą, opowiadający o dziejach królów, bohaterów i wybitnych rodów.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "epic_story",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Historycy czerpią wiedzę o wikingach, analizując stare islandzkie <sagi>.",
                            "target_lang_translations": {
                                "ru": "Историки черпают знания о викингах, анализируя старые исландские <w>саги</w>."
                            }
                        },
                        {
                            "text": "Każda <saga> przekazywała wartości i legendy ważne dla ówczesnej społeczności skandynawskiej.",
                            "target_lang_translations": {
                                "ru": "Каждая <w>сага</w> передавала ценности и легенды, важные для скандинавского общества того времени."
                            }
                        }
                    ],
                    "synonyms": [
                        "podanie",
                        "legenda"
                    ],
                    "common_phrases": [
                        "sagi islandzkie",
                        "sagi skandynawskie",
                        "bohater sagi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Древнескандинавское эпическое произведение в прозе, повествующее об истории королей, героев и знатных родов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сага",
                                "target_lang_sense_clarification": "скандинавский эпос"
                            },
                            {
                                "target_lang_word": "сказание",
                                "target_lang_sense_clarification": "легенда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cef6be4d-79be-3ec1-95b4-915a09ba0c13",
                    "sense_definition": "Bok, strona lub ukos (używane w gwarach).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopi przetoczyli ciężki pień na <sagę>, aby odblokować wiejską drogę.",
                            "target_lang_translations": {
                                "ru": "Крестьяне откатили тяжелый ствол в <w>сторону</w>, чтобы разблокировать деревенскую дорогу."
                            }
                        },
                        {
                            "text": "Ułóż te deski na <sadze>, żeby szybciej wyschły na wietrze.",
                            "target_lang_translations": {
                                "ru": "Сложи эти доски под <w>наклоном</w>, чтобы они быстрее высохли на ветру."
                            }
                        }
                    ],
                    "synonyms": [
                        "bok",
                        "ukos"
                    ],
                    "common_phrases": [
                        "na sagę"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "regional usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бок, сторона или наклон (используется в диалектах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бок",
                                "target_lang_sense_clarification": "сторона"
                            },
                            {
                                "target_lang_word": "наклон",
                                "target_lang_sense_clarification": "косое положение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ef1bf448-d9a0-3d35-a175-2a5226895bd3",
                    "sense_definition": "A large metal or plastic basket on wheels provided by a supermarket for customers to use while shopping.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "container_vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She pushed the <w>cart</w> down the aisle to find the cereal.",
                            "target_lang_translations": {
                                "ru": "Она толкала <w>тележку</w> по проходу, чтобы найти хлопья."
                            }
                        },
                        {
                            "text": "The wheel on my grocery <w>cart</w> is stuck and making a noise.",
                            "target_lang_translations": {
                                "ru": "Колесо на моей продуктовой <w>тележке</w> застряло и скрипит."
                            }
                        }
                    ],
                    "synonyms": [
                        "trolley",
                        "buggy",
                        "basket"
                    ],
                    "common_phrases": [
                        "shopping cart",
                        "grocery cart",
                        "push the cart"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "North America"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая металлическая или пластиковая корзина на колесах, предоставляемая супермаркетом для использования покупателями во время покупок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тележка",
                                "target_lang_sense_clarification": "в супермаркете"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d99ea8e8-c424-325e-929a-e8e857f359b1",
                    "sense_definition": "A vehicle with two or four wheels that is pulled by an animal or pushed by a person, used for transporting goods.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "container_vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer loaded the wooden <w>cart</w> with fresh hay.",
                            "target_lang_translations": {
                                "ru": "Фермер нагрузил деревянную <w>телегу</w> свежим сеном."
                            }
                        },
                        {
                            "text": "A horse and <w>cart</w> moved slowly along the country road.",
                            "target_lang_translations": {
                                "ru": "Лошадь с <w>повозкой</w> медленно двигалась по проселочной дороге."
                            }
                        }
                    ],
                    "synonyms": [
                        "wagon",
                        "dray",
                        "carriage"
                    ],
                    "common_phrases": [
                        "horse and cart",
                        "upset the apple cart",
                        "cart before the horse"
                    ],
                    "target_lang_definitions": {
                        "ru": "Транспортное средство с двумя или четырьмя колесами, которое тянет животное или толкает человек, используемое для перевозки грузов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "телега",
                                "target_lang_sense_clarification": "гужевая"
                            },
                            {
                                "target_lang_word": "повозка",
                                "target_lang_sense_clarification": "общее название"
                            },
                            {
                                "target_lang_word": "воз",
                                "target_lang_sense_clarification": "груженая телега"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e9cea56-0f23-30bb-85f2-04ad54e89730",
                    "sense_definition": "A small casing containing a memory chip, magnetic tape, or other substance, designed to be inserted into a device; a clipping of cartridge.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tech_clipping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He blew into the game <w>cart</w> to try and make it work.",
                            "target_lang_translations": {
                                "ru": "Он подул в игровой <w>картридж</w>, пытаясь заставить его работать."
                            }
                        },
                        {
                            "text": "The store sells vintage consoles and old video game <w>carts</w>.",
                            "target_lang_translations": {
                                "ru": "Магазин продает винтажные консоли и старые игровые <w>картриджи</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "cartridge",
                        "cassette"
                    ],
                    "common_phrases": [
                        "game cart",
                        "flash cart"
                    ],
                    "traits": [

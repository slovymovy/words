    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "14d95acf-770b-35e0-8de8-1f5cb1fa5b19",
                    "sense_definition": "Having good physical or mental health; not sick or injured.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is the father of three <w>healthy</w> children.",
                            "target_lang_translations": {
                                "ru": "Он отец троих <w>здоровых</w> детей."
                            }
                        },
                        {
                            "text": "You must eat well to keep your body <w>healthy</w> and strong.",
                            "target_lang_translations": {
                                "ru": "Нужно хорошо питаться, чтобы тело оставалось <w>здоровым</w> и сильным."
                            }
                        }
                    ],
                    "synonyms": [
                        "well",
                        "fit"
                    ],
                    "antonyms": [
                        "ill",
                        "unhealthy",
                        "diseased"
                    ],
                    "common_phrases": [
                        "stay healthy",
                        "healthy body",
                        "healthy mind"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий хорошим физическим или психическим здоровьем; не больной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здоровый",
                                "target_lang_sense_clarification": "о человеке или организме"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5d8bd3a-e038-3393-b453-9f577bd5eabd",
                    "sense_definition": "Good for your health; helping to produce or maintain good health.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <w>healthy</w> diet helps you maintain a good weight.",
                            "target_lang_translations": {
                                "ru": "<w>Здоровая</w> диета помогает поддерживать нормальный вес."
                            }
                        },
                        {
                            "text": "Fresh fruit and vegetables are very <w>healthy</w> for you.",
                            "target_lang_translations": {
                                "ru": "Свежие фрукты и овощи очень <w>полезны</w> для вас."
                            }
                        }
                    ],
                    "synonyms": [
                        "wholesome",
                        "healthful"
                    ],
                    "antonyms": [
                        "unhealthy"
                    ],
                    "common_phrases": [
                        "healthy diet",
                        "healthy food",
                        "healthy lifestyle",
                        "healthy eating"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полезный для здоровья; способствующий укреплению здоровья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полезный",
                                "target_lang_sense_clarification": "благотворный для организма"
                            },
                            {
                                "target_lang_word": "здоровый",
                                "target_lang_sense_clarification": "об образе жизни, питании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "688d56c7-8547-32e7-9c23-9c24205df9d9",
                    "sense_definition": "Showing visible signs of being strong and well.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has a <w>healthy</w> glow on her face after the walk.",
                            "target_lang_translations": {
                                "ru": "После прогулки на её лице появился <w>здоровый</w> румянец."
                            }
                        },
                        {
                            "text": "The doctor said the baby looks <w>healthy</w> and happy.",
                            "target_lang_translations": {
                                "ru": "Врач сказал, что ребенок выглядит <w>здоровым</w> и счастливым."
                            }
                        }
                    ],
                    "common_phrases": [
                        "healthy glow",
                        "healthy look"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свидетельствующий о хорошем самочувствии и силе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здоровый",
                                "target_lang_sense_clarification": "о внешнем виде, цвете лица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07acb5c3-ff7c-3090-a3e6-71c41d7dcec4",
                    "sense_definition": "Large in amount, size, or degree; significant.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He took a <w>healthy</w> bite of his sandwich.",
                            "target_lang_translations": {
                                "ru": "Он откусил <w>большой</w> кусок от своего сэндвича."
                            }
                        },
                        {
                            "text": "The company made a <w>healthy</w> profit this year.",
                            "target_lang_translations": {
                                "ru": "В этом году компания получила <w>солидную</w> прибыль."
                            }
                        }
                    ],
                    "synonyms": [
                        "healthy",
                        "sound",
                        "fit"
                    ],
                    "common_phrases": [
                        "healthy appetite",
                        "healthy respect",
                        "healthy profit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой по количеству, размеру или степени; значительный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "солидный",
                                "target_lang_sense_clarification": "значительный (о сумме, прибыли)"
                            },
                            {
                                "target_lang_word": "большой",
                                "target_lang_sense_clarification": "крупный (о размере)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
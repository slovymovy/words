{
    "word_family": [
        "zawijas",
        "zawiniątko",
        "zawijanie",
        "owijak"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b728083a-7be7-3ec2-b8a8-79cd4fec4921",
                    "sense_definition": "Owijać przedmiot papierem, folią lub tkaniną w celu jego zapakowania, ukrycia lub zabezpieczenia.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "wrapping_objects",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę ładnie <w>zawijać</w> wszystkie prezenty, żeby leżały gotowe pod choinką.",
                            "target_lang_translations": {
                                "ru": "Я должен красиво <w>заворачивать</w> все подарки, чтобы они лежали готовые под елкой."
                            }
                        },
                        {
                            "text": "Sprzedawczyni zaczęła starannie <w>zawijać</w> świeżą rybę w gruby papier.",
                            "target_lang_translations": {
                                "ru": "Продавщица начала тщательно <w>заворачивать</w> свежую рыбу в плотную бумагу."
                            }
                        }
                    ],
                    "synonyms": [
                        "pakować",
                        "owijać"
                    ],
                    "antonyms": [
                        "rozwijać",
                        "odpakowywać"
                    ],
                    "common_phrases": [
                        "zawijać w papier",
                        "zawijać w sreberko",
                        "zawijać prezent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оборачивать предмет бумагой, пленкой или тканью, чтобы упаковать или защитить его."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заворачивать",
                                "target_lang_sense_clarification": "оборачивать во что-то"
                            },
                            {
                                "target_lang_word": "упаковывать",
                                "target_lang_sense_clarification": "делать упаковку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "faa006b3-edb3-38cf-8cac-adbb3e960da1",
                    "sense_definition": "Zaginać krawędzie materiału, papieru lub innej rzeczy do środka lub na drugą stronę.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "folding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy myję naczynia, zawsze muszę <w>zawijać</w> rękawy koszuli, żeby ich nie zamoczyć.",
                            "target_lang_translations": {
                                "ru": "Когда я мою посуду, мне всегда приходится <w>закатывать</w> рукава рубашки, чтобы их не намочить."
                            }
                        },
                        {
                            "text": "Nie wolno <w>zawijać</w> rogów w książkach wypożyczonych z biblioteki.",
                            "target_lang_translations": {
                                "ru": "Нельзя <w>загибать</w> уголки в книгах, взятых из библиотеки."
                            }
                        }
                    ],
                    "synonyms": [
                        "podwijać",
                        "zaginać"
                    ],
                    "antonyms": [
                        "prostować",
                        "odwijać"
                    ],
                    "common_phrases": [
                        "zawijać rękawy",
                        "zawijać nogawki",
                        "zawijać rogi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Загибать края материала или бумаги; подворачивать одежду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закатывать",
                                "target_lang_sense_clarification": "рукава или штанины"
                            },
                            {
                                "target_lang_word": "загибать",
                                "target_lang_sense_clarification": "края или углы"
                            },
                            {
                                "target_lang_word": "подворачивать",
                                "target_lang_sense_clarification": "одежду"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fceda823-c32c-3328-ae85-e5389a435d02",
                    "sense_definition": "Szczelnie okrywać własne ciało kocem, kołdrą lub ubraniem dla ciepła lub ochrony.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "wrapping_self",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W zimowe wieczory lubię <w>zawijać</w> się w ciepły koc i czytać książkę.",
                            "target_lang_translations": {
                                "ru": "Зимними вечерами я люблю <w>кутаться</w> в теплый плед и читать книгу."
                            }
                        },
                        {
                            "text": "Dziecko zaczęło <w>zawijać</w> się w ręcznik zaraz po wyjściu z zimnej wody.",
                            "target_lang_translations": {
                                "ru": "Ребенок начал <w>заворачиваться</w> в полотенце сразу после выхода из холодной воды."
                            }
                        }
                    ],
                    "synonyms": [
                        "otulać się",
                        "owijać się"
                    ],
                    "antonyms": [
                        "odkrywać się"
                    ],
                    "common_phrases": [
                        "zawijać się w koc",
                        "zawijać się w kołdrę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плотно укутывать себя одеялом или одеждой для тепла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кутаться",
                                "target_lang_sense_clarification": "укрываться для тепла"
                            },
                            {
                                "target_lang_word": "заворачиваться",
                                "target_lang_sense_clarification": "обматывать себя чем-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc9a3f4f-356d-389d-89d2-594fe22cacfa",
                    "sense_definition": "Szybko opuszczać jakieś miejsce, uciekać lub kończyć jakąś czynność i odchodzić.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "leaving_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Impreza się skończyła, więc czas się stąd <w>zawijać</w> do domu.",
                            "target_lang_translations": {
                                "ru": "Вечеринка закончилась, так что пора отсюда <w>сваливать</w> домой."
                            }
                        },
                        {
                            "text": "Musimy się szybko <w>zawijać</w>, zanim ktoś zauważy naszą obecność w tym miejscu.",
                            "target_lang_translations": {
                                "ru": "Мы должны быстро <w>сматываться</w>, пока кто-нибудь не заметил нашего присутствия в этом месте."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmywać się",
                        "uciekać",
                        "spadać"
                    ],
                    "common_phrases": [
                        "zawijać się stąd",
                        "zawijać wrotki",
                        "zawijać kiecę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро покидать место, убегать или заканчивать дело (разговорное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сматываться",
                                "target_lang_sense_clarification": "убегать, уходить (слэнг)"
                            },
                            {
                                "target_lang_word": "сваливать",
                                "target_lang_sense_clarification": "уходить (грубое/слэнг)"
                            },
                            {
                                "target_lang_word": "закругляться",
                                "target_lang_sense_clarification": "заканчивать дело"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "377d092f-3fc8-3f26-a5b4-8c2a135a82b4",
                    "sense_definition": "Wpływać statkiem do portu lub zatrzymywać się w określonym miejscu podczas rejsu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nautical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten duży statek pasażerski będzie <w>zawijać</w> do portu w Gdyni jutro rano.",
                            "target_lang_translations": {
                                "ru": "Этот большой пассажирский корабль будет <w>заходить</w> в порт Гдыни завтра утром."
                            }
                        },
                        {
                            "text": "Podczas sztormu musieliśmy awaryjnie <w>zawijać</w> do najbliższej bezpiecznej przystani.",
                            "target_lang_translations": {
                                "ru": "Во время шторма нам пришлось аварийно <w>заходить</w> в ближайшую безопасную гавань."
                            }
                        }
                    ],
                    "synonyms": [
                        "przybijać",
                        "wpływać"
                    ],
                    "antonyms": [
                        "wypływać"
                    ],
                    "common_phrases": [
                        "zawijać do portu",
                        "zawijać do przystani"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заходить в порт или останавливаться в месте назначения (о судне)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заходить",
                                "target_lang_sense_clarification": "в порт"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "sense_definition": "Het hele leven durend of geldig blijvend; tot aan de dood.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft een <w>levenslange</w> gevangenisstraf gekregen voor zijn daden.",
                            "target_lang_translations": {
                                "en": "He received a <w>life</w> sentence for his actions.",
                                "ru": "Он получил <w>пожизненное</w> тюремное заключение за свои деяния."
                            }
                        },
                        {
                            "text": "Mijn grootouders hebben een <w>levenslange</w> vriendschap met hun buren.",
                            "target_lang_translations": {
                                "en": "My grandparents have a <w>lifelong</w> friendship with their neighbors.",
                                "ru": "Моих бабушку и дедушку связывает <w>пожизненная</w> дружба с соседями."
                            }
                        }
                    ],
                    "synonyms": [
                        "eeuwigdurend",
                        "permanent"
                    ],
                    "antonyms": [
                        "tijdelijk",
                        "kortstondig"
                    ],
                    "common_phrases": [
                        "levenslang leren",
                        "een levenslange gevangenisstraf",
                        "levenslange garantie"
                    ],
                    "target_lang_definitions": {
                        "en": "Lasting the whole life or remaining valid; until death.",
                        "ru": "Длится всю жизнь или остается в силе до самой смерти."
                    },
                    "translations": {

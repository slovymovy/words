    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3b96fd53-1eb9-31c1-a4a1-fe66992f118a",
                    "sense_definition": "Klawisz specjalny na klawiaturze komputerowej, znajdujący się po obu stronach spacji, używany w połączeniu z innymi klawiszami do wywoływania określonych funkcji lub znaków.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aby wpisać polski znak, musisz przytrzymać prawy <alt>.",
                            "target_lang_translations": {
                                "ru": "Чтобы ввести польский символ, нужно удерживать правый <w>Alt</w>."
                            }
                        },
                        {
                            "text": "Skrót klawiszowy <alt> i F4 zamyka aktywne okno programu.",
                            "target_lang_translations": {
                                "ru": "Сочетание клавиш <w>Alt</w> и F4 закрывает активное окно программы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wcisnąć alt",
                        "lewy alt",
                        "prawy alt",
                        "trzymać alt"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "informatyka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Специальная клавиша на компьютерной клавиатуре, расположенная по обе стороны от пробела, используемая в сочетании с другими клавишами для вызова определенных функций или символов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Alt",
                                "target_lang_sense_clarification": "клавиша"
                            },
                            {
                                "target_lang_word": "альт",
                                "target_lang_sense_clarification": "разговорное название клавиши"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4a227a2-a7eb-37c1-9e8f-7022862fb4f7",
                    "sense_definition": "Niski głos żeński lub chłopięcy, a także bardzo wysoki głos męski.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "music_voice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej naturalny <alt> brzmi bardzo ciepło i głęboko w tej piosence.",
                            "target_lang_translations": {
                                "ru": "Ее естественный <w>альт</w> звучит очень тепло и глубоко в этой песне."
                            }
                        },
                        {
                            "text": "W szkolnym chórze zawsze wolałam śpiewać <altem> niż sopranem.",
                            "target_lang_translations": {
                                "ru": "В школьном хоре я всегда предпочитала петь <w>альтом</w>, а не сопрано."
                            }
                        }
                    ],
                    "synonyms": [
                        "kontralt",
                        "kontratenor"
                    ],
                    "antonyms": [
                        "sopran"
                    ],
                    "common_phrases": [
                        "śpiewać altem",
                        "głęboki alt",
                        "ciepły alt",
                        "niski alt"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "muzyka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Низкий женский или детский голос, а также очень высокий мужской голос."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "альт",
                                "target_lang_sense_clarification": "голос"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42a88831-5f83-37ea-b0ca-79d3b15a6c02",
                    "sense_definition": "Osoba śpiewająca niskim głosem żeńskim lub chłopięcym.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "music_voice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrygent poprosił wszystkie <alty> o powtórzenie ostatniego fragmentu utworu.",
                            "target_lang_translations": {
                                "ru": "Дирижер попросил все <w>альты</w> повторить последний фрагмент произведения."
                            }
                        },
                        {
                            "text": "Jako <alt>, Maria stoi zazwyczaj w drugim rzędzie chóru.",
                            "target_lang_translations": {
                                "ru": "Как <w>альт</w>, Мария обычно стоит во втором ряду хора."
                            }
                        }
                    ],
                    "synonyms": [
                        "altysta",
                        "alcista"
                    ],
                    "common_phrases": [
                        "sekcja altów",
                        "chór altów"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "muzyka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, поющий низким женским или детским голосом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "альт",
                                "target_lang_sense_clarification": "певец или певица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6dbd38e-6134-3557-8306-22371204d714",
                    "sense_definition": "Instrument muzyczny o średniej skali dźwięku, niższej od sopranu, a wyższej od tenora (np. altówka lub saksofon altowy).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "music_instrument",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten utwór został skomponowany specjalnie na <alt> i fortepian.",
                            "target_lang_translations": {
                                "ru": "Это произведение было написано специально для <w>альта</w> и фортепиано."
                            }
                        },
                        {
                            "text": "Dźwięk <altu> jest łagodniejszy i ciemniejszy niż skrzypiec.",
                            "target_lang_translations": {
                                "ru": "Звук <w>альта</w> мягче и темнее, чем у скрипки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "grać na alcie",
                        "stroić alt"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "muzyka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Музыкальный инструмент среднего регистра, ниже сопрано, но выше тенора (например, альт или альт-саксофон)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "альт",
                                "target_lang_sense_clarification": "музыкальный инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa49a7ed-a7eb-35d2-af7c-8e10ef61d2bb",
                    "sense_definition": "Enzym wątrobowy (aminotransferaza alaninowa), którego stężenie we krwi jest wskaźnikiem stanu zdrowia wątroby.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zlecił pacjentowi badanie poziomu <alt> w surowicy krwi.",
                            "target_lang_translations": {
                                "ru": "Врач назначил пациенту анализ уровня <w>АЛТ</w> в сыворотке крови."
                            }
                        },
                        {
                            "text": "Znacznie podwyższony <alt> może świadczyć o uszkodzeniu komórek wątroby.",
                            "target_lang_translations": {
                                "ru": "Значительно повышенный <w>АЛТ</w> может свидетельствовать о повреждении клеток печени."
                            }
                        }
                    ],
                    "common_phrases": [
                        "poziom alt",
                        "badanie alt",
                        "podwyższony alt"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "medycyna"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Печеночный фермент (аланинаминотрансфераза), уровень которого в крови является показателем состояния здоровья печени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "АЛТ",
                                "target_lang_sense_clarification": "медицинская аббревиатура"
                            },
                            {
                                "target_lang_word": "аланинаминотрансфераза",
                                "target_lang_sense_clarification": "полное название фермента"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39da5212-663c-3554-af3a-350066843f77",
                    "sense_definition": "Dobry humor, zapał lub entuzjazm.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "archaic_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo podeszłego wieku, dziadek wciąż opowiada historie z wielkim <altem>.",
                            "target_lang_translations": {
                                "ru": "Несмотря на преклонный возраст, дедушка все еще рассказывает истории с большим <w>задором</w>."
                            }
                        },
                        {
                            "text": "To człowiek z <altem>, zawsze skory do zabawy i żartów.",
                            "target_lang_translations": {
                                "ru": "Это человек с <w>огоньком</w>, всегда готовый к веселью и шуткам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "z altem"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "regional",
                            "comment": "literackie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Хорошее настроение, задор или энтузиазм."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задор",
                                "target_lang_sense_clarification": "эмоциональное состояние"
                            },
                            {
                                "target_lang_word": "оживление",
                                "target_lang_sense_clarification": "настроение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

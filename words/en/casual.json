    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "64c5c212-ca21-3e1b-ad64-dc82e9752780",
                    "sense_definition": "Designed for informal or everyday use; not formal.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "informal_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dress code for the party is <w>casual</w>, so you can wear jeans.",
                            "target_lang_translations": {
                                "ru": "Дресс-код вечеринки — <w>неформа́льный</w>, так что можно пойти в джинсах."
                            }
                        },
                        {
                            "text": "I prefer wearing <w>casual</w> clothes when I am relaxing at home.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю носить <w>повседне́вную</w> одежду, когда отдыхаю дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "informal"
                    ],
                    "antonyms": [
                        "formal"
                    ],
                    "common_phrases": [
                        "casual wear",
                        "smart casual",
                        "casual clothes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предназначенный для неофициального или повседневного использования; не парадный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повседне́вный",
                                "target_lang_sense_clarification": "об одежде или вещах"

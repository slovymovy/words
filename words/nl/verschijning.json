{
    "word_family": [
        "verschijnen",
        "verschijnsel",
        "schijnen",
        "schijn",
        "tevoorschijn"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b7adc048-8c9e-38fe-a593-d63fa90f8015",
                    "sense_definition": "Het moment waarop iets of iemand zichtbaar wordt of beschikbaar komt, zoals een boek of een gast.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "act_of_appearing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De krant besteedde veel aandacht aan de <w>verschijning</w> van zijn nieuwe roman.",
                            "target_lang_translations": {
                                "en": "The newspaper paid much attention to the <w>publication</w> of his new novel."
                            }
                        },
                        {
                            "text": "Haar plotselinge <w>verschijning</w> op het feest zorgde voor veel opschudding.",
                            "target_lang_translations": {
                                "en": "Her sudden <w>appearance</w> at the party caused quite a stir."
                            }
                        }
                    ],
                    "synonyms": [
                        "komst",
                        "publicatie",
                        "uitgave"
                    ],
                    "antonyms": [
                        "verdwijning"
                    ],
                    "common_phrases": [
                        "eerste verschijning",
                        "datum van verschijning",
                        "na verschijning"
                    ],
                    "target_lang_definitions": {
                        "en": "The moment when something or someone becomes visible or available, such as a book or a guest."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appearance",
                                "target_lang_sense_clarification": "the act of becoming visible or available"
                            },
                            {
                                "target_lang_word": "publication",
                                "target_lang_sense_clarification": "the release of a book or article"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a9c1116-e3d1-3cb6-8403-e96b55563dd2",
                    "sense_definition": "Iemand of iets dat men waarneemt of tegenkomt; een persoon of fenomeen dat zich vertoont.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "object_appearing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tornado's zijn een zeldzame <w>verschijning</w> in de stad.",
                            "target_lang_translations": {
                                "en": "Tornadoes are a rare <w>sight</w> in the city."
                            }
                        },
                        {
                            "text": "Met haar rode hoed was ze een opvallende <w>verschijning</w> in de menigte.",
                            "target_lang_translations": {
                                "en": "With her red hat, she was a striking <w>figure</w> in the crowd."
                            }
                        }
                    ],
                    "synonyms": [
                        "fenomeen",
                        "vertoning"
                    ],
                    "common_phrases": [
                        "zeldzame verschijning",
                        "opvallende verschijning",
                        "bekende verschijning"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone or something that is perceived or encountered; a person or phenomenon that shows itself."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sight",
                                "target_lang_sense_clarification": "something seen, especially if rare or remarkable"
                            },
                            {
                                "target_lang_word": "appearance",
                                "target_lang_sense_clarification": "a person or thing that appears"
                            },
                            {
                                "target_lang_word": "figure",
                                "target_lang_sense_clarification": "a person seen, often with a specific impression"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aaf8e7c9-c4f6-3baf-a700-4f1edf318a43",
                    "sense_definition": "De manier waarop iemand of iets eruitziet voor de buitenwereld.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "look",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij besteedt altijd veel zorg aan zijn uiterlijke <w>verschijning</w>.",
                            "target_lang_translations": {
                                "en": "He always takes great care of his outward <w>appearance</w>."
                            }
                        },
                        {
                            "text": "Ondanks zijn ruwe <w>verschijning</w> heeft hij een heel vriendelijk karakter.",
                            "target_lang_translations": {
                                "en": "Despite his rough <w>appearance</w>, he has a very friendly character."
                            }
                        }
                    ],
                    "synonyms": [
                        "uiterlijk",
                        "aanzien",
                        "voorkomen"
                    ],
                    "common_phrases": [
                        "uiterlijke verschijning",
                        "verzorgde verschijning"
                    ],
                    "target_lang_definitions": {
                        "en": "The way someone or something looks to the outside world."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appearance",
                                "target_lang_sense_clarification": "physical look or external aspect"
                            },
                            {
                                "target_lang_word": "look",
                                "target_lang_sense_clarification": "visual impression or physical appearance"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d732add7-8f3b-3005-bf66-2d0f439d6b2a",
                    "sense_definition": "Een beeld van iets dat er in werkelijkheid niet fysiek is, vaak met een bovennatuurlijk karakter.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "apparition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dorpelingen spraken vol angst over een mysterieuze <w>verschijning</w> op het kerkhof.",
                            "target_lang_translations": {
                                "en": "The villagers spoke fearfully of a mysterious <w>apparition</w> in the graveyard."
                            }
                        },
                        {
                            "text": "Hij beweerde dat hij een heilige <w>verschijning</w> had gezien tijdens zijn gebed.",
                            "target_lang_translations": {
                                "en": "He claimed to have seen a holy <w>apparition</w> during his prayer."
                            }
                        }
                    ],
                    "synonyms": [
                        "geest",
                        "visioen",
                        "spook"
                    ],
                    "common_phrases": [
                        "mysterieuze verschijning",
                        "heilige verschijning"
                    ],
                    "target_lang_definitions": {
                        "en": "An image of something that is not physically present, often of a supernatural nature."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "apparition",
                                "target_lang_sense_clarification": "a supernatural appearance or ghost"
                            },
                            {
                                "target_lang_word": "vision",
                                "target_lang_sense_clarification": "something seen in a dream or trance"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
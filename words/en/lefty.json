    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "915f833c-82d6-337b-8d23-547372695bc1",
                    "sense_definition": "A person who naturally uses their left hand for writing and other activities.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "handedness_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My brother is a <w>lefty</w>, so he always sits at the end of the table to avoid bumping elbows.",
                            "target_lang_translations": {
                                "ru": "Мой брат — <w>левша</w>, поэтому он всегда садится с краю стола, чтобы не толкаться локтями."
                            }
                        },
                        {
                            "text": "Being a <w>lefty</w> can be challenging when using tools designed for right-handed people.",
                            "target_lang_translations": {
                                "ru": "Быть <w>левшой</w> может быть непросто, когда пользуешься инструментами, созданными для правшей."
                            }
                        }
                    ],
                    "synonyms": [
                        "left-hander",
                        "southpaw",
                        "leftie"
                    ],
                    "antonyms": [
                        "righty",
                        "rightie"
                    ],
                    "common_phrases": [
                        "a natural lefty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который от природы лучше владеет левой рукой при письме и других действиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "левша",
                                "target_lang_sense_clarification": "человек, владеющий левой рукой лучше, чем правой"
                            },
                            {
                                "target_lang_word": "леворукий",
                                "target_lang_sense_clarification": "более формальный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0212d18-5e68-3dbe-9c3d-d9dba76bb185",
                    "sense_definition": "A person who supports left-wing political ideologies or socialism.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "politics_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The candidate is a committed <w>lefty</w> who advocates for higher taxes on the wealthy.",
                            "target_lang_translations": {
                                "ru": "Кандидат — убежденный <w>левак</w>, выступающий за повышение налогов для богатых."
                            }
                        },
                        {
                            "text": "Some people use the term <w>lefty</w> as an insult, while others embrace it with pride.",
                            "target_lang_translations": {
                                "ru": "Некоторые используют слово «<w>левак</w>» как оскорбление, в то время как другие носят его с гордостью."
                            }
                        }
                    ],
                    "synonyms": [
                        "leftist",
                        "left-winger",
                        "leftie"
                    ],
                    "antonyms": [
                        "righty",
                        "rightie"
                    ],
                    "common_phrases": [
                        "a radical lefty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сторонник левых политических взглядов или социализма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "левак",
                                "target_lang_sense_clarification": "разговорное, иногда пренебрежительное название сторонника левых взглядов"
                            },
                            {
                                "target_lang_word": "левый",
                                "target_lang_sense_clarification": "общий термин для приверженца левых партий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c9e191e-5a90-3a30-8392-c1291316983b",

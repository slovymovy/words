        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "619185db-efee-34b4-b6de-2417d12342c2",
                    "sense_definition": "Een bevoegdheid of aanspraak die iemand heeft volgens de wet of regels.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "entitlement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Iedere burger heeft het <w>recht</w> om te stemmen.",
                            "target_lang_translations": {
                                "ru": "Каждый гражданин имеет <w>право</w> голосовать.",
                                "en": "Every citizen has the <w>right</w> to vote."
                            }
                        },
                        {
                            "text": "Je hebt geen enkel <w>recht</w> om mijn spullen te pakken.",
                            "target_lang_translations": {
                                "ru": "У тебя нет никакого <w>права</w> брать мои вещи.",
                                "en": "You have no <w>right</w> whatsoever to take my stuff."
                            }
                        }
                    ],
                    "antonyms": [
                        "plicht"
                    ],
                    "common_phrases": [
                        "recht hebben op",
                        "het recht behouden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полномочие или правопритязание, которое лицо имеет согласно закону или правилам.",
                        "en": "An authority or claim that someone has according to the law or rules."

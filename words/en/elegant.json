            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "52819259-89ef-3fc2-b7a1-300409ee828e",
                    "sense_definition": "Graceful and stylish in appearance or manner.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "style_grace",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She looked incredibly <w>elegant</w> in her long black evening gown.",
                            "target_lang_translations": {
                                "ru": "Она выглядела невероятно <w>элегантно</w> в своем длинном черном вечернем платье."
                            }
                        },
                        {
                            "text": "The hotel lobby features an <w>elegant</w> design with marble floors and crystal chandeliers.",
                            "target_lang_translations": {
                                "ru": "Вестибюль отеля отличается <w>элегантным</w> дизайном с мраморными полами и хрустальными люстрами."
                            }
                        }
                    ],
                    "synonyms": [
                        "graceful",
                        "classy",
                        "dressy"
                    ],
                    "antonyms": [
                        "clumsy",
                        "inelegant"
                    ],
                    "common_phrases": [
                        "elegant dress",
                        "elegant dining",
                        "elegant style"
                    ],
                    "target_lang_definitions": {

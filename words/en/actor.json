            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c97d92fd-c201-3567-82b4-24d75729a12e",
                    "sense_definition": "A person who performs a role in a play, film, or television show.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lead <w>actor</w> won an award for his amazing performance in the movie.",
                            "target_lang_translations": {
                                "ru": "Ведущий <w>актёр</w> получил награду за свою потрясающую игру в фильме."
                            }
                        },
                        {
                            "text": "She wants to move to Hollywood and become a famous <w>actor</w>.",
                            "target_lang_translations": {
                                "ru": "Она хочет переехать в Голливуд и стать известной <w>актрисой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "performer",
                        "player"
                    ],
                    "common_phrases": [
                        "lead actor",
                        "supporting actor",
                        "voice actor",
                        "famous actor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, исполняющий роль в пьесе, фильме или телешоу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "актёр",
                                "target_lang_sense_clarification": "исполнитель ролей"
                            },
                            {
                                "target_lang_word": "артист",
                                "target_lang_sense_clarification": "творческий работник, исполнитель"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c9eb20d-65b6-3b96-96e3-d6695c965749",
                    "sense_definition": "A person, group, or entity that takes part in a specific action or process.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The country has become a key political <w>actor</w> in the region.",
                            "target_lang_translations": {
                                "ru": "Страна стала ключевым политическим <w>актором</w> в регионе."
                            }
                        },
                        {
                            "text": "Every <w>actor</w> in the conflict must agree to the terms of the peace treaty.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>участник</w> конфликта должен согласиться с условиями мирного договора."
                            }
                        }
                    ],
                    "synonyms": [
                        "participant",
                        "doer"
                    ],
                    "common_phrases": [
                        "key actor",
                        "political actor",
                        "state actor",
                        "bad actor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, группа или организация, принимающие участие в конкретном действии или процессе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "участник",
                                "target_lang_sense_clarification": "тот, кто участвует в чем-либо"
                            },
                            {
                                "target_lang_word": "актор",
                                "target_lang_sense_clarification": "субъект действия (политология, социология)"
                            },
                            {
                                "target_lang_word": "деятель",
                                "target_lang_sense_clarification": "активное лицо в общественной жизни"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5d29d16-7695-346d-9d5e-a49fcb25625d",
                    "sense_definition": "A person who starts a legal lawsuit against someone else.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "legal_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In this civil case, the <w>actor</w> must prove that the damage occurred.",
                            "target_lang_translations": {
                                "ru": "В этом гражданском деле <w>истец</w> должен доказать факт причинения ущерба."
                            }
                        },
                        {
                            "text": "The court heard the arguments presented by the <w>actor</w> regarding the property dispute.",
                            "target_lang_translations": {
                                "ru": "Суд заслушал доводы, представленные <w>истцом</w> касательно имущественного спора."
                            }
                        }
                    ],
                    "synonyms": [
                        "plaintiff",
                        "complainant"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, возбуждающее судебный иск против кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "истец",
                                "target_lang_sense_clarification": "юридический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "430da7ab-a033-38ea-b826-8756dcaeb457",
                    "sense_definition": "The grammatical subject that performs the action expressed by a verb.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "linguistics_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the sentence 'John threw the ball', John is the <w>actor</w>.",
                            "target_lang_translations": {
                                "ru": "В предложении «Джон бросил мяч» Джон — это <w>агенс</w>."
                            }
                        },
                        {
                            "text": "You should identify the <w>actor</w> and the undergoer to understand the sentence structure.",
                            "target_lang_translations": {
                                "ru": "Вам следует определить <w>агенса</w> и пациенса, чтобы понять структуру предложения."
                            }
                        }
                    ],
                    "antonyms": [
                        "undergoer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Грамматическое подлежащее, выполняющее действие, выраженное глаголом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "агенс",
                                "target_lang_sense_clarification": "лингвистический термин"
                            },
                            {
                                "target_lang_word": "субъект действия",
                                "target_lang_sense_clarification": "грамматика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b69dd7a-bb7a-3b79-9e36-610c3329968d",
                    "sense_definition": "An entity or role that interacts with a system in software analysis.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "computing_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The system administrator is the primary <w>actor</w> in this use case scenario.",
                            "target_lang_translations": {
                                "ru": "Системный администратор является основным <w>актором</w> в этом сценарии использования."
                            }
                        },
                        {
                            "text": "We need to define how the external <w>actor</w> communicates with the database.",
                            "target_lang_translations": {
                                "ru": "Нам нужно определить, как внешний <w>актор</w> взаимодействует с базой данных."
                            }
                        }
                    ],
                    "synonyms": [
                        "role"
                    ],
                    "common_phrases": [
                        "system actor",
                        "primary actor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сущность или роль, взаимодействующая с системой при анализе программного обеспечения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "актор",
                                "target_lang_sense_clarification": "в UML и проектировании систем"
                            },
                            {
                                "target_lang_word": "действующее лицо",
                                "target_lang_sense_clarification": "роль в сценарии использования"
                            }
                        ]
                    }
                }
            ]
        }

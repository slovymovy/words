    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "bce44150-7f93-38fe-8ee3-df1b446dbd5f",
                    "sense_definition": "Zich soepel en gelijkmatig over een oppervlak bewegen, vaak door gladheid of met weinig wrijving.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "motion_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen <gleden> met hun slee van de besneeuwde heuvel.",
                            "target_lang_translations": {
                                "en": "The children <slid> down the snowy hill on their sled.",
                                "ru": "Дети <съезжали> на санках с заснеженной горки."
                            }
                        },
                        {
                            "text": "Hij liet het koude water over zijn handen <glijden> om af te koelen.",
                            "target_lang_translations": {
                                "en": "He let the cold water <glide> over his hands to cool down.",
                                "ru": "Он позволил холодной воде <скользить> по рукам, чтобы остыть."
                            }
                        }
                    ],
                    "synonyms": [
                        "schuiven",
                        "glibberen"
                    ],
                    "common_phrases": [
                        "over het ijs glijden",
                        "uit de handen glijden"
                    ],
                    "target_lang_definitions": {
                        "en": "To move smoothly and evenly over a surface, often due to slipperiness or low friction.",
                        "ru": "Плавно и равномерно двигаться по поверхности, часто из-за скользкости или малого трения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slide",
                                "target_lang_sense_clarification": "General movement over a surface"
                            },
                            {
                                "target_lang_word": "glide",
                                "target_lang_sense_clarification": "Smooth, effortless movement"
                            },
                            {
                                "target_lang_word": "slip",
                                "target_lang_sense_clarification": "To slide quickly or unintentionally, e.g., out of hands"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "скользить",
                                "target_lang_sense_clarification": "Двигаться плавно по гладкой поверхности"
                            },
                            {
                                "target_lang_word": "съезжать",
                                "target_lang_sense_clarification": "Двигаться вниз по наклонной поверхности (например, на санках)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "98e00b39-48d4-3564-b766-11d20ab72b80",
                    "sense_definition": "Als spel van een glijbaan naar beneden gaan.",
                    "learner_level": "A2",
                    "frequency": "Low",
                    "semantic_group_id": "motion_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de speeltuin wilden de kleuters de hele middag <glijden>.",
                            "target_lang_translations": {
                                "en": "In the playground, the toddlers wanted to <slide> all afternoon.",
                                "ru": "На детской площадке малыши хотели весь день <кататься с горки>."
                            }
                        },
                        {
                            "text": "Mijn zoontje heeft gisteren wel tien keer van de hoge baan <gegleden>.",
                            "target_lang_translations": {
                                "en": "My son <slid> down the high slide at least ten times yesterday.",
                                "ru": "Вчера мой сын не меньше десяти раз <съехал> с высокой горки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "van de glijbaan glijden"
                    ],
                    "target_lang_definitions": {
                        "en": "To go down a slide as a game.",
                        "ru": "Спускаться с горки в качестве игры."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slide",
                                "target_lang_sense_clarification": "To use a playground slide"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "кататься с горки",
                                "target_lang_sense_clarification": "Использовать детскую горку для игры"
                            },
                            {
                                "target_lang_word": "съезжать",
                                "target_lang_sense_clarification": "Спускаться вниз (с горки)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6dcff27-26e7-3ef6-86a3-9b3021461b05",
                    "sense_definition": "Langzaam, ongemerkt en soepel in een andere toestand of situatie overgaan.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "change_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verbijstering <gleed> langzaam van haar gezicht toen ze het nieuws begreep.",
                            "target_lang_translations": {
                                "en": "The bewilderment slowly <slid> from her face when she understood the news.",
                                "ru": "Растерянность медленно <сползала> с её лица, когда она осознала новости."
                            }
                        },
                        {
                            "text": "Na een lange werkdag <gleed> hij rustig in een diepe slaap.",
                            "target_lang_translations": {
                                "en": "After a long workday, he quietly <slipped> into a deep sleep.",
                                "ru": "После долгого рабочего дня он спокойно <погрузился> в глубокий сон."
                            }
                        }
                    ],
                    "synonyms": [
                        "afglijden",
                        "wegzakken"
                    ],
                    "common_phrases": [
                        "in slaap glijden",
                        "van zich af laten glijden"
                    ],
                    "target_lang_definitions": {
                        "en": "To transition slowly, unnoticed, and smoothly into another state or situation.",
                        "ru": "Медленно, незаметно и плавно переходить в другое состояние или ситуацию."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slip",
                                "target_lang_sense_clarification": "To gradually pass into a state"
                            },
                            {
                                "target_lang_word": "slide",
                                "target_lang_sense_clarification": "To change from one state to another"
                            },
                            {
                                "target_lang_word": "glide",
                                "target_lang_sense_clarification": "To transition effortlessly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "погружаться",
                                "target_lang_sense_clarification": "Плавно переходить в состояние (например, в сон)"
                            },
                            {
                                "target_lang_word": "сползать",
                                "target_lang_sense_clarification": "Медленно исчезать или сходить (об эмоциях на лице)"
                            },
                            {
                                "target_lang_word": "соскальзывать",
                                "target_lang_sense_clarification": "Незаметно переходить к чему-то другому"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
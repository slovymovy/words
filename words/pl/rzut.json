    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "47021e30-80fb-3e24-a610-b29619c5c2d5",
                    "sense_definition": "Czynność polegająca na gwałtownym wyrzuceniu przedmiotu w powietrze z użyciem siły; także konkurencja sportowa oparta na tej czynności.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "O wyniku meczu zadecydował <w>rzut</w> monetą przez sędziego.",
                            "target_lang_translations": {
                                "ru": "Исход матча решил <w>бросок</w> монеты судьёй."
                            }
                        },
                        {
                            "text": "Polski zawodnik oddał najdłuższy <w>rzut</w> oszczepem w tym sezonie.",
                            "target_lang_translations": {
                                "ru": "Польский спортсмен совершил самое дальнее <w>метание</w> копья в этом сезоне."
                            }
                        }
                    ],
                    "synonyms": [
                        "rzucenie",
                        "wyrzut"
                    ],
                    "antonyms": [
                        "chwyt",
                        "złapanie"
                    ],
                    "common_phrases": [
                        "rzut monetą",
                        "rzut dyskiem",
                        "rzut oszczepem",
                        "rzut kulą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, заключающееся в резком выбрасывании предмета в воздух с применением силы; также спортивная дисциплина, основанная на этом действии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бросок",
                                "target_lang_sense_clarification": "общее значение; баскетбол, жребий"
                            },
                            {
                                "target_lang_word": "метание",
                                "target_lang_sense_clarification": "легкая атлетика: копье, диск, молот"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95175cf5-0a9c-30fe-b381-54a2b08156ca",
                    "sense_definition": "W piłce nożnej i innych grach zespołowych: wprowadzenie piłki do gry ze stałego fragmentu boiska, często wykonywane nogą.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sport_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sędzia podyktował <w>rzut</w> karny po faulu w polu bramkowym.",
                            "target_lang_translations": {
                                "ru": "Судья назначил одиннадцатиметровый <w>удар</w> (пенальти) после фола в штрафной площади."
                            }
                        },
                        {
                            "text": "Drużyna gości zdobyła bramkę bezpośrednio po <w>rzucie</w> rożnym.",
                            "target_lang_translations": {
                                "ru": "Команда гостей забила гол прямо с углового <w>удара</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rzut karny",
                        "rzut rożny",
                        "rzut wolny",
                        "rzut sędziowski"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "sport"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В футболе и других командных играх: введение мяча в игру со стандартного положения, часто выполняемое ногой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "удар",
                                "target_lang_sense_clarification": "штрафной, угловой, от ворот"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f396a241-6f29-307a-a197-05d8d484f8fa",
                    "sense_definition": "Technika w sportach walki polegająca na powaleniu przeciwnika na matę.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sport_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Judo słynie z efektownych <w>rzutów</w>, które wymagają dużej precyzji.",
                            "target_lang_translations": {
                                "ru": "Дзюдо славится эффектными <w>бросками</w>, которые требуют большой точности."
                            }
                        },
                        {
                            "text": "Zawodnik wykonał <w>rzut</w> przez biodro i wygrał walkę przed czasem.",
                            "target_lang_translations": {
                                "ru": "Спортсмен выполнил <w>бросок</w> через бедро и выиграл схватку досрочно."
                            }
                        }
                    ],
                    "synonyms": [
                        "obalenie"
                    ],
                    "common_phrases": [
                        "rzut przez biodro",
                        "rzut przez ramię"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "sporty walki"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Техника в единоборствах, заключающаяся в поваливании противника на мат."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бросок",
                                "target_lang_sense_clarification": "дзюдо, борьба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b30512c1-4950-35df-b5b0-04ef95c60130",
                    "sense_definition": "Przedstawienie figury przestrzennej lub obiektu trójwymiarowego na płaszczyźnie (np. na papierze) według określonych zasad geometrycznych.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "technical_representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na lekcji geometrii rysowaliśmy <w>rzut</w> prostopadły ostrosłupa.",
                            "target_lang_translations": {
                                "ru": "На уроке геометрии мы чертили ортогональную <w>проекцию</w> пирамиды."
                            }
                        },
                        {
                            "text": "Inżynier przygotował <w>rzut</w> boczny silnika do dokumentacji technicznej.",
                            "target_lang_translations": {
                                "ru": "Инженер подготовил боковую <w>проекцию</w> двигателя для технической документации."
                            }
                        }
                    ],
                    "synonyms": [
                        "odwzorowanie"
                    ],
                    "common_phrases": [
                        "rzut prostopadły",
                        "rzut izometryczny",
                        "rzut boczny",
                        "rzut poziomy"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "geometria, technika"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Изображение пространственной фигуры или трехмерного объекта на плоскости (например, на бумаге) по определенным геометрическим правилам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проекция",
                                "target_lang_sense_clarification": "геометрия, черчение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c7ddcab-b340-3e47-b64e-19220b4f4c1c",
                    "sense_definition": "Rysunek architektoniczny przedstawiający układ pomieszczeń budynku widziany z góry.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical_representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Architekt pokazał nam <w>rzut</w> parteru naszego przyszłego domu.",
                            "target_lang_translations": {
                                "ru": "Архитектор показал нам <w>план</w> первого этажа нашего будущего дома."
                            }
                        },
                        {
                            "text": "Na <w>rzucie</w> mieszkania widać dokładnie rozmieszczenie wszystkich ścian działowych.",
                            "target_lang_translations": {
                                "ru": "На <w>плане</w> квартиры точно видно расположение всех перегородок."
                            }
                        }
                    ],
                    "synonyms": [
                        "plan"
                    ],
                    "common_phrases": [
                        "rzut mieszkania",
                        "rzut kondygnacji",
                        "rzut parteru"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "architektura"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Архитектурный чертеж, показывающий расположение помещений здания, вид сверху."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "план",
                                "target_lang_sense_clarification": "архитектура, строительство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b9fc2c4-5182-30fb-bab8-2a0dabd171f6",
                    "sense_definition": "Szybki, gwałtowny ruch częścią ciała lub nagłe skierowanie wzroku w jakimś kierunku.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wystarczył jeden <w>rzut</w> oka, by ocenić sytuację na drodze.",
                            "target_lang_translations": {
                                "ru": "Достаточно было одного <w>взгляда</w>, чтобы оценить ситуацию на дороге."
                            }
                        },
                        {
                            "text": "Gwałtownym <w>rzutem</w> głowy odrzuciła włosy do tyłu.",
                            "target_lang_translations": {
                                "ru": "Резким <w>взмахом</w> головы она отбросила волосы назад."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rzut oka",
                        "rzut taśmy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстрое, резкое движение частью тела или внезапное направление взгляда в какую-либо сторону."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взгляд",
                                "target_lang_sense_clarification": "в выражении 'rzut oka'"
                            },
                            {
                                "target_lang_word": "взмах",
                                "target_lang_sense_clarification": "резкое движение головой или рукой"
                            }
                        ]
                    }
                }
            ]
        }
    ]

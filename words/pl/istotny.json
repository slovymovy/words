                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фактический",
                                "target_lang_sense_clarification": "реальный, основанный на фактах"
                            },
                            {
                                "target_lang_word": "действительный",
                                "target_lang_sense_clarification": "существующий в действительности"
                            }
                        ]
                    }
                }
            ]
        }

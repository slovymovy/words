    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ab70726a-60b6-38cc-acb1-390d72d07ca8",
                    "sense_definition": "Dotknąć kogoś lub czegoś bardzo lekko, delikatnie i przez krótką chwilę.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "touch_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopak nieśmiało <w>musnął</w> dłoń swojej dziewczyny podczas spaceru.",
                            "target_lang_translations": {
                                "ru": "Парень застенчиво <w>коснулся</w> руки своей девушки во время прогулки."
                            }
                        },
                        {
                            "text": "Poczułem, jak chłodny wiatr <w>musnął</w> moją twarz.",
                            "target_lang_translations": {
                                "ru": "Я почувствовал, как прохладный ветер <w>коснулся</w> моего лица."
                            }
                        }
                    ],
                    "synonyms": [
                        "dotknąć",
                        "otrzeć się"
                    ],
                    "antonyms": [
                        "uderzyć",
                        "przycisnąć"
                    ],
                    "common_phrases": [
                        "delikatnie musnąć",
                        "musnąć dłonią",
                        "musnąć policzek",
                        "wiatr musnął"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дотронуться до кого-либо или чего-либо очень легко, нежно и на короткое мгновение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коснуться",
                                "target_lang_sense_clarification": "дотронуться слегка"
                            },
                            {
                                "target_lang_word": "задеть",
                                "target_lang_sense_clarification": "слегка коснуться проходя мимо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ce11f0b-73a4-3bf2-b642-2c9e6747b081",
                    "sense_definition": "Pokryć powierzchnię cienką warstwą substancji lub wykonać delikatny ruch narzędziem, na przykład pędzlem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "application",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Malarz ostatni raz <w>musnął</w> płótno pędzlem, kończąc swoje dzieło.",
                            "target_lang_translations": {
                                "ru": "Художник в последний раз <w>коснулся</w> холста кистью, завершая свое произведение."
                            }
                        },
                        {
                            "text": "Przed wyjściem z domu Anna <w>musnęła</w> usta czerwoną szminką.",
                            "target_lang_translations": {
                                "ru": "Перед выходом из дома Анна <w>тронула</w> губы красной помадой."
                            }
                        }
                    ],
                    "synonyms": [
                        "pociągnąć",
                        "pomalować"
                    ],
                    "common_phrases": [
                        "musnąć pędzlem",
                        "musnąć usta szminką",
                        "musnąć rzęsy tuszem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрыть поверхность тонким слоем вещества или сделать легкое движение инструментом, например кистью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тронуть",
                                "target_lang_sense_clarification": "сделать легкое движение инструментом"
                            },
                            {
                                "target_lang_word": "коснуться",
                                "target_lang_sense_clarification": "прикоснуться кистью"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
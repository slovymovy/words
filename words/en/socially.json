    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ae8f904f-e415-3a5d-bbac-2cd7c21632cc",
                    "sense_definition": "In a way that relates to society, social structure, or the way people interact with each other.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is not <w>socially</w> acceptable to speak loudly in a quiet library.",
                            "target_lang_translations": {
                                "ru": "Громко разговаривать в тихой библиотеке <w>социально</w> неприемлемо."
                            }
                        },
                        {
                            "text": "He felt <w>socially</w> awkward when he realized he was underdressed for the party.",
                            "target_lang_translations": {
                                "ru": "Он почувствовал себя <w>социально</w> неловко, когда понял, что одет неподходяще для вечеринки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "socially acceptable",
                        "socially awkward",
                        "socially responsible",
                        "socially conscious"
                    ],
                    "target_lang_definitions": {
                        "ru": "Таким образом, который относится к обществу, социальной структуре или взаимодействию людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "социально",
                                "target_lang_sense_clarification": "относящийся к обществу или общественным нормам"
                            },
                            {
                                "target_lang_word": "в общественном отношении",
                                "target_lang_sense_clarification": "с точки зрения общества"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "608e30ea-6a4e-3ce1-aebc-af231b185836",
                    "sense_definition": "In a friendly manner that involves spending time with other people for enjoyment rather than for work.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I know him from work, but we do not hang out <w>socially</w>.",
                            "target_lang_translations": {
                                "ru": "Я знаю его по работе, но мы не общаемся <w>в неформальной обстановке</w>."
                            }
                        },
                        {
                            "text": "It is important to interact <w>socially</w> with friends to maintain a healthy work-life balance.",
                            "target_lang_translations": {
                                "ru": "Важно взаимодействовать с друзьями <w>ради общения</w>, чтобы поддерживать здоровый баланс между работой и личной жизнью."
                            }
                        }
                    ],
                    "synonyms": [
                        "sociably"
                    ],
                    "antonyms": [
                        "professionally"
                    ],
                    "common_phrases": [
                        "know someone socially",
                        "meet socially",
                        "interact socially"
                    ],
                    "target_lang_definitions": {
                        "ru": "В дружеской манере, подразумевающей общение с людьми ради удовольствия, а не по работе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в неформальной обстановке",
                                "target_lang_sense_clarification": "вне работы, в частной жизни"
                            },
                            {
                                "target_lang_word": "ради общения",
                                "target_lang_sense_clarification": "для удовольствия от компании"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
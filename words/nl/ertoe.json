{
    "word_family": [
        "hiertoe",
        "daartoe",
        "waartoe"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "72b9ad93-e965-32c6-b16a-bac2305cb066",
                    "sense_definition": "Vorm van het voorzetsel <w>tot</w> gecombineerd met 'er', verwijzend naar iets dat eerder genoemd is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De nieuwe regels leidden <w>ertoe</w> dat het werk sneller ging.",
                            "target_lang_translations": {
                                "ru": "Новые правила привели <w>к тому</w>, что работа пошла быстрее."
                            }
                        },
                        {
                            "text": "Hij werd <w>ertoe</w> gedwongen om zijn excuses aan te bieden.",
                            "target_lang_translations": {
                                "ru": "Его принудили <w>к тому</w>, чтобы принести извинения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ertoe leiden",
                        "ertoe dwingen",
                        "ertoe behoren"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "voornaamwoordelijk bijwoord"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Местоименное наречие, образованное от предлога 'tot' и 'er', указывающее на что-то, упомянутое ранее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "к этому",
                                "target_lang_sense_clarification": "указание на ранее упомянутый объект или ситуацию"
                            },
                            {
                                "target_lang_word": "к тому",
                                "target_lang_sense_clarification": "используется для связи с последующим придаточным предложением"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6b6c01f-ece2-3ca5-b0c4-f9ac323c1b0e",
                    "sense_definition": "Deel van de vaste uitdrukking 'ertoe doen', wat betekent dat iets belangrijk of relevant is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Geld is niet het enige dat <w>ertoe</w> doet in het leven.",
                            "target_lang_translations": {
                                "ru": "Деньги — не единственное, что <w>важно</w> в жизни."
                            }
                        },
                        {
                            "text": "Zijn mening doet <w>ertoe</w> omdat hij veel ervaring heeft.",
                            "target_lang_translations": {
                                "ru": "Его мнение <w>имеет значение</w>, так как у него большой опыт."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ertoe doen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть устойчивого выражения 'ertoe doen', означающего, что что-то важно или имеет значение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иметь значение",
                                "target_lang_sense_clarification": "в составе выражения 'ertoe doen'"
                            },
                            {
                                "target_lang_word": "быть важным",
                                "target_lang_sense_clarification": "смысловой перевод выражения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
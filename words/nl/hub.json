{
    "word_family": [
        "USB-hub"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "446b13d4-6d81-3699-aa88-0b36811daebf",
                    "sense_definition": "Een centraal knooppunt in het luchtverkeer waar passagiers en vracht samenkomen om over te stappen op vluchten naar andere bestemmingen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "transport_hub",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Schiphol is een belangrijke <w>hub</w> voor reizigers die binnen Europa vliegen.",
                            "target_lang_translations": {
                                "ru": "Схипхол — важный <w>хаб</w> для путешественников, летающих внутри Европы."
                            }
                        },
                        {
                            "text": "De luchtvaartmaatschappij gebruikt Dubai als internationale <w>hub</w> tussen Azië en Europa.",
                            "target_lang_translations": {
                                "ru": "Авиакомпания использует Дубай как международный <w>хаб</w> между Азией и Европой."
                            }
                        }
                    ],
                    "synonyms": [
                        "knooppunt",
                        "luchthavenknooppunt"
                    ],
                    "common_phrases": [
                        "internationale hub",
                        "belangrijke hub"
                    ],
                    "target_lang_definitions": {
                        "ru": "Центральный узел воздушного сообщения, где пассажиры и грузы пересаживаются на рейсы в другие пункты назначения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хаб",
                                "target_lang_sense_clarification": "транспортный узел, особенно в авиации"
                            },
                            {
                                "target_lang_word": "транспортный узел",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43617797-3a2f-3da0-9620-e05f32246092",
                    "sense_definition": "Een apparaat in een computernetwerk dat meerdere computers of apparaten met elkaar verbindt en signalen doorstuurt.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "tech_hub",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Omdat de oude <w>hub</w> het netwerk vertraagde, hebben we deze vervangen door een switch.",
                            "target_lang_translations": {
                                "ru": "Поскольку старый <w>концентратор</w> замедлял работу сети, мы заменили его на коммутатор."
                            }
                        },
                        {
                            "text": "Je kunt een <w>hub</w> gebruiken om meerdere netwerkkabels op één punt samen te laten komen.",
                            "target_lang_translations": {
                                "ru": "Вы можете использовать <w>хаб</w>, чтобы свести несколько сетевых кабелей в одну точку."
                            }
                        }
                    ],
                    "synonyms": [
                        "netwerkknooppunt"
                    ],
                    "common_phrases": [
                        "netwerk-hub"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство в компьютерной сети, которое соединяет несколько компьютеров или устройств и передает сигналы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "концентратор",
                                "target_lang_sense_clarification": "технический термин"
                            },
                            {
                                "target_lang_word": "хаб",
                                "target_lang_sense_clarification": "компьютерный термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "b65e98c9-7b5a-3cbc-b74b-52acefe2bdaf",
                    "sense_definition": "Een jongensnaam.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mijn oom heet <w>Hub</w> en hij komt uit Limburg.",
                            "target_lang_translations": {
                                "ru": "Моего дядю зовут <w>Хуб</w>, и он родом из Лимбурга."
                            }
                        },
                        {
                            "text": "We hebben <w>Hub</w> uitgenodigd voor het familiefeest.",
                            "target_lang_translations": {
                                "ru": "Мы пригласили <w>Хуба</w> на семейный праздник."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хуб",
                                "target_lang_sense_clarification": "имя собственное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25c692b4-2006-360e-8864-46c6019cdb28",
                    "sense_definition": "Een buurtschap in de gemeente Peel en Maas in de Nederlandse provincie Limburg.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Tijdens onze fietstocht kwamen we door het kleine plaatsje <w>Hub</w>.",
                            "target_lang_translations": {
                                "ru": "Во время нашей велопрогулки мы проезжали через маленькое местечко <w>Хуб</w>."
                            }
                        },
                        {
                            "text": "De familie woont al jaren in <w>Hub</w> in Limburg.",
                            "target_lang_translations": {
                                "ru": "Семья уже много лет живет в <w>Хубе</w> в Лимбурге."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Хутор в общине Пел-эн-Мас в нидерландской провинции Лимбург."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хуб",
                                "target_lang_sense_clarification": "географическое название"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
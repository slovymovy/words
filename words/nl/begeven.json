                {
                    "sense_id": "cd521440-284f-32b5-8cff-2cd60b93db93",
                    "sense_definition": "Zich naar een bepaalde plaats verplaatsen of ergens naartoe gaan, vaak gebruikt in een formele context.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gasten <w>begaven</w> zich na het diner naar de balzaal voor het feest.",
                            "target_lang_translations": {
                                "ru": "После ужина гости <w>направились</w> в бальный зал на праздник.",
                                "en": "After dinner, the guests <w>proceeded</w> to the ballroom for the party."
                            }
                        },
                        {
                            "text": "Hij <w>begaf</w> zich onmiddellijk naar het ziekenhuis toen hij het nieuws hoorde.",
                            "target_lang_translations": {
                                "ru": "Услышав новости, он немедленно <w>отправился</w> в больницу.",
                                "en": "He immediately <w>made his way</w> to the hospital when he heard the news."
                            }
                        }
                    ],
                    "synonyms": [
                        "gaan",
                        "vertrekken"
                    ],
                    "antonyms": [
                        "blijven"
                    ],
                    "common_phrases": [
                        "zich naar huis begeven",
                        "zich ter plaatse begeven"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "wederkerend (zich begeven)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Направляться или идти в определенное место, часто используется в формальном контексте.",
                        "en": "To move to a specific place or go somewhere, often used in a formal context."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "направляться",
                                "target_lang_sense_clarification": "двигаться в определенном направлении"
                            },
                            {
                                "target_lang_word": "отправляться",
                                "target_lang_sense_clarification": "начинать путь куда-либо"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "proceed",
                                "target_lang_sense_clarification": "to go to a place, often formally"
                            },
                            {
                                "target_lang_word": "make one's way",
                                "target_lang_sense_clarification": "to go or travel towards a destination"
                            },
                            {
                                "target_lang_word": "go",
                                "target_lang_sense_clarification": "general movement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1884e268-9175-3da6-ad7f-dfd432e71320",
                    "sense_definition": "Kapotgaan, instorten of ophouden met functioneren omdat de druk of belasting te groot wordt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "failure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude houten brug <w>begaf</w> het onder het zware gewicht van de vrachtwagen.",
                            "target_lang_translations": {
                                "ru": "Старый деревянный мост <w>рухнул</w> под тяжестью грузовика.",
                                "en": "The old wooden bridge <w>gave way</w> under the heavy weight of the truck."
                            }
                        },
                        {
                            "text": "Mijn stem <w>begaf</w> het bijna van de zenuwen tijdens de belangrijke presentatie.",
                            "target_lang_translations": {
                                "ru": "Мой голос почти <w>сорвался</w> от нервов во время важной презентации.",
                                "en": "My voice almost <w>failed</w> from nerves during the important presentation."
                            }
                        }
                    ],
                    "synonyms": [

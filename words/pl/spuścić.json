            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4d8e0f91-6a25-34de-8e60-fab333691608",
                    "sense_definition": "Sprawić, że płyn lub gaz wypłynie ze zbiornika; opróżnić coś z zawartości.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "drain_release",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <w>spuścić</w> wodę z wanny, zanim zaczniesz ją myć.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>спустить</w> воду из ванны, прежде чем начнешь её мыть."
                            }
                        },
                        {
                            "text": "Mechanik musiał <w>spuścić</w> powietrze z opony, aby ją naprawić.",
                            "target_lang_translations": {
                                "ru": "Механик должен был <w>спустить</w> воздух из шины, чтобы починить её."
                            }
                        }
                    ],
                    "synonyms": [
                        "opuścić",
                        "opuścić się"
                    ],
                    "antonyms": [
                        "napełnić"
                    ],
                    "common_phrases": [
                        "spuścić wodę",
                        "spuścić powietrze",
                        "spuścić olej"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставить жидкость или газ вытечь из резервуара; опорожнить что-либо от содержимого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спустить",
                                "target_lang_sense_clarification": "воду, воздух, шину"
                            },
                            {
                                "target_lang_word": "слить",
                                "target_lang_sense_clarification": "жидкость, масло"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "353a9e55-29c1-3348-822d-08fd617e10d1",
                    "sense_definition": "Przemieścić coś lub kogoś w dół; skierować coś ku dołowi (np. głowę, wzrok).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "lower_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zawstydzony chłopiec <w>spuścił</w> głowę i przestał się odzywać.",
                            "target_lang_translations": {
                                "ru": "Пристыженный мальчик <w>опустил</w> голову и перестал разговаривать."
                            }
                        },
                        {
                            "text": "Proszę <w>spuścić</w> rolety, ponieważ słońce świeci mi prosto w oczy.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>опустите</w> жалюзи, так как солнце светит мне прямо в глаза."
                            }
                        }
                    ],
                    "synonyms": [
                        "opuścić"
                    ],
                    "antonyms": [
                        "podnieść"
                    ],
                    "common_phrases": [
                        "spuścić głowę",
                        "spuścić wzrok",
                        "spuścić rolety",
                        "spuścić na dół"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переместить что-либо или кого-либо вниз; направить что-либо книзу (например, голову, взгляд)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опустить",
                                "target_lang_sense_clarification": "голову, глаза, жалюзи"
                            },
                            {
                                "target_lang_word": "спустить",
                                "target_lang_sense_clarification": "переместить вниз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b909fd28-44d8-3fb5-85c6-afd951add3ef",
                    "sense_definition": "Uwolnić psa z uwięzi, smyczy lub łańcucha, często w celu obrony terenu lub ataku.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "unleash",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Właściciel posesji zagroził, że może <w>spuścić</w> psy na intruzów.",
                            "target_lang_translations": {
                                "ru": "Владелец участка пригрозил, что может <w>спустить</w> собак на нарушителей."
                            }
                        },
                        {
                            "text": "W tym parku nie wolno <w>spuścić</w> psa ze smyczy.",
                            "target_lang_translations": {
                                "ru": "В этом парке нельзя <w>спускать</w> собаку с поводка."
                            }
                        }
                    ],
                    "antonyms": [
                        "uwiązać",
                        "przywiązać"
                    ],
                    "common_phrases": [
                        "spuścić psa",
                        "spuścić ze smyczy",
                        "spuścić z łańcucha"
                    ],
                    "target_lang_definitions": {
                        "ru": "Освободить собаку от привязи, поводка или цепи, часто для защиты территории или нападения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спустить",
                                "target_lang_sense_clarification": "с поводка, с цепи, на кого-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93dbc089-04fb-3eef-8c93-391917aac2a4",
                    "sense_definition": "Doprowadzić do wytrysku nasienia; szczytować.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sexual_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wulgarnie mówi się, że chłopak <w>spuścił</w> się zbyt wcześnie.",
                            "target_lang_translations": {
                                "ru": "Грубо говорят, что парень <w>кончил</w> слишком рано."
                            }
                        },
                        {
                            "text": "To potoczne określenie oznacza, że mężczyzna musiał się <w>spuścić</w>.",
                            "target_lang_translations": {
                                "ru": "Это сленговое выражение означает, что мужчине нужно было <w>кончить</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dojść",
                        "wytrysnąć"
                    ],
                    "common_phrases": [
                        "spuścić się"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "vulgar"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Довести до семяизвержения; испытать оргазм."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кончить",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "спустить",
                                "target_lang_sense_clarification": "слэнг"
                            }
                        ]
                    }
                }
            ]
        }

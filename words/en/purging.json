            "senses": [
                {
                    "sense_id": "e9137b95-5e10-3c72-a9d2-bb3db07899ee",
                    "sense_definition": "The act of eliminating contamination or removing undesirable people from a group or organization.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The political <purging> of opposition leaders caused international outrage.",
                            "target_lang_translations": {
                                "ru": "Политическая <чистка> лидеров оппозиции вызвала международное возмущение."
                            }
                        },
                        {
                            "text": "Regular <purging> of unnecessary documents keeps the archive organized.",
                            "target_lang_translations": {
                                "ru": "Регулярное <удаление> ненужных документов поддерживает порядок в архиве."
                            }

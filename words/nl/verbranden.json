                    "synonyms": [
                        "discrediteren",
                        "compromitteren",
                        "verknoeien"
                    ],
                    "common_phrases": [
                        "politiek verbrand zijn",
                        "zichzelf verbranden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрушить чью-то репутацию или карьеру, лишив доверия"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дискредитировать",
                                "target_lang_sense_clarification": "подрывать доверие"
                            },
                            {
                                "target_lang_word": "губить",
                                "target_lang_sense_clarification": "репутацию или карьеру"
                            }
                        ]
                    }
                }
            ]
        }

            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1be12c92-d53b-3686-809a-3eeb45d7ff26",
                    "sense_definition": "Принадлежащий разбойнику, относящийся к нему или состоящий из разбойников.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "relational",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В густом лесу скрывалась опасная <w>разбойничья</w> шайка.",
                            "target_lang_translations": {
                                "en": "A dangerous <w>robber</w> gang was hiding in the dense forest."
                            }
                        },
                        {
                            "text": "Путники случайно набрели на старый <w>разбойничий</w> лагерь.",
                            "target_lang_translations": {
                                "en": "The travelers accidentally stumbled upon an old <w>bandit</w> camp."
                            }
                        }
                    ],
                    "synonyms": [
                        "бандитский",
                        "воровской"
                    ],
                    "common_phrases": [
                        "разбойничья шайка",
                        "разбойничий притон",
                        "разбойничий набег",
                        "разбойничье гнездо"
                    ],
                    "target_lang_definitions": {
                        "en": "Belonging to a robber, relating to him, or consisting of robbers."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "robber",
                                "target_lang_sense_clarification": "attributive use; relating to or belonging to robbers"
                            },
                            {
                                "target_lang_word": "bandit",
                                "target_lang_sense_clarification": "characteristic of or belonging to bandits"
                            },
                            {
                                "target_lang_word": "brigand",
                                "target_lang_sense_clarification": "pertaining to brigands or highwaymen"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a71b2b5-9c18-3219-aa18-5b4b76976974",
                    "sense_definition": "Хищнический, грабительский; выражающий дикую удаль, грубость или жестокость.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта компания вела <w>разбойничий</w> лов рыбы в заповедных водах.",
                            "target_lang_translations": {
                                "en": "This company conducted <w>predatory</w> fishing in protected waters."
                            }
                        },
                        {
                            "text": "В его глазах светился какой-то дикий, <w>разбойничий</w> огонь.",
                            "target_lang_translations": {
                                "en": "A kind of wild, <w>bandit</w> fire shone in his eyes."
                            }
                        }
                    ],
                    "synonyms": [
                        "хищнический",
                        "грабительский",
                        "пиратский",
                        "варварский"
                    ],
                    "antonyms": [
                        "мирный"
                    ],
                    "common_phrases": [
                        "разбойничий свист",
                        "разбойничий нрав",
                        "разбойничий захват"
                    ],
                    "target_lang_definitions": {
                        "en": "Predatory, plundering; expressing wild daring, rudeness, or cruelty."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "predatory",
                                "target_lang_sense_clarification": "involving plunder, exploitation, or destruction"
                            },
                            {
                                "target_lang_word": "rapacious",
                                "target_lang_sense_clarification": "aggressively greedy or grasping"
                            },
                            {
                                "target_lang_word": "piratical",

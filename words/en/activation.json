    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "07cbc158-ea21-36a4-9537-9794f197f120",
                    "sense_definition": "The action or process of making something active, effective, or operative.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The automatic <w>activation</w> of the emergency lights helped guide people to safety.",
                            "target_lang_translations": {
                                "ru": "Автоматическая <w>активация</w> аварийного освещения помогла вывести людей в безопасное место."
                            }
                        },
                        {
                            "text": "Voice <w>activation</w> allows users to operate the device without using their hands.",
                            "target_lang_translations": {
                                "ru": "Голосовая <w>активация</w> позволяет пользователям управлять устройством без помощи рук."
                            }
                        }
                    ],
                    "synonyms": [
                        "triggering",
                        "stimulation",
                        "start-up"
                    ],
                    "antonyms": [
                        "deactivation"
                    ],
                    "common_phrases": [
                        "voice activation",
                        "automatic activation",
                        "system activation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или процесс приведения чего-либо в активное, рабочее состояние."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "активация",
                                "target_lang_sense_clarification": "приведение в действие"
                            },
                            {
                                "target_lang_word": "включение",
                                "target_lang_sense_clarification": "запуск устройства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87cf6a98-5cc9-355f-b7e1-1e24f0a55eda",
                    "sense_definition": "The execution of marketing campaigns or events designed to engage consumers with a brand.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company planned a major brand <w>activation</w> at the summer music festival.",
                            "target_lang_translations": {
                                "ru": "Компания запланировала крупную <w>активацию</w> бренда на летнем музыкальном фестивале."
                            }
                        },
                        {
                            "text": "Effective shopper <w>activation</w> can turn casual browsers into loyal customers.",
                            "target_lang_translations": {
                                "ru": "Эффективная <w>активация</w> покупателей может превратить случайных посетителей в лояльных клиентов."
                            }
                        }
                    ],
                    "synonyms": [
                        "promotion"
                    ],
                    "common_phrases": [
                        "brand activation",
                        "shopper activation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Реализация маркетинговых кампаний или мероприятий, направленных на вовлечение потребителей во взаимодействие с брендом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "активация",
                                "target_lang_sense_clarification": "маркетинг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c933490-4164-3bea-931c-18eb5af352b6",
                    "sense_definition": "The process by which molecules are made able to react chemically.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Enzyme <w>activation</w> is a critical step in the digestive process.",
                            "target_lang_translations": {
                                "ru": "<w>Активация</w> ферментов — критически важный этап в процессе пищеварения."
                            }
                        },
                        {
                            "text": "The chemist studied the energy required for the <w>activation</w> of the compound.",
                            "target_lang_translations": {
                                "ru": "Химик изучал энергию, необходимую для <w>активации</w> соединения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "activation energy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс, в результате которого молекулы приобретают способность вступать в химическую реакцию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "активация",
                                "target_lang_sense_clarification": "химия/биология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79737214-f7e9-3226-a509-1a1f8823f729",
                    "sense_definition": "The process of creating a radioisotope by bombarding a stable element with neutrons or protons.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Neutron <w>activation</w> analysis helps scientists determine the composition of a sample.",
                            "target_lang_translations": {
                                "ru": "Анализ методом нейтронной <w>активации</w> помогает ученым определить состав образца."
                            }
                        },
                        {
                            "text": "The <w>activation</w> of the material occurred inside the nuclear reactor.",
                            "target_lang_translations": {
                                "ru": "<w>Активация</w> материала произошла внутри ядерного реактора."
                            }
                        }
                    ],
                    "common_phrases": [
                        "neutron activation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс создания радиоизотопа путем облучения стабильного элемента нейтронами или протонами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "активация",
                                "target_lang_sense_clarification": "ядерная физика"
                            }
                        ]
                    }
                }
            ]
        }
    ]

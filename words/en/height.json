{
    "word_family": [
        "high",
        "heighten",
        "highly",
        "highness"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9f9f5a4c-0f82-348d-acb3-d974b7306020",
                    "sense_definition": "The vertical distance from the ground to the highest part of a standing person or animal.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please state your age, weight, and <w>height</w> on the form.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, укажите в анкете свой возраст, вес и <w>рост</w>."
                            }
                        },
                        {
                            "text": "She is exactly the same <w>height</w> as her brother.",
                            "target_lang_translations": {
                                "ru": "Она точно такого же <w>роста</w>, как и её брат."
                            }
                        }
                    ],
                    "synonyms": [
                        "stature"
                    ],
                    "common_phrases": [
                        "average height",
                        "same height",
                        "medium height"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вертикальное расстояние от земли до самой высокой части стоящего человека или животного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рост",
                                "target_lang_sense_clarification": "человека или животного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c138b7fc-6ae2-336d-94f5-b2aafd02e6c3",
                    "sense_definition": "The measurement of how tall something is from its base to its top.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You need to measure the <w>height</w> of the window before buying curtains.",
                            "target_lang_translations": {
                                "ru": "Вам нужно измерить <w>высоту</w> окна перед покупкой штор."
                            }
                        },
                        {
                            "text": "The <w>height</w> of the new building exceeds two hundred meters.",
                            "target_lang_translations": {
                                "ru": "<w>Высота</w> нового здания превышает двести метров."
                            }
                        }
                    ],
                    "antonyms": [
                        "depth"
                    ],
                    "common_phrases": [
                        "height and width",
                        "base times height"
                    ],
                    "target_lang_definitions": {
                        "ru": "Измерение того, насколько высок предмет от основания до верха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высота",
                                "target_lang_sense_clarification": "предмета, здания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31665491-4b5d-383d-90e5-d4150c50d824",
                    "sense_definition": "The distance of something above the ground or sea level.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The aircraft reached a <w>height</w> of 30,000 feet shortly after takeoff.",
                            "target_lang_translations": {
                                "ru": "Самолет набрал <w>высоту</w> 30 000 футов вскоре после взлета."
                            }
                        },
                        {
                            "text": "He refused to climb the ladder because he has a fear of <w>heights</w>.",
                            "target_lang_translations": {
                                "ru": "Он отказался лезть на лестницу, потому что боится <w>высоты</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "altitude"
                    ],
                    "common_phrases": [
                        "fear of heights",
                        "great height",
                        "from a height"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расстояние от объекта до земли или уровня моря."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "высота",
                                "target_lang_sense_clarification": "над уровнем земли/моря"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "28bd38f8-7d4b-367a-a31e-3fb745300112",
                    "sense_definition": "The point when something is at its best, strongest, or most active.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_peak",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to retire from professional sports at the <w>height</w> of her career.",
                            "target_lang_translations": {
                                "ru": "Она решила уйти из профессионального спорта на <w>пике</w> своей карьеры."
                            }
                        },
                        {
                            "text": "It was the <w>height</w> of summer, and the days were long and hot.",
                            "target_lang_translations": {
                                "ru": "Был самый <w>разгар</w> лета, и дни были длинными и жаркими."
                            }
                        }
                    ],
                    "synonyms": [
                        "zenith",
                        "pinnacle",
                        "summit",
                        "climax",
                        "apex",
                        "high point"
                    ],
                    "common_phrases": [
                        "height of fashion",
                        "height of summer",
                        "height of success"
                    ],
                    "target_lang_definitions": {
                        "ru": "Момент, когда что-либо находится в наилучшем, сильнейшем или наиболее активном состоянии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разгар",
                                "target_lang_sense_clarification": "сезона, события"
                            },
                            {
                                "target_lang_word": "пик",
                                "target_lang_sense_clarification": "карьеры, активности"
                            },
                            {
                                "target_lang_word": "вершина",
                                "target_lang_sense_clarification": "успеха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af1b2dcb-c7f0-37d0-982d-6e3b53ed82e6",
                    "sense_definition": "A high place or area of land, such as a hill or cliff.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The castle was built on a rocky <w>height</w> overlooking the valley.",
                            "target_lang_translations": {
                                "ru": "Замок был построен на скалистой <w>возвышенности</w>, возвышающейся над долиной."
                            }
                        },
                        {
                            "text": "They climbed the <w>heights</w> to get a better view of the ocean.",
                            "target_lang_translations": {
                                "ru": "Они поднялись на <w>возвышенности</w>, чтобы лучше рассмотреть океан."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокое место или участок земли, например, холм или утес."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возвышенность",
                                "target_lang_sense_clarification": "холм, утес"
                            },
                            {
                                "target_lang_word": "высота",
                                "target_lang_sense_clarification": "географическая точка"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "89f96bf4-6019-303e-a553-6c28e78af493",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Heights</w> was known for his contributions to the local library.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Хайтс</w> был известен своим вкладом в местную библиотеку."
                            }
                        },
                        {
                            "text": "We looked up the name <w>Heights</w> in the telephone directory.",
                            "target_lang_translations": {
                                "ru": "Мы искали фамилию <w>Хайтс</w> в телефонном справочнике."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Хайтс",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
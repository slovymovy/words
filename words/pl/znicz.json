    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bbe5f03a-db13-358f-8f98-6db0db2060cf",
                    "sense_definition": "Świeca w naczyniu, najczęściej szklanym lub plastikowym, zapalana na grobach lub pod pomnikami w celu upamiętnienia zmarłych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "candle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dniu Wszystkich Świętych na cmentarzach płoną tysiące <w>zniczy</w>.",
                            "target_lang_translations": {
                                "ru": "В День Всех Святых на кладбищах горят тысячи <w>лампад</w>."
                            }
                        },
                        {
                            "text": "Muszę kupić nowy wkład, bo ten <w>znicz</w> na grobie babci już zgasł.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новый вкладыш, потому что эта <w>лампада</w> на могиле бабушки уже погасла."
                            }
                        }
                    ],
                    "synonyms": [
                        "lampka nagrobkowa"
                    ],
                    "common_phrases": [
                        "zapalić znicz",
                        "znicz na grobie",
                        "kupić znicze",
                        "wkład do znicza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свеча в сосуде (обычно стеклянном или пластиковом), которую зажигают на могилах или у памятников в память об умерших."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лампада",
                                "target_lang_sense_clarification": "намогильная"
                            },
                            {
                                "target_lang_word": "поминальная свеча",
                                "target_lang_sense_clarification": "свеча памяти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a96866ad-6ab7-3dc2-b268-a8b6c4680bdf",
                    "sense_definition": "Symboliczny ogień, który płonie stale w miejscu pamięci narodowej lub podczas trwania ważnych wydarzeń sportowych.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "flame",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znany sportowiec uroczyście zapalił <w>znicz</w> olimpijski na stadionie.",
                            "target_lang_translations": {
                                "ru": "Известный спортсмен торжественно зажег олимпийский <w>огонь</w> на стадионе."
                            }
                        },
                        {
                            "text": "Przed pomnikiem nieznanego żołnierza płonie wieczny <w>znicz</w>.",
                            "target_lang_translations": {
                                "ru": "Перед памятником Неизвестному солдату горит <w>вечный огонь</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "znicz olimpijski",
                        "wieczny znicz",
                        "zapalenie znicza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Символический огонь, который постоянно горит в местах национальной памяти или во время важных спортивных событий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вечный огонь",
                                "target_lang_sense_clarification": "у памятников"
                            },
                            {
                                "target_lang_word": "огонь",
                                "target_lang_sense_clarification": "символический, олимпийский"
                            },
                            {
                                "target_lang_word": "факел",
                                "target_lang_sense_clarification": "переносной источник огня"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3cd58355-6f5a-380c-b35c-2407f17aaac4",
                    "sense_definition": "Gegevens of informatie in een computer of systeem typen of vastleggen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "input_data",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet je gebruikersnaam en wachtwoord correct <w>invoeren</w>.",
                            "target_lang_translations": {
                                "en": "You must <w>enter</w> your username and password correctly.",
                                "ru": "Ты должен правильно <w>ввести</w> свое имя пользователя и пароль."
                            }
                        },
                        {
                            "text": "De assistent is bezig met het <w>invoeren</w> van de patiëntgegevens.",
                            "target_lang_translations": {
                                "en": "The assistant is busy <w>entering</w> the patient data.",
                                "ru": "Ассистент занят <w>вводом</w> данных пациента."
                            }
                        }
                    ],
                    "synonyms": [
                        "inbrengen"
                    ],
                    "common_phrases": [
                        "gegevens invoeren",
                        "een code invoeren",
                        "een wachtwoord invoeren"
                    ],
                    "target_lang_definitions": {
                        "en": "To type or record data or information into a computer or system.",
                        "ru": "Вводить данные или информацию в компьютер или систему."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "enter",
                                "target_lang_sense_clarification": "data, passwords, etc."
                            },
                            {
                                "target_lang_word": "input",
                                "target_lang_sense_clarification": "data processing"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вводить",
                                "target_lang_sense_clarification": "данные, пароль"
                            },
                            {
                                "target_lang_word": "вносить",
                                "target_lang_sense_clarification": "информацию в список или базу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b9e3ef0e-10b7-358c-8586-096ca6291c45",
                    "sense_definition": "Iets nieuws, zoals een wet, regel of systeem, in gebruik nemen of introduceren.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "introduce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De regering wil volgend jaar een nieuwe belastingregel <w>invoeren</w>.",
                            "target_lang_translations": {
                                "en": "The government wants to <w>introduce</w> a new tax rule next year.",
                                "ru": "Правительство хочет <w>ввести</w> новое налоговое правило в следующем году."
                            }
                        },
                        {
                            "text": "De school gaat strengere regels <w>invoeren</w> voor het gebruik van mobiele telefoons.",
                            "target_lang_translations": {
                                "en": "The school is going to <w>introduce</w> stricter rules for the use of mobile phones.",
                                "ru": "Школа собирается <w>ввести</w> более строгие правила использования мобильных телефонов."
                            }
                        }
                    ],
                    "synonyms": [
                        "introduceren",
                        "inleiden"
                    ],
                    "antonyms": [
                        "afschaffen"
                    ],
                    "common_phrases": [
                        "een wet invoeren",
                        "een systeem invoeren",
                        "maatregelen invoeren"
                    ],
                    "target_lang_definitions": {
                        "en": "To put something new, such as a law, rule, or system, into use or practice.",
                        "ru": "Вводить в действие или представлять что-то новое, например, закон, правило или систему."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "introduce",
                                "target_lang_sense_clarification": "new measures, laws, systems"
                            },
                            {
                                "target_lang_word": "implement",
                                "target_lang_sense_clarification": "put into effect"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вводить",
                                "target_lang_sense_clarification": "закон, правило"
                            },
                            {
                                "target_lang_word": "внедрять",
                                "target_lang_sense_clarification": "систему, инновации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71b63c57-c4cc-3f4c-b837-d7a36f995d71",
                    "sense_definition": "Goederen of producten uit het buitenland naar het eigen land halen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "trade",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nederland moet veel grondstoffen <w>invoeren</w> voor de industrie.",
                            "target_lang_translations": {
                                "en": "The Netherlands has to <w>import</w> many raw materials for the industry.",
                                "ru": "Нидерланды должны <w>импортировать</w> много сырья для промышленности."
                            }
                        },
                        {
                            "text": "Het bedrijf <w>voert</w> al jaren exclusieve wijnen <w>in</w> uit Frankrijk.",
                            "target_lang_translations": {
                                "en": "The company has been <w>importing</w> exclusive wines from France for years.",
                                "ru": "Компания уже много лет <w>ввозит</w> эксклюзивные вина из Франции."
                            }
                        }
                    ],
                    "synonyms": [
                        "importeren"
                    ],
                    "antonyms": [
                        "uitvoeren",
                        "exporteren"
                    ],
                    "common_phrases": [
                        "goederen invoeren",
                        "producten invoeren"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring goods or products from abroad into one's own country.",
                        "ru": "Ввозить товары или продукты из-за границы в свою страну."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "import",
                                "target_lang_sense_clarification": "trade, goods"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "импортировать",
                                "target_lang_sense_clarification": "товары"
                            },
                            {
                                "target_lang_word": "ввозить",
                                "target_lang_sense_clarification": "продукцию"
                            }
                        ]
                    }
                },
                {

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ba1406d2-4c3b-3668-92e5-23f7bab1437b",
                    "sense_definition": "Niewielki, naturalny lub sztuczny zbiornik wodny, mniejszy od jeziora, często wykorzystywany do hodowli ryb.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "water_bodies",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zimą ten płytki <w>staw</w> całkowicie zamarza, tworząc naturalne lodowisko.",
                            "target_lang_translations": {
                                "ru": "Зимой этот мелкий <w>пруд</w> полностью замерзает, образуя естественный каток."
                            }
                        },
                        {
                            "text": "Mój dziadek ma za domem mały <w>staw</w> rybny, w którym hoduje karpie.",
                            "target_lang_translations": {
                                "ru": "У моего дедушки за домом есть маленький рыбный <w>пруд</w>, в котором он разводит карпов."
                            }
                        }
                    ],
                    "synonyms": [
                        "sadzawka",
                        "jeziorko",
                        "zbiornik",
                        "reg. śl. rybnik"
                    ],
                    "common_phrases": [
                        "staw rybny",
                        "nad stawem",
                        "wpaść do stawu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой естественный или искусственный водоем, меньше озера, часто используемый для разведения рыбы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пруд",
                                "target_lang_sense_clarification": "водоём"
                            },
                            {
                                "target_lang_word": "ставок",
                                "target_lang_sense_clarification": "региональное/разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad957b97-11db-33b5-9a8d-34f604190832",
                    "sense_definition": "Ruchome połączenie kości w organizmie, umożliwiające wykonywanie ruchów.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sportowiec musiał przerwać trening z powodu silnego bólu w <w>stawie</w> kolanowym.",
                            "target_lang_translations": {
                                "ru": "Спортсмен был вынужден прервать тренировку из-за сильной боли в коленном <w>суставе</w>."
                            }
                        },
                        {
                            "text": "Lekarz zalecił pacjentowi ćwiczenia, które wzmacniają <w>stawy</w> i poprawiają ich ruchomość.",
                            "target_lang_translations": {
                                "ru": "Врач рекомендовал пациенту упражнения, которые укрепляют <w>суставы</w> и улучшают их подвижность."
                            }
                        }
                    ],
                    "common_phrases": [
                        "staw kolanowy",
                        "staw łokciowy",
                        "staw skokowy",
                        "zapalenie stawów",
                        "ból stawów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвижное соединение костей в организме, обеспечивающее возможность движения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сустав",
                                "target_lang_sense_clarification": "анатомия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4f0fd5d-ef14-31d6-8765-72a1add64741",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <w>stawa</w> (znak nawigacyjny).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wzdłuż wybrzeża nie było widać żadnych <w>staw</w>, co utrudniało nawigację.",
                            "target_lang_translations": {
                                "ru": "Вдоль побережья не было видно никаких <w>створных знаков</w>, что затрудняло навигацию."
                            }
                        },
                        {
                            "text": "Konserwacja tych starych <w>staw</w> wymaga specjalistycznego sprzętu alpinistycznego.",
                            "target_lang_translations": {
                                "ru": "Обслуживание этих старых <w>створных знаков</w> требует специального альпинистского снаряжения."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural of stawa"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа существительного stawa (навигационный знак)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "створный знак",
                                "target_lang_sense_clarification": "навигация"
                            },
                            {
                                "target_lang_word": "веха",
                                "target_lang_sense_clarification": "морской термин"
                            }
                        ]
                    }
                }
            ]
        },

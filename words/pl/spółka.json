                            {
                                "target_lang_word": "общество",
                                "target_lang_sense_clarification": "организационно-правовая форма"
                            },
                            {
                                "target_lang_word": "фирма",
                                "target_lang_sense_clarification": "предприятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "edc8390f-0b33-367f-aedb-2b113cdbcfe9",
                    "sense_definition": "Porozumienie lub układ między ludźmi w celu wspólnego działania, często używane w kontekście robienia czegoś razem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zrobili to do <w>spółki</w> z sąsiadami, aby szybciej skończyć pracę.",
                            "target_lang_translations": {
                                "ru": "Они сделали это <w>сообща</w> с соседями, чтобы быстрее закончить работу."
                            }
                        },
                        {
                            "text": "Nie chcę mieć z nim żadnej <w>spółki</w> w tej podejrzanej sprawie.",
                            "target_lang_translations": {
                                "ru": "Я не хочу иметь с ним никаких <w>общих дел</w> в этом подозрительном деле."
                            }
                        }
                    ],
                    "common_phrases": [
                        "do spółki",
                        "w spółce"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соглашение или договоренность между людьми о совместных действиях, часто используется в контексте совместной работы."
                    },
                    "translations": {

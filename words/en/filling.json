                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "начинка",
                                "target_lang_sense_clarification": "содержимое пирога, конфет и т.п."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f2c1d35-6938-3643-af74-1ee6b0db4814",
                    "sense_definition": "A small amount of material, such as metal or plastic, used by a dentist to fill a hole in a tooth.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The dentist told me I need a <w>filling</w> in my back tooth.",
                            "target_lang_translations": {
                                "ru": "Стоматолог сказал, что мне нужна <w>пломба</w> в заднем зубе."
                            }
                        },
                        {
                            "text": "My temporary <w>filling</w> fell out while I was eating.",
                            "target_lang_translations": {
                                "ru": "Моя временная <w>пломба</w> выпала, пока я ел."
                            }
                        }
                    ],
                    "synonyms": [
                        "stopping"
                    ],
                    "common_phrases": [
                        "temporary filling",
                        "loose filling",
                        "gold filling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшое количество материала (металла или пластика), используемое стоматологом для закрытия полости в зубе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пломба",
                                "target_lang_sense_clarification": "в стоматологии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0da2139e-f9ff-3f4f-8924-64ac3d61177a",
                    "sense_definition": "Any material used to stuff or fill an object, such as a cushion or toy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>filling</w> of this pillow is made of soft feathers.",
                            "target_lang_translations": {
                                "ru": "<w>Наполнитель</w> этой подушки сделан из мягких перьев."
                            }
                        },
                        {
                            "text": "The old teddy bear had lost some of its <w>filling</w>.",
                            "target_lang_translations": {
                                "ru": "Старый плюшевый мишка потерял часть своей <w>набивки</w>."

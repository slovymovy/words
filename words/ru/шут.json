                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used in colloquial speech as a substitute for words like \"devil\" or \"hell\" in set expressions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "devil",
                                "target_lang_sense_clarification": "used in expressions like 'the devil knows'"
                            },
                            {
                                "target_lang_word": "heck",
                                "target_lang_sense_clarification": "mild euphemism in expressions"
                            },
                            {
                                "target_lang_word": "hell",
                                "target_lang_sense_clarification": "used in expressions like 'to hell with'"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "428e60d6-40c7-3d8c-82aa-db1652641832",
                    "sense_definition": "Uroczysta lub oficjalna wypowiedź skierowana do zgromadzonych osób z jakiejś okazji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "speech_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor szkoły wygłosił długą <w>przemowę</w> na rozpoczęcie roku szkolnego.",
                            "target_lang_translations": {
                                "ru": "Директор школы произнес длинную <w>речь</w> в начале учебного года."
                            }
                        },
                        {
                            "text": "Muszę przygotować krótką <w>przemowę</w> z okazji urodzin mojego dziadka.",
                            "target_lang_translations": {
                                "ru": "Мне нужно подготовить короткую <w>речь</w> по случаю дня рождения моего дедушки."
                            }
                        }
                    ],
                    "synonyms": [
                        "przemówienie",
                        "mowa"
                    ],
                    "common_phrases": [
                        "wygłosić przemowę",
                        "krótka przemowa",
                        "uroczysta przemowa",
                        "przygotować przemowę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Торжественное или официальное устное выступление перед аудиторией по определенному случаю."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "речь",
                                "target_lang_sense_clarification": "публичное выступление"
                            },
                            {
                                "target_lang_word": "выступление",
                                "target_lang_sense_clarification": "официальное обращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "394bba6e-9cf9-399d-b964-3e2fce842ca8",
                    "sense_definition": "Zbyt długa, często nudna lub moralizatorska wypowiedź, której słuchanie jest męczące.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "speech_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zamiast konkretnej odpowiedzi usłyszeliśmy kolejną polityczną <w>przemowę</w> bez znaczenia.",
                            "target_lang_translations": {
                                "ru": "Вместо конкретного ответа мы услышали очередную бессмысленную политическую <w>речь</w>."
                            }
                        },
                        {
                            "text": "Oszczędź mi tej umoralniającej <w>przemowy</w> i powiedz po prostu, o co ci chodzi.",
                            "target_lang_translations": {
                                "ru": "Избавь меня от этой нравоучительной <w>речи</w> и просто скажи, чего ты хочешь."
                            }
                        }
                    ],
                    "synonyms": [
                        "kazanie"
                    ],
                    "common_phrases": [
                        "nudna przemowa",
                        "długa przemowa",
                        "walnąć przemowę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слишком длинное, часто скучное или нравоучительное высказывание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нотация",
                                "target_lang_sense_clarification": "нравоучение"
                            },
                            {
                                "target_lang_word": "тирада",
                                "target_lang_sense_clarification": "длинная, утомительная речь"
                            },
                            {
                                "target_lang_word": "проповедь",
                                "target_lang_sense_clarification": "перен. скучное наставление"
                            }
                        ]
                    }
                }

{
    "word_family": [
        "openen",
        "opening",
        "openheid",
        "openlijk"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a89d7b1c-9c2d-30d8-9b40-1cdb202dbc39",
                    "sense_definition": "Niet gesloten; zo dat er doorgang mogelijk is of de inhoud bereikbaar is.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De deur van de winkel is al <w>open</w>.",
                            "target_lang_translations": {
                                "ru": "Дверь магазина уже <w>открыта</w>."
                            }
                        },
                        {
                            "text": "Kun je het raam even <w>open</w> zetten voor wat frisse lucht?",
                            "target_lang_translations": {
                                "ru": "Можешь ненадолго <w>открыть</w> окно, чтобы впустить свежий воздух?"
                            }
                        }
                    ],
                    "antonyms": [
                        "dicht",
                        "gesloten",
                        "toe"
                    ],
                    "common_phrases": [
                        "open en bloot",
                        "de deur open doen",
                        "het raam open zetten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не закрытый; в состоянии, допускающем проход или доступ к содержимому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "не запертый"
                            },
                            {
                                "target_lang_word": "раскрытый",
                                "target_lang_sense_clarification": "настежь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce36b095-084b-3cc2-aa72-ef959943a736",
                    "sense_definition": "Voor iedereen toegankelijk; openbaar.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "accessibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de <w>open</w> dag mag iedereen het bedrijf bekijken.",
                            "target_lang_translations": {
                                "ru": "Во время дня <w>открытых</w> дверей любой может осмотреть компанию."
                            }
                        },
                        {
                            "text": "Dit is een <w>open</w> bijeenkomst waar iedereen welkom is.",
                            "target_lang_translations": {
                                "ru": "Это <w>открытое</w> собрание, где рады каждому."
                            }
                        }
                    ],
                    "synonyms": [
                        "openbaar",
                        "toegankelijk"
                    ],
                    "antonyms": [
                        "gesloten",
                        "dicht"
                    ],
                    "common_phrases": [
                        "open dag",
                        "open huis",
                        "in het openbaar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доступный для всех; публичный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "публичный, общедоступный"
                            },
                            {
                                "target_lang_word": "доступный",
                                "target_lang_sense_clarification": "для посещения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef94e2ab-bfdc-3078-be7f-dd54d49af127",
                    "sense_definition": "Eerlijk en bereid om gedachten of gevoelens te delen; niet terughoudend.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "personality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij is een heel <w>open</w> persoon die makkelijk over haar gevoelens praat.",
                            "target_lang_translations": {
                                "ru": "Она очень <w>открытый</w> человек, который легко говорит о своих чувствах."
                            }
                        },
                        {
                            "text": "Hij gaf een eerlijk en <w>open</w> antwoord op de moeilijke vraag.",
                            "target_lang_translations": {
                                "ru": "Он дал честный и <w>открытый</w> ответ на трудный вопрос."
                            }
                        }
                    ],
                    "synonyms": [
                        "openhartig",
                        "eerlijk",
                        "oprecht"
                    ],
                    "antonyms": [
                        "gesloten"
                    ],
                    "common_phrases": [
                        "een open boek",
                        "open kaart spelen",
                        "open vizier"
                    ],
                    "target_lang_definitions": {
                        "ru": "Честный и готовый делиться мыслями или чувствами; не скрытный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "искренний, общительный"
                            },
                            {
                                "target_lang_word": "откровенный",
                                "target_lang_sense_clarification": "прямой, честный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "309e6641-7554-3fb8-b86f-b95dd0c85cd4",
                    "sense_definition": "Niet bezet of ingevuld; beschikbaar.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "availability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er zijn nog enkele <w>open</w> plaatsen voor het concert van vanavond.",
                            "target_lang_translations": {
                                "ru": "Есть ещё несколько <w>свободных</w> мест на сегодняшний концерт."
                            }
                        },
                        {
                            "text": "We hebben een <w>open</w> vacature voor een nieuwe manager.",
                            "target_lang_translations": {
                                "ru": "У нас есть <w>открытая</w> вакансия нового менеджера."
                            }
                        }
                    ],
                    "synonyms": [
                        "beschikbaar",
                        "vrij"
                    ],
                    "antonyms": [
                        "dicht",
                        "gesloten",
                        "toe"
                    ],
                    "common_phrases": [
                        "open plaats",
                        "openstaande vacature"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не занятый или не заполненный; доступный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свободный",
                                "target_lang_sense_clarification": "незанятый (место, время)"
                            },
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "вакантный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d28a17a-1ecc-330c-b479-4600a1b5a229",
                    "sense_definition": "Zonder overdekking; blootgesteld aan de buitenlucht.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We zaten gezellig bij het <w>open</w> vuur te praten.",
                            "target_lang_translations": {
                                "ru": "Мы уютно сидели и болтали у <w>открытого</w> огня."
                            }
                        },
                        {
                            "text": "Het concert vond plaats in de <w>open</w> lucht.",
                            "target_lang_translations": {
                                "ru": "Концерт проходил под <w>открытым</w> небом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "open haard",
                        "open lucht",
                        "open zee",
                        "open vuur"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без покрытия или навеса; подверженный воздействию свежего воздуха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "не огороженный, без навеса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1dac083-08eb-35e1-9025-c708d9529b12",
                    "sense_definition": "Nog niet beslist, afgerond of begrensd.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De film heeft een <w>open</w> einde, dus je weet niet hoe het afloopt.",
                            "target_lang_translations": {
                                "ru": "У фильма <w>открытый</w> финал, так что неизвестно, чем всё закончится."
                            }
                        },
                        {
                            "text": "Dat is een <w>open</w> vraag waar je alle kanten mee op kunt.",
                            "target_lang_translations": {
                                "ru": "Это <w>открытый</w> вопрос, который можно трактовать по-разному."
                            }
                        }
                    ],
                    "antonyms": [
                        "gesloten"
                    ],
                    "common_phrases": [
                        "open einde",
                        "open vraag",
                        "open zenuw"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ещё не решённый, не завершённый или не ограниченный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открытый",
                                "target_lang_sense_clarification": "нерешённый (вопрос, финал)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "40b98e9b-6b21-3279-a756-8c33da676726",
                    "sense_definition": "Vervoeging van <w>openen</w>: eerste persoon enkelvoud tegenwoordige tijd.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>open</w> de deur voor de gasten.",
                            "target_lang_translations": {
                                "ru": "Я <w>открываю</w> дверь гостям."
                            }
                        },
                        {
                            "text": "Elke ochtend <w>open</w> ik de winkel om negen uur.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я <w>открываю</w> магазин в девять часов."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спряжение глагола openen: первое лицо единственного числа настоящего времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открываю",
                                "target_lang_sense_clarification": "я открываю"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c2a566d-2ba5-3b0a-8e47-fc44bb316d0a",
                    "sense_definition": "Vervoeging van <w>openen</w>: gebiedende wijs.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Open</w> je boek op bladzijde tien.",
                            "target_lang_translations": {
                                "ru": "<w>Открой</w> книгу на десятой странице."
                            }
                        },
                        {
                            "text": "<w>Open</w> de deur, er wordt aangebeld!",
                            "target_lang_translations": {
                                "ru": "<w>Открой</w> дверь, звонят!"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спряжение глагола openen: повелительное наклонение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открой",
                                "target_lang_sense_clarification": "обращение на 'ты'"
                            },
                            {
                                "target_lang_word": "откройте",
                                "target_lang_sense_clarification": "обращение на 'вы'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d7dbeb0f-f7b0-3629-be37-629c1d1e4f50",
                    "sense_definition": "Vervoeging van <w>openen</w>: tweede persoon enkelvoud tegenwoordige tijd (bij inversie).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Open</w> jij het raam even?",
                            "target_lang_translations": {
                                "ru": "<w>Откроешь</w> окно?"
                            }
                        },
                        {
                            "text": "Waarom <w>open</w> je die brief niet?",
                            "target_lang_translations": {
                                "ru": "Почему ты не <w>открываешь</w> это письмо?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спряжение глагола openen: второе лицо единственного числа настоящего времени (при инверсии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "откроешь",
                                "target_lang_sense_clarification": "просьба или будущее время"
                            },
                            {
                                "target_lang_word": "открываешь",
                                "target_lang_sense_clarification": "настоящее время"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "3d24fbac-9f61-3fd8-9f6a-f9978d7a58b2",
                    "sense_definition": "Op een eerlijke en niet-verhullende manier.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij sprak daar heel <w>open</w> over met zijn vrienden.",
                            "target_lang_translations": {
                                "ru": "Он говорил об этом очень <w>открыто</w> со своими друзьями."
                            }
                        },
                        {
                            "text": "Ze durfden niet <w>open</w> voor hun mening uit te komen.",
                            "target_lang_translations": {
                                "ru": "Они не осмелились <w>открыто</w> высказать свое мнение."
                            }
                        }
                    ],
                    "synonyms": [
                        "openlijk",
                        "vrijuit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Честным и нескрываемым образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "открыто",
                                "target_lang_sense_clarification": "прямо, публично"
                            },
                            {
                                "target_lang_word": "откровенно",
                                "target_lang_sense_clarification": "чистосердечно"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
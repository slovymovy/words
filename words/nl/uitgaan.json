            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a98fd846-d76b-3178-b404-39e4443abb41",
                    "sense_definition": "Naar openbare gelegenheden gaan voor plezier, zoals een café, discotheek, theater of bioscoop.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zullen we vanavond samen <uitgaan> in de stad?",
                            "target_lang_translations": {
                                "en": "Shall we <go out> in the city together tonight?",
                                "ru": "Пойдем сегодня вечером вместе <развлечься> в город?"
                            }
                        },
                        {
                            "text": "Mijn vrienden willen elk weekend <uitgaan>.",
                            "target_lang_translations": {
                                "en": "My friends want to <go out> every weekend.",
                                "ru": "Мои друзья хотят каждые выходные <ходить развлекаться>."
                            }
                        }
                    ],
                    "synonyms": [
                        "stappen"
                    ],
                    "common_phrases": [
                        "een avondje uitgaan",
                        "samen uitgaan"
                    ],
                    "target_lang_definitions": {
                        "en": "To leave one's home to visit public places for entertainment, such as a café, club, theater, or cinema.",
                        "ru": "Посещать общественные места (кафе, клубы, театры) ради развлечения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "go out",
                                "target_lang_sense_clarification": "socializing/entertainment"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "развлекаться",
                                "target_lang_sense_clarification": "проводить время с удовольствием вне дома"
                            },
                            {
                                "target_lang_word": "гулять",
                                "target_lang_sense_clarification": "ходить куда-то с друзьями (разг.)"
                            },
                            {
                                "target_lang_word": "выходить в свет",
                                "target_lang_sense_clarification": "посещать мероприятия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d55b3e0-112b-302b-a9fb-b9ac99962d65",
                    "sense_definition": "Ophouden met branden of functioneren (van licht, vuur of apparaten).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kaars is door de harde wind <uitgegaan>.",
                            "target_lang_translations": {
                                "en": "The candle <went out> due to the strong wind.",
                                "ru": "Свеча <погасла> из-за сильного ветра."
                            }
                        },
                        {
                            "text": "Ik merkte dat het licht in de gang plotseling <uitging>.",
                            "target_lang_translations": {
                                "en": "I noticed that the light in the hallway suddenly <went out>.",
                                "ru": "Я заметил, что свет в коридоре внезапно <погас>."
                            }
                        }
                    ],
                    "synonyms": [
                        "doven"
                    ],
                    "antonyms": [
                        "aangaan"
                    ],
                    "common_phrases": [
                        "het licht gaat uit",
                        "de kachel gaat uit"
                    ],
                    "target_lang_definitions": {

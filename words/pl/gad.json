{
    "word_family": [
        "gadzina",
        "gadzi"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "91f7deab-5d43-34c0-8d16-bdb2e7f1696f",
                    "sense_definition": "Zmiennocieplne zwierzę kręgowe, którego ciało pokryte jest łuskami lub tarczkami, oddychające płucami (np. wąż, jaszczurka, krokodyl, żółw).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "biology_reptile",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W terrarium siedział wielki, zielony <gad>, który obserwował nas nieruchomym wzrokiem.",
                            "target_lang_translations": {
                                "ru": "В террариуме сидела огромная зеленая <рептилия>, которая наблюдала за нами неподвижным взглядом."
                            }
                        },
                        {
                            "text": "Dinozaury to wymarłe <gady>, które dominowały na Ziemi miliony lat temu.",
                            "target_lang_translations": {
                                "ru": "Динозавры — это вымершие <пресмыкающиеся>, которые господствовали на Земле миллионы лет назад."
                            }
                        }
                    ],
                    "common_phrases": [
                        "era gadów",
                        "gady kopalne",
                        "świat gadów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Холоднокровное позвоночное животное, тело которого покрыто чешуей или щитками, дышащее легкими (например, змея, ящерица, крокодил, черепаха)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рептилия",
                                "target_lang_sense_clarification": "общеупотребительное"
                            },
                            {
                                "target_lang_word": "пресмыкающееся",
                                "target_lang_sense_clarification": "научный термин"
                            },
                            {
                                "target_lang_word": "гад",
                                "target_lang_sense_clarification": "разговорное/устаревшее, о животных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e0062a0-da95-3375-b5e6-a0e6b9dafb81",
                    "sense_definition": "Człowiek podły, fałszywy, budzący wstręt swoim zachowaniem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "insult_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie ufaj mu, to przebiegły <gad>, który dba tylko o własne interesy.",
                            "target_lang_translations": {
                                "ru": "Не доверяй ему, это хитрый <гад>, который печется только о собственных интересах."
                            }
                        },
                        {
                            "text": "Ten wstrętny <gad> oszukał staruszkę na dużą sumę pieniędzy.",
                            "target_lang_translations": {
                                "ru": "Этот отвратительный <гад> обманул старушку на крупную сумму денег."
                            }
                        }
                    ],
                    "synonyms": [
                        "podlec",
                        "potwór"
                    ],
                    "common_phrases": [
                        "wstrętny gad",
                        "stary gad",
                        "podły gad"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подлый, лживый человек, вызывающий отвращение своим поведением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гад",
                                "target_lang_sense_clarification": "бранное слово"
                            },
                            {
                                "target_lang_word": "мерзавец",
                                "target_lang_sense_clarification": "синоним"
                            },
                            {
                                "target_lang_word": "подлец",
                                "target_lang_sense_clarification": "синоним"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9c87e880-35db-3551-8de8-f41a8d0efa55",
                    "sense_definition": "Funkcjonariusz służby więziennej (w gwarze więziennej).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slang_prison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Więźniowie przestali rozmawiać, gdy na korytarzu pojawił się <gad>.",
                            "target_lang_translations": {
                                "ru": "Заключенные перестали разговаривать, когда в коридоре появился <вертухай>."
                            }
                        },
                        {
                            "text": "Osadzony został ukarany za obrażenie <gada> podczas apelu.",
                            "target_lang_translations": {
                                "ru": "Заключенный был наказан за оскорбление <вертухая> во время переклички."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "prison slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сотрудник тюремной охраны (на тюремном жаргоне)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вертухай",
                                "target_lang_sense_clarification": "тюремный жаргон"
                            },
                            {
                                "target_lang_word": "надзиратель",
                                "target_lang_sense_clarification": "нейтральное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "10cd91b1-7515-3f0f-8bb7-fa3f447e56dc",
                    "sense_definition": "Postać biblijna, jeden z synów Jakuba; także nazwa jednego z dwunastu plemion Izraela.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "biblical_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "W Starym Testamencie czytamy, że <Gad> był siódmym synem patriarchy Jakuba.",
                            "target_lang_translations": {
                                "ru": "В Ветхом Завете мы читаем, что <Гад> был седьмым сыном патриарха Иакова."
                            }
                        },
                        {
                            "text": "Plemię wywodzące się od <Gada> osiedliło się na wschodnim brzegu Jordanu.",
                            "target_lang_translations": {
                                "ru": "Племя, происходящее от <Гада>, поселилось на восточном берегу Иордана."
                            }
                        }
                    ],
                    "common_phrases": [
                        "plemię Gada",
                        "synowie Gada"
                    ],
                    "target_lang_definitions": {
                        "ru": "Библейский персонаж, один из сыновей Иакова; также название одного из двенадцати колен Израилевых."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Гад",
                                "target_lang_sense_clarification": "имя собственное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3fb5eda-dfc7-3a64-9cf3-5d547cbbf9c3",
                    "sense_definition": "Nazwisko używane w Polsce.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <Gad> został wybrany na przewodniczącego rady osiedla.",
                            "target_lang_translations": {
                                "ru": "Господин <Гад> был избран председателем совета жилого комплекса."
                            }
                        },
                        {
                            "text": "Na liście obecności widnieje nazwisko Anny <Gad>.",
                            "target_lang_translations": {
                                "ru": "В списке присутствующих значится фамилия Анны <Гад>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия, встречающаяся в Польше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Гад",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "произношение",
        "произносительный"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9570194a-823d-38ff-a6e2-d75ea43f6858",
                    "sense_definition": "Передавать голосом звуки и слова, выговаривать их.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "articulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она старалась правильно <произносить> каждое слово на иностранном языке.",
                            "target_lang_translations": {
                                "en": "She tried to <pronounce> every word in the foreign language correctly."
                            }
                        },
                        {
                            "text": "Актер должен уметь четко <произносить> свои реплики со сцены.",
                            "target_lang_translations": {
                                "en": "An actor must be able to clearly <articulate> his lines from the stage."
                            }
                        }
                    ],
                    "synonyms": [
                        "выговаривать"
                    ],
                    "common_phrases": [
                        "произносить слова",
                        "четко произносить",
                        "произносить звуки",
                        "произносить имя"
                    ],
                    "target_lang_definitions": {
                        "en": "To produce sounds and words with the voice; to articulate."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pronounce",
                                "target_lang_sense_clarification": "to sound out words or phrases"
                            },
                            {
                                "target_lang_word": "articulate",
                                "target_lang_sense_clarification": "to speak clearly and distinctly"
                            },
                            {
                                "target_lang_word": "utter",
                                "target_lang_sense_clarification": "to produce sounds with the voice"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27dc0cab-0e8f-3630-9004-ff2a85225d27",
                    "sense_definition": "Выступать публично с речью, говорить что-либо торжественно или официально.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "speech_delivery",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Президент готовился <произносить> важную речь перед народом.",
                            "target_lang_translations": {
                                "en": "The president was preparing to <deliver> an important speech to the people."
                            }
                        },
                        {
                            "text": "За праздничным столом гости начали <произносить> тосты.",
                            "target_lang_translations": {
                                "en": "At the festive table, the guests began to <make> toasts."
                            }
                        }
                    ],
                    "synonyms": [
                        "говорить",
                        "высказывать"
                    ],
                    "common_phrases": [
                        "произносить речь",
                        "произносить тост",
                        "произносить клятву",
                        "произносить приговор"
                    ],
                    "target_lang_definitions": {
                        "en": "To speak publicly, to give a speech, or to declare something solemnly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deliver",
                                "target_lang_sense_clarification": "to give a formal speech or address"
                            },
                            {
                                "target_lang_word": "pronounce",
                                "target_lang_sense_clarification": "to declare formally or officially"
                            }
                        ]
                    }
                }
            ]
        }
    ]

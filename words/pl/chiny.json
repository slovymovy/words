                    "sense_id": "3609117d-83f3-30d1-bc9d-34fa7990ae80",
                    "sense_definition": "Państwo w Azji Wschodniej, ze stolicą w Pekinie, jedno z największych i najludniejszych na świecie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "W zeszłym roku na wakacjach byliśmy w <w>Chinach</w>.",
                            "target_lang_translations": {
                                "ru": "В прошлом году в отпуске мы были в <w>Китае</w>."
                            }
                        },
                        {
                            "text": "<w>Chiny</w> są obecnie jedną z największych gospodarek na świecie.",
                            "target_lang_translations": {
                                "ru": "<w>Китай</w> в настоящее время является одной из крупнейших экономик в мире."
                            }
                        }
                    ],
                    "synonyms": [
                        "Chińska Republika Ludowa",
                        "ChRL",
                        "Państwo Środka"
                    ],
                    "common_phrases": [
                        "lecieć do Chin",
                        "pochodzić z Chin",
                        "wycieczka do Chin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Государство в Восточной Азии со столицей в Пекине, одно из крупнейших и самых густонаселенных в мире."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Китай",
                                "target_lang_sense_clarification": "страна"
                            }
                        ]

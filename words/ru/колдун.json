    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e1bb3252-1a91-331b-a136-aa6f2eb0220a",
                    "sense_definition": "Человек, занимающийся магией, чародейством; волшебник.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "magic_practitioner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В старой башне жил могущественный <колдун>, которого боялись все жители деревни.",
                            "target_lang_translations": {
                                "en": "A powerful <w>sorcerer</w> lived in the old tower, whom all the villagers feared."
                            }
                        },
                        {
                            "text": "Злой <колдун> произнёс заклинание и исчез в облаке дыма.",
                            "target_lang_translations": {
                                "en": "The evil <w>wizard</w> cast a spell and vanished in a cloud of smoke."
                            }
                        }
                    ],
                    "synonyms": [
                        "волшебник",
                        "маг",
                        "чародей",
                        "чернокни́жник",
                        "ведьмак",
                        "кудесник"
                    ],
                    "common_phrases": [
                        "злой колдун",
                        "старый колдун",
                        "могущественный колдун"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who practices magic or sorcery; a wizard."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sorcerer",
                                "target_lang_sense_clarification": "A person who claims or is believed to have magic powers; often implies a darker or more primal magic."
                            },
                            {
                                "target_lang_word": "wizard",
                                "target_lang_sense_clarification": "A man who has magical powers, especially in legends and fairy tales."
                            },
                            {
                                "target_lang_word": "warlock",
                                "target_lang_sense_clarification": "A man who practices witchcraft; a male witch."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c84a8c0-66f1-3186-8e3f-43b59e4ee402",
                    "sense_definition": "Блюдо из теста или картофеля с мясной начинкой, похожее на пельмени или драники.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бабушка приготовила на ужин вкусные <колдуны> со сметаной.",
                            "target_lang_translations": {
                                "en": "Grandma cooked delicious <w>kolduny</w> with sour cream for dinner."
                            }
                        },
                        {
                            "text": "В этом ресторане подают отличные <колдуны> с грибным соусом.",
                            "target_lang_translations": {
                                "en": "This restaurant serves excellent <w>potato dumplings</w> with mushroom sauce."
                            }
                        }
                    ],
                    "common_phrases": [
                        "картофельные колдуны",
                        "колдуны с мясом"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "блюдо белорусской, литовской и польской кухни"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A dish made of dough or potatoes with a meat filling, similar to dumplings or potato pancakes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "kolduny",
                                "target_lang_sense_clarification": "A traditional dumpling or stuffed potato pancake from Belarusian, Lithuanian, and Polish cuisine."
                            },
                            {
                                "target_lang_word": "potato dumpling",
                                "target_lang_sense_clarification": "A descriptive translation for the dish."
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55e7eee3-2710-3434-9bbe-c7304a90f42c",
                    "sense_definition": "Ветроуказатель; конус для определения направления и силы ветра на аэродромах.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technical_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пилот посмотрел на полосатый <колдун>, чтобы оценить боковой ветер перед посадкой.",
                            "target_lang_translations": {
                                "en": "The pilot looked at the striped <w>windsock</w> to estimate the crosswind before landing."
                            }
                        },
                        {
                            "text": "На крыше ангара установили новый яркий <колдун>.",
                            "target_lang_translations": {
                                "en": "A new bright <w>windsock</w> was installed on the roof of the hangar."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "профессиональный жаргон"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A windsock; a cone used to determine wind direction and strength at airfields."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "windsock",
                                "target_lang_sense_clarification": "A conical textile tube designed to indicate wind direction and relative wind speed."
                            }
                        ]
                    }
                }
            ]
        },

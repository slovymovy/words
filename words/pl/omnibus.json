{
    "word_family": [
        "omnibusowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9d8cff22-9e6a-37be-87a3-9c9887f32336",
                    "sense_definition": "Człowiek posiadający wszechstronną i rozległą wiedzę z wielu różnych dziedzin.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_knowledge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój wujek to prawdziwy <w>omnibus</w>, który potrafi odpowiedzieć na każde pytanie z historii.",
                            "target_lang_translations": {
                                "ru": "Мой дядя — настоящий <w>эрудит</w>, который может ответить на любой вопрос по истории."
                            }
                        },
                        {
                            "text": "W naszej klasie Marek uchodzi za <w>omnibusa</w>, bo wygrywa wszystkie konkursy wiedzy.",
                            "target_lang_translations": {
                                "ru": "В нашем классе Марек считается <w>ходячей энциклопедией</w>, потому что выигрывает все викторины."
                            }
                        }
                    ],
                    "synonyms": [
                        "erudyta",
                        "chodząca encyklopedia",
                        "człowiek renesansu"
                    ],
                    "antonyms": [
                        "ignorant",
                        "dyletant"
                    ],
                    "common_phrases": [
                        "prawdziwy omnibus",
                        "być omnibusem",
                        "szkolny omnibus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, обладающий разносторонними и обширными знаниями во многих областях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эрудит",
                                "target_lang_sense_clarification": "человек с глубокими познаниями"
                            },
                            {
                                "target_lang_word": "ходячая энциклопедия",
                                "target_lang_sense_clarification": "идиома"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "917c3113-aa4a-34ec-9af4-b30e70f25cb6",
                    "sense_definition": "Dawny miejski pojazd konny, wieloosobowy, kursujący po stałych trasach (poprzednik autobusu).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dziewiętnastowiecznej Warszawie mieszkańcy często podróżowali konnym <w>omnibusem</w>.",
                            "target_lang_translations": {
                                "ru": "В Варшаве девятнадцатого века жители часто ездили на конном <w>омнибусе</w>."
                            }
                        },
                        {
                            "text": "Stary <w>omnibus</w> toczył się powoli po brukowanej ulicy miasta.",
                            "target_lang_translations": {
                                "ru": "Старый <w>омнибус</w> медленно катился по мощёной улице города."
                            }
                        }
                    ],
                    "common_phrases": [
                        "konny omnibus",
                        "jechać omnibusem"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "pojazd historyczny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинный городской многоместный конный экипаж, курсировавший по определённым маршрутам (предшественник автобуса)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "омнибус",
                                "target_lang_sense_clarification": "транспортное средство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb016db7-16aa-375b-926a-c4ddf0b9db62",
                    "sense_definition": "Film fabularny składający się z kilku oddzielnych nowel, połączonych wspólnym tematem lub motywem.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "film_genre",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten festiwalowy <w>omnibus</w> składa się z pięciu krótkich historii o miłości.",
                            "target_lang_translations": {
                                "ru": "Этот фестивальный <w>киноальманах</w> состоит из пяти коротких историй о любви."
                            }
                        },
                        {
                            "text": "Reżyserzy z różnych krajów stworzyli wspólnie filmowy <w>omnibus</w> o przyszłości Europy.",
                            "target_lang_translations": {
                                "ru": "Режиссёры из разных стран совместно создали <w>киноальманах</w> о будущем Европы."
                            }
                        }
                    ],
                    "synonyms": [
                        "film omnibusowy"
                    ],
                    "common_phrases": [
                        "filmowy omnibus"
                    ],
                    "target_lang_definitions": {
                        "ru": "Художественный фильм, состоящий из нескольких отдельных новелл, объединённых общей темой или мотивом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "киноальманах",
                                "target_lang_sense_clarification": "сборник киноновелл"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6132fc73-24be-3842-a9bd-38c88774432f",
                    "sense_definition": "Вид зрелищного искусства, включающий выступления акробатов, эквилибристов, жонглёров, клоунов, фокусников и дрессированных животных.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "entertainment_core",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дети очень любят <цирк> и всегда смеются над клоунами.",
                            "target_lang_translations": {
                                "en": "Children really love the <circus> and always laugh at the clowns."
                            }
                        },
                        {
                            "text": "В наш город приехал известный китайский <цирк> с новой программой.",
                            "target_lang_translations": {
                                "en": "A famous Chinese <circus> has arrived in our city with a new program."
                            }
                        }
                    ],
                    "common_phrases": [
                        "любить цирк",
                        "советский цирк",
                        "артист цирка",
                        "звезда цирка"
                    ],
                    "target_lang_definitions": {
                        "en": "A form of performing art involving acrobats, equilibrists, jugglers, clowns, magicians, and trained animals."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "circus",
                                "target_lang_sense_clarification": "entertainment / show"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1289e6b1-808f-3e02-b551-528923190997",
                    "sense_definition": "Здание с круглой ареной (манежем) и местами для зрителей, предназначенное для цирковых представлений.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "entertainment_core",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В воскресенье мы всей семьёй пошли в <цирк>.",
                            "target_lang_translations": {
                                "en": "On Sunday, our whole family went to the <circus>."
                            }
                        },
                        {
                            "text": "Возле вокзала построили новый современный <цирк>.",
                            "target_lang_translations": {
                                "en": "A new modern <circus> was built near the train station."
                            }
                        }
                    ],
                    "common_phrases": [
                        "пойти в цирк",
                        "билет в цирк",
                        "купол цирка",
                        "арена цирка"
                    ],
                    "target_lang_definitions": {
                        "en": "A building with a round arena and seating for spectators, designed for circus performances."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "circus",
                                "target_lang_sense_clarification": "building"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2b199ff-931d-3413-a942-4de3e0a0ce62",
                    "sense_definition": "Комическая, нелепая или абсурдная ситуация; беспорядок, шум и неразбериха.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На собрании начался настоящий <цирк>, все кричали и никто не слушал директора.",
                            "target_lang_translations": {
                                "en": "A real <circus> started at the meeting; everyone was shouting and no one listened to the director."
                            }
                        },
                        {
                            "text": "Хватит устраивать этот <цирк>, давай поговорим серьёзно.",
                            "target_lang_translations": {
                                "en": "Stop making this <scene>, let's talk seriously."
                            }
                        }
                    ],
                    "synonyms": [
                        "клоунада",
                        "дурдом",
                        "потеха"
                    ],
                    "common_phrases": [
                        "устроить цирк",
                        "настоящий цирк",
                        "цирк с конями",
                        "какой-то цирк"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A comic, ridiculous, or absurd situation; disorder, noise, and confusion."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "circus",
                                "target_lang_sense_clarification": "figurative - chaotic situation"
                            },
                            {
                                "target_lang_word": "madhouse",
                                "target_lang_sense_clarification": "chaos / disorder"
                            },
                            {
                                "target_lang_word": "farce",
                                "target_lang_sense_clarification": "absurd situation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df57cf13-7003-39b4-9b94-be355d10bf87",
                    "sense_definition": "В Древнем Риме — место для конных скачек, состязаний колесниц и гладиаторских боёв.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Римский <цирк> был переполнен зрителями, ожидающими начала гонок на колесницах.",
                            "target_lang_translations": {
                                "en": "The Roman <circus> was crowded with spectators waiting for the chariot races to start."
                            }
                        },
                        {
                            "text": "Император приказал построить новый огромный <цирк> для народных увеселений.",
                            "target_lang_translations": {
                                "en": "The emperor ordered the construction of a huge new <circus> for public entertainment."
                            }
                        }
                    ],
                    "common_phrases": [
                        "римский цирк",
                        "арена цирка"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "In Ancient Rome, a venue for horse races, chariot racing, and gladiator fights."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "circus",
                                "target_lang_sense_clarification": "historical - Roman arena"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "361cff4b-d667-3512-a96a-442e6c2e4a76",
                    "sense_definition": "Естественное чашеобразное углубление в горах, где скапливается снег и образуется ледник.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "scientific_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Геологи исследовали горный <цирк>, расположенный на северном склоне хребта.",
                            "target_lang_translations": {
                                "en": "Geologists explored the mountain <cirque> located on the northern slope of the ridge."
                            }
                        },
                        {
                            "text": "Ледниковый <цирк> напоминает по своей форме огромный амфитеатр.",
                            "target_lang_translations": {
                                "en": "The glacial <cirque> resembles a huge amphitheater in shape."
                            }
                        }
                    ],
                    "synonyms": [
                        "кар",
                        "котловина"
                    ],
                    "common_phrases": [
                        "ледниковый цирк",
                        "горный цирк"
                    ],
                    "target_lang_definitions": {
                        "en": "A natural bowl-shaped depression in mountains where snow accumulates and a glacier forms."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cirque",
                                "target_lang_sense_clarification": "geology"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99a40ef0-2048-33e7-8d57-ac2e140701d3",
                    "sense_definition": "Крупный лунный кратер с ровным дном и кольцевым валом.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "scientific_shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В телескоп можно разглядеть огромный <цирк> на поверхности Луны.",
                            "target_lang_translations": {
                                "en": "Through a telescope, one can make out a huge <circus> on the surface of the Moon."
                            }
                        },
                        {
                            "text": "Астрономы называют эти образования кратерами или лунными <цирками>.",
                            "target_lang_translations": {
                                "en": "Astronomers call these formations craters or lunar <circuses>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "лунный цирк"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "astronomical term"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A large lunar crater with a flat floor and a ring-shaped rim."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lunar circus",
                                "target_lang_sense_clarification": "astronomy - dated"
                            },
                            {
                                "target_lang_word": "walled plain",
                                "target_lang_sense_clarification": "astronomy"
                            }
                        ]
                    }
                }
            ]
        }

            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "74a18282-66bc-3acb-96e8-d40d66bf66f7",
                    "sense_definition": "Het eerste, echte exemplaar van iets, dat als voorbeeld dient voor latere kopieën.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "oorspronkelijk",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kopie was zo goed dat hij bijna niet van het <w>origineel</w> te onderscheiden was.",
                            "target_lang_translations": {
                                "en": "The copy was so good that it was almost indistinguishable from the <w>original</w>.",
                                "ru": "Копия была так хороша, что её почти невозможно было отличить от <w>оригинала</w>."
                            }
                        },
                        {
                            "text": "Je moet het <w>origineel</w> van het contract goed bewaren voor de administratie.",
                            "target_lang_translations": {
                                "en": "You must keep the <w>original</w> of the contract safe for the administration.",
                                "ru": "Вы должны бережно хранить <w>оригинал</w> контракта для отчётности."
                            }
                        }
                    ],
                    "antonyms": [
                        "kopie",
                        "vervalsing"
                    ],
                    "common_phrases": [
                        "het origineel bewaren",
                        "naar het origineel"
                    ],
                    "target_lang_definitions": {
                        "en": "The first, genuine specimen of something, serving as a model for later copies.",
                        "ru": "Первый, настоящий экземпляр чего-либо, служащий образцом для последующих копий."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "original",

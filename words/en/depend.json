    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "94b7f995-1756-3f21-8c21-721d69a6dea7",
                    "sense_definition": "To be determined or decided by something else; to require something as a necessary condition.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Our plans for the picnic <w>depend</w> entirely on the weather."
                        },
                        {
                            "text": "Whether we can buy the house <w>depends</w> on the bank approving our loan."
                        }
                    ],
                    "synonyms": [
                        "hinge on",
                        "ride",
                        "pivot"
                    ],
                    "common_phrases": [
                        "depend on",
                        "it depends"
                    ]
                },
                {
                    "sense_id": "92fd50cf-8f10-3664-bf52-5d308ab80239",
                    "sense_definition": "To trust or rely on someone or something for support, help, or certainty.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "trust",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can always <w>depend</w> on him to help when you are in trouble."
                        },
                        {
                            "text": "Many charities <w>depend</w> on donations from the public to survive."
                        }
                    ],
                    "synonyms": [
                        "pivot",
                        "hinge on",
                        "ride",
                        "pivot"
                    ],
                    "common_phrases": [
                        "depend on someone",
                        "depend upon"
                    ]
                },
                {
                    "sense_id": "1b076cb8-effc-363f-aadb-08c8c2c523e6",
                    "sense_definition": "To hang down from something; to be suspended.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_suspension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Long icicles <w>depended</w> from the roof of the old cabin."
                        },
                        {
                            "text": "Heavy velvet curtains <w>depended</w> from the high windows of the ballroom."
                        }
                    ],
                    "synonyms": [
                        "ride"
                    ],
                    "traits": [

                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объем занимаемой памяти",
                                "target_lang_sense_clarification": "оперативная память"
                            },
                            {
                                "target_lang_word": "размер",
                                "target_lang_sense_clarification": "занимаемое место на диске"
                            }

                        "verstrikken"
                    ],
                    "antonyms": [
                        "ontwarren"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "het werkwoord wordt in deze vorm weinig meer gebruikt; 'verwarren' is gebruikelijker"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To confuse or cause to become entangled.",
                        "ru": "Приводить в беспорядок, запутывать (волосы, нити) или сбивать с толку (мысли)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to tangle",
                                "target_lang_sense_clarification": "to make hair or threads knotted"
                            },
                            {
                                "target_lang_word": "to confuse",
                                "target_lang_sense_clarification": "to make thoughts unclear"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "спутывать",
                                "target_lang_sense_clarification": "приводить в беспорядок (о волосах, нитках)"

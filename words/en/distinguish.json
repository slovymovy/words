    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1f24686d-3eca-322c-955d-78b1f8f0f2ed",
                    "sense_definition": "To notice or understand the difference between two people or things.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "differentiation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It can be very difficult to <w>distinguish</w> between identical twins."
                        },
                        {
                            "text": "Small children often cannot <w>distinguish</w> right from wrong."
                        }
                    ],
                    "synonyms": [
                        "differentiate",
                        "tell apart",
                        "discriminate",
                        "tell"
                    ],
                    "antonyms": [
                        "confuse"
                    ],
                    "common_phrases": [
                        "distinguish between",
                        "distinguish from"
                    ]
                },
                {
                    "sense_id": "cf4acd2a-c21c-3bf6-9451-1b0723321fe4",
                    "sense_definition": "To be the quality or fact that makes someone or something different from others.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "differentiation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ability to use complex language seems to <w>distinguish</w> humans from other animals."
                        },
                        {
                            "text": "What <w>distinguishes</w> this company is its commitment to excellent customer service."
                        }
                    ],
                    "synonyms": [
                        "set apart",
                        "differentiate"
                    ],
                    "common_phrases": [
                        "distinguish from"
                    ]
                },
                {
                    "sense_id": "09ec02f6-fa48-3c29-a381-8b648b8d3d35",
                    "sense_definition": "To manage to see, hear, or recognize something that is not clear.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I could barely <w>distinguish</w> the shape of a boat through the thick fog."
                        },
                        {
                            "text": "He spoke so quietly that I could not <w>distinguish</w> what he was saying."
                        }
                    ],
                    "synonyms": [
                        "discern",
                        "make out"
                    ],
                    "common_phrases": [
                        "hard to distinguish",
                        "barely distinguish"
                    ]
                },
                {
                    "sense_id": "2c454b4e-6319-3a31-a726-65dbcf7c487e",
                    "sense_definition": "To do something very well and make oneself famous or respected.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "excellence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The young soldier <w>distinguished</w> himself by showing great bravery in battle."
                        },
                        {
                            "text": "She <w>distinguished</w> herself as a brilliant lawyer early in her career."
                        }
                    ],
                    "synonyms": [
                        "set apart"
                    ],
                    "common_phrases": [
                        "distinguish oneself"
                    ]
                }
            ]
        }
    ]

{
    "word_family": [
        "disappoint",
        "disappointingly"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e9a2428e-b8a7-3a89-bd7f-1454d45569cf",
                    "sense_definition": "The feeling of sadness or frustration when hopes or expectations are not met.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She tried to hide her <w>disappointment</w> when she did not get the job.",
                            "target_lang_translations": {
                                "ru": "Она пыталась скрыть своё <w>разочарование</w>, когда не получила работу."
                            }
                        },
                        {
                            "text": "To my great <w>disappointment</w>, the concert was cancelled at the last minute.",
                            "target_lang_translations": {
                                "ru": "К моему огромному <w>разочарованию</w>, концерт отменили в последнюю минуту."
                            }
                        }
                    ],
                    "antonyms": [
                        "satisfaction",
                        "contentment"
                    ],
                    "common_phrases": [
                        "express disappointment",
                        "hide one's disappointment",
                        "deep disappointment",
                        "bitter disappointment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство грусти или досады, возникающее, когда надежды или ожидания не оправдываются."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разочарова́ние",
                                "target_lang_sense_clarification": "чувство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96557a35-3812-3da2-87b0-d9c585ee8852",
                    "sense_definition": "A person, thing, or event that fails to meet expectations.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "cause_of_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new movie was a big <w>disappointment</w> despite all the advertising.",
                            "target_lang_translations": {
                                "ru": "Новый фильм стал большим <w>разочарованием</w>, несмотря на всю рекламу."
                            }
                        },
                        {
                            "text": "He felt like a <w>disappointment</w> to his parents after failing the exam.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал себя <w>разочарованием</w> для родителей после провала на экзамене."
                            }
                        }
                    ],
                    "synonyms": [
                        "let-down"
                    ],
                    "antonyms": [
                        "success",
                        "pleasure"
                    ],
                    "common_phrases": [
                        "big disappointment",
                        "major disappointment",
                        "huge disappointment",
                        "prove a disappointment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, предмет или событие, которые не оправдали ожиданий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разочарова́ние",
                                "target_lang_sense_clarification": "причина или источник"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
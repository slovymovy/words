        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8170823d-71de-3a45-bcf5-6b64066365a7",
                    "sense_definition": "To strike someone or something with a sudden, sharp blow.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_hit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The angry player tried to <w>biff</w> the referee during the argument.",
                            "target_lang_translations": {
                                "ru": "Разъяренный игрок попытался <w>врезать</w> судье во время спора."
                            }
                        },
                        {
                            "text": "If you keep annoying him, he might <w>biff</w> you on the nose.",
                            "target_lang_translations": {
                                "ru": "Если будешь продолжать его доставать, он может <w>дать</w> тебе по носу."
                            }
                        }
                    ],
                    "synonyms": [
                        "punch",
                        "hit",
                        "wallop",
                        "clout",
                        "strike"
                    ],
                    "common_phrases": [
                        "biff someone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нанести кому-то или чему-то внезапный, резкий удар."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "врезать",
                                "target_lang_sense_clarification": "разговорное, ударить сильно"
                            },
                            {
                                "target_lang_word": "ударить",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "стукнуть",
                                "target_lang_sense_clarification": "нанести удар"
                            }
                        ]
                    }
                },
                {

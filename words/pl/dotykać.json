                    "sense_id": "6f8ad2ca-ec41-321c-9c58-67704c732b26",
                    "sense_definition": "Nawiązywać kontakt fizyczny z kimś lub czymś poprzez przyłożenie ręki lub innej części ciała.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "kontakt_fizyczny",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę nie <w>dotykać</w> eksponatów w muzeum, ponieważ są bardzo delikatne.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>трогайте</w> экспонаты в музее, так как они очень хрупкие."
                            }
                        },
                        {
                            "text": "Dziecko ostrożnie <w>dotykało</w> miękkiego futra małego kota.",
                            "target_lang_translations": {
                                "ru": "Ребенок осторожно <w>трогал</w> мягкую шерсть маленького кота."
                            }
                        }
                    ],
                    "synonyms": [
                        "musnąć",
                        "trącić"
                    ],
                    "antonyms": [
                        "unikać",
                        "omijać"
                    ],
                    "common_phrases": [

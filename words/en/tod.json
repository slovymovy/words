                },
                {
                    "sense_id": "1161990a-14b6-3e1e-a571-0a9ec05c3b9c",
                    "sense_definition": "An old English unit of weight, primarily used for wool, equal to 28 pounds (approximately 12.7 kg).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The merchant purchased a <w>tod</w> of high-quality wool.",
                            "target_lang_translations": {
                                "ru": "Купец приобрел <w>тод</w> высококачественной шерсти."
                            }
                        },
                        {
                            "text": "In the old market records, prices were listed per <w>tod</w> rather than per pound.",
                            "target_lang_translations": {
                                "ru": "В старых рыночных записях цены указывались за <w>тод</w>, а не за фунт."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical measurement"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинная английская мера веса, использовавшаяся преимущественно для шерсти, равная 28 фунтам (около 12,7 кг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тод",
                                "target_lang_sense_clarification": "старинная мера веса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a353e7e9-6593-33a6-a870-275275aa0a6f",

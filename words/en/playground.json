    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f1fb86af-ac69-33f8-a4b0-021cc9e0ac84",
                    "sense_definition": "A large open space for children to play in, usually having dedicated play equipment such as swings and slides.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "recreation_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The kids have to go to the <w>playground</w> during recess, no matter the weather.",
                            "target_lang_translations": {
                                "ru": "Во время перемены детям приходится идти на <w>игровую площадку</w>, какая бы ни была погода."
                            }
                        },
                        {
                            "text": "The local council have set up a new <w>playground</w>, equipped with slides, swings, and a climbing wall.",
                            "target_lang_translations": {
                                "ru": "Местный совет построил новую <w>детскую площадку</w>, оборудованную горками, качелями и стеной для скалолазания."
                            }
                        }
                    ],
                    "synonyms": [
                        "play area",
                        "park"
                    ],
                    "common_phrases": [
                        "school playground",
                        "adventure playground",
                        "playground equipment",
                        "level playground"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое открытое пространство для игр детей, обычно оснащенное специальным оборудованием, таким как качели и горки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "детская площадка",
                                "target_lang_sense_clarification": "место для игр детей"
                            },
                            {
                                "target_lang_word": "игровая площадка",
                                "target_lang_sense_clarification": "оборудованная зона для игр"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0b05d68-59d1-3603-a099-4c635073da28",
                    "sense_definition": "Any physical or metaphysical space in which a person or organization has free rein to do as they please.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The exclusive tropical island was a millionaire's <w>playground</w>.",
                            "target_lang_translations": {
                                "ru": "Этот эксклюзивный тропический остров был <w>местом развлечений</w> для миллионеров."
                            }
                        },
                        {
                            "text": "This vast untamed area of forest and mountain is known as Oslo's <w>playground</w>.",
                            "target_lang_translations": {
                                "ru": "Эта обширная дикая местность с лесами и горами известна как <w>зона отдыха</w> жителей Осло."
                            }
                        }
                    ],
                    "synonyms": [
                        "domain",
                        "stomping ground"
                    ],
                    "common_phrases": [
                        "millionaire's playground",
                        "rich man's playground"
                    ],
                    "target_lang_definitions": {
                        "ru": "Любое физическое или метафорическое пространство, в котором человек или организация имеют полную свободу действий или развлечений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "место развлечений",
                                "target_lang_sense_clarification": "курорт или зона отдыха"
                            },
                            {
                                "target_lang_word": "сфера деятельности",
                                "target_lang_sense_clarification": "область свободных действий"
                            },
                            {
                                "target_lang_word": "игровая площадка",
                                "target_lang_sense_clarification": "метафорическое использование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b37c8473-d51b-3c12-aa38-3dc2ed5145b9",
                    "sense_definition": "A sandbox environment for testing calls to an application programming interface or experimenting with code.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "computing_environment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Developers use the API <w>playground</w> to test their requests before writing the actual code.",
                            "target_lang_translations": {
                                "ru": "Разработчики используют <w>песочницу</w> API, чтобы протестировать свои запросы перед написанием реального кода."
                            }
                        },
                        {
                            "text": "The site offers a coding <w>playground</w> where you can experiment with new features safely.",
                            "target_lang_translations": {
                                "ru": "Сайт предлагает <w>площадку</w> для программирования, где вы можете безопасно экспериментировать с новыми функциями."
                            }
                        }
                    ],
                    "synonyms": [
                        "sandbox",
                        "testbed"
                    ],
                    "antonyms": [
                        "production environment"
                    ],
                    "common_phrases": [
                        "API playground",
                        "code playground"
                    ],
                    "target_lang_definitions": {
                        "ru": "Среда («песочница») для тестирования вызовов программного интерфейса (API) или экспериментов с кодом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "песочница",
                                "target_lang_sense_clarification": "среда для тестирования ПО"
                            },
                            {
                                "target_lang_word": "тестовая площадка",
                                "target_lang_sense_clarification": "место для экспериментов с кодом"
                            }
                        ]
                    }
                }
            ]
        }
    ]

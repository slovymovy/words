                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "синий",
                                "target_lang_sense_clarification": "цвет"
                            },
                            {
                                "target_lang_word": "синева",
                                "target_lang_sense_clarification": "оттенок или свойство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4154d2e8-fc7c-3a27-b51b-e7688cbb812b",
                    "sense_definition": "De politie, verwijzend naar de kleur van de uniformen.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De burgemeester wil meer <w>blauw</w> op straat om de veiligheid te vergroten.",
                            "target_lang_translations": {
                                "ru": "Мэр хочет видеть больше <w>полиции</w> на улицах для повышения безопасности."
                            }
                        },
                        {
                            "text": "Toen het <w>blauw</w> arriveerde, renden de daders snel weg.",
                            "target_lang_translations": {
                                "ru": "Когда прибыла <w>полиция</w>, преступники быстро убежали."
                            }
                        }
                    ],
                    "synonyms": [
                        "politie"
                    ],
                    "common_phrases": [
                        "blauw op straat"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "metonymy used in media and speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Полиция (по цвету униформы)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полиция",
                                "target_lang_sense_clarification": "правоохранительные органы"
                            }
                        ]
                    }
                }
            ]
        },
        {

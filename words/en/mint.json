                    ],
                    "synonyms": [
                        "bundle",
                        "pile",
                        "small fortune"
                    ],
                    "common_phrases": [
                        "cost a mint",
                        "make a mint",
                        "worth a mint"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""

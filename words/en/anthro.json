    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fc88b7fd-d38b-3d77-a871-3260dc35f763",
                    "sense_definition": "The study of human societies, cultures, and their development; a short form of <w>anthropology</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "academic_field",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to major in <anthro> because she is fascinated by ancient cultures.",
                            "target_lang_translations": {
                                "ru": "Она решила специализироваться на <w>антропологии</w>, так как она очарована древними культурами."
                            }
                        },
                        {
                            "text": "My <anthro> professor assigned three books for us to read this semester.",
                            "target_lang_translations": {
                                "ru": "Мой профессор <w>антропологии</w> задал нам прочитать три книги в этом семестре."
                            }
                        }
                    ],
                    "synonyms": [
                        "anthropology"
                    ],
                    "common_phrases": [
                        "anthro class",
                        "anthro major",
                        "cultural anthro"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изучение человеческих обществ, культур и их развития; сокращение от «антропология»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "антропология",
                                "target_lang_sense_clarification": "научная дисциплина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2cccbd4-1d9c-3e47-bcc2-466eca92ad6d",
                    "sense_definition": "An anthropomorphic character, typically an animal with human traits such as walking on two legs or speaking.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fandom_character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The artist draws amazing <anthros> that look like wolves wearing medieval armor.",
                            "target_lang_translations": {
                                "ru": "Художник рисует удивительных <w>антро</w>, которые выглядят как волки в средневековых доспехах."
                            }
                        },
                        {
                            "text": "Many cartoons feature <anthros> as the main protagonists instead of humans.",
                            "target_lang_translations": {
                                "ru": "Во многих мультфильмах главными героями выступают <w>антро</w>, а не люди."
                            }
                        }
                    ],
                    "synonyms": [
                        "furry"
                    ],
                    "antonyms": [
                        "feral"
                    ],
                    "common_phrases": [
                        "wolf anthro",
                        "anthro character",
                        "anthro art"
                    ],
                    "target_lang_definitions": {
                        "ru": "Антропоморфный персонаж, обычно животное с человеческими чертами, такими как хождение на двух ногах или речь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "антро",
                                "target_lang_sense_clarification": "персонаж (сленг)"
                            },
                            {
                                "target_lang_word": "антропоморф",
                                "target_lang_sense_clarification": "существо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "947fa53f-7f38-3089-8fc8-4ed9d63b5765",
                    "sense_definition": "A person who studies human history and physiological characteristics; a short form of <w>anthropologist</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "academic_field",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <anthros> gathered at the conference to discuss their recent field studies.",
                            "target_lang_translations": {
                                "ru": "<w>Антропологи</w> собрались на конференции, чтобы обсудить свои недавние полевые исследования."
                            }
                        },
                        {
                            "text": "Local <anthros> have been documenting the tribe's customs for decades.",
                            "target_lang_translations": {
                                "ru": "Местные <w>антропологи</w> десятилетиями документируют обычаи племени."
                            }
                        }
                    ],
                    "synonyms": [
                        "anthropologist"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, изучающий историю человечества и физиологические особенности; сокращение от «антрополог»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "антрополог",
                                "target_lang_sense_clarification": "ученый"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1b5a74ed-c7fd-39eb-884d-46b61aa85756",
                    "sense_definition": "Having human characteristics or form, usually referring to animals; short for <w>anthropomorphic</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fandom_character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He enjoys drawing <anthro> animals in his sketchbook during his free time.",
                            "target_lang_translations": {
                                "ru": "В свободное время ему нравится рисовать <w>антропоморфных</w> животных в своем скетчбуке."
                            }
                        },
                        {
                            "text": "The movie features <anthro> cats living in a modern city environment.",
                            "target_lang_translations": {
                                "ru": "В фильме показаны <w>антропоморфные</w> коты, живущие в современном городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "anthropomorphic",
                        "humanlike"
                    ],
                    "antonyms": [
                        "feral"
                    ],
                    "common_phrases": [
                        "anthro art",
                        "anthro style"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий человеческими чертами или формой, обычно относится к животным; сокращение от «антропоморфный»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "антропоморфный",
                                "target_lang_sense_clarification": "человекоподобный"
                            },
                            {
                                "target_lang_word": "антро",
                                "target_lang_sense_clarification": "стиль или характеристика"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
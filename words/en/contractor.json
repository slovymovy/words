    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c3ba5657-629f-38e4-96a7-fee30b8847ec",
                    "sense_definition": "A person or company that builds, improves, or maintains buildings and structures.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "construction_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hired a <w>contractor</w> to renovate the kitchen and install new cabinets.",
                            "target_lang_translations": {
                                "ru": "Мы наняли <w>подрядчика</w> для ремонта кухни и установки новых шкафов."
                            }
                        },
                        {
                            "text": "The general <w>contractor</w> is responsible for managing all the workers on the construction site.",
                            "target_lang_translations": {
                                "ru": "Генеральный <w>подрядчик</w> отвечает за руководство всеми рабочими на строительной площадке."
                            }
                        }
                    ],
                    "synonyms": [
                        "builder"
                    ],
                    "common_phrases": [
                        "general contractor",
                        "building contractor",
                        "licensed contractor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек или компания, которые строят, ремонтируют или обслуживают здания и сооружения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подрядчик",
                                "target_lang_sense_clarification": "строительство и ремонт"
                            },
                            {
                                "target_lang_word": "строительный подрядчик",
                                "target_lang_sense_clarification": "уточнение сферы деятельности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6402a05c-b0a7-3bc2-bc33-8d8ec5a25c58",
                    "sense_definition": "A person hired to do a specific job on a business contract rather than as a permanent employee.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "employment_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She works as an independent <w>contractor</w>, which gives her the flexibility to choose her own projects.",
                            "target_lang_translations": {
                                "ru": "Она работает как независимый <w>подрядчик</w>, что дает ей свободу в выборе собственных проектов."
                            }
                        },
                        {
                            "text": "The company employs a <w>contractor</w> to handle their IT support instead of hiring full-time staff.",
                            "target_lang_translations": {
                                "ru": "Компания привлекает <w>подрядчика</w> для техподдержки вместо того, чтобы нанимать штатных сотрудников."
                            }
                        }
                    ],
                    "synonyms": [
                        "freelancer"
                    ],
                    "antonyms": [
                        "employee",
                        "staff"
                    ],
                    "common_phrases": [
                        "independent contractor",
                        "private contractor",
                        "defense contractor",
                        "government contractor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, нанятое для выполнения определенной работы по контракту, а не в качестве постоянного сотрудника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подрядчик",
                                "target_lang_sense_clarification": "сторона в договоре"
                            },
                            {
                                "target_lang_word": "исполнитель",
                                "target_lang_sense_clarification": "официальное название в контракте"
                            },
                            {
                                "target_lang_word": "внештатный сотрудник",
                                "target_lang_sense_clarification": "фрилансер"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [

                    "sense_id": "326ca691-6a24-30d5-b2a6-71f25b6bc7c7",
                    "sense_definition": "The ability to use both hands with equal skill.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_skill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pianist's <w>ambidexterity</w> allowed him to play complex melodies with either hand.",
                            "target_lang_translations": {
                                "ru": "<w>Амбидекстрия</w> пианиста позволяла ему играть сложные мелодии любой рукой."
                            }
                        },
                        {
                            "text": "She practiced writing with her left hand to achieve <w>ambidexterity</w>.",
                            "target_lang_translations": {
                                "ru": "Она тренировалась писать левой рукой, чтобы развить <w>амбидекстрию</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ambidextrousness"
                    ],
                    "common_phrases": [

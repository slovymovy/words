            "pos": "verb",
            "senses": [
                {
                    "sense_id": "064d78c2-1480-36b1-bf27-3019dd5fb582",
                    "sense_definition": "Найти решение проблемы, создать что-то новое мысленно или изобрести.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Никто ещё не <придумал> машину времени, чтобы путешествовать в прошлое.",
                            "target_lang_translations": {
                                "en": "No one has <invented> a time machine to travel to the past yet."
                            }
                        },
                        {
                            "text": "Нам нужно срочно <придумать> план, как выбраться из этой ситуации.",
                            "target_lang_translations": {
                                "en": "We urgently need to <come up with> a plan on how to get out of this situation."
                            }
                        }
                    ],
                    "synonyms": [
                        "изобрести",
                        "сообразить",
                        "решить"
                    ],
                    "common_phrases": [
                        "придумать план",
                        "придумать способ",
                        "придумать название",
                        "придумать решение"
                    ],
                    "target_lang_definitions": {
                        "en": "To find a solution to a problem, mentally create something new, or invent."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come up with",
                                "target_lang_sense_clarification": "a plan, idea, or solution"
                            },
                            {
                                "target_lang_word": "invent",
                                "target_lang_sense_clarification": "a device or mechanism"
                            },
                            {
                                "target_lang_word": "think up",
                                "target_lang_sense_clarification": "an idea"
                            },
                            {
                                "target_lang_word": "devise",
                                "target_lang_sense_clarification": "a method or strategy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65a8ac5a-eead-386c-81f8-515fb20157e4",
                    "sense_definition": "Сочинить или создать в воображении то, чего не было на самом деле.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Писатель <придумал> интересный сюжет для своего нового фантастического романа.",
                            "target_lang_translations": {
                                "en": "The writer <thought up> an interesting plot for his new sci-fi novel."
                            }
                        },
                        {
                            "text": "Не нужно <придумывать> оправдания, лучше просто скажи мне правду.",
                            "target_lang_translations": {
                                "en": "You don't need to <make up> excuses, better just tell me the truth."
                            }
                        }
                    ],
                    "synonyms": [
                        "сочинить",
                        "выдумать",
                        "вообразить"
                    ],
                    "common_phrases": [
                        "придумать историю",
                        "придумать сказку",
                        "придумать причину",
                        "придумать отговорку"
                    ],
                    "target_lang_definitions": {
                        "en": "To compose or create something in the imagination that did not actually exist."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "make up",
                                "target_lang_sense_clarification": "a story, excuse, or lie"
                            },
                            {
                                "target_lang_word": "invent",
                                "target_lang_sense_clarification": "something fictional"
                            },
                            {
                                "target_lang_word": "think up",
                                "target_lang_sense_clarification": "something imaginary"
                            }
                        ]
                    }
                }
            ]

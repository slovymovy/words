    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "dac1b21b-e8e9-3c31-a5b6-f08270e8e3e9",
                    "sense_definition": "Taki, który nie jest znany; nieznajomy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_unknown",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na ulicy zaczepił mnie jakiś <obcy> człowiek i zapytał o drogę.",
                            "target_lang_translations": {
                                "ru": "На улице ко мне подошел какой-то <незнакомый> человек и спросил дорогу."
                            }
                        },
                        {
                            "text": "To miasto jest mi zupełnie <obce>, bo nigdy wcześniej tu nie byłem.",
                            "target_lang_translations": {
                                "ru": "Этот город мне совершенно <незнаком>, так как я никогда раньше здесь не был."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieznany",
                        "nieznajomy"
                    ],
                    "antonyms": [
                        "znany",
                        "bliski",
                        "swój"
                    ],
                    "common_phrases": [
                        "obcy człowiek",
                        "obcy głos",
                        "obcy kraj"
                    ],
                    "target_lang_definitions": {
                        "ru": "Такой, который не известен; незнакомый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "незнакомый",
                                "target_lang_sense_clarification": "неизвестный"
                            },
                            {
                                "target_lang_word": "чужой",
                                "target_lang_sense_clarification": "не близкий, посторонний"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81a3c5a8-5e00-343f-8664-33d28e5d0eb1",
                    "sense_definition": "Nienależący do danej grupy, narodu lub rodziny; pochodzący z zewnątrz lub będący własnością kogoś innego.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_foreign",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W szkole uczymy się dwóch <obcych> języków.",
                            "target_lang_translations": {
                                "ru": "В школе мы учим два <иностранных> языка."
                            }
                        },
                        {
                            "text": "Nie powinieneś wtrącać się w <obce> sprawy, jeśli cię nie dotyczą.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует вмешиваться в <чужие> дела, если они тебя не касаются."
                            }
                        }
                    ],
                    "synonyms": [
                        "cudzy"
                    ],
                    "antonyms": [
                        "własny",
                        "rodzimy",
                        "swój"
                    ],
                    "common_phrases": [
                        "obcy język",
                        "obcy kapitał",
                        "obca waluta",
                        "obce sprawy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не принадлежащий к данной группе, народу или семье; происходящий извне или принадлежащий кому-то другому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чужой",
                                "target_lang_sense_clarification": "принадлежащий другим"
                            },
                            {
                                "target_lang_word": "иностранный",
                                "target_lang_sense_clarification": "зарубежный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41a99486-c74c-347a-9ee6-760c0d7ed337",
                    "sense_definition": "Pochodzący z innej planety; pozaziemski.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_alien",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W filmie science-fiction Ziemię zaatakowała <obca> cywilizacja.",
                            "target_lang_translations": {
                                "ru": "В научно-фантастическом фильме Землю атаковала <внеземная> цивилизация."
                            }
                        },
                        {
                            "text": "Naukowcy wciąż poszukują śladów <obcej> inteligencji w kosmosie.",
                            "target_lang_translations": {
                                "ru": "Ученые все еще ищут следы <внеземного> разума в космосе."
                            }
                        }
                    ],
                    "antonyms": [
                        "ziemski"
                    ],
                    "common_phrases": [
                        "obca cywilizacja",
                        "obca planeta",
                        "obca forma życia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий с другой планеты; внеземной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внеземной",
                                "target_lang_sense_clarification": "космический"
                            },
                            {
                                "target_lang_word": "инопланетный",
                                "target_lang_sense_clarification": "с другой планеты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9403da4-f8a8-332d-953d-f32f9126803a",
                    "sense_definition": "Niewłaściwy w danym miejscu; nienaturalny dla danego otoczenia lub organizmu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_inappropriate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz musiał usunąć ciało <obce> z oka pacjenta.",
                            "target_lang_translations": {
                                "ru": "Врач должен был удалить <инородное> тело из глаза пациента."
                            }
                        },
                        {
                            "text": "Ten nowoczesny biurowiec jest <obcym> elementem w zabytkowej części miasta.",
                            "target_lang_translations": {
                                "ru": "Это современное офисное здание является <чужеродным> элементом в исторической части города."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ciało obce",
                        "obcy element"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неуместный в данном месте; неестественный для данной среды или организма."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "инородный",
                                "target_lang_sense_clarification": "о теле или предмете"
                            },
                            {
                                "target_lang_word": "чужеродный",
                                "target_lang_sense_clarification": "несвойственный элементу"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d8093d89-0db1-3dc8-95fc-20835eb084c7",
                    "sense_definition": "Osoba nieznana lub nienależąca do danej grupy społecznej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_unknown",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na teren zakładu pracy <obcym> wstęp jest surowo wzbroniony.",
                            "target_lang_translations": {
                                "ru": "На территорию предприятия вход <посторонним> строго воспрещен."
                            }
                        },
                        {
                            "text": "Dzieciom nie wolno otwierać drzwi <obcym>, gdy są same w domu.",
                            "target_lang_translations": {
                                "ru": "Детям нельзя открывать дверь <чужим>, когда они одни дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieznajomy",
                        "osoba postronna"
                    ],
                    "antonyms": [
                        "swój",
                        "znajomy"
                    ],
                    "common_phrases": [
                        "wstęp dla obcych",
                        "rozmowa z obcym"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неизвестный человек или тот, кто не принадлежит к данной социальной группе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посторонний",
                                "target_lang_sense_clarification": "чужой человек, не свой"
                            },
                            {
                                "target_lang_word": "чужой",
                                "target_lang_sense_clarification": "незнакомец"
                            },
                            {
                                "target_lang_word": "незнакомец",
                                "target_lang_sense_clarification": "неизвестный человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4eed5d99-bed9-391c-bb69-b20228fc6da5",
                    "sense_definition": "Istota pozaziemska; przybysz z innej planety.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_alien",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielu ludzi wierzy, że <obcy> odwiedzili Ziemię w starożytności.",
                            "target_lang_translations": {
                                "ru": "Многие люди верят, что <пришельцы> посещали Землю в древности."
                            }
                        },
                        {
                            "text": "Główny bohater filmu walczy z <obcymi> na pokładzie statku kosmicznego.",
                            "target_lang_translations": {
                                "ru": "Главный герой фильма сражается с <пришельцами> на борту космического корабля."
                            }
                        }
                    ],
                    "synonyms": [
                        "kosmita",
                        "ufoludek"
                    ],
                    "antonyms": [
                        "Ziemianin"
                    ],
                    "common_phrases": [
                        "atak obcych",
                        "inwazja obcych",
                        "spotkanie z obcym"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внеземное существо; пришелец с другой планеты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пришелец",
                                "target_lang_sense_clarification": "существо из космоса"
                            },
                            {
                                "target_lang_word": "инопланетянин",
                                "target_lang_sense_clarification": "житель другой планеты"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
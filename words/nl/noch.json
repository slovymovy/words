            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "a9f22a45-a75a-3d91-aee3-c53aab03f339",
                    "sense_definition": "Niet het een en ook niet het ander; gebruikt in de constructie 'noch ... noch' om meerdere mogelijkheden tegelijk te ontkennen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "correlative_negation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb <w>noch</w> tijd <w>noch</w> geld voor een lange vakantie.",
                            "target_lang_translations": {
                                "en": "I have <w>neither</w> time <w>nor</w> money for a long vacation."
                            }
                        },
                        {
                            "text": "Het gerecht was <w>noch</w> warm <w>noch</w> koud toen het werd geserveerd.",
                            "target_lang_translations": {
                                "en": "The dish was <w>neither</w> hot <w>nor</w> cold when it was served."
                            }
                        }
                    ],
                    "antonyms": [
                        "zowel",
                        "of"
                    ],
                    "common_phrases": [
                        "noch vlees noch vis",
                        "noch min noch meer"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Not the one nor the other; used in the construction 'neither ... nor' to negate multiple possibilities simultaneously."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "neither",
                                "target_lang_sense_clarification": "used in 'neither ... nor'"
                            },
                            {
                                "target_lang_word": "nor",
                                "target_lang_sense_clarification": "used in 'neither ... nor'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05d04c51-36de-37fe-9b77-3a76d7240104",
                    "sense_definition": "En evenmin; gebruikt na een eerdere ontkenning om een nieuwe ontkenning in te leiden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "continuation_negation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij wilde niet luisteren, <w>noch</w> wilde hij praten over het probleem.",
                            "target_lang_translations": {
                                "en": "He did not want to listen, <w>nor</w> did he want to talk about the problem."
                            }
                        },
                        {
                            "text": "Ik heb het document niet gezien, <w>noch</w> heb ik het ondertekend.",
                            "target_lang_translations": {
                                "en": "I have not seen the document, <w>nor</w> have I signed it."
                            }
                        }
                    ],
                    "synonyms": [
                        "evenmin"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "And neither; used after a previous negation to introduce a new negation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nor",
                                "target_lang_sense_clarification": "continuation of negation"
                            },
                            {
                                "target_lang_word": "neither",
                                "target_lang_sense_clarification": "continuation of negation"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
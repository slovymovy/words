{
    "word_family": [
        "unwanted",
        "wanting"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ff3a826d-ecb6-3f33-8456-f8cf7c2588aa",
                    "sense_definition": "To feel a desire or wish for something.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "desire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>want</w> to go home now.",
                            "target_lang_translations": {
                                "ru": "Я <w>хочу</w> пойти домой сейчас."
                            }
                        },
                        {
                            "text": "Do you <w>want</w> some coffee?",
                            "target_lang_translations": {
                                "ru": "Ты <w>хочешь</w> кофе?"
                            }
                        }
                    ],
                    "synonyms": [
                        "wish for",
                        "would like",
                        "desire",
                        "crave",
                        "fancy",
                        "wish"
                    ],
                    "antonyms": [
                        "dislike",
                        "hate"
                    ],
                    "common_phrases": [
                        "want to",
                        "want something",
                        "really want"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствовать желание или стремление к чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хотеть",
                                "target_lang_sense_clarification": "иметь желание"
                            },
                            {
                                "target_lang_word": "желать",
                                "target_lang_sense_clarification": "более формально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffd571d3-61c6-328e-996a-09596896b876",
                    "sense_definition": "To be advised or encouraged to do something; used to give a suggestion.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "advice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You will <w>want</w> to arrive early to get a good seat.",
                            "target_lang_translations": {
                                "ru": "Вам <w>стоит</w> приехать пораньше, чтобы занять хорошее место."
                            }
                        },
                        {
                            "text": "You might <w>want</w> to bring an umbrella just in case.",
                            "target_lang_translations": {
                                "ru": "Возможно, вам <w>стоит</w> взять зонт на всякий случай."
                            }
                        }
                    ],
                    "common_phrases": [
                        "you will want",
                        "might want to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь необходимость или рекомендацию сделать что-либо (используется для совета)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стоить",
                                "target_lang_sense_clarification": "быть полезным или нужным (в советах)"
                            },
                            {
                                "target_lang_word": "следует",
                                "target_lang_sense_clarification": "рекомендация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "256266e5-e8c1-33e2-9e85-9af2e3edd0f3",
                    "sense_definition": "To feel sexual desire for someone.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "desire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He knew he <w>wanted</w> her the moment they met.",
                            "target_lang_translations": {
                                "ru": "Он знал, что <w>хочет</w> её, с того момента, как они встретились."
                            }
                        },
                        {
                            "text": "She <w>wants</w> him so bad.",
                            "target_lang_translations": {
                                "ru": "Она так сильно его <w>хочет</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "desire",
                        "fancy",
                        "crave"
                    ],
                    "common_phrases": [
                        "want someone bad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать сексуальное влечение к кому-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хотеть",
                                "target_lang_sense_clarification": "испытывать влечение"
                            },
                            {
                                "target_lang_word": "желать",
                                "target_lang_sense_clarification": "вожделеть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca1512fe-02e6-3c20-81f4-19721ee32aa7",
                    "sense_definition": "To require or need a particular action to be done to it.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "need",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The grass <w>wants</w> cutting before the rain starts.",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "afb9ed84-507e-33fc-8d68-c7999d3e0bd1",
                    "sense_definition": "A female deity.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient Greeks built many temples to honor the <w>goddess</w> Athena.",
                            "target_lang_translations": {
                                "ru": "Древние греки построили множество храмов в честь <w>богини</w> Афины."
                            }
                        },
                        {
                            "text": "She is often depicted in art as a powerful <w>goddess</w> of war and wisdom.",
                            "target_lang_translations": {
                                "ru": "В искусстве её часто изображают как могущественную <w>богиню</w> войны и мудрости."
                            }
                        }
                    ],
                    "synonyms": [
                        "deity",
                        "divinity"
                    ],
                    "antonyms": [
                        "god"
                    ],
                    "common_phrases": [
                        "Greek goddess",
                        "goddess of love",
                        "fertility goddess",
                        "river goddess"
                    ],
                    "target_lang_definitions": {
                        "ru": "Божество женского пола."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "богиня",
                                "target_lang_sense_clarification": "женское божество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8eee846d-7c38-38f9-826a-2a24e45c68da",
                    "sense_definition": "A woman honored or adored as physically attractive or of superior charm and intelligence.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "With her elegant dress and confident smile, she looked like a <w>goddess</w> on the red carpet.",
                            "target_lang_translations": {
                                "ru": "В своём элегантном платье и с уверенной улыбкой она выглядела как <w>богиня</w> на красной дорожке."
                            }
                        },
                        {
                            "text": "He treats his wife like a <w>goddess</w>, bringing her flowers and gifts every week.",
                            "target_lang_translations": {
                                "ru": "Он относится к жене как к <w>богине</w>, принося ей цветы и подарки каждую неделю."
                            }
                        }
                    ],
                    "synonyms": [
                        "idol",
                        "diva",
                        "beauty",
                        "queen"
                    ],
                    "common_phrases": [
                        "domestic goddess",
                        "screen goddess",
                        "sex goddess"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина, которой восхищаются за её красоту, обаяние или ум."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "богиня",
                                "target_lang_sense_clarification": "перен. красавица"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "7236af62-298c-30c9-b39d-455c538a9d54",
                    "sense_definition": "The primary female deity in Wicca and various other neopagan or duotheistic religions, often contrasted with the God.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Many modern pagans worship the <w>Goddess</w> as the source of all life and nature.",
                            "target_lang_translations": {
                                "ru": "Многие современные язычники поклоняются <w>Богине</w> как источнику всей жизни и природы."
                            }
                        },
                        {
                            "text": "The ritual was performed under the full moon to invoke the blessings of the <w>Goddess</w>.",
                            "target_lang_translations": {
                                "ru": "Ритуал проводился в полнолуние, чтобы призвать благословение <w>Богини</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "The Lady"
                    ],
                    "antonyms": [
                        "The God"
                    ],
                    "common_phrases": [
                        "The Goddess"
                    ],
                    "target_lang_definitions": {
                        "ru": "Главное женское божество в викке и других неоязыческих религиях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Богиня",
                                "target_lang_sense_clarification": "в неоязычестве"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
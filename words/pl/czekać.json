            "senses": [
                {
                    "sense_id": "4ace7d24-485e-3d8a-a535-04b62e3f38d4",
                    "sense_definition": "Pozostawać w jednym miejscu lub nie podejmować działania do momentu, aż coś się wydarzy lub ktoś przyjdzie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "waiting_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy cierpliwie <czekać> na przystanku, aż przyjedzie nasz autobus."
                        },
                        {
                            "text": "Nie lubię <czekać> w długich kolejkach do kasy w supermarkecie."
                        }
                    ],
                    "synonyms": [
                        "oczekiwać",
                        "wyczekiwać"
                    ],
                    "antonyms": [
                        "spieszyć się"
                    ],
                    "common_phrases": [
                        "czekać na autobus",
                        "czekać w kolejce",
                        "kazać na siebie czekać",
                        "czekać na telefon"
                    ]
                },
                {
                    "sense_id": "30456d6a-a59e-3ddc-8fbd-72f91e02eaa8",
                    "sense_definition": "Przerywać wykonywanie jakiejś czynności lub wstrzymywać się z nią na chwilę; często używane w trybie rozkazującym, aby kogoś zatrzymać.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "imperative_stop",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Czekaj>, nie możesz wejść do środka bez ważnego biletu!"
                        },
                        {
                            "text": "<Czekajcie>, chyba zapomniałem wziąć kluczyków do samochodu z biurka."
                        }
                    ],
                    "common_phrases": [
                        "czekaj chwilę",
                        "czekaj no"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used as an interjection"
                        }
                    ]
                },
                {
                    "sense_id": "d2a4d0ea-2769-3907-8454-2c1ed0b9f635",
                    "sense_definition": "Spodziewać się nadejścia kogoś lub wystąpienia czegoś, często z nadzieją, niepokojem lub tęsknotą.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "waiting_mental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z wielką niecierpliwością <czekamy> na oficjalne wyniki egzaminu końcowego."
                        },
                        {
                            "text": "Cała rodzina <czekała> na powrót ojca z dalekiej podróży służbowej."
                        }
                    ],
                    "synonyms": [
                        "oczekiwać",
                        "wyglądać"
                    ],
                    "common_phrases": [
                        "czekać na cud",
                        "czekać na okazję",
                        "czekać z niecierpliwością",
                        "czekać na list"
                    ]
                },
                {
                    "sense_id": "daa52724-0075-3a77-b91a-e654a1623fdf",
                    "sense_definition": "Być gotowym do użycia, dostępnym lub przygotowanym dla kogoś (o rzeczach).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "waiting_objects",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gorący obiad już <czeka> na stole, więc chodźcie szybko jeść."
                        },
                        {
                            "text": "Na parkingu przed lotniskiem <czekała> na nas zamówiona wcześniej taksówka."
                        }
                    ],
                    "common_phrases": [

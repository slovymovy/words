    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "16d9436b-c1a1-3600-b17f-614242932bcb",
                    "sense_definition": "Iemand die bij een gebeurtenis aanwezig was en kan vertellen wat er gebeurd is, of iemand die in een rechtszaak een verklaring aflegt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "person_witness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie zoekt een <w>getuige</w> die het ongeluk heeft zien gebeuren.",
                            "target_lang_translations": {
                                "en": "The police are looking for a <w>witness</w> who saw the accident happen.",
                                "ru": "Полиция ищет <w>свидетеля</w>, который видел, как произошла авария."
                            }
                        },
                        {
                            "text": "Mijn beste vriend was <w>getuige</w> bij mijn huwelijk op het stadhuis.",
                            "target_lang_translations": {
                                "en": "My best friend was a <w>witness</w> at my wedding at the city hall.",
                                "ru": "Мой лучший друг был <w>свидетелем</w> на моей свадьбе в мэрии."
                            }
                        }
                    ],
                    "synonyms": [
                        "ooggetuige"
                    ],
                    "common_phrases": [
                        "getuige zijn van",
                        "oproepen als getuige",
                        "kroongetuige",
                        "valse getuige"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who was present at an event and can tell what happened, or someone who gives a statement in a lawsuit.",
                        "ru": "Тот, кто присутствовал при событии и может рассказать о случившемся, или тот, кто дает показания в суде."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "witness",
                                "target_lang_sense_clarification": "person who sees an event or testifies in court"
                            },
                            {
                                "target_lang_word": "eyewitness",
                                "target_lang_sense_clarification": "person who has personally seen an event"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "свидетель",
                                "target_lang_sense_clarification": "человек, располагающий сведениями об обстоятельствах дела"
                            },
                            {
                                "target_lang_word": "очевидец",
                                "target_lang_sense_clarification": "тот, кто видел событие своими глазами"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "a1fa5558-1186-3715-99e6-b9f26938125c",
                    "sense_definition": "Zoals blijkt uit; zoals aangetoond wordt door.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "logic_evidence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf heeft een goed jaar achter de rug, <w>getuige</w> de hoge winstcijfers.",
                            "target_lang_translations": {
                                "en": "The company has had a good year, <w>witness</w> the high profit figures.",
                                "ru": "У компании был хороший год, <w>о чём свидетельствуют</w> высокие показатели прибыли."
                            }
                        },
                        {
                            "text": "Hij heeft hard gewerkt in de tuin, <w>getuige</w> zijn vuile handen.",
                            "target_lang_translations": {
                                "en": "He has worked hard in the garden, <w>witness</w> his dirty hands.",
                                "ru": "Он усердно работал в саду, <w>о чём свидетельствуют</w> его грязные руки."
                            }
                        }
                    ],
                    "synonyms": [
                        "blijkens",
                        "gezien"
                    ],
                    "common_phrases": [
                        "getuige de feiten",
                        "getuige het feit dat"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "As shown by; as demonstrated by.",
                        "ru": "Как видно из; как подтверждается фактами (используется для введения доказательства)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "witness",
                                "target_lang_sense_clarification": "used to introduce evidence or proof"
                            },
                            {
                                "target_lang_word": "as evidenced by",
                                "target_lang_sense_clarification": "phrase indicating proof"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "о чём свидетельствует",
                                "target_lang_sense_clarification": "указание на доказательство"
                            },
                            {
                                "target_lang_word": "судя по",
                                "target_lang_sense_clarification": "вывод на основе фактов"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e233fdb6-668e-3747-993c-8b31dfa0a605",
                    "sense_definition": "Enkelvoud tegenwoordige tijd aanvoegende wijs van <w>getuigen</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moge de toekomst <w>getuige</w> van onze goede bedoelingen.",
                            "target_lang_translations": {
                                "en": "May the future <w>bear witness</w> to our good intentions.",
                                "ru": "Пусть будущее <w>будет свидетелем</w> наших добрых намерений."
                            }
                        },
                        {
                            "text": "God <w>getuige</w> dat ik de waarheid spreek.",
                            "target_lang_translations": {
                                "en": "God <w>witness</w> that I speak the truth.",
                                "ru": "Бог <w>свидетель</w>, что я говорю правду."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",

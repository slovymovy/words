{
    "word_family": [
        "minachten",
        "minachting"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "927856bc-1ea9-33a7-af08-882675032bc0",
                    "sense_definition": "Getuigend van een gebrek aan respect; het gevoel tonend dat iemand of iets minderwaardig is.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij wierp haar een <w>minachtende</w> blik toe toen ze een domme vraag stelde.",
                            "target_lang_translations": {
                                "en": "He gave her a <w>disdainful</w> look when she asked a stupid question."
                            }
                        },
                        {
                            "text": "Met een <w>minachtend</w> gebaar schoof hij het bord eten van zich af.",
                            "target_lang_translations": {
                                "en": "With a <w>contemptuous</w> gesture, he pushed the plate of food away."
                            }
                        },
                        {
                            "text": "Haar <w>minachtende</w> houding zorgde voor veel irritatie bij haar collega's.",
                            "target_lang_translations": {
                                "en": "Her <w>sneering</w> attitude caused a lot of irritation among her colleagues."
                            }
                        }
                    ],
                    "synonyms": [
                        "neerbuigend",
                        "geringschattend",
                        "denigrerend",
                        "respectloos",
                        "kleinerend"
                    ],
                    "antonyms": [
                        "hoogachtend"
                    ],
                    "common_phrases": [
                        "een minachtende blik",
                        "minachtend snuiven",
                        "een minachtend gebaar",
                        "minachtende opmerkingen"
                    ],
                    "target_lang_definitions": {
                        "en": "Showing a lack of respect; expressing the feeling that someone or something is inferior."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "contemptuous",
                                "target_lang_sense_clarification": "showing that you feel someone or something is unimportant or deserves no respect"
                            },
                            {
                                "target_lang_word": "disdainful",
                                "target_lang_sense_clarification": "showing that you believe someone or something is not good enough to deserve your respect"
                            },
                            {
                                "target_lang_word": "sneering",
                                "target_lang_sense_clarification": "smiling or speaking in a mocking, disrespectful manner"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "09032226-433e-3a34-9f58-7b46616d890a",
                    "sense_definition": "Onvoltooid deelwoord van <w>minachten</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij liep dwars door het rode licht, de verkeersregels volledig <w>minachtend</w>.",
                            "target_lang_translations": {
                                "en": "He walked straight through the red light, completely <w>disregarding</w> the traffic rules."
                            }
                        },
                        {
                            "text": "De dictator regeerde met harde hand, de rechten van zijn volk <w>minachtend</w>.",
                            "target_lang_translations": {
                                "en": "The dictator ruled with an iron fist, <w>despising</w> the rights of his people."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Present participle of <w>to despise</w>."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "disregarding",
                                "target_lang_sense_clarification": "ignoring something, especially rules or standards"
                            },
                            {
                                "target_lang_word": "despising",
                                "target_lang_sense_clarification": "feeling a strong dislike and lack of respect for someone or something"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "sense_id": "11847915-9538-3d35-a6b5-a85d0d116b04",
                    "sense_definition": "Een gewerveld dier met kieuwen en vinnen dat in het water leeft.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De goudkleurige <w>vis</w> zwemt rustig rond in de kom.",
                            "target_lang_translations": {
                                "ru": "Золотая <w>рыбка</w> спокойно плавает в аквариуме."
                            }
                        },
                        {
                            "text": "Hij voelt zich als een <w>vis</w> in het water op zijn nieuwe school.",
                            "target_lang_translations": {
                                "ru": "В новой школе он чувствует себя как <w>рыба</w> в воде."
                            }
                        }
                    ],
                    "common_phrases": [
                        "als een vis in het water",
                        "boter bij de vis",
                        "grote vissen",
                        "kleine visjes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Позвоночное животное с жабрами и плавниками, живущее в воде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рыба",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]

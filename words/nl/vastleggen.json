                },
                {
                    "sense_id": "8a0c558a-990e-32f7-b47c-e3ea99d8bbf7",
                    "sense_definition": "Officieel bepalen en schriftelijk bevestigen, zoals regels of afspraken",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "establishing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het contract staat <w>vastgelegd</w> hoeveel uur je moet werken.",
                            "target_lang_translations": {
                                "ru": "В контракте <w>закреплено</w>, сколько часов вы должны работать."
                            }
                        },
                        {
                            "text": "De overheid heeft nieuwe regels voor het verkeer <w>vastgelegd</w>.",
                            "target_lang_translations": {
                                "ru": "Правительство <w>установило</w> новые правила дорожного движения."
                            }
                        }
                    ],
                    "synonyms": [
                        "bepalen",
                        "vaststellen"
                    ],
                    "common_phrases": [

    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "27eced12-7302-34a9-804d-4ca271c4e5af",
                    "sense_definition": "Получить что-либо в собственность в обмен на деньги.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <купить> новую машину в следующем году.",
                            "target_lang_translations": {
                                "en": "We decided to <buy> a new car next year."
                            }
                        },
                        {
                            "text": "По дороге домой мне нужно <купить> хлеб и молоко.",
                            "target_lang_translations": {
                                "en": "On the way home I need to <buy> bread and milk."
                            }
                        }
                    ],
                    "synonyms": [
                        "приобрести"
                    ],
                    "antonyms": [
                        "продать"
                    ],
                    "common_phrases": [
                        "купить билет",
                        "купить подарок",
                        "купить продукты",
                        "купить квартиру",
                        "хочу купить"
                    ],
                    "target_lang_definitions": {
                        "en": "To obtain something in exchange for money."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "buy",
                                "target_lang_sense_clarification": "general term"
                            },
                            {
                                "target_lang_word": "purchase",
                                "target_lang_sense_clarification": "more formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e662a614-05b0-35da-a65b-dd2fdf078084",
                    "sense_definition": "Привлечь кого-либо на свою сторону взяткой или подарками; заставить действовать в своих интересах.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Преступники пытались <купить> судью, но он отказался от денег.",
                            "target_lang_translations": {
                                "en": "The criminals tried to <buy> the judge, but he refused the money."
                            }
                        },
                        {
                            "text": "Его честность известна всем, его невозможно <купить>.",
                            "target_lang_translations": {
                                "en": "His honesty is known to everyone; he cannot be <bought>."
                            }
                        }
                    ],
                    "synonyms": [
                        "подкупить"
                    ],
                    "common_phrases": [
                        "купить молчание",
                        "купить чиновника"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To win someone over through bribery or gifts; to corrupt someone to act in one's interest."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bribe",
                                "target_lang_sense_clarification": "to give money illegally"
                            },
                            {
                                "target_lang_word": "buy",
                                "target_lang_sense_clarification": "figurative, to corrupt"
                            },
                            {
                                "target_lang_word": "buy off",
                                "target_lang_sense_clarification": "to pay someone to stop opposing you"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "107ed4e1-d099-3fc5-8b10-1ba2cf714de3",
                    "sense_definition": "Обмануть, перехитрить или разыграть кого-либо.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я просто пошутил, а ты поверил — я тебя <купил>!",
                            "target_lang_translations": {
                                "en": "I was just joking, and you believed it — I <fooled> you!"
                            }
                        },
                        {
                            "text": "Не пытайся меня <купить> этой старой уловкой.",
                            "target_lang_translations": {
                                "en": "Don't try to <trick> me with that old ploy."
                            }
                        }
                    ],
                    "common_phrases": [
                        "купить на шутку"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To deceive, outsmart, or trick someone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fool",
                                "target_lang_sense_clarification": "to trick or deceive"
                            },
                            {
                                "target_lang_word": "trick",
                                "target_lang_sense_clarification": "to deceive"
                            },
                            {
                                "target_lang_word": "catch out",
                                "target_lang_sense_clarification": "to trap or expose someone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e809ff92-6897-3be7-90db-d937762da531",
                    "sense_definition": "В карточных играх: взять дополнительные карты из колоды.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Игрок решил <купить> две карты, чтобы собрать комбинацию.",
                            "target_lang_translations": {
                                "en": "The player decided to <draw> two cards to complete the combination."
                            }
                        },
                        {
                            "text": "В этой ситуации лучше не <купить> прикуп, а играть своими картами.",
                            "target_lang_translations": {
                                "en": "In this situation, it is better not to <buy> the talon, but to play with one's own cards."
                            }
                        }
                    ],
                    "common_phrases": [
                        "купить прикуп"
                    ],
                    "target_lang_definitions": {
                        "en": "In card games: to take additional cards from the deck."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "draw",
                                "target_lang_sense_clarification": "standard card game term"
                            },
                            {
                                "target_lang_word": "buy",
                                "target_lang_sense_clarification": "specific game terminology"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
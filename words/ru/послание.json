    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5051c50e-dcd1-31ce-865f-d373b96bc91b",
                    "sense_definition": "Письменное обращение или письмо к кому-либо, часто имеющее торжественный или важный характер.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он оставил ей прощальное <w>послание</w> на столе.",
                            "target_lang_translations": {
                                "en": "He left her a farewell <w>message</w> on the table."
                            }
                        },
                        {
                            "text": "В бутылке нашли старинное <w>послание</w> от моряков.",
                            "target_lang_translations": {
                                "en": "An ancient <w>message</w> from sailors was found in the bottle."
                            }
                        }
                    ],
                    "synonyms": [
                        "письмо"
                    ],
                    "common_phrases": [
                        "любовное послание",
                        "короткое послание",
                        "получить послание",
                        "отправить послание"
                    ],
                    "target_lang_definitions": {
                        "en": "A written communication or letter sent to someone, often of a serious, solemn, or significant nature."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "message",
                                "target_lang_sense_clarification": "general written communication"
                            },
                            {
                                "target_lang_word": "letter",
                                "target_lang_sense_clarification": "written document"
                            },
                            {
                                "target_lang_word": "missive",
                                "target_lang_sense_clarification": "formal or official letter"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e4ce0cb-32b1-32d2-9e21-fb3fcdbf3137",
                    "sense_definition": "Официальное обращение государственного или общественного деятеля к другим официальным лицам или широкой аудитории.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Президент огласил ежегодное <w>послание</w> парламенту.",
                            "target_lang_translations": {
                                "en": "The president delivered the annual <w>address</w> to the parliament."
                            }
                        },
                        {
                            "text": "В своём <w>послании</w> губернатор говорил о планах развития региона.",
                            "target_lang_translations": {
                                "en": "In his <w>address</w>, the governor spoke about plans for the region's development."
                            }
                        }
                    ],
                    "common_phrases": [
                        "послание президента",
                        "ежегодное послание",
                        "обратиться с посланием"
                    ],
                    "target_lang_definitions": {
                        "en": "An official statement or speech delivered by a public figure to a specific audience or the general public."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "address",
                                "target_lang_sense_clarification": "formal speech to an assembly"
                            },
                            {
                                "target_lang_word": "message",
                                "target_lang_sense_clarification": "official communication"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "846b3c2d-4b72-3455-8397-8c4e7762bdb7",
                    "sense_definition": "Одно из произведений раннехристианской литературы, представляющее собой обращение апостолов к верующим.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Священник прочитал отрывок из <w>послания</w> апостола Павла.",
                            "target_lang_translations": {
                                "en": "The priest read an excerpt from the <w>Epistle</w> of the Apostle Paul."
                            }
                        },
                        {
                            "text": "Эти <w>послания</w> входят в состав Нового Завета.",
                            "target_lang_translations": {
                                "en": "These <w>epistles</w> are part of the New Testament."
                            }
                        }
                    ],
                    "common_phrases": [
                        "апостольские послания",
                        "послание к римлянам"
                    ],
                    "target_lang_definitions": {
                        "en": "A book of the New Testament in the form of a letter from an Apostle to a group of Christians."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "epistle",
                                "target_lang_sense_clarification": "biblical letter"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b69acd2a-76cf-3620-954d-651b82ad207c",
                    "sense_definition": "Литературное произведение, написанное в форме авторского обращения к какому-либо лицу.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Поэт написал дружеское <w>послание</w> своему коллеге.",
                            "target_lang_translations": {
                                "en": "The poet wrote a friendly <w>epistle</w> to his colleague."
                            }
                        },
                        {
                            "text": "Жанр стихотворного <w>послания</w> был очень популярен в прошлом.",
                            "target_lang_translations": {
                                "en": "The genre of the poetic <w>epistle</w> was very popular in the past."
                            }
                        }
                    ],
                    "common_phrases": [
                        "стихотворное послание",
                        "жанр послания"
                    ],
                    "target_lang_definitions": {
                        "en": "A literary work, typically a poem, written in the form of a letter to a specific person."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "epistle",
                                "target_lang_sense_clarification": "literary genre"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
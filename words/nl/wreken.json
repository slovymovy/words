{
    "word_family": [
        "wraak",
        "wreker",
        "wraakzuchtig"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1f35fcc4-238d-34cb-b8cf-a84765ca74ba",
                    "sense_definition": "Een onrechtmatige daad of misdaad vergelden door de dader te straffen of iets soortgelijks aan te doen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "revenge_retribution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jonge prins zwoer dat hij de dood van zijn vader zou <w>wreken</w>.",
                            "target_lang_translations": {
                                "ru": "Молодой принц поклялся, что <w>отомстит</w> за смерть своего отца."
                            }
                        },
                        {
                            "text": "Zij probeerde het onrecht te <w>wreken</w> dat haar familie was aangedaan.",
                            "target_lang_translations": {
                                "ru": "Она пыталась <w>отомстить</w> за несправедливость, причиненную ее семье."
                            }
                        }
                    ],
                    "synonyms": [
                        "vergelden",
                        "betaald zetten"
                    ],
                    "antonyms": [
                        "vergeven",
                        "laten rusten"
                    ],
                    "common_phrases": [
                        "een moord wreken",
                        "onrecht wreken",
                        "bloed wreken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершить возмездие за несправедливый поступок или преступление, наказав виновного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отомстить",
                                "target_lang_sense_clarification": "совершить месть за что-либо"
                            },
                            {
                                "target_lang_word": "мстить",
                                "target_lang_sense_clarification": "совершать месть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "567b2969-748d-35e6-8717-81a9149991ca",
                    "sense_definition": "Zichzelf genoegdoening verschaffen voor aangedaan onrecht; wraak nemen op iemand.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "revenge_retribution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij beloofde dat hij zich op zijn pestkoppen zou <w>wreken</w>.",
                            "target_lang_translations": {
                                "ru": "Он пообещал, что <w>отомстит</w> своим обидчикам."
                            }
                        },
                        {
                            "text": "De verliezende partij wilde zich in de volgende wedstrijd sportief <w>wreken</w>.",
                            "target_lang_translations": {
                                "ru": "Проигравшая сторона хотела по-спортивному <w>отыграться</w> в следующем матче."
                            }
                        }
                    ],
                    "synonyms": [
                        "wraak nemen"
                    ],
                    "antonyms": [
                        "vergeven"
                    ],
                    "common_phrases": [
                        "zich wreken op",
                        "zich bloedig wreken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получить удовлетворение за причиненную обиду; отомстить кому-либо за себя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отомстить",
                                "target_lang_sense_clarification": "наказать обидчика"
                            },
                            {
                                "target_lang_word": "отыграться",
                                "target_lang_sense_clarification": "взять реванш (в переносном смысле или спорте)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
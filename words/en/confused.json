        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3c51adfa-dd85-30e1-9f33-0e57b5f86ff7",
                    "sense_definition": "Unable to think clearly or understand what is happening or what is being said.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am totally <w>confused</w> about which form I need to fill out.",
                            "target_lang_translations": {
                                "ru": "Я совершенно <w>запутался</w> в том, какую форму мне нужно заполнить."
                            }
                        },
                        {
                            "text": "She looked <w>confused</w> when I asked her for directions to the station.",
                            "target_lang_translations": {
                                "ru": "Она выглядела <w>растерянной</w>, когда я спросил у неё дорогу до станции."
                            }
                        }
                    ],
                    "synonyms": [
                        "puzzled",
                        "perplexed",
                        "bewildered",
                        "baffled",
                        "mixed up"
                    ],
                    "antonyms": [
                        "clear-headed"
                    ],
                    "common_phrases": [
                        "confused about",
                        "look confused",
                        "get confused",
                        "feel confused"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неспособный ясно мыслить или понимать происходящее или сказанное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "растерянный",
                                "target_lang_sense_clarification": "находящийся в состоянии недоумения или беспомощности"
                            },
                            {
                                "target_lang_word": "сбитый с толку",
                                "target_lang_sense_clarification": "озадаченный, переставший понимать суть происходящего"
                            },
                            {
                                "target_lang_word": "запутанный",
                                "target_lang_sense_clarification": "находящийся в замешательстве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81ddd6d6-fa3a-3662-85fe-d0e59db14ba5",
                    "sense_definition": "Not knowing where you are, what time it is, or which direction you should go.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The elderly man became <w>confused</w> and wandered away from his home.",
                            "target_lang_translations": {
                                "ru": "Пожилой мужчина <w>потерял ориентацию</w> и ушёл из дома."
                            }
                        },
                        {
                            "text": "After the accident, the driver was dazed and <w>confused</w>.",
                            "target_lang_translations": {
                                "ru": "После аварии водитель был ошеломлён и <w>дезориентирован</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "disoriented",
                        "dazed"
                    ],
                    "antonyms": [
                        "oriented"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не понимающий, где находится, который час или в каком направлении идти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дезориентированный",
                                "target_lang_sense_clarification": "потерявший представление о месте или времени"
                            },
                            {
                                "target_lang_word": "в замешательстве",
                                "target_lang_sense_clarification": "состояние неясности сознания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "297d6ccd-313a-3976-bd99-c4590f24e8e5",
                    "sense_definition": "Difficult to understand because of a lack of order, clarity, or logic.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "quality_of_things",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The report was a <w>confused</w> mess of data that no one could interpret.",
                            "target_lang_translations": {
                                "ru": "Отчёт представлял собой <w>беспорядочную</w> мешанину данных, которую никто не мог истолковать."
                            }
                        },
                        {
                            "text": "He gave a <w>confused</w> explanation that only made the problem worse.",
                            "target_lang_translations": {
                                "ru": "Он дал <w>сбивчивое</w> объяснение, которое только усугубило проблему."
                            }

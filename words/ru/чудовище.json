    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1cdaa96a-0f86-33ab-9bc0-f38d00d344e3",
                    "sense_definition": "Сказочное существо страшного вида, часто огромных размеров и обладающее большой силой.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В глубоком озере, по легенде, обитает древнее <w>чудовище</w>.",
                            "target_lang_translations": {
                                "en": "According to legend, an ancient <w>monster</w> inhabits the deep lake."
                            }
                        },
                        {
                            "text": "Герой должен победить страшное <w>чудовище</w>, чтобы спасти принцессу.",
                            "target_lang_translations": {
                                "en": "The hero must defeat the terrible <w>monster</w> to save the princess."
                            }
                        }
                    ],
                    "synonyms": [
                        "монстр",
                        "чудище",
                        "страшилище"
                    ],
                    "common_phrases": [
                        "морское чудовище",
                        "страшное чудовище",
                        "огромное чудовище"
                    ],
                    "target_lang_definitions": {
                        "en": "A fairy-tale creature of frightening appearance, often of huge size and possessing great strength."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "monster",
                                "target_lang_sense_clarification": "mythical or frightening creature"
                            },
                            {
                                "target_lang_word": "beast",
                                "target_lang_sense_clarification": "large and dangerous creature"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9aa7c97-1e3e-3abf-b6d7-4ddfee7a693c",
                    "sense_definition": "Существо или человек с крайне отталкивающей, безобразной внешностью.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за шрамов он считал себя настоящим <w>чудовищем</w> и прятал лицо.",
                            "target_lang_translations": {
                                "en": "Because of his scars, he considered himself a real <w>monster</w> and hid his face."
                            }
                        },
                        {
                            "text": "В зеркале отразилось лохматое <w>чудовище</w> с красными глазами.",
                            "target_lang_translations": {
                                "en": "A shaggy <w>monster</w> with red eyes was reflected in the mirror."
                            }
                        }
                    ],
                    "synonyms": [
                        "урод",
                        "страшилище"
                    ],
                    "antonyms": [
                        "красавец",
                        "красавица"
                    ],
                    "common_phrases": [
                        "настоящее чудовище",
                        "выглядеть как чудовище"
                    ],
                    "target_lang_definitions": {
                        "en": "A creature or person with an extremely repulsive, ugly appearance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "monster",
                                "target_lang_sense_clarification": "physically repulsive being"
                            },
                            {
                                "target_lang_word": "hideous creature",
                                "target_lang_sense_clarification": "descriptive phrase"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4bb6e795-be8e-3ea2-b171-e0f23213c063",
                    "sense_definition": "Крайне жестокий, безнравственный человек, способный на подлые поступки.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот диктатор был <w>чудовищем</w>, погубившим тысячи людей.",
                            "target_lang_translations": {
                                "en": "This dictator was a <w>monster</w> who destroyed thousands of people."
                            }
                        },
                        {
                            "text": "Как ты мог так поступить, ты просто бессердечное <w>чудовище</w>!",
                            "target_lang_translations": {
                                "en": "How could you do that? You are just a heartless <w>monster</w>!"
                            }
                        }
                    ],
                    "synonyms": [
                        "монстр"
                    ],
                    "common_phrases": [
                        "моральное чудовище",
                        "бессердечное чудовище"
                    ],
                    "target_lang_definitions": {
                        "en": "An extremely cruel, immoral person capable of vile acts."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "monster",
                                "target_lang_sense_clarification": "cruel person"
                            },
                            {
                                "target_lang_word": "fiend",
                                "target_lang_sense_clarification": "evil or wicked person"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
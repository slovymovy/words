            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e2f676fb-8862-300e-8ae7-d3b1e4cb0402",
                    "sense_definition": "Издавать, производить звук или звуки (о музыкальных инструментах, предметах).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старое расстроенное пианино <звучит> немного фальшиво."
                        },
                        {
                            "text": "Если сильно натянуть струну, она будет <звучать> очень тонко."
                        }
                    ],
                    "common_phrases": [
                        "громко звучать",
                        "тихо звучать",
                        "чисто звучать"
                    ]
                },
                {
                    "sense_id": "eb56acbb-0a6c-3f71-b257-886bb4dc953e",
                    "sense_definition": "Раздаваться, быть слышным (о музыке, песне, голосе, шуме).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из открытого окна на всю улицу <звучала> весёлая музыка."
                        },
                        {
                            "text": "В полной тишине наши шаги <звучали> особенно громко."
                        }
                    ],
                    "synonyms": [
                        "раздаваться",
                        "слышаться"
                    ],
                    "common_phrases": [
                        "музыка звучит",
                        "песня звучит",
                        "звучать в голове"
                    ]
                },
                {
                    "sense_id": "20460ebc-9e86-36a6-8111-1bfc79cdc27f",
                    "sense_definition": "Производить определённое впечатление, казаться каким-либо по смыслу или содержанию.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "impression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Твоё предложение поехать в отпуск <звучит> очень заманчиво."
                        },
                        {
                            "text": "Эти слова <звучат> как оправдание твоего поступка."
                        }
                    ],
                    "common_phrases": [
                        "звучать гордо",
                        "странно звучать",
                        "звучать убедительно",
                        "звучать глупо"
                    ]
                },
                {
                    "sense_id": "668a8aff-edf0-3af5-9021-4a62c9d462ce",
                    "sense_definition": "Проявляться, чувствоваться или обнаруживаться (в голосе, интонации, словах).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "manifestation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В её спокойном голосе <звучала> скрытая тревога за сына."
                        },
                        {
                            "text": "В его словах <звучала> искренняя радость от встречи с другом."
                        }
                    ],
                    "common_phrases": [
                        "звучать в голосе",
                        "звучать в словах"
                    ]
                }
            ]
        }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9ad37a3f-015f-309b-ba3e-3e2e8c78969e",
                    "sense_definition": "Установление границы, разделение чего-либо на отдельные части, зоны или области (обычно в территориальном или физическом смысле).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Комиссия завершила <w>разграничение</w> спорных территорий между двумя странами.",
                            "target_lang_translations": {
                                "en": "The commission completed the <w>demarcation</w> of disputed territories between the two countries."
                            }
                        },
                        {
                            "text": "На карте была проведена чёткая линия <w>разграничения</w> зон влияния.",
                            "target_lang_translations": {
                                "en": "A clear line of <w>demarcation</w> of influence zones was drawn on the map."
                            }
                        }
                    ],
                    "synonyms": [
                        "разделение",
                        "размежевание"
                    ],
                    "antonyms": [
                        "объединение",
                        "слияние"
                    ],
                    "common_phrases": [
                        "линия разграничения",
                        "разграничение земель",
                        "разграничение доступа"
                    ],
                    "target_lang_definitions": {
                        "en": "The establishment of boundaries or the division of something into separate parts, zones, or areas (usually in a territorial or physical sense)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "demarcation",
                                "target_lang_sense_clarification": "setting boundaries"
                            },
                            {
                                "target_lang_word": "delimitation",
                                "target_lang_sense_clarification": "formal fixing of limits"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f7ac7bd-a3ef-3317-9473-e424bb738c2f",
                    "sense_definition": "Определение различий между понятиями или чёткое распределение прав, обязанностей и функций.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В договоре прописано строгое <w>разграничение</w> ответственности сторон.",
                            "target_lang_translations": {
                                "en": "The contract stipulates a strict <w>delineation</w> of the parties' responsibilities."
                            }
                        },
                        {
                            "text": "Для понимания темы важно <w>разграничение</w> этих двух научных терминов.",
                            "target_lang_translations": {
                                "en": "The <w>differentiation</w> of these two scientific terms is important for understanding the topic."
                            }
                        }
                    ],
                    "synonyms": [
                        "различение",
                        "дифференциация"
                    ],
                    "antonyms": [
                        "смешение",
                        "обобщение"
                    ],
                    "common_phrases": [
                        "разграничение полномочий",
                        "разграничение обязанностей",
                        "разграничение понятий",
                        "разграничение компетенции"
                    ],
                    "target_lang_definitions": {
                        "en": "The determination of differences between concepts or the clear distribution of rights, duties, and functions."
                    },

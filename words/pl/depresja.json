            "senses": [
                {
                    "sense_id": "de81689e-6ed5-32d4-ac8c-b3e294fd3a16",
                    "sense_definition": "Stan psychiczny charakteryzujący się długotrwałym obniżeniem nastroju, przygnębieniem, brakiem energii i utratą zainteresowań.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "psychology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po stracie pracy Marek wpadł w głęboką <w>depresję</w>.",
                            "target_lang_translations": {
                                "ru": "После потери работы Марек впал в глубокую <w>депрессию</w>."
                            }
                        },
                        {
                            "text": "Lekarz przepisał pacjentowi leki, aby pomóc mu zwalczyć <w>depresję</w>.",
                            "target_lang_translations": {
                                "ru": "Врач выписал пациенту лекарства, чтобы помочь ему побороть <w>депрессию</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "przygnębienie",
                        "melancholia",
                        "spleen"
                    ],
                    "antonyms": [
                        "euforia"
                    ],
                    "common_phrases": [
                        "głęboka depresja",
                        "wpaść w depresję",
                        "leczyć depresję",
                        "objawy depresji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Психическое состояние, характеризующееся длительным снижением настроения, подавленностью, отсутствием энергии и потерей интересов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "депрессия",
                                "target_lang_sense_clarification": "психическое расстройство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9764420-0cbf-3ac4-b23e-4f2389a67276",
                    "sense_definition": "Obszar lądu położony poniżej poziomu morza.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znaczna część terytorium Holandii to <w>depresja</w>.",
                            "target_lang_translations": {
                                "ru": "Значительная часть территории Нидерландов — это <w>депрессия</w>."
                            }
                        },
                        {
                            "text": "Najniżej położona <w>depresja</w> na świecie znajduje się nad Morzem Martwym.",
                            "target_lang_translations": {
                                "ru": "Самая низкая <w>впадина</w> в мире находится у Мертвого моря."
                            }
                        }
                    ],
                    "common_phrases": [
                        "obszar depresji",
                        "depresja terenu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участок суши, расположенный ниже уровня моря."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "депрессия",
                                "target_lang_sense_clarification": "география"
                            },

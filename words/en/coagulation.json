    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d18e0695-489d-39bf-b160-29387f550837",
                    "sense_definition": "The biological process by which blood changes from a liquid to a solid or semi-solid state to form a clot.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "solidification_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vitamin K is essential for proper blood <w>coagulation</w> after an injury.",
                            "target_lang_translations": {
                                "ru": "Витамин К необходим для нормального <w>свёртывания</w> крови после травмы."
                            }
                        },
                        {
                            "text": "The doctor ordered a test to check the patient's <w>coagulation</w> rate.",
                            "target_lang_translations": {
                                "ru": "Врач назначил анализ для проверки скорости <w>свёртывания</w> крови пациента."
                            }
                        }
                    ],
                    "synonyms": [
                        "clotting"
                    ],
                    "antonyms": [
                        "dissolution"
                    ],
                    "common_phrases": [
                        "blood coagulation",
                        "coagulation factor",
                        "coagulation test"
                    ],
                    "target_lang_definitions": {
                        "ru": "Биологический процесс, при котором кровь переходит из жидкого состояния в твердое или полутвердое, образуя сгусток."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свёртывание",
                                "target_lang_sense_clarification": "крови"
                            },
                            {
                                "target_lang_word": "коагуляция",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "711bfaf7-eb47-32b1-83d3-87a2f3775d87",
                    "sense_definition": "The process where a liquid changes into a solid or semi-solid mass, or where suspended particles clump together.",
                    "learner_level": "C2",
                    "frequency": "Middle",
                    "semantic_group_id": "solidification_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "High temperatures can cause the <w>coagulation</w> of proteins in the egg.",
                            "target_lang_translations": {
                                "ru": "Высокие температуры могут вызвать <w>свёртывание</w> белков в яйце."
                            }
                        },
                        {
                            "text": "The water treatment plant uses chemical <w>coagulation</w> to remove impurities.",
                            "target_lang_translations": {
                                "ru": "Станция очистки воды использует химическую <w>коагуляцию</w> для удаления примесей."
                            }
                        }
                    ],
                    "synonyms": [
                        "solidification",
                        "thickening",
                        "curdling"
                    ],
                    "antonyms": [
                        "dissolution",
                        "peptization",
                        "declumping"
                    ],
                    "common_phrases": [
                        "protein coagulation",
                        "chemical coagulation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс перехода жидкости в твердую или полутвердую массу, или слипание взвешенных частиц."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коагуляция",
                                "target_lang_sense_clarification": "химический процесс, очистка воды"
                            },
                            {
                                "target_lang_word": "свёртывание",
                                "target_lang_sense_clarification": "белков"
                            },
                            {
                                "target_lang_word": "сгущение",
                                "target_lang_sense_clarification": "уплотнение массы"
                            }
                        ]
                    }
                }
            ]
        }
    ]

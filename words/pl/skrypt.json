                {
                    "sense_id": "19a7dfa7-a206-3a62-92f3-ab5749e3bac8",
                    "sense_definition": "Podręcznik akademicki lub zbiór notatek studenckich zawierający najważniejsze informacje z wykładów, często wydawany przez uczelnię.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Profesor polecił nam kupić <skrypt> do gramatyki historycznej w księgarni uniwersyteckiej.",
                            "target_lang_translations": {
                                "ru": "Профессор посоветовал нам купить <учебное пособие> по исторической грамматике в университетском книжном магазине."
                            }
                        },
                        {
                            "text": "Przed egzaminem muszę dokładnie przeczytać <skrypt> z wykładów, żeby zdać.",
                            "target_lang_translations": {
                                "ru": "Перед экзаменом я должен внимательно прочитать <конспект лекций>, чтобы сдать."
                            }
                        }
                    ],
                    "synonyms": [
                        "podręcznik",
                        "materiały"
                    ],
                    "common_phrases": [
                        "skrypt akademicki",
                        "skrypt z wykładów",
                        "wydać skrypt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Академический учебник или сборник студенческих заметок, содержащий важнейшую информацию из лекций, часто издаваемый университетом."
                    },
                    "translations": {

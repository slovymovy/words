{
    "word_family": [
        "bescheidenheid"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c1cb197f-1305-3fba-a161-b3285d97f8c7",
                    "sense_definition": "Zonder arrogantie; iemand die zichzelf niet belangrijker maakt dan anderen en niet opschept over eigen prestaties.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "personality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is een zeer <w>bescheiden</w> jongen, maar hij heeft veel talent.",
                            "target_lang_translations": {
                                "en": "He is a very <w>modest</w> boy, but he has a lot of talent."
                            }
                        },
                        {
                            "text": "Ze was heel lief en ik heb haar leren kennen als een <w>bescheiden</w> vrouw.",
                            "target_lang_translations": {
                                "en": "She was very sweet and I got to know her as a <w>modest</w> woman."
                            }
                        }
                    ],
                    "synonyms": [
                        "ingetogen"
                    ],
                    "antonyms": [
                        "onbescheiden",
                        "arrogant",
                        "hoogmoedig",
                        "verwaand"
                    ],
                    "common_phrases": [
                        "zich bescheiden opstellen",
                        "een bescheiden mens"
                    ],
                    "target_lang_definitions": {
                        "en": "Not arrogant or boastful about oneself or one's achievements."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "modest",
                                "target_lang_sense_clarification": "regarding personality or achievements"
                            },
                            {
                                "target_lang_word": "humble",
                                "target_lang_sense_clarification": "having a low estimate of one's own importance"
                            },
                            {
                                "target_lang_word": "unassuming",
                                "target_lang_sense_clarification": "not pretentious"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "814d5781-8f32-3609-8b2a-f8e3081586f1",
                    "sense_definition": "Beperkt in omvang, hoeveelheid of grootte; niet overdadig of groots.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met <w>bescheiden</w> hulpmiddelen trok hij het oerwoud in.",
                            "target_lang_translations": {
                                "en": "He went into the jungle with <w>modest</w> resources."
                            }
                        },
                        {
                            "text": "De economie van dat land is vrij <w>bescheiden</w> in vergelijking met de buurlanden.",
                            "target_lang_translations": {
                                "en": "The economy of that country is quite <w>modest</w> compared to the neighboring countries."
                            }
                        }
                    ],
                    "antonyms": [
                        "groots",
                        "overvloedig",
                        "bombastisch",
                        "grotesk"
                    ],
                    "common_phrases": [
                        "een bescheiden bedrag",
                        "van bescheiden omvang"
                    ],
                    "target_lang_definitions": {
                        "en": "Limited in size, amount, or extent; not large or excessive."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "modest",
                                "target_lang_sense_clarification": "small or limited in quantity"
                            },
                            {
                                "target_lang_word": "small",
                                "target_lang_sense_clarification": "not large"
                            },
                            {
                                "target_lang_word": "moderate",
                                "target_lang_sense_clarification": "average in amount or intensity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9912c4e1-c8d1-362e-bfb7-7d4a6eb7474b",
                    "sense_definition": "Niet opdringerig; beleefd en terughoudend bij het uiten van een mening.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Naar mijn <w>bescheiden</w> mening is dat niet waar.",
                            "target_lang_translations": {
                                "en": "In my <w>humble</w> opinion, that is not true."
                            }
                        },
                        {
                            "text": "Hij gaf zijn kritiek op een heel <w>bescheiden</w> manier.",
                            "target_lang_translations": {
                                "en": "He gave his criticism in a very <w>modest</w> way."
                            }
                        }
                    ],
                    "antonyms": [
                        "opdringerig"
                    ],
                    "common_phrases": [
                        "naar mijn bescheiden mening"
                    ],
                    "target_lang_definitions": {
                        "en": "Polite and respectful, especially when expressing an opinion."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "humble",
                                "target_lang_sense_clarification": "often used in 'humble opinion'"
                            },
                            {
                                "target_lang_word": "modest",
                                "target_lang_sense_clarification": "reserved in manner"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d60eff0c-17d3-3640-a5de-4f90de007985",
                    "sense_definition": "Verstandig en in staat om goed te oordelen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "archaic_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit boek is opgedragen aan de <w>bescheiden</w> lezer.",
                            "target_lang_translations": {
                                "en": "This book is dedicated to the <w>modest</w> reader."
                            }
                        },
                        {
                            "text": "Een <w>bescheiden</w> man weet wanneer hij moet spreken en wanneer hij moet zwijgen.",
                            "target_lang_translations": {
                                "en": "A <w>discreet</w> man knows when to speak and when to remain silent."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Showing prudence, discretion, or good judgment (archaic)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "discreet",
                                "target_lang_sense_clarification": "careful and prudent"
                            },
                            {
                                "target_lang_word": "modest",
                                "target_lang_sense_clarification": "decorous or proper"
                            }
                        ]
                    }
                }
            ]
        },

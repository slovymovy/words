{
    "word_family": [
        "czołowy",
        "czołówka",
        "naczelny",
        "przyczółek"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "21bd9b68-2660-34b9-802c-f042176bda1d",
                    "sense_definition": "Górna część twarzy człowieka, znajdująca się między brwiami a linią włosów.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "body_parts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko nabiło sobie dużego guza na <czole>.",
                            "target_lang_translations": {
                                "ru": "Ребёнок набил себе большую шишку на <w>лбу</w>."
                            }
                        },
                        {
                            "text": "Mężczyzna otarł pot z <czoła> wierzchem dłoni.",
                            "target_lang_translations": {
                                "ru": "Мужчина вытер пот со <w>лба</w> тыльной стороной ладони."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wysokie czoło",
                        "zmarszczyć czoło",
                        "chylić czoła",
                        "stawić czoła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхняя часть лица человека, находящаяся между бровями и линией волос."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лоб",
                                "target_lang_sense_clarification": "часть лица"
                            },
                            {
                                "target_lang_word": "чело",
                                "target_lang_sense_clarification": "устаревшее, поэтическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b18ee6f-a9ca-3ae0-a692-0a8ec3809c69",
                    "sense_definition": "Przednia, początkowa część grupy poruszających się ludzi, pojazdów lub przedmiotów.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_relations",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polski kolarz niespodziewanie wysunął się na <czoło> wyścigu.",
                            "target_lang_translations": {
                                "ru": "Польский велогонщик неожиданно вырвался в <w>лидеры</w> гонки."
                            }
                        },
                        {
                            "text": "Orkiestra wojskowa maszerowała na <czele> świątecznego pochodu.",
                            "target_lang_translations": {
                                "ru": "Военный оркестр маршировал во <w>главе</w> праздничного шествия."
                            }
                        }
                    ],
                    "synonyms": [
                        "przód",
                        "początek",
                        "awangarda",
                        "front"
                    ],
                    "antonyms": [
                        "ogon",
                        "ariergarda"
                    ],
                    "common_phrases": [
                        "na czele",
                        "wysunąć się na czoło",
                        "czoło pochodu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передняя, начальная часть группы движущихся людей, транспортных средств или предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "голова",
                                "target_lang_sense_clarification": "передняя часть колонны или группы"
                            },
                            {
                                "target_lang_word": "авангард",
                                "target_lang_sense_clarification": "передовая часть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f7d6311-d030-3199-a764-3f4fc1882ffb",
                    "sense_definition": "Pozycja przywódcza, kierownicza lub miejsce w ścisłej czołówce jakiejś grupy czy rankingu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hierarchy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy prezes stanął na <czele> zarządu spółki.",
                            "target_lang_translations": {
                                "ru": "Новый председатель встал во <w>главе</w> правления компании."
                            }
                        },
                        {
                            "text": "Nasz kraj znajduje się w ścisłym <czole> producentów żywności.",
                            "target_lang_translations": {
                                "ru": "Наша страна находится в числе безусловных <w>лидеров</w> среди производителей продовольствия."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stać na czele",
                        "być w czole"
                    ],
                    "target_lang_definitions": {
                        "ru": "Руководящая позиция или место в числе лучших в какой-либо группе или рейтинге."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глава",
                                "target_lang_sense_clarification": "руководитель, обычно в выражении \"во главе\""
                            },
                            {
                                "target_lang_word": "лидеры",
                                "target_lang_sense_clarification": "ведущая группа, первые места"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7b658922-e59a-3c5a-a47c-c2845d4cc11b",
                    "sense_definition": "Het onderste deel van de romp van een mens of dier, waar de maag en darmen zitten.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn dochter heeft de laatste tijd soms pijn in haar <w>buik</w>.",
                            "target_lang_translations": {
                                "ru": "У моей дочери в последнее время иногда болит <w>живот</w>."
                            }
                        },
                        {
                            "text": "Hij slaapt het liefst op zijn <w>buik</w>.",
                            "target_lang_translations": {
                                "ru": "Он предпочитает спать на <w>животе</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "abdomen"
                    ],
                    "common_phrases": [
                        "pijn in de buik",
                        "op de buik",
                        "platte buik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нижняя часть туловища человека или животного, где находятся желудок и кишечник."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "живот",
                                "target_lang_sense_clarification": "часть тела"
                            },
                            {
                                "target_lang_word": "брюшная полость",
                                "target_lang_sense_clarification": "анатомический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3e0bfea-ee36-361a-991a-8f49da1c1cb9",
                    "sense_definition": "Een dikke of uitpuilende buik door overgewicht.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sinds hij gestopt is met sporten, begint hij een <w>buik</w> te krijgen.",
                            "target_lang_translations": {
                                "ru": "С тех пор как он бросил спорт, у него начал расти <w>живот</w>."
                            }
                        },
                        {
                            "text": "Hij wil afvallen omdat zijn <w>buik</w> in de weg zit.",
                            "target_lang_translations": {
                                "ru": "Он хочет похудеть, потому что <w>живот</w> ему мешает."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een buikje krijgen",
                        "een dikke buik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой или выступающий живот из-за лишнего веса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "живот",
                                "target_lang_sense_clarification": "лишний вес"
                            },
                            {
                                "target_lang_word": "пузо",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "брюшко",
                                "target_lang_sense_clarification": "уменьшительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ccf1295-0f6e-3b27-901e-58bce7637795",
                    "sense_definition": "Het onderste, binnenste deel van de romp van een schip.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De lading werd diep in de <w>buik</w> van het schip opgeslagen.",
                            "target_lang_translations": {
                                "ru": "Груз был размещен глубоко в <w>трюме</w> корабля."
                            }
                        },
                        {
                            "text": "Er stond een laag water in de <w>buik</w> van de oude boot.",
                            "target_lang_translations": {
                                "ru": "В <w>трюме</w> старой лодки стоял слой воды."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in de buik van het schip"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутренняя, нижняя часть корпуса корабля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трюм",
                                "target_lang_sense_clarification": "корабля"
                            },
                            {
                                "target_lang_word": "чрево",
                                "target_lang_sense_clarification": "переносное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1dc4072-2439-3554-979b-013bd8d3dfaf",
                    "sense_definition": "De plaats waar een trilling of golf de grootste uitwijking heeft.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij een staande golf wisselen knopen en <w>buiken</w> elkaar af.",
                            "target_lang_translations": {
                                "ru": "В стоячей волне узлы и <w>пучности</w> чередуются."
                            }
                        },
                        {
                            "text": "De maximale amplitude bevindt zich in de <w>buik</w>.",
                            "target_lang_translations": {
                                "ru": "Максимальная амплитуда находится в <w>пучности</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "knoop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Точка, в которой колебание или волна имеет наибольшую амплитуду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пучность",
                                "target_lang_sense_clarification": "физика"
                            }
                        ]
                    }
                }
            ]
        }
    ]

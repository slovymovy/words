    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "71f8e4e9-65c5-32fa-bbbc-235e291875ba",
                    "sense_definition": "Wykonywanie jakiejś czynności, trud włożony w zrobienie czegoś; potoczne określenie pracy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "work_activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy wziąć się ostro do <w>roboty</w>, żeby skończyć ten projekt na czas.",
                            "target_lang_translations": {
                                "ru": "Мы должны серьезно взяться за <w>работу</w>, чтобы закончить этот проект вовремя."
                            }
                        },
                        {
                            "text": "To była bardzo ciężka <w>robota</w>, ale efekt końcowy nas zadowolił.",
                            "target_lang_translations": {
                                "ru": "Это была очень тяжелая <w>работа</w>, но конечный результат нас удовлетворил."
                            }
                        }
                    ],
                    "synonyms": [
                        "praca"
                    ],
                    "antonyms": [
                        "nieróbstwo"
                    ],
                    "common_phrases": [
                        "wziąć się do roboty",
                        "kupa roboty",
                        "ręczna robota",
                        "szukać roboty"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "słowo powszechnie używane w mowie potocznej zamiast słowa 'praca'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнение какого-либо действия, усилия, затраченные на то, чтобы что-то сделать; разговорное название работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "работа",
                                "target_lang_sense_clarification": "деятельность, занятие"
                            },
                            {
                                "target_lang_word": "труд",
                                "target_lang_sense_clarification": "усилие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e688ef83-7709-3232-8140-4614991839d1",
                    "sense_definition": "Zatrudnienie, stałe zajęcie zarobkowe, za które otrzymuje się wynagrodzenie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "work_job",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat w końcu znalazł stałą <w>robotę</w> w warsztacie samochodowym.",
                            "target_lang_translations": {
                                "ru": "Мой брат наконец нашел постоянную <w>работу</w> в автомастерской."
                            }
                        },
                        {
                            "text": "Nie ma go teraz w domu, bo poszedł do <w>roboty</w> na drugą zmianę.",
                            "target_lang_translations": {
                                "ru": "Его сейчас нет дома, потому что он пошел на <w>работу</w> во вторую смену."
                            }
                        }
                    ],
                    "synonyms": [
                        "praca"
                    ],
                    "antonyms": [
                        "bezrobocie"
                    ],
                    "common_phrases": [
                        "iść do roboty",
                        "stracić robotę",
                        "szukać roboty"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Трудоустройство, постоянное оплачиваемое занятие, за которое получают вознаграждение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "работа",
                                "target_lang_sense_clarification": "место работы, должность"
                            },
                            {
                                "target_lang_word": "служба",
                                "target_lang_sense_clarification": "занятость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb7a1b2c-f904-3b50-9bb4-ad449f355c2f",
                    "sense_definition": "Wynik czyjegoś działania, wytworzony przedmiot lub efekt wysiłku.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "work_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To naprawdę dobra <w>robota</w>, gratuluję świetnego wyniku.",
                            "target_lang_translations": {
                                "ru": "Это действительно хорошая <w>работа</w>, поздравляю с отличным результатом."
                            }
                        },
                        {
                            "text": "Ten wełniany sweter to precyzyjna ręczna <w>robota</w> mojej babci.",
                            "target_lang_translations": {
                                "ru": "Этот шерстяной свитер — тонкая ручная <w>работа</w> моей бабушки."
                            }
                        }
                    ],
                    "synonyms": [
                        "wytwór",
                        "owoc"
                    ],
                    "common_phrases": [
                        "dobra robota",
                        "koronkowa robota",
                        "kawał dobrej roboty"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Результат чьей-либо деятельности, изготовленный предмет или эффект усилий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "работа",
                                "target_lang_sense_clarification": "изделие, результат труда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1a90d56-c7ab-32b2-a4ac-d9139b590bf0",
                    "sense_definition": "Działania budowlane, remontowe lub inżynieryjne prowadzone w terenie (zwykle w liczbie mnogiej).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "work_construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na autostradzie trwają <w>roboty</w> drogowe, więc tworzą się ogromne korki.",
                            "target_lang_translations": {
                                "ru": "На шоссе идут дорожные <w>работы</w>, поэтому образуются огромные пробки."
                            }
                        },
                        {
                            "text": "Władze miasta ogłosiły przetarg na <w>roboty</w> wykończeniowe w nowym szpitalu.",
                            "target_lang_translations": {
                                "ru": "Власти города объявили тендер на отделочные <w>работы</w> в новой больнице."
                            }
                        }
                    ],
                    "synonyms": [
                        "praca"
                    ],
                    "common_phrases": [
                        "roboty drogowe",
                        "roboty budowlane",
                        "roboty ziemne",
                        "roboty publiczne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Строительные, ремонтные или инженерные действия, проводимые на местности (обычно во множественном числе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "работы",
                                "target_lang_sense_clarification": "строительные, дорожные"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0357867-b059-3e02-b494-3b61f972fbdc",
                    "sense_definition": "Przymusowa praca chłopa na rzecz pana w zamian za użytkowanie ziemi; pańszczyzna.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "work_historical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopi musieli stawiać się na darmową <w>robotę</w> w polu dziedzica kilka dni w tygodniu.",
                            "target_lang_translations": {
                                "ru": "Крестьяне должны были являться на бесплатную <w>работу</w> на поле помещика несколько дней в неделю."
                            }
                        },
                        {
                            "text": "Zniesienie pańszczyzny uwolniło wieś od przymusowej <w>roboty</w> na rzecz dworu.",
                            "target_lang_translations": {
                                "ru": "Отмена крепостного права освободила деревню от принудительной <w>работы</w> в пользу двора."
                            }
                        }
                    ],
                    "synonyms": [
                        "pańszczyzna"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "dotyczy realiów historycznych"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Принудительный труд крестьянина в пользу помещика взамен на пользование землей; барщина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "барщина",
                                "target_lang_sense_clarification": "историческое"
                            },
                            {
                                "target_lang_word": "работа",
                                "target_lang_sense_clarification": "труд на пана"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f42ef6fa-c673-34ae-9201-b39a7fb9760a",
                    "sense_definition": "Forma fleksyjna słowa <w>robot</w> (dopełniacz i biernik liczby pojedynczej).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "robot_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem synowi małego <w>robota</w>, który potrafi tańczyć i śpiewać.",
                            "target_lang_translations": {
                                "ru": "Я купил сыну маленького <w>робота</w>, который умеет танцевать и петь."
                            }
                        },
                        {
                            "text": "Naukowcy zaprojektowali <w>robota</w> do badania dna oceanu.",
                            "target_lang_translations": {
                                "ru": "Ученые спроектировали <w>робота</w> для исследования дна океана."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "odmiana rzeczownika żywotnego"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грамматическая форма слова robot (родительный и винительный падеж единственного числа)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "робота",
                                "target_lang_sense_clarification": "форма слова 'робот'"
                            }
                        ]
                    }
                }
            ]
        }
    ]

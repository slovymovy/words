            "senses": [
                {
                    "sense_id": "a718f469-dbcc-3aab-8cf1-41dd3df41bce",
                    "sense_definition": "Een tocht van de ene plaats naar de andere, meestal over een langere afstand.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij heeft een lange <w>reis</w> door Azië gemaakt.",
                            "target_lang_translations": {
                                "ru": "Она совершила долгое <w>путешествие</w> по Азии."
                            }
                        },
                        {
                            "text": "We wensen jullie een hele goede <w>reis</w> naar huis.",
                            "target_lang_translations": {
                                "ru": "Мы желаем вам благополучной <w>поездки</w> домой."
                            }
                        }
                    ],
                    "synonyms": [
                        "tocht",
                        "trip",
                        "uitstapje"
                    ],
                    "common_phrases": [
                        "op reis",
                        "goede reis",
                        "een reis maken",
                        "op reis gaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поездка из одного места в другое, обычно на значительное расстояние."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "путешествие",
                                "target_lang_sense_clarification": "длительная или дальняя поездка"
                            },
                            {
                                "target_lang_word": "поездка",
                                "target_lang_sense_clarification": "перемещение из одного места в другое"
                            }
                        ]
                    }
                }
            ]
        },

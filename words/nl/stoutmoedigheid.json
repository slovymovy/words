                {
                    "sense_id": "55638f82-f875-3b1f-aacd-9b926e10a596",
                    "sense_definition": "Grote dapperheid en bereidheid om risico's te nemen in gevaarlijke situaties.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "moed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De ridder vocht met grote <w>stoutmoedigheid</w> tegen de vijand.",
                            "target_lang_translations": {
                                "en": "The knight fought against the enemy with great <w>valour</w>.",
                                "ru": "Рыцарь сражался с врагом с великой <w>отвагой</w>."
                            }
                        },
                        {
                            "text": "Zijn <w>stoutmoedigheid</w> tijdens de reddingsactie werd door iedereen geprezen.",
                            "target_lang_translations": {
                                "en": "His <w>boldness</w> during the rescue operation was praised by everyone.",
                                "ru": "Его <w>бесстрашие</w> во время спасательной операции было всеми оценено."
                            }
                        }
                    ],
                    "synonyms": [
                        "dapperheid",
                        "onverschrokkenheid",
                        "durf",
                        "heldhaftigheid",
                        "kloekmoedigheid"
                    ],
                    "common_phrases": [
                        "met grote stoutmoedigheid",
                        "buitengewone stoutmoedigheid"
                    ],
                    "target_lang_definitions": {
                        "en": "Great bravery and willingness to take risks in dangerous situations.",
                        "ru": "Большая храбрость и готовность идти на риск в опасных ситуациях."
                    },
                    "translations": {

                {
                    "sense_id": "4a00223a-9b85-3def-a6e9-feebdf674b92",
                    "sense_definition": "A slow-moving, often stagnant creek or river.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "water_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They paddled their canoe silently down the narrow <w>bayou</w>.",
                            "target_lang_translations": {
                                "ru": "Они молча плыли на каноэ по узкой <w>протоке</w>."
                            }
                        },
                        {
                            "text": "Fishermen often catch catfish in the murky waters of the <w>bayou</w>.",
                            "target_lang_translations": {
                                "ru": "Рыбаки часто ловят сомов в мутных водах <w>заболоченной реки</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "creek",
                        "stream",
                        "tributary"
                    ],
                    "common_phrases": [
                        "on the bayou",
                        "bayou country"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Southern US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Медленно текущая, часто застойная река или ручей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "протока",
                                "target_lang_sense_clarification": "боковой рукав реки или канал"
                            },
                            {
                                "target_lang_word": "заболоченная река",
                                "target_lang_sense_clarification": "медленно текущая река с илистым дном"
                            },
                            {
                                "target_lang_word": "старица",
                                "target_lang_sense_clarification": "участок прежнего русла реки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a67d6751-0784-301f-8fd2-2f98fdda07cc",
                    "sense_definition": "A swamp or a marshy, stagnant body of water.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "water_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy rains turned the low field into a muddy <w>bayou</w>.",
                            "target_lang_translations": {
                                "ru": "Сильные дожди превратили низкое поле в грязное <w>болото</w>."
                            }
                        },
                        {
                            "text": "Many unique birds nest in the trees surrounding the <w>bayou</w>.",
                            "target_lang_translations": {
                                "ru": "Многие уникальные птицы гнездятся на деревьях, окружающих <w>болото</w>."
                            }
                        }
                    ],
                    "synonyms": [

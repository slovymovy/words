            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f9630f01-a3cf-3886-8c24-acc0331ab79b",
                    "sense_definition": "Cielesny ustrój człowieka lub zwierzęcia, traktowany jako funkcjonalna całość.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "body_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na starość nasz <w>organizm</w> zaczyna niestety szwankować.",
                            "target_lang_translations": {
                                "ru": "К старости наш <w>организм</w>, к сожалению, начинает давать сбои."
                            }
                        },
                        {
                            "text": "Musisz dostarczać swojemu <w>organizmowi</w> niezbędnych witamin i minerałów.",
                            "target_lang_translations": {
                                "ru": "Ты должен обеспечивать свой <w>организм</w> необходимыми витаминами и минералами."
                            }
                        }
                    ],
                    "synonyms": [
                        "ciało"
                    ],
                    "common_phrases": [
                        "osłabiony organizm",
                        "hartować organizm",
                        "oczyszczać organizm",
                        "wycieńczenie organizmu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Телесная система человека или животного, рассматриваемая как функциональное целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "организм",
                                "target_lang_sense_clarification": "тело, физиология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76cf2f4e-70bf-35ee-9382-7bc2d42c8033",
                    "sense_definition": "Istota żywa (roślina, zwierzę, grzyb lub mikroorganizm) charakteryzująca się procesami życiowymi.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "living_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Energia słoneczna jest bardzo ważna dla wszystkich <w>organizmów</w> żywych.",
                            "target_lang_translations": {
                                "ru": "Солнечная энергия очень важна для всех живых <w>организмов</w>."
                            }
                        },
                        {
                            "text": "Każdy <w>organizm</w> musi przystosować się do warunków panujących w jego środowisku.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>организм</w> должен приспособиться к условиям окружающей среды."
                            }
                        }
                    ],
                    "synonyms": [
                        "osobnik",
                        "jednostka"
                    ],
                    "common_phrases": [
                        "organizm żywy",
                        "organizm jednokomórkowy",
                        "organizm modyfikowany genetycznie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Живое существо (растение, животное, гриб или микроорганизм), характеризующееся жизненными процессами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "организм",
                                "target_lang_sense_clarification": "живое существо"
                            },
                            {
                                "target_lang_word": "существо",
                                "target_lang_sense_clarification": "особь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d100ecb-75ed-3d9e-b1f3-8f56a5bf5690",
                    "sense_definition": "Złożona struktura lub instytucja funkcjonująca dzięki współpracy swoich części składowych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Służby oczyszczania to ważny element sprawnego <w>organizmu</w> miasta.",
                            "target_lang_translations": {
                                "ru": "Службы уборки — это важный элемент исправно работающего <w>организма</w> города."
                            }
                        },
                        {
                            "text": "Wielka korporacja działała jak jeden skomplikowany <w>organizm</w>.",
                            "target_lang_translations": {
                                "ru": "Крупная корпорация действовала как один сложный <w>организм</w>."

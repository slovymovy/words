            "senses": [
                {
                    "sense_id": "fe90e741-0273-3c3b-b6a5-8f7b1d55f233",
                    "sense_definition": "Unappetizing, semi-liquid food or drink that looks messy or tasteless.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "food_substance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cafeteria served a grey <w>slop</w> that no one wanted to eat.",
                            "target_lang_translations": {
                                "ru": "В столовой подавали серую <w>баланду</w>, которую никто не хотел есть."
                            }
                        },
                        {
                            "text": "Prisoners in the movie complained about the tasteless <w>slop</w> on their trays.",
                            "target_lang_translations": {
                                "ru": "Заключенные в фильме жаловались на безвкусные <w>помои</w> на своих подносах."
                            }
                        }
                    ],
                    "synonyms": [
                        "swill"
                    ],
                    "common_phrases": [
                        "disgusting slop",
                        "serve slop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Невкусная, полужидкая еда или напиток, выглядящие неаппетитно."

{
    "word_family": [
        "verstarren",
        "star",
        "starheid"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "485189fe-4c7e-398d-8aa0-75f0817c99db",
                    "sense_definition": "Het proces waarbij iemand of iets minder flexibel wordt in denken, handelen of regelgeving; het onvermogen om zich aan te passen aan nieuwe situaties.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_rigidity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>verstarring</w> van de bureaucratie zorgt voor veel vertraging bij nieuwe projecten.",
                            "target_lang_translations": {
                                "en": "The <w>ossification</w> of the bureaucracy causes a lot of delays with new projects."
                            }
                        },
                        {
                            "text": "We moeten oppassen voor geestelijke <w>verstarring</w> naarmate we ouder worden.",
                            "target_lang_translations": {
                                "en": "We must watch out for mental <w>rigidity</w> as we get older."
                            }
                        }
                    ],
                    "synonyms": [
                        "inflexibiliteit",
                        "rigiditeit",
                        "verharding",
                        "onwrikbaarheid"
                    ],
                    "common_phrases": [
                        "politieke verstarring",
                        "geestelijke verstarring",
                        "bureaucratische verstarring"
                    ],
                    "target_lang_definitions": {
                        "en": "The process of becoming less flexible in thinking, acting, or regulations; the inability to adapt to new situations."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rigidity",
                                "target_lang_sense_clarification": "general lack of flexibility in systems or mind"
                            },
                            {
                                "target_lang_word": "ossification",
                                "target_lang_sense_clarification": "formal; becoming fixed and unable to change (e.g., bureaucracy)"
                            },
                            {
                                "target_lang_word": "inflexibility",
                                "target_lang_sense_clarification": "inability to change or adapt"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a09d2279-3198-3e5a-872f-75454030aa29",
                    "sense_definition": "Het stijf en onbeweeglijk worden van het lichaam of de spieren, vaak veroorzaakt door kou, schrik of een medische aandoening.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_stiffness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de extreme kou trad er al snel een <w>verstarring</w> op in zijn vingers.",
                            "target_lang_translations": {
                                "en": "Due to the extreme cold, <w>stiffness</w> quickly set in in his fingers."
                            }
                        },
                        {
                            "text": "Haar gezicht toonde een plotselinge <w>verstarring</w> toen ze het slechte nieuws hoorde.",
                            "target_lang_translations": {
                                "en": "Her face showed a sudden <w>freezing</w> when she heard the bad news."
                            }
                        }
                    ],
                    "synonyms": [
                        "verstijving"
                    ],
                    "common_phrases": [
                        "toestand van verstarring",
                        "lichamelijke verstarring"
                    ],
                    "target_lang_definitions": {
                        "en": "The stiffening and becoming immobile of the body or muscles, often caused by cold, shock, or a medical condition."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stiffening",
                                "target_lang_sense_clarification": "physical process of becoming stiff"
                            },
                            {
                                "target_lang_word": "freezing",
                                "target_lang_sense_clarification": "becoming motionless due to shock or fear"
                            },
                            {
                                "target_lang_word": "rigidity",
                                "target_lang_sense_clarification": "medical or physical stiffness"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
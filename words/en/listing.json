    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "99fe17ad-57e9-3007-8ff4-fd87fe63dcc0",
                    "sense_definition": "An individual item or entry included in a list or directory.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "list_record",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We found a <w>listing</w> for a reliable plumber in the local directory.",
                            "target_lang_translations": {
                                "ru": "Мы нашли <w>объявление</w> надежного сантехника в местном справочнике."
                            }
                        },
                        {
                            "text": "The job <w>listing</w> requires candidates to have three years of experience.",
                            "target_lang_translations": {
                                "ru": "<w>Объявление о вакансии</w> требует от кандидатов наличия трехлетнего опыта."
                            }
                        }
                    ],
                    "synonyms": [
                        "entry",
                        "record",
                        "item"
                    ],
                    "common_phrases": [
                        "job listing",
                        "real estate listing",
                        "directory listing",
                        "new listing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный пункт или запись, включенные в список или каталог."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объявление",
                                "target_lang_sense_clarification": "о вакансии, продаже недвижимости и т.д."
                            },
                            {
                                "target_lang_word": "запись",
                                "target_lang_sense_clarification": "в реестре или справочнике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "518cae2f-23b5-377b-ba25-ce031bbafd96",
                    "sense_definition": "The act of making a list or including something in a list.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "list_record",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>listing</w> of ingredients on food labels is required by law.",
                            "target_lang_translations": {
                                "ru": "<w>Указание</w> ингредиентов на этикетках продуктов питания требуется по закону."
                            }
                        },
                        {
                            "text": "She is responsible for the <w>listing</w> of new products on the company website.",
                            "target_lang_translations": {
                                "ru": "Она отвечает за <w>размещение</w> новых товаров на веб-сайте компании."
                            }
                        }
                    ],
                    "synonyms": [
                        "recording",
                        "registering",
                        "cataloging"
                    ],
                    "common_phrases": [
                        "listing agent",
                        "listing agreement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс составления списка или включения чего-либо в список."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "составление списка",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "перечисление",
                                "target_lang_sense_clarification": "упоминание пунктов по порядку"
                            },
                            {
                                "target_lang_word": "размещение",
                                "target_lang_sense_clarification": "публикация информации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e99b06b-2906-3a1c-aa1f-404d3dc64169",
                    "sense_definition": "An entry on a register of securities accepted for trading on a stock exchange.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "list_record",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company may lose its <w>listing</w> on the stock exchange if profits do not improve.",
                            "target_lang_translations": {
                                "ru": "Компания может потерять свой <w>листинг</w> на фондовой бирже, если прибыль не увеличится."
                            }
                        },
                        {
                            "text": "They are planning a public <w>listing</w> to raise capital for expansion.",
                            "target_lang_translations": {
                                "ru": "Они планируют публичный <w>листинг</w> для привлечения капитала на расширение."
                            }
                        }
                    ],
                    "synonyms": [
                        "quotation"
                    ],
                    "common_phrases": [
                        "stock market listing",
                        "public listing",
                        "secondary listing"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "finance"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Включение ценных бумаг в список активов, допущенных к торгам на бирже."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "листинг",
                                "target_lang_sense_clarification": "финансовый термин"
                            },
                            {
                                "target_lang_word": "допуск к торгам",
                                "target_lang_sense_clarification": "на бирже"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51ae4449-87c6-35cf-bc44-d1b47d01171b",
                    "sense_definition": "A printout of a computer program or data set.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "list_record",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The programmer examined the code <w>listing</w> to find the source of the error.",
                            "target_lang_translations": {
                                "ru": "Программист изучил <w>листинг</w> кода, чтобы найти источник ошибки."
                            }
                        },
                        {
                            "text": "Please print a <w>listing</w> of the latest version of the software.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, распечатайте <w>листинг</w> последней версии программного обеспечения."
                            }
                        }
                    ],
                    "synonyms": [
                        "printout"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Распечатка текста компьютерной программы или набора данных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распечатка",
                                "target_lang_sense_clarification": "общий термин"
                            },
                            {
                                "target_lang_word": "листинг",
                                "target_lang_sense_clarification": "компьютерный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1516e3eb-b9da-356a-8f66-d8d61a0436e2",
                    "sense_definition": "The act of ploughing land with a lister.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The farmer finished the <w>listing</w> of the field before the rain started.",
                            "target_lang_translations": {
                                "ru": "Фермер закончил <w>нарезку борозд</w> на поле до начала дождя."
                            }
                        },
                        {
                            "text": "Proper <w>listing</w> helps prepare the soil for the next planting season.",
                            "target_lang_translations": {
                                "ru": "Правильная <w>нарезка борозд</w> помогает подготовить почву к следующему посевному сезону."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "agriculture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс вспашки земли с помощью окучника (листера)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нарезка борозд",
                                "target_lang_sense_clarification": "сельское хозяйство"
                            },
                            {
                                "target_lang_word": "окучивание",
                                "target_lang_sense_clarification": "обработка почвы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6498d3d3-65aa-38fc-b48b-ef0bcb4e6ae0",
                    "sense_definition": "Present participle of <w>list</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is currently <w>listing</w> all the guests who will attend the wedding.",
                            "target_lang_translations": {
                                "ru": "В данный момент она <w>составляет список</w> всех гостей, которые придут на свадьбу."
                            }
                        },
                        {
                            "text": "The damaged ship was <w>listing</w> heavily to one side after the storm.",
                            "target_lang_translations": {
                                "ru": "Поврежденный корабль сильно <w>кренился</w> на один бок после шторма."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени от глагола list (составлять список; крениться)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "составляющий список",
                                "target_lang_sense_clarification": "от глагола list"
                            },
                            {
                                "target_lang_word": "кренящийся",
                                "target_lang_sense_clarification": "о судне"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3d6e5756-62b4-3b30-a138-d81f5ced44e4",
                    "sense_definition": "Relating to the price or details as shown on a list.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "list_record",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>listing</w> price of the house was much higher than we expected.",
                            "target_lang_translations": {
                                "ru": "<w>Заявленная</w> цена дома была намного выше, чем мы ожидали."
                            }
                        },
                        {
                            "text": "You should contact the <w>listing</w> agent if you want to view the property.",
                            "target_lang_translations": {
                                "ru": "Вам следует связаться с <w>агентом продавца</w>, если вы хотите осмотреть недвижимость."
                            }
                        }
                    ],
                    "common_phrases": [
                        "listing price",
                        "listing agent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к цене или деталям, указанным в списке или объявлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заявленный",
                                "target_lang_sense_clarification": "о цене"
                            },
                            {
                                "target_lang_word": "указанный в списке",
                                "target_lang_sense_clarification": "соответствующий списку"
                            }
                        ]
                    }
                }
            ]
        }
    ]

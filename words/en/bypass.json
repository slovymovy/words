{
    "word_family": [
        "pass",
        "passage",
        "overpass",
        "underpass",
        "passable",
        "trespass",
        "surpass"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8e8ab514-d93c-3f1c-9a9b-f807ae9bc867",
                    "sense_definition": "To ignore or avoid the usual rules, systems, or people in authority to get something done faster.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "avoidance_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He tried to <w>bypass</w> the manager and speak directly to the owner of the company.",
                            "target_lang_translations": {
                                "ru": "Он попытался <w>обойти</w> менеджера и поговорить напрямую с владельцем компании."
                            }
                        },
                        {
                            "text": "Hackers managed to <w>bypass</w> the security firewall and access the private data.",
                            "target_lang_translations": {
                                "ru": "Хакерам удалось <w>обойти</w> защитный брандмауэр и получить доступ к личным данным."
                            }
                        }
                    ],
                    "synonyms": [
                        "circumvent",
                        "evade",
                        "ignore",
                        "skip"
                    ],
                    "antonyms": [
                        "follow",
                        "obey"
                    ],
                    "common_phrases": [
                        "bypass security",
                        "bypass the system",
                        "bypass regulations",
                        "bypass authority"
                    ],
                    "target_lang_definitions": {
                        "ru": "Игнорировать или избегать обычных правил, систем или людей, облеченных властью, чтобы сделать что-то быстрее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обходить",
                                "target_lang_sense_clarification": "правила, инстанции"
                            },
                            {
                                "target_lang_word": "действовать в обход",
                                "target_lang_sense_clarification": "фразеологизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c153ff67-e5a5-3797-a4d9-1f5f54ff0b8b",
                    "sense_definition": "To go around a place or obstacle instead of going through it.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "avoidance_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to <w>bypass</w> the city center to avoid the heavy morning traffic.",
                            "target_lang_translations": {
                                "ru": "Мы решили <w>объехать</w> центр города, чтобы избежать плотного утреннего движения."
                            }
                        },
                        {
                            "text": "The new high-speed train will <w>bypass</w> several small towns to reduce travel time.",
                            "target_lang_translations": {
                                "ru": "Новый скоростной поезд будет <w>миновать</w> несколько небольших городов, чтобы сократить время в пути."
                            }
                        }
                    ],
                    "synonyms": [
                        "skirt",
                        "circumnavigate"
                    ],
                    "common_phrases": [
                        "bypass the city",
                        "bypass the traffic"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ехать или идти в обход места или препятствия, вместо того чтобы проходить через него."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объезжать",
                                "target_lang_sense_clarification": "на транспорте"
                            },
                            {
                                "target_lang_word": "обходить",
                                "target_lang_sense_clarification": "пешком или общее значение"
                            },
                            {
                                "target_lang_word": "миновать",
                                "target_lang_sense_clarification": "проходить мимо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "062a6744-5085-3e2a-bbc9-fe05896306d2",
                    "sense_definition": "A road built around a town or busy area so that traffic does not have to go through the center.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "road_infrastructure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We took the <w>bypass</w> to save time on our journey to the coast.",
                            "target_lang_translations": {
                                "ru": "Мы поехали по <w>объездной дороге</w>, чтобы сэкономить время в поездке к побережью."
                            }
                        },
                        {
                            "text": "The construction of the new <w>bypass</w> has reduced noise and pollution in the village.",
                            "target_lang_translations": {
                                "ru": "Строительство новой <w>объездной дороги</w> снизило уровень шума и загрязнения в деревне."
                            }
                        }
                    ],
                    "synonyms": [
                        "ring road",
                        "beltway"
                    ],
                    "common_phrases": [
                        "city bypass",
                        "take the bypass",
                        "southern bypass"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дорога, построенная вокруг города или оживленного района, чтобы транспортному потоку не приходилось ехать через центр."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объездная дорога",
                                "target_lang_sense_clarification": "шоссе"
                            },
                            {
                                "target_lang_word": "обходной путь",
                                "target_lang_sense_clarification": "маршрут"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f1089a2-0c92-3f4a-8549-d5d8014af4c1",
                    "sense_definition": "A surgical operation that creates a new path for blood to flow around a blocked artery.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "medical_procedure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor recommended a heart <w>bypass</w> to improve his blood circulation.",
                            "target_lang_translations": {
                                "ru": "Врач порекомендовал <w>шунтирование</w> сердца для улучшения кровообращения."
                            }
                        },
                        {
                            "text": "She is recovering in the hospital after undergoing a triple <w>bypass</w> surgery.",
                            "target_lang_translations": {
                                "ru": "Она восстанавливается в больнице после операции тройного <w>шунтирования</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "heart bypass",
                        "gastric bypass",
                        "triple bypass",
                        "coronary bypass"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хирургическая операция, создающая новый путь для кровотока в обход заблокированной артерии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шунтирование",
                                "target_lang_sense_clarification": "медицинская операция"
                            },
                            {
                                "target_lang_word": "шунт",
                                "target_lang_sense_clarification": "искусственный сосуд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20ff91f2-7670-335f-8de7-31a9fdbd268e",
                    "sense_definition": "A secondary pipe or channel that allows fluid or gas to flow around a blockage or specific device.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mechanical_diversion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The plumber installed a <w>bypass</w> so the water supply could continue during repairs.",
                            "target_lang_translations": {
                                "ru": "Сантехник установил <w>байпас</w>, чтобы подача воды могла продолжаться во время ремонта."
                            }
                        },
                        {
                            "text": "You must open the <w>bypass</w> valve to release the pressure from the main tank.",
                            "target_lang_translations": {
                                "ru": "Вы должны открыть клапан <w>байпаса</w>, чтобы сбросить давление из основного бака."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bypass valve",
                        "bypass pipe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вспомогательная труба или канал, позволяющий жидкости или газу течь в обход засора или определенного устройства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "байпас",
                                "target_lang_sense_clarification": "технический термин"
                            },
                            {
                                "target_lang_word": "обводная труба",
                                "target_lang_sense_clarification": "сантехника"
                            },
                            {
                                "target_lang_word": "обвод",
                                "target_lang_sense_clarification": "канал"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
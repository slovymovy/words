    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "99610b08-f6e3-31cd-bedf-a37f1cfab2b7",
                    "sense_definition": "sprawdzian wiedzy lub umiejętności, często o mniejszej randze niż egzamin, którego zdanie jest warunkiem ukończenia przedmiotu",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "exam_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jutro czeka mnie trudne <w>zaliczenie</w> z historii, więc muszę się dużo uczyć.",
                            "target_lang_translations": {
                                "ru": "Завтра меня ждёт сложный <w>зачёт</w> по истории, поэтому мне нужно много учиться."
                            }
                        },
                        {
                            "text": "Profesor wyznaczył pierwszy termin <w>zaliczenia</w> na przyszły wtorek.",
                            "target_lang_translations": {
                                "ru": "Профессор назначил первую дату <w>зачёта</w> на следующий вторник."
                            }
                        }
                    ],
                    "synonyms": [
                        "kolokwium",
                        "sprawdzian",
                        "test"
                    ],
                    "common_phrases": [
                        "zaliczenie z przedmiotu",
                        "termin zaliczenia",
                        "uczyć się na zaliczenie",
                        "poprawkowe zaliczenie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проверка знаний или навыков, часто менее формальная, чем экзамен, сдача которой является условием завершения предмета"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зачёт",
                                "target_lang_sense_clarification": "проверка знаний"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bb561c48-8c15-362f-882b-f33749049229",
                    "sense_definition": "oficjalne potwierdzenie, zwykle w postaci wpisu w indeksie lub systemie, że student spełnił wymagania stawiane w ramach zajęć",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "credit_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nareszcie dostałem <w>zaliczenie</w> z matematyki i mogę zacząć wakacje.",
                            "target_lang_translations": {
                                "ru": "Наконец-то я получил <w>зачёт</w> по математике и могу начать каникулы."
                            }
                        },
                        {
                            "text": "Brakowało mi jednego punktu, żeby uzyskać <w>zaliczenie</w> w pierwszym terminie.",
                            "target_lang_translations": {
                                "ru": "Мне не хватало одного балла, чтобы получить <w>зачёт</w> в первый срок сдачи."
                            }
                        }
                    ],
                    "synonyms": [
                        "wpis"
                    ],
                    "common_phrases": [
                        "dostać zaliczenie",
                        "uzyskać zaliczenie",
                        "mieć zaliczenie",
                        "brak zaliczenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное подтверждение (обычно запись в зачётной книжке) того, что студент выполнил требования учебного курса"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зачёт",
                                "target_lang_sense_clarification": "отметка о сдаче"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95adfd9f-5104-3468-8178-505ce7eb012f",
                    "sense_definition": "rzeczownik odczasownikowy od <w>zaliczyć</w> (czynność uznania czegoś, wliczenia lub przyporządkowania do jakiejś kategorii)",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "action_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sędzia podjął kontrowersyjną decyzję o <w>zaliczeniu</w> bramki dla drużyny gości.",
                            "target_lang_translations": {
                                "ru": "Судья принял спорное решение о <w>засчитывании</w> гола команде гостей."
                            }
                        },
                        {
                            "text": "Proszę o <w>zaliczenie</w> tej wpłaty na poczet moich przyszłych rachunków.",
                            "target_lang_translations": {
                                "ru": "Прошу о <w>зачислении</w> этого платежа в счёт моих будущих счетов."
                            }
                        }
                    ],
                    "synonyms": [
                        "uznanie",
                        "wliczenie",
                        "zakwalifikowanie"
                    ],
                    "antonyms": [
                        "niezaliczenie",
                        "odrzucenie"
                    ],
                    "common_phrases": [
                        "zaliczenie w poczet",
                        "zaliczenie do kosztów",
                        "zaliczenie gola"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verbal noun"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отглагольное существительное от 'zaliczyć': действие по признанию чего-либо, включению в счёт или отнесению к категории"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "засчитывание",
                                "target_lang_sense_clarification": "признание действительным (напр. гола)"
                            },
                            {
                                "target_lang_word": "зачисление",
                                "target_lang_sense_clarification": "денежных средств"
                            },
                            {
                                "target_lang_word": "отнесение",
                                "target_lang_sense_clarification": "к категории"
                            }
                        ]
                    }
                }
            ]
        }
    ]

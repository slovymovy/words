{
    "word_family": [
        "ambasador",
        "ambasadorski"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7de6c197-e015-378d-bcb2-794f871bc682",
                    "sense_definition": "Stała instytucja (misja dyplomatyczna) o najwyższej randze, utrzymywana przez państwo w innym kraju lub przy organizacji międzynarodowej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "diplomatic_mission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę udać się do <w>ambasady</w>, aby złożyć wniosek o wizę.",
                            "target_lang_translations": {
                                "ru": "Мне нужно пойти в <w>посольство</w>, чтобы подать заявление на визу."
                            }
                        },
                        {
                            "text": "Polska <w>ambasada</w> w Londynie zorganizowała spotkanie dla Polonii.",
                            "target_lang_translations": {
                                "ru": "Польское <w>посольство</w> в Лондоне организовало встречу для польской диаспоры."
                            }
                        }
                    ],
                    "common_phrases": [
                        "polska ambasada",
                        "ambasada amerykańska",
                        "pracownik ambasady"
                    ],
                    "target_lang_definitions": {
                        "ru": "Постоянное дипломатическое представительство высшего ранга, учрежденное одним государством в другом или при международной организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посольство",
                                "target_lang_sense_clarification": "дипломатическая миссия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea63998d-a36b-339a-9150-278cac2bdbb3",
                    "sense_definition": "Wydzielony budynek lub teren, w którym mieści się siedziba misji dyplomatycznej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "diplomatic_mission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Demonstranci zebrali się przed <w>ambasadą</w>, aby wyrazić swój sprzeciw.",
                            "target_lang_translations": {
                                "ru": "Демонстранты собрались перед <w>посольством</w>, чтобы выразить свой протест."
                            }
                        },
                        {
                            "text": "Nowa <w>ambasada</w> została uroczyście otwarta w centrum miasta.",
                            "target_lang_translations": {
                                "ru": "Новое <w>посольство</w> было торжественно открыто в центре города."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przed ambasadą",
                        "budynek ambasady",
                        "teren ambasady"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельное здание или территория, где располагается штаб-квартира дипломатической миссии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посольство",
                                "target_lang_sense_clarification": "здание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6a882d6-df18-3a33-99f8-f0b847ca128b",
                    "sense_definition": "Ogół pracowników zatrudnionych w danej misji dyplomatycznej.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "diplomatic_mission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cała <w>ambasada</w> świętowała sukces trudnych negocjacji.",
                            "target_lang_translations": {
                                "ru": "Всё <w>посольство</w> праздновало успех трудных переговоров."
                            }
                        },
                        {
                            "text": "Na oficjalnym przyjęciu pojawiła się niemal cała <w>ambasada</w>.",
                            "target_lang_translations": {
                                "ru": "На официальном приеме появилось почти всё <w>посольство</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "cała ambasada"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность сотрудников, работающих в данной дипломатической миссии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посольство",
                                "target_lang_sense_clarification": "персонал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c745251-33be-3a6c-a888-340c0a7c992a",
                    "sense_definition": "W gwarze więziennej: brzuch lub żołądek.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po tak sytym obiedzie moja <w>ambasada</w> jest zupełnie pełna.",
                            "target_lang_translations": {
                                "ru": "После такого сытного обеда моё <w>брюхо</w> совершенно полно."
                            }
                        },
                        {
                            "text": "Podczas bójki więzień dostał mocny cios prosto w <w>ambasadę</w>.",
                            "target_lang_translations": {
                                "ru": "Во время драки заключенный получил сильный удар прямо в <w>живот</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gwara więzienna bandzioch",
                        "bebech",
                        "samara"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "gwara więzienna"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "На тюремном жаргоне: живот или желудок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "живот",
                                "target_lang_sense_clarification": "часть тела"
                            },
                            {
                                "target_lang_word": "брюхо",
                                "target_lang_sense_clarification": "грубое/просторечное"
                            }
                        ]
                    }
                }
            ]
        }
    ]

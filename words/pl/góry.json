    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f54a2cc7-d4e5-3d5b-a477-d6515df69af8",
                    "sense_definition": "Teren o pofałdowanej powierzchni i dużych różnicach wysokości; obszar górski, często cel wyjazdów turystycznych.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "mountains_terrain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W te wakacje jedziemy w <góry>, żeby odpocząć od miasta.",
                            "target_lang_translations": {
                                "ru": "В эти каникулы мы едем в <w>горы</w>, чтобы отдохнуть от города."
                            }
                        },
                        {
                            "text": "Podczas pobytu w <górach> spotkała mnie niezwykła przygoda.",
                            "target_lang_translations": {
                                "ru": "Во время пребывания в <w>горах</w> со мной случилось необычное приключение."
                            }
                        }
                    ],
                    "synonyms": [
                        "pasmo górskie"
                    ],
                    "antonyms": [
                        "niziny",
                        "morze"
                    ],
                    "common_phrases": [
                        "jechać w góry",
                        "chodzić po górach",
                        "wakacje w górach",
                        "kochać góry"
                    ],
                    "target_lang_definitions": {
                        "ru": "Местность с гористым рельефом и большими перепадами высот; горный район, часто являющийся целью туристических поездок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горы",
                                "target_lang_sense_clarification": "местность, горный район"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed699803-8247-39d4-939e-1167cbffbb57",
                    "sense_definition": "*Liczba mnoga od* <w>góra</w>; naturalne wzniesienia terenu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "mountains_plural",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W oddali widać wysokie <góry> pokryte śniegiem.",
                            "target_lang_translations": {
                                "ru": "Вдали видны высокие <w>горы</w>, покрытые снегом."
                            }
                        },
                        {
                            "text": "Te dwie <góry> są najwyższe w całej okolicy.",
                            "target_lang_translations": {
                                "ru": "Эти две <w>горы</w> — самые высокие во всей округе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wysokie góry",
                        "szczyty gór"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число от слова «гора»; естественные возвышенности рельефа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горы",
                                "target_lang_sense_clarification": "возвышенности (мн. ч.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b4257689-6e75-3380-a3bf-49e80dc21f83",
                    "sense_definition": "*Dopełniacz liczby pojedynczej od* <w>góra</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "mountain_singular_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze szczytu tej <góry> roztacza się piękny widok.",
                            "target_lang_translations": {
                                "ru": "С вершины этой <w>горы</w> открывается прекрасный вид."
                            }
                        },
                        {
                            "text": "Nie widzę wierzchołka <góry> z powodu gęstej mgły.",
                            "target_lang_translations": {
                                "ru": "Я не вижу верхушки <w>горы</w> из-за густого тумана."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szczyt góry",
                        "u podnóża góry"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Родительный падеж единственного числа слова «гора»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горы",
                                "target_lang_sense_clarification": "форма родительного падежа (чего?)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

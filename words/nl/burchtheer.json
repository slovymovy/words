{
    "word_family": [
        "burcht",
        "burchtvrouw",
        "burchtvoogd"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "47b44dff-547b-3f8a-b396-947b7ebbe307",
                    "sense_definition": "De eigenaar of heer die in een burcht woont en daar de leiding heeft.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "owner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>burchtheer</w> ontving zijn gasten in de grote zaal van het kasteel.",
                            "target_lang_translations": {
                                "ru": "<w>Владелец замка</w> принимал гостей в большом зале замка."
                            }
                        },
                        {
                            "text": "De boeren uit de omgeving vroegen de <w>burchtheer</w> om bescherming.",
                            "target_lang_translations": {
                                "ru": "Окрестные крестьяне просили <w>владельца замка</w> о защите."
                            }
                        }
                    ],
                    "synonyms": [
                        "kasteelheer",
                        "slotheer"
                    ],
                    "common_phrases": [
                        "de machtige burchtheer",
                        "de middeleeuwse burchtheer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Владелец или господин, проживающий в замке и управляющий им."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "владелец замка",
                                "target_lang_sense_clarification": "собственник"
                            },
                            {
                                "target_lang_word": "хозяин крепости",
                                "target_lang_sense_clarification": "господин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3358d929-00b4-3651-b87b-4dfd07bc2953",
                    "sense_definition": "Iemand die is aangesteld om een burcht te beheren en te verdedigen, vaak namens een vorst.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "manager",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De koning stelde een nieuwe <w>burchtheer</w> aan om de grens te bewaken.",
                            "target_lang_translations": {
                                "ru": "Король назначил нового <w>коменданта крепости</w> для охраны границы."
                            }
                        },
                        {
                            "text": "Als trouwe <w>burchtheer</w> zorgde hij goed voor de verdedigingswerken.",
                            "target_lang_translations": {
                                "ru": "Как верный <w>кастелян</w>, он хорошо заботился об оборонительных сооружениях."
                            }
                        }
                    ],
                    "synonyms": [
                        "slotvoogd",
                        "burchtvoogd",
                        "kastelein"
                    ],
                    "common_phrases": [
                        "de trouwe burchtheer"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лицо, назначенное для управления крепостью и её обороны, часто от имени монарха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кастелян",
                                "target_lang_sense_clarification": "управляющий замком"
                            },
                            {
                                "target_lang_word": "комендант крепости",
                                "target_lang_sense_clarification": "военный начальник"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
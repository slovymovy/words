    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0f28fa80-ce6c-35fe-bf35-4c54e3ce0b3a",
                    "sense_definition": "Happening once every year.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "frequency_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>yearly</w> festival attracts tourists from all over the country.",
                            "target_lang_translations": {
                                "ru": "<w>Ежегодный</w> фестиваль привлекает туристов со всей страны."
                            }
                        },
                        {
                            "text": "We have a <w>yearly</w> tradition of visiting the mountains in winter.",
                            "target_lang_translations": {
                                "ru": "У нас есть <w>ежегодная</w> традиция ездить в горы зимой."
                            }
                        }
                    ],
                    "synonyms": [
                        "annual"
                    ],
                    "common_phrases": [
                        "yearly event",
                        "yearly meeting",
                        "yearly basis"
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходящий один раз в год."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ежего́дный",
                                "target_lang_sense_clarification": "происходящий раз в год"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82c3c66d-6b24-3230-97a1-6cbac254cbf9",
                    "sense_definition": "Calculated over the course of one year.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cumulative_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His <w>yearly</w> income has increased significantly since he changed jobs.",
                            "target_lang_translations": {
                                "ru": "Его <w>годовой</w> доход значительно вырос с тех пор, как он сменил работу."
                            }
                        },
                        {
                            "text": "The <w>yearly</w> rainfall in this region is lower than average.",
                            "target_lang_translations": {
                                "ru": "<w>Годовое</w> количество осадков в этом регионе ниже среднего."
                            }
                        }
                    ],
                    "synonyms": [
                        "annual"
                    ],
                    "common_phrases": [
                        "yearly income",
                        "yearly average",
                        "yearly salary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рассчитанный или суммированный за период одного года."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "годово́й",
                                "target_lang_sense_clarification": "относящийся к целому году, рассчитанный за год"
                            },
                            {
                                "target_lang_word": "ежего́дный",
                                "target_lang_sense_clarification": "повторяющийся каждый год"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "853f7911-1977-3780-8db9-023d488c48c4",
                    "sense_definition": "Once a year; every year.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee meets <w>yearly</w> to discuss the budget and future plans.",
                            "target_lang_translations": {
                                "ru": "Комитет собирается <w>ежегодно</w>, чтобы обсудить бюджет и планы на будущее."
                            }
                        },
                        {
                            "text": "This magazine is published <w>yearly</w> rather than every month.",
                            "target_lang_translations": {
                                "ru": "Этот журнал выходит <w>ежегодно</w>, а не каждый месяц."
                            }
                        }
                    ],
                    "synonyms": [
                        "annually"
                    ],
                    "common_phrases": [
                        "updated yearly",
                        "paid yearly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Один раз в год; каждый год."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ежего́дно",
                                "target_lang_sense_clarification": "каждый год"
                            },
                            {
                                "target_lang_word": "раз в год",
                                "target_lang_sense_clarification": "однократно в течение года"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1cfbee38-0ab4-3223-877c-1450aa682ae3",
                    "sense_definition": "Something that is published once a year.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The library subscribes to several academic <w>yearlies</w> for research purposes.",
                            "target_lang_translations": {
                                "ru": "Библиотека выписывает несколько научных <w>ежегодников</w> для исследовательских целей."
                            }
                        },
                        {
                            "text": "This <w>yearly</w> contains a collection of the best essays from the past twelve months.",
                            "target_lang_translations": {

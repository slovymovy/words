{
    "word_family": [
        "Daniela",
        "Danielek",
        "danielowy"
    ],
    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "4b0311a9-33c3-3533-8609-992ddc364456",
                    "sense_definition": "Męskie imię własne pochodzenia biblijnego.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "names",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mój najlepszy przyjaciel ma na imię <w>Daniel</w>.",
                            "target_lang_translations": {
                                "ru": "Моего лучшего друга зовут <w>Даниил</w>."
                            }
                        },
                        {
                            "text": "Wczoraj <w>Daniel</w> pomógł mi naprawić samochód.",
                            "target_lang_translations": {
                                "ru": "Вчера <w>Даниил</w> помог мне починить машину."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mam na imię Daniel",
                        "święty Daniel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя собственное библейского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Даниил",
                                "target_lang_sense_clarification": "мужское имя (русский эквивалент)"
                            },
                            {
                                "target_lang_word": "Даниэль",
                                "target_lang_sense_clarification": "мужское имя (польская форма)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d25bd46-43ac-3f03-b3ea-cbc460da1292",
                    "sense_definition": "Prorok biblijny, główna postać jednej z ksiąg Starego Testamentu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "religious_figures",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Prorok <w>Daniel</w> zasłynął z umiejętności tłumaczenia snów.",
                            "target_lang_translations": {
                                "ru": "Пророк <w>Даниил</w> прославился способностью толковать сны."
                            }
                        },
                        {
                            "text": "Historia o tym, jak <w>Daniel</w> przetrwał w jaskini lwów, jest powszechnie znana.",
                            "target_lang_translations": {
                                "ru": "История о том, как <w>Даниил</w> выжил в рове со львами, широко известна."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Księga Daniela",
                        "prorok Daniel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Библейский пророк, главная фигура одной из книг Ветхого Завета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Даниил",
                                "target_lang_sense_clarification": "библейский пророк"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e02558e-3b43-314a-aeb4-34f7265750e4",
                    "sense_definition": "Forma fleksyjna (dopełniacz liczby mnogiej) imienia <w>Daniela</w>.",
                    "learner_level": "B2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "forms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszej klasie nie ma żadnych <w>Daniel</w>, są tylko dwie Julie.",
                            "target_lang_translations": {
                                "ru": "В нашем классе нет никаких <w>Даниэл</w>, есть только две Юлии."
                            }
                        },
                        {
                            "text": "Lista obecności tych <w>Daniel</w> została zgubiona.",
                            "target_lang_translations": {
                                "ru": "Список присутствия этих <w>Даниэл</w> был утерян."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural of Daniela"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма склонения (родительный падеж множественного числа) имени <w>Daniela</w>."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Даниэл",
                                "target_lang_sense_clarification": "форма имени Даниэла"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34ba7529-0557-3947-9bdf-540038559f56",
                    "sense_definition": "Nazwisko męskie lub żeńskie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "surnames",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Daniel</w> został poproszony o podejście do recepcji.",
                            "target_lang_translations": {
                                "ru": "Господина <w>Даниэля</w> попросили подойти к стойке регистрации."
                            }
                        },
                        {
                            "text": "Nie znam nikogo o nazwisku <w>Daniel</w> w tej firmie.",
                            "target_lang_translations": {
                                "ru": "Я не знаю никого по фамилии <w>Даниэль</w> в этой фирме."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужская или женская фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Даниэль",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3ca02292-ba6b-376a-8f07-33fcbea62f64",
                    "sense_definition": "Ssak z rodziny jeleniowatych, mniejszy od jelenia, charakteryzujący się łopatowatym porożem i często cętkowaną sierścią.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas spaceru po lesie zobaczyliśmy, jak <w>daniel</w> przebiega przez ścieżkę.",
                            "target_lang_translations": {
                                "ru": "Во время прогулки по лесу мы увидели, как <w>лань</w> перебегает тропинку."
                            }
                        },
                        {
                            "text": "Ten <w>daniel</w> ma wyjątkowo piękne i rozłożyste poroże.",
                            "target_lang_translations": {
                                "ru": "У этой <w>лани</w> исключительно красивые и раскидистые рога."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stado danieli",
                        "hodowla danieli",
                        "poroże daniela"
                    ],
                    "target_lang_definitions": {
                        "ru": "Млекопитающее из семейства оленевых, меньше оленя, с лопатообразными рогами и часто пятнистой шерстью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лань",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "hopać",
        "hopka",
        "hopsasa"
    ],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "70d60328-aace-3446-bb68-546b9f944e4d",
                    "sense_definition": "Okrzyk towarzyszący wykonywaniu skoku, podrzucaniu kogoś lub zachęcający do podskoczenia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "jump_exclamation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policz do trzech i zrób <hop> do wody.",
                            "target_lang_translations": {
                                "ru": "Сосчитай до трёх и <прыг> в воду."
                            }
                        },
                        {
                            "text": "Dziecko krzyknęło <hop>, przeskakując przez dużą kałużę.",
                            "target_lang_translations": {
                                "ru": "Ребёнок крикнул <оп>, перепрыгивая через большую лужу."
                            }
                        }
                    ],
                    "synonyms": [
                        "hops!",
                        "hopsa!",
                        "hopsasa!",
                        "hopla!"
                    ],
                    "common_phrases": [
                        "hop do góry",
                        "raz dwa i hop",
                        "hop do łóżka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Восклицание, сопровождающее прыжок, подбрасывание кого-либо или побуждающее к прыжку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оп",
                                "target_lang_sense_clarification": "при прыжке"
                            },
                            {
                                "target_lang_word": "хоп",
                                "target_lang_sense_clarification": "резкое движение"
                            },
                            {
                                "target_lang_word": "ап",
                                "target_lang_sense_clarification": "команда (алле-ап)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c22fbf73-990a-3008-bf32-7442522f5b68",
                    "sense_definition": "Okrzyk służący do nawoływania kogoś, zwracania na siebie uwagi lub sprawdzania, gdzie ktoś się znajduje.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "call_exclamation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Hop>, <hop>, czy ktoś mnie słyszy w tym lesie?",
                            "target_lang_translations": {
                                "ru": "<Ау>, <ау>, кто-нибудь меня слышит в этом лесу?"
                            }
                        },
                        {
                            "text": "Wołałem do nich <hop>, ale byli już zbyt daleko.",
                            "target_lang_translations": {
                                "ru": "Я кричал им <эй>, но они были уже слишком далеко."
                            }
                        }
                    ],
                    "synonyms": [
                        "hej!"
                    ],
                    "common_phrases": [
                        "hop hop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возглас, используемый для оклика, привлечения внимания или проверки местонахождения кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ау",
                                "target_lang_sense_clarification": "перекличка в лесу"
                            },
                            {
                                "target_lang_word": "эй",
                                "target_lang_sense_clarification": "привлечение внимания"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e72ba62a-e773-38ba-9076-1541c84e979b",
                    "sense_definition": "Pojedynczy skok lub podskok.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "jump_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jeden zwinny <hop> wystarczył, by kot znalazł się na płocie.",
                            "target_lang_translations": {
                                "ru": "Одного ловкого <прыжка> хватило, чтобы кот оказался на заборе."
                            }
                        },
                        {
                            "text": "Tancerz wykonał efektowny <hop> w rytm szybkiej muzyki.",
                            "target_lang_translations": {
                                "ru": "Танцор сделал эффектный <прыжок> в ритме быстрой музыки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used mainly in informal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одиночный прыжок или подскок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прыжок",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "скачок",
                                "target_lang_sense_clarification": "резкое перемещение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
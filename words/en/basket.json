    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b688e82b-2a45-3b42-9cae-7d5db4489f02",
                    "sense_definition": "A lightweight container, generally round, open at the top, and tapering toward the bottom, often made of woven material.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She placed the fresh apples into the wicker <w>basket</w> on the table.",
                            "target_lang_translations": {
                                "ru": "Она положила свежие яблоки в плетеную <w>корзину</w> на столе."
                            }
                        },
                        {
                            "text": "We packed a large picnic <w>basket</w> with sandwiches and drinks for the trip.",
                            "target_lang_translations": {
                                "ru": "Мы собрали большую <w>корзину</w> для пикника с сэндвичами и напитками для поездки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wicker basket",
                        "laundry basket",
                        "picnic basket",
                        "fruit basket"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкая емкость, обычно круглая, открытая сверху и сужающаяся к низу, часто сплетенная из прутьев или другого материала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корзина",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "корзинка",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное"
                            },
                            {
                                "target_lang_word": "лукошко",
                                "target_lang_sense_clarification": "небольшая ручная корзина из луба или прутьев"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c74bdcc-c61e-3a00-8cf4-234d553bb417",
                    "sense_definition": "A wire or plastic container used for carrying articles for purchase in a shop.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "shopping_senses",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I picked up a red <w>basket</w> at the entrance of the grocery store.",
                            "target_lang_translations": {
                                "ru": "Я взял красную <w>корзину</w> у входа в продуктовый магазин."
                            }
                        },
                        {
                            "text": "Her shopping <w>basket</w> was full of vegetables and bread.",
                            "target_lang_translations": {
                                "ru": "Её <w>корзина</w> для покупок была полна овощей и хлеба."
                            }
                        }
                    ],
                    "synonyms": [
                        "shopping basket"
                    ],
                    "common_phrases": [
                        "shopping basket"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проволочная или пластиковая емкость, используемая для переноски товаров в магазине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корзина",
                                "target_lang_sense_clarification": "для покупок"
                            },
                            {
                                "target_lang_word": "корзинка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10ab01ba-3d08-3854-96ae-5401b409cfcc",
                    "sense_definition": "In an online shop, a virtual listing of a customer's chosen items before they are ordered.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "shopping_senses",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Click the button to add this book to your online <w>basket</w>.",
                            "target_lang_translations": {
                                "ru": "Нажмите кнопку, чтобы добавить эту книгу в вашу виртуальную <w>корзину</w>."
                            }
                        },
                        {
                            "text": "You can review the items in your <w>basket</w> before you pay.",
                            "target_lang_translations": {
                                "ru": "Вы можете просмотреть товары в вашей <w>корзине</w> перед оплатой."
                            }
                        }
                    ],
                    "synonyms": [
                        "cart",
                        "shopping cart"
                    ],
                    "common_phrases": [
                        "add to basket",
                        "view basket",
                        "empty basket"
                    ],
                    "target_lang_definitions": {
                        "ru": "В интернет-магазине: виртуальный список выбранных покупателем товаров перед оформлением заказа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корзина",
                                "target_lang_sense_clarification": "на сайте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8804b9ae-2c86-34e0-abca-59203a948453",
                    "sense_definition": "A circular hoop with a suspended net through which players try to throw the ball in basketball.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "basketball_senses",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The player jumped high and dunked the ball into the <w>basket</w>.",
                            "target_lang_translations": {
                                "ru": "Игрок высоко подпрыгнул и забросил мяч в <w>корзину</w> сверху."
                            }
                        },
                        {
                            "text": "He threw the ball, but it missed the <w>basket</w> completely.",
                            "target_lang_translations": {
                                "ru": "Он бросил мяч, но вообще не попал в <w>корзину</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "hoops"
                    ],
                    "common_phrases": [
                        "aim for the basket",
                        "hit the basket"
                    ],
                    "target_lang_definitions": {
                        "ru": "Круглый обруч с подвешенной сеткой, в который игроки пытаются забросить мяч в баскетболе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корзина",
                                "target_lang_sense_clarification": "баскетбольная"
                            },
                            {
                                "target_lang_word": "кольцо",
                                "target_lang_sense_clarification": "часть конструкции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d2a3908-af23-3007-80f5-3a2dd9e6970c",
                    "sense_definition": "The act of putting the ball through the hoop, thereby scoring points.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "basketball_senses",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team needed one more <w>basket</w> to win the championship game.",
                            "target_lang_translations": {
                                "ru": "Команде нужно было еще одно <w>попадание</w>, чтобы выиграть финальный матч."
                            }
                        },
                        {
                            "text": "She scored a <w>basket</w> from the three-point line just before the buzzer.",
                            "target_lang_translations": {
                                "ru": "Она забросила <w>мяч</w> с трехочковой линии прямо перед сиреной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "score a basket",
                        "three-point basket"
                    ],
                    "target_lang_definitions": {
                        "ru": "Попадание мячом в кольцо, приносящее очки."

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "46da03a5-a19d-3f21-8494-fbc6884e0f87",
                    "sense_definition": "Harmful to health; likely to cause illness or poor physical condition.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Eating too much sugar is <w>unhealthy</w> for your teeth.",
                            "target_lang_translations": {
                                "ru": "Есть слишком много сахара — <w>вредно</w> для зубов."
                            }
                        },
                        {
                            "text": "He decided to change his <w>unhealthy</w> lifestyle and start exercising.",
                            "target_lang_translations": {
                                "ru": "Он решил изменить свой <w>нездоровый</w> образ жизни и начать заниматься спортом."
                            }
                        }
                    ],
                    "antonyms": [
                        "healthy"
                    ],
                    "common_phrases": [
                        "unhealthy diet",
                        "unhealthy food",
                        "unhealthy lifestyle",
                        "unhealthy habit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вредный для здоровья; способный вызвать болезнь или плохое самочувствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вредный",
                                "target_lang_sense_clarification": "опасный для здоровья (о еде, привычках)"
                            },
                            {
                                "target_lang_word": "нездоровый",
                                "target_lang_sense_clarification": "не способствующий здоровью (об образе жизни)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "268bb33b-49ce-3fd8-9d3a-9e1f68b1ff7b",
                    "sense_definition": "Not in good physical condition; suffering from illness or weakness.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The child looked pale and <w>unhealthy</w> after the long winter.",
                            "target_lang_translations": {
                                "ru": "Ребенок выглядел бледным и <w>нездоровым</w> после долгой зимы."
                            }
                        },
                        {
                            "text": "He has been <w>unhealthy</w> for most of his life due to a chronic condition.",
                            "target_lang_translations": {
                                "ru": "Он был <w>болезненным</w> большую часть жизни из-за хронического заболевания."
                            }
                        }
                    ],
                    "synonyms": [
                        "diseased"
                    ],
                    "antonyms": [
                        "healthy"
                    ],
                    "common_phrases": [
                        "look unhealthy",
                        "unhealthy appearance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий слабое здоровье; выглядящий больным или слабым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нездоровый",
                                "target_lang_sense_clarification": "больной, плохо себя чувствующий"
                            },
                            {
                                "target_lang_word": "болезненный",
                                "target_lang_sense_clarification": "имеющий признаки болезни (о внешнем виде)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4cedb931-d428-3169-b1ab-dde17fb3a61a",
                    "sense_definition": "Not normal or natural, often suggesting mental instability or obsession.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has an <w>unhealthy</w> obsession with checking her phone every minute.",
                            "target_lang_translations": {
                                "ru": "У нее <w>нездоровая</w> одержимость проверкой телефона каждую минуту."
                            }
                        },
                        {
                            "text": "Staying in an <w>unhealthy</w> relationship can cause a lot of emotional stress.",
                            "target_lang_translations": {
                                "ru": "Пребывание в <w>нездоровых</w> отношениях может вызвать сильный эмоциональный стресс."
                            }
                        }
                    ],
                    "antonyms": [
                        "healthy"
                    ],
                    "common_phrases": [
                        "unhealthy obsession",
                        "unhealthy relationship",
                        "unhealthy interest"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ненормальный или неестественный, часто указывающий на психическую нестабильность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нездоровый",
                                "target_lang_sense_clarification": "патологический, ненормальный (об интересе, отношениях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b378d1f5-3f50-3d4f-918d-7603f5a73500",
                    "sense_definition": "Morally harmful or tending to corrupt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The book was criticized for promoting <w>unhealthy</w> ideas to young readers.",
                            "target_lang_translations": {
                                "ru": "Книгу критиковали за продвижение <w>вредных</w> идей среди юных читателей."
                            }
                        },
                        {
                            "text": "He fell under the <w>unhealthy</w> influence of a local gang.",
                            "target_lang_translations": {
                                "ru": "Он попал под <w>дурное</w> влияние местной банды."
                            }
                        }
                    ],
                    "antonyms": [
                        "healthy"
                    ],
                    "common_phrases": [
                        "unhealthy influence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Морально вредный или развращающий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вредный",
                                "target_lang_sense_clarification": "негативный (об идеях)"
                            },
                            {
                                "target_lang_word": "дурной",
                                "target_lang_sense_clarification": "плохой (о влиянии)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

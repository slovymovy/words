{
    "word_family": [
        "kawałek",
        "kawalarz",
        "kawalątko"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "20975e2e-ad30-31eb-9ea1-12f8715416c2",
                    "sense_definition": "Krótka, zabawna historyjka zakończona zaskakującą puentą, opowiadana w celu rozśmieszenia słuchaczy.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "humor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój wujek zawsze opowiada przy stole śmieszne <w>kawały</w>.",
                            "target_lang_translations": {
                                "ru": "Мой дядя всегда рассказывает за столом смешные <w>анекдоты</w>."
                            }
                        },
                        {
                            "text": "Znasz ten stary <w>kawał</w> o bacy i turyście?",
                            "target_lang_translations": {
                                "ru": "Ты знаешь этот старый <w>анекдот</w> про пастуха и туриста?"
                            }
                        }
                    ],
                    "synonyms": [
                        "dowcip",
                        "żart",
                        "wic",
                        "anegdota"
                    ],
                    "common_phrases": [
                        "opowiadać kawały",
                        "śmieszny kawał",
                        "stary kawał",
                        "znać kawał"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткая забавная история с неожиданной концовкой, которую рассказывают, чтобы рассмешить слушателей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "анекдот",
                                "target_lang_sense_clarification": "смешная история"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb4ada57-06c3-38a7-b4e6-663048cdf025",
                    "sense_definition": "Duża, odcięta lub oderwana część czegoś; spory fragment materii.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ukroił sobie solidny <w>kawał</w> chleba do zupy.",
                            "target_lang_translations": {
                                "ru": "Он отрезал себе солидный <w>ломоть</w> хлеба к супу."
                            }
                        },
                        {
                            "text": "Pies porwał ze stołu wielki <w>kawał</w> mięsa.",
                            "target_lang_translations": {
                                "ru": "Собака стащила со стола огромный <w>кусок</w> мяса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kawał chleba",
                        "kawał mięsa",
                        "kawał ciasta",
                        "kawał drewna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая, отрезанная или оторванная часть чего-либо; крупный фрагмент материи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кусок",
                                "target_lang_sense_clarification": "часть чего-либо"
                            },
                            {
                                "target_lang_word": "ломоть",
                                "target_lang_sense_clarification": "большой отрезанный кусок (обычно хлеба)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ba35f097-14c9-3f71-ab41-fad9c103ce55",
                    "sense_definition": "Psikus lub żart sytuacyjny zrobiony komuś, często sprawiający tej osobie kłopot lub zaskoczenie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "humor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uczniowie zrobili nauczycielowi głupi <w>kawał</w> na prima aprilis.",
                            "target_lang_translations": {
                                "ru": "Ученики устроили учителю глупый <w>розыгрыш</w> на первое апреля."
                            }
                        },
                        {
                            "text": "To nie była prawda, tylko taki niewinny <w>kawał</w>.",
                            "target_lang_translations": {
                                "ru": "Это была не правда, а просто такая невинная <w>шутка</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "psikus",
                        "żart"
                    ],
                    "common_phrases": [
                        "zrobić komuś kawał",
                        "głupi kawał",
                        "niezły kawał"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проделка или ситуативная шутка над кем-то, часто доставляющая этому человеку хлопоты или вызывающая удивление."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "розыгрыш",
                                "target_lang_sense_clarification": "шутка над кем-то"
                            },
                            {
                                "target_lang_word": "шутка",
                                "target_lang_sense_clarification": "поступок ради забавы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c954cc07-02d6-3375-ae32-45032235acce",
                    "sense_definition": "Znaczna ilość czegoś, duża odległość lub długi okres czasu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mamy jeszcze spory <w>kawał</w> drogi do domu.",
                            "target_lang_translations": {
                                "ru": "Нам до дома ещё приличный <w>путь</w>."
                            }
                        },
                        {
                            "text": "Nie widzieliśmy się z Markiem już <w>kawał</w> czasu.",
                            "target_lang_translations": {
                                "ru": "Мы не виделись с Марком уже <w>кучу</w> времени."
                            }
                        }
                    ],
                    "synonyms": [
                        "czas"
                    ],
                    "common_phrases": [
                        "kawał drogi",
                        "kawał czasu",
                        "kawał roboty",
                        "kawał świata"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in phrases describing large quantities or distances"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Значительное количество чего-либо, большое расстояние или долгий период времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "куча",
                                "target_lang_sense_clarification": "разговорное, о большом количестве"
                            },
                            {
                                "target_lang_word": "масса",
                                "target_lang_sense_clarification": "множество"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                {
                    "sense_id": "39cfb028-9ede-3285-9ab3-ff233623c1b3",
                    "sense_definition": "De hoeveelheid, waarde of het bedrag van iets groter maken.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "increase",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De supermarkt heeft de prijzen van het fruit weer <w>verhoogd</w>.",
                            "target_lang_translations": {
                                "ru": "Супермаркет снова <w>повысил</w> цены на фрукты."
                            }
                        },
                        {
                            "text": "De regering <w>verhoogde</w> de belastingen om meer geld binnen te krijgen.",
                            "target_lang_translations": {
                                "ru": "Правительство <w>повысило</w> налоги, чтобы собрать больше денег."
                            }
                        }
                    ],
                    "synonyms": [
                        "vermeerderen"
                    ],
                    "antonyms": [
                        "verlagen"
                    ],
                    "common_phrases": [
                        "de prijs verhogen",
                        "de belasting verhogen",
                        "het salaris verhogen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увеличивать количество, стоимость или сумму чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повышать",
                                "target_lang_sense_clarification": "цены, налоги, зарплату"
                            },
                            {
                                "target_lang_word": "увеличивать",
                                "target_lang_sense_clarification": "количество, объем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93c25bac-4e6e-3733-9726-fd02e97c8c86",
                    "sense_definition": "Iets naar een hoger niveau, peil of betere stemming brengen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "heighten",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vrolijke muziek <w>verhoogt</w> de sfeer van het feest aanzienlijk.",

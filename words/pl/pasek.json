    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0a922d62-223c-3283-8bd6-920d629fa249",
                    "sense_definition": "Wąski pas materiału, skóry lub tworzywa, noszony w talii w celu podtrzymania ubrania lub jako ozdoba; także pasek do zegarka.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "clothing_accessory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę kupić nowy skórzany <pasek> do tych spodni.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новый кожаный <ремень> к этим брюкам."
                            }
                        },
                        {
                            "text": "Zapiął <pasek> na ostatnią dziurkę, bo schudł.",
                            "target_lang_translations": {
                                "ru": "Он застегнул <ремень> на последнюю дырку, так как похудел."
                            }
                        }
                    ],
                    "synonyms": [
                        "pas"
                    ],
                    "common_phrases": [
                        "pasek do spodni",
                        "skórzany pasek",
                        "zapiąć pasek",
                        "pasek od zegarka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Узкая полоса материала, кожи или пластика, которую носят на талии для поддержания одежды или как украшение; также ремешок для часов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ремень",
                                "target_lang_sense_clarification": "для одежды"
                            },
                            {
                                "target_lang_word": "ремешок",
                                "target_lang_sense_clarification": "для часов"
                            },
                            {
                                "target_lang_word": "пояс",
                                "target_lang_sense_clarification": "аксессуар"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a681203-d17a-3c45-b03d-224f8c1fd863",
                    "sense_definition": "Wąska linia lub smuga odróżniająca się kolorem lub fakturą od tła, często występująca jako element wzoru na tkaninie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "pattern",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej ulubiona sukienka ma białe i niebieskie <paski>.",
                            "target_lang_translations": {
                                "ru": "На её любимом платье белые и синие <полоски>."
                            }
                        },
                        {
                            "text": "Narysował czerwony <pasek> na dole kartki papieru.",
                            "target_lang_translations": {
                                "ru": "Он нарисовал красную <полоску> внизу листа бумаги."
                            }
                        }
                    ],
                    "synonyms": [
                        "prążek",
                        "pas"
                    ],
                    "common_phrases": [
                        "w paski",
                        "koszula w paski",
                        "czarny pasek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Узкая линия или полоса, отличающаяся цветом или фактурой от фона, часто встречающаяся как элемент узора на ткани."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полоска",
                                "target_lang_sense_clarification": "узор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "579e8a16-0766-39c0-a660-658a55ec8829",
                    "sense_definition": "Wąski, podłużny kawałek jakiegoś materiału, papieru lub wycinek powierzchni.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "shape_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wyciął wąski <pasek> z gazety, aby zaznaczyć stronę.",
                            "target_lang_translations": {
                                "ru": "Он вырезал узкую <полоску> из газеты, чтобы отметить страницу."
                            }
                        },
                        {
                            "text": "Szliśmy wąskim <paskiem> plaży tuż przy samym morzu.",
                            "target_lang_translations": {
                                "ru": "Мы шли по узкой <полосе> пляжа у самого моря."
                            }
                        }
                    ],
                    "synonyms": [
                        "pas"
                    ],
                    "common_phrases": [
                        "pasek papieru",
                        "pasek ziemi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Узкий, продолговатый кусок какого-либо материала, бумаги или участок поверхности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полоска",
                                "target_lang_sense_clarification": "кусок материала"
                            },
                            {
                                "target_lang_word": "полоса",
                                "target_lang_sense_clarification": "участок пространства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e9baf1f7-8b88-33bd-8d2d-344797ded8da",
                    "sense_definition": "Naszywka na mundurze w kształcie krótkiej linii, oznaczająca stopień wojskowy lub służbowy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "insignia",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młody oficer dumnie prezentował nowy <pasek> na pagonie.",
                            "target_lang_translations": {
                                "ru": "Молодой офицер гордо демонстрировал новую <лычку> на погоне."
                            }
                        },
                        {
                            "text": "Za zasługi w służbie otrzymał kolejne <paski> i awans.",
                            "target_lang_translations": {
                                "ru": "За заслуги по службе он получил очередные <лычки> и повышение."
                            }
                        }
                    ],
                    "synonyms": [
                        "belka"
                    ],
                    "common_phrases": [
                        "dostać paski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нашивка на форме в виде короткой линии, обозначающая воинское или служебное звание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лычка",
                                "target_lang_sense_clarification": "знак различия"
                            },
                            {
                                "target_lang_word": "нашивка",
                                "target_lang_sense_clarification": "на форме"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "ac71e8e6-49dc-39cf-9ea6-444b6276506c",
                    "sense_definition": "Polskie nazwisko męskie lub żeńskie.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Jan <Pasek> jest autorem słynnych pamiętników z siedemnastego wieku.",
                            "target_lang_translations": {
                                "ru": "Ян <Пасек> является автором знаменитых мемуаров семнадцатого века."
                            }
                        },
                        {
                            "text": "Nie ma dzisiaj w pracy pani <Pasek>, ponieważ wzięła urlop.",
                            "target_lang_translations": {
                                "ru": "Сегодня на работе нет пани <Пасек>, так как она взяла отпуск."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "popularne nazwisko w Polsce"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Польская мужская или женская фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Пасек",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

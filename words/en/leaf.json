        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c4ebb517-6591-3a33-b7ac-47da8417b0db",
                    "sense_definition": "To produce or grow leaves.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "plant_growth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The trees began to <w>leaf</w> out early this year due to the warm spring.",
                            "target_lang_translations": {
                                "ru": "Деревья начали <w>распускаться</w> рано в этом году из-за теплой весны."
                            }
                        },
                        {
                            "text": "Most oak trees <w>leaf</w> later than other types of trees in this forest.",
                            "target_lang_translations": {
                                "ru": "Большинство дубов <w>покрываются листвой</w> позже, чем другие виды деревьев в этом лесу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "leaf out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выпускать или отращивать листья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распускаться",
                                "target_lang_sense_clarification": "о листьях/деревьях"
                            },
                            {
                                "target_lang_word": "покрываться листвой",
                                "target_lang_sense_clarification": "о деревьях"
                            },
                            {
                                "target_lang_word": "зеленеть",
                                "target_lang_sense_clarification": "становиться зеленым"
                            }
                        ]
                    }
                }
            ]
        },
        {

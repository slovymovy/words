                },
                {
                    "sense_id": "257a310e-8190-3cba-b06e-18fcc7eb0a24",
                    "sense_definition": "Een idee, indruk of voorstelling die iemand in gedachten heeft over iets of iemand.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental_representation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb nog geen duidelijk <w>beeld</w> van wat er precies is gebeurd.",
                            "target_lang_translations": {
                                "en": "I do not yet have a clear <w>picture</w> of what exactly happened.",
                                "ru": "У меня пока нет ясного <w>представления</w> о том, что именно произошло."
                            }
                        },
                        {
                            "text": "Zijn verhalen gaven een vertekend <w>beeld</w> van de werkelijkheid.",
                            "target_lang_translations": {
                                "en": "His stories gave a distorted <w>picture</w> of reality.",
                                "ru": "Его рассказы давали искаженное <w>представление</w> о действительности."
                            }
                        }
                    ],
                    "synonyms": [
                        "idee",
                        "indruk",
                        "voorstelling"
                    ],
                    "common_phrases": [
                        "een beeld vormen",
                        "een goed beeld krijgen",
                        "een vertekend beeld"
                    ],
                    "target_lang_definitions": {
                        "en": "An idea, impression, or mental picture that someone has about something or someone.",
                        "ru": "Идея, впечатление или мысленный образ, который кто-то имеет о чем-то или ком-то."
                    },
                    "translations": {

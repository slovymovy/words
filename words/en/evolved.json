            "senses": [
                {
                    "sense_id": "799cf43e-e5dd-3d10-90ff-dbc203dd4d9f",
                    "sense_definition": "Having arisen or developed through a process of gradual change or evolution.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Humans are considered a highly <w>evolved</w> species with complex social structures.",
                            "target_lang_translations": {
                                "ru": "Люди считаются высоко <w>развитым</w> видом со сложными социальными структурами."
                            }
                        },
                        {
                            "text": "This is a more <w>evolved</w> version of the software we used last year.",
                            "target_lang_translations": {
                                "ru": "Это более <w>совершенная</w> версия программного обеспечения, которое мы использовали в прошлом году."
                            }
                        }
                    ],
                    "synonyms": [
                        "advanced",
                        "developed"
                    ],
                    "antonyms": [
                        "primitive",
                        "undeveloped"
                    ],
                    "common_phrases": [
                        "highly evolved",
                        "evolved form",
                        "more evolved"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достигший высокого уровня развития; возникший или сформировавшийся в процессе постепенных изменений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "развитый",
                                "target_lang_sense_clarification": "находящийся на высокой ступени развития"

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "62234b71-b679-32b3-a3b0-453ed12b0c0e",
                    "sense_definition": "To produce one or more copies of a text or image on a surface, especially by machine.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "produce_copy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>print</w> my homework assignment before class starts.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>распечатать</w> домашнее задание до начала урока."
                            }
                        },
                        {
                            "text": "Can you please <w>print</w> two copies of the train tickets?",
                            "target_lang_translations": {
                                "ru": "Можешь, пожалуйста, <w>распечатать</w> две копии билетов на поезд?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "print out",
                        "print a document",
                        "print copies"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать копии текста или изображения на поверхности, обычно с помощью машины (принтера)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "печатать",
                                "target_lang_sense_clarification": "процесс создания копий"
                            },
                            {
                                "target_lang_word": "распечатывать",
                                "target_lang_sense_clarification": "выводить на принтере (завершенное действие или процесс)"
                            },
                            {
                                "target_lang_word": "напечатать",
                                "target_lang_sense_clarification": "создать копию (совершенный вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "155a1f08-a378-3fa4-9882-ce2b08f81ecf",
                    "sense_definition": "To write very clearly, especially to write without connecting the letters as in cursive.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "handwriting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>print</w> your name clearly at the top of the form.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>напишите</w> ваше имя <w>печатными буквами</w> разборчиво в верхней части бланка."
                            }
                        },
                        {
                            "text": "Young children usually learn to <w>print</w> before they learn to write in cursive.",
                            "target_lang_translations": {
                                "ru": "Маленькие дети обычно учатся <w>писать печатными буквами</w> до того, как учатся писать прописью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "print your name",
                        "print clearly"
                    ],
                    "target_lang_definitions": {
                        "ru": "Писать очень четко, не соединяя буквы между собой, в отличие от курсива."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "писать печатными буквами",
                                "target_lang_sense_clarification": "способ письма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73f24f12-1629-3067-8ce1-5352a995da5a",
                    "sense_definition": "To publish text in a book, newspaper, or magazine.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "publish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The newspaper refused to <w>print</w> the rumor without proof.",
                            "target_lang_translations": {
                                "ru": "Газета отказалась <w>печатать</w> слух без доказательств."
                            }
                        },
                        {
                            "text": "They will <w>print</w> the winning story in next month's issue.",
                            "target_lang_translations": {
                                "ru": "Они <w>напечатают</w> победивший рассказ в выпуске следующего месяца."
                            }
                        }
                    ],
                    "common_phrases": [
                        "print a story",
                        "fit to print",
                        "print an article"
                    ],
                    "target_lang_definitions": {
                        "ru": "Публиковать текст в книге, газете или журнале."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "публиковать",
                                "target_lang_sense_clarification": "издавать в свет"
                            },
                            {
                                "target_lang_word": "печатать",
                                "target_lang_sense_clarification": "размещать в издании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "54b3e086-c651-3d62-ada4-854a4c819fee",
                    "sense_definition": "To display a string of characters or data on a computer screen or terminal.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The program will <w>print</w> an error message if the data is invalid.",
                            "target_lang_translations": {
                                "ru": "Программа <w>выведет</w> сообщение об ошибке, если данные неверны."
                            }
                        },
                        {
                            "text": "You can use this command to <w>print</w> the list of files to the screen.",
                            "target_lang_translations": {
                                "ru": "Вы можете использовать эту команду, чтобы <w>вывести</w> список файлов на экран."
                            }
                        }
                    ],
                    "common_phrases": [
                        "print to screen",
                        "print a variable"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отображать строку символов или данные на экране компьютера или терминале."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выводить",
                                "target_lang_sense_clarification": "на экран"
                            },
                            {
                                "target_lang_word": "печатать",
                                "target_lang_sense_clarification": "компьютерный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c97ccbfb-accf-3fbd-9552-a02d7afda26b",
                    "sense_definition": "To mark a surface by pressing something into it or stamping it with ink.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "impress",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy boots <w>printed</w> deep marks into the soft mud.",
                            "target_lang_translations": {
                                "ru": "Тяжелые ботинки <w>оставили</w> глубокие <w>отпечатки</w> в мягкой грязи."
                            }
                        },
                        {
                            "text": "The machine can <w>print</w> a complex design onto the fabric.",
                            "target_lang_translations": {
                                "ru": "Эта машина может <w>наносить</w> сложный узор на ткань."
                            }
                        }
                    ],
                    "common_phrases": [
                        "print a pattern",
                        "print on fabric"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оставлять след на поверхности путем надавливания или нанесения краски."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оставлять отпечаток",
                                "target_lang_sense_clarification": "следить"
                            },
                            {
                                "target_lang_word": "наносить",
                                "target_lang_sense_clarification": "рисунок или узор"
                            }
                        ]
                    }
                }
            ]
        },
        {

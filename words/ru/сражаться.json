                        "отступать"
                    ],
                    "common_phrases": [
                        "сражаться за родину",
                        "сражаться с врагом",
                        "сражаться до конца",
                        "храбро сражаться"
                    ],
                    "target_lang_definitions": {
                        "en": "To engage in combat or armed conflict with an enemy."
                    },
                    "translations": {

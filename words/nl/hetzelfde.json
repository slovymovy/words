            "senses": [
                {
                    "sense_id": "f0cfb060-023c-38cd-a0ad-85abfa424b0c",
                    "sense_definition": "Op een manier die niet verschilt van iets anders; op gelijke wijze.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De tweelingbroers denken over veel zaken precies <w>hetzelfde</w>.",
                            "target_lang_translations": {
                                "en": "The twin brothers think exactly <w>the same</w> about many things.",
                                "ru": "Братья-близнецы думают о многих вещах совершенно <w>одинаково</w>."
                            }
                        },
                        {
                            "text": "Deze twee koekjes smaken <w>hetzelfde</w>, ook al zien ze er anders uit.",
                            "target_lang_translations": {
                                "en": "These two cookies taste <w>the same</w>, even though they look different.",
                                "ru": "Эти два печенья <w>одинаковы</w> на вкус, хотя и выглядят по-разному."
                            }
                        }
                    ],
                    "synonyms": [
                        "gelijk",
                        "identiek",
                        "eender"
                    ],
                    "antonyms": [
                        "anders",
                        "verschillend"
                    ],
                    "common_phrases": [
                        "precies hetzelfde"
                    ],
                    "target_lang_definitions": {
                        "en": "In a way that does not differ from something else; in a similar manner.",
                        "ru": "Образом, который не отличается от чего-либо другого; одинаково."
                    },
                    "translations": {
                        "en": [

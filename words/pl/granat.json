    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c8df8fbc-5733-3a45-8cb1-814a3bd3a983",
                    "sense_definition": "Egzotyczny owoc o twardej skórce, wypełniony wieloma nasionami otoczonymi soczystym, czerwonym miąższem",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "botany_food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem w sklepie dwa dojrzałe <w>granaty</w>.",
                            "target_lang_translations": {
                                "ru": "Я купил в магазине два спелых <w>граната</w>."
                            }
                        },
                        {
                            "text": "Sok z <w>granatu</w> jest bardzo zdrowy i orzeźwiający.",
                            "target_lang_translations": {
                                "ru": "Сок из <w>граната</w> очень полезный и освежающий."
                            }
                        }
                    ],
                    "synonyms": [
                        "owoc granatowca"
                    ],
                    "common_phrases": [
                        "sok z granatu",
                        "pestki granatu",
                        "dojrzały granat",
                        "obierać granat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Экзотический фрукт с твердой кожурой, наполненный множеством семян, окруженных сочной красной мякотью"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гранат",
                                "target_lang_sense_clarification": "фрукт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2d29f0e-9816-3881-b7bf-d71128f6fce8",
                    "sense_definition": "Pocisk wypełniony materiałem wybuchowym, wyrzucany ręcznie lub wystrzeliwany z broni",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "military_weapon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żołnierz wyciągnął zawleczkę i rzucił <w>granat</w> w stronę celu.",
                            "target_lang_translations": {
                                "ru": "Солдат выдернул чеку и бросил <w>гранату</w> в сторону цели."
                            }
                        },
                        {
                            "text": "Eksplozja <w>granatu</w> uszkodziła pobliski mur.",
                            "target_lang_translations": {
                                "ru": "Взрыв <w>гранаты</w> повредил стену поблизости."
                            }
                        }
                    ],
                    "common_phrases": [
                        "granat ręczny",
                        "wybuch granatu",
                        "rzucić granat",
                        "granat dymny",
                        "odłamki granatu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снаряд, наполненный взрывчатым веществом, который бросают рукой или выстреливают из оружия"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "граната",
                                "target_lang_sense_clarification": "боеприпас"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7833ccbe-8f5f-3369-adea-298927af61b5",
                    "sense_definition": "Bardzo ciemny odcień koloru niebieskiego",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej nowy płaszcz ma odcień głębokiego <w>granatu</w>.",
                            "target_lang_translations": {
                                "ru": "Её новое пальто имеет оттенок глубокого <w>тёмно-синего цвета</w>."
                            }
                        },
                        {
                            "text": "W tym sezonie w modzie dominuje czerń i <w>granat</w>.",
                            "target_lang_translations": {
                                "ru": "В этом сезоне в моде доминируют чёрный и <w>тёмно-синий</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "głęboki granat",
                        "odcień granatu",
                        "ubrany w granat"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень темный оттенок синего цвета"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тёмно-синий цвет",
                                "target_lang_sense_clarification": "цвет"
                            },
                            {
                                "target_lang_word": "тёмно-синий",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3643968-4782-3805-a913-92fdd7ef3a43",
                    "sense_definition": "Twardy minerał, kamień półszlachetny o barwie najczęściej ciemnoczerwonej, używany w jubilerstwie",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mineralogy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dostała od męża piękne kolczyki z <w>granatami</w>.",
                            "target_lang_translations": {
                                "ru": "Она получила от мужа красивые серьги с <w>гранатами</w>."
                            }
                        },
                        {
                            "text": "Ten <w>granat</w> ma wyjątkowo czystą barwę i idealny szlif.",
                            "target_lang_translations": {
                                "ru": "У этого <w>граната</w> исключительно чистый цвет и идеальная огранка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pierścionek z granatem",
                        "naszyjnik z granatów",
                        "szlifowanie granatów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердый минерал, полудрагоценный камень, чаще всего темно-красного цвета, используемый в ювелирном деле"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гранат",
                                "target_lang_sense_clarification": "минерал"
                            }
                        ]
                    }
                }

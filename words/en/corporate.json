    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1c965e5f-741a-3aab-a6b9-7169222133cb",
                    "sense_definition": "Of or relating to a large company or group.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "business_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She climbed the <w>corporate</w> ladder very quickly after joining the firm.",
                            "target_lang_translations": {
                                "ru": "Она очень быстро поднялась по <w>корпоративной</w> лестнице после прихода в фирму."
                            }
                        },
                        {
                            "text": "The company has a very strict <w>corporate</w> policy regarding dress code.",
                            "target_lang_translations": {
                                "ru": "В компании действует очень строгая <w>корпоративная</w> политика касательно дресс-кода."
                            }
                        }
                    ],
                    "synonyms": [
                        "business",
                        "commercial"
                    ],
                    "antonyms": [
                        "private",
                        "personal"
                    ],
                    "common_phrases": [
                        "corporate culture",
                        "corporate strategy",
                        "corporate identity",
                        "corporate ladder"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к крупной компании или группе компаний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корпорати́вный",
                                "target_lang_sense_clarification": "относящийся к корпорации"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13b43bb4-bd9e-35eb-a861-ac289017dd35",
                    "sense_definition": "Shared by or involving a whole group of people.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "collective_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee made a <w>corporate</w> decision to delay the project until next year.",
                            "target_lang_translations": {
                                "ru": "Комитет принял <w>коллективное</w> решение отложить проект до следующего года."
                            }
                        },
                        {
                            "text": "They accepted <w>corporate</w> responsibility for the failure of the mission.",
                            "target_lang_translations": {
                                "ru": "Они приняли на себя <w>коллективную</w> ответственность за провал миссии."
                            }
                        }
                    ],
                    "synonyms": [
                        "collective",
                        "joint",
                        "communal",
                        "shared"
                    ],
                    "antonyms": [
                        "individual"
                    ],
                    "common_phrases": [
                        "corporate responsibility",
                        "corporate body",
                        "corporate worship"
                    ],
                    "target_lang_definitions": {
                        "ru": "Общий для группы людей или вовлекающий всю группу; совместный."

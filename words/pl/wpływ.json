    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a7c58b0b-be2d-35f7-96f3-06e4a01d2f56",
                    "sense_definition": "Oddziaływanie na kogoś lub na coś, zdolność wywoływania zmian w czyimś zachowaniu, myśleniu lub w przebiegu jakichś spraw",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego decyzja miała ogromny <w>wpływ</w> na naszą dalszą współpracę.",
                            "target_lang_translations": {
                                "ru": "Его решение оказало огромное <w>влияние</w> на наше дальнейшее сотрудничество."
                            }
                        },
                        {
                            "text": "Nie powinieneś podejmować ważnych decyzji, będąc pod <w>wpływem</w> silnych emocji.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует принимать важные решения под <w>влиянием</w> сильных эмоций."
                            }
                        }
                    ],
                    "synonyms": [
                        "oddziaływanie",
                        "działanie"
                    ],
                    "common_phrases": [
                        "mieć wpływ",
                        "pod wpływem",
                        "wywierać wpływ",
                        "zły wpływ"
                    ],
                    "target_lang_definitions": {
                        "ru": "Воздействие на кого-либо или что-либо, способность вызывать изменения в чьем-либо поведении, мышлении или ходе дел"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "влияние",
                                "target_lang_sense_clarification": "воздействие"
                            },
                            {
                                "target_lang_word": "воздействие",
                                "target_lang_sense_clarification": "активное влияние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e98bc62b-7a7f-3c08-a608-ed161ee800d4",
                    "sense_definition": "Możliwość załatwienia czegoś dzięki posiadanej pozycji, autorytetowi lub znajomościom; często używane w liczbie mnogiej",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polityk wykorzystał swoje <w>wpływy</w>, aby zapewnić finansowanie dla lokalnego szpitala.",
                            "target_lang_translations": {
                                "ru": "Политик использовал свое <w>влияние</w>, чтобы обеспечить финансирование для местной больницы."
                            }
                        },
                        {
                            "text": "Wielkie mocarstwa rywalizują o strefę <w>wpływów</w> w tym regionie świata.",
                            "target_lang_translations": {
                                "ru": "Великие державы соперничают за сферу <w>влияния</w> в этом регионе мира."
                            }
                        }
                    ],
                    "synonyms": [
                        "władza",
                        "władztwo"
                    ],
                    "common_phrases": [
                        "strefa wpływów",
                        "mieć wpływy",
                        "tracić wpływy"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "często w liczbie mnogiej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Возможность добиться чего-либо благодаря занимаемому положению, авторитету или связям"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "влияние",
                                "target_lang_sense_clarification": "авторитет, власть"
                            },
                            {
                                "target_lang_word": "связи",
                                "target_lang_sense_clarification": "знакомства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d237566-8e7c-3f6e-8141-d3e2669c6105",
                    "sense_definition": "Pieniądze, które wpływają do kasy, banku lub budżetu instytucji; dochód",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tegoroczne <w>wpływy</w> z podatków okazały się wyższe niż przewidywali ekonomiści.",
                            "target_lang_translations": {
                                "ru": "Налоговые <w>поступления</w> в этом году оказались выше, чем прогнозировали экономисты."
                            }
                        },
                        {
                            "text": "Całe <w>wpływy</w> ze sprzedaży biletów zostaną przekazane na cele charytatywne.",
                            "target_lang_translations": {
                                "ru": "Все <w>доходы</w> от продажи билетов будут переданы на благотворительные цели."
                            }
                        }
                    ],
                    "synonyms": [
                        "władza"
                    ],
                    "antonyms": [
                        "wypływ"
                    ],
                    "common_phrases": [
                        "wpływy do budżetu",
                        "wpływy z biletów"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "zazwyczaj w liczbie mnogiej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежные средства, поступающие в кассу, банк или бюджет; доход"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поступления",

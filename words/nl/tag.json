                },
                {
                    "sense_id": "062e78a2-c26b-3994-91a8-c7f211fc4600",
                    "sense_definition": "Een code in een opmaaktaal die aangeeft hoe tekst of data moet worden weergegeven.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "digital_identifier",
                    "name_type": "no",
                    "examples": [

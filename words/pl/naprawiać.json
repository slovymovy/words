                {
                    "sense_id": "4084143b-dbc5-3442-9917-f35a3e06e074",
                    "sense_definition": "Przywracać zepsuty lub uszkodzony przedmiot do stanu używalności; sprawiać, że coś znów działa.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój tata potrafi <naprawić> prawie każde urządzenie domowe.",
                            "target_lang_translations": {
                                "ru": "Мой папа может <w>починить</w> почти любой бытовой прибор."
                            }
                        },
                        {
                            "text": "Musimy wezwać mechanika, który <naprawia> silniki samochodowe.",
                            "target_lang_translations": {
                                "ru": "Нам нужно вызвать механика, который <w>ремонтирует</w> автомобильные двигатели."
                            }
                        }
                    ],
                    "synonyms": [
                        "reperować",
                        "prostować"
                    ],
                    "antonyms": [
                        "psuć",
                        "zniszczyć",
                        "uszkodzić",
                        "popsuć",
                        "złamać"
                    ],
                    "common_phrases": [
                        "naprawiać samochód",
                        "naprawiać dach",
                        "naprawiać buty",
                        "naprawiać sprzęt"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить сломанный или поврежденный предмет в рабочее состояние; делать так, чтобы что-то снова работало."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чинить",
                                "target_lang_sense_clarification": "общие предметы, бытовой ремонт"
                            },
                            {
                                "target_lang_word": "ремонтировать",
                                "target_lang_sense_clarification": "технику, здания, более формально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90326f46-bf05-3264-82a3-906d05156556",
                    "sense_definition": "Usuwać błędy, korygować pomyłki lub czynić złą sytuację lepszą.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel poprosił ucznia, aby <naprawił> błędy w swoim wypracowaniu.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил ученика <w>исправить</w> ошибки в своем сочинении."
                            }
                        },
                        {
                            "text": "Próbuję <naprawić> moje relacje z bratem po naszej ostatniej kłótni.",
                            "target_lang_translations": {
                                "ru": "Я пытаюсь <w>наладить</w> свои отношения с братом после нашей последней ссоры."
                            }
                        }
                    ],
                    "synonyms": [

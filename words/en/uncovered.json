                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers stood <w>uncovered</w> during the solemn ceremony.",
                            "target_lang_translations": {
                                "ru": "Солдаты стояли <w>с непокрытыми головами</w> во время торжественной церемонии."
                            }
                        },
                        {
                            "text": "He walked <w>uncovered</w> through the storm, ignoring the rain.",
                            "target_lang_translations": {
                                "ru": "Он шел сквозь бурю <w>с непокрытой головой</w>, не обращая внимания на дождь."
                            }
                        }
                    ],
                    "antonyms": [
                        "covered"
                    ],
                    "common_phrases": [
                        "stood uncovered"
                    ],
                    "target_lang_definitions": {

                },
                {
                    "sense_id": "cc723a16-2eca-3418-867e-84e40092edea",
                    "sense_definition": "Forma mianownika, biernika i wołacza liczby mnogiej rzeczownika <w>białko</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "form_białko",
                    "name_type": "no",
                    "examples": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d7811de6-170d-36aa-a147-0df42f522bc0",
                    "sense_definition": "Two similar or identical things used together or regarded as a unit.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "set_of_two",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to buy a new <w>pair</w> of shoes for work.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новую <w>пару</w> туфель для работы."
                            }
                        },
                        {
                            "text": "She put on a fresh <w>pair</w> of socks before going out.",
                            "target_lang_translations": {
                                "ru": "Она надела чистую <w>пару</w> носков перед выходом."
                            }
                        }
                    ],
                    "synonyms": [
                        "couple"
                    ],
                    "common_phrases": [
                        "pair of shoes",
                        "pair of socks",
                        "pair of eyes",
                        "pair of hands"
                    ],
                    "target_lang_definitions": {
                        "ru": "Два похожих или одинаковых предмета, используемых вместе или рассматриваемых как единое целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пара",
                                "target_lang_sense_clarification": "набор из двух предметов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11d3b498-e5c0-35f6-93c2-39eb4c2c62df",

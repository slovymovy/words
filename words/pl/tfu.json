    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "0fe20a33-37df-3f72-9a96-03a2a431902e",
                    "sense_definition": "Dźwięk naśladujący splunięcie, używany do wyrażenia silnego wstrętu, obrzydzenia lub pogardy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "disgust",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spróbował zupy i od razu krzyknął: <w>tfu</w>, jakie to słone!",
                            "target_lang_translations": {
                                "ru": "Он попробовал суп и сразу крикнул: <w>тьфу</w>, какой он соленый!"
                            }
                        },
                        {
                            "text": "<w>Tfu</w>, nie chcę mieć z tymi ludźmi nic wspólnego.",
                            "target_lang_translations": {
                                "ru": "<w>Тьфу</w>, не хочу иметь с этими людьми ничего общего."
                            }
                        }
                    ],
                    "synonyms": [
                        "fe",
                        "pfu",
                        "tfuj"
                    ],
                    "common_phrases": [
                        "a tfu",
                        "tfu na psa urok"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Звук, имитирующий плевок, используемый для выражения сильного отвращения, брезгливости или презрения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тьфу",
                                "target_lang_sense_clarification": "выражение отвращения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0fdc06bc-078a-3730-9235-758d71c0e99e",
                    "sense_definition": "Wykrzyknik używany w mowie potocznej do natychmiastowego zasygnalizowania i poprawienia własnego przejęzyczenia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "correction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spotkamy się we wtorek, <w>tfu</w>, w środę po południu.",
                            "target_lang_translations": {
                                "ru": "Встретимся во вторник, <w>тьфу</w>, в среду после обеда."
                            }
                        },
                        {
                            "text": "To kosztowało sto złotych, <w>tfu</w>, dwieście złotych.",
                            "target_lang_translations": {
                                "ru": "Это стоило сто злотых, <w>тьфу</w>, двести злотых."
                            }
                        }
                    ],
                    "synonyms": [
                        "pfu"
                    ],
                    "common_phrases": [
                        "tfu, to znaczy",
                        "tfu, wróć"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Междометие, используемое в разговорной речи для немедленного исправления оговорки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тьфу",
                                "target_lang_sense_clarification": "исправление оговорки"
                            },
                            {
                                "target_lang_word": "тьфу ты",
                                "target_lang_sense_clarification": "досада при ошибке"
                            }
                        ]
                    }
                }
            ]
        }
    ]

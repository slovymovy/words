    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5de91af5-8b0b-3b71-84d7-1e618d6f42df",
                    "sense_definition": "Dokument uprawniający do wejścia na teren strzeżony, wyjścia z niego lub przebywania w określonym miejscu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "document_permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ochroniarz przy bramie sprawdził moją <w>przepustkę</w> i pozwolił mi wejść na teren fabryki.",
                            "target_lang_translations": {
                                "ru": "Охранник у ворот проверил мой <w>пропуск</w> и разрешил мне войти на территорию фабрики."
                            }
                        },
                        {
                            "text": "Zgubiłem swoją <w>przepustkę</w>, więc nie mogę dzisiaj dostać się do biura.",
                            "target_lang_translations": {
                                "ru": "Я потерял свой <w>пропуск</w>, поэтому не могу сегодня попасть в офис."
                            }
                        }
                    ],
                    "synonyms": [
                        "wejściówka",
                        "zezwolenie",
                        "identyfikator"
                    ],
                    "common_phrases": [
                        "okazać przepustkę",
                        "wydać przepustkę",
                        "przepustka stała",
                        "przepustka jednorazowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Документ, дающий право на вход на охраняемую территорию, выход с неё или пребывание в определённом месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пропуск",
                                "target_lang_sense_clarification": "документ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1876df29-19aa-3ba3-90de-11210b69b175",
                    "sense_definition": "Sposób, wydarzenie lub rzecz, która umożliwia osiągnięcie sukcesu lub dostęp do czegoś pożądanego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_access",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dobra znajomość języków obcych to twoja <w>przepustka</w> do międzynarodowej kariery.",
                            "target_lang_translations": {
                                "ru": "Хорошее знание иностранных языков — это твой <w>пропуск</w> в международную карьеру."
                            }
                        },
                        {
                            "text": "Wygrana w tym ważnym turnieju była dla drużyny <w>przepustką</w> do finału.",
                            "target_lang_translations": {
                                "ru": "Победа в этом важном турнире стала для команды <w>путевкой</w> в финал."
                            }
                        }
                    ],
                    "synonyms": [
                        "klucz",
                        "bilet",
                        "szansa",
                        "droga"
                    ],
                    "common_phrases": [
                        "przepustka do kariery",
                        "przepustka do sławy",
                        "przepustka do wolności",
                        "przepustka do świata"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способ, событие или предмет, открывающие путь к успеху или доступ к чему-либо желаемому."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пропуск",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "путевка",
                                "target_lang_sense_clarification": "шанс, возможность"
                            },
                            {
                                "target_lang_word": "билет",
                                "target_lang_sense_clarification": "доступ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "393e1b6c-bc95-32a6-a4ef-e3e8d43ce8d4",
                    "sense_definition": "Ruchoma przegroda w kanale lub rowie melioracyjnym, służąca do regulowania poziomu przepływu wody.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "technical_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnicy otworzyli <w>przepustkę</w>, aby nawodnić sąsiednie pola w czasie suszy.",
                            "target_lang_translations": {
                                "ru": "Фермеры открыли <w>заслонку</w>, чтобы оросить соседние поля во время засухи."
                            }
                        },
                        {
                            "text": "Stara, zardzewiała <w>przepustka</w> na kanale wymagała natychmiastowego remontu.",
                            "target_lang_translations": {
                                "ru": "Старая, ржавая <w>заслонка</w> на канале требовала немедленного ремонта."
                            }
                        }
                    ],
                    "synonyms": [
                        "zastawka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвижная перегородка в канале или мелиоративной канаве, служащая для регулирования уровня потока воды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заслонка",
                                "target_lang_sense_clarification": "гидротехника"
                            },
                            {
                                "target_lang_word": "шлюз",
                                "target_lang_sense_clarification": "регулятор воды"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "7dc1f999-cedb-3eca-a79d-8bd5f2c8a5c8",
                    "sense_definition": "Physically or mentally dependent on a particular substance or activity.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "dependency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He became <w>addicted</w> to painkillers after the surgery.",
                            "target_lang_translations": {
                                "ru": "Он стал <w>зависимым</w> от обезболивающих после операции."
                            }
                        },
                        {
                            "text": "Many people are <w>addicted</w> to nicotine without realizing how hard it is to quit.",
                            "target_lang_translations": {
                                "ru": "Многие люди <w>зависимы</w> от никотина, не осознавая, как трудно бросить."
                            }
                        }
                    ],
                    "synonyms": [
                        "dependent",
                        "hooked"
                    ],
                    "antonyms": [
                        "clean"
                    ],
                    "common_phrases": [
                        "addicted to drugs",
                        "addicted to alcohol",
                        "become addicted"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физически или психологически зависимый от определенного вещества или занятия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зависимый",
                                "target_lang_sense_clarification": "нуждающийся в чем-либо (наркотиках, алкоголе)"
                            },
                            {
                                "target_lang_word": "пристрастившийся",
                                "target_lang_sense_clarification": "приобретший пагубную привычку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "524ec1d1-262f-3c40-a526-8c13f6494426",
                    "sense_definition": "Very interested in or enthusiastic about something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "dependency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My brother is completely <w>addicted</w> to that new video game.",
                            "target_lang_translations": {
                                "ru": "Мой брат полностью <w>помешан</w> на этой новой видеоигре."
                            }
                        },
                        {
                            "text": "She is <w>addicted</w> to reading mystery novels and finishes one every week.",
                            "target_lang_translations": {
                                "ru": "Она <w>одержима</w> чтением детективных романов и прочитывает по одному каждую неделю."
                            }
                        }
                    ],
                    "synonyms": [
                        "obsessed",
                        "hooked"
                    ],
                    "antonyms": [
                        "indifferent"
                    ],
                    "common_phrases": [
                        "addicted to games",
                        "addicted to chocolate",
                        "addicted to shopping"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень заинтересованный чем-то или увлеченный чем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помешанный",
                                "target_lang_sense_clarification": "сильно увлеченный чем-то"
                            },
                            {
                                "target_lang_word": "одержимый",
                                "target_lang_sense_clarification": "страстно увлеченный"
                            },
                            {
                                "target_lang_word": "подсевший",
                                "target_lang_sense_clarification": "разговорное; увлеченный игрой, сериалом и т.п."
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "63f5ab16-e3fb-34cf-953e-b9c97c1102c3",

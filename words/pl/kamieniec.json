                {
                    "sense_id": "4a1f98b9-5bd7-3ee1-b80e-a3eebbbb6304",
                    "sense_definition": "Nazwa wielu miejscowości i wsi na terenie Polski.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "places",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Mój dziadek urodził się w małej wsi o nazwie <w>Kamieniec</w>.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка родился в маленькой деревне под названием <w>Каменец</w>."
                            }
                        },
                        {
                            "text": "Na mapie województwa dolnośląskiego łatwo odnaleźć <w>Kamieniec</w> Ząbkowicki.",
                            "target_lang_translations": {
                                "ru": "На карте Нижнесилезского воеводства легко найти <w>Каменец</w>-Зомбковицки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gmina Kamieniec",
                        "wieś Kamieniec"
                    ],
                    "target_lang_definitions": {
                        "ru": "Название многих населенных пунктов и деревень на территории Польши."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Каменец",
                                "target_lang_sense_clarification": "название населенного пункта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c25cdf6a-60a7-324e-bbf5-8391818b4a14",
                    "sense_definition": "Skrócona nazwa miasta Kamieniec Podolski, położonego na Ukrainie, znanego z historycznej twierdzy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "places",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Historyczna twierdza w <w>Kamieńcu</w> była kluczowym punktem obrony Rzeczypospolitej.",
                            "target_lang_translations": {
                                "ru": "Историческая крепость в <w>Каменце</w> была ключевым пунктом обороны Речи Посполитой."
                            }
                        },
                        {
                            "text": "Wielu turystów każdego roku odwiedza zabytkowy <w>Kamieniec</w> i jego okolice.",
                            "target_lang_translations": {
                                "ru": "Много туристов ежегодно посещает старинный <w>Каменец</w> и его окрестности."
                            }
                        }
                    ],
                    "synonyms": [
                        "Kamieniec Podolski"
                    ],
                    "common_phrases": [
                        "twierdza w Kamieńcu",
                        "upadek Kamieńca",
                        "obrona Kamieńca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращенное название города Каменец-Подольский на Украине, известного своей исторической крепостью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Каменец",
                                "target_lang_sense_clarification": "город Каменец-Подольский"

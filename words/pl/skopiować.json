                        ]
                    }
                },
                {
                    "sense_id": "f3dea6bb-9272-3bef-ae88-e99709b55030",
                    "sense_definition": "Wykonać dokładną kopię fizycznego obiektu, dokumentu lub dzieła.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "reproduction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę pójść do punktu ksero, aby <skopiować> te dokumenty potrzebne do urzędu.",
                            "target_lang_translations": {
                                "ru": "Мне нужно пойти в копировальный центр, чтобы <отксерокопировать> эти документы, необходимые для ведомства."
                            }
                        },
                        {
                            "text": "Czy mógłbyś <skopiować> dla mnie notatki z wczorajszego wykładu?",
                            "target_lang_translations": {
                                "ru": "Не мог бы ты <отксерокопировать> для меня конспекты со вчерашней лекции?"
                            }
                        }
                    ],
                    "synonyms": [
                        "powielić",
                        "zduplikować"
                    ],
                    "common_phrases": [
                        "skopiować dokument",
                        "skopiować klucz",
                        "skopiować obraz"
                    ],
                    "target_lang_definitions": {

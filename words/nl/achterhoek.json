            "pos": "name",
            "senses": [
                {
                    "sense_id": "8d9abdee-c3d9-30f8-b83b-f91572040678",
                    "sense_definition": "Een streek in het oostelijke deel van de Nederlandse provincie Gelderland.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "region_gelderland",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Mijn grootouders wonen in een oude boerderij in de <w>Achterhoek</w>.",
                            "target_lang_translations": {
                                "ru": "Мои дедушка и бабушка живут на старой ферме в <w>Ахтерхуке</w>."
                            }
                        },
                        {
                            "text": "De <w>Achterhoek</w> staat bekend om zijn prachtige kastelen en landgoederen.",
                            "target_lang_translations": {
                                "ru": "<w>Ахтерхук</w> известен своими прекрасными замками и поместьями."
                            }
                        },
                        {
                            "text": "Veel toeristen gaan in de zomer fietsen in de <w>Achterhoek</w>.",
                            "target_lang_translations": {
                                "ru": "Летом многие туристы ездят кататься на велосипеде в <w>Ахтерхук</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in de Achterhoek",
                        "de Gelderse Achterhoek",
                        "trots op de Achterhoek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Регион в восточной части нидерландской провинции Гелдерланд."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ахтерхук",
                                "target_lang_sense_clarification": "регион в Нидерландах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7c4f2100-edf1-3512-a480-daee92022ac4",
                    "sense_definition": "Een hoek die zich aan de achterkant van een ruimte, gebouw of voorwerp bevindt.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_corner",
                    "name_type": "no",
                    "examples": [
                        {

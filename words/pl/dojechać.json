            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b5c02360-efc0-35de-a2ac-e7d35557333c",
                    "sense_definition": "Dotrzeć do określonego miejsca lub celu podróży, poruszając się środkiem transportu (np. samochodem, pociągiem, rowerem).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport_arrival",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy szybko <w>dojechać</w> na lotnisko, żeby zdążyć na samolot.",
                            "target_lang_translations": {
                                "ru": "Нам нужно быстро <w>доехать</w> до аэропорта, чтобы успеть на самолёт."
                            }
                        },
                        {
                            "text": "W końcu udało nam się szczęśliwie <w>dojechać</w> do domu mimo korków.",
                            "target_lang_translations": {
                                "ru": "В конце концов нам удалось благополучно <w>доехать</w> до дома, несмотря на пробки."
                            }
                        }
                    ],
                    "synonyms": [
                        "przyjechać",
                        "dotrzeć",
                        "zajechać"
                    ],
                    "antonyms": [
                        "wyjechać",
                        "odjechać"
                    ],
                    "common_phrases": [
                        "dojechać do celu",
                        "dojechać na miejsce",
                        "dojechać do pracy",
                        "szczęśliwie dojechać",
                        "dojechać pociągiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добраться до определенного места или пункта назначения, передвигаясь на транспортном средстве (например, на машине, поезде, велосипеде)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доехать",
                                "target_lang_sense_clarification": "прибыть на транспорте"
                            },
                            {
                                "target_lang_word": "добраться",
                                "target_lang_sense_clarification": "достичь места"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed17ffe2-c998-3589-9e69-8c4798c5c055",
                    "sense_definition": "Dopaść kogoś w celu wyrządzenia mu krzywdy, zemsty lub zniszczenia go psychicznie albo fizycznie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "aggression_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie próbuj mnie oszukać, bo cię w końcu <w>dojadę</w>.",
                            "target_lang_translations": {
                                "ru": "Не пытайся меня обмануть, иначе я тебя в конце концов <w>достану</w>."
                            }
                        },
                        {
                            "text": "Lokalni chuligani chcieli go <w>dojechać</w> za to, że rozmawiał z policją.",
                            "target_lang_translations": {
                                "ru": "Местные хулиганы хотели <w>расправиться</w> с ним за то, что он разговаривал с полицией."
                            }
                        }
                    ],
                    "synonyms": [
                        "wykończyć",
                        "załatwić",
                        "zniszczyć",
                        "dopaść"
                    ],
                    "common_phrases": [
                        "dojechać kogoś",
                        "zaraz cię dojadę"
                    ],
                    "traits": [
                        {

        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c10d2322-8e2a-38d1-a637-685dfe49ef0b",
                    "sense_definition": "Przygotowywać żywność, poddając ją działaniu wysokiej temperatury, zwykle w piekarniku lub nad ogniem.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cooking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja babcia co tydzień <w>piecze</w> świeży chleb.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка каждую неделю <w>печёт</w> свежий хлеб."
                            }
                        },
                        {
                            "text": "Latem często <w>pieczemy</w> ziemniaki w ognisku.",
                            "target_lang_translations": {
                                "ru": "Летом мы часто <w>печём</w> картошку в костре."
                            }
                        }
                    ],
                    "synonyms": [
                        "przypiekać"
                    ],
                    "antonyms": [
                        "dusić",
                        "smażyć"
                    ],
                    "common_phrases": [
                        "piec ciasto",
                        "piec chleb",
                        "piec mięso",
                        "piec kiełbaski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовить пищу, подвергая её воздействию высокой температуры, обычно в духовке или на огне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "печь",
                                "target_lang_sense_clarification": "готовить в духовке"
                            },
                            {
                                "target_lang_word": "выпекать",
                                "target_lang_sense_clarification": "процесс готовки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be9a6ef6-015f-3b14-ab0d-c69cc7edfc2b",
                    "sense_definition": "Powodować ból, szczypanie lub uczucie gorąca na skórze lub w oczach.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Od krojenia cebuli zaczęły mnie <w>piec</w> oczy.",
                            "target_lang_translations": {
                                "ru": "От нарезания лука у меня начали <w>щипать</w> глаза."
                            }
                        },
                        {
                            "text": "Ranę trzeba przemyć, choć może to trochę <w>piec</w>.",
                            "target_lang_translations": {
                                "ru": "Рану нужно промыть, хотя может немного <w>щипать</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "gryźć",
                        "palić",
                        "dokuczać"

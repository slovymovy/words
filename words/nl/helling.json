            "senses": [
                {
                    "sense_id": "495b15db-1e6e-32d7-a4a6-4fd02630f70c",
                    "sense_definition": "Een schuin oplopend of aflopend vlak in het landschap.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "terrain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De fietsers hadden veel moeite om de steile <w>helling</w> op te komen.",
                            "target_lang_translations": {
                                "ru": "Велосипедистам стоило больших усилий подняться по крутому <w>склону</w>.",
                                "en": "The cyclists had great difficulty getting up the steep <w>slope</w>."
                            }
                        },
                        {
                            "text": "In de winter glijden de kinderen met hun slee van de <w>helling</w> af.",
                            "target_lang_translations": {
                                "ru": "Зимой дети катаются на санках со <w>склона</w>.",
                                "en": "In winter, the children slide down the <w>slope</w> on their sleds."
                            }
                        }
                    ],
                    "synonyms": [
                        "glooiing",
                        "steilte"
                    ],
                    "antonyms": [
                        "vlakte"
                    ],
                    "common_phrases": [
                        "steile helling",
                        "flauwe helling"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наклонная поверхность местности, поднимающаяся или опускающаяся; склон.",
                        "en": "A sloping surface of land, rising or falling."

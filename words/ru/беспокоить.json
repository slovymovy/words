{
    "word_family": [
        "покой",
        "беспокойство",
        "беспокойный",
        "спокойный",
        "успокаивать",
        "побеспокоить",
        "обеспокоить"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d003c416-6584-34df-a446-7814a954ecea",
                    "sense_definition": "Нарушать чей-либо покой, отвлекать от дел или доставлять неудобства.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "disturb_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Извините, я не хотел вас <w>беспокоить</w> в такой поздний час.",
                            "target_lang_translations": {
                                "en": "Sorry, I didn't mean to <w>disturb</w> you at such a late hour."
                            }
                        },
                        {
                            "text": "Прошу не <w>беспокоить</w> меня, пока я работаю над отчётом.",
                            "target_lang_translations": {
                                "en": "Please do not <w>disturb</w> me while I am working on the report."
                            }
                        }
                    ],
                    "synonyms": [
                        "мешать",
                        "досажда́ть"
                    ],
                    "common_phrases": [
                        "извините, что беспокою",
                        "прошу не беспокоить",
                        "не стоит беспокоить"
                    ],
                    "target_lang_definitions": {
                        "en": "To interrupt someone's peace, distract them from tasks, or cause inconvenience."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "disturb",
                                "target_lang_sense_clarification": "to interrupt or break someone's peace/concentration"
                            },
                            {
                                "target_lang_word": "bother",
                                "target_lang_sense_clarification": "to annoy or cause inconvenience"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94e10133-5fa0-39d1-8b83-c50a65996f83",
                    "sense_definition": "Вызывать чувство тревоги, волнения или страха; заставлять переживать.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "worry_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Её долгое молчание начинает меня серьёзно <w>беспокоить</w>.",
                            "target_lang_translations": {
                                "en": "Her long silence is starting to seriously <w>worry</w> me."
                            }
                        },
                        {
                            "text": "Меня это <w>беспокоит</w> гораздо больше, чем я показываю.",
                            "target_lang_translations": {
                                "en": "This <w>worries</w> me much more than I show."
                            }
                        }
                    ],
                    "synonyms": [
                        "волновать",
                        "тревожить"
                    ],
                    "antonyms": [
                        "успокаивать"
                    ],
                    "common_phrases": [
                        "меня это беспокоит",
                        "что тебя беспокоит",
                        "сильно беспокоить"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause feelings of anxiety, worry, or fear; to make someone concerned."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "worry",
                                "target_lang_sense_clarification": "to cause anxiety or mental distress"
                            },
                            {
                                "target_lang_word": "concern",
                                "target_lang_sense_clarification": "to be of interest or importance to; to make anxious"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94454a67-bccd-3ea3-a7eb-02c17e54e711",
                    "sense_definition": "Причинять физическую боль или неприятные ощущения (о болезни или части тела).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_pain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Доктор спросил пациента, что именно его <w>беспокоит</w>.",
                            "target_lang_translations": {
                                "en": "The doctor asked the patient what exactly was <w>bothering</w> him."
                            }
                        },
                        {
                            "text": "Старая травма ноги начала снова <w>беспокоить</w> при ходьбе.",
                            "target_lang_translations": {
                                "en": "The old leg injury started to <w>bother</w> him again when walking."
                            }
                        }
                    ],
                    "synonyms": [
                        "болеть"
                    ],
                    "antonyms": [
                        "проходить",
                        "отпускать"
                    ],
                    "common_phrases": [
                        "что вас беспокоит",
                        "рана беспокоит",
                        "кашель беспокоит"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause physical pain or discomfort (regarding an illness or body part)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bother",
                                "target_lang_sense_clarification": "to cause physical discomfort or pain"
                            },
                            {
                                "target_lang_word": "trouble",
                                "target_lang_sense_clarification": "to cause pain or difficulty (often chronic)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    ],
                    "target_lang_definitions": {
                        "ru": "Экземпляр компьютерной программы, которая выполняется в данный момент."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "процесс",
                                "target_lang_sense_clarification": "информатика"

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7b95154f-23a6-39aa-a0db-ff0bd0ed09ac",
                    "sense_definition": "Cecha tego, co jest z czymś zgodne, nie jest sprzeczne; wzajemne dopasowanie elementów.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Urzędnik musi potwierdzić <zgodność> kopii dokumentu z oryginałem.",
                            "target_lang_translations": {
                                "ru": "Чиновник должен подтвердить <соответствие> копии документа оригиналу."
                            }
                        },
                        {
                            "text": "Sprawdzamy <zgodność> towaru z zamówieniem przed jego wysyłką.",
                            "target_lang_translations": {
                                "ru": "Мы проверяем <соответствие> товара заказу перед его отправкой."
                            }
                        }
                    ],
                    "antonyms": [
                        "niezgodnosć"
                    ],
                    "common_phrases": [
                        "zgodność z prawem",
                        "zgodność z oryginałem",
                        "certyfikat zgodności",
                        "zgodność danych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Свойство того, что соответствует чему-либо, не противоречит; взаимное соответствие элементов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соответствие",
                                "target_lang_sense_clarification": "соотношение между чем-либо, выражающее согласованность"
                            },
                            {
                                "target_lang_word": "совпадение",
                                "target_lang_sense_clarification": "полное сходство, тождество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ada4810f-7588-3cc9-86ac-b3491cb47547",
                    "sense_definition": "Jednomyślność; stan, w którym wszyscy mają takie samo zdanie lub dążą do tego samego celu.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej kwestii panuje między nami pełna <zgodność>.",
                            "target_lang_translations": {
                                "ru": "В этом вопросе между нами царит полное <согласие>."
                            }
                        },
                        {
                            "text": "Rzadko zdarza się taka <zgodność> poglądów w tak dużej grupie ludzi.",
                            "target_lang_translations": {
                                "ru": "Редко встречается такое <единство> взглядов в такой большой группе людей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pełna zgodność",
                        "osiągnąć zgodność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единодушие; состояние, при котором все имеют одинаковое мнение или стремятся к одной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "согласие",
                                "target_lang_sense_clarification": "единство мнений, точек зрения"
                            },
                            {
                                "target_lang_word": "единодушие",
                                "target_lang_sense_clarification": "полное согласие в мнениях, действиях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4022661-4c5d-3981-9076-701b39957ef1",
                    "sense_definition": "Brak konfliktów; harmonijne współżycie lub współpraca.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ich małżeńska <zgodność> i wzajemny szacunek budziły podziw sąsiadów.",
                            "target_lang_translations": {
                                "ru": "Их супружеское <согласие> и взаимное уважение вызывали восхищение соседей."
                            }
                        },
                        {
                            "text": "Dobra atmosfera w pracy opiera się na wzajemnej <zgodności> członków zespołu.",
                            "target_lang_translations": {
                                "ru": "Хорошая атмосфера на работе основывается на взаимной <совместимости> членов команды."
                            }
                        }
                    ],
                    "synonyms": [
                        "bezkonfliktowość",
                        "niekonfliktowość"
                    ],
                    "antonyms": [
                        "konfliktowość"
                    ],
                    "common_phrases": [
                        "zgodność charakterów",
                        "żyć w zgodności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отсутствие конфликтов; гармоничное сосуществование или сотрудничество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "согласие",
                                "target_lang_sense_clarification": "мирные, дружественные отношения"
                            },
                            {
                                "target_lang_word": "совместимость",
                                "target_lang_sense_clarification": "способность без конфликтов существовать или работать вместе"
                            },
                            {
                                "target_lang_word": "гармония",
                                "target_lang_sense_clarification": "согласованность, стройность в отношениях"
                            }
                        ]
                    }
                }
            ]
        }
    ]

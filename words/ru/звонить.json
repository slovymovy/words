    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0b6c3e03-fa8c-3b88-a023-2f89d34866c1",
                    "sense_definition": "Связываться с кем-либо, вести разговор или вызывать кого-либо по телефону.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я буду <звонить> тебе каждый вечер после работы.",
                            "target_lang_translations": {
                                "en": "I will <w>call</w> you every evening after work."
                            }
                        },
                        {
                            "text": "Ты можешь <звонить> мне в любое время дня и ночи.",
                            "target_lang_translations": {
                                "en": "You can <w>call</w> me at any time of day or night."
                            }
                        }
                    ],
                    "synonyms": [
                        "вызывать"
                    ],
                    "common_phrases": [
                        "звонить по телефону",
                        "звонить другу",
                        "звонить домой",
                        "звонить на работу"
                    ],
                    "target_lang_definitions": {
                        "en": "To contact someone, have a conversation, or summon someone via telephone."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "call",
                                "target_lang_sense_clarification": "general term for contacting by phone"
                            },
                            {
                                "target_lang_word": "phone",
                                "target_lang_sense_clarification": "to use a telephone to call someone"
                            },
                            {
                                "target_lang_word": "ring",
                                "target_lang_sense_clarification": "British/colloquial"
                            },
                            {
                                "target_lang_word": "call up",
                                "target_lang_sense_clarification": "phrasal verb, slightly informal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f9e7703e-7de0-38b7-8372-564576191900",
                    "sense_definition": "Издавать высокий, резкий звук (о телефоне, будильнике, колоколе или другом устройстве).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sound_emission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В коридоре громко <звонит> старый телефон.",
                            "target_lang_translations": {
                                "en": "An old telephone is <w>ringing</w> loudly in the hallway."
                            }
                        },
                        {
                            "text": "Будильник <звонил> уже пять минут, но я не просыпался.",
                            "target_lang_translations": {
                                "en": "The alarm clock had been <w>ringing</w> for five minutes, but I didn't wake up."
                            }
                        }
                    ],
                    "synonyms": [
                        "звенеть"
                    ],
                    "common_phrases": [
                        "телефон звонит",
                        "будильник звонит",
                        "колокола звонят"
                    ],
                    "target_lang_definitions": {
                        "en": "To emit a high, sharp sound (of a telephone, alarm clock, bell, or other device)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ring",
                                "target_lang_sense_clarification": "to produce a resonant sound"
                            },
                            {
                                "target_lang_word": "go off",
                                "target_lang_sense_clarification": "of an alarm or phone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4bdbb77c-a4cb-3ea2-be57-eec4237d9256",
                    "sense_definition": "Подавать звуковой сигнал, нажимая на кнопку звонка или раскачивая колокол.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Кто-то настойчиво <звонит> в дверь нашей квартиры.",
                            "target_lang_translations": {
                                "en": "Someone is persistently <w>ringing</w> the doorbell of our apartment."
                            }
                        },
                        {
                            "text": "Почтальон <звонил> два раза, чтобы ему открыли.",
                            "target_lang_translations": {
                                "en": "The postman <w>rang</w> twice so that they would open up."
                            }
                        }
                    ],
                    "common_phrases": [
                        "звонить в дверь",
                        "звонить в колокол"
                    ],
                    "target_lang_definitions": {
                        "en": "To give a sound signal by pressing a doorbell button or swinging a bell."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ring",
                                "target_lang_sense_clarification": "to cause a bell or doorbell to sound"
                            },
                            {
                                "target_lang_word": "buzz",
                                "target_lang_sense_clarification": "to press a buzzer/intercom"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "46437469-86d0-3882-a31f-598b9ff7dc41",
                    "sense_definition": "Распространять слухи или сплетни, рассказывать всем подряд о чём-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gossip",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Не стоит <звонить> об этом секрете на каждом углу.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>blab</w> about this secret on every corner."
                            }
                        },
                        {
                            "text": "Она любит <звонить> о чужих проблемах всем знакомым.",
                            "target_lang_translations": {
                                "en": "She loves to <w>gossip</w> about other people's problems to all her acquaintances."
                            }
                        }
                    ],
                    "synonyms": [
                        "трезвонить"
                    ],
                    "common_phrases": [
                        "звонить на всех углах"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To spread rumors or gossip; to tell everyone indiscriminately about something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "blab",
                                "target_lang_sense_clarification": "colloquial, to reveal secrets"
                            },
                            {
                                "target_lang_word": "gossip",
                                "target_lang_sense_clarification": "to talk about others' private lives"
                            },
                            {
                                "target_lang_word": "spread rumors",
                                "target_lang_sense_clarification": "to circulate unverified information"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
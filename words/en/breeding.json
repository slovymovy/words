        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "59e1dfd2-284d-3afe-95d3-65ec14ff726d",
                    "sense_definition": "The biological process of producing offspring, especially in animals or plants.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>breeding</w> season for these birds begins in early spring.",
                            "target_lang_translations": {
                                "ru": "Сезон <w>размножения</w> у этих птиц начинается ранней весной."
                            }
                        },
                        {
                            "text": "Farmers use selective <w>breeding</w> to produce cows that give more milk.",
                            "target_lang_translations": {
                                "ru": "Фермеры используют селекционное <w>разведение</w>, чтобы получить коров, дающих больше молока."
                            }
                        }
                    ],
                    "synonyms": [
                        "reproduction",
                        "procreation",
                        "propagation"
                    ],
                    "common_phrases": [
                        "captive breeding",
                        "selective breeding",
                        "breeding season",
                        "plant breeding"
                    ],
                    "target_lang_definitions": {
                        "ru": "Биологический процесс получения потомства, особенно у животных или растений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разведение",
                                "target_lang_sense_clarification": "выращивание животных или растений"
                            },
                            {
                                "target_lang_word": "размножение",
                                "target_lang_sense_clarification": "биологический процесс воспроизводства"
                            },
                            {
                                "target_lang_word": "селекция",
                                "target_lang_sense_clarification": "выведение улучшенных пород или сортов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5bc69bf-41f3-33f4-8ec6-9f5ed21c7291",
                    "sense_definition": "Polite social behavior and good manners that are considered a result of being raised well.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His rude comments showed a distinct lack of <w>breeding</w>.",
                            "target_lang_translations": {
                                "ru": "Его грубые комментарии указывали на явный недостаток <w>воспитания</w>."
                            }
                        },
                        {
                            "text": "She is a woman of wealth and good <w>breeding</w>.",
                            "target_lang_translations": {
                                "ru": "Она женщина состоятельная и с хорошим <w>воспитанием</w>."
                            }

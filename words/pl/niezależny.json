{
    "word_family": [
        "niezależność",
        "niezależnie"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6db2aa23-fac2-3844-8196-a9f5fa048913",
                    "sense_definition": "Taki, który o sobie decyduje, nie musi polegać na innych i sam zaspokaja swoje potrzeby.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat jest już dorosły i całkowicie <w>niezależny</w> finansowo.",
                            "target_lang_translations": {
                                "ru": "Мой брат уже взрослый и полностью финансово <w>независим</w>."
                            }
                        },
                        {
                            "text": "Chcę być <w>niezależny</w> i sam podejmować wszystkie ważne decyzje w moim życiu.",
                            "target_lang_translations": {
                                "ru": "Я хочу быть <w>независимым</w> и сам принимать все важные решения в своей жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "samodzielny",
                        "wolny"
                    ],
                    "antonyms": [
                        "zależny",
                        "niesamodzielny"
                    ],
                    "common_phrases": [
                        "niezależny finansowo",
                        "być niezależnym",
                        "człowiek niezależny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Такой, который принимает решения самостоятельно, не зависит от других и сам обеспечивает свои потребности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "независимый",
                                "target_lang_sense_clarification": "самостоятельный"
                            },
                            {
                                "target_lang_word": "самостоятельный",
                                "target_lang_sense_clarification": "обеспечивающий себя сам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d581940d-6883-368d-afdb-9463bcd4d1bc",
                    "sense_definition": "O państwie lub terytorium: posiadający własną władzę, niepodlegający rządom innego państwa.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Polska jest wolnym i <w>niezależnym</w> krajem w Europie.",
                            "target_lang_translations": {
                                "ru": "Польша — свободная и <w>независимая</w> страна в Европе."
                            }
                        },
                        {
                            "text": "Wiele kolonii walczyło o to, by stać się <w>niezależnym</w> państwem.",
                            "target_lang_translations": {
                                "ru": "Многие колонии боролись за то, чтобы стать <w>независимым</w> государством."
                            }
                        }
                    ],
                    "synonyms": [
                        "niepodległy",
                        "suwerenny",
                        "wolny",
                        "autonomiczny"
                    ],
                    "antonyms": [
                        "zależny",
                        "podległy"
                    ],
                    "common_phrases": [
                        "niezależne państwo",
                        "kraj niezależny"
                    ],
                    "target_lang_definitions": {
                        "ru": "О государстве или территории: имеющий собственную власть, не подчиняющийся правительству другого государства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "независимый",
                                "target_lang_sense_clarification": "суверенный"
                            },
                            {
                                "target_lang_word": "суверенный",
                                "target_lang_sense_clarification": "обладающий верховной властью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3349e792-98a9-3768-9642-a176f4ba55ab",
                    "sense_definition": "Niekierujący się interesem żadnej grupy, sprawiedliwy i obiektywny w swoich ocenach (np. sąd, ekspert).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sprawę wypadku powinien zbadać <w>niezależny</w> ekspert z zewnątrz.",
                            "target_lang_translations": {
                                "ru": "Дело об аварии должен расследовать внешний <w>независимый</w> эксперт."
                            }
                        },
                        {
                            "text": "W demokratycznym państwie sądy muszą być <w>niezależne</w> od polityków.",
                            "target_lang_translations": {
                                "ru": "В демократическом государстве суды должны быть <w>независимы</w> от политиков."
                            }
                        }
                    ],
                    "synonyms": [
                        "niezawisły"
                    ],
                    "antonyms": [
                        "zależny"
                    ],
                    "common_phrases": [
                        "niezależny ekspert",
                        "niezależny obserwator",
                        "niezależny sąd",
                        "niezależne media"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не руководствующийся интересами какой-либо группы, справедливый и объективный в своих оценках (например, суд, эксперт)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "независимый",
                                "target_lang_sense_clarification": "объективный, беспристрастный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c052caf1-ca7f-3500-abdb-6e8ae660fd94",
                    "sense_definition": "Niemający związku z czymś innym, występujący oddzielnie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To są dwie zupełnie <w>niezależne</w> od siebie sprawy, których nie należy łączyć.",
                            "target_lang_translations": {
                                "ru": "Это два совершенно <w>независимых</w> друг от друга дела, которые не следует объединять."
                            }
                        },
                        {
                            "text": "Awaria nastąpiła z przyczyn <w>niezależnych</w> od operatora sieci.",
                            "target_lang_translations": {
                                "ru": "Авария произошла по причинам, <w>независимым</w> от сетевого оператора."
                            }
                        }
                    ],
                    "synonyms": [
                        "oddzielny",
                        "osobny"
                    ],
                    "antonyms": [
                        "zależny"
                    ],
                    "common_phrases": [
                        "przyczyny niezależne",
                        "zjawiska niezależne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не имеющий связи с чем-то другим, существующий отдельно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "независимый",
                                "target_lang_sense_clarification": "не связанный с чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "461f73f7-60bc-3e44-a596-3aaa1c272585",
                    "sense_definition": "Nienależący do żadnej partii politycznej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tegorocznych wyborach wystartował jako kandydat <w>niezależny</w>.",
                            "target_lang_translations": {
                                "ru": "На выборах в этом году он баллотировался как <w>независимый</w> кандидат."
                            }
                        },
                        {
                            "text": "Polityk <w>niezależny</w> nie musi słuchać rozkazów szefa partii.",
                            "target_lang_translations": {
                                "ru": "<w>Независимый</w> политик не обязан слушать приказы главы партии."
                            }
                        }
                    ],
                    "synonyms": [
                        "bezpartyjny"
                    ],
                    "common_phrases": [
                        "kandydat niezależny",
                        "senator niezależny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не принадлежащий ни к какой политической партии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "независимый",
                                "target_lang_sense_clarification": "беспартийный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
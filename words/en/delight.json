{
    "word_family": [
        "delighted",
        "delightful",
        "delightfully"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b0399ac7-49be-3cb0-91dc-698bc73e38b1",
                    "sense_definition": "A feeling of great pleasure, satisfaction, or happiness.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_feeling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children opened their presents with squeals of <w>delight</w>.",
                            "target_lang_translations": {
                                "ru": "Дети открывали подарки с визгом от <w>восторга</w>."
                            }
                        },
                        {
                            "text": "To my <w>delight</w>, the sun came out just in time for the picnic.",
                            "target_lang_translations": {
                                "ru": "К моей <w>радости</w>, солнце выглянуло как раз к пикнику."
                            }
                        }
                    ],
                    "synonyms": [
                        "joy",
                        "happiness",
                        "glee",
                        "pleasure",
                        "gladness"
                    ],
                    "antonyms": [
                        "sorrow",
                        "displeasure",
                        "misery"
                    ],
                    "common_phrases": [
                        "with delight",
                        "to someone's delight",
                        "squeal with delight",
                        "take delight in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство огромного удовольствия, удовлетворения или счастья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "восторг",
                                "target_lang_sense_clarification": "сильный подъём радостных чувств"
                            },
                            {
                                "target_lang_word": "наслаждение",
                                "target_lang_sense_clarification": "высшая степень удовольствия"
                            },
                            {
                                "target_lang_word": "радость",
                                "target_lang_sense_clarification": "чувство счастья и удовлетворения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7eeffd78-bef1-3908-9661-ea89b256da53",
                    "sense_definition": "Something or someone that causes great pleasure or happiness.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "noun_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The beautiful garden is a <w>delight</w> to visit in the spring.",
                            "target_lang_translations": {
                                "ru": "Посещение этого прекрасного сада весной — одно <w>удовольствие</w>."
                            }
                        },
                        {
                            "text": "Her singing voice is a pure <w>delight</w> to listen to.",
                            "target_lang_translations": {
                                "ru": "Слушать её пение — чистое <w>наслаждение</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "joy",
                        "pleasure"
                    ],
                    "common_phrases": [
                        "a pure delight",
                        "culinary delights",
                        "absolute delight"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-то или кто-то, доставляющий огромное удовольствие или радость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наслаждение",
                                "target_lang_sense_clarification": "источник высшего удовольствия"
                            },
                            {
                                "target_lang_word": "удовольствие",
                                "target_lang_sense_clarification": "приятное переживание или развлечение"
                            },
                            {
                                "target_lang_word": "прелесть",
                                "target_lang_sense_clarification": "о ком-то или чём-то очаровательном"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "63757bfa-1094-3f48-813d-7051daf32f7e",
                    "sense_definition": "To give someone a lot of pleasure or enjoyment.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_transitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The magician's tricks <w>delighted</w> the audience for hours.",
                            "target_lang_translations": {
                                "ru": "Фокусы иллюзиониста часами <w>восхищали</w> публику."
                            }
                        },
                        {
                            "text": "It would <w>delight</w> me to join you for dinner tonight.",
                            "target_lang_translations": {
                                "ru": "Меня <w>обрадует</w> возможность поужинать с вами сегодня."
                            }
                        }
                    ],
                    "synonyms": [
                        "please",
                        "gladden",
                        "gratify",
                        "cheer",
                        "exhilarate"
                    ],
                    "antonyms": [
                        "displease",
                        "bore"
                    ],
                    "common_phrases": [
                        "delight the eye",
                        "delight the senses",
                        "delight the audience"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доставлять кому-либо большое удовольствие или радость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "восхищать",
                                "target_lang_sense_clarification": "вызывать чувство восторга"
                            },
                            {
                                "target_lang_word": "радовать",
                                "target_lang_sense_clarification": "доставлять радость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca6b8db1-bfb0-3286-b4fc-83f4e842901e",
                    "sense_definition": "To get great pleasure from something; to enjoy something very much.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_intransitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>delights</w> in telling stories to her grandchildren.",
                            "target_lang_translations": {
                                "ru": "Она <w>обожает</w> рассказывать истории своим внукам."
                            }
                        },
                        {
                            "text": "The cat <w>delights</w> in chasing the laser pointer around the room.",
                            "target_lang_translations": {
                                "ru": "Кот с <w>наслаждением</w> гоняется за лазерной указкой по комнате."
                            }
                        }
                    ],
                    "synonyms": [
                        "rejoice"
                    ],
                    "common_phrases": [
                        "delight in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получать огромное удовольствие от чего-либо; очень любить делать что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наслаждаться",
                                "target_lang_sense_clarification": "испытывать удовольствие от процесса"
                            },
                            {
                                "target_lang_word": "обожать",
                                "target_lang_sense_clarification": "очень сильно любить что-то делать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
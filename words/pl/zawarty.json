    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1b3e0f63-02a6-3210-a145-31adfa5b844e",
                    "sense_definition": "Znajdujący się wewnątrz czegoś lub stanowiący część jakiejś całości (np. tekstu, substancji).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "contained",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystkie ważne informacje są <zawarte> w instrukcji obsługi.",
                            "target_lang_translations": {
                                "ru": "Вся важная информация <содержится> в инструкции по эксплуатации."
                            }
                        },
                        {
                            "text": "Cukier <zawarty> w owocach jest zdrowszy niż ten w słodyczach.",
                            "target_lang_translations": {
                                "ru": "Сахар, <содержащийся> во фруктах, полезнее того, что в сладостях."
                            }
                        }
                    ],
                    "synonyms": [
                        "obecny",
                        "znajdujący się",
                        "umieszczony",
                        "ujęty"
                    ],
                    "antonyms": [
                        "brakujący",
                        "pominięty"
                    ],
                    "common_phrases": [
                        "informacje zawarte w",
                        "składniki zawarte w",
                        "przepisy zawarte w"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся внутри чего-либо или составляющий часть какого-либо целого (например, текста, вещества)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "содержащийся",
                                "target_lang_sense_clarification": "находящийся внутри, имеющийся в составе"
                            },
                            {
                                "target_lang_word": "включённый",
                                "target_lang_sense_clarification": "входящий в состав чего-либо"
                            },
                            {
                                "target_lang_word": "заключённый",
                                "target_lang_sense_clarification": "скрытый или находящийся внутри (часто абстрактно)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ceb04e62-83fb-3eeb-a5ca-db75951f9ef5",
                    "sense_definition": "Taki, który został oficjalnie uzgodniony, podpisany lub ustanowiony (dotyczy np. umowy, związku, pokoju); imiesłów od słowa <w>zawrzeć</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "concluded",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Umowa <zawarta> między firmami gwarantuje współpracę przez rok.",
                            "target_lang_translations": {
                                "ru": "Договор, <заключённый> между фирмами, гарантирует сотрудничество в течение года."
                            }
                        },
                        {
                            "text": "Pokój został <zawarty> po długich i trudnych negocjacjach.",
                            "target_lang_translations": {
                                "ru": "Мир был <заключён> после долгих и трудных переговоров."
                            }
                        }
                    ],
                    "synonyms": [
                        "podpisany",
                        "uzgodniony",
                        "sfinalizowany"
                    ],
                    "antonyms": [
                        "zerwany",
                        "rozwiązany",
                        "unieważniony"
                    ],
                    "common_phrases": [
                        "umowa zawarta",
                        "małżeństwo zawarte",
                        "pokój zawarty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официально согласованный, подписанный или установленный (относится, например, к договору, союзу, миру)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заключённый",
                                "target_lang_sense_clarification": "о договоре, браке, мире"
                            },
                            {
                                "target_lang_word": "подписанный",
                                "target_lang_sense_clarification": "официально оформленный"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
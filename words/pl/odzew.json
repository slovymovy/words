{
    "word_family": [
        "odzywać się",
        "odezwa"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "242b72bf-5e2f-3ad1-926f-6d4e275cba51",
                    "sense_definition": "Reakcja na czyjeś słowa, działanie lub na jakieś wydarzenie, często wyrażająca zainteresowanie lub poparcie.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój apel o pomoc dla schroniska spotkał się z natychmiastowym <w>odzewem</w>.",
                            "target_lang_translations": {
                                "ru": "Мой призыв о помощи приюту встретил мгновенный <w>отклик</w>."
                            }
                        },
                        {
                            "text": "Artykuł opublikowany wczoraj w gazecie wywołał szeroki <w>odzew</w> wśród czytelników.",
                            "target_lang_translations": {
                                "ru": "Опубликованная вчера в газете статья вызвала широкий <w>отклик</w> среди читателей."
                            }
                        }
                    ],
                    "synonyms": [
                        "reakcja",
                        "odpowiedź",
                        "oddźwięk"
                    ],
                    "common_phrases": [
                        "szeroki odzew",
                        "brak odzewu",
                        "spotkać się z odzewem",
                        "pozytywny odzew"
                    ],
                    "target_lang_definitions": {
                        "ru": "Реакция на чьи-либо слова, действия или события, часто выражающая интерес или поддержку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отклик",
                                "target_lang_sense_clarification": "реакция, ответное чувство или действие"
                            },
                            {
                                "target_lang_word": "отзыв",
                                "target_lang_sense_clarification": "мнение, суждение или реакция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c784ed6-bd06-3452-b1cc-3159f2913970",
                    "sense_definition": "Ustalone słowo lub zdanie wypowiadane w odpowiedzi na hasło, służące do identyfikacji (np. w wojsku).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wartownik nie przepuścił go, ponieważ żołnierz zapomniał właściwego <w>odzewu</w>.",
                            "target_lang_translations": {
                                "ru": "Часовой не пропустил его, так как солдат забыл правильный <w>отзыв</w>."
                            }
                        },
                        {
                            "text": "Każdej nocy ustalano nowe hasło i <w>odzew</w> dla bezpieczeństwa obozu.",
                            "target_lang_translations": {
                                "ru": "Каждую ночь для безопасности лагеря устанавливали новые пароль и <w>отзыв</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "odpowiedź"
                    ],
                    "common_phrases": [
                        "hasło i odzew",
                        "znać odzew"
                    ],
                    "target_lang_definitions": {
                        "ru": "Условное слово или фраза, произносимые в ответ на пароль для идентификации (например, в армии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отзыв",
                                "target_lang_sense_clarification": "ответ на пароль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b8bfcc9-2a89-31e5-b307-a3f8e00d51aa",
                    "sense_definition": "Dawne określenie na odwołanie się od wyroku sądowego do wyższej instancji.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szlachcic wniósł <w>odzew</w> do trybunału, licząc na zmianę niekorzystnego wyroku.",
                            "target_lang_translations": {
                                "ru": "Шляхтич подал <w>апелляцию</w> в трибунал, рассчитывая на изменение неблагоприятного приговора."
                            }
                        },
                        {
                            "text": "Sąd królewski ostatecznie odrzucił jego <w>odzew</w> jako bezzasadny.",
                            "target_lang_translations": {
                                "ru": "Королевский суд окончательно отклонил его <w>апелляцию</w> как необоснованную."
                            }
                        }
                    ],
                    "synonyms": [
                        "apelacja"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее название обжалования судебного приговора в вышестоящей инстанции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "апелляция",
                                "target_lang_sense_clarification": "юридический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "41899406-c0dc-31e0-97b7-6455e86ba037",
                    "sense_definition": "Powtarzająca się część utworu poetyckiego lub muzycznego.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po każdej zwrotce pieśni następował radosny <w>odzew</w> śpiewany przez chór.",
                            "target_lang_translations": {
                                "ru": "После каждого куплета песни следовал радостный <w>припев</w>, исполняемый хором."
                            }
                        },
                        {
                            "text": "Ten melodyjny <w>odzew</w> nadawał wierszowi rytmiczny charakter.",
                            "target_lang_translations": {
                                "ru": "Этот мелодичный <w>рефрен</w> придавал стихотворению ритмичный характер."
                            }
                        }
                    ],
                    "synonyms": [
                        "refren"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повторяющаяся часть поэтического или музыкального произведения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "припев",
                                "target_lang_sense_clarification": "в песне"
                            },
                            {
                                "target_lang_word": "рефрен",
                                "target_lang_sense_clarification": "в поэзии или музыке"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
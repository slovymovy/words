    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ac3e749f-cd1f-3c41-aae6-1ae9cab3ad1b",
                    "sense_definition": "Oud, maar waardevol vanwege de leeftijd, kwaliteit of zeldzaamheid.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "valuable_old",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn grootouders hebben een prachtige <w>antieke</w> kast in de woonkamer staan.",
                            "target_lang_translations": {
                                "ru": "У моих бабушки и дедушки в гостиной стоит прекрасный <w>антикварный</w> шкаф."
                            }
                        },
                        {
                            "text": "Op de markt zocht hij naar <w>antieke</w> meubels om zijn huis mee in te richten.",
                            "target_lang_translations": {
                                "ru": "На рынке он искал <w>антикварную</w> мебель, чтобы обставить свой дом."
                            }
                        }
                    ],
                    "synonyms": [
                        "oud",
                        "klassiek"
                    ],
                    "antonyms": [
                        "modern",
                        "nieuw",
                        "hedendaags"
                    ],
                    "common_phrases": [
                        "antieke meubels",
                        "antieke klok",
                        "antieke sieraden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Старый, но ценный из-за возраста, качества или редкости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "антикварный",
                                "target_lang_sense_clarification": "ценный старинный"
                            },
                            {
                                "target_lang_word": "старинный",
                                "target_lang_sense_clarification": "древний, сделанный в старину"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "57c06cb7-a6a8-3e7f-890c-6729c2964c74",
                    "sense_definition": "Met betrekking tot de klassieke Grieks-Romeinse oudheid.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "historical_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de les geschiedenis leerden de studenten over de <w>antieke</w> beschavingen.",
                            "target_lang_translations": {
                                "ru": "На уроке истории студенты изучали <w>античные</w> цивилизации."
                            }
                        },
                        {
                            "text": "De <w>antieke</w> filosofie heeft nog steeds veel invloed op ons huidige denken.",
                            "target_lang_translations": {
                                "ru": "<w>Античная</w> философия до сих пор оказывает большое влияние на наше современное мышление."
                            }
                        }
                    ],
                    "synonyms": [
                        "klassiek"
                    ],
                    "antonyms": [
                        "modern"
                    ],
                    "common_phrases": [
                        "de antieke oudheid",
                        "antieke cultuur"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к классической греко-римской древности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "античный",
                                "target_lang_sense_clarification": "относящийся к Древней Греции или Риму"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0ad81932-e66f-3538-9b99-12ef06bafb0d",
                    "sense_definition": "Oude kunst-, sier- en gebruiksvoorwerpen die verzameld en verhandeld worden.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "valuable_old",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij bezoekt elke zondag markten omdat ze graag <w>antiek</w> verzamelt.",
                            "target_lang_translations": {
                                "ru": "Она посещает рынки каждое воскресенье, потому что любит собирать <w>антиквариат</w>."
                            }
                        },
                        {
                            "text": "Hij besloot te gaan handelen in <w>antiek</w> om van zijn hobby zijn werk te maken.",
                            "target_lang_translations": {
                                "ru": "Он решил заняться торговлей <w>антиквариатом</w>, чтобы превратить свое хобби в работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "oudheden"
                    ],
                    "common_phrases": [
                        "handelen in antiek",
                        "liefhebber van antiek",
                        "winkel in antiek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинные предметы искусства, украшения и быта, представляющие ценность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "антиквариат",
                                "target_lang_sense_clarification": "старинные ценные вещи"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "powiększać",
        "powiększenie",
        "powiększalnik"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e8cd5c41-efaf-3cf7-8dcf-7ce6449f9a19",
                    "sense_definition": "Sprawić, że coś stanie się większe pod względem rozmiaru, liczby, objętości lub zakresu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "change_size",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy <powiększyć> ten pokój, burząc ścianę działową.",
                            "target_lang_translations": {
                                "ru": "Мы должны <расширить> эту комнату, снеся перегородку."
                            }
                        },
                        {
                            "text": "Firma planuje znacznie <powiększyć> swoje zyski w przyszłym roku.",
                            "target_lang_translations": {
                                "ru": "Компания планирует значительно <увеличить> свою прибыль в следующем году."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwiększyć",
                        "rozszerzyć"
                    ],
                    "antonyms": [
                        "zmniejszyć",
                        "pomniejszyć",
                        "zredukować"
                    ],
                    "common_phrases": [
                        "powiększyć zyski",
                        "powiększyć rodzinę",
                        "powiększyć zakres"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать что-то больше по размеру, количеству, объему или охвату."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "увеличить",
                                "target_lang_sense_clarification": "сделать больше по размеру или количеству"
                            },
                            {
                                "target_lang_word": "расширить",
                                "target_lang_sense_clarification": "сделать шире или просторнее (о помещении, знаниях)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15c13261-ff34-3a6e-92ad-6db497921e40",
                    "sense_definition": "Sprawić, że obraz przedmiotu wydaje się większy niż w rzeczywistości, używając przyrządów optycznych lub cyfrowych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "optics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Możesz <powiększyć> ten fragment mapy na ekranie telefonu.",
                            "target_lang_translations": {
                                "ru": "Ты можешь <увеличить> этот фрагмент карты на экране телефона."
                            }
                        },
                        {
                            "text": "Biolog musiał <powiększyć> obraz pod mikroskopem, aby dostrzec bakterie.",
                            "target_lang_translations": {
                                "ru": "Биологу пришлось <увеличить> изображение под микроскопом, чтобы разглядеть бактерии."
                            }
                        }
                    ],
                    "synonyms": [
                        "przybliżyć"
                    ],
                    "antonyms": [
                        "pomniejszyć",
                        "oddalić"
                    ],
                    "common_phrases": [
                        "powiększyć zdjęcie",
                        "powiększyć czcionkę",
                        "powiększyć obraz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать изображение предмета визуально крупнее с помощью оптических или цифровых средств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "увеличить",
                                "target_lang_sense_clarification": "сделать изображение крупнее (зум)"
                            },
                            {
                                "target_lang_word": "приблизить",
                                "target_lang_sense_clarification": "визуально приблизить объект"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6f67cd7-2629-3a76-8a16-08ae8e5dac51",
                    "sense_definition": "Stać się większym, wzrosnąć pod względem rozmiaru, liczby lub intensywności (używane z zaimkiem zwrotnym „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "change_size",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ostatnich latach liczba mieszkańców miasta znacznie się <powiększyła>.",
                            "target_lang_translations": {
                                "ru": "В последние годы число жителей города значительно <увеличилось>."
                            }
                        },
                        {
                            "text": "Dziura w spodniach <powiększyła> się po gwałtownym ruchu.",
                            "target_lang_translations": {
                                "ru": "Дырка на штанах <увеличилась> после резкого движения."
                            }
                        }
                    ],
                    "synonyms": [
                        "wzrosnąć",
                        "urosnąć"
                    ],
                    "antonyms": [
                        "zmaleć",
                        "zmniejszyć się",
                        "skurczyć się"
                    ],
                    "common_phrases": [
                        "rodzina się powiększyła",
                        "dziura się powiększyła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать больше по размеру, количеству или интенсивности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "увеличиться",
                                "target_lang_sense_clarification": "стать больше (возвратная форма)"
                            },
                            {
                                "target_lang_word": "вырасти",
                                "target_lang_sense_clarification": "стать больше в числе или размере"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
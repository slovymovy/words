{
    "word_family": [
        "dooien",
        "dooi",
        "ontdooiing"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fefeaab9-912d-3340-abb3-eb5af4d0c7c1",
                    "sense_definition": "Iets uit bevroren toestand halen door het te verwarmen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "literal_melting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat vlees kan ook in de magnetron <w>ontdooid</w> worden.",
                            "target_lang_translations": {
                                "en": "That meat can also be <w>defrosted</w> in the microwave."
                            }
                        },
                        {
                            "text": "Je moet de vriezer minstens één keer per jaar <w>ontdooien</w>.",
                            "target_lang_translations": {
                                "en": "You must <w>defrost</w> the freezer at least once a year."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontvriezen"
                    ],
                    "antonyms": [
                        "invriezen"
                    ],
                    "common_phrases": [
                        "vlees ontdooien",
                        "de vriezer ontdooien",
                        "snel ontdooien"
                    ],
                    "target_lang_definitions": {
                        "en": "To cause something frozen to melt or become soft; to remove ice from something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "defrost",
                                "target_lang_sense_clarification": "to remove ice or unfreeze food"
                            },
                            {
                                "target_lang_word": "thaw",
                                "target_lang_sense_clarification": "to cause to unfreeze"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86950bfb-1399-3b5f-8178-f7eaa01ae503",
                    "sense_definition": "Ophouden bevroren te zijn; zacht of vloeibaar worden door een stijgende temperatuur.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "literal_melting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die kip <w>ontdooit</w> maar langzaam als je hem niet uit de verpakking haalt.",
                            "target_lang_translations": {
                                "en": "That chicken <w>thaws</w> only slowly if you don't take it out of the packaging."
                            }
                        },
                        {
                            "text": "Het ijs op de sloten <w>ontdooit</w> snel door de felle zon.",
                            "target_lang_translations": {
                                "en": "The ice on the ditches <w>thaws</w> quickly due to the bright sun."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontvriezen"
                    ],
                    "antonyms": [
                        "bevriezen"
                    ],
                    "common_phrases": [
                        "langzaam ontdooien",
                        "volledig ontdooien"
                    ],
                    "target_lang_definitions": {
                        "en": "To become liquid or soft again after being frozen."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thaw",
                                "target_lang_sense_clarification": "to melt or unfreeze naturally"
                            },
                            {
                                "target_lang_word": "defrost",
                                "target_lang_sense_clarification": "to become unfrozen"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b464f14c-49f5-3cb5-9d9f-949dcd72c15d",
                    "sense_definition": "Minder stijf, formeel of afstandelijk worden in de omgang met anderen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gelukkig <w>ontdooiden</w> ze wat toen de kinderen binnengekomen waren.",
                            "target_lang_translations": {
                                "en": "Fortunately, they <w>thawed</w> a bit when the children had come in."
                            }
                        },
                        {
                            "text": "Na een ongemakkelijke stilte <w>ontdooide</w> het gesprek eindelijk.",
                            "target_lang_translations": {
                                "en": "After an awkward silence, the conversation finally <w>thawed</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ontvriezen",
                        "ontvriezen"
                    ],
                    "target_lang_definitions": {
                        "en": "To become less stiff, formal, or distant in social interactions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "thaw",
                                "target_lang_sense_clarification": "figuratively: to become friendlier"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
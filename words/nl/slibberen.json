            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f9266952-61db-35a1-946e-390046f700cd",
                    "sense_definition": "Glijden of bijna vallen door gladheid, vaak met een slippende beweging.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De auto begon gevaarlijk te <w>slibberen</w> op het bevroren wegdek.",
                            "target_lang_translations": {
                                "ru": "Машина начала опасно <w>скользить</w> на обледенелой дороге.",
                                "en": "The car started to <w>skid</w> dangerously on the frozen road surface."
                            }
                        },
                        {
                            "text": "Ik voelde de banden van mijn fiets <w>slibberen</w> in de modder.",
                            "target_lang_translations": {
                                "ru": "Я чувствовал, как шины моего велосипеда <w>проскальзывают</w> в грязи.",
                                "en": "I felt my bike's tires <w>slip</w> in the mud."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitglijden",
                        "slippen",
                        "glibberen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Скользить или терять равновесие на скользкой поверхности, часто с заносом.",
                        "en": "To slide or lose balance on a slippery surface, often with a skidding motion."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скользить",
                                "target_lang_sense_clarification": "двигаться по гладкой поверхности"
                            },
                            {
                                "target_lang_word": "буксовать",
                                "target_lang_sense_clarification": "прокручиваться на месте (о колесах)"
                            },
                            {
                                "target_lang_word": "проскальзывать",
                                "target_lang_sense_clarification": "терять сцепление с дорогой"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "skid",
                                "target_lang_sense_clarification": "to slide sideways or lose traction (especially vehicles)"
                            },
                            {
                                "target_lang_word": "slip",
                                "target_lang_sense_clarification": "to lose footing or grip on a slick surface"
                            },
                            {
                                "target_lang_word": "slide",
                                "target_lang_sense_clarification": "to move smoothly over a surface"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed0933be-420a-39af-beec-1d64324bee43",
                    "sense_definition": "Op moreel vlak de verkeerde kant opgaan of zondigen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "moral_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In oude teksten staat geschreven dat de rechtvaardige niet zal <w>slibberen</w>.",
                            "target_lang_translations": {
                                "ru": "В древних текстах написано, что праведник не <w>оступится</w>.",
                                "en": "In ancient texts it is written that the righteous shall not <w>slip</w>."
                            }
                        },
                        {
                            "text": "Hij was bang om moreel te <w>slibberen</w> en zijn principes te verliezen.",
                            "target_lang_translations": {
                                "ru": "Он боялся морально <w>оступиться</w> и потерять свои принципы.",
                                "en": "He was afraid to <w>slip</w> morally and lose his principles."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "voornamelijk in bijbelse of literaire context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершать ошибку в моральном плане или грешить (устаревшее).",
                        "en": "To err morally or sin (archaic/literary)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оступаться",
                                "target_lang_sense_clarification": "совершать нравственную ошибку"
                            },
                            {
                                "target_lang_word": "грешить",
                                "target_lang_sense_clarification": "нарушать религиозные или моральные нормы"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "slip",
                                "target_lang_sense_clarification": "to make a moral error or mistake"
                            },
                            {
                                "target_lang_word": "stumble",
                                "target_lang_sense_clarification": "to falter in conduct or righteousness"
                            }
                        ]

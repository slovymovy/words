    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "044cbe71-744e-324f-84f6-e639e817afd7",
                    "sense_definition": "Having a liking or affection for someone or something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am very <w>fond</w> of walking in the park early in the morning.",
                            "target_lang_translations": {
                                "ru": "Я очень <w>люблю</w> гулять в парке рано утром."
                            }
                        },
                        {
                            "text": "She has always been <w>fond</w> of classical music.",
                            "target_lang_translations": {
                                "ru": "Она всегда <w>любила</w> классическую музыку."
                            }
                        }
                    ],
                    "synonyms": [
                        "pet",
                        "fondle"
                    ],
                    "common_phrases": [
                        "fond of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывающий симпатию или привязанность к кому-либо или чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "любить",
                                "target_lang_sense_clarification": "испытывать глубокую привязанность или интерес"
                            },
                            {
                                "target_lang_word": "нравиться",
                                "target_lang_sense_clarification": "быть по вкусу, вызывать симпатию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50ef6e5e-75f7-3059-8276-edc43122da41",
                    "sense_definition": "Characterized by or displaying warmth, love, and affection.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We bade our friends a <w>fond</w> farewell at the station.",
                            "target_lang_translations": {
                                "ru": "Мы <w>тепло</w> попрощались с друзьями на вокзале."
                            }
                        },
                        {
                            "text": "He has many <w>fond</w> memories of his childhood summers.",
                            "target_lang_translations": {
                                "ru": "У него осталось много <w>тёплых</w> воспоминаний о летних каникулах в детстве."
                            }
                        }
                    ],
                    "synonyms": [
                        "pet",
                        "fondle"
                    ],
                    "common_phrases": [
                        "fond memories",
                        "fond farewell"
                    ],
                    "target_lang_definitions": {
                        "ru": "Характеризующийся теплотой, любовью и нежностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нежный",
                                "target_lang_sense_clarification": "выражающий любовь и ласку"
                            },
                            {
                                "target_lang_word": "тёплый",
                                "target_lang_sense_clarification": "приятный, душевный (о воспоминаниях, прощании)"
                            },
                            {
                                "target_lang_word": "любящий",
                                "target_lang_sense_clarification": "проявляющий любовь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4e15fec-d000-39fb-ac32-9c53958b3085",
                    "sense_definition": "Held with a strong but often naive or unrealistic hope or belief.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She clung to the <w>fond</w> hope that he would eventually return.",
                            "target_lang_translations": {
                                "ru": "Она цеплялась за <w>наивную</w> надежду, что он в конце концов вернется."
                            }
                        },
                        {
                            "text": "It was a <w>fond</w> dream to think we could finish the project in one day.",
                            "target_lang_translations": {
                                "ru": "Было <w>наивной</w> мечтой полагать, что мы сможем закончить проект за один день."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fond hope",
                        "fond illusion"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in literary or formal contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Основанный на сильной, но часто наивной или несбыточной надежде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наивный",
                                "target_lang_sense_clarification": "простодушный, необоснованный (о надеждах)"
                            },
                            {
                                "target_lang_word": "несбыточный",
                                "target_lang_sense_clarification": "нереалистичный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9163771c-7f33-3a7d-b352-c91e2a187d95",
                    "sense_definition": "Brown residue of roasted meat and vegetables that sticks to the bottom of a pan, used to make sauces.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "group_4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chef used red wine to deglaze the <w>fond</w> from the skillet.",
                            "target_lang_translations": {
                                "ru": "Шеф-повар использовал красное вино, чтобы деглазировать <w>прижарки</w> со сковороды."
                            }
                        },
                        {
                            "text": "A rich sauce requires dissolving the tasty <w>fond</w> left after searing the meat.",
                            "target_lang_translations": {
                                "ru": "Для насыщенного соуса необходимо растворить вкусные <w>прижарки</w>, оставшиеся после обжаривания мяса."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "culinary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Коричневый осадок от жареного мяса и овощей на дне сковороды, используемый для соусов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прижарки",
                                "target_lang_sense_clarification": "кулинарный термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "79f4a44b-f544-3efa-88f4-b77bacf710a7",
                    "sense_definition": "To caress or touch affectionately.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_5",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mother gently <w>fonded</w> the sleeping baby's cheek.",
                            "target_lang_translations": {
                                "ru": "Мать нежно <w>погладила</w> щеку спящего ребенка."
                            }
                        },
                        {
                            "text": "He <w>fonded</w> the dog's ears while sitting by the fire.",
                            "target_lang_translations": {
                                "ru": "Сидя у огня, он <w>ласкал</w> уши собаки."
                            }
                        }
                    ],
                    "synonyms": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b45ce6bf-22b0-30cc-92f2-54135804fa60",
                    "sense_definition": "The hard, fibrous material that forms the main substance of the trunk and branches of a tree, used for fuel or timber.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "material_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This heavy table is made of solid <w>wood</w>.",
                            "target_lang_translations": {
                                "ru": "Этот тяжелый стол сделан из цельного <w>дерева</w>."
                            }
                        },
                        {
                            "text": "He gathered some dry <w>wood</w> to start a fire on the beach.",
                            "target_lang_translations": {
                                "ru": "Он собрал сухих <w>дров</w>, чтобы развести костер на пляже."
                            }
                        }
                    ],
                    "synonyms": [
                        "timber"
                    ],
                    "common_phrases": [
                        "made of wood",
                        "piece of wood",
                        "solid wood",
                        "hard wood"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твердый волокнистый материал, составляющий основную часть ствола и ветвей дерева, используемый как топливо или строительный материал."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дерево",
                                "target_lang_sense_clarification": "материал"
                            },
                            {
                                "target_lang_word": "древесина",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae2c1f21-9eea-3241-9308-028a754c22be",
                    "sense_definition": "An area of land covered with growing trees, usually smaller than a forest.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "place_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They went for a long walk in the <w>wood</w> behind their house.",
                            "target_lang_translations": {
                                "ru": "Они отправились на долгую прогулку в <w>лес</w> за своим домом."
                            }
                        },
                        {
                            "text": "The path led us through a dark and dense <w>wood</w>.",
                            "target_lang_translations": {
                                "ru": "Тропинка вела нас через темный и густой <w>лес</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "forest",
                        "woodland",
                        "woods"
                    ],
                    "common_phrases": [
                        "in the woods",
                        "neck of the woods",
                        "out of the woods"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участок земли, покрытый растущими деревьями, обычно меньше, чем лес."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лес",
                                "target_lang_sense_clarification": "местность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f38a479-9806-3c39-bebd-437c7942a06b",
                    "sense_definition": "Trees or branches cut and prepared for burning as fuel.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "material_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to chop more <w>wood</w> for the stove before winter comes.",
                            "target_lang_translations": {
                                "ru": "Нам нужно нарубить больше <w>дров</w> для печи до наступления зимы."
                            }
                        },
                        {
                            "text": "Put another log of <w>wood</w> on the fire to keep it burning.",
                            "target_lang_translations": {
                                "ru": "Положи еще один кусок <w>дерева</w> в огонь, чтобы поддержать пламя."
                            }
                        }
                    ],
                    "synonyms": [
                        "timber"
                    ],
                    "common_phrases": [
                        "chop wood",
                        "fire wood",
                        "gather wood"
                    ],
                    "target_lang_definitions": {
                        "ru": "Деревья или ветки, нарезанные и подготовленные для сжигания в качестве топлива."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дрова",
                                "target_lang_sense_clarification": "топливо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7aee9c2e-cdf2-3f16-9123-996caf2a0d4f",
                    "sense_definition": "The material obtained from a particular species of tree, distinguished by its qualities.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "material_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Teak is a very durable <w>wood</w> that is often used for outdoor furniture.",
                            "target_lang_translations": {
                                "ru": "Тик — это очень прочная <w>древесина</w>, которую часто используют для садовой мебели."
                            }
                        },
                        {
                            "text": "The carpenter selected a fine cherry <w>wood</w> for the cabinets.",
                            "target_lang_translations": {
                                "ru": "Плотник выбрал отличную вишневую <w>древесину</w> для шкафов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hard wood",
                        "soft wood"
                    ],
                    "target_lang_definitions": {
                        "ru": "Материал, получаемый от определенного вида деревьев, отличающийся своими качествами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "древесина",
                                "target_lang_sense_clarification": "сорт"
                            },
                            {
                                "target_lang_word": "дерево",
                                "target_lang_sense_clarification": "сорт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25d30ac7-d45f-31c2-a12e-fdb7a9f0ac29",
                    "sense_definition": "A type of golf club with a large head, traditionally made of wood but now often made of metal.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sport_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He selected a three <w>wood</w> to hit the ball down the long fairway.",
                            "target_lang_translations": {
                                "ru": "Он выбрал третий <w>вуд</w>, чтобы послать мяч вдоль длинного фервея."
                            }
                        },
                        {
                            "text": "Most modern golfers prefer a metal driver over a traditional <w>wood</w>.",
                            "target_lang_translations": {
                                "ru": "Большинство современных гольфистов предпочитают металлический драйвер традиционному <w>вуду</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "three wood",
                        "fairway wood"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тип клюшки для гольфа с большой головкой, традиционно изготавливаемой из дерева, но сейчас часто из металла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вуд",
                                "target_lang_sense_clarification": "клюшка для гольфа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd2edb8c-424b-36ff-a5fc-1065a2050358",
                    "sense_definition": "A woodwind instrument in an orchestra.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "music_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The conductor asked the <w>wood</w> section to play softer during the adagio.",
                            "target_lang_translations": {
                                "ru": "Дирижер попросил группу <w>деревянных духовых</w> играть тише во время адажио."
                            }
                        },
                        {
                            "text": "She plays a <w>wood</w> in the local symphony orchestra.",
                            "target_lang_translations": {
                                "ru": "Она играет на <w>деревянном духовом инструменте</w> в местном симфоническом оркестре."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "musician jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Деревянный духовой инструмент в оркестре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "деревянный духовой инструмент",
                                "target_lang_sense_clarification": "музыка"
                            }
                        ]

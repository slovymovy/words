{
    "word_family": [
        "fearful",
        "fearless",
        "fearsome",
        "fearfully",
        "fearlessness"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e61d7ac0-05ff-31dc-a761-c31973a9daf7",
                    "sense_definition": "A strong, unpleasant emotion caused by being aware of danger or a threat.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "emotion_danger",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She felt a sudden <w>fear</w> when the lights went out.",
                            "target_lang_translations": {
                                "ru": "Она почувствовала внезапный <w>страх</w>, когда погас свет."
                            }
                        },
                        {
                            "text": "He has no <w>fear</w> of the dark.",
                            "target_lang_translations": {
                                "ru": "Он не испытывает <w>страха</w> перед темнотой."
                            }
                        }
                    ],
                    "synonyms": [
                        "dread",
                        "afraidness"
                    ],
                    "antonyms": [
                        "courage"
                    ],
                    "common_phrases": [
                        "overcome with fear",
                        "tremble with fear",
                        "face your fear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильная, неприятная эмоция, вызванная осознанием опасности или угрозы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страх",
                                "target_lang_sense_clarification": "эмоция"
                            },
                            {
                                "target_lang_word": "боязнь",
                                "target_lang_sense_clarification": "состояние страха"
                            },
                            {
                                "target_lang_word": "опасение",
                                "target_lang_sense_clarification": "предчувствие опасности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18cdbe85-de22-3d0d-a9aa-ab414cb803b8",
                    "sense_definition": "A specific feeling of anxiety regarding a particular object, activity, or situation; a phobia.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "emotion_danger",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My biggest <w>fear</w> is speaking in public.",
                            "target_lang_translations": {
                                "ru": "Мой самый большой <w>страх</w> — выступать на публике."
                            }
                        },
                        {
                            "text": "She finally conquered her <w>fear</w> of spiders.",
                            "target_lang_translations": {
                                "ru": "Она наконец поборола свою <w>боязнь</w> пауков."
                            }
                        }
                    ],
                    "common_phrases": [
                        "irrational fear",
                        "deep-seated fear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специфическое чувство тревоги по отношению к определенному объекту, деятельности или ситуации; фобия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страх",
                                "target_lang_sense_clarification": "перед чем-то конкретным"
                            },
                            {
                                "target_lang_word": "боязнь",
                                "target_lang_sense_clarification": "фобия"
                            },
                            {
                                "target_lang_word": "фобия",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "faacc530-3870-30eb-b6e2-4570bd9fb4ff",
                    "sense_definition": "Deep respect and awe, especially towards a deity or religious figure.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "veneration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They lived in <w>fear</w> of the Lord.",
                            "target_lang_translations": {
                                "ru": "Они жили в <w>страхе</w> Господнем."
                            }
                        },
                        {
                            "text": "His <w>fear</w> of God guided his decisions.",
                            "target_lang_translations": {
                                "ru": "Его <w>страх</w> перед Богом руководил его решениями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fear of God"
                    ],
                    "target_lang_definitions": {
                        "ru": "Глубокое уважение и трепет, особенно по отношению к божеству или религиозной фигуре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страх",
                                "target_lang_sense_clarification": "религиозный трепет"
                            },
                            {
                                "target_lang_word": "трепет",
                                "target_lang_sense_clarification": "благоговение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d07be44-e758-3c6f-bf16-4b2ecce94b1f",
                    "sense_definition": "A feeling of intense anxiety experienced after drinking too much alcohol.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "emotion_danger",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I woke up with the <w>fear</w> after the party last night.",
                            "target_lang_translations": {
                                "ru": "Я проснулся с чувством <w>страха</w> после вчерашней вечеринки."
                            }
                        },
                        {
                            "text": "Drinking too much wine always gives me the <w>fear</w> the next day.",
                            "target_lang_translations": {
                                "ru": "Употребление слишком большого количества вина всегда вызывает у меня <w>тревогу</w> на следующий день."
                            }
                        }
                    ],
                    "synonyms": [
                        "hangxiety"
                    ],
                    "common_phrases": [
                        "have the fear"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "often referred to as 'the fear'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство сильной тревоги, возникающее после употребления слишком большого количества алкоголя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тревога",
                                "target_lang_sense_clarification": "беспокойство"
                            },
                            {
                                "target_lang_word": "страх",
                                "target_lang_sense_clarification": "состояние ужаса"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "49b7c3e3-5e7b-3bbb-b84a-71337f362219",
                    "sense_definition": "To be scared of someone or something; to expect something bad with alarm.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emotion_danger",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>fear</w> that we are lost in the woods.",
                            "target_lang_translations": {
                                "ru": "Я <w>боюсь</w>, что мы заблудились в лесу."
                            }
                        },
                        {
                            "text": "Many people <w>fear</w> change more than anything else.",
                            "target_lang_translations": {
                                "ru": "Многие люди <w>боятся</w> перемен больше всего на свете."
                            }
                        }
                    ],
                    "synonyms": [
                        "be afraid of",
                        "be frightened of",
                        "be scared of",
                        "dread"
                    ],
                    "common_phrases": [
                        "fear the worst",
                        "fear for your life"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бояться кого-то или чего-то; с тревогой ожидать чего-то плохого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бояться",
                                "target_lang_sense_clarification": "испытывать страх"
                            },
                            {
                                "target_lang_word": "опасаться",
                                "target_lang_sense_clarification": "остерегаться"
                            },
                            {
                                "target_lang_word": "страшиться",
                                "target_lang_sense_clarification": "сильно бояться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d7b21f6-3d9e-3045-84f9-dddb670e6c42",
                    "sense_definition": "To regret; used to politely introduce bad news or a disagreement.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "polite_regret",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>fear</w> I have made a terrible mistake.",
                            "target_lang_translations": {
                                "ru": "<w>Боюсь</w>, я совершил ужасную ошибку."
                            }
                        },
                        {
                            "text": "We <w>fear</w> that we cannot accept your offer at this time.",
                            "target_lang_translations": {
                                "ru": "Мы <w>боимся</w>, что не можем принять ваше предложение в данный момент."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fear I cannot",
                        "fear it is true"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal or polite usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сожалеть; используется для вежливого сообщения плохих новостей или выражения несогласия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бояться",
                                "target_lang_sense_clarification": "в значении 'к сожалению'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95b6239c-d5ef-3be3-9aa8-72596cba18d3",
                    "sense_definition": "To feel worry or concern about someone's safety or well-being.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "concern",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She <w>fears</w> for her son's safety when he travels.",
                            "target_lang_translations": {
                                "ru": "Она <w>боится</w> за безопасность сына, когда он путешествует."
                            }
                        },
                        {
                            "text": "The doctors <w>fear</w> for his life after the accident.",
                            "target_lang_translations": {
                                "ru": "Врачи <w>опасаются</w> за его жизнь после аварии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fear for someone"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствовать беспокойство или тревогу за чью-то безопасность или благополучие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бояться",
                                "target_lang_sense_clarification": "за кого-то"
                            },
                            {
                                "target_lang_word": "опасаться",
                                "target_lang_sense_clarification": "беспокоиться о последствиях"
                            },
                            {
                                "target_lang_word": "переживать",
                                "target_lang_sense_clarification": "волноваться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f815eca6-cffe-3c30-9162-7fdb0a14b995",
                    "sense_definition": "To feel fear (used without an object).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "emotion_danger",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Never <w>fear</w>, help is on the way.",
                            "target_lang_translations": {
                                "ru": "Не <w>бойся</w>, помощь уже в пути."
                            }
                        },
                        {
                            "text": "There is no need to <w>fear</w> when you are prepared.",
                            "target_lang_translations": {
                                "ru": "Нет нужды <w>бояться</w>, когда ты готов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "never fear"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать страх (используется без дополнения)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бояться",
                                "target_lang_sense_clarification": "быть напуганным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3a0b6d6-8a0c-38f6-9ee3-849e316ebb24",
                    "sense_definition": "To feel deep respect or awe towards a deity.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "veneration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was taught to <w>fear</w> the Lord from a young age.",
                            "target_lang_translations": {
                                "ru": "Его учили <w>бояться</w> Господа с ранних лет."
                            }
                        },
                        {
                            "text": "They <w>fear</w> no man, only God.",
                            "target_lang_translations": {
                                "ru": "Они не <w>боятся</w> людей, только Бога."
                            }
                        }
                    ],
                    "synonyms": [
                        "revere",
                        "venerate",
                        "be in awe of"
                    ],
                    "antonyms": [
                        "belittle",
                        "contemn"
                    ],
                    "common_phrases": [
                        "fear God"
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытывать глубокое уважение или трепет перед божеством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бояться",
                                "target_lang_sense_clarification": "чтить со страхом"
                            },
                            {
                                "target_lang_word": "страшиться",
                                "target_lang_sense_clarification": "благоговеть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10db21fd-f468-3c0c-add6-f528400676ff",
                    "sense_definition": "To frighten or scare someone.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "cause_fear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The loud noises did not <w>fear</w> the brave knight.",
                            "target_lang_translations": {
                                "ru": "Громкие звуки не <w>пугали</w> храброго рыцаря."
                            }
                        },
                        {
                            "text": "Do not let the ghost stories <w>fear</w> you.",
                            "target_lang_translations": {
                                "ru": "Не позволяй историям о привидениях <w>пугать</w> тебя."
                            }
                        }
                    ],
                    "synonyms": [
                        "terrorize"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern usage prefers 'frighten' or 'scare'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пугать или страшить кого-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пугать",
                                "target_lang_sense_clarification": "вызывать страх"
                            },
                            {
                                "target_lang_word": "устрашать",
                                "target_lang_sense_clarification": "наводить ужас"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "59a34ef7-246f-3e44-88ba-80e319c8cb4b",
                    "sense_definition": "To suspect or doubt something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "suspicion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>fear</w> his intentions are not honorable.",
                            "target_lang_translations": {
                                "ru": "Я <w>подозреваю</w>, что его намерения не благородны."
                            }
                        },
                        {
                            "text": "Do you <w>fear</w> her loyalty to the cause?",
                            "target_lang_translations": {
                                "ru": "Ты <w>сомневаешься</w> в её преданности делу?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подозревать или сомневаться в чем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подозревать",
                                "target_lang_sense_clarification": "сомневаться"
                            },
                            {
                                "target_lang_word": "сомневаться",
                                "target_lang_sense_clarification": "не доверять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd92ca61-ff71-384d-ad77-f38f3b9403d4",
                    "sense_definition": "To be anxious or solicitous for.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "concern",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>fear</w> you, lest you fall into trouble.",
                            "target_lang_translations": {
                                "ru": "Я <w>беспокоюсь</w> о тебе, как бы ты не попал в беду."
                            }
                        },
                        {
                            "text": "He <w>feared</w> his friend's reckless behavior.",
                            "target_lang_translations": {
                                "ru": "Его <w>тревожило</w> безрассудное поведение друга."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть озабоченным или тревожиться за кого-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беспокоиться",
                                "target_lang_sense_clarification": "волноваться"
                            },
                            {
                                "target_lang_word": "тревожиться",
                                "target_lang_sense_clarification": "испытывать тревогу"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "85734dfd-0810-344e-a8a9-421fa7485192",
                    "sense_definition": "Able, strong, or sound in health.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "health",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old man remained <w>fear</w> and healthy despite his age.",
                            "target_lang_translations": {
                                "ru": "Старик оставался <w>крепким</w> и здоровым, несмотря на возраст."
                            }
                        },
                        {
                            "text": "He wished to stay <w>fear</w> in body and mind.",
                            "target_lang_translations": {
                                "ru": "Он хотел оставаться <w>здоровым</w> телом и духом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hale and fear"
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "rare or obsolete usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Способный, сильный или здоровый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здоровый",
                                "target_lang_sense_clarification": "физически крепкий"
                            },
                            {
                                "target_lang_word": "крепкий",
                                "target_lang_sense_clarification": "выносливый"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "43e3c32f-0652-383e-949f-00d46f6fea8a",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Fears</w> is waiting in the lobby.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Фирс</w> ждет в вестибюле."
                            }
                        },
                        {
                            "text": "I met a woman named Sarah <w>Fears</w> yesterday.",
                            "target_lang_translations": {
                                "ru": "Я встретил женщину по имени Сара <w>Фирс</w> вчера."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Фирс",
                                "target_lang_sense_clarification": "транслитерация"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "359b7742-d1d3-3abd-80db-2d6faa3c13e3",
                    "sense_definition": "Een luxe hotelkamer die uit meerdere vertrekken bestaat, vaak met een apart zitgedeelte.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "accommodation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Voor hun huwelijksreis boekten ze de duurste <w>suite</w> van het hotel.",
                            "target_lang_translations": {
                                "en": "For their honeymoon, they booked the hotel's most expensive <w>suite</w>.",
                                "ru": "Для своего медового месяца они забронировали самый дорогой <w>люкс</w> в отеле."
                            }
                        },
                        {
                            "text": "De president verbleef in een beveiligde <w>suite</w> op de bovenste verdieping.",
                            "target_lang_translations": {
                                "en": "The president stayed in a secured <w>suite</w> on the top floor.",
                                "ru": "Президент остановился в охраняемом <w>люксе</w> на верхнем этаже."
                            }
                        }
                    ],
                    "synonyms": [
                        "appartement"
                    ],
                    "common_phrases": [
                        "een luxe suite",
                        "de koninklijke suite",
                        "een suite boeken"
                    ],
                    "target_lang_definitions": {
                        "en": "A luxury hotel room consisting of multiple rooms, often with a separate sitting area.",
                        "ru": "Роскошный гостиничный номер, состоящий из нескольких комнат, часто с отдельной гостиной."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suite",
                                "target_lang_sense_clarification": "luxury hotel accommodation"
                            },
                            {
                                "target_lang_word": "hotel suite",
                                "target_lang_sense_clarification": "set of connected rooms"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "люкс",
                                "target_lang_sense_clarification": "в отеле"
                            },
                            {
                                "target_lang_word": "номер люкс",
                                "target_lang_sense_clarification": "полное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f82c3615-cae0-3d5e-8af8-9723e82fcbad",
                    "sense_definition": "Een kamerindeling in een woning waarbij een voor- en achterkamer gescheiden zijn door schuifdeuren.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het oude herenhuis heeft nog een prachtige <w>suite</w> met glas-in-looddeuren.",
                            "target_lang_translations": {
                                "en": "The old mansion still has a beautiful <w>suite</w> with stained-glass doors.",
                                "ru": "В старом особняке сохранились прекрасные <w>смежные комнаты</w> с витражными дверями."
                            }
                        },
                        {
                            "text": "We hebben de schuifdeuren van de <w>suite</w> opengezet om meer ruimte te creëren.",
                            "target_lang_translations": {
                                "en": "We opened the sliding doors of the <w>en suite</w> to create more space.",
                                "ru": "Мы открыли раздвижные двери <w>смежных комнат</w>, чтобы создать больше пространства."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kamer en suite",
                        "deuren en suite"
                    ],
                    "target_lang_definitions": {
                        "en": "A room layout in a house where a front and back room are separated by sliding doors.",
                        "ru": "Планировка, при которой передняя и задняя комнаты разделены раздвижными дверями."
                    },
                    "translations": {
                        "en": [

                },
                {
                    "sense_id": "8f0b0687-5494-3836-b3ef-385125aa1dee",
                    "sense_definition": "Een beslissing of handeling die achteraf onverstandig blijkt te zijn of slechte gevolgen heeft.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mistake_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was een <w>grote vergissing</w> om zonder voorbereiding aan dit project te beginnen.",
                            "target_lang_translations": {
                                "en": "It was a <w>big mistake</w> to start this project without preparation.",
                                "ru": "Было <w>большой ошибкой</w> начинать этот проект без подготовки."
                            }
                        },
                        {
                            "text": "De aankoop van dat dure huis bleek achteraf een financiële <w>vergissing</w>.",
                            "target_lang_translations": {
                                "en": "The purchase of that expensive house turned out to be a financial <w>mistake</w> in hindsight.",
                                "ru": "Покупка этого дорогого дома оказалась впоследствии финансовой <w>ошибкой</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "misrekening",
                        "misstap",
                        "flater"
                    ],
                    "common_phrases": [
                        "een grote vergissing",
                        "een pijnlijke vergissing",
                        "een fatale vergissing"
                    ],
                    "target_lang_definitions": {
                        "en": "A decision or action that turns out to be unwise or has bad consequences.",
                        "ru": "Решение или действие, которое впоследствии оказывается неразумным или имеет плохие последствия."
                    },

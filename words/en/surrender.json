    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6df54baf-2fe1-3a41-8aae-931b7bb5bdd9",
                    "sense_definition": "To stop fighting and admit that you have been defeated.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "military_submission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers were forced to <w>surrender</w> after running out of ammunition."
                        },
                        {
                            "text": "They decided to <w>surrender</w> unconditionally rather than risk more lives."
                        }
                    ],
                    "synonyms": [
                        "give up",
                        "capitulate",
                        "yield",
                        "submit",
                        "wave the white flag",
                        "throw in the towel",
                        "cave in",
                        "knuckle under"
                    ],
                    "antonyms": [
                        "stand firm",
                        "persevere"
                    ],
                    "common_phrases": [
                        "surrender unconditionally",
                        "refuse to surrender",
                        "surrender to the enemy"
                    ]
                },
                {
                    "sense_id": "4d926c34-5d69-30bd-821a-ebd782314e4b",
                    "sense_definition": "To give something such as a ticket, passport, or right to an official person because you are forced to do so.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "relinquish_possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You will be asked to <w>surrender</w> your passport at the border control."
                        },
                        {
                            "text": "He had to <w>surrender</w> his driving license after the accident."
                        }
                    ],
                    "synonyms": [
                        "relinquish",
                        "hand over",
                        "cede",
                        "turn in",
                        "give up"
                    ],
                    "common_phrases": [
                        "surrender a passport",
                        "surrender rights",
                        "surrender weapons"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ]
                },
                {
                    "sense_id": "92c5ab3e-2955-3936-a069-1e046f1832eb",
                    "sense_definition": "To allow yourself to be controlled by a feeling, habit, or influence; to stop resisting.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "yield_to_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After weeks of struggle, she finally had to <w>surrender</w> to sleep."
                        },
                        {
                            "text": "He refused to <w>surrender</w> to despair despite the bad news."
                        }
                    ],
                    "synonyms": [
                        "give in",
                        "yield",
                        "submit"
                    ],
                    "antonyms": [
                        "control"
                    ],
                    "common_phrases": [
                        "surrender to grief",
                        "surrender to temptation",
                        "surrender to despair"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary"
                        }
                    ]
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6f578250-7932-38f8-9546-0fd0aab803d6",
                    "sense_definition": "The act of admitting defeat and stopping a fight.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "military_submission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The general signed the document of <w>surrender</w> to end the war."
                        },
                        {
                            "text": "They demanded an unconditional <w>surrender</w> from the opposing forces."
                        }
                    ],
                    "synonyms": [
                        "capitulation"
                    ],
                    "common_phrases": [
                        "unconditional surrender",
                        "terms of surrender",
                        "demand surrender"
                    ]
                }
            ]
        }
    ]

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a39f88fa-6bad-3723-ab8e-8b64dab90bee",
                    "sense_definition": "Vloeibare, vettige stof die wordt gebruikt om de bewegende delen van een machine te smeren zodat deze soepel blijven draaien.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "lubricants",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De monteur gebruikt speciale <w>machineolie</w> om de oude motor te smeren.",
                            "target_lang_translations": {
                                "ru": "Механик использует специальное <w>машинное масло</w>, чтобы смазать старый двигатель."
                            }
                        },
                        {
                            "text": "Er lag een grote plas gelekte <w>machineolie</w> op de vloer van de werkplaats.",
                            "target_lang_translations": {
                                "ru": "На полу мастерской была большая лужа пролитого <w>машинного масла</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "smeerolie",
                        "motorolie"
                    ],
                    "common_phrases": [
                        "lekkende machineolie",
                        "machineolie gebruiken",
                        "naar machineolie ruiken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жидкое маслянистое вещество, используемое для смазки движущихся частей механизмов для обеспечения их плавной работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "машинное масло",
                                "target_lang_sense_clarification": "смазочный материал для механизмов"
                            },
                            {
                                "target_lang_word": "смазочное масло",
                                "target_lang_sense_clarification": "масло для смазки деталей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff133e34-be04-3daf-9eb9-669d757078ed",
                    "sense_definition": "Een drank die zeer slecht, vies of chemisch smaakt.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deze goedkope koffie is zo bitter dat het net <w>machineolie</w> lijkt.",
                            "target_lang_translations": {
                                "ru": "Этот дешевый кофе такой горький, что похож на <w>машинное масло</w>."
                            }
                        },
                        {
                            "text": "Hij kreeg een vreemd drankje voorgeschoteld dat helaas naar <w>machineolie</w> smaakte.",
                            "target_lang_translations": {
                                "ru": "Ему подали странный напиток, который, к сожалению, был на вкус как <w>машинное масло</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "smaken naar machineolie"
                    ],
                    "target_lang_definitions": {

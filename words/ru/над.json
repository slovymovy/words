    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "aff5bcb3-8f87-3584-8dd0-7d75c4eb7e13",
                    "sense_definition": "Обозначает нахождение выше, поверх кого-либо или чего-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Огромная туча нависла прямо <w>над</w> нашим домом.",
                            "target_lang_translations": {
                                "en": "A huge cloud hung right <w>over</w> our house."
                            }
                        },
                        {
                            "text": "Самолёт пролетел <w>над</w> городом и скрылся в облаках.",
                            "target_lang_translations": {
                                "en": "The plane flew <w>over</w> the city and disappeared into the clouds."
                            }
                        }
                    ],
                    "synonyms": [
                        "надо",
                        "выше"
                    ],
                    "antonyms": [
                        "под",
                        "подо"
                    ],
                    "common_phrases": [
                        "над головой",
                        "над уровнем моря",
                        "над землёй"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates a position higher than or directly above someone or something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "over",
                                "target_lang_sense_clarification": "spatial position or movement above"
                            },
                            {
                                "target_lang_word": "above",
                                "target_lang_sense_clarification": "at a higher level than"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "05dc3083-8aaf-3828-8a14-91e6d0321d20",
                    "sense_definition": "Указывает на объект, на который направлено действие (работа, смех, победа, контроль).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студент усердно трудился <w>над</w> сложным докладом.",
                            "target_lang_translations": {
                                "en": "The student worked hard <w>on</w> the complex report."
                            }
                        },
                        {
                            "text": "Не стоит смеяться <w>над</w> чужими неудачами.",
                            "target_lang_translations": {
                                "en": "One shouldn't laugh <w>at</w> others' failures."
                            }
                        }
                    ],
                    "common_phrases": [
                        "работать над собой",
                        "победа над врагом",
                        "смеяться над шуткой"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to indicate the object of specific actions, such as working, laughing, or exercising power."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "on",
                                "target_lang_sense_clarification": "used with work, labor, or effort"
                            },
                            {
                                "target_lang_word": "at",
                                "target_lang_sense_clarification": "used with laughing, mocking, or scoffing"
                            },
                            {
                                "target_lang_word": "over",
                                "target_lang_sense_clarification": "used with victory, control, or superiority"
                            }
                        ]
                    }
                }
            ]
        },
        {

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "856cf2f3-f0cf-3476-84f0-4413d6dc0d44",
                    "sense_definition": "Worthy of attention or notice; remarkable and distinguished.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "importance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum holds a <w>notable</w> collection of ancient artifacts from the region.",
                            "target_lang_translations": {
                                "ru": "В музее хранится <w>примечательная</w> коллекция древних артефактов из этого региона."
                            }
                        },
                        {
                            "text": "She achieved a <w>notable</w> success in the competition despite her lack of experience.",
                            "target_lang_translations": {
                                "ru": "Она добилась <w>выдающегося</w> успеха в соревновании, несмотря на отсутствие опыта."
                            }
                        },
                        {
                            "text": "There was a <w>notable</w> exception to the rule that everyone found surprising.",
                            "target_lang_translations": {
                                "ru": "Было одно <w>примечательное</w> исключение из правил, которое всех удивило."
                            }
                        }
                    ],
                    "synonyms": [
                        "noteworthy",
                        "remarkable",
                        "distinguished",
                        "outstanding",
                        "prominent",
                        "eminent"
                    ],
                    "antonyms": [
                        "unremarkable",
                        "trivial",
                        "irrelevant",
                        "negligible",
                        "obscure",
                        "nameless"
                    ],
                    "common_phrases": [
                        "notable achievement",
                        "notable exception",
                        "notable example",
                        "notable success"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заслуживающий внимания; выдающийся и примечательный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выдающийся",
                                "target_lang_sense_clarification": "исключительный, выделяющийся среди других"
                            },
                            {
                                "target_lang_word": "примечательный",
                                "target_lang_sense_clarification": "достойный того, чтобы быть замеченным"
                            },
                            {
                                "target_lang_word": "достойный внимания",
                                "target_lang_sense_clarification": "заслуживающий интереса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "484ebf8f-6a14-3042-a0ac-0f575b540e4e",
                    "sense_definition": "Easily seen or noticed; clear and apparent.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "observation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is a <w>notable</w> difference between the two versions of the software.",
                            "target_lang_translations": {
                                "ru": "Между двумя версиями программного обеспечения есть <w>заметная</w> разница."
                            }
                        },
                        {
                            "text": "A <w>notable</w> change in the weather occurred just before the storm arrived.",
                            "target_lang_translations": {
                                "ru": "Перед самым началом шторма произошла <w>ощутимая</w> перемена погоды."
                            }
                        }
                    ],
                    "synonyms": [
                        "noticeable",
                        "obvious",
                        "evident",
                        "apparent",
                        "manifest",
                        "clear"
                    ],
                    "antonyms": [
                        "subtle",
                        "obscure"
                    ],
                    "common_phrases": [
                        "notable difference",
                        "notable change",
                        "notable increase"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легко замечаемый; очевидный и явный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заметный",
                                "target_lang_sense_clarification": "легко видимый или обнаруживаемый"
                            },
                            {
                                "target_lang_word": "ощутимый",
                                "target_lang_sense_clarification": "существенный, явно чувствующийся"
                            },
                            {
                                "target_lang_word": "явный",
                                "target_lang_sense_clarification": "открытый, не скрываемый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70445796-6f81-32ef-acdc-82aca69d47aa",
                    "sense_definition": "Industrious and efficient, particularly in household management.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "character",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was regarded as a <w>notable</w> woman who kept her home in perfect order.",
                            "target_lang_translations": {
                                "ru": "Ее считали <w>домовитой</w> женщиной, которая содержала дом в идеальном порядке."
                            }
                        },
                        {
                            "text": "The novel describes the protagonist's mother as busy and <w>notable</w> in her domestic duties.",
                            "target_lang_translations": {
                                "ru": "В романе мать главного героя описывается как деятельная и <w>хозяйственная</w> в своих домашних обязанностях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "notable woman",
                        "notable housewife"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Used to describe capability in domestic management."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Трудолюбивый и умелый, особенно в ведении домашнего хозяйства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "домовитый",
                                "target_lang_sense_clarification": "умелый в домашнем хозяйстве"
                            },
                            {
                                "target_lang_word": "хозяйственный",
                                "target_lang_sense_clarification": "бережливый, заботящийся о хозяйстве"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {

    ],
    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "f85d5a2e-646f-332d-bedc-deb8a3d33711",
                    "sense_definition": "The supreme evil spirit in religions like Christianity, Judaism, and Islam, who is the enemy of God and tempts people to do bad things.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "the_devil",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Many religious people believe that <w>Satan</w> tries to lead humans into sin.",
                            "target_lang_translations": {
                                "ru": "Многие верующие считают, что <w>Сатана</w> пытается склонить людей к греху."
                            }
                        },
                        {
                            "text": "In the story, the hero had to fight against the dark powers of <w>Satan</w>.",
                            "target_lang_translations": {
                                "ru": "В этой истории герою пришлось сражаться с темными силами <w>Сатаны</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "The Devil",
                        "Lucifer",
                        "Beelzebub",
                        "Prince of Darkness",
                        "The Evil One"
                    ],
                    "antonyms": [
                        "God"
                    ],
                    "common_phrases": [
                        "worship Satan",
                        "sold his soul to Satan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верховный злой дух в таких религиях, как христианство, иудаизм и ислам; враг Бога, искушающий людей совершать грехи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сатана",
                                "target_lang_sense_clarification": "имя собственное, верховный дух зла"
                            },
                            {
                                "target_lang_word": "Дьявол",
                                "target_lang_sense_clarification": "распространенный синоним"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6694a3a1-ca61-3b80-ad16-d729032e3725",
                    "sense_definition": "A very cruel, wicked, or annoying person.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "evil_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My boss acts like <w>Satan</w> whenever we miss a deadline.",
                            "target_lang_translations": {
                                "ru": "Мой начальник ведет себя как <w>сатана</w>, когда мы срываем сроки."
                            }
                        },
                        {
                            "text": "When the difficult customer started shouting, I just thought, \"Not today, <w>Satan</w>.\"",
                            "target_lang_translations": {
                                "ru": "Когда сложный клиент начал кричать, я просто подумал: «Не сегодня, <w>сатана</w>»."
                            }
                        }
                    ],
                    "synonyms": [
                        "devil",
                        "fiend",
                        "monster",
                        "beast"
                    ],
                    "common_phrases": [
                        "not today Satan"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used humorously or hyperbolically"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень жестокий, злой или невыносимый человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сатана",
                                "target_lang_sense_clarification": "бранное, о злом человеке"
                            },
                            {
                                "target_lang_word": "изверг",
                                "target_lang_sense_clarification": "о жестоком человеке"
                            },
                            {
                                "target_lang_word": "дьявол",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4d19bc28-a108-3459-95f2-dc9d2fad1824",
                    "sense_definition": "A demon or evil spirit that serves the Devil.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "demon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient text describes how a <w>satan</w> would whisper lies to the king.",
                            "target_lang_translations": {
                                "ru": "Древний текст описывает, как <w>демон</w> нашептывал ложь королю."
                            }
                        },
                        {
                            "text": "They believed that invisible <w>satans</w> were causing the strange events in the house.",
                            "target_lang_translations": {
                                "ru": "Они верили, что невидимые <w>бесы</w> были причиной странных событий в доме."
                            }
                        }
                    ],
                    "synonyms": [
                        "demon",
                        "devil",
                        "fallen angel",
                        "imp"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mostly found in religious or historical texts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Демон или злой дух, служащий Дьяволу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "демон",
                                "target_lang_sense_clarification": "злой дух"
                            },
                            {
                                "target_lang_word": "бес",
                                "target_lang_sense_clarification": "мелкий злой дух"
                            }
                        ]
                    }
                }
            ]
        }
    ]

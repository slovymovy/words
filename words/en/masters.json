                        {
                            "text": "Having a <masters> degree can open up more career opportunities.",
                            "target_lang_translations": {
                                "ru": "Наличие <степени магистра> может открыть больше карьерных возможностей."
                            }
                        }
                    ],
                    "synonyms": [
                        "master's degree"
                    ],
                    "common_phrases": [
                        "masters degree",
                        "do a masters",
                        "get a masters"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ученая степень, следующая за бакалавриатом; магистратура."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "магистратура",
                                "target_lang_sense_clarification": "курс обучения"
                            },

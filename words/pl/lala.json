    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "52ca6e70-2f0a-3d10-997c-51716fe85fd7",
                    "sense_definition": "Potoczne lub dziecięce określenie lalki (zabawki w kształcie człowieka).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "toy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mała dziewczynka nie chciała zasnąć bez swojej ulubionej <w>lali</w>.",
                            "target_lang_translations": {
                                "ru": "Маленькая девочка не хотела засыпать без своей любимой <w>ляли</w>."
                            }
                        },
                        {
                            "text": "Na wystawie sklepowej siedziała piękna porcelanowa <w>lala</w> w sukience.",
                            "target_lang_translations": {
                                "ru": "На витрине магазина сидела красивая фарфоровая <w>кукла</w> в платье."
                            }
                        }
                    ],
                    "synonyms": [
                        "lalka",
                        "lalunia"
                    ],
                    "common_phrases": [
                        "ukochana lala",
                        "bawić się lalą",
                        "lala dla dziecka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "childish language"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное или детское название куклы (игрушки в форме человека)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ляля",
                                "target_lang_sense_clarification": "детское слово"
                            },
                            {
                                "target_lang_word": "кукла",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "лялька",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9cab8b2-34d7-321d-b3dd-196c9f24ef91",
                    "sense_definition": "Potoczne określenie atrakcyjnej, ładnej dziewczyny lub młodej kobiety.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "woman",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój kolega umawia się z niezłą <w>lalą</w> z sąsiedniego osiedla.",
                            "target_lang_translations": {
                                "ru": "Мой друг встречается с классной <w>цыпочкой</w> из соседнего района."
                            }
                        },
                        {
                            "text": "Wszyscy oglądali się za tą <w>lalą</w>, gdy weszła do klubu.",
                            "target_lang_translations": {
                                "ru": "Все оглядывались на эту <w>красотку</w>, когда она вошла в клуб."
                            }
                        }
                    ],
                    "synonyms": [
                        "laska",
                        "lalka"
                    ],
                    "common_phrases": [
                        "niezła lala",
                        "fajna lala"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное обозначение привлекательной, красивой девушки или молодой женщины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "куколка",
                                "target_lang_sense_clarification": "о красивой девушке"
                            },
                            {
                                "target_lang_word": "красотка",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "цыпочка",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39138a21-d3e0-3e97-bd66-db1218dc1509",
                    "sense_definition": "Lekceważące określenie kobiety, która dba przesadnie o wygląd, ale jest postrzegana jako mało inteligentna lub próżna.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "woman",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie bądź taką pustą <w>lalą</w> i zacznij interesować się czymś więcej niż modą.",
                            "target_lang_translations": {
                                "ru": "Не будь такой пустой <w>куклой</w> и начни интересоваться чем-то большим, чем мода."
                            }
                        },
                        {
                            "text": "To tylko malowana <w>lala</w>, która nie ma nic mądrego do powiedzenia.",
                            "target_lang_translations": {
                                "ru": "Это просто крашеная <w>кукла</w>, которой нечего умного сказать."
                            }
                        }
                    ],
                    "synonyms": [
                        "barbietka",
                        "lalka"
                    ],
                    "common_phrases": [
                        "malowana lala",
                        "pusta lala",
                        "głupia lala"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "pejorative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пренебрежительное название женщины, которая чрезмерно заботится о внешности, но воспринимается как неумная или тщеславная."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фифа",
                                "target_lang_sense_clarification": "о манерной, пустой женщине"
                            },
                            {
                                "target_lang_word": "пустышка",
                                "target_lang_sense_clarification": "о глупой женщине"
                            },
                            {
                                "target_lang_word": "кукла",
                                "target_lang_sense_clarification": "пренебрежительно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "25b9edf5-4710-33cb-b92c-b153fedc73eb",
                    "sense_definition": "Dawny okrzyk używany podczas polowania (przy szczuciu psami) lub jako okrzyk wojenny.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Myśliwi głośno krzyczeli <w>lala</w>, aby zachęcić psy do pościgu za zwierzyną.",
                            "target_lang_translations": {
                                "ru": "Охотники громко кричали <w>ату</w>, чтобы побудить собак к погоне за дичью."
                            }
                        },
                        {
                            "text": "W starych pieśniach można usłyszeć bojowe <w>lala</w> wznoszone przez wojowników.",
                            "target_lang_translations": {
                                "ru": "В старых песнях можно услышать боевое <w>алала</w>, возносимое воинами."
                            }
                        }
                    ],
                    "synonyms": [
                        "lalasz!",

                {
                    "sense_id": "c1d68192-61e1-345e-9af8-910a166689b9",
                    "sense_definition": "Op alle plaatsen; op elke plek waar je bent of kijkt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb mijn sleutels <w>overal</w> gezocht, maar ik kan ze niet vinden.",
                            "target_lang_translations": {
                                "en": "I looked for my keys <w>everywhere</w>, but I can't find them.",
                                "ru": "Я искал свои ключи <w>везде</w>, но не могу их найти."
                            }
                        },
                        {
                            "text": "In de lente zie je <w>overal</w> in het park kleurrijke bloemen.",
                            "target_lang_translations": {
                                "en": "In spring, you see colorful flowers <w>everywhere</w> in the park.",
                                "ru": "Весной в парке <w>повсюду</w> видны разноцветные цветы."
                            }
                        }
                    ],
                    "synonyms": [
                        "alom",
                        "wijd en zijd",
                        "allerwegen"
                    ],
                    "antonyms": [
                        "nergens"
                    ],
                    "common_phrases": [
                        "overal en nergens",
                        "overal ter wereld",
                        "overal vandaan"
                    ],
                    "target_lang_definitions": {
                        "en": "At all places; at every spot where you are or look.",
                        "ru": "Во всех местах; в любом месте, где вы находитесь или куда смотрите."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "everywhere",
                                "target_lang_sense_clarification": "in or to all places"
                            },
                            {
                                "target_lang_word": "all over",
                                "target_lang_sense_clarification": "covering the whole area"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "везде",
                                "target_lang_sense_clarification": "наречие места"
                            },
                            {
                                "target_lang_word": "повсюду",
                                "target_lang_sense_clarification": "во всех местах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c4f13ea-76ca-337e-91e6-24c40107c3b6",
                    "sense_definition": "Vorm van 'alles' die wordt gebruikt in combinatie met een voorzetsel (voornaamwoordelijk bijwoord).",

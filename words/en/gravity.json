    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "44f0169c-45d6-36be-ae85-dc9185bb2303",
                    "sense_definition": "The natural force that attracts objects towards the center of the earth, or towards any other physical body having mass.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "physics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Astronauts float in space because there is very little <w>gravity</w> to hold them down.",
                            "target_lang_translations": {
                                "ru": "Астронавты парят в космосе, потому что там слишком слабая <w>гравитация</w>, чтобы удерживать их."
                            }
                        },
                        {
                            "text": "Do you know that <w>gravity</w> is the force pulling you towards the ground right now?",
                            "target_lang_translations": {
                                "ru": "Знаешь ли ты, что <w>гравитация</w> — это сила, которая прямо сейчас притягивает тебя к земле?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "zero gravity",
                        "center of gravity",
                        "force of gravity",
                        "law of gravity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественная сила, которая притягивает объекты к центру Земли или к любому другому физическому телу, обладающему массой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гравитация",
                                "target_lang_sense_clarification": "физическое явление"
                            },
                            {
                                "target_lang_word": "притяжение",
                                "target_lang_sense_clarification": "сила, притягивающая предметы"
                            },
                            {
                                "target_lang_word": "тяготение",
                                "target_lang_sense_clarification": "физический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be48cb88-3755-3467-a3b7-cafe52080ad5",
                    "sense_definition": "The extreme importance or seriousness of a situation or event.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "seriousness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I hope you appreciate the <w>gravity</w> of the situation and act accordingly.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, вы понимаете всю <w>серьёзность</w> ситуации и будете действовать соответствующим образом."
                            }
                        },
                        {
                            "text": "The <w>gravity</w> of his crime shocked the entire community.",
                            "target_lang_translations": {
                                "ru": "<w>Серьёзность</w> его преступления потрясла всё общество."
                            }
                        }
                    ],
                    "synonyms": [
                        "seriousness",
                        "graveness"
                    ],
                    "antonyms": [
                        "levity"
                    ],
                    "common_phrases": [
                        "gravity of the situation",
                        "utmost gravity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крайняя важность или серьёзность ситуации или события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "серьёзность",
                                "target_lang_sense_clarification": "положения или ситуации"
                            },
                            {
                                "target_lang_word": "важность",
                                "target_lang_sense_clarification": "значимость события"
                            }
                        ]
                    }
                }
            ]
        }
    ]

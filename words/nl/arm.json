            "senses": [
                {
                    "sense_id": "4c66ad37-8c55-3e77-9486-e9cbedb599e8",
                    "sense_definition": "Met weinig geld of bezittingen; niet rijk.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "financial_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn grootouders waren vroeger erg <w>arm</w> en hadden geen auto.",
                            "target_lang_translations": {
                                "en": "My grandparents used to be very <w>poor</w> and didn't have a car.",
                                "ru": "Мои бабушка и дедушка раньше были очень <w>бедными</w> и у них не было машины."
                            }
                        },
                        {
                            "text": "Er is een groot verschil tussen <w>arm</w> en rijk in dit land.",
                            "target_lang_translations": {
                                "en": "There is a big difference between <w>poor</w> and rich in this country.",
                                "ru": "В этой стране существует большая разница между <w>бедными</w> и богатыми."
                            }
                        }
                    ],
                    "synonyms": [
                        "behoeftig",
                        "bezitloos"
                    ],
                    "antonyms": [
                        "rijk",
                        "welgesteld"
                    ],
                    "common_phrases": [
                        "arm en rijk",
                        "arm zijn"
                    ],
                    "target_lang_definitions": {
                        "en": "Having little money or possessions; not rich.",
                        "ru": "Имеющий мало денег или имущества; не богатый."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "poor",
                                "target_lang_sense_clarification": "financial"
                            }
                        ],
                        "ru": [

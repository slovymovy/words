        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d8b3c16f-753b-3a07-945b-c059fa279315",
                    "sense_definition": "A long, flat, and hard piece of wood or similar material used for building or making things.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "material_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The carpenter nailed a loose <w>board</w> back onto the fence.",
                            "target_lang_translations": {
                                "ru": "Плотник прибил шатающуюся <w>доску</w> обратно к забору."
                            }
                        },
                        {
                            "text": "She placed the vegetables on the cutting <w>board</w> to chop them.",
                            "target_lang_translations": {
                                "ru": "Она положила овощи на разделочную <w>доску</w>, чтобы нарезать их."
                            }
                        }
                    ],
                    "synonyms": [
                        "plank",
                        "panel",
                        "slat"
                    ],
                    "common_phrases": [
                        "cutting board",
                        "ironing board",
                        "wooden board",
                        "floor board"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинный плоский кусок дерева или подобного материала, используемый для строительства или изготовления предметов."
                    },
                    "translations": {
                        "ru": [
                            {

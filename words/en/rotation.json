{
    "word_family": [
        "rotate",
        "rotational",
        "rotator",
        "rotary"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e9ca2ecc-4f2e-31bb-a12b-9304f1860d92",
                    "sense_definition": "The act of turning around a center point or an axis.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The Earth's <w>rotation</w> on its axis causes the cycle of day and night.",
                            "target_lang_translations": {
                                "ru": "<w>Вращение</w> Земли вокруг своей оси вызывает смену дня и ночи."
                            }
                        },
                        {
                            "text": "You can adjust the speed of the fan's <w>rotation</w> using this dial.",
                            "target_lang_translations": {
                                "ru": "Вы можете регулировать скорость <w>вращения</w> вентилятора с помощью этого переключателя."
                            }
                        }
                    ],
                    "synonyms": [
                        "turning",
                        "revolution"
                    ],
                    "common_phrases": [
                        "axis of rotation",
                        "direction of rotation",
                        "speed of rotation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие вращения вокруг центральной точки или оси."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вращение",
                                "target_lang_sense_clarification": "процесс кругового движения"
                            },
                            {
                                "target_lang_word": "поворот",
                                "target_lang_sense_clarification": "изменение направления или положения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb8e6698-a791-3357-aac6-c83f342c3b57",
                    "sense_definition": "A single complete turn around a center or an axis.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The satellite completes one full <w>rotation</w> around the planet every ninety minutes.",
                            "target_lang_translations": {
                                "ru": "Спутник совершает один полный <w>оборот</w> вокруг планеты каждые девяносто минут."
                            }
                        },
                        {
                            "text": "The figure skater performed a perfect <w>rotation</w> before landing on the ice.",
                            "target_lang_translations": {
                                "ru": "Фигурист выполнил идеальный <w>оборот</w> перед приземлением на лед."
                            }
                        }
                    ],
                    "synonyms": [
                        "revolution",
                        "turn"
                    ],
                    "common_phrases": [
                        "complete a rotation",
                        "full rotation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Один полный оборот вокруг центра или оси."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оборот",
                                "target_lang_sense_clarification": "полный круг вращения"
                            },
                            {
                                "target_lang_word": "виток",
                                "target_lang_sense_clarification": "один цикл спирального движения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e09e0143-330e-3855-9d8f-bf5426781770",
                    "sense_definition": "A regular schedule in which people or things take turns performing a task or being used.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "scheduling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The staff members work at the front desk in <w>rotation</w> to ensure fairness.",
                            "target_lang_translations": {
                                "ru": "Сотрудники работают на стойке регистрации в порядке <w>очереди</w> для обеспечения справедливости."
                            }
                        },
                        {
                            "text": "Farmers use crop <w>rotation</w> to keep the soil healthy and prevent nutrient depletion.",
                            "target_lang_translations": {
                                "ru": "Фермеры используют <w>севооборот</w> для поддержания здоровья почвы и предотвращения истощения питательных веществ."
                            }
                        }
                    ],
                    "synonyms": [
                        "roster",
                        "schedule",
                        "rota",
                        "turn"
                    ],
                    "common_phrases": [
                        "in rotation",
                        "job rotation",
                        "crop rotation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Регулярный график или порядок, при котором люди или предметы сменяют друг друга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чередование",
                                "target_lang_sense_clarification": "последовательная смена"
                            },
                            {
                                "target_lang_word": "ротация",
                                "target_lang_sense_clarification": "смена кадров или обязанностей"
                            },
                            {
                                "target_lang_word": "севооборот",
                                "target_lang_sense_clarification": "сельскохозяйственный термин (смена культур)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "71ce0195-d8a4-3309-8c05-e4975cb6e934",
                    "sense_definition": "The repeated playing of songs or advertisements on a radio station or television channel.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "media",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The catchy new song has been in heavy <w>rotation</w> on the radio all week.",
                            "target_lang_translations": {
                                "ru": "Эта новая запоминающаяся песня была в жесткой <w>ротации</w> на радио всю неделю."
                            }
                        },
                        {
                            "text": "The network put the commercial into regular <w>rotation</w> during the evening news.",
                            "target_lang_translations": {
                                "ru": "Телесеть включила этот рекламный ролик в регулярную <w>ротацию</w> во время вечерних новостей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "heavy rotation",
                        "regular rotation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Многократное повторение песен или рекламы в эфире радиостанции или телеканала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ротация",
                                "target_lang_sense_clarification": "повторение в эфире"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1b69f4b-0edf-3352-8631-62ee6b97341e",
                    "sense_definition": "The movement of investments between different sectors or styles based on market predictions.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sector <w>rotation</w> is a strategy used by investors to capitalize on economic cycles.",
                            "target_lang_translations": {
                                "ru": "Секторальная <w>ротация</w> — это стратегия, используемая инвесторами для извлечения выгоды из экономических циклов."
                            }
                        },
                        {
                            "text": "We observed a <w>rotation</w> of funds from technology stocks into more stable utilities.",
                            "target_lang_translations": {
                                "ru": "Мы наблюдали <w>ротацию</w> средств из технологических акций в более стабильные коммунальные предприятия."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sector rotation",
                        "style rotation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещение капитала между различными секторами рынка или стилями инвестирования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ротация",
                                "target_lang_sense_clarification": "перераспределение активов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9359c48d-6ad0-3cf7-a8fb-64e85c4ed775",
                    "sense_definition": "The group of starting pitchers on a baseball team who take turns starting games.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team needs to strengthen its starting <w>rotation</w> before the playoffs begin.",
                            "target_lang_translations": {
                                "ru": "Команде необходимо усилить свою стартовую <w>ротацию</w> перед началом плей-офф."
                            }
                        },
                        {
                            "text": "He is considered the ace of the pitching <w>rotation</w> this season.",
                            "target_lang_translations": {
                                "ru": "В этом сезоне он считается асом <w>ротации</w> питчеров."
                            }
                        }
                    ],
                    "common_phrases": [
                        "starting rotation",
                        "pitching rotation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа стартовых питчеров в бейсбольной команде, которые выходят на игру по очереди."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ротация",
                                "target_lang_sense_clarification": "состав питчеров"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a63dae7d-3ef7-3b70-8e61-a88413dc9afe",
                    "sense_definition": "An operation that turns a geometric figure around a fixed point.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "mathematics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <w>rotation</w> of ninety degrees transforms the coordinates of the shape.",
                            "target_lang_translations": {
                                "ru": "<w>Поворот</w> на девяносто градусов преобразует координаты фигуры."
                            }
                        },
                        {
                            "text": "The teacher explained how to perform a <w>rotation</w> on the graph paper.",
                            "target_lang_translations": {
                                "ru": "Учитель объяснил, как выполнить <w>поворот</w> на миллиметровой бумаге."
                            }
                        }
                    ],
                    "common_phrases": [
                        "angle of rotation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Геометрическое преобразование, поворачивающее фигуру вокруг фиксированной точки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поворот",
                                "target_lang_sense_clarification": "геометрическое преобразование"
                            },
                            {
                                "target_lang_word": "вращение",
                                "target_lang_sense_clarification": "движение фигуры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd13c886-2ad0-3c30-a4db-07ad2b5daf4c",
                    "sense_definition": "The action of lifting the nose wheel of an aircraft off the ground during takeoff.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "aviation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pilot initiated <w>rotation</w> as soon as the plane reached the correct speed.",
                            "target_lang_translations": {
                                "ru": "Пилот начал <w>подъём передней стойки</w> как только самолет достиг нужной скорости."
                            }
                        },
                        {
                            "text": "Smooth <w>rotation</w> is critical for a safe and comfortable takeoff.",
                            "target_lang_translations": {
                                "ru": "Плавный <w>подъём носа</w> критически важен для безопасного и комфортного взлета."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rotation speed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по поднятию носовой стойки шасси самолета от земли во время взлета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подъём передней стойки",
                                "target_lang_sense_clarification": "фаза взлета"
                            },
                            {
                                "target_lang_word": "ротация",
                                "target_lang_sense_clarification": "авиационный термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
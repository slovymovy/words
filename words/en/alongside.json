    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "31dad896-d9e5-3c51-8815-3f5570badf20",
                    "sense_definition": "Close to the side of; next to.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "spatial_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new bicycle path runs <w>alongside</w> the main road.",
                            "target_lang_translations": {
                                "ru": "Новая велосипедная дорожка проходит <w>вдоль</w> главной дороги."
                            }
                        },
                        {
                            "text": "He parked his car <w>alongside</w> the curb to answer the phone.",
                            "target_lang_translations": {
                                "ru": "Он припарковал машину <w>вдоль</w> бордюра, чтобы ответить на звонок."
                            }
                        }
                    ],
                    "synonyms": [
                        "next to",
                        "beside"
                    ],
                    "common_phrases": [
                        "park alongside",
                        "run alongside",
                        "walk alongside"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рядом с чем-то или вдоль чего-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вдоль",
                                "target_lang_sense_clarification": "в направлении длины чего-либо"
                            },
                            {
                                "target_lang_word": "рядом с",
                                "target_lang_sense_clarification": "в непосредственной близости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13ffadd0-3fb9-39eb-8841-ed28d05f49cb",
                    "sense_definition": "Together with or in cooperation with someone.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "cooperation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The nurse worked <w>alongside</w> the doctor to save the patient.",
                            "target_lang_translations": {
                                "ru": "Медсестра работала <w>бок о бок с</w> врачом, чтобы спасти пациента."
                            }
                        },
                        {
                            "text": "Local volunteers fought <w>alongside</w> the fire department to control the blaze.",
                            "target_lang_translations": {
                                "ru": "Местные добровольцы боролись с огнем <w>вместе с</w> пожарными."
                            }
                        }
                    ],
                    "synonyms": [
                        "beside",
                        "level"
                    ],
                    "common_phrases": [
                        "work alongside",
                        "serve alongside",
                        "fight alongside"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вместе или в сотрудничестве с кем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бок о бок с",
                                "target_lang_sense_clarification": "в тесном сотрудничестве"
                            },
                            {
                                "target_lang_word": "вместе с",
                                "target_lang_sense_clarification": "совместно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "e5ff0495-c342-3289-812f-f07697c8e21b",
                    "sense_definition": "At or to the side of someone or something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police car pulled <w>alongside</w> and signaled for us to stop.",
                            "target_lang_translations": {
                                "ru": "Полицейская машина пристроилась <w>рядом</w> и подала нам сигнал остановиться."
                            }
                        },
                        {
                            "text": "A small boat came <w>alongside</w> to unload the passengers.",
                            "target_lang_translations": {
                                "ru": "Маленькая лодка встала <w>борт о борт</w>, чтобы высадить пассажиров."
                            }
                        }
                    ],
                    "synonyms": [
                        "abreast",
                        "level"
                    ],
                    "common_phrases": [
                        "pull alongside",
                        "come alongside",
                        "draw alongside"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сбоку от кого-то или чего-то; на одной линии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рядом",
                                "target_lang_sense_clarification": "поблизости, сбоку"
                            },
                            {
                                "target_lang_word": "борт о борт",
                                "target_lang_sense_clarification": "совсем рядом (обычно о судах или машинах)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    }
                },
                {
                    "sense_id": "c39ae350-b51d-3d3b-8bfd-b97c67f6b0f0",
                    "sense_definition": "O człowieku: chory psychicznie lub zachowujący się w sposób nienormalny",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "insane_colloquial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy ty jesteś <w>psychiczny</w>, żeby wchodzić tam bez żadnego zabezpieczenia?",
                            "target_lang_translations": {
                                "ru": "Ты что, <w>ненормальный</w>, чтобы входить туда без какой-либо страховки?"
                            }
                        },
                        {
                            "text": "Ten twój sąsiad jest jakiś <w>psychiczny</w>, ciągle krzyczy na ludzi.",
                            "target_lang_translations": {
                                "ru": "Этот твой сосед какой-то <w>психованный</w>, постоянно кричит на людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "nienormalny",
                        "szalony",
                        "walnięty"
                    ],
                    "antonyms": [
                        "fizyczny"
                    ],
                    "traits": [
                        {

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0f580b2d-884c-322d-914d-3d349f326d42",
                    "sense_definition": "Занять собой обширное пространство или протянуться на большое расстояние.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Перед путешественниками <w>простёрлась</w> бескрайняя зелёная долина.",
                            "target_lang_translations": {
                                "en": "An endless green valley <w>stretched out</w> before the travelers."
                            }
                        },
                        {
                            "text": "Густой туман <w>простёрся</w> над рекой до самого утра.",
                            "target_lang_translations": {
                                "en": "A thick fog <w>spread</w> over the river until the very morning."
                            }
                        }
                    ],
                    "synonyms": [
                        "раскинуться",
                        "протянуться",
                        "распространиться"
                    ],
                    "common_phrases": [
                        "простереться до горизонта",
                        "простереться на километры",
                        "простереться вдаль"
                    ],
                    "target_lang_definitions": {
                        "en": "To extend over a large area or stretch out into the distance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stretch out",
                                "target_lang_sense_clarification": "over a distance or landscape"
                            },
                            {
                                "target_lang_word": "extend",
                                "target_lang_sense_clarification": "over an area or space"
                            },
                            {
                                "target_lang_word": "spread",
                                "target_lang_sense_clarification": "to cover a surface or area"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f085f67-9910-31a4-b077-d0f420bbdf58",
                    "sense_definition": "Лечь плашмя, вытянувшись во весь рост (часто в знак покорности или от изнеможения).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Уставший путник <w>простёрся</w> на траве и мгновенно уснул.",
                            "target_lang_translations": {
                                "en": "The tired traveler <w>stretched out</w> on the grass and fell asleep instantly."
                            }
                        },
                        {
                            "text": "Люди в страхе <w>простёрлись</w> ниц перед грозным правителем.",
                            "target_lang_translations": {
                                "en": "The people <w>prostrated themselves</w> in fear before the formidable ruler."
                            }
                        }
                    ],
                    "synonyms": [
                        "пасть",
                        "повалиться",
                        "распластаться"
                    ],
                    "antonyms": [
                        "встать",
                        "подняться"
                    ],
                    "common_phrases": [
                        "простереться ниц",
                        "простереться на земле",
                        "простереться у ног"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "устаревшее или книжное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To lie flat on the ground, often in submission or exhaustion."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "prostrate oneself",
                                "target_lang_sense_clarification": "in submission or worship"
                            },
                            {
                                "target_lang_word": "stretch out",
                                "target_lang_sense_clarification": "lie flat on the ground"
                            }
                        ]

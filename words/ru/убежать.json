    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f3caf253-96f8-32e6-8d7e-6ed32dd4bcb0",
                    "sense_definition": "Удалиться откуда-либо бегом или очень быстро.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мальчик испугался большой собаки и быстро <убежал> домой.",
                            "target_lang_translations": {
                                "en": "The boy got scared of the big dog and quickly <ran away> home."
                            }
                        },
                        {
                            "text": "Мы решили <убежать> с последнего урока, чтобы погулять в парке.",
                            "target_lang_translations": {
                                "en": "We decided to <run away> from the last class to take a walk in the park."
                            }
                        }
                    ],
                    "synonyms": [
                        "удалиться",
                        "смыться",
                        "дать тягу"
                    ],
                    "antonyms": [
                        "прибежать"
                    ],
                    "common_phrases": [
                        "убежать домой",
                        "быстро убежать",
                        "убежать далеко"
                    ],
                    "target_lang_definitions": {
                        "en": "To leave a place by running or very quickly."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run away",
                                "target_lang_sense_clarification": "general movement"
                            },
                            {
                                "target_lang_word": "run off",
                                "target_lang_sense_clarification": "leave quickly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b0198655-0d04-32e6-afa2-da67a655c80c",
                    "sense_definition": "Совершить побег, тайно или насильно покинуть место заключения или опасности.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Пленникам удалось <убежать> из лагеря под покровом ночи.",
                            "target_lang_translations": {
                                "en": "The prisoners managed to <escape> from the camp under the cover of night."
                            }
                        },
                        {
                            "text": "Герой фильма пытается <убежать> от преследователей через лес.",
                            "target_lang_translations": {
                                "en": "The movie hero is trying to <run away> from pursuers through the forest."
                            }
                        }
                    ],
                    "synonyms": [
                        "дать тягу",
                        "смыться"
                    ],
                    "common_phrases": [
                        "убежать из тюрьмы",
                        "убежать из плена",
                        "убежать за границу"
                    ],
                    "target_lang_definitions": {
                        "en": "To escape from confinement or danger."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "escape",
                                "target_lang_sense_clarification": "from confinement"
                            },
                            {
                                "target_lang_word": "flee",
                                "target_lang_sense_clarification": "from danger"
                            },
                            {
                                "target_lang_word": "run away",
                                "target_lang_sense_clarification": "general"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "47831e3c-5d28-31db-a5e5-1053809f14ea",
                    "sense_definition": "Подняться и перелиться через край при кипении (о жидкости, например, молоке).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "liquid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я засмотрелся телевизор, и молоко на плите <убежало>.",
                            "target_lang_translations": {
                                "en": "I got distracted by the TV, and the milk on the stove <boiled over>."
                            }
                        },
                        {
                            "text": "Сними кастрюлю с огня, иначе суп сейчас <убежит>.",
                            "target_lang_translations": {
                                "en": "Take the pot off the fire, otherwise the soup will <boil over> any moment."
                            }
                        }
                    ],
                    "common_phrases": [
                        "молоко убежало",
                        "кофе убежал"
                    ],
                    "target_lang_definitions": {
                        "en": "To rise and spill over the edge while boiling (of a liquid)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "boil over",
                                "target_lang_sense_clarification": "culinary"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e20b165-0088-3fc5-ad63-37a356df8e30",
                    "sense_definition": "Начать показывать время больше истинного; уйти вперёд (о часах).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мои наручные часы <убежали> на десять минут вперёд.",
                            "target_lang_translations": {
                                "en": "My wristwatch <gained> ten minutes."
                            }
                        },
                        {
                            "text": "Если будильник постоянно <убегает>, его нужно отнести в ремонт.",
                            "target_lang_translations": {
                                "en": "If the alarm clock constantly <runs fast>, it needs to be taken for repairs."
                            }
                        }
                    ],
                    "common_phrases": [
                        "часы убежали",
                        "убежать вперёд"
                    ],
                    "target_lang_definitions": {
                        "en": "To show a time later than the actual time (of a clock)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "run fast",
                                "target_lang_sense_clarification": "of a timepiece"
                            },
                            {
                                "target_lang_word": "gain time",
                                "target_lang_sense_clarification": "accumulate error"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e3d642a-ad26-334b-acc4-7c706a195c73",
                    "sense_definition": "Избежать чего-либо неприятного, уклониться от чего-либо (часто в переносном смысле).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Никто не может <убежать> от своей собственной судьбы.",
                            "target_lang_translations": {
                                "en": "No one can <run away> from their own destiny."
                            }
                        },
                        {
                            "text": "Он тщетно надеялся <убежать> от ответственности за свои поступки.",
                            "target_lang_translations": {
                                "en": "He vainly hoped to <escape> responsibility for his actions."
                            }
                        }
                    ],
                    "synonyms": [
                        "избежать"
                    ],
                    "common_phrases": [
                        "убежать от судьбы",
                        "убежать от смерти"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "поэтическое"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To avoid or evade something unpleasant (figurative)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "escape",
                                "target_lang_sense_clarification": "figurative"
                            },
                            {
                                "target_lang_word": "run away",
                                "target_lang_sense_clarification": "from problems/fate"
                            }
                        ]
                    }
                }
            ]
        }

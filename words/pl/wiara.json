    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f40a3244-1460-3113-b797-3004cf9018bd",
                    "sense_definition": "System przekonań religijnych, uznawanie istnienia Boga i prawd objawionych; przynależność do określonego kościoła lub związku wyznaniowego.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "religion_belief",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W trudnych chwilach <w>wiara</w> daje wielu ludziom siłę i nadzieję na lepsze jutro.",
                            "target_lang_translations": {
                                "ru": "В трудные минуты <w>вера</w> дает многим людям силу и надежду на лучшее завтра."
                            }
                        },
                        {
                            "text": "Męczennik był torturowany, ponieważ nie chciał wyrzec się swojej <w>wiary</w>.",
                            "target_lang_translations": {
                                "ru": "Мученика пытали, так как он не хотел отречься от своей <w>веры</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "religia",
                        "wyznanie"
                    ],
                    "common_phrases": [
                        "wiara katolicka",
                        "wyznanie wiary",
                        "obrońca wiary",
                        "zmiana wiary"
                    ],
                    "target_lang_definitions": {
                        "ru": "Система религиозных убеждений, признание существования Бога; принадлежность к определенной церкви или конфессии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вера",
                                "target_lang_sense_clarification": "религиозное чувство"
                            },
                            {
                                "target_lang_word": "вероисповедание",
                                "target_lang_sense_clarification": "принадлежность к религии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "270512a0-b86f-35c9-91fc-61917515ecd2",
                    "sense_definition": "Głębokie przekonanie, że coś jest prawdą, lub pełne zaufanie do kogoś, mimo braku twardych dowodów.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "general_belief",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego niezachwiana <w>wiara</w> w sukces projektu motywowała cały zespół do ciężkiej pracy.",
                            "target_lang_translations": {
                                "ru": "Его непоколебимая <w>вера</w> в успех проекта мотивировала всю команду усердно работать."
                            }
                        },
                        {
                            "text": "Twoja <w>wiara</w> w to, że czterolistna koniczyna przynosi szczęście, jest bardzo naiwna.",
                            "target_lang_translations": {
                                "ru": "Твоя <w>вера</w> в то, что четырехлистный клевер приносит удачу, очень наивна."
                            }
                        }
                    ],
                    "synonyms": [
                        "zaufanie"
                    ],
                    "common_phrases": [
                        "wiara w siebie",
                        "dobra wiara",
                        "dać wiarę",
                        "w dobrej wierze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Глубокая убежденность в истинности чего-либо или полное доверие кому-либо без доказательств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вера",
                                "target_lang_sense_clarification": "убежденность"
                            },
                            {
                                "target_lang_word": "уверенность",
                                "target_lang_sense_clarification": "твердое убеждение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7861e3d-baed-32af-92c6-059d1107a3c1",
                    "sense_definition": "Grupa ludzi, często żołnierzy lub bliskich kompanów, którzy są ze sobą zżyci.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cała nasza <w>wiara</w> zebrała się wieczorem przy ognisku, żeby wspólnie pośpiewać.",
                            "target_lang_translations": {
                                "ru": "Вся наша <w>братия</w> собралась вечером у костра, чтобы вместе попеть."
                            }
                        },
                        {
                            "text": "Dowódca krzyknął głośno: za mną, <w>wiara</w>, musimy zdobyć to wzgórze!",
                            "target_lang_translations": {
                                "ru": "Командир громко крикнул: за мной, <w>братцы</w>, мы должны взять этот холм!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "stara wiara"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in military or camaraderie context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, часто солдат или близких товарищей (разговорное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "братия",
                                "target_lang_sense_clarification": "группа людей, компания"
                            },
                            {
                                "target_lang_word": "ребята",
                                "target_lang_sense_clarification": "обращение к группе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed7e45f3-6c75-3674-b215-0b4eb4e8a3ea",
                    "sense_definition": "Wierność danemu słowu, lojalność, rzetelność lub stałość w uczuciach.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "fidelity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rycerz uroczyście przysiągł królowi, że dochowa mu <w>wiary</w> aż do śmierci.",
                            "target_lang_translations": {
                                "ru": "Рыцарь торжественно поклялся королю, что сохранит ему <w>верность</w> до самой смерти."
                            }
                        },
                        {
                            "text": "Mimo upływu wielu lat, małżonkowie zachowali sobie wzajemną <w>wiarę</w> i szacunek.",
                            "target_lang_translations": {
                                "ru": "Несмотря на то, что прошло много лет, супруги сохранили взаимную <w>верность</w> и уважение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dochować wiary",
                        "złamać wiarę"
                    ],
                    "traits": [

    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "973c30c5-e50d-3857-88da-d5fd2c38f761",
                    "sense_definition": "Женский род, именительный падеж единственного числа притяжательного местоимения <мой>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это <моя> любимая книга, которую я перечитываю каждый год.",
                            "target_lang_translations": {
                                "en": "This is <my> favorite book, which I reread every year."
                            }
                        },
                        {
                            "text": "Где <моя> сумка, я не могу её найти?",
                            "target_lang_translations": {
                                "en": "Where is <my> bag? I can't find it."
                            }
                        }
                    ],
                    "antonyms": [
                        "твоя",
                        "чужая"
                    ],
                    "common_phrases": [
                        "моя семья",
                        "моя мама",
                        "моя вина",
                        "моя страна",
                        "моя очередь"
                    ],
                    "target_lang_definitions": {
                        "en": "Feminine singular form of the possessive pronoun \"my\"."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "my",
                                "target_lang_sense_clarification": "possessive determiner"
                            },
                            {
                                "target_lang_word": "mine",
                                "target_lang_sense_clarification": "predicative possessive"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "39a18c0f-64a1-3e93-8922-1729af7ea4e2",
                    "sense_definition": "Деепричастие настоящего времени от глагола <мыть>; действие по очищению чего-либо водой.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Она стояла у раковины, <моя> посуду после ужина.",
                            "target_lang_translations": {
                                "en": "She stood by the sink, <washing> the dishes after dinner."
                            }
                        },
                        {
                            "text": "Тщательно <моя> руки с мылом, вы защищаете себя от болезней.",
                            "target_lang_translations": {
                                "en": "By thoroughly <washing> your hands with soap, you protect yourself from diseases."
                            }
                        }
                    ],
                    "synonyms": [
                        "очищая",
                        "отмывая"
                    ],
                    "antonyms": [
                        "пачкая",
                        "загрязняя"
                    ],
                    "common_phrases": [
                        "моя руки",
                        "моя посуду",
                        "моя пол",
                        "моя машину"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "gerund / adverbial participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Present adverbial participle of the verb \"to wash\"; while washing."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "washing",
                                "target_lang_sense_clarification": "action of cleaning with water"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "31db29da-eba4-3c0f-ba51-3b7bc9d0f3d8",
                    "sense_definition": "Жена, супруга или возлюбленная.",
                    "learner_level": "B2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Моя> сегодня уехала к родителям, поэтому я дома один.",
                            "target_lang_translations": {
                                "en": "<My wife> went to her parents' today, so I am home alone."
                            }
                        },
                        {
                            "text": "Я должен спросить, что <моя> думает по этому поводу.",
                            "target_lang_translations": {
                                "en": "I have to ask what <my missus> thinks about this."
                            }
                        }
                    ],
                    "synonyms": [
                        "жена",
                        "супруга",
                        "благоверная"
                    ],
                    "antonyms": [
                        "муж"
                    ],
                    "common_phrases": [
                        "моя сказала",
                        "где моя"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "informal way to refer to one's wife"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Colloquial term for one's wife or female partner."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "my wife",
                                "target_lang_sense_clarification": "informal reference"
                            },
                            {
                                "target_lang_word": "the missus",
                                "target_lang_sense_clarification": "slang/colloquial"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "dimensional",
        "multidimensional",
        "dimensionality",
        "dimensionless"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6f8d60e5-7984-3def-b5d4-fb1a00221412",
                    "sense_definition": "A measurement of the size of something in a particular direction, such as length, width, or height.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to measure the exact <w>dimensions</w> of the kitchen before buying new cabinets.",
                            "target_lang_translations": {
                                "ru": "Нам нужно измерить точные <w>размеры</w> кухни перед покупкой новых шкафов."
                            }
                        },
                        {
                            "text": "Objects in the physical world exist in three <w>dimensions</w>.",
                            "target_lang_translations": {
                                "ru": "Объекты в физическом мире существуют в трех <w>измерениях</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "size",
                        "magnitude",
                        "proportion"
                    ],
                    "common_phrases": [
                        "three dimensions",
                        "exact dimensions",
                        "large dimensions"
                    ],
                    "target_lang_definitions": {
                        "ru": "Измерение величины предмета в определенном направлении, например, длины, ширины или высоты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "размер",
                                "target_lang_sense_clarification": "величина предмета"
                            },
                            {
                                "target_lang_word": "габариты",
                                "target_lang_sense_clarification": "внешние очертания, предельные размеры"
                            },
                            {
                                "target_lang_word": "измерение",
                                "target_lang_sense_clarification": "пространственная протяженность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "52424668-8163-358b-b41b-fbc5451eb944",
                    "sense_definition": "A specific feature, part, or aspect of a situation or thing.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her sense of humor adds a new <w>dimension</w> to her personality.",
                            "target_lang_translations": {
                                "ru": "Ее чувство юмора добавляет новую <w>грань</w> ее личности."
                            }
                        },
                        {
                            "text": "The report failed to address the political <w>dimension</w> of the crisis.",
                            "target_lang_translations": {
                                "ru": "В докладе не был затронут политический <w>аспект</w> кризиса."
                            }
                        }
                    ],
                    "synonyms": [
                        "aspect"
                    ],
                    "common_phrases": [
                        "new dimension",
                        "political dimension",
                        "social dimension",
                        "added dimension"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретная особенность, часть или аспект ситуации или вещи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "аспект",
                                "target_lang_sense_clarification": "точка зрения, сторона"
                            },
                            {
                                "target_lang_word": "грань",
                                "target_lang_sense_clarification": "сторона личности или явления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "668bdf35-1f2d-3293-9ee4-fd20bfd69d3a",
                    "sense_definition": "A hypothetical universe or plane of existence that is different from our own, often used in science fiction.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The characters in the movie travel to a parallel <w>dimension</w> where time runs backwards.",
                            "target_lang_translations": {
                                "ru": "Герои фильма путешествуют в параллельное <w>измерение</w>, где время идет вспять."
                            }
                        },
                        {
                            "text": "It felt as if we had stepped into another <w>dimension</w> entirely.",
                            "target_lang_translations": {
                                "ru": "Казалось, будто мы попали в совершенно другое <w>измерение</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "parallel dimension",
                        "another dimension",
                        "fourth dimension"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гипотетическая вселенная или план бытия, отличный от нашего, часто упоминаемый в научной фантастике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "измерение",
                                "target_lang_sense_clarification": "параллельный мир"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b50669a-1a09-3a4e-9e74-c2c10e3ab82a",
                    "sense_definition": "The number of coordinates required to locate a point in a specific space.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "math_physics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In physics, time is often described as the fourth <w>dimension</w>.",
                            "target_lang_translations": {
                                "ru": "В физике время часто описывают как четвертое <w>измерение</w>."
                            }
                        },
                        {
                            "text": "A line has only one <w>dimension</w>, whereas a square has two.",
                            "target_lang_translations": {
                                "ru": "Линия имеет только одно <w>измерение</w>, тогда как квадрат — два."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество координат, необходимых для определения положения точки в пространстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "измерение",
                                "target_lang_sense_clarification": "координата пространства"
                            },
                            {
                                "target_lang_word": "размерность",
                                "target_lang_sense_clarification": "свойство пространства"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2b1b0045-97b8-386d-8b33-212e03df2300",
                    "sense_definition": "To cut, shape, or mark something according to specific measurements.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The architect carefully <w>dimensioned</w> the floor plan to ensure accuracy.",
                            "target_lang_translations": {
                                "ru": "Архитектор тщательно <w>проставил размеры</w> на плане этажа, чтобы обеспечить точность."
                            }
                        },
                        {
                            "text": "Each part must be properly <w>dimensioned</w> before the machine can be assembled.",
                            "target_lang_translations": {
                                "ru": "Каждой детали должны быть правильно <w>заданы размеры</w> перед сборкой машины."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Придавать форму или наносить разметку в соответствии с определенными измерениями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проставлять размеры",
                                "target_lang_sense_clarification": "на чертеже"
                            },
                            {
                                "target_lang_word": "задавать размеры",
                                "target_lang_sense_clarification": "определять величину"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
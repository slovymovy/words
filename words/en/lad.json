    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "15412081-6183-3643-a6f3-647ec596d5cc",
                    "sense_definition": "A boy or young man.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "human_male",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The young <w>lad</w> helped his father fix the car in the driveway.",
                            "target_lang_translations": {
                                "ru": "Молодой <w>паренёк</w> помог отцу починить машину на подъездной дорожке."
                            }
                        },
                        {
                            "text": "She saw a <w>lad</w> running down the street with a football under his arm.",
                            "target_lang_translations": {
                                "ru": "Она увидела <w>парня</w>, бегущего по улице с футбольным мячом под мышкой."
                            }
                        }
                    ],
                    "synonyms": [
                        "stable boy"
                    ],
                    "antonyms": [
                        "lass"
                    ],
                    "common_phrases": [
                        "young lad",
                        "nice lad"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in British and Irish English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мальчик или молодой человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "парень",
                                "target_lang_sense_clarification": "разговорное, общее значение"
                            },
                            {
                                "target_lang_word": "мальчик",
                                "target_lang_sense_clarification": "ребенок или подросток"
                            },
                            {
                                "target_lang_word": "юноша",
                                "target_lang_sense_clarification": "более формальное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c81b5950-9aac-379a-ab47-ff0e34ee4314",
                    "sense_definition": "A familiar term of address for a young man.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "human_male",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Come here, <w>lad</w>, and help me move these heavy boxes.",
                            "target_lang_translations": {
                                "ru": "Иди сюда, <w>парень</w>, и помоги мне перенести эти тяжелые коробки."
                            }
                        },
                        {
                            "text": "Don't worry about it, <w>lad</w>, you did your best today.",
                            "target_lang_translations": {
                                "ru": "Не волнуйся об этом, <w>сынок</w>, ты сегодня сделал всё, что мог."
                            }
                        }
                    ],
                    "common_phrases": [
                        "listen lad",
                        "alright lad"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used in Northern England, Scotland, and Ireland"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамильярное обращение к молодому человеку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "парень",
                                "target_lang_sense_clarification": "обращение"
                            },
                            {
                                "target_lang_word": "сынок",
                                "target_lang_sense_clarification": "обращение старшего к младшему"
                            },
                            {
                                "target_lang_word": "дружок",
                                "target_lang_sense_clarification": "фамильярное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "82ed9bd0-fa94-3340-9aaf-ac6335c84158",
                    "sense_definition": "A young man who behaves in a boisterous or irresponsible way, often enjoying drinking and male company.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "human_male",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He reckons he is a bit of a <w>lad</w> because he stays out late every weekend.",
                            "target_lang_translations": {
                                "ru": "Он считает себя немного <w>гулякой</w>, потому что гуляет допоздна каждые выходные."
                            }
                        },
                        {
                            "text": "I am going out for a few drinks with the <w>lads</w> tonight.",
                            "target_lang_translations": {
                                "ru": "Я иду выпить с <w>пацанами</w> сегодня вечером."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bit of a lad",
                        "one of the lads",
                        "night out with the lads"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "associated with 'lad culture'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Молодой человек, ведущий шумный или безответственный образ жизни, часто любящий выпивку и мужскую компанию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пацан",
                                "target_lang_sense_clarification": "в контексте мужской компании"
                            },
                            {
                                "target_lang_word": "гуляка",
                                "target_lang_sense_clarification": "любитель развлечений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe8ccad6-b103-36c8-83a6-ed0b15309ad6",
                    "sense_definition": "A groom who works with horses.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "job",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The stable <w>lad</w> woke up early to feed and brush the horses.",
                            "target_lang_translations": {
                                "ru": "<w>Конюх</w> проснулся рано, чтобы покормить и почистить лошадей."
                            }
                        },
                        {
                            "text": "He worked as a <w>lad</w> in the racing stables for over five years.",
                            "target_lang_translations": {
                                "ru": "Он проработал <w>помощником конюха</w> в скаковых конюшнях более пяти лет."
                            }
                        }
                    ],
                    "synonyms": [
                        "stable boy",
                        "stable lad"
                    ],
                    "common_phrases": [
                        "stable lad",
                        "racing lad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конюх, работающий с лошадьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конюх",

                    "frequency": "High",
                    "semantic_group_id": "posture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я очень устал и решил <сесть> на скамейку в парке.",
                            "target_lang_translations": {
                                "en": "I was very tired and decided <to sit down> on a bench in the park."
                            }
                        },
                        {
                            "text": "Учитель попросил всех учеников <сесть> на свои места.",
                            "target_lang_translations": {
                                "en": "The teacher asked all the students <to take> their seats."
                            }
                        }
                    ],
                    "synonyms": [
                        "усесться"
                    ],
                    "antonyms": [
                        "встать"
                    ],
                    "common_phrases": [
                        "сесть на стул",
                        "сесть за стол",
                        "сесть рядом"
                    ],
                    "target_lang_definitions": {
                        "en": "To take a sitting position, bending the legs and lowering oneself onto something (a chair, the ground, etc.)."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8735fb89-5e98-3cc8-bfcf-8a0d2dceca03",
                    "sense_definition": "Специалист, который занимается изготовлением копий документов, чертежей или рукописей.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "professional_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В проектном бюро опытный <w>копировщик</w> быстро подготовил дубликаты всех чертежей.",
                            "target_lang_translations": {
                                "en": "In the design bureau, an experienced <w>tracer</w> quickly prepared duplicates of all the drawings."
                            }
                        },
                        {
                            "text": "Раньше <w>копировщик</w> был востребованной профессией, но сейчас его заменила техника.",
                            "target_lang_translations": {
                                "en": "Previously, a <w>copyist</w> was a sought-after profession, but now technology has replaced him."
                            }
                        }
                    ],
                    "synonyms": [
                        "копиист"
                    ],
                    "common_phrases": [
                        "работа копировщика",
                        "технический копировщик"
                    ],
                    "target_lang_definitions": {
                        "en": "A specialist who makes copies of documents, technical drawings, or manuscripts."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "copyist",
                                "target_lang_sense_clarification": "general term for documents or manuscripts"
                            },
                            {
                                "target_lang_word": "tracer",
                                "target_lang_sense_clarification": "specifically for technical drawings"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc5dc663-20a6-358b-9e74-9c4053ce5ce6",
                    "sense_definition": "Художник, создающий копии картин других мастеров.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "professional_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В музее работал талантливый <w>копировщик</w>, чьи репродукции было трудно отличить от оригиналов.",
                            "target_lang_translations": {
                                "en": "A talented <w>copyist</w> worked at the museum, whose reproductions were difficult to distinguish from the originals."
                            }
                        },
                        {
                            "text": "Молодой <w>копировщик</w> учился технике живописи, повторяя мазки великих художников.",
                            "target_lang_translations": {
                                "en": "The young <w>copyist</w> learned painting techniques by repeating the brushstrokes of great artists."
                            }
                        }
                    ],
                    "synonyms": [
                        "копиист"
                    ],
                    "common_phrases": [
                        "искусный копировщик",
                        "копировщик картин"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "standard term is 'копиист'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An artist who creates copies of paintings by other masters."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "copyist",
                                "target_lang_sense_clarification": "artistic context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "846ccb33-28d0-3ab5-b683-18782b3bc430",
                    "sense_definition": "Человек, который не создает ничего нового, а лишь слепо подражает кому-либо или повторяет чужие идеи.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он не настоящий писатель, а простой <w>копировщик</w> чужих сюжетов и стилей.",
                            "target_lang_translations": {
                                "en": "He is not a real writer, but a mere <w>imitator</w> of other people's plots and styles."
                            }
                        },
                        {
                            "text": "Чтобы стать мастером, нужно перестать быть просто <w>копировщиком</w> и найти свой путь.",
                            "target_lang_translations": {
                                "en": "To become a master, one must stop being just a <w>copycat</w> and find one's own path."
                            }
                        }
                    ],
                    "synonyms": [
                        "подражатель",
                        "имитатор"
                    ],
                    "antonyms": [
                        "творец",
                        "автор",
                        "новатор"
                    ],
                    "common_phrases": [
                        "бездарный копировщик",
                        "слепой копировщик"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who does not create anything new, but merely blindly imitates someone or repeats others' ideas."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "copycat",
                                "target_lang_sense_clarification": "informal, negative"
                            },
                            {
                                "target_lang_word": "imitator",
                                "target_lang_sense_clarification": "general term"
                            }
                        ]
                    }
                }
            ]
        }
    ]

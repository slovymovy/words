    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bee2126f-bf71-3ed5-9249-9b96a45c0b94",
                    "sense_definition": "Urządzenie służące do pieczenia potraw na ruszcie, zazwyczaj przy użyciu węgla drzewnego, gazu lub prądu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłem nowy <w>grill</w> gazowy, aby szybciej przygotowywać obiady w ogrodzie.",
                            "target_lang_translations": {
                                "ru": "Я купил новый газовый <w>гриль</w>, чтобы быстрее готовить обеды в саду."
                            }
                        },
                        {
                            "text": "Po każdym użyciu należy dokładnie wyczyścić <w>grill</w> z tłuszczu i resztek jedzenia.",
                            "target_lang_translations": {
                                "ru": "После каждого использования нужно тщательно очищать <w>гриль</w> от жира и остатков еды."
                            }
                        }
                    ],
                    "synonyms": [
                        "ruszt"
                    ],
                    "common_phrases": [
                        "grill węglowy",
                        "grill gazowy",
                        "grill elektryczny",
                        "rozpalić grilla"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство для жарки пищи на решетке, обычно с использованием угля, газа или электричества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гриль",
                                "target_lang_sense_clarification": "устройство"
                            },
                            {
                                "target_lang_word": "мангал",
                                "target_lang_sense_clarification": "жаровня для углей"
                            },
                            {
                                "target_lang_word": "барбекю",
                                "target_lang_sense_clarification": "печь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b80c2e3b-1027-3d20-a1a6-5cc72d591865",
                    "sense_definition": "Spotkanie towarzyskie na świeżym powietrzu, podczas którego przygotowuje się i spożywa jedzenie pieczone na ruszcie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W przyszłą sobotę moi sąsiedzi organizują <w>grilla</w> dla wszystkich mieszkańców ulicy.",
                            "target_lang_translations": {
                                "ru": "В следующую субботу мои соседи устраивают <w>барбекю</w> для всех жителей улицы."
                            }
                        },
                        {
                            "text": "Czy masz ochotę wpaść do nas na <w>grilla</w> w ten weekend?",
                            "target_lang_translations": {
                                "ru": "Хочешь заглянуть к нам на <w>шашлыки</w> в эти выходные?"
                            }
                        }
                    ],
                    "synonyms": [
                        "barbecue"
                    ],
                    "common_phrases": [
                        "robić grilla",
                        "zaprosić na grilla",
                        "sezon na grilla"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дружеская встреча на свежем воздухе, во время которой готовят и едят пищу, жаренную на решетке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шашлыки",
                                "target_lang_sense_clarification": "мероприятие"
                            },
                            {
                                "target_lang_word": "барбекю",
                                "target_lang_sense_clarification": "вечеринка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "31c0a949-9c89-3881-b197-9317c79eab0e",
                    "sense_definition": "Przednia część nadwozia samochodu osłaniająca chłodnicę, często pełniąca funkcję ozdobną.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "car_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten model samochodu wyróżnia się potężnym, chromowanym <w>grillem</w>.",
                            "target_lang_translations": {
                                "ru": "Эта модель автомобиля выделяется мощной хромированной <w>решёткой радиатора</w>."
                            }
                        },
                        {
                            "text": "W wyniku stłuczki uszkodzeniu uległ przedni zderzak oraz <w>grill</w>.",
                            "target_lang_translations": {
                                "ru": "В результате аварии были повреждены передний бампер и <w>решётка радиатора</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chromowany grill",
                        "przedni grill"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передняя часть кузова автомобиля, закрывающая радиатор, часто выполняющая декоративную функцию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "решётка радиатора",
                                "target_lang_sense_clarification": "деталь автомобиля"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70c1e3de-25e9-30ee-9353-0539637ab324",
                    "sense_definition": "Ozdobna nakładka na zęby, często wykonana z metali szlachetnych i wysadzana kamieniami, popularna w kulturze hip-hopowej.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "jewelry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Znany raper uśmiechnął się szeroko, prezentując swój diamentowy <w>grill</w>.",
                            "target_lang_translations": {
                                "ru": "Известный рэпер широко улыбнулся, демонстрируя свои бриллиантовые <w>гриллз</w>."
                            }
                        },
                        {
                            "text": "Noszenie złotego <w>grilla</w> jest elementem wizerunku niektórych artystów muzycznych.",
                            "target_lang_translations": {
                                "ru": "Ношение золотых <w>гриллз</w> является частью имиджа некоторых музыкальных исполнителей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "złoty grill"
                    ],
                    "traits": [
                        {

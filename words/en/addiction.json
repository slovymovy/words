    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f1e8a2d7-55f9-3ba5-ad70-607bc4d8ddc4",
                    "sense_definition": "A condition in which a person is unable to stop using a substance or engaging in a behavior, usually causing harm.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "dependency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is struggling to overcome his <w>addiction</w> to alcohol.",
                            "target_lang_translations": {
                                "ru": "Он пытается побороть свою <w>зависимость</w> от алкоголя."
                            }
                        },
                        {
                            "text": "Gambling <w>addiction</w> can lead to severe financial problems for the whole family.",
                            "target_lang_translations": {
                                "ru": "<w>Зависимость</w> от азартных игр может привести к серьезным финансовым проблемам для всей семьи."
                            }
                        }
                    ],
                    "synonyms": [
                        "dependence",
                        "dependency",
                        "habit"
                    ],
                    "common_phrases": [
                        "drug addiction",
                        "addiction to",
                        "overcome an addiction",
                        "treat addiction"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние, при котором человек не может прекратить употребление вещества или определенное поведение, что обычно причиняет вред."
                    },
                    "translations": {
                        "ru": [
                            {

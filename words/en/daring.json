                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смелость",
                                "target_lang_sense_clarification": "способность преодолевать страх"
                            },
                            {
                                "target_lang_word": "отвага",
                                "target_lang_sense_clarification": "бесстрашие, храбрость высокого стиля"
                            },
                            {

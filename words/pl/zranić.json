{
    "word_family": [
        "ranić",
        "zranienie",
        "rana"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "42780803-b76e-33d3-97a5-7a0f0ef0d7f8",
                    "sense_definition": "Spowodować u siebie obrażenia ciała; zrobić sobie krzywdę.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "group_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wczoraj przypadkowo <zraniłem> się nożem podczas krojenia chleba.",
                            "target_lang_translations": {
                                "ru": "Вчера я случайно <поранился> ножом, когда резал хлеб."
                            }
                        },
                        {
                            "text": "Uważaj na rozbite szkło, żebyś nie <zranił> się w stopę.",
                            "target_lang_translations": {
                                "ru": "Осторожно с разбитым стеклом, чтобы не <поранить> ногу."
                            }
                        }
                    ],
                    "synonyms": [
                        "skaleczyć się"
                    ],
                    "common_phrases": [
                        "zranić się w palec",
                        "zranić się w rękę",
                        "lekko się zranić",
                        "poważnie się zranić"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem zwrotnym 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причинить себе физическую боль или травму; пораниться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пораниться",
                                "target_lang_sense_clarification": "получить травму"
                            },
                            {
                                "target_lang_word": "порезаться",
                                "target_lang_sense_clarification": "острым предметом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7dba768a-e73d-3a4e-8e9d-0e9cf0691674",
                    "sense_definition": "Spowodować uszkodzenie ciała u kogoś innego; zadać komuś ranę.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Napastnik <zranił> ofiarę ostrym narzędziem i uciekł z miejsca zdarzenia.",
                            "target_lang_translations": {
                                "ru": "Нападавший <ранил> жертву острым предметом и скрылся с места происшествия."
                            }
                        },
                        {
                            "text": "Możesz kogoś <zranić>, jeśli będziesz nieostrożnie rzucać kamieniami.",
                            "target_lang_translations": {
                                "ru": "Ты можешь кого-нибудь <ранить>, если будешь неосторожно бросать камни."
                            }
                        }
                    ],
                    "synonyms": [
                        "skaleczyć"
                    ],
                    "common_phrases": [
                        "zranić kogoś nożem",
                        "ciężko zranić",
                        "śmiertelnie zranić",
                        "zranić w nogę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нанести кому-либо рану или телесное повреждение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ранить",
                                "target_lang_sense_clarification": "нанести рану"
                            },
                            {
                                "target_lang_word": "поранить",
                                "target_lang_sense_clarification": "повредить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "655d8dd1-d6b3-391c-a536-f8f63ca9efcb",
                    "sense_definition": "Sprawić komuś ból psychiczny, smutek lub przykrość swoim zachowaniem lub słowami.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_emotional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziewczyna bardzo mnie <zraniła>, mówiąc, że już mnie nie kocha.",
                            "target_lang_translations": {
                                "ru": "Девушка очень меня <ранила>, сказав, что больше меня не любит."
                            }
                        },
                        {
                            "text": "Nie chciałem <zranić> twoich uczuć moją szczerą opinią.",
                            "target_lang_translations": {
                                "ru": "Я не хотел <задеть> твои чувства своим искренним мнением."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zranić czyjeś uczucia",
                        "zranić do żywego",
                        "zranić czyjąś dumę",
                        "głęboko zranić"
                    ],
                    "target_lang_definitions": {
                        "ru": "Причинить кому-либо душевную боль, страдание или обиду."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ранить",
                                "target_lang_sense_clarification": "причинить душевную боль"
                            },
                            {
                                "target_lang_word": "задеть",
                                "target_lang_sense_clarification": "чувства или самолюбие"
                            },
                            {
                                "target_lang_word": "обидеть",
                                "target_lang_sense_clarification": "расстроить"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
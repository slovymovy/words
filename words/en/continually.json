            "senses": [
                {
                    "sense_id": "33975b2c-2e7b-392c-8c59-86103e58021b",
                    "sense_definition": "In regular or repeated succession; very often.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "frequency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company is <w>continually</w> looking for ways to improve its products.",
                            "target_lang_translations": {
                                "ru": "Компания <w>постоянно</w> ищет способы улучшить свою продукцию."
                            }
                        },
                        {
                            "text": "He <w>continually</w> interrupts the teacher with unrelated questions.",
                            "target_lang_translations": {
                                "ru": "Он <w>то и дело</w> перебивает учителя вопросами не по теме."
                            }
                        }
                    ],
                    "synonyms": [
                        "time and again",
                        "at every turn"
                    ],
                    "common_phrases": [
                        "continually changing",
                        "continually evolving",
                        "continually growing"
                    ],
                    "target_lang_definitions": {
                        "ru": "Регулярно или повторяясь; очень часто."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постоянно",
                                "target_lang_sense_clarification": "все время, регулярно"
                            },
                            {
                                "target_lang_word": "то и дело",
                                "target_lang_sense_clarification": "повторяясь снова и снова"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7de92ee5-213c-348d-b698-bf32aa862955",
                    "sense_definition": "In a continuous manner; non-stop.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It rained <w>continually</w> for three days, flooding the streets.",
                            "target_lang_translations": {
                                "ru": "Дождь шёл <w>непрерывно</w> в течение трёх дней, затопив улицы."
                            }
                        },
                        {
                            "text": "The old machine runs <w>continually</w> in the background.",
                            "target_lang_translations": {
                                "ru": "Старая машина <w>непрерывно</w> работает в фоновом режиме."
                            }
                        }
                    ],
                    "synonyms": [
                        "continuously",
                        "nonstop",
                        "ceaselessly",
                        "incessantly"
                    ],
                    "target_lang_definitions": {

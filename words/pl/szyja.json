    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4ae42587-ff10-33c1-936d-424a39aaf0bf",
                    "sense_definition": "Część ciała łącząca głowę z tułowiem.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Żyrafa jest zwierzęciem, które ma bardzo długą <w>szyję</w>.",
                            "target_lang_translations": {
                                "ru": "Жираф — это животное, у которого очень длинная <w>шея</w>."
                            }
                        },
                        {
                            "text": "Zimą zawsze noszę ciepły szalik na <w>szyi</w>.",
                            "target_lang_translations": {
                                "ru": "Зимой я всегда ношу тёплый шарф на <w>шее</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kark"
                    ],
                    "common_phrases": [
                        "długa szyja",
                        "rzucić się na szyję",
                        "boleć szyja",
                        "szalik na szyi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть тела, соединяющая голову с туловищем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шея",
                                "target_lang_sense_clarification": "часть тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2630768-92b1-3322-9c55-c1be51b94888",
                    "sense_definition": "Wąska, górna część butelki lub innego naczynia, prowadząca do otworu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "object_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wino powoli wypływało przez wąską <w>szyję</w> butelki.",
                            "target_lang_translations": {
                                "ru": "Вино медленно вытекало через узкое <w>горлышко</w> бутылки."
                            }
                        },
                        {
                            "text": "Korek utknął głęboko w <w>szyi</w> naczynia.",
                            "target_lang_translations": {
                                "ru": "Пробка застряла глубоко в <w>горлышке</w> сосуда."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szyja butelki",
                        "wąska szyja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Узкая верхняя часть бутылки или другого сосуда, ведущая к отверстию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горлышко",
                                "target_lang_sense_clarification": "бутылки"
                            },
                            {
                                "target_lang_word": "горло",
                                "target_lang_sense_clarification": "сосуда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b55148d4-50f7-3a51-8167-acee1e9d5a4f",
                    "sense_definition": "Życie lub bezpieczeństwo osobiste (używane w kontekście ryzyka i zagrożenia).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_life",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie warto ryzykować własną <w>szyją</w> dla tak małych pieniędzy.",
                            "target_lang_translations": {
                                "ru": "Не стоит рисковать собственной <w>головой</w> ради таких маленьких денег."
                            }
                        },
                        {
                            "text": "To jest niebezpieczna sprawa, która może kosztować cię <w>szyję</w>.",
                            "target_lang_translations": {
                                "ru": "Это опасное дело, которое может стоить тебе <w>головы</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "życie"
                    ],
                    "common_phrases": [
                        "dać szyję",
                        "nadstawiać szyi",
                        "ryzykować szyją"

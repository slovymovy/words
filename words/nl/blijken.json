                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проявления",
                                "target_lang_sense_clarification": "выражения чувств"
                            },
                            {
                                "target_lang_word": "знаки",
                                "target_lang_sense_clarification": "символические действия"
                            },
                            {
                                "target_lang_word": "свидетельства",
                                "target_lang_sense_clarification": "доказательства"
                            }
                        ]
                    }
                }
            ]
        }

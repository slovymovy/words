            "senses": [
                {
                    "sense_id": "c41c296c-0771-3784-bb0e-9e1bc69b567d",
                    "sense_definition": "Męskie imię pochodzenia semickiego.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mój dziadek <w>Abraham</w> był bardzo mądrym i szanowanym człowiekiem.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка <w>Авраам</w> был очень мудрым и уважаемым человеком."
                            }
                        },
                        {
                            "text": "W naszej klasie uczy się nowy chłopiec o imieniu <w>Abraham</w>.",
                            "target_lang_translations": {
                                "ru": "В нашем классе учится новый мальчик по имени <w>Авраам</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Abrahamek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя семитского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Авраам",
                                "target_lang_sense_clarification": "мужское имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11c17b2d-d1ea-32b4-8661-fabff4a5ddda",
                    "sense_definition": "Postać biblijna, patriarcha czczony w judaizmie, chrześcijaństwie i islamie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Bóg zawarł przymierze z <w>Abrahamem</w>, obiecując mu liczne potomstwo.",
                            "target_lang_translations": {
                                "ru": "Бог заключил завет с <w>Авраамом</w>, пообещав ему многочисленное потомство."
                            }
                        },
                        {
                            "text": "Historia ofiarowania Izaaka przez <w>Abrahama</w> jest znana na całym świecie.",
                            "target_lang_translations": {
                                "ru": "История жертвоприношения Исаака <w>Авраамом</w> известна во всем мире."
                            }
                        }
                    ],
                    "synonyms": [
                        "Abram"
                    ],
                    "common_phrases": [
                        "łono Abrahama",
                        "przenieść się na łono Abrahama"
                    ],
                    "target_lang_definitions": {
                        "ru": "Библейский персонаж, патриарх, почитаемый в иудаизме, христианстве и исламе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Авраам",
                                "target_lang_sense_clarification": "библейский персонаж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc1f0222-55f4-3a1d-860d-231578e6cfd0",
                    "sense_definition": "Nazwisko męskie lub żeńskie używane w Polsce i na świecie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Abraham</w> został wybrany na nowego przewodniczącego rady osiedla.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Абрахам</w> был избран новым председателем совета жилого комплекса."
                            }
                        },
                        {
                            "text": "W książce telefonicznej widnieje kilku abonentów o nazwisku <w>Abraham</w>.",
                            "target_lang_translations": {
                                "ru": "В телефонной книге значится несколько абонентов с фамилией <w>Абрахам</w>."
                            }
                        }
                    ],
                    "traits": [
                        {

{
    "word_family": [
        "ekspresowy",
        "ekspresowo"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8a9f8f85-7a73-3182-8e86-6b5bca44b506",
                    "sense_definition": "Urządzenie elektryczne służące do parzenia kawy, często pod ciśnieniem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "coffee_machine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rano zawsze piję świeżą kawę z <w>ekspresu</w>.",
                            "target_lang_translations": {
                                "ru": "По утрам я всегда пью свежий кофе из <w>кофемашины</w>."
                            }
                        },
                        {
                            "text": "Nasz stary <w>ekspres</w> zepsuł się wczoraj i musimy kupić nowy.",
                            "target_lang_translations": {
                                "ru": "Наша старая <w>кофеварка</w> сломалась вчера, и нам нужно купить новую."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kawa z ekspresu",
                        "ekspres ciśnieniowy",
                        "ekspres przelewowy",
                        "ekspres do kawy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Электрический прибор для приготовления кофе, часто под давлением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кофемашина",
                                "target_lang_sense_clarification": "автоматическое устройство"
                            },
                            {
                                "target_lang_word": "кофеварка",
                                "target_lang_sense_clarification": "общее название прибора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6deedb92-86b8-3c80-9d67-9688039d0a38",
                    "sense_definition": "Szybszy pociąg osobowy o wysokim standardzie, zatrzymujący się tylko w dużych miastach.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy się pospieszyć, żeby zdążyć na <w>ekspres</w> do Warszawy.",
                            "target_lang_translations": {
                                "ru": "Мы должны поторопиться, чтобы успеть на <w>экспресс</w> до Варшавы."
                            }
                        },
                        {
                            "text": "Ten <w>ekspres</w> jedzie bezpośrednio do Krakowa bez przystanków.",
                            "target_lang_translations": {
                                "ru": "Этот <w>экспресс</w> идет прямо до Кракова без остановок."
                            }
                        }
                    ],
                    "synonyms": [
                        "pociąg ekspresowy",
                        "ekspresiak"
                    ],
                    "common_phrases": [
                        "wsiąść w ekspres",
                        "jechać ekspresem",
                        "międzynarodowy ekspres"
                    ],
                    "target_lang_definitions": {
                        "ru": "Скоростной пассажирский поезд высокого класса, останавливающийся только в крупных городах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экспресс",
                                "target_lang_sense_clarification": "поезд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1180fc67-b3f0-3902-8f22-6434c7144fc9",
                    "sense_definition": "Zamek błyskawiczny; rodzaj zapięcia składającego się z dwóch rzędów ząbków, używanego w odzieży lub torbach.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "clothing_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zaciął mi się <w>ekspres</w> w kurtce zimowej i nie mogę jej zapiąć.",
                            "target_lang_translations": {
                                "ru": "У меня заела <w>молния</w> на зимней куртке, и я не могу её застегнуть."
                            }
                        },
                        {
                            "text": "Krawcowa wymieniła zepsuty <w>ekspres</w> w moich spodniach.",
                            "target_lang_translations": {
                                "ru": "Портниха заменила сломанный <w>замок</w> на моих брюках."
                            }
                        }
                    ],
                    "synonyms": [
                        "zamek błyskawiczny",
                        "suwak",
                        "zamek",
                        "ekler"
                    ],
                    "common_phrases": [
                        "zapiąć ekspres",
                        "wszyć ekspres",
                        "zepsuty ekspres"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken language, though 'zamek' is also frequent"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Застежка-молния; вид застежки, состоящей из двух рядов зубчиков, используемой в одежде или сумках."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молния",
                                "target_lang_sense_clarification": "застежка"
                            },
                            {
                                "target_lang_word": "замок",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f22be39-9e35-31d2-bcf0-77b7bc9a09c2",
                    "sense_definition": "Sposób wykonania usługi znacznie szybciej niż w standardowym trybie, zazwyczaj za dodatkową opłatą.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "service_speed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zamówiłem wywołanie zdjęć w <w>ekspresie</w>, więc będą gotowe za godzinę.",
                            "target_lang_translations": {
                                "ru": "Я заказал проявку фотографий в режиме <w>экспресс</w>, так что они будут готовы через час."
                            }
                        },
                        {
                            "text": "Pralnia oferuje czyszczenie garniturów w <w>ekspresie</w> dla zapracowanych klientów.",
                            "target_lang_translations": {
                                "ru": "Химчистка предлагает чистку костюмов <w>экспрессом</w> для занятых клиентов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "w ekspresie",
                        "usługa w ekspresie",
                        "zrobić coś w ekspresie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способ выполнения услуги значительно быстрее стандартного режима, обычно за дополнительную плату."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экспресс-услуга",
                                "target_lang_sense_clarification": "ускоренный сервис"
                            },
                            {
                                "target_lang_word": "срочное исполнение",
                                "target_lang_sense_clarification": "режим работы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4b9bc97-84d0-3b88-a346-48a11c5029aa",
                    "sense_definition": "Pośpieszna przesyłka pocztowa, którą dostarcza się priorytetowo.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mail",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wyślij te ważne dokumenty <w>ekspresem</w>, żeby dotarły jutro rano.",
                            "target_lang_translations": {
                                "ru": "Отправь эти важные документы <w>экспресс-почтой</w>, чтобы они пришли завтра утром."
                            }
                        },
                        {
                            "text": "Otrzymałem wczoraj <w>ekspres</w> z urzędu, ale nie było mnie w domu.",
                            "target_lang_translations": {
                                "ru": "Я получил вчера <w>экспресс-отправление</w> из ведомства, но меня не было дома."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nadać ekspres",
                        "wysłać ekspresem",
                        "list ekspres"
                    ],
                    "target_lang_definitions": {
                        "ru": "Срочное почтовое отправление, которое доставляется в приоритетном порядке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экспресс-почта",
                                "target_lang_sense_clarification": "служба доставки"
                            },
                            {
                                "target_lang_word": "экспресс-доставка",
                                "target_lang_sense_clarification": "процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a797b852-8104-3536-baa0-04b20976ddce",
                    "sense_definition": "Dwa karabinki połączone krótką, usztywnioną pętlą z taśmy, używane we wspinaczce do asekuracji.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "climbing_gear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wspinacz sprawnie wpiął linę w <w>ekspres</w> wiszący na skale.",
                            "target_lang_translations": {
                                "ru": "Скалолаз ловко вщелкнул веревку в <w>оттяжку</w>, висящую на скале."
                            }
                        },
                        {
                            "text": "Na tę długą drogę wspinaczkową będziesz potrzebować co najmniej dwanaście <w>ekspresów</w>.",
                            "target_lang_translations": {
                                "ru": "Для этого длинного маршрута тебе понадобится как минимум двенадцать <w>оттяжек</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wpiąć ekspres",
                        "zestaw ekspresów"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "jargon used by climbers"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Два карабина, соединенных короткой жесткой петлей из стропы, используемые в скалолазании для страховки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оттяжка",
                                "target_lang_sense_clarification": "альпинизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "409f53fd-677a-30ae-9359-177e9e12bd40",
                    "sense_definition": "Rodzaj broni myśliwskiej; sztucer o dwóch lufach gwintowanych.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "weapon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony myśliwy zabrał na polowanie swój niezawodny <w>ekspres</w>.",
                            "target_lang_translations": {
                                "ru": "Опытный охотник взял на охоту свой надежный <w>штуцер</w>."
                            }
                        },
                        {
                            "text": "Ten zabytkowy <w>ekspres</w> jest cenionym eksponatem w kolekcji broni.",
                            "target_lang_translations": {
                                "ru": "Этот старинный <w>штуцер-экспресс</w> является ценным экспонатом в коллекции оружия."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "hunting jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид охотничьего оружия; штуцер с двумя нарезными стволами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штуцер",
                                "target_lang_sense_clarification": "охотничье ружье"
                            },
                            {
                                "target_lang_word": "экспресс",
                                "target_lang_sense_clarification": "вид штуцера"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
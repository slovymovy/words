            "senses": [
                {
                    "sense_id": "60f7a115-43d1-3633-941d-abc0f849afcc",
                    "sense_definition": "A large medical facility where sick or injured people receive medical or surgical treatment.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "medical_facility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ambulance rushed the patient to the <w>hospital</w>.",
                            "target_lang_translations": {
                                "ru": "Скорая помощь срочно доставила пациента в <w>больницу</w>."
                            }
                        },
                        {
                            "text": "She had to stay in <w>hospital</w> for three days after the surgery.",
                            "target_lang_translations": {
                                "ru": "Ей пришлось остаться в <w>больнице</w> на три дня после операции."
                            }
                        }
                    ],
                    "common_phrases": [
                        "go to hospital",
                        "in hospital",
                        "admitted to hospital",
                        "discharged from hospital"
                    ],
                    "target_lang_definitions": {

                            {
                                "target_lang_word": "curse",
                                "target_lang_sense_clarification": "imperative"
                            },
                            {
                                "target_lang_word": "damn",
                                "target_lang_sense_clarification": "imperative"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "проклинайте",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            },
                            {
                                "target_lang_word": "прокляните",
                                "target_lang_sense_clarification": "повелительное наклонение (совершенный вид)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c53e746e-7146-37d0-9dba-e82619299252",
                    "sense_definition": "Значительное достижение, резкий скачок вперёд в развитии науки, технологии или других областей.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "metaphorical_breakthrough",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот научный <w>прорыв</w> позволит лечить ранее неизлечимые болезни.",
                            "target_lang_translations": {
                                "en": "This scientific <w>breakthrough</w> will allow treating previously incurable diseases."
                            }
                        },
                        {
                            "text": "Настоящий <w>прорыв</w> в переговорах случился только поздно вечером.",
                            "target_lang_translations": {
                                "en": "A real <w>breakthrough</w> in negotiations happened only late in the evening."
                            }
                        }
                    ],
                    "synonyms": [
                        "провал"
                    ],
                    "antonyms": [
                        "успех"
                    ],
                    "common_phrases": [
                        "технологический прорыв",
                        "настоящий прорыв",
                        "совершить прорыв",
                        "научный прорыв"
                    ],
                    "target_lang_definitions": {
                        "en": "A significant achievement or sharp leap forward in the development of science, technology, or other areas."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "breakthrough",
                                "target_lang_sense_clarification": "major achievement or discovery"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1640e51-d147-3db2-9b11-18528f2e55eb",
                    "sense_definition": "Нарушение целостности чего-либо (трубы, плотины, ограждения) под давлением изнутри или снаружи.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_break",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за сильных морозов в подвале произошёл <w>прорыв</w> трубы с горячей водой.",
                            "target_lang_translations": {
                                "en": "Due to severe frosts, a hot water pipe <w>burst</w> occurred in the basement."
                            }
                        },
                        {
                            "text": "Спасатели срочно эвакуировали жителей, опасаясь, что случится <w>прорыв</w> плотины.",
                            "target_lang_translations": {
                                "en": "Rescuers urgently evacuated residents, fearing that a dam <w>breach</w> would occur."
                            }
                        }
                    ],
                    "common_phrases": [
                        "прорыв трубы",
                        "прорыв плотины",
                        "прорыв воды",
                        "ликвидировать прорыв"
                    ],
                    "target_lang_definitions": {
                        "en": "A violation of the integrity of something (pipe, dam, enclosure) under pressure from inside or outside."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rupture",
                                "target_lang_sense_clarification": "breaking of a pipe or container"
                            },
                            {
                                "target_lang_word": "burst",
                                "target_lang_sense_clarification": "sudden breaking open"
                            },
                            {
                                "target_lang_word": "breach",
                                "target_lang_sense_clarification": "gap in a dam or barrier"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3745e5bb-272f-3ab1-a803-44d8edcbc356",
                    "sense_definition": "Воен. Место, где линия фронта или оборона противника была прорвана атакующими войсками.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military_breach",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Командир приказал направить резервные танки в <w>прорыв</w>, чтобы развить наступление.",
                            "target_lang_translations": {
                                "en": "The commander ordered reserve tanks to be sent into the <w>breach</w> to develop the offensive."
                            }
                        },
                        {
                            "text": "Пехота устремилась в <w>прорыв</w>, стараясь закрепиться на новых позициях до подхода врага.",
                            "target_lang_translations": {
                                "en": "The infantry rushed into the <w>breakthrough</w>, trying to secure new positions before the enemy arrived."
                            }
                        }
                    ],
                    "common_phrases": [
                        "идти в прорыв",
                        "бросить в прорыв",
                        "закрыть прорыв"
                    ],
                    "target_lang_definitions": {
                        "en": "Military. A place where the front line or enemy defense has been broken by attacking forces."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "breach",
                                "target_lang_sense_clarification": "gap in defense lines"
                            },
                            {
                                "target_lang_word": "breakthrough",
                                "target_lang_sense_clarification": "military advance through enemy lines"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e824960a-df0c-3eee-a9f3-e1c231e64859",
                    "sense_definition": "Нарушение хода работы, ведущее к невыполнению плана или задания (обычно в советском контексте).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "work_disruption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Директор завода собрал совещание, чтобы обсудить серьёзный <w>прорыв</w> в графике поставок.",
                            "target_lang_translations": {
                                "en": "The factory director called a meeting to discuss a serious <w>disruption</w> in the delivery schedule."
                            }
                        },
                        {
                            "text": "Бригада работала в две смены, чтобы устранить <w>прорыв</w> и сдать объект вовремя.",
                            "target_lang_translations": {
                                "en": "The brigade worked in two shifts to eliminate the <w>lag</w> and hand over the object on time."
                            }
                        }
                    ],
                    "synonyms": [
                        "провал"
                    ],
                    "antonyms": [
                        "успех"
                    ],
                    "common_phrases": [
                        "ликвидация прорыва",
                        "вывести из прорыва"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "советский контекст"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A disruption in the workflow leading to non-fulfillment of a plan or task (usually in a Soviet context)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "disruption",
                                "target_lang_sense_clarification": "interruption of process"
                            },
                            {
                                "target_lang_word": "breakdown",
                                "target_lang_sense_clarification": "failure in a system or plan"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "98f86251-dea8-37e6-aec7-b668c38954e1",
                    "sense_definition": "Деепричастие прошедшего времени от глагола <w>прорыть</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Прорыв</w> глубокую траншею, строители начали укладывать кабель.",
                            "target_lang_translations": {
                                "en": "<w>Having dug</w> a deep trench, the builders started laying the cable."
                            }
                        },
                        {
                            "text": "Крот, <w>прорыв</w> ход под забором, выбрался на соседний участок.",
                            "target_lang_translations": {
                                "en": "The mole, <w>having dug</w> a passage under the fence, got out onto the neighboring plot."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "деепричастие"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Past adverbial participle of the verb 'to dig through'."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "having dug",
                                "target_lang_sense_clarification": "completed action of digging"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
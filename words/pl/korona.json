    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b8252f83-85c1-3ca0-92d2-c3c1b8e62005",
                    "sense_definition": "Ozdobne nakrycie głowy w kształcie obręczy, wykonane ze szlachetnych metali i kamieni, będące symbolem władzy królewskiej lub cesarskiej.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "symbol_wladzy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Król uroczyście włożył na głowę złotą <koronę>.",
                            "target_lang_translations": {
                                "ru": "Король торжественно надел на голову золотую <w>корону</w>."
                            }
                        },
                        {
                            "text": "W muzeum podziwialiśmy insygnia władzy, w tym piękną <koronę>.",
                            "target_lang_translations": {
                                "ru": "В музее мы любовались знаками власти, в том числе красивой <w>короной</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "złota korona",
                        "nosić koronę",
                        "korona cierniowa",
                        "włożyć koronę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративный головной убор в форме обруча из драгоценных металлов, символ королевской власти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корона",
                                "target_lang_sense_clarification": "головной убор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a50b25bf-980f-3bc7-8f51-2c8fa41a2f42",
                    "sense_definition": "Jednostka monetarna używana w niektórych krajach, na przykład w Czechach, Islandii oraz państwach skandynawskich.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "waluta",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed wyjazdem do Pragi muszę kupić czeskie <korony>.",
                            "target_lang_translations": {
                                "ru": "Перед поездкой в Прагу мне нужно купить чешские <w>кроны</w>."
                            }
                        },
                        {
                            "text": "Za ten obiad zapłaciliśmy trzysta <koron>.",
                            "target_lang_translations": {
                                "ru": "За этот обед мы заплатили триста <w>крон</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "CZK",
                        "Kč"
                    ],
                    "common_phrases": [
                        "korona czeska",
                        "korona duńska",
                        "kurs korony",
                        "wymienić korony"
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежная единица, используемая в некоторых странах (например, в Чехии, Исландии, Скандинавии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крона",
                                "target_lang_sense_clarification": "валюта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f1891b99-251f-33a8-a063-98648e0a89d8",
                    "sense_definition": "Potoczne określenie wirusa SARS-CoV-2 lub wywoływanej przez niego choroby COVID-19.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "choroba",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad zachorował na <koronę> w zeszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "Мой сосед заболел <w>короной</w> на прошлой неделе."
                            }
                        },
                        {
                            "text": "Wszyscy nosili maseczki z obawy przed <koroną>.",
                            "target_lang_translations": {
                                "ru": "Все носили маски из страха перед <w>короной</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "koronawirus"
                    ],
                    "common_phrases": [
                        "mieć koronę",
                        "bać się korony",
                        "szczepionka na koronę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название вируса SARS-CoV-2 или вызываемой им болезни COVID-19."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корона",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f1faac1-f0ec-3327-bc78-21eb7914e9dc",
                    "sense_definition": "Górna, rozgałęziona część drzewa wraz z liśćmi, znajdująca się nad pniem.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "roslina",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ptaki zbudowały gniazdo w gęstej <koronie> dębu.",
                            "target_lang_translations": {
                                "ru": "Птицы свили гнездо в густой <w>кроне</w> дуба."
                            }
                        },
                        {
                            "text": "Ogrodnik musiał przyciąć <koronę> jabłoni, aby drzewo lepiej owocowało.",
                            "target_lang_translations": {
                                "ru": "Садовник должен был подрезать <w>крону</w> яблони, чтобы дерево лучше плодоносило."
                            }
                        }
                    ],
                    "common_phrases": [
                        "korona drzewa",
                        "rozłożysta korona",
                        "przyciąć koronę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхняя, разветвленная часть дерева вместе с листвой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крона",
                                "target_lang_sense_clarification": "дерева"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dbd83ab3-3ff8-3939-bf96-beb29f35021d",
                    "sense_definition": "Sztuczne uzupełnienie zęba, nakładane na jego zniszczoną część w celu odbudowy kształtu i funkcji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "stomatologia",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dentysta zaproponował mi założenie porcelanowej <korony> na złamany ząb.",
                            "target_lang_translations": {
                                "ru": "Стоматолог предложил мне поставить фарфоровую <w>коронку</w> на сломанный зуб."
                            }
                        },
                        {
                            "text": "Nowa <korona> wygląda bardzo naturalnie i nie różni się od innych zębów.",
                            "target_lang_translations": {
                                "ru": "Новая <w>коронка</w> выглядит очень естественно и не отличается от других зубов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "założyć koronę",
                        "korona porcelanowa",
                        "korona na zębie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искусственный протез, надеваемый на разрушенную часть зуба для восстановления его формы и функции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коронка",
                                "target_lang_sense_clarification": "зубная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "729c38f0-556e-3ff5-9ccd-c22074e03d8b",
                    "sense_definition": "Instytucja władzy królewskiej lub państwo rządzone przez monarchę.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "polityka",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy urzędnicy musieli złożyć przysięgę wierności <koronie>.",
                            "target_lang_translations": {
                                "ru": "Все чиновники должны были принести присягу на верность <w>короне</w>."
                            }
                        },
                        {
                            "text": "To terytorium należy bezpośrednio do <korony>.",
                            "target_lang_translations": {
                                "ru": "Эта территория принадлежит непосредственно <w>короне</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "monarchia"
                    ],
                    "common_phrases": [
                        "służyć koronie",
                        "w imieniu korony",
                        "wierny koronie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Институт королевской власти или государство, управляемое монархом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корона",
                                "target_lang_sense_clarification": "власть/монархия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af7191b9-8417-30f2-af93-f3e0d0c1054e",
                    "sense_definition": "Zewnętrzna, bardzo gorąca warstwa atmosfery Słońca lub innej gwiazdy, najlepiej widoczna podczas zaćmienia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "astronomia",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas całkowitego zaćmienia Słońca mogliśmy obserwować jego <koronę>.",
                            "target_lang_translations": {
                                "ru": "Во время полного солнечного затмения мы могли наблюдать его <w>корону</w>."
                            }
                        },
                        {
                            "text": "Naukowcy badają temperaturę <korony> słonecznej.",
                            "target_lang_translations": {
                                "ru": "Ученые изучают температуру солнечной <w>короны</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "korona słoneczna",
                        "korona gwiazdy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "scientific"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Внешний, очень горячий слой атмосферы Солнца или другой звезды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "корона",
                                "target_lang_sense_clarification": "астрономия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2f39957-9863-3fcf-869e-3dfd58cd355b",
                    "sense_definition": "Znak w notacji muzycznej, który nakazuje przedłużenie czasu trwania nuty lub pauzy.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "muzyka",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrygent przytrzymał ostatni dźwięk utworu oznaczony <koroną>.",
                            "target_lang_translations": {
                                "ru": "Дирижер задержал последний звук произведения, обозначенный <w>ферматой</w>."
                            }
                        },
                        {
                            "text": "Nad tą nutą znajduje się <korona>, więc musisz ją grać dłużej.",
                            "target_lang_translations": {
                                "ru": "Над этой нотой стоит <w>фермата</w>, поэтому ты должен играть её дольше."
                            }
                        }
                    ],
                    "synonyms": [
                        "fermata"
                    ],
                    "common_phrases": [
                        "nuta z koroną"
                    ],
                    "target_lang_definitions": {
                        "ru": "Знак в музыкальной нотации, предписывающий увеличение длительности ноты или паузы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фермата",
                                "target_lang_sense_clarification": "музыкальный термин"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e3044790-cb83-3792-b8f1-58f5568d45a0",
                    "sense_definition": "Stevig; niet makkelijk in te drukken of te vervormen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_hardness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De grond is te <w>hard</w> om in te graven.",
                            "target_lang_translations": {
                                "ru": "Земля слишком <w>твёрдая</w>, чтобы её копать."
                            }
                        },
                        {
                            "text": "Ik slaap niet graag op een <w>hard</w> matras.",
                            "target_lang_translations": {
                                "ru": "Я не люблю спать на <w>жёстком</w> матрасе."
                            }
                        }
                    ],
                    "antonyms": [
                        "zacht"
                    ],
                    "common_phrases": [
                        "hard als steen",
                        "hard brood"
                    ],
                    "target_lang_definitions": {
                        "ru": "Твёрдый; не поддающийся сжатию или деформации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "твёрдый",
                                "target_lang_sense_clarification": "о материале"
                            },
                            {
                                "target_lang_word": "жёсткий",
                                "target_lang_sense_clarification": "о поверхности, напр. матрас"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e5347023-fca8-3eba-b901-42579008d284",
                    "sense_definition": "Luid; met veel geluid.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De buren draaien vaak <w>harde</w> muziek.",
                            "target_lang_translations": {
                                "ru": "Соседи часто включают <w>громкую</w> музыку."
                            }
                        },
                        {
                            "text": "Er klonk een <w>harde</w> knal uit de keuken.",
                            "target_lang_translations": {
                                "ru": "Из кухни раздался <w>громкий</w> хлопок."
                            }
                        }
                    ],
                    "synonyms": [
                        "luid"
                    ],
                    "antonyms": [
                        "zacht"
                    ],
                    "common_phrases": [
                        "harde muziek",
                        "een harde klap"
                    ],
                    "target_lang_definitions": {
                        "ru": "Громкий; производящий много шума."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "громкий",
                                "target_lang_sense_clarification": "о звуке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8c0d06e-58cf-36c0-911e-02644699930a",
                    "sense_definition": "Krachtig of met hoge snelheid (vaak over wind of regen).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "force_speed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de <w>harde</w> wind vielen er takken van de bomen.",
                            "target_lang_translations": {
                                "ru": "Из-за <w>сильного</w> ветра с деревьев падали ветки."
                            }
                        },
                        {
                            "text": "We bleven binnen vanwege de <w>harde</w> regen.",
                            "target_lang_translations": {
                                "ru": "Мы остались дома из-за <w>сильного</w> дождя."
                            }
                        }
                    ],
                    "synonyms": [
                        "snel",
                        "vlug"
                    ],
                    "antonyms": [
                        "zacht",
                        "langzaam"
                    ],
                    "common_phrases": [
                        "harde wind",
                        "harde regen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильный или быстрый (о ветре, дожде)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сильный",
                                "target_lang_sense_clarification": "о погодных явлениях"

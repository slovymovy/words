            "pos": "verb",
            "senses": [
                {
                    "sense_id": "8ab9fbf6-0a44-3361-bbbf-156d36ab1690",
                    "sense_definition": "Mieć zamiar coś zrobić lub osiągnąć w przyszłości; myśleć o realizacji jakiegoś celu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "intention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym roku <w>planuję</w> zaprosić wszystkich moich przyjaciół na wielką imprezę urodzinową.",
                            "target_lang_translations": {
                                "ru": "В этом году я <w>планирую</w> пригласить всех своих друзей на большую вечеринку в честь дня рождения."
                            }
                        },
                        {
                            "text": "Czy ty w ogóle <w>planujesz</w> kiedyś skończyć te studia i znaleźć stałą pracę?",
                            "target_lang_translations": {
                                "ru": "Ты вообще <w>планируешь</w> когда-нибудь закончить эту учебу и найти постоянную работу?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zamierzać",
                        "postanawiać",
                        "snuć plany"
                    ],
                    "common_phrases": [
                        "planować przyszłość",
                        "planować wakacje",
                        "planować wyjazd",
                        "planować ślub"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь намерение сделать что-либо или достичь чего-либо в будущем; думать о реализации какой-либо цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "планировать",
                                "target_lang_sense_clarification": "намереваться"
                            },
                            {
                                "target_lang_word": "собираться",
                                "target_lang_sense_clarification": "иметь намерение"
                            },
                            {
                                "target_lang_word": "намереваться",
                                "target_lang_sense_clarification": "официальное намерение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9c61d6b-3e5d-31d5-8690-ff2a2dbc7ea4",
                    "sense_definition": "Ustalać szczegóły, harmonogram i kolejność działań potrzebnych do realizacji jakiegoś przedsięwzięcia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "organization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sztab wojenny szczegółowo <w>planuje</w> właśnie kolejną ważną kampanię na froncie wschodnim.",
                            "target_lang_translations": {
                                "ru": "Военный штаб как раз детально <w>планирует</w> очередную важную кампанию на восточном фронте."
                            }
                        },
                        {
                            "text": "Musimy dokładnie <w>planować</w> nasze domowe wydatki, żeby wystarczyło nam pieniędzy do końca miesiąca.",
                            "target_lang_translations": {
                                "ru": "Мы должны тщательно <w>планировать</w> наши домашние расходы, чтобы нам хватило денег до конца месяца."
                            }
                        }
                    ],
                    "synonyms": [
                        "organizować",
                        "rozplanowywać",
                        "opracowywać plan",
                        "opracowywać plany"
                    ],
                    "common_phrases": [
                        "planować budżet",
                        "planować strategię",
                        "planować działania",
                        "dokładnie planować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определять детали, график и порядок действий, необходимых для осуществления какого-либо мероприятия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "планировать",
                                "target_lang_sense_clarification": "составлять план действий"
                            },
                            {
                                "target_lang_word": "распределять",
                                "target_lang_sense_clarification": "бюджет или ресурсы"
                            },
                            {
                                "target_lang_word": "разрабатывать",
                                "target_lang_sense_clarification": "стратегию или план"

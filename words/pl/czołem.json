{
    "word_family": [
        "czołowy",
        "czołówka"
    ],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "1f444f9b-3c8a-3d29-88a5-12d16717892e",
                    "sense_definition": "Potoczne, koleżeńskie powitanie, używane często w relacjach nieoficjalnych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "greeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszedł do pokoju i głośno krzyknął <w>czołem</w> do wszystkich zebranych.",
                            "target_lang_translations": {
                                "ru": "Он вошёл в комнату и громко крикнул: «<w>Привет</w>!» всем собравшимся."
                            }
                        },
                        {
                            "text": "<w>Czołem</w>, dawno cię nie widziałem w naszym biurze.",
                            "target_lang_translations": {
                                "ru": "<w>Салют</w>, давно не видел тебя в нашем офисе."
                            }
                        }
                    ],
                    "synonyms": [
                        "witam"
                    ],
                    "common_phrases": [
                        "czołem załoga",
                        "czołem wszystkim"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное, дружеское приветствие, часто используемое в неофициальной обстановке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привет",
                                "target_lang_sense_clarification": "универсальное приветствие"
                            },
                            {
                                "target_lang_word": "салют",
                                "target_lang_sense_clarification": "дружеское приветствие"
                            },
                            {
                                "target_lang_word": "здорово",
                                "target_lang_sense_clarification": "просторечное, часто мужское"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff24f50e-4285-3282-b66b-e1e13ecedc7d",
                    "sense_definition": "Oficjalne powitanie używane w wojsku, harcerstwie i innych służbach mundurowych, często w relacji z przełożonymi.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "greeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Czołem</w> panie kapitanie, melduję gotowość do odprawy.",
                            "target_lang_translations": {
                                "ru": "<w>Здравия желаю</w>, господин капитан, докладываю о готовности к брифингу."
                            }
                        },
                        {
                            "text": "Żołnierze odpowiedzieli chórem: <w>czołem</w> panie generale!",
                            "target_lang_translations": {
                                "ru": "Солдаты хором ответили: «<w>Здравия желаем</w>, господин генерал!»"
                            }
                        }
                    ],
                    "synonyms": [
                        "witam"
                    ],
                    "common_phrases": [
                        "czołem żołnierze",
                        "czołem panie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное приветствие в армии, скаутинге и других силовых структурах, часто при обращении к старшим по званию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "здравия желаю",
                                "target_lang_sense_clarification": "уставное военное приветствие"
                            },
                            {
                                "target_lang_word": "приветствую",
                                "target_lang_sense_clarification": "формальное приветствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "accfd1df-490b-35d4-8688-615376ebbfcb",
                    "sense_definition": "Potoczne słowo używane na pożegnanie, rzadziej spotykane niż przy powitaniu.",
                    "learner_level": "A2",
                    "frequency": "Low",
                    "semantic_group_id": "farewell",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę już uciekać na autobus, <w>czołem</w>!",
                            "target_lang_translations": {
                                "ru": "Мне уже пора бежать на автобус, <w>пока</w>!"
                            }
                        },
                        {
                            "text": "No to <w>czołem</w>, zobaczymy się w przyszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "Ну, <w>бывай</w>, увидимся на следующей неделе."
                            }
                        }
                    ],
                    "synonyms": [
                        "żegnam"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное слово, используемое при прощании; встречается реже, чем при приветствии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пока",
                                "target_lang_sense_clarification": "универсальное прощание"
                            },
                            {
                                "target_lang_word": "бывай",
                                "target_lang_sense_clarification": "дружеское прощание"
                            },
                            {
                                "target_lang_word": "счастливо",
                                "target_lang_sense_clarification": "пожелание удачи при прощании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3907b076-bf85-395d-bc4d-936c5cf1eaa3",
                    "sense_definition": "Oficjalne pożegnanie używane w wojsku i służbach mundurowych, kierowane do przełożonych lub podwładnych.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "farewell",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Czołem</w> dowódco, udaję się na wyznaczony posterunek.",
                            "target_lang_translations": {
                                "ru": "<w>До свидания</w>, командир, отправляюсь на назначенный пост."
                            }
                        },
                        {
                            "text": "Oficer zasalutował i powiedział krótko: <w>czołem</w>.",
                            "target_lang_translations": {
                                "ru": "Офицер отдал честь и коротко сказал: «<w>Честь имею</w>»."
                            }
                        }
                    ],
                    "synonyms": [
                        "żegnam"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное прощание в армии и силовых структурах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "до свидания",
                                "target_lang_sense_clarification": "формальное прощание"
                            },
                            {
                                "target_lang_word": "честь имею",
                                "target_lang_sense_clarification": "офицерское прощание"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0fa03dee-8abc-30ae-8fc2-8bcbedb4b771",
                    "sense_definition": "Narzędnik liczby pojedynczej rzeczownika <w>czoło</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wierni w kościele leżeli krzyżem, dotykając <w>czołem</w> posadzki.",
                            "target_lang_translations": {
                                "ru": "Верующие в церкви лежали ниц, касаясь <w>лбом</w> пола."
                            }
                        },
                        {
                            "text": "Przez nieuwagę uderzył się mocno <w>czołem</w> o niską framugę drzwi.",
                            "target_lang_translations": {
                                "ru": "По неосторожности он сильно ударился <w>лбом</w> о низкую дверную раму."
                            }
                        }
                    ],
                    "synonyms": [
                        "głową"
                    ],
                    "common_phrases": [
                        "bić czołem"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Творительный падеж единственного числа существительного <w>czoło</w> (лоб)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лбом",
                                "target_lang_sense_clarification": "часть тела"
                            },
                            {
                                "target_lang_word": "челом",
                                "target_lang_sense_clarification": "устаревшее или поэтическое"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
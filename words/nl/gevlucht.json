            "senses": [
                {
                    "sense_id": "e5ef9f04-65b3-3c58-9903-e98be09e24c3",
                    "sense_definition": "Voltooid deelwoord van <w>vluchten</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hun moeder was met hen <w>gevlucht</w> uit Marokko vanwege het aanhoudende geweld.",
                            "target_lang_translations": {
                                "ru": "Их мать <w>бежала</w> с ними из Марокко из-за продолжающегося насилия."
                            }
                        },
                        {
                            "text": "De verdachte is na de overval direct te voet <w>gevlucht</w>.",
                            "target_lang_translations": {
                                "ru": "Подозреваемый сразу после ограбления <w>сбежал</w> пешком."
                            }
                        }
                    ],
                    "common_phrases": [
                        "naar het buitenland gevlucht",
                        "voor de oorlog gevlucht",
                        "uit de gevangenis gevlucht"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие прошедшего времени от глагола 'vluchten' (бежать, спасаться бегством)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбежать",
                                "target_lang_sense_clarification": "совершить побег, скрыться"
                            },
                            {
                                "target_lang_word": "бежать",
                                "target_lang_sense_clarification": "спасаться бегством"
                            },
                            {
                                "target_lang_word": "скрыться",
                                "target_lang_sense_clarification": "уйти незамеченным"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a33e550b-bbc9-390a-a67d-89575796124e",
                    "sense_definition": "Beschrijft personen of dieren die weggegaan zijn uit een onveilige situatie uit angst voor gevaar.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De voor het vuur <w>gevluchte</w> dieren moesten de rivier over zwemmen om veilig te zijn.",
                            "target_lang_translations": {
                                "ru": "Животные, <w>сбежавшие</w> от огня, должны были переплыть реку, чтобы оказаться в безопасности."
                            }
                        },
                        {
                            "text": "De <w>gevluchte</w> regering zette haar werkzaamheden voort vanuit Londen.",
                            "target_lang_translations": {
                                "ru": "<w>Бежавшее</w> правительство продолжило свою работу из Лондона."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gevluchte personen",
                        "gevluchte dieren",
                        "gevluchte gezinnen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Описывает людей или животных, покинувших опасное место из страха перед угрозой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбежавший",
                                "target_lang_sense_clarification": "покинувший место опасности"
                            },
                            {
                                "target_lang_word": "бежавший",
                                "target_lang_sense_clarification": "спасшийся бегством"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cfbcaeb4-4fa5-38ba-b7df-4fe475bb3067",
                    "sense_definition": "Het geheel van de wieken en de as van een windmolen.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "windmill_parts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De molenaar controleerde het <w>gevlucht</w> zorgvuldig op schade na de zware storm.",
                            "target_lang_translations": {
                                "ru": "Мельник тщательно осмотрел <w>крылья</w> на предмет повреждений после сильного шторма."
                            }
                        },
                        {
                            "text": "Tijdens de restauratie van de molen werd het volledige <w>gevlucht</w> vervangen.",
                            "target_lang_translations": {
                                "ru": "Во время реставрации мельницы были заменены все <w>крылья</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wiekenkruis"
                    ],
                    "common_phrases": [
                        "het gevlucht draaien",
                        "oud-hollands gevlucht"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность крыльев и оси ветряной мельницы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крылья мельницы",
                                "target_lang_sense_clarification": "лопасти ветряка"
                            },
                            {
                                "target_lang_word": "мах",
                                "target_lang_sense_clarification": "размах крыльев мельницы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "142534e4-30de-3da1-8814-e8e66deb9adf",
                    "sense_definition": "De afstand tussen de uiteinden van de uitgespreide vleugels van een vogel.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De condor is een vogel met een indrukwekkend <w>gevlucht</w> van enkele meters.",
                            "target_lang_translations": {
                                "ru": "Кондор — это птица с внушительным <w>размахом крыльев</w> в несколько метров."
                            }
                        },
                        {
                            "text": "Met zijn brede <w>gevlucht</w> kan de roofvogel urenlang op de thermiek zweven.",
                            "target_lang_translations": {
                                "ru": "Благодаря своему широкому <w>размаху крыльев</w> хищная птица может часами парить в термических потоках."
                            }
                        }
                    ],
                    "synonyms": [
                        "spanwijdte"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расстояние между кончиками распростертых крыльев птицы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "размах крыльев",
                                "target_lang_sense_clarification": "измерение ширины птицы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3ffb4c8-59ac-3acf-8bce-061fc6dd58f9",
                    "sense_definition": "Het aantal omwentelingen dat de wieken van een molen maken.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "windmill_parts",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij harde wind kan het <w>gevlucht</w> van de molen flink oplopen.",
                            "target_lang_translations": {
                                "ru": "При сильном ветре <w>число оборотов</w> мельницы может значительно возрасти."
                            }
                        },
                        {
                            "text": "De molenaar hield het <w>gevlucht</w> goed in de gaten om overbelasting te voorkomen.",
                            "target_lang_translations": {
                                "ru": "Мельник внимательно следил за <w>ходом</w> мельницы, чтобы предотвратить перегрузку."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Количество оборотов, совершаемых крыльями мельницы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "число оборотов",
                                "target_lang_sense_clarification": "скорость вращения"
                            },
                            {
                                "target_lang_word": "ход",
                                "target_lang_sense_clarification": "движение механизма"
                            }
                        ]
                    }
                }
            ]
        }
    ]

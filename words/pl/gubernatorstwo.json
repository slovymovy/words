    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "64438237-065a-3c3a-bc38-b1c3301e4a99",
                    "sense_definition": "Urząd, stanowisko lub godność gubernatora.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego <w>gubernatorstwo</w> trwało zaledwie dwa lata z powodu nagłego wybuchu wojny domowej.",
                            "target_lang_translations": {
                                "ru": "Его <w>губернаторство</w> длилось всего два года из-за внезапной вспышки гражданской войны."
                            }
                        },
                        {
                            "text": "Sanczo Pansa w powieści Cervantesa marzył, by objąć obiecane mu <w>gubernatorstwo</w> na wyspie.",
                            "target_lang_translations": {
                                "ru": "Санчо Панса в романе Сервантеса мечтал получить обещанное ему <w>губернаторство</w> на острове."
                            }
                        }
                    ],
                    "common_phrases": [
                        "objąć gubernatorstwo",
                        "utracić gubernatorstwo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Должность, пост или звание губернатора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "губернаторство",
                                "target_lang_sense_clarification": "должность или период правления"
                            },
                            {
                                "target_lang_word": "пост губернатора",
                                "target_lang_sense_clarification": "должность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a2bff81-c24a-3613-9cfe-385a1a242633",
                    "sense_definition": "Prowincja lub terytorium zarządzane przez gubernatora.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "territory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To rozległe <w>gubernatorstwo</w> było kluczowym regionem dla gospodarki całego imperium.",
                            "target_lang_translations": {
                                "ru": "Это обширное <w>губернаторство</w> было ключевым регионом для экономики всей империи."
                            }
                        },
                        {
                            "text": "W czasach okupacji niemieckiej na części ziem polskich utworzono Generalne <w>Gubernatorstwo</w>.",
                            "target_lang_translations": {
                                "ru": "Во времена немецкой оккупации на части польских земель было создано Генерал-<w>губернаторство</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Generalne Gubernatorstwo",
                        "zarządzać gubernatorstwem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Провинция или территория, находящаяся под управлением губернатора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "губернаторство",
                                "target_lang_sense_clarification": "административная единица"
                            },
                            {
                                "target_lang_word": "генерал-губернаторство",
                                "target_lang_sense_clarification": "историческая административная единица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cf20409-6996-3579-a79d-09ecc8d88f38",
                    "sense_definition": "Budynek będący siedzibą lub biurem gubernatora.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Protestujący tłum zgromadził się pod budynkiem, w którym mieściło się miejscowe <w>gubernatorstwo</w>.",
                            "target_lang_translations": {
                                "ru": "Протестующая толпа собралась у здания, в котором размещалось местное <w>губернаторство</w>."
                            }
                        },
                        {
                            "text": "Nowe <w>gubernatorstwo</w> zostało wybudowane w samym centrum stolicy regionu, tuż obok ratusza.",
                            "target_lang_translations": {
                                "ru": "Новая <w>резиденция губернатора</w> была построена в самом центре столицы региона, прямо рядом с ратушей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "gmach gubernatorstwa",
                        "siedziba gubernatorstwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Здание, служащее резиденцией или канцелярией губернатора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "резиденция губернатора",
                                "target_lang_sense_clarification": "здание"
                            },
                            {
                                "target_lang_word": "губернаторство",
                                "target_lang_sense_clarification": "здание администрации (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5096ddb6-a11a-338b-8c84-1486473584b3",
                    "sense_definition": "Gubernator wraz z małżonką.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na wczorajszym uroczystym balu charytatywnym pojawili się również państwo <w>gubernatorstwo</w>.",
                            "target_lang_translations": {
                                "ru": "На вчерашнем торжественном благотворительном балу появилась также <w>чета губернатора</w>."
                            }
                        },
                        {
                            "text": "Miejscowa elita z niecierpliwością oczekiwała przyjazdu nowego <w>gubernatorstwa</w> do miasta.",
                            "target_lang_translations": {
                                "ru": "Местная элита с нетерпением ожидала приезда новой <w>четы губернатора</w> в город."
                            }
                        }
                    ],
                    "common_phrases": [
                        "państwo gubernatorstwo"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "forma używana dawniej na określenie małżeństwa"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Губернатор вместе с супругой (устаревшая форма)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чета губернатора",
                                "target_lang_sense_clarification": "супружеская пара"
                            },
                            {
                                "target_lang_word": "губернатор с супругой",
                                "target_lang_sense_clarification": "описание пары"
                            }
                        ]
                    }
                }
            ]
        }
    ]

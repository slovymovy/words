            "senses": [
                {
                    "sense_id": "9e2a7eac-4497-3491-b3d4-aa5dac0ddf41",
                    "sense_definition": "Przedmiot służący do czyszczenia, mycia, czesania lub zamiatania, składający się z oprawki i przymocowanego do niej włosia",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "tool_brush",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę kupić nową <w>szczotkę</w> do włosów, ponieważ stara się połamała.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новую <w>щётку</w> для волос, так как старая сломалась."
                            }
                        },
                        {
                            "text": "Ta <w>szczotka</w> do zamiatania ma bardzo sztywne włosie, idealne na zewnątrz.",
                            "target_lang_translations": {
                                "ru": "У этой <w>щётки</w> для подметания очень жесткая щетина, идеально подходит для улицы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "szczotka do zębów",
                        "szczotka do włosów",
                        "szczotka do butów",
                        "szczotka do zamiatania",
                        "szczotka klozetowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет, используемый для чистки, мытья, расчесывания или подметания, состоящий из колодки и прикрепленной к ней щетины"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щётка",
                                "target_lang_sense_clarification": "инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb2512bf-b47c-3d1d-97ed-a0b3ccdc9fec",
                    "sense_definition": "Element silnika elektrycznego lub prądnicy, który zapewnia połączenie elektryczne z wirującą częścią maszyny",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Silnik w wiertarce przestał działać, więc muszę wymienić zużyte <w>szczotki</w>.",
                            "target_lang_translations": {
                                "ru": "Двигатель в дрели перестал работать, поэтому мне нужно заменить изношенные <w>щётки</w>."
                            }
                        },
                        {
                            "text": "Węglowa <w>szczotka</w> ślizga się po komutatorze, przekazując prąd do wirnika.",
                            "target_lang_translations": {
                                "ru": "Угольная <w>щётка</w> скользит по коллектору, передавая ток на ротор."
                            }

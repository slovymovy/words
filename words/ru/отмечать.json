    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "887f2bcf-32a6-36bc-9ea2-550f642cded6",
                    "sense_definition": "Праздновать какое-либо событие, торжественную дату или достижение.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "celebration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы каждый год весело <отмечаем> этот семейный праздник.",
                            "target_lang_translations": {
                                "en": "Every year we happily <celebrate> this family holiday."
                            }
                        },
                        {
                            "text": "Коллеги решили <отмечать> юбилей фирмы в ресторане.",
                            "target_lang_translations": {
                                "en": "The colleagues decided to <celebrate> the firm's anniversary in a restaurant."
                            }
                        }
                    ],
                    "synonyms": [
                        "праздновать"
                    ],
                    "common_phrases": [
                        "отмечать день рождения",
                        "отмечать новый год",
                        "отмечать победу",
                        "отмечать юбилей",
                        "отмечать праздник"
                    ],
                    "target_lang_definitions": {
                        "en": "To celebrate an event, solemn date, or achievement."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "celebrate",
                                "target_lang_sense_clarification": "to observe an event with festivities"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e553022a-46eb-3970-8ef4-8317ec71170d",
                    "sense_definition": "Обращать внимание на что-либо, выделять или упоминать факт в речи или тексте.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "observation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В своей новой статье автор <отмечает> важность экологии.",
                            "target_lang_translations": {
                                "en": "In his new article, the author <notes> the importance of ecology."
                            }
                        },
                        {
                            "text": "Врачи часто <отмечают> улучшение здоровья после отдыха.",
                            "target_lang_translations": {
                                "en": "Doctors often <note> an improvement in health after a rest."
                            }
                        }
                    ],
                    "synonyms": [
                        "замечать",
                        "подчёркивать",
                        "упоминать",
                        "констатировать"
                    ],
                    "antonyms": [
                        "игнорировать",
                        "упускать"
                    ],
                    "common_phrases": [
                        "отмечать важность",
                        "отмечать успехи",
                        "автор отмечает",
                        "стоит отмечать",
                        "как отмечают эксперты"
                    ],
                    "target_lang_definitions": {
                        "en": "To pay attention to something, highlight, or mention a fact in speech or text."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "note",
                                "target_lang_sense_clarification": "to notice or mention a fact"
                            },
                            {
                                "target_lang_word": "point out",
                                "target_lang_sense_clarification": "to draw attention to something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "545a6d68-71bf-3fde-824d-91073ee1b858",
                    "sense_definition": "Ставить знак или метку на поверхности чего-либо для памяти или учёта.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "marking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Туристы любят <отмечать> свой пройденный маршрут на карте.",
                            "target_lang_translations": {
                                "en": "Tourists love to <mark> their completed route on the map."
                            }
                        },
                        {
                            "text": "В тесте нужно <отмечать> правильные ответы галочкой.",
                            "target_lang_translations": {
                                "en": "In the test, you need to <mark> the correct answers with a check."
                            }
                        }
                    ],
                    "synonyms": [
                        "помечать"
                    ],
                    "antonyms": [
                        "стирать"
                    ],
                    "common_phrases": [
                        "отмечать галочкой",
                        "отмечать крестиком",
                        "отмечать на карте",
                        "отмечать в календаре",
                        "отмечать места"
                    ],
                    "target_lang_definitions": {
                        "en": "To put a sign or mark on a surface for memory or record-keeping."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mark",
                                "target_lang_sense_clarification": "to put a visible sign on something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd05e23e-5906-3933-81d7-869593abe420",
                    "sense_definition": "Фиксировать в списке присутствие или отсутствие кого-либо, регистрировать документы.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "registration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель начал <отмечать> присутствующих в классе учеников.",
                            "target_lang_translations": {
                                "en": "The teacher started to <mark> the students present in the class."
                            }
                        },
                        {
                            "text": "Секретарь должен <отмечать> все входящие письма в журнале.",
                            "target_lang_translations": {
                                "en": "The secretary must <register> all incoming letters in the log."
                            }
                        }
                    ],
                    "synonyms": [
                        "регистрировать",
                        "записывать"
                    ],
                    "common_phrases": [
                        "отмечать присутствующих",
                        "отмечать в списке",
                        "отмечать в журнале",
                        "отмечать командировку"
                    ],
                    "target_lang_definitions": {
                        "en": "To record someone's presence or absence in a list, or to register documents."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mark",
                                "target_lang_sense_clarification": "to record presence in a list"
                            },
                            {
                                "target_lang_word": "register",
                                "target_lang_sense_clarification": "to record officially in a log"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
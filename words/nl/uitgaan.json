                },
                {
                    "sense_id": "2296496e-e117-395a-953d-4be5a127bdda",
                    "sense_definition": "Het beëindigen van een liefdesrelatie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is gisteren onverwacht <uitgegaan> tussen hen.",
                            "target_lang_translations": {
                                "en": "They unexpectedly <broke up> yesterday.",
                                "ru": "Вчера у них неожиданно всё <закончилось>."
                            }
                        },
                        {
                            "text": "Ze was erg verdrietig toen het <uitging> met haar vriend.",
                            "target_lang_translations": {
                                "en": "She was very sad when she <broke up> with her boyfriend.",
                                "ru": "Она была очень грустной, когда всё <закончилось> с её парнем."
                            }
                        }
                    ],
                    "antonyms": [
                        "aangaan"
                    ],
                    "common_phrases": [

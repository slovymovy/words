    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ade4a3e8-8ff0-35de-8dff-330b18b5d275",
                    "sense_definition": "Een situatie die veel moeite kost, ingewikkeld is of gepaard gaat met onnodige drukte.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "hassle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het was een heel <w>gedoe</w> om alle papieren voor de vakantie in orde te krijgen.",
                            "target_lang_translations": {
                                "ru": "Привести в порядок все документы для отпуска было целой <w>морокой</w>."
                            }
                        },
                        {
                            "text": "Ik heb geen zin in <w>gedoe</w> om niks, dus laten we gewoon rustig blijven.",
                            "target_lang_translations": {
                                "ru": "Я не хочу <w>разборок</w> из-за пустяков, так что давай просто сохранять спокойствие."
                            }
                        }
                    ],
                    "synonyms": [
                        "soesa"
                    ],
                    "common_phrases": [
                        "een heel gedoe",
                        "wat een gedoe",
                        "gedoe om niks"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация, которая требует много усилий, сложна или сопровождается ненужной суетой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "морока",
                                "target_lang_sense_clarification": "хлопотное, утомительное дело (разг.)"
                            },
                            {
                                "target_lang_word": "возня",
                                "target_lang_sense_clarification": "беспорядочные хлопоты или длительные сборы"
                            },
                            {
                                "target_lang_word": "канитель",
                                "target_lang_sense_clarification": "нудное, затяжное дело (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ab0f5c6-1544-349a-820c-6f7b5935fd90",
                    "sense_definition": "Iets wat men doet; een bezigheid of activiteit.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na al het <w>gedoe</w> van de dag keerde de rust eindelijk terug in het huis.",
                            "target_lang_translations": {
                                "ru": "После всей дневной <w>суеты</w> в дом наконец вернулся покой."
                            }
                        },
                        {
                            "text": "Hij keek naar het drukke <w>gedoe</w> van de mieren op de grond.",
                            "target_lang_translations": {
                                "ru": "Он наблюдал за оживленной <w>возней</w> муравьев на земле."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "То, чем занимаются; занятие, деятельность или движение (часто хаотичное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "суета",
                                "target_lang_sense_clarification": "беспорядочное движение, торопливость"
                            },
                            {
                                "target_lang_word": "беготня",
                                "target_lang_sense_clarification": "суетливое передвижение туда-сюда"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "e997fdc0-6b10-3f70-b70b-a23ea9da5d90",
                    "sense_definition": "Een mannelijk persoon, vaak gebruikt in een informele context.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "man_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die <kerel> heeft me gisteren geholpen met mijn fiets.",
                            "target_lang_translations": {
                                "en": "That <guy> helped me with my bike yesterday."
                            }
                        },
                        {
                            "text": "Hij is echt een toffe <kerel> met wie je goed kunt lachen.",
                            "target_lang_translations": {
                                "en": "He is really a cool <guy> with whom you can have a good laugh."
                            }
                        }
                    ],
                    "synonyms": [
                        "vent",
                        "gast"
                    ],
                    "common_phrases": [
                        "toffe kerel",
                        "vreemde kerel",
                        "aardige kerel"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A male person, often used in an informal context."
                    },
                    "translations": {

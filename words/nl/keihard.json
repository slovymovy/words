                },
                {
                    "sense_id": "491a3a49-137b-3c13-93ca-40810ddf139c",
                    "sense_definition": "Met zeer hoge snelheid.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "speed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jongen kwam <w>keihard</w> de berg af rennen.",

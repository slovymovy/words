    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cf1a3cbb-8198-3e2c-aa47-3dc0546449cb",
                    "sense_definition": "Jeden z organów parlamentu w niektórych państwach lub wyższy urząd administracji państwowej.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "organizations",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sejm jest niższą <izbą> polskiego parlamentu.",
                            "target_lang_translations": {
                                "ru": "Сейм является нижней <палатой> польского парламента."
                            }
                        },
                        {
                            "text": "Najwyższa <Izba> Kontroli opublikowała nowy raport o finansach.",
                            "target_lang_translations": {
                                "ru": "Высшая контрольная <палата> опубликовала новый отчет о финансах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "izba niższa",
                        "izba wyższa",
                        "izba refleksji",
                        "Najwyższa Izba Kontroli"
                    ],
                    "target_lang_definitions": {
                        "ru": "Один из органов парламента в некоторых государствах или высшее учреждение государственного управления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "палата",
                                "target_lang_sense_clarification": "парламентская"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4c44041-3ffa-345c-a838-b9207e3726cc",
                    "sense_definition": "Główne pomieszczenie mieszkalne w tradycyjnym wiejskim domu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "rooms",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starej chacie cała rodzina spała w jednej <izbie>.",
                            "target_lang_translations": {
                                "ru": "В старой хате вся семья спала в одной <комнате>."
                            }
                        },
                        {
                            "text": "Goście zostali zaproszeni do świątecznie przystrojonej <izby>.",
                            "target_lang_translations": {
                                "ru": "Гостей пригласили в празднично украшенную <горницу>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokój"
                    ],
                    "common_phrases": [
                        "biała izba",
                        "czarna izba",
                        "izba czeladna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Главное жилое помещение в традиционном сельском доме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горница",
                                "target_lang_sense_clarification": "в крестьянском доме"
                            },
                            {
                                "target_lang_word": "комната",
                                "target_lang_sense_clarification": "помещение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9518ae21-0e49-3579-a616-89d5e2456d03",
                    "sense_definition": "Organ samorządu zawodowego lub gospodarczego reprezentujący interesy danej grupy.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "organizations",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Okręgowa <izba> lekarska przyznała mu prawo wykonywania zawodu.",
                            "target_lang_translations": {
                                "ru": "Окружная врачебная <палата> предоставила ему право заниматься профессиональной деятельностью."
                            }
                        },
                        {
                            "text": "Lokalna <izba> gospodarcza wspiera małe przedsiębiorstwa w regionie.",
                            "target_lang_translations": {
                                "ru": "Местная хозяйственная <палата> поддерживает малые предприятия в регионе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "izba handlowa",
                        "izba lekarska",
                        "izba rzemieślnicza",
                        "izba rolnicza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Орган профессионального или хозяйственного самоуправления, представляющий интересы данной группы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "палата",
                                "target_lang_sense_clarification": "организация"

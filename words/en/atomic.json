                            }
                        },
                        {
                            "text": "Programmers use <w>atomic</w> operations to manage shared memory safely.",
                            "target_lang_translations": {
                                "ru": "Программисты используют <w>атомарные</w> операции для безопасного управления общей памятью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "atomic operation",
                        "atomic transaction"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Гарантированно выполняющийся полностью без прерываний или не выполняющийся вовсе, предотвращая ошибки данных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "атомарный",
                                "target_lang_sense_clarification": "в информатике"

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a77d3699-1eba-3d72-9eeb-aea95f3ef887",
                    "sense_definition": "The time of darkness between sunset and sunrise.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_period",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Most animals are awake during the day and sleep at <w>night</w>.",
                            "target_lang_translations": {
                                "ru": "Большинство животных бодрствуют днем и спят <w>ночью</w>."
                            }
                        },
                        {
                            "text": "It was a cold and stormy <w>night</w> in the middle of winter.",
                            "target_lang_translations": {
                                "ru": "Это была холодная и штормовая <w>ночь</w> в середине зимы."
                            }
                        }
                    ],
                    "synonyms": [
                        "nighttime"
                    ],
                    "antonyms": [
                        "day",
                        "daytime"
                    ],
                    "common_phrases": [
                        "at night",
                        "day and night",
                        "last night",
                        "tomorrow night",
                        "all night long"
                    ],
                    "target_lang_definitions": {
                        "ru": "Темное время суток между закатом и восходом солнца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ночь",
                                "target_lang_sense_clarification": "время суток"
                            }
                        ]

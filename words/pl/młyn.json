    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4433bd2e-70ad-318f-908c-58ac0418e73e",
                    "sense_definition": "Budynek wyposażony w urządzenia do mielenia zboża na mąkę.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "building_machine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary <w>młyn</w> nad rzeką został przerobiony na atrakcję turystyczną.",
                            "target_lang_translations": {
                                "ru": "Старая <w>мельница</w> у реки была переделана в туристическую достопримечательность."
                            }
                        },
                        {
                            "text": "Młyn stał nisko, szczyty jego dachów były na poziomie wody.",
                            "target_lang_translations": {
                                "ru": "<w>Мельница</w> стояла низко, коньки её крыш были на уровне воды."
                            }
                        }
                    ],
                    "common_phrases": [
                        "młyn wodny",
                        "młyn wiatrowy",
                        "stary młyn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Здание, оборудованное приспособлениями для размола зерна в муку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мельница",
                                "target_lang_sense_clarification": "здание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2ca757a-c5a8-3451-bfdb-47ad4e1c81d1",
                    "sense_definition": "Maszyna przemysłowa służąca do rozdrabniania, kruszenia lub mielenia twardych materiałów.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "building_machine",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W fabryce zainstalowano nowy <w>młyn</w> do kruszenia skał wapiennych.",
                            "target_lang_translations": {
                                "ru": "На заводе установили новую <w>мельницу</w> для дробления известняка."
                            }
                        },
                        {
                            "text": "Ten potężny <w>młyn</w> węglowy pracuje bez przerwy przez całą dobę.",
                            "target_lang_translations": {
                                "ru": "Эта мощная угольная <w>мельница</w> работает без перерыва круглые сутки."
                            }
                        }
                    ],
                    "synonyms": [
                        "kruszarka",
                        "rozdrabniarka"
                    ],
                    "common_phrases": [
                        "młyn kulowy",
                        "młyn węglowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Промышленная машина для измельчения, дробления или размола твердых материалов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мельница",
                                "target_lang_sense_clarification": "оборудование"
                            },
                            {
                                "target_lang_word": "дробилка",
                                "target_lang_sense_clarification": "машина"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c419566-8b6a-30bf-b9e2-1bed28e27b9b",
                    "sense_definition": "Sytuacja charakteryzująca się dużym chaosem, pośpiechem, zamieszaniem lub nadmiarem obowiązków.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "chaos_trouble",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ale mieliśmy dziś <w>młyn</w> w pracy, nie było czasu nawet na kawę.",
                            "target_lang_translations": {
                                "ru": "Ну и <w>запарка</w> у нас сегодня была на работе, даже на кофе времени не было."
                            }
                        },
                        {
                            "text": "Przed świętami w sklepach panuje straszny <w>młyn</w> i tłok.",
                            "target_lang_translations": {
                                "ru": "Перед праздниками в магазинах царит страшная <w>суматоха</w> и давка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mieć młyn",
                        "niezły młyn",
                        "straszny młyn"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in everyday speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация, характеризующаяся большим хаосом, спешкой, неразберихой или переизбытком обязанностей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "суматоха",
                                "target_lang_sense_clarification": "беспорядок"
                            },
                            {
                                "target_lang_word": "запарка",
                                "target_lang_sense_clarification": "разг. спешка на работе"
                            },
                            {
                                "target_lang_word": "дурдом",
                                "target_lang_sense_clarification": "разг. хаос"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e4b232e5-8402-38e7-b2c9-3d80adcc1e92",
                    "sense_definition": "Stały fragment gry w rugby, w którym zawodnicy obu drużyn napierają na siebie, walcząc o piłkę.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sędzia podyktował <w>młyn</w> po przypadkowym zagraniu piłki ręką.",
                            "target_lang_translations": {
                                "ru": "Судья назначил <w>схватку</w> после случайной игры рукой."
                            }
                        },
                        {
                            "text": "Nasza drużyna ma bardzo silny <w>młyn</w>, co daje nam przewagę fizyczną.",
                            "target_lang_translations": {
                                "ru": "У нашей команды очень сильная <w>схватка</w>, что дает нам физическое преимущество."
                            }
                        }
                    ],
                    "common_phrases": [
                        "młyn dyktowany",
                        "wiązać młyn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стандартное положение в регби, в котором игроки обеих команд смыкаются, борясь за мяч."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "схватка",
                                "target_lang_sense_clarification": "регби"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1987498d-e37d-3d02-9dad-1e1b07a4efbb",
                    "sense_definition": "Sektor na stadionie zajmowany przez najbardziej zagorzałych i głośno dopingujących kibiców.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cały <w>młyn</w> śpiewał pieśni klubowe przez dziewięćdziesiąt minut meczu.",
                            "target_lang_translations": {
                                "ru": "Весь <w>фанатский сектор</w> пел клубные песни на протяжении девяноста минут матча."
                            }
                        },
                        {
                            "text": "Kupiłem bilety do <w>młyna</w>, bo tam jest najlepsza atmosfera.",
                            "target_lang_translations": {
                                "ru": "Я купил билеты на <w>фанатскую трибуну</w>, потому что там лучшая атмосфера."
                            }
                        }
                    ],
                    "common_phrases": [
                        "siedzieć w młynie",
                        "doping z młyna"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "sports slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сектор на стадионе, занимаемый самыми ярыми и громкими болельщиками."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фанатский сектор",
                                "target_lang_sense_clarification": "трибуна"
                            },
                            {
                                "target_lang_word": "вираж",
                                "target_lang_sense_clarification": "сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fba44d6e-1bd6-307a-b2e9-41830ea83024",
                    "sense_definition": "Trudna sytuacja życiowa, kłopoty lub pułapka, z której ciężko się wydostać.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "chaos_trouble",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przez nieprzemyślane decyzje wpadł w finansowy <w>młyn</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за необдуманных решений он попал в финансовый <w>переплёт</w>."
                            }
                        },
                        {
                            "text": "Musisz uważać, żeby ten <w>potrzask</w> nie zniszczył twojej kariery.",
                            "target_lang_translations": {
                                "ru": "Ты должен быть осторожен, чтобы эта <w>ловушка</w> не разрушила твою карьеру."
                            }
                        }
                    ],
                    "synonyms": [
                        "potrzask"
                    ],
                    "common_phrases": [
                        "wpaść w młyn"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Трудная жизненная ситуация, неприятности или ловушка, из которой сложно выбраться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переплёт",
                                "target_lang_sense_clarification": "трудная ситуация"
                            },
                            {
                                "target_lang_word": "жернова",
                                "target_lang_sense_clarification": "метафора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1279c228-bbfe-3c45-b824-722a0e7645fd",
                    "sense_definition": "Więzienie lub areszt.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "prison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiadomo! Przez niego, pętaka, sześć miesięcy <w>młyna</w> otrzymam.",
                            "target_lang_translations": {
                                "ru": "Понятное дело! Из-за него, щенка, я получу шесть месяцев <w>тюряги</w>."
                            }
                        },
                        {
                            "text": "Po dwóch latach wyszedł z <w>młyna</w> i próbuje ułożyć sobie życie.",
                            "target_lang_translations": {
                                "ru": "После двух лет он вышел из <w>тюрьмы</w> и пытается наладить жизнь."
                            }
                        }
                    ],
                    "synonyms": [
                        "mykwa"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "prison or criminal slang"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тюрьма или место заключения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тюряга",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "кутузка",
                                "target_lang_sense_clarification": "разг."
                            }
                        ]
                    }
                }
            ]
        }
    ]

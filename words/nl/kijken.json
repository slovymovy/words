            "pos": "verb",
            "senses": [
                {
                    "sense_id": "77a135f7-6976-3a75-86f1-5e5cf493f5e8",
                    "sense_definition": "De ogen ergens op richten om iets of iemand te zien.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "waarneming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij bleef naar de prachtige zonsondergang <w>kijken</w>.",
                            "target_lang_translations": {
                                "ru": "Он продолжал <w>смотреть</w> на прекрасный закат."
                            }
                        },
                        {
                            "text": "Ik <w>kijk</w> graag naar de sterrenhemel op een heldere nacht.",
                            "target_lang_translations": {
                                "ru": "Я люблю <w>смотреть</w> на звездное небо в ясную ночь."
                            }
                        }
                    ],
                    "antonyms": [
                        "wegkijken"
                    ],
                    "common_phrases": [
                        "kijken naar",
                        "kijk eens",
                        "om zich heen kijken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направлять взгляд на что-либо или кого-либо, чтобы увидеть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смотреть",
                                "target_lang_sense_clarification": "направлять взгляд"
                            },
                            {
                                "target_lang_word": "глядеть",
                                "target_lang_sense_clarification": "направлять взор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f421701e-7fc1-3a33-92f0-e9d5edddecae",
                    "sense_definition": "Met aandacht iets volgen dat beweegt of verandert, zoals een film of programma.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "waarneming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We gaan vanavond samen een film <w>kijken</w>.",
                            "target_lang_translations": {
                                "ru": "Сегодня вечером мы будем вместе <w>смотреть</w> фильм."
                            }
                        },
                        {
                            "text": "Hij zat de hele middag videoclips te <w>kijken</w>.",
                            "target_lang_translations": {
                                "ru": "Он весь день сидел и <w>смотрел</w> видеоклипы."
                            }
                        }
                    ],
                    "synonyms": [
                        "bekijken",
                        "gadeslaan"
                    ],
                    "common_phrases": [
                        "televisie kijken",
                        "film kijken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внимательно следить за чем-то движущимся или меняющимся, например, за фильмом или программой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смотреть",
                                "target_lang_sense_clarification": "фильмы, передачи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae36867d-133f-36f5-8f32-f30f12bdb741",
                    "sense_definition": "Onderzoeken of controleren hoe iets is of werkt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "onderzoek",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zal even <w>kijken</w> of de trein op tijd vertrekt.",
                            "target_lang_translations": {
                                "ru": "Я <w>посмотрю</w>, вовремя ли отправляется поезд."
                            }
                        },
                        {
                            "text": "Je moet daar echt naar laten <w>kijken</w>, want dat geluid is niet normaal.",
                            "target_lang_translations": {
                                "ru": "Тебе действительно нужно дать это <w>посмотреть</w>, потому что этот звук ненормальный."
                            }
                        }
                    ],
                    "synonyms": [
                        "controleren",
                        "nagaan",
                        "onderzoeken"
                    ],
                    "common_phrases": [
                        "kijken of",
                        "even kijken",
                        "laten kijken naar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исследовать или проверять состояние или работу чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проверять",
                                "target_lang_sense_clarification": "контролировать, выяснять"
                            },
                            {
                                "target_lang_word": "посмотреть",
                                "target_lang_sense_clarification": "взглянуть, чтобы узнать"
                            }
                        ]
                    }
                }
            ]
        }
    ]

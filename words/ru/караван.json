    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f8fa55d8-f936-3c10-a097-19f343096d5d",
                    "sense_definition": "Группа вьючных животных (обычно верблюдов), перевозящих грузы и людей в пустыне или степи",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Торговый <караван> медленно двигался через жаркую пустыню к оазису.",
                            "target_lang_translations": {
                                "en": "The trading <caravan> moved slowly through the hot desert towards the oasis."
                            }
                        },
                        {
                            "text": "Путешественники увидели длинный <караван> верблюдов, нагруженных тяжёлыми тюками.",
                            "target_lang_translations": {
                                "en": "The travelers saw a long <caravan> of camels loaded with heavy bales."
                            }
                        }
                    ],
                    "common_phrases": [
                        "караван верблюдов",
                        "торговый караван",
                        "идти караваном"
                    ],
                    "target_lang_definitions": {
                        "en": "A group of pack animals (usually camels) transporting goods and people in a desert or steppe"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "caravan",
                                "target_lang_sense_clarification": "group of travelers or animals"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae3299e9-4f03-3b7c-8d06-54446035870a",
                    "sense_definition": "Группа транспортных судов или машин, следующих друг за другом для выполнения общей задачи или безопасности",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мощный ледокол прокладывал путь сквозь льды для <каравана> грузовых судов.",
                            "target_lang_translations": {
                                "en": "A powerful icebreaker forged a path through the ice for a <convoy> of cargo ships."
                            }
                        },
                        {
                            "text": "Гуманитарный <караван> успешно доставил продукты и медикаменты в пострадавший район.",
                            "target_lang_translations": {
                                "en": "The humanitarian <convoy> successfully delivered food and medicine to the affected area."
                            }
                        }
                    ],
                    "synonyms": [
                        "колонна",
                        "конвой"
                    ],
                    "common_phrases": [
                        "караван судов",
                        "гуманитарный караван",
                        "морской караван"
                    ],
                    "target_lang_definitions": {
                        "en": "A group of transport vessels or vehicles following each other for a common task or safety"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "convoy",
                                "target_lang_sense_clarification": "group of vehicles or ships"
                            },
                            {
                                "target_lang_word": "column",
                                "target_lang_sense_clarification": "formation of vehicles"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99ab8d64-4a55-3c9a-b34c-6bb2c8956701",
                    "sense_definition": "Длинная вереница движущихся друг за другом живых существ или объектов",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Осенью в небе потянулись <караваны> перелётных птиц, направляющихся на юг.",
                            "target_lang_translations": {
                                "en": "In autumn, <strings> of migratory birds heading south stretched across the sky."
                            }
                        },
                        {
                            "text": "По узкой горной дороге полз пёстрый <караван> туристов с рюкзаками.",
                            "target_lang_translations": {
                                "en": "A colorful <line> of tourists with backpacks crawled along the narrow mountain road."
                            }
                        }
                    ],
                    "synonyms": [
                        "вереница"
                    ],
                    "common_phrases": [
                        "птичий караван"
                    ],
                    "target_lang_definitions": {
                        "en": "A long line of moving living beings or objects one after another"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "string",
                                "target_lang_sense_clarification": "line of moving objects"
                            },
                            {
                                "target_lang_word": "train",
                                "target_lang_sense_clarification": "procession"
                            },
                            {
                                "target_lang_word": "line",
                                "target_lang_sense_clarification": "general sequence"
                            }
                        ]
                    }
                }
            ]
        }
    ]

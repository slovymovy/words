{
    "word_family": [],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "39b935cd-1396-3ca0-a5ed-a2f466ee013b",
                    "sense_definition": "A respectful term of address to an adult male, especially if his name is unknown.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "vocative_address",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Excuse me, <w>sir</w>, could you please tell me the time?",
                            "target_lang_translations": {
                                "ru": "Простите, <w>сэр</w>, не могли бы вы подсказать мне время?"
                            }
                        },
                        {
                            "text": "I am sorry, <w>sir</w>, but the store is closed right now.",
                            "target_lang_translations": {
                                "ru": "Мне жаль, <w>сэр</w>, но магазин сейчас закрыт."
                            }
                        }
                    ],
                    "antonyms": [
                        "madam",
                        "ma'am"
                    ],
                    "common_phrases": [
                        "yes sir",
                        "no sir",
                        "excuse me sir",
                        "dear Sir"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вежливое обращение к мужчине, особенно если его имя неизвестно"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сэр",
                                "target_lang_sense_clarification": "в англоязычном контексте"
                            },
                            {
                                "target_lang_word": "господи́н",
                                "target_lang_sense_clarification": "формальное обращение"
                            },
                            {
                                "target_lang_word": "су́дарь",
                                "target_lang_sense_clarification": "устаревшее, вежливое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20c6a6e0-1b78-369e-991a-e621b305379a",
                    "sense_definition": "A formal term of address used to a male superior, such as a military officer or a teacher.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "vocative_address",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldier stood at attention and shouted, \"Yes, <w>sir</w>!\"",
                            "target_lang_translations": {
                                "ru": "Солдат встал по стойке смирно и крикнул: «Так точно, <w>сэр</w>!»"
                            }
                        },
                        {
                            "text": "Here is my history report, <w>sir</w>.",
                            "target_lang_translations": {
                                "ru": "Вот мой доклад по истории, <w>сэр</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "ma'am"
                    ],
                    "common_phrases": [
                        "sir yes sir"
                    ],
                    "target_lang_definitions": {
                        "ru": "Формальное обращение к старшему по званию или должности (например, к офицеру или учителю)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сэр",
                                "target_lang_sense_clarification": "обращение к начальнику"
                            },
                            {
                                "target_lang_word": "господи́н",
                                "target_lang_sense_clarification": "официальное обращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3f3d7897-80b3-38f0-be30-5a3f8de18dfc",
                    "sense_definition": "The titular prefix given to a knight or baronet, used before the given name.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "title_or_honorific_name",
                    "examples": [
                        {
                            "text": "<w>Sir</w> Elton John is famous for his music and charity work.",
                            "target_lang_translations": {
                                "ru": "<w>Сэр</w> Элтон Джон известен своей музыкой и благотворительностью."
                            }
                        },
                        {
                            "text": "The queen knighted him, and he became <w>Sir</w> Thomas.",
                            "target_lang_translations": {
                                "ru": "Королева посвятила его в рыцари, и он стал <w>сэром</w> Томасом."
                            }
                        }
                    ],
                    "antonyms": [
                        "Dame"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "always capitalized"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Титул, используемый перед именем рыцаря или баронета"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сэр",
                                "target_lang_sense_clarification": "титул"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "77628ac1-1f54-3d5a-b58f-4a18dab31fd9",
                    "sense_definition": "To address another individual using the word \"sir\".",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please do not <w>sir</w> me; I am not that old yet.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не <w>называйте</w> меня <w>сэром</w>, я ещё не так стар."
                            }
                        },
                        {
                            "text": "The polite young waiter <w>sirred</w> every customer who walked through the door.",
                            "target_lang_translations": {
                                "ru": "Вежливый молодой официант <w>называл сэром</w> каждого посетителя, который входил в дверь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "don't sir me"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обращаться к кому-либо, используя слово «сэр»"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "называть сэром",
                                "target_lang_sense_clarification": "величать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
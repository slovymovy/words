            "senses": [
                {
                    "sense_id": "36bb0d66-5b97-37fc-9d46-5850fa27e5da",
                    "sense_definition": "One of the main characters of the play Romeo and Juliet by William Shakespeare, who is the passionate lover of Juliet.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "shakespeare_character",
                    "name_type": "fictional_name",
                    "examples": [
                        {
                            "text": "The actor played <w>Romeo</w> with great passion and intensity on stage.",
                            "target_lang_translations": {
                                "ru": "Актер сыграл <w>Ромео</w> с большой страстью и напором на сцене."
                            }
                        },
                        {
                            "text": "In the famous balcony scene, <w>Romeo</w> declares his undying love for Juliet.",
                            "target_lang_translations": {
                                "ru": "В знаменитой сцене на балконе <w>Ромео</w> признается Джульетте в вечной любви."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Один из главных героев пьесы Уильяма Шекспира «Ромео и Джульетта», страстный возлюбленный Джульетты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ромео",
                                "target_lang_sense_clarification": "персонаж Шекспира"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d27c5ff5-543e-30d1-b8fc-ad81865f2769",
                    "sense_definition": "A male given name of Romance origin.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My cousin <w>Romeo</w> lives in Italy with his family.",
                            "target_lang_translations": {
                                "ru": "Мой двоюродный брат <w>Ромео</w> живет в Италии со своей семьей."
                            }
                        },
                        {
                            "text": "They decided to name their newborn son <w>Romeo</w>.",
                            "target_lang_translations": {
                                "ru": "Они решили назвать своего новорожденного сына <w>Ромео</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское имя романского происхождения."
                    },
                    "translations": {

        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5b716080-882f-3a1d-8e44-5cdd578ae8c2",
                    "sense_definition": "To slightly sprain, strain, or wrench a part of the body, such as the neck or back.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_injury",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I managed to <w>rick</w> my neck while sleeping in an awkward position.",
                            "target_lang_translations": {
                                "ru": "Я умудрился <w>потянуть</w> шею, пока спал в неудобной позе."
                            }
                        },
                        {
                            "text": "He <w>ricked</w> his back when he tried to lift the heavy box alone.",
                            "target_lang_translations": {
                                "ru": "Он <w>потянул</w> спину, когда попытался в одиночку поднять тяжёлую коробку."
                            }
                        }
                    ],
                    "synonyms": [
                        "sprain",
                        "strain",
                        "wrench",
                        "twist"
                    ],
                    "common_phrases": [
                        "rick one's neck",
                        "rick one's back"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слегка потянуть, растянуть или вывихнуть часть тела, например шею или спину."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "потянуть",
                                "target_lang_sense_clarification": "мышцу или связку"
                            },
                            {
                                "target_lang_word": "свернуть",
                                "target_lang_sense_clarification": "шею (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d49a57b3-829f-3adc-bebe-17174e7f5dc2",

        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1c2fbf58-eab0-323a-9ce3-8cc06699ae0b",
                    "sense_definition": "Oneness; the state or fact of being one undivided entity.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "state_of_oneness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The president emphasized the need for national <w>unity</w> during the crisis.",
                            "target_lang_translations": {
                                "ru": "Президент подчеркнул необходимость национального <w>единства</w> во время кризиса."
                            }
                        },
                        {
                            "text": "A shared language can help create a sense of <w>unity</w> among the people.",
                            "target_lang_translations": {
                                "ru": "Общий язык может помочь создать чувство <w>единства</w> среди людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "oneness",
                        "wholeness",
                        "integrity"
                    ],
                    "antonyms": [
                        "disunity",
                        "plurality",
                        "multiplicity"
                    ],
                    "common_phrases": [
                        "national unity",
                        "sense of unity",
                        "unity of purpose",
                        "strength in unity"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние цельности; факт существования как единое, неделимое целое."
                    },
                    "translations": {
                        "ru": [
                            {

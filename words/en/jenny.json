                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My sister <w>Jenny</w> is coming to visit us this weekend.",
                            "target_lang_translations": {
                                "ru": "Моя сестра <w>Дженни</w> приедет навестить нас в эти выходные."
                            }

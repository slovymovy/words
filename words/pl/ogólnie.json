    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "9bf1e095-f690-3baf-9c4e-0a8b1afcda60",
                    "sense_definition": "W sposób powierzchowny, biorąc pod uwagę całość, a nie wchodząc w szczegóły.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "without_details",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ogólnie czuje się nieźle, tylko jeszcze czasami boli go blizna po operacji.",
                            "target_lang_translations": {
                                "ru": "<w>В целом</w> он чувствует себя неплохо, только шрам после операции иногда ещё болит."
                            }
                        },
                        {
                            "text": "Ogólnie rzecz biorąc, jestem bardzo zadowolony z wyników tego projektu.",
                            "target_lang_translations": {
                                "ru": "<w>В общем</w> говоря, я очень доволен результатами этого проекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "generalnie"
                    ],
                    "antonyms": [
                        "szczególnie"
                    ],
                    "common_phrases": [
                        "ogólnie mówiąc",
                        "ogólnie rzecz biorąc"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поверхностно, рассматривая всё в совокупности, не вдаваясь в детали."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в целом",
                                "target_lang_sense_clarification": "рассматривая ситуацию полностью"
                            },
                            {
                                "target_lang_word": "в общем",
                                "target_lang_sense_clarification": "говоря обобщенно"
                            },
                            {
                                "target_lang_word": "в основном",
                                "target_lang_sense_clarification": "по большей части"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90038bb2-258f-3d6e-8aa0-41cae84e278c",
                    "sense_definition": "W sposób powszechny, dotyczący wszystkich lub większości ludzi; wspólnie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "universally",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uczeń złamał ogólnie przyjęte zasady zachowania się na terenie szkoły.",
                            "target_lang_translations": {
                                "ru": "Ученик нарушил <w>общепринятые</w> правила поведения на территории школы."
                            }
                        },
                        {
                            "text": "Ten park jest ogólnie dostępny dla wszystkich mieszkańców miasta.",
                            "target_lang_translations": {
                                "ru": "Этот парк <w>общедоступен</w> для всех жителей города."
                            }
                        }
                    ],
                    "synonyms": [
                        "generalnie"
                    ],
                    "antonyms": [
                        "indywidualnie"
                    ],
                    "common_phrases": [
                        "ogólnie dostępny",
                        "ogólnie znany",
                        "ogólnie przyjęty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Всеобщим образом, касаясь всех или большинства; публично."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "всеобще",
                                "target_lang_sense_clarification": "касаясь всех"
                            },
                            {
                                "target_lang_word": "широко",
                                "target_lang_sense_clarification": "среди большого количества людей"
                            },
                            {
                                "target_lang_word": "повсеместно",
                                "target_lang_sense_clarification": "везде, во многих местах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "d1a4e9d2-b580-388f-a22e-44540413ce74",
                    "sense_definition": "Słowo używane do podsumowania wypowiedzi lub wyrażenia ogólnej opinii, pomijając wyjątki.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "particle_summary",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ogólnie to nie lubię horrorów, ale ten film był całkiem ciekawy.",
                            "target_lang_translations": {
                                "ru": "<w>Вообще</w> я не люблю ужастики, но этот фильм был довольно интересным."
                            }

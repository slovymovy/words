    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3dc01035-4e4d-31b1-b4ac-3170f5f7efbb",
                    "sense_definition": "The process of forming a close personal relationship or emotional attachment with others.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The weekend retreat was designed to encourage team <w>bonding</w> among the staff.",
                            "target_lang_translations": {
                                "ru": "Выездной семинар был организован для поощрения <w>сплочения</w> коллектива."
                            }
                        },
                        {
                            "text": "Psychologists emphasize the importance of early <w>bonding</w> between a mother and her newborn child.",
                            "target_lang_translations": {
                                "ru": "Психологи подчеркивают важность раннего формирования <w>привязанности</w> между матерью и ее новорожденным ребенком."
                            }
                        }
                    ],
                    "synonyms": [
                        "connection",
                        "rapport",
                        "attachment"
                    ],
                    "antonyms": [
                        "alienation",
                        "estrangement",
                        "detachment"
                    ],
                    "common_phrases": [
                        "team bonding",
                        "male bonding",
                        "bonding experience",
                        "social bonding"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс формирования близких личных отношений или эмоциональной привязанности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сближение",
                                "target_lang_sense_clarification": "процесс становления ближе друг к другу"
                            },
                            {
                                "target_lang_word": "сплочение",
                                "target_lang_sense_clarification": "объединение группы людей или команды"
                            },
                            {
                                "target_lang_word": "привязанность",
                                "target_lang_sense_clarification": "эмоциональная связь, часто между матерью и ребенком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d3a3596-2b29-34da-a802-da91eabbdbef",
                    "sense_definition": "The act or process of joining things together securely, often using glue, heat, or chemical forces.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>bonding</w> of the two metal surfaces requires a specialized industrial adhesive.",
                            "target_lang_translations": {
                                "ru": "Для <w>склеивания</w> двух металлических поверхностей требуется специальный промышленный клей."
                            }
                        },
                        {
                            "text": "Students learned about the chemical <w>bonding</w> that holds atoms together in a molecule.",
                            "target_lang_translations": {
                                "ru": "Студенты узнали о химической <w>связи</w>, которая удерживает атомы вместе в молекуле."
                            }
                        }
                    ],
                    "synonyms": [
                        "adhesion",
                        "fusion",
                        "joining"
                    ],
                    "antonyms": [
                        "separation",
                        "disconnection"
                    ],
                    "common_phrases": [
                        "chemical bonding",
                        "adhesive bonding",
                        "thermal bonding"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или процесс надежного соединения предметов, часто с использованием клея, тепла или химических сил."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соединение",
                                "target_lang_sense_clarification": "общее значение скрепления чего-либо"
                            },
                            {
                                "target_lang_word": "склеивание",
                                "target_lang_sense_clarification": "соединение с помощью клея"
                            },
                            {
                                "target_lang_word": "связь",
                                "target_lang_sense_clarification": "химическое взаимодействие атомов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42301d9c-f389-3c9f-af21-1b087f58bd2b",
                    "sense_definition": "A method of combining multiple network interfaces into a single logical interface to improve performance or reliability.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Network <w>bonding</w> ensures that the server stays online even if one cable fails.",
                            "target_lang_translations": {
                                "ru": "Сетевой <w>бондинг</w> гарантирует, что сервер останется в сети, даже если один кабель выйдет из строя."
                            }
                        },
                        {
                            "text": "The technician configured channel <w>bonding</w> to double the data transfer speed.",
                            "target_lang_translations": {
                                "ru": "Техник настроил <w>объединение каналов</w>, чтобы удвоить скорость передачи данных."
                            }
                        }
                    ],
                    "synonyms": [

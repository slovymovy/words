    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "eb211b4d-240d-3606-99e8-1caba1e3523e",
                    "sense_definition": "Część pojazdu lub maszyny w kształcie dysku lub obręczy, która obraca się na osi i umożliwia ruch.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W moim rowerze przebiło się tylne <w>koło</w>.",
                            "target_lang_translations": {
                                "ru": "У меня на велосипеде пробило заднее <w>колесо</w>."
                            }
                        },
                        {
                            "text": "Ten samochód ma napęd na cztery <w>koła</w>.",
                            "target_lang_translations": {
                                "ru": "У этой машины привод на четыре <w>колеса</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "koło zapasowe",
                        "napęd na cztery koła",
                        "piąte koło u wozu",
                        "dwa kółka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть транспортного средства или машины в форме диска или обода, которая вращается на оси и обеспечивает движение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колесо",
                                "target_lang_sense_clarification": "часть машины или транспортного средства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dccd2d10-2b42-3b77-ac34-a017e0990e31",
                    "sense_definition": "Figura geometryczna płaska ograniczona okręgiem; potocznie także sam okrąg lub dowolny przedmiot o takim kształcie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczycielka narysowała na tablicy idealne <w>koło</w>.",
                            "target_lang_translations": {
                                "ru": "Учительница нарисовала на доске идеальный <w>круг</w>."
                            }
                        },
                        {
                            "text": "Dzieci złapały się za ręce i utworzyły duże <w>koło</w>.",
                            "target_lang_translations": {
                                "ru": "Дети взялись за руки и образовали большой <w>круг</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "okrąg",
                        "krąg",
                        "kolo"
                    ],
                    "common_phrases": [
                        "błędne koło",
                        "koło ratunkowe",
                        "zatoczyć koło",
                        "w kółko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Плоская геометрическая фигура, ограниченная окружностью; в разговорной речи также сама окружность или любой предмет такой формы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "круг",
                                "target_lang_sense_clarification": "геометрическая фигура"
                            },
                            {
                                "target_lang_word": "окружность",
                                "target_lang_sense_clarification": "граница круга"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "34ba85a6-c817-3973-afd0-aa4d189c6acb",
                    "sense_definition": "Grupa osób zorganizowana wokół wspólnych zainteresowań, celów lub działalności.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "organization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja siostra zapisała się na <w>koło</w> teatralne w szkole.",
                            "target_lang_translations": {
                                "ru": "Моя сестра записалась в театральный <w>кружок</w> в школе."
                            }
                        },
                        {
                            "text": "Miejscowe <w>koło</w> gospodyń wiejskich organizuje festyn.",
                            "target_lang_translations": {
                                "ru": "Местный <w>кружок</w> сельских хозяек организует праздник."
                            }
                        }
                    ],
                    "synonyms": [
                        "grono",
                        "krąg",
                        "okrąg"
                    ],
                    "common_phrases": [
                        "koło naukowe",
                        "koło gospodyń wiejskich",
                        "koło łowieckie",
                        "koło poselskie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, организованная вокруг общих интересов, целей или деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кружок",
                                "target_lang_sense_clarification": "группа по интересам"
                            },
                            {
                                "target_lang_word": "общество",
                                "target_lang_sense_clarification": "организация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e54d96c8-cb74-3890-aad0-04fad1599a74",
                    "sense_definition": "Potoczne określenie kolokwium, czyli sprawdzianu wiedzy na studiach.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "exam_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie mogę iść na imprezę, bo jutro mam <w>koło</w> z anatomii.",
                            "target_lang_translations": {
                                "ru": "Я не могу пойти на вечеринку, потому что завтра у меня <w>коллоквиум</w> по анатомии."
                            }
                        },
                        {
                            "text": "Jeśli nie zaliczysz tego <w>koła</w>, będziesz miał poprawkę.",
                            "target_lang_translations": {
                                "ru": "Если не сдашь этот <w>коллоквиум</w>, у тебя будет пересдача."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "studencki"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название коллоквиума, то есть проверки знаний в вузе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коллоквиум",
                                "target_lang_sense_clarification": "проверка знаний"
                            },
                            {
                                "target_lang_word": "коллок",
                                "target_lang_sense_clarification": "студенческий сленг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e75905c-567d-3f89-bb91-c05de3cc8079",
                    "sense_definition": "Potoczne określenie kwoty tysiąca złotych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "money_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Naprawa samochodu kosztowała mnie prawie dwa <w>koła</w>.",
                            "target_lang_translations": {
                                "ru": "Ремонт машины обошелся мне почти в две <w>штуки</w>."
                            }
                        },
                        {
                            "text": "Pożyczyłem od brata jedno <w>koło</w> na wakacje.",
                            "target_lang_translations": {
                                "ru": "Я одолжил у брата одну <w>штуку</w> на отпуск."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название суммы в тысячу злотых."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "штука",
                                "target_lang_sense_clarification": "тысяча (сленг)"
                            },
                            {
                                "target_lang_word": "косарь",
                                "target_lang_sense_clarification": "тысяча (сленг)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3c3c0c4-c665-3e94-9e44-834b74bf69da",
                    "sense_definition": "Rower (określenie regionalne).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "vehicle_regional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie jeżdżę do pracy na <w>kole</w>.",
                            "target_lang_translations": {
                                "ru": "Я каждый день езжу на работу на <w>велосипеде</w>."
                            }
                        },
                        {
                            "text": "Muszę napompować opony w moim starym <w>kole</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно накачать шины на моем старом <w>велосипеде</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rower"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Górny Śląsk"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Велосипед (региональное название)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "велосипед",
                                "target_lang_sense_clarification": "транспортное средство"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "4b01d7b1-b065-3c05-a99d-45add08bdd23",
                    "sense_definition": "Wskazuje na bliskość przestrzenną; obok, przy, w sąsiedztwie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_prep",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przystanek autobusowy znajduje się zaraz <w>koło</w> mojego bloku.",
                            "target_lang_translations": {
                                "ru": "Автобусная остановка находится прямо <w>возле</w> моего дома."
                            }
                        },
                        {
                            "text": "Postaw to krzesło <w>koło</w> stołu w kuchni.",
                            "target_lang_translations": {
                                "ru": "Поставь этот стул <w>возле</w> стола на кухне."
                            }
                        }
                    ],
                    "synonyms": [
                        "obok",
                        "przy",
                        "niedaleko",
                        "podle"
                    ],
                    "common_phrases": [
                        "siedzieć koło siebie",
                        "przejść koło domu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на пространственную близость; рядом, возле, по соседству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возле",
                                "target_lang_sense_clarification": "рядом с"
                            },
                            {
                                "target_lang_word": "около",
                                "target_lang_sense_clarification": "поблизости"
                            },
                            {
                                "target_lang_word": "у",
                                "target_lang_sense_clarification": "вплотную к"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "becfc186-ac21-3282-aec8-0c596e0ab727",
                    "sense_definition": "Wskazuje na przybliżoną ilość lub czas; około, mniej więcej.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "approximation_prep",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Będę w domu <w>koło</w> godziny osiemnastej.",
                            "target_lang_translations": {
                                "ru": "Я буду дома <w>около</w> шести часов."
                            }
                        },
                        {
                            "text": "Na koncert przyszło <w>koło</w> stu osób.",
                            "target_lang_translations": {
                                "ru": "На концерт пришло <w>около</w> ста человек."
                            }
                        }
                    ],
                    "synonyms": [
                        "około"
                    ],
                    "common_phrases": [
                        "koło południa",
                        "koło godziny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на приблизительное количество или время; около, примерно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "около",
                                "target_lang_sense_clarification": "приблизительно"
                            },
                            {
                                "target_lang_word": "примерно",
                                "target_lang_sense_clarification": "приблизительно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "6f95d764-23cf-354b-b4ad-02c0cd74b901",
                    "sense_definition": "Miasto w Polsce, w województwie wielkopolskim.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "city_name",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Moi dziadkowie mieszkają w mieście <w>Koło</w>.",
                            "target_lang_translations": {
                                "ru": "Мои бабушка и дедушка живут в городе <w>Коло</w>."
                            }
                        },
                        {
                            "text": "<w>Koło</w> leży nad rzeką Wartą.",
                            "target_lang_translations": {
                                "ru": "<w>Коло</w> стоит на реке Варте."
                            }
                        }
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "8b2b4c87-93ba-36da-816f-6f58f9773409",
                    "sense_definition": "Having two parts, functions, or aspects; pertaining to a pair or two people or things.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new device serves a <w>dual</w> purpose as both a heater and an air purifier.",
                            "target_lang_translations": {
                                "ru": "Новое устройство имеет <w>двойное</w> назначение: оно служит и обогревателем, и очистителем воздуха."
                            }
                        },
                        {
                            "text": "She holds <w>dual</w> citizenship in the United States and France.",
                            "target_lang_translations": {
                                "ru": "У нее <w>двойное</w> гражданство США и Франции."
                            }
                        }
                    ],
                    "synonyms": [
                        "double",
                        "twofold",
                        "binary",
                        "twin"
                    ],
                    "antonyms": [
                        "single"
                    ],
                    "common_phrases": [
                        "dual citizenship",
                        "dual purpose",
                        "dual role",
                        "dual nature"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий две части, функции или аспекта; относящийся к паре или двум людям/вещам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "двойно́й",
                                "target_lang_sense_clarification": "состоящий из двух частей, имеющий два назначения"
                            },
                            {
                                "target_lang_word": "дво́йственный",
                                "target_lang_sense_clarification": "противоречивый или имеющий две стороны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7e82cf3-d7c5-3656-86cc-065a68f6bca7",
                    "sense_definition": "Relating to a grammatical form in some languages that denotes exactly two people or things, distinct from singular and plural.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In Ancient Greek, the <w>dual</w> number is used specifically for pairs, such as eyes or hands.",
                            "target_lang_translations": {
                                "ru": "В древнегреческом языке <w>двойственное</w> число используется специально для парных предметов, таких как глаза или руки."
                            }
                        },
                        {
                            "text": "The student found it difficult to memorize the <w>dual</w> endings for the verbs.",
                            "target_lang_translations": {
                                "ru": "Студенту было трудно запомнить окончания глаголов <w>двойственного</w> числа."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dual number",
                        "dual form"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к грамматической форме, обозначающей ровно два предмета или лица."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дво́йственный",
                                "target_lang_sense_clarification": "грамматический термин (обычно в сочетании 'двойственное число')"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3e69de12-0bd7-3d1e-a0e5-cf2d5af5e848",
                    "sense_definition": "The grammatical form used to refer to two people or things.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Some languages have a singular, a <w>dual</w>, and a plural.",
                            "target_lang_translations": {
                                "ru": "В некоторых языках есть единственное, <w>двойственное</w> и множественное число."
                            }
                        },
                        {
                            "text": "The use of the <w>dual</w> has disappeared from many modern Indo-European languages.",
                            "target_lang_translations": {
                                "ru": "Использование <w>двойственного числа</w> исчезло из многих современных индоевропейских языков."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грамматическая форма, используемая для обозначения двух людей или предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дво́йственное число́",
                                "target_lang_sense_clarification": "грамматическая категория"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ff3aac5-40e2-3f11-81dc-14fcda4c320d",
                    "sense_definition": "A scheduled competition or match between two teams, particularly in sports like wrestling.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The wrestling team won their first <w>dual</w> of the season against a local rival.",
                            "target_lang_translations": {
                                "ru": "Команда по борьбе выиграла свою первую <w>матчевую встречу</w> в сезоне против местного соперника."
                            }
                        },
                        {
                            "text": "Tickets for the upcoming <w>dual</w> are sold out due to the high ranking of both schools.",
                            "target_lang_translations": {
                                "ru": "Билеты на предстоящий <w>поединок</w> распроданы из-за высокого рейтинга обеих школ."
                            }
                        }
                    ],
                    "common_phrases": [

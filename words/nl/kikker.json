    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5a859a16-126a-3615-8f78-1e93384f878b",
                    "sense_definition": "Een amfibie met een gladde huid, lange achterpoten om te springen en geen staart.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De groene <w>kikker</w> sprong met een grote boog in de sloot.",
                            "target_lang_translations": {
                                "en": "The green <w>frog</w> jumped in a high arc into the ditch."
                            }
                        },
                        {
                            "text": "Ik kan niet goed praten, want ik heb een <w>kikker</w> in mijn keel.",
                            "target_lang_translations": {
                                "en": "I can't speak well, because I have a <w>frog</w> in my throat."
                            }
                        }
                    ],
                    "synonyms": [
                        "kikvors"
                    ],
                    "common_phrases": [
                        "kikker in de keel"
                    ],
                    "target_lang_definitions": {
                        "en": "An amphibian with smooth skin, long hind legs for jumping, and no tail."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "frog",
                                "target_lang_sense_clarification": "amphibian"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94253f51-b3b1-3d42-9f73-a8f1bc2dc24e",
                    "sense_definition": "Iemand die koelbloedig is of weinig emotie toont (vaak gebruikt in samenstellingen of uitdrukkingen).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de crisis bleef hij een koele <w>kikker</w>.",
                            "target_lang_translations": {
                                "en": "During the crisis, he remained a cool <w>customer</w>."
                            }
                        },
                        {
                            "text": "Mijn buurman is een koude <w>kikker</w> die nooit gedag zegt.",
                            "target_lang_translations": {
                                "en": "My neighbor is a cold <w>fish</w> who never says hello."
                            }
                        }
                    ],
                    "common_phrases": [
                        "een koele kikker",
                        "een koude kikker"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone who is cold-blooded or shows little emotion (often used in compounds or expressions)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fish",
                                "target_lang_sense_clarification": "in 'cold fish' (unemotional person)"
                            },
                            {
                                "target_lang_word": "customer",
                                "target_lang_sense_clarification": "in 'cool customer' (calm person)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "487ae5c7-b0b4-36d9-8565-c45fb7a93de3",
                    "sense_definition": "Een dubbele haak om een touw aan vast te maken, vooral gebruikt op boten.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nautical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De matroos maakte de tros vast aan de <w>kikker</w>.",
                            "target_lang_translations": {
                                "en": "The sailor fastened the hawser to the <w>cleat</w>."
                            }
                        },
                        {
                            "text": "Zorg dat het touw stevig om de <w>kikker</w> zit.",
                            "target_lang_translations": {
                                "en": "Make sure the rope is securely around the <w>cleat</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A double hook to fasten a rope to, especially used on boats."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cleat",
                                "target_lang_sense_clarification": "nautical device"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90585a97-af45-33e8-baa4-9acc7490466e",
                    "sense_definition": "Een lid van een specifieke jongerencultuur (beatniks) in Den Haag in de jaren zestig.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "history",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de jaren zestig was hij een bekende <w>kikker</w> in de stad.",
                            "target_lang_translations": {
                                "en": "In the sixties, he was a well-known <w>beatnik</w> in the city."
                            }
                        },
                        {
                            "text": "De <w>kikkers</w> kwamen vaak in conflict met andere groepen jongeren.",
                            "target_lang_translations": {
                                "en": "The <w>beatniks</w> often came into conflict with other groups of young people."
                            }
                        }
                    ],
                    "traits": [
                        {

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2bac6512-5a8d-3945-93ed-c3a9a678c3fc",
                    "sense_definition": "Osoba, która pływa na statku wodnym, zwłaszcza żaglowym, zawodowo, sportowo lub rekreacyjnie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "person_sailing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój syn został <w>żeglarzem</w> i wypłynął w długi rejs po oceanie.",
                            "target_lang_translations": {
                                "ru": "Мой сын стал <w>мореплавателем</w> и отправился в долгое плавание по океану."
                            }
                        },
                        {
                            "text": "Każdy doświadczony <w>żeglarz</w> potrafi przewidzieć zmianę pogody na podstawie obserwacji nieba.",
                            "target_lang_translations": {
                                "ru": "Каждый опытный <w>моряк</w> может предсказать изменение погоды, наблюдая за небом."
                            }
                        }
                    ],
                    "synonyms": [
                        "marynarz"
                    ],
                    "common_phrases": [
                        "doświadczony żeglarz",
                        "samotny żeglarz",
                        "klub żeglarza",
                        "węzeł żeglarza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который плавает на судне, особенно парусном, профессионально, как спортсмен или для отдыха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "яхтсмен",
                                "target_lang_sense_clarification": "спортсмен"
                            },
                            {
                                "target_lang_word": "мореплаватель",
                                "target_lang_sense_clarification": "путешественник"
                            },
                            {
                                "target_lang_word": "моряк",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58436e62-9820-3365-b87e-1b2df24dc575",
                    "sense_definition": "Duży motyl z rodziny paziowatych o jasnych skrzydłach z czarnymi pręgami; paź żeglarz.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal_butterfly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W naszym ogrodzie na kwiatach usiadł rzadki paź <w>żeglarz</w>.",
                            "target_lang_translations": {
                                "ru": "В нашем саду на цветы сел редкий парусник <w>подалирий</w>."
                            }
                        },
                        {
                            "text": "Ten gatunek motyla, znany jako <w>żeglarz</w>, jest objęty ochroną gatunkową.",
                            "target_lang_translations": {
                                "ru": "Этот вид бабочки, известный как <w>парусник</w>, находится под охраной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "paź żeglarz"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупная бабочка из семейства парусников со светлыми крыльями и черными полосами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "парусник",
                                "target_lang_sense_clarification": "бабочка"
                            },
                            {
                                "target_lang_word": "подалирий",
                                "target_lang_sense_clarification": "вид бабочки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e901389a-bc3a-3d08-92bf-8864b025179a",
                    "sense_definition": "Parzydełkowiec morski z charakterystycznym pęcherzem pławnym unoszącym się na powierzchni wody; żeglarz portugalski.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal_hydrozoan",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Poparzenie przez <w>żeglarza</w> portugalskiego może być bardzo niebezpieczne dla człowieka.",
                            "target_lang_translations": {
                                "ru": "Ожог от <w>португальского кораблика</w> может быть очень опасен для человека."
                            }
                        },
                        {
                            "text": "Fale wyrzuciły na plażę martwego <w>żeglarza</w>, którego omijali turyści.",
                            "target_lang_translations": {
                                "ru": "Волны выбросили на пляж мертвого <w>португальского кораблика</w>, которого обходили туристы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "żeglarz portugalski"
                    ],
                    "target_lang_definitions": {
                        "ru": "Морское стрекающее с характерным плавательным пузырем, плавающим на поверхности воды; португальский кораблик."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "португальский кораблик",
                                "target_lang_sense_clarification": "сифонофора"
                            }
                        ]
                    }
                }
            ]
        }
    ]

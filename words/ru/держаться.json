    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "874798d0-2d86-32fc-98ad-da3180a2b4ac",
                    "sense_definition": "Сохранять равновесие или положение, ухватившись за что-либо руками.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_hold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В автобусе нужно обязательно <держаться> за поручень."
                        },
                        {
                            "text": "Ребёнок крепко <держался> за мамину руку."
                        }
                    ],
                    "synonyms": [
                        "сохраняться"
                    ],
                    "antonyms": [
                        "падать",
                        "отпускать"
                    ],
                    "common_phrases": [
                        "держаться за руки",
                        "держаться за перила",
                        "крепко держаться"
                    ]
                },
                {
                    "sense_id": "a50cb323-cffe-3417-8143-5997e4481356",
                    "sense_definition": "Вести себя определённым образом, иметь определённый вид или осанку.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На собеседовании он <держался> очень уверенно и спокойно."
                        },
                        {
                            "text": "Несмотря на усталость, солдат <держался> прямо."
                        }
                    ],
                    "synonyms": [
                        "вести себя"
                    ],
                    "common_phrases": [
                        "держаться уверенно",
                        "держаться молодцом",
                        "держаться скромно",
                        "держаться прямо"
                    ]
                },
                {
                    "sense_id": "e2a865ed-18ac-357d-ba26-cf18b207f28c",
                    "sense_definition": "Находиться в определённом месте, не отходить далеко или следовать определённой стороне при движении.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location_direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Во время экскурсии прошу всех <держаться> вместе."
                        },
                        {
                            "text": "Водитель должен <держаться> правой стороны дороги."
                        }
                    ],
                    "synonyms": [
                        "оставаться",
                        "придерживаться"
                    ],
                    "common_phrases": [
                        "держаться вместе",
                        "держаться в стороне",
                        "держаться правой стороны",
                        "держаться берега"
                    ]
                },
                {
                    "sense_id": "e1af116b-ca78-3221-a1d6-b57eb39d8d4c",
                    "sense_definition": "Сохранять силу духа, не сдаваться в трудной ситуации; оказывать сопротивление.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "resilience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Враг наступает, но наши войска продолжают <держаться>."
                        },
                        {
                            "text": "Ты должен <держаться> и не падать духом ради семьи."
                        }
                    ],
                    "synonyms": [
                        "крепиться",
                        "сопротивляться",
                        "сохраняться"
                    ],
                    "antonyms": [
                        "сдаваться"
                    ],
                    "common_phrases": [
                        "держись",
                        "держаться до последнего",
                        "держаться из последних сил"
                    ]
                },
                {
                    "sense_id": "6d4097c1-32e9-3bb6-bd60-2bc0aa886904",
                    "sense_definition": "Продолжать существовать, не исчезать и не меняться (о погоде, состоянии, запахе и т.п.).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хорошая погода <держится> уже целую неделю."
                        },
                        {
                            "text": "Высокая температура у больного <держалась> три дня."
                        }
                    ],
                    "synonyms": [
                        "сохраняться",
                        "оставаться"
                    ],
                    "antonyms": [
                        "исчезать",
                        "проходить"
                    ],
                    "common_phrases": [
                        "погода держится",
                        "мороз держится",
                        "запах держится"
                    ]
                },
                {
                    "sense_id": "2e070417-fa18-355f-b872-fbd5a88ab107",
                    "sense_definition": "Следовать чему-либо (правилам, советам), не отступать от своих принципов или мнений.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "adherence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я предпочитаю <держаться> своего мнения в этом вопросе."
                        },
                        {
                            "text": "Чтобы похудеть, нужно строго <держаться> диеты."
                        }
                    ],
                    "synonyms": [
                        "придерживаться",
                        "поддерживаться"
                    ],
                    "antonyms": [
                        "нарушать"
                    ],
                    "common_phrases": [
                        "держаться курса",
                        "держаться правил",
                        "держаться мнения"
                    ]
                },
                {
                    "sense_id": "77235aed-1385-3476-ac89-8fdeb035c17f",
                    "sense_definition": "Быть прикреплённым к чему-либо, не падать благодаря опоре или креплению.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_hold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старая картина <держится> на стене на одном гвозде."
                        },
                        {
                            "text": "Пуговица на пальто еле <держится>."
                        }
                    ],
                    "synonyms": [
                        "крепиться",
                        "сохраняться"
                    ],
                    "antonyms": [
                        "отваливаться"
                    ],
                    "common_phrases": [
                        "держаться на клею",
                        "держаться на честном слове",
                        "еле держаться"
                    ]
                }
            ]
        }
    ]

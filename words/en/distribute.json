{
    "word_family": [
        "distribution",
        "distributor",
        "distributive",
        "redistribution",
        "redistribute"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2930ab32-0ce2-3125-a9f0-d7fa8348953a",
                    "sense_definition": "To give things to a large number of people, such as leaflets, forms, or information.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "give_out",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The volunteers <w>distributed</w> food and water to the homeless people in the park.",
                            "target_lang_translations": {
                                "ru": "Волонтеры <w>раздавали</w> еду и воду бездомным в парке."
                            }
                        },
                        {
                            "text": "Before the exam started, the teacher <w>distributed</w> the question papers to all the students.",
                            "target_lang_translations": {
                                "ru": "Перед началом экзамена учитель <w>раздал</w> бланки с заданиями всем студентам."
                            }
                        }
                    ],
                    "synonyms": [
                        "give out",
                        "issue",
                        "deal out",
                        "dispense"
                    ],
                    "antonyms": [
                        "gather"
                    ],
                    "common_phrases": [
                        "distribute leaflets",
                        "distribute copies",
                        "distribute information"
                    ],
                    "target_lang_definitions": {
                        "ru": "Раздавать вещи большому количеству людей, например листовки, бланки или информацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раздавать",
                                "target_lang_sense_clarification": "выдавать многим людям"
                            },
                            {
                                "target_lang_word": "распространять",
                                "target_lang_sense_clarification": "информацию или печатные материалы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d5c060b-4099-3582-938c-de90d39c6075",
                    "sense_definition": "To divide something into portions and share it among several people, groups, or places.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "share",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government plans to <w>distribute</w> the funds equally among the local charities.",
                            "target_lang_translations": {
                                "ru": "Правительство планирует поровну <w>распределить</w> средства между местными благотворительными организациями."
                            }
                        },
                        {
                            "text": "We need to <w>distribute</w> the workload so that no one is overwhelmed.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>распределить</w> рабочую нагрузку так, чтобы никто не был перегружен."
                            }
                        }
                    ],
                    "synonyms": [
                        "share out",
                        "apportion",
                        "allot",
                        "allocate",
                        "divide",
                        "portion out"
                    ],
                    "antonyms": [
                        "amass",
                        "gather"
                    ],
                    "common_phrases": [
                        "distribute wealth",
                        "distribute resources",
                        "evenly distributed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делить что-либо на части и распределять между несколькими людьми, группами или местами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распределять",
                                "target_lang_sense_clarification": "ресурсы, обязанности или нагрузку"
                            },
                            {
                                "target_lang_word": "делить",
                                "target_lang_sense_clarification": "между кем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef8bfca2-83c2-3944-8980-fcf7b9b4e3fc",
                    "sense_definition": "To supply goods to shops, businesses, or customers for sale or use.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "commercial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company manufactures the toys in China and <w>distributes</w> them across Europe.",
                            "target_lang_translations": {
                                "ru": "Компания производит игрушки в Китае и <w>распространяет</w> их по всей Европе."
                            }
                        },
                        {
                            "text": "A large network of trucks is used to <w>distribute</w> fresh produce to supermarkets daily.",
                            "target_lang_translations": {
                                "ru": "Огромная сеть грузовиков используется, чтобы ежедневно <w>развозить</w> свежие продукты по супермаркетам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "distribute goods",
                        "distribute products",
                        "exclusively distributed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поставлять товары в магазины, предприятия или клиентам для продажи или использования."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распространять",
                                "target_lang_sense_clarification": "товары или продукцию"
                            },
                            {
                                "target_lang_word": "распределять",
                                "target_lang_sense_clarification": "в логистике и снабжении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a26c5191-601c-312a-a52d-274383800c2b",
                    "sense_definition": "To spread or scatter something over an area or space.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "spread",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The machine <w>distributes</w> the seeds evenly over the field to ensure good growth.",
                            "target_lang_translations": {
                                "ru": "Машина равномерно <w>распределяет</w> семена по полю для обеспечения хорошего роста."
                            }
                        },
                        {
                            "text": "Make sure the weight is <w>distributed</w> on all four wheels to keep the vehicle stable.",
                            "target_lang_translations": {
                                "ru": "Убедитесь, что вес <w>распределен</w> на все четыре колеса для сохранения устойчивости автомобиля."
                            }
                        }
                    ],
                    "synonyms": [
                        "disperse",
                        "strew"
                    ],
                    "antonyms": [
                        "concentrate"
                    ],
                    "common_phrases": [
                        "distribute weight",
                        "distribute the load",
                        "widely distributed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рассеивать или размещать что-либо по какой-либо площади или пространству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распределять",
                                "target_lang_sense_clarification": "вес, нагрузку или вещество по поверхности"
                            },
                            {
                                "target_lang_word": "разбрасывать",
                                "target_lang_sense_clarification": "раскидывать в разные стороны"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cbb1dad1-3d4b-3361-884f-714483eee0b0",
                    "sense_definition": "To classify or separate items into specific categories.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "categorize",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The software <w>distributes</w> incoming emails into different folders based on the sender.",
                            "target_lang_translations": {
                                "ru": "Программа <w>распределяет</w> входящие письма по разным папкам в зависимости от отправителя."
                            }
                        },
                        {
                            "text": "Scientists <w>distributed</w> the samples into groups for further analysis.",
                            "target_lang_translations": {
                                "ru": "Ученые <w>распределили</w> образцы по группам для дальнейшего анализа."
                            }
                        }
                    ],
                    "synonyms": [
                        "categorize",
                        "classify",
                        "sort"
                    ],
                    "target_lang_definitions": {
                        "ru": "Классифицировать или разделять предметы по определенным категориям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распределять",
                                "target_lang_sense_clarification": "по группам или категориям"
                            },
                            {
                                "target_lang_word": "разделять",
                                "target_lang_sense_clarification": "сортировать"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
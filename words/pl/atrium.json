            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a328ee2f-48c1-3f73-84b4-256e709dabdf",
                    "sense_definition": "Przestronne, reprezentacyjne pomieszczenie wewnątrz budynku, często przeszklone od góry i obejmujące kilka kondygnacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wchodząc do hotelu, zachwyciliśmy się jego ogromnym, jasnym <w>atrium</w>.",
                            "target_lang_translations": {
                                "ru": "Войдя в отель, мы были восхищены его огромным, светлым <w>атриумом</w>."
                            }
                        },
                        {
                            "text": "Nowoczesne biurowce często posiadają <w>atrium</w> pełne zieleni, które służy jako miejsce spotkań.",
                            "target_lang_translations": {
                                "ru": "Современные офисные здания часто имеют <w>атриум</w>, полный зелени, который служит местом встреч."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przeszklone atrium",
                        "atrium hotelowe",
                        "atrium biurowca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Просторное помещение внутри современного здания, часто с верхним светом и высотой в несколько этажей"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "атриум",
                                "target_lang_sense_clarification": "современная архитектура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88a7aae1-8dbe-3fe1-a8d6-b5431b351475",
                    "sense_definition": "Główne, centralne pomieszczenie w starożytnym domu rzymskim, zazwyczaj z otworem w dachu i zbiornikiem na wodę deszczową.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prokonsul rozmawiał w <w>atrium</w> z senatorem.",
                            "target_lang_translations": {
                                "ru": "Проконсул беседовал в <w>атриуме</w> с сенатором."
                            }
                        },
                        {
                            "text": "Bogaci Rzymianie przyjmowali swoich gości w reprezentacyjnym <w>atrium</w>.",
                            "target_lang_translations": {
                                "ru": "Богатые римляне принимали гостей в парадном <w>атриуме</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rzymskie atrium",
                        "atrium domu"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "starożytność"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Центральная часть древнеримского дома, внутренний двор с отверстием в крыше"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "атриум",
                                "target_lang_sense_clarification": "античная архитектура"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f811f59e-d825-3d79-910b-cdc2ff443486",
                    "sense_definition": "Jedna z dwóch górnych jam serca, do której napływa krew z żył.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Krew z żył głównych wpływa bezpośrednio do prawego <w>atrium</w>.",
                            "target_lang_translations": {
                                "ru": "Кровь из полых вен поступает прямо в правое <w>предсердие</w>."
                            }
                        },
                        {
                            "text": "Skurcz <w>atrium</w> powoduje przepchnięcie krwi do komory serca.",
                            "target_lang_translations": {
                                "ru": "Сокращение <w>предсердия</w> проталкивает кровь в желудочек сердца."
                            }
                        }
                    ],
                    "synonyms": [
                        "przedsionek"
                    ],
                    "common_phrases": [
                        "lewe atrium",
                        "prawe atrium"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одна из камер сердца, принимающая кровь из вен"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предсердие",
                                "target_lang_sense_clarification": "анатомия"
                            }
                        ]
                    }
                }
            ]
        }
    ]

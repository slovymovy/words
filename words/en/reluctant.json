            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "dd883636-9c0a-3a61-9b79-0f31c3cb2162",
                    "sense_definition": "Unwilling and hesitant to do something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was <w>reluctant</w> to lend him the money.",
                            "target_lang_translations": {
                                "ru": "Она <w>неохотно</w> одолжила ему деньги."
                            }
                        },
                        {
                            "text": "The <w>reluctant</w> student finally raised his hand to answer the question.",
                            "target_lang_translations": {
                                "ru": "Ученик <w>с неохотой</w> наконец поднял руку, чтобы ответить на вопрос."
                            }
                        }
                    ],
                    "synonyms": [
                        "disinclined",
                        "loath",
                        "grudging"
                    ],
                    "common_phrases": [
                        "reluctant to admit",
                        "reluctant agreement",
                        "reluctant hero"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не желающий что-либо делать, колеблющийся; совершаемый без желания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неохотный",
                                "target_lang_sense_clarification": "проявляющий нежелание или совершаемый без охоты"
                            },
                            {
                                "target_lang_word": "несклонный",
                                "target_lang_sense_clarification": "не имеющий расположения к чему-либо"
                            },
                            {
                                "target_lang_word": "вынужденный",
                                "target_lang_sense_clarification": "совершаемый в силу обстоятельств, а не по желанию (напр. вынужденный герой)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e3637a15-3aac-31b4-a048-c20d8db00a80",
                    "sense_definition": "Describing a quantifier in regular expressions that matches as few characters as possible.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {

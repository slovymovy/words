                    "frequency": "High",
                    "semantic_group_id": "time_unit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Поезд отправится ровно через одну <w>минуту</w>.",
                            "target_lang_translations": {
                                "en": "The train will depart in exactly one <w>minute</w>."
                            }
                        },
                        {
                            "text": "В одном часе содержится шестьдесят <w>минут</w>.",
                            "target_lang_translations": {
                                "en": "There are sixty <w>minutes</w> in one hour."
                            }
                        }
                    ],
                    "common_phrases": [
                        "пять минут",
                        "через минуту",
                        "каждую минуту",
                        "тридцать минут"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "616b3b28-076c-34fe-a013-c5464f3b96a8",
                    "sense_definition": "Program komputerowy wykonujący automatycznie powtarzalne czynności w zastępstwie człowieka, często w internecie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "computer_program",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten <w>bot</w> potrafi automatycznie odpowiadać na proste pytania klientów.",
                            "target_lang_translations": {
                                "ru": "Этот <w>бот</w> может автоматически отвечать на простые вопросы клиентов."
                            }
                        },
                        {
                            "text": "Musisz udowodnić, że nie jesteś <w>botem</w>, przepisując kod z obrazka.",
                            "target_lang_translations": {
                                "ru": "Вы должны доказать, что вы не <w>бот</w>, переписав код с картинки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bot internetowy",
                        "atak botów",
                        "rozmowa z botem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компьютерная программа, которая автоматически выполняет повторяющиеся задачи вместо человека, часто в интернете."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бот",
                                "target_lang_sense_clarification": "программа"
                            },
                            {
                                "target_lang_word": "робот",
                                "target_lang_sense_clarification": "автоматическая система"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45fcf354-05f6-3e60-86ad-5357a6ae34fe",
                    "sense_definition": "Rodzaj buta z wyższą cholewką, noszonego zazwyczaj przez kobiety lub dzieci.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "footwear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na zimę kupiła sobie ciepłe, skórzane <w>boty</w>.",
                            "target_lang_translations": {
                                "ru": "На зиму она купила себе теплые кожаные <w>полусапожки</w>."
                            }
                        },
                        {
                            "text": "Jeden <w>bot</w> okazał się za ciasny i musiała go wymienić.",
                            "target_lang_translations": {
                                "ru": "Один <w>ботинок</w> оказался слишком тесным, и ей пришлось его поменять."
                            }
                        }
                    ],
                    "synonyms": [
                        "botek"
                    ],
                    "common_phrases": [
                        "skórzane boty",
                        "zimowe boty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид обуви с высоким голенищем, которую обычно носят женщины или дети."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полусапожок",
                                "target_lang_sense_clarification": "женская обувь"
                            },
                            {
                                "target_lang_word": "ботинок",
                                "target_lang_sense_clarification": "обувь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3d8c92c-153f-30a1-a3fa-f4b236ba3219",
                    "sense_definition": "Dawna forma słowa <w>but</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szlachcic nosił przy pasie szablę, a na nodze wysoki <w>bot</w>.",
                            "target_lang_translations": {
                                "ru": "Шляхтич носил у пояса саблю, а на ноге — высокий <w>сапог</w>."
                            }
                        },
                        {
                            "text": "W starym tekście przeczytaliśmy o cenie za jeden <w>bot</w>.",
                            "target_lang_translations": {
                                "ru": "В старом тексте мы прочитали о цене за один <w>сапог</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "używane w języku staropolskim"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшая форма слова «сапог» или «ботинок»."
                    },
                    "translations": {

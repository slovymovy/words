                {
                    "sense_id": "cf0e0575-46f0-3889-9f4d-baf244482e82",
                    "sense_definition": "Een specifiek gebied van kennis, werk of interesse.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "expertise",
                    "name_type": "no",
                    "examples": [

    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "a0ab64e7-0735-3147-9223-b156433d07ea",
                    "sense_definition": "Used to indicate that one did not hear or understand what was said.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "request_repetition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Huh</w>? Could you please speak a little louder?",
                            "target_lang_translations": {
                                "ru": "<w>А</w>? Не могли бы вы говорить немного громче?"
                            }
                        },
                        {
                            "text": "I am sorry, <w>huh</w>? I missed the last part.",
                            "target_lang_translations": {
                                "ru": "Прошу прощения, <w>что</w>? Я пропустил последнюю часть."
                            }
                        }
                    ],
                    "synonyms": [
                        "pardon",
                        "what",
                        "come again"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется, чтобы показать, что человек не расслышал или не понял сказанное."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "а",
                                "target_lang_sense_clarification": "разговорное, переспрос"
                            },
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "вопрос при непонимании"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b43ee27-ee63-3fe4-a5c3-5db958974fd0",
                    "sense_definition": "Used at the end of a sentence to ask for confirmation or agreement.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "tag_question",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is getting really late, <w>huh</w>?",
                            "target_lang_translations": {
                                "ru": "Уже очень поздно, <w>да</w>?"
                            }
                        },
                        {
                            "text": "You are pretty good at this game, <w>huh</w>?",
                            "target_lang_translations": {
                                "ru": "Ты неплохо играешь в эту игру, <w>а</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "pardon"
                    ],
                    "common_phrases": [
                        "right huh"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в конце предложения, чтобы спросить подтверждения или согласия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "вопрос для подтверждения"
                            },
                            {
                                "target_lang_word": "а",
                                "target_lang_sense_clarification": "разговорное, побуждение к ответу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90cd4bba-d0d1-3929-9419-02080e41a45e",
                    "sense_definition": "Used to express doubt, confusion, or surprise.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emotional_reaction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Huh</w>? I thought you said you were staying home.",
                            "target_lang_translations": {
                                "ru": "<w>Что</w>? Я думал, ты сказал, что останешься дома."
                            }
                        },
                        {
                            "text": "<w>Huh</w>? Where did everybody go so suddenly?",
                            "target_lang_translations": {
                                "ru": "<w>А</w>? Куда все так внезапно подевались?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common in spoken English"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения сомнения, замешательства или удивления."

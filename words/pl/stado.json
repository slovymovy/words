            "senses": [
                {
                    "sense_id": "b97cd3c0-ec86-3848-b90e-7fdbdc3dce71",
                    "sense_definition": "Grupa zwierząt tego samego gatunku żyjąca razem na określonym terenie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na zielonej łące pasło się duże <w>stado</w> krów.",
                            "target_lang_translations": {
                                "ru": "На зелёном лугу паслось большое <w>стадо</w> коров."
                            }
                        },
                        {
                            "text": "W lesie można czasem spotkać <w>stado</w> dzikich zwierząt.",
                            "target_lang_translations": {
                                "ru": "В лесу иногда можно встретить <w>стадо</w> диких животных."
                            }
                        }
                    ],
                    "synonyms": [
                        "gromada",
                        "grupa"
                    ],
                    "common_phrases": [
                        "stado wilków",
                        "stado owiec",
                        "stado krów",
                        "stado dzików"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа животных одного вида, живущих вместе на определенной территории."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стадо",
                                "target_lang_sense_clarification": "для копытных животных: коров, овец, свиней"
                            },
                            {
                                "target_lang_word": "стая",
                                "target_lang_sense_clarification": "для хищников, птиц, рыб: волков, уток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa879a36-c390-375d-aea0-9ec99b9a9620",
                    "sense_definition": "Duża grupa ludzi, często zachowująca się głośno lub chaotycznie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "people_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To <w>stado</w> krzyczących demonstrantów działało mi na nerwy.",
                            "target_lang_translations": {
                                "ru": "Это <w>стадо</w> кричащих демонстрантов действовало мне на нервы."
                            }
                        },
                        {
                            "text": "Do sklepu wpadło nagle całe <w>stado</w> głośnych turystów.",
                            "target_lang_translations": {
                                "ru": "В магазин внезапно ввалилось целое <w>стадо</w> шумных туристов."
                            }
                        }
                    ],
                    "synonyms": [
                        "tłum",
                        "chmara",
                        "horda",
                        "gromada"
                    ],
                    "common_phrases": [
                        "stado ludzi",
                        "stado dzieci",
                        "stado turystów"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często o dużej grupie ludzi"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая группа людей, часто ведущая себя шумно или хаотично."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стадо",
                                "target_lang_sense_clarification": "пренебрежительно, о неорганизованной толпе"
                            },
                            {
                                "target_lang_word": "толпа",
                                "target_lang_sense_clarification": "большое скопление людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2349d85-6d7f-3d79-b797-83d388bb2a0e",
                    "sense_definition": "Duże skupisko podobnych do siebie przedmiotów lub zjawisk.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "objects_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wysoko na niebie ułożyły się <w>stada</w> lekkich chmurek.",
                            "target_lang_translations": {
                                "ru": "Высоко в небе собрались <w>стаи</w> лёгких облачков."
                            }
                        },
                        {
                            "text": "Na parkingu przed fabryką stało <w>stado</w> nowych samochodów.",
                            "target_lang_translations": {

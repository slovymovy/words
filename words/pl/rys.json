                },
                {
                    "sense_id": "f445f4c5-9770-3d6d-8589-4261c9b6a2bf",
                    "sense_definition": "Forma (dopełniacz liczby mnogiej) rzeczownika <w>rysa</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na szklanym stole nie zauważyłem żadnych <w>rys</w>.",

                },
                {
                    "sense_id": "8c9a4f98-a4da-359c-829d-7d81a5e96591",
                    "sense_definition": "Jednostka wagi stosowana na terenie starożytnego Bliskiego Wschodu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Archeolodzy znaleźli odważnik o wadze jednej <beki>.",
                            "target_lang_translations": {
                                "ru": "Археологи нашли гирьку весом в одну <w>беку</w>."
                            }
                        },
                        {
                            "text": "W starych tekstach wspomniano o ofierze ważącej pół <beki>.",
                            "target_lang_translations": {
                                "ru": "В старых текстах упоминалось о жертве весом в пол-<w>беки</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historyczny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Единица веса, использовавшаяся на древнем Ближнем Востоке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бека",
                                "target_lang_sense_clarification": "историческая мера веса"

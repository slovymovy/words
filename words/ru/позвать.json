    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b3726b4f-d961-3ead-9d15-48a3283d95ba",
                    "sense_definition": "Громко произнести имя или обратиться к кому-либо голосом или жестом, чтобы привлечь внимание или попросить подойти.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "— Мама, — слабо <w>позвал</w> он и тронул её за плечо.",
                            "target_lang_translations": {
                                "en": "\"Mom,\" he <w>called</w> weakly and touched her on the shoulder."
                            }
                        },
                        {
                            "text": "Заметив проходящего человека, он громко <w>позвал</w> его на помощь.",
                            "target_lang_translations": {
                                "en": "Noticing a person passing by, he loudly <w>called</w> him for help."
                            }
                        }
                    ],
                    "common_phrases": [
                        "позвать на помощь",
                        "позвать врача",
                        "позвать маму"
                    ],
                    "target_lang_definitions": {
                        "en": "To say a name loudly or address someone by voice or gesture to attract attention or ask them to come closer."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "call",
                                "target_lang_sense_clarification": "to cry out or summon"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9a2cd57-e27b-3507-9b49-27a686d2d673",
                    "sense_definition": "Предложить кому-либо прийти куда-либо, посетить какое-то место или принять участие в чём-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Когда режиссёр <w>позвал</w> его на главную роль, он сразу согласился.",
                            "target_lang_translations": {
                                "en": "When the director <w>invited</w> him for the lead role, he agreed immediately."
                            }
                        },
                        {
                            "text": "Я хочу <w>позвать</w> своих школьных друзей в гости на выходные.",
                            "target_lang_translations": {
                                "en": "I want to <w>invite</w> my school friends over for the weekend."
                            }
                        }
                    ],
                    "synonyms": [
                        "пригласить"
                    ],
                    "common_phrases": [
                        "позвать в гости",
                        "позвать замуж",
                        "позвать на свидание",
                        "позвать в кино"
                    ],
                    "target_lang_definitions": {
                        "en": "To suggest that someone come somewhere, visit a place, or participate in something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "invite",
                                "target_lang_sense_clarification": "to ask someone to come"
                            },
                            {
                                "target_lang_word": "ask",
                                "target_lang_sense_clarification": "to invite"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
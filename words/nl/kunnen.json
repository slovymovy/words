{
    "word_family": [
        "kunde",
        "kundig",
        "aankunnen",
        "verkennen"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3f5b0e23-0231-3412-80c1-179e6152fd16",
                    "sense_definition": "Het vermogen of de vaardigheid hebben om iets te doen; in staat zijn tot iets.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "ability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <kon> heel hard rennen omdat hij veel had getraind.",
                            "target_lang_translations": {
                                "en": "He <could> run very fast because he had trained a lot.",
                                "ru": "Он <мог> очень быстро бегать, потому что много тренировался."
                            }
                        },
                        {
                            "text": "Ik <kan> helaas geen piano spelen.",
                            "target_lang_translations": {
                                "en": "Unfortunately, I <cannot> play the piano.",
                                "ru": "К сожалению, я не <умею> играть на пианино."
                            }
                        },
                        {
                            "text": "Zij <kan> heel goed koken voor grote groepen mensen.",
                            "target_lang_translations": {
                                "en": "She <can> cook very well for large groups of people.",
                                "ru": "Она <умеет> очень хорошо готовить для больших групп людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "vermogen"
                    ],
                    "common_phrases": [
                        "goed kunnen",
                        "niet kunnen"
                    ],
                    "target_lang_definitions": {
                        "en": "To have the ability or skill to do something; to be able to.",
                        "ru": "Иметь способность или навык что-то делать; быть в состоянии."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "can",
                                "target_lang_sense_clarification": "general ability or skill"
                            },
                            {
                                "target_lang_word": "be able to",
                                "target_lang_sense_clarification": "specific capacity or ability"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "мочь",
                                "target_lang_sense_clarification": "физическая возможность или способность"
                            },
                            {
                                "target_lang_word": "уметь",
                                "target_lang_sense_clarification": "навык или знание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90f7e18a-043e-3594-a1aa-75fa0deea2fb",
                    "sense_definition": "Mogelijk zijn; er bestaat een kans dat iets gebeurt of waar is.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "possibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <kan> zijn dat de trein vertraging heeft.",
                            "target_lang_translations": {
                                "en": "It <is possible> that the train is delayed.",
                                "ru": "<Может> быть, поезд опаздывает."
                            }
                        },
                        {
                            "text": "Dat <kan> echt niet waar zijn.",
                            "target_lang_translations": {
                                "en": "That really <cannot> be true.",
                                "ru": "Это действительно не <может> быть правдой."
                            }
                        },
                        {
                            "text": "Het <kan> morgen gaan regenen.",
                            "target_lang_translations": {
                                "en": "It <could> rain tomorrow.",
                                "ru": "Завтра <может> пойти дождь."
                            }
                        }
                    ],
                    "synonyms": [
                        "mogelijk zijn"
                    ],
                    "common_phrases": [
                        "dat kan",
                        "het kan zijn",
                        "alles kan"
                    ],
                    "target_lang_definitions": {
                        "en": "To be possible; there is a chance that something happens or is true.",
                        "ru": "Быть возможным; вероятность того, что что-то произойдет или является правдой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "can",
                                "target_lang_sense_clarification": "possibility or potential"
                            },
                            {
                                "target_lang_word": "be possible",
                                "target_lang_sense_clarification": "existence of a possibility"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "мочь",
                                "target_lang_sense_clarification": "вероятность"
                            },
                            {
                                "target_lang_word": "быть возможным",
                                "target_lang_sense_clarification": "возможность события"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "112a1589-a859-3a4a-bd20-c6bce320dc2d",
                    "sense_definition": "Toestemming hebben of de vrijheid hebben om iets te doen; mogen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je <kunt> me altijd bellen als je vragen hebt.",
                            "target_lang_translations": {
                                "en": "You <can> always call me if you have questions.",
                                "ru": "Ты всегда <можешь> позвонить мне, если у тебя есть вопросы."
                            }
                        },
                        {
                            "text": "U <kunt> hier betalen met uw pinpas.",
                            "target_lang_translations": {
                                "en": "You <can> pay here with your debit card.",
                                "ru": "Вы <можете> заплатить здесь картой."
                            }
                        },
                        {
                            "text": "Je <kunt> nu gaan als je klaar bent.",
                            "target_lang_translations": {
                                "en": "You <can> go now if you are ready.",
                                "ru": "Ты <можешь> идти, если готов."
                            }
                        }
                    ],
                    "synonyms": [
                        "mogen"
                    ],
                    "common_phrases": [
                        "je kunt beter"
                    ],
                    "target_lang_definitions": {
                        "en": "To have permission or the freedom to do something; to be allowed.",
                        "ru": "Иметь разрешение или свободу действий; иметь возможность."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "can",
                                "target_lang_sense_clarification": "permission (informal/neutral)"
                            },
                            {
                                "target_lang_word": "be allowed to",
                                "target_lang_sense_clarification": "permission"
                            },
                            {
                                "target_lang_word": "may",
                                "target_lang_sense_clarification": "permission (formal)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "мочь",
                                "target_lang_sense_clarification": "разрешение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ac003fd-2bb5-3df8-a859-267035a19195",
                    "sense_definition": "Beschikbaar zijn; tijd of gelegenheid hebben voor een afspraak.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "availability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <kan> morgenavond niet omdat ik moet werken.",
                            "target_lang_translations": {
                                "en": "I <can't> make it tomorrow evening because I have to work.",
                                "ru": "Я не <могу> завтра вечером, потому что должен работать."
                            }
                        },
                        {
                            "text": "Wanneer <kun> je afspreken voor een vergadering?",
                            "target_lang_translations": {
                                "en": "When <can> you meet for a meeting?",
                                "ru": "Когда ты <можешь> встретиться для совещания?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "ik kan niet"
                    ],
                    "target_lang_definitions": {
                        "en": "To be available; to have time or opportunity for an appointment.",
                        "ru": "Быть свободным; иметь время или возможность для встречи."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "can",
                                "target_lang_sense_clarification": "availability"
                            },
                            {
                                "target_lang_word": "be free",
                                "target_lang_sense_clarification": "having time"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "мочь",
                                "target_lang_sense_clarification": "наличие времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "94888d30-a6e0-3d3b-9230-7e8d43d8fd16",
                    "sense_definition": "Ergens naartoe kunnen gaan of komen (waarbij het werkwoord 'gaan' of 'komen' vaak is weggelaten).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik ben mijn sleutel vergeten en <kan> mijn huis niet meer in.",
                            "target_lang_translations": {
                                "en": "I forgot my key and <can't> get into my house anymore.",
                                "ru": "Я забыл ключ и больше не <могу> попасть домой."
                            }
                        },
                        {
                            "text": "We <kunnen> naar huis, want het feest is afgelopen.",
                            "target_lang_translations": {
                                "en": "We <can> go home, because the party is over.",
                                "ru": "Мы <можем> пойти домой, так как вечеринка закончилась."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To be able to go or come somewhere (often with the motion verb omitted).",
                        "ru": "Иметь возможность пойти или войти куда-либо (часто с опущенным глаголом движения)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "can",
                                "target_lang_sense_clarification": "movement/direction implied"
                            },
                            {
                                "target_lang_word": "be able to",
                                "target_lang_sense_clarification": "movement/direction implied"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "мочь",
                                "target_lang_sense_clarification": "возможность движения/перемещения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "02e3b280-61f5-3af2-9252-908c38e79e4c",
                    "sense_definition": "Meervoud van <kunne>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In oude teksten wordt soms gesproken over de beide <kunnen>.",
                            "target_lang_translations": {
                                "en": "Old texts sometimes speak of both <sexes>.",
                                "ru": "В старых текстах иногда говорится об обоих <полах>."
                            }
                        },
                        {
                            "text": "Het onderscheid tussen de <kunnen> is in deze context niet relevant.",
                            "target_lang_translations": {
                                "en": "The distinction between the <sexes> is not relevant in this context.",
                                "ru": "Различие между <полами> в данном контексте неактуально."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of 'kunne' (archaic: sex or gender).",
                        "ru": "Множественное число от 'kunne' (пол, род)."
                    },
                    "translations": {
                        "en": [],
                        "ru": [
                            {
                                "target_lang_word": "полы",
                                "target_lang_sense_clarification": "биологический пол (устар.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
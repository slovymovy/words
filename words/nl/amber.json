                {
                    "sense_id": "bd58edcd-986c-3b9f-befc-c27298f860e1",
                    "sense_definition": "Fossiele hars van naaldbomen, vaak geel of oranje van kleur, die als edelsteen wordt gebruikt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "resin_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het museum zagen we een insect dat in <w>amber</w> bewaard was gebleven.",
                            "target_lang_translations": {
                                "en": "In the museum, we saw an insect that had been preserved in <w>amber</w>.",
                                "ru": "В музее мы видели насекомое, сохранившееся в <w>янтаре</w>."
                            }
                        },
                        {
                            "text": "Zij draagt graag een ketting met een hanger van <w>amber</w>.",
                            "target_lang_translations": {
                                "en": "She likes wearing a necklace with an <w>amber</w> pendant.",
                                "ru": "Она любит носить цепочку с кулоном из <w>янтаря</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "barnsteen"
                    ],
                    "common_phrases": [
                        "in amber gevat",
                        "stuk amber"
                    ],
                    "target_lang_definitions": {
                        "en": "Fossil resin from coniferous trees, often yellow or orange in color, used as a gemstone.",
                        "ru": "Окаменевшая смола хвойных деревьев, часто желтого или оранжевого цвета, используемая как драгоценный камень."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "amber",
                                "target_lang_sense_clarification": "fossilized resin"
                            }

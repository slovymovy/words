                },
                {
                    "sense_id": "2f879dc9-897e-380c-a7da-49b536dbab86",
                    "sense_definition": "Dawne określenie księdza katolickiego, używane w średniowieczu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "priest_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary <w>pop</w> ochrzcił w sobotę małą Emilkę.",
                            "target_lang_translations": {
                                "ru": "Старый <w>поп</w> крестил в субботу маленькую Эмильку."
                            }
                        },
                        {
                            "text": "W dawnych kronikach zapisano, że <w>pop</w> pobłogosławił rycerzy.",
                            "target_lang_translations": {
                                "ru": "В старинных хрониках записано, что <w>поп</w> благословил рыцарей."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historyczne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинное название католического священника, использовавшееся в Средние века."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поп",
                                "target_lang_sense_clarification": "устар."
                            }
                        ]
                    }
                }
            ]
        },

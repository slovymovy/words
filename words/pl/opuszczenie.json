                            },
                            {
                                "target_lang_word": "заброшенность",
                                "target_lang_sense_clarification": "отсутствие ухода"
                            },
                            {
                                "target_lang_word": "оставление",
                                "target_lang_sense_clarification": "действие (бросание)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

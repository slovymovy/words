                    "common_phrases": [
                        "играть на трубе",
                        "звук трубы",
                        "медная труба"
                    ],
                    "target_lang_definitions": {
                        "en": "A brass wind musical instrument with a strong, ringing timbre."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trumpet",
                                "target_lang_sense_clarification": "musical instrument"
                            }
                        ]
                    }
                },
                {

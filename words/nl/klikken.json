    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c1a60cbe-1c9e-3eee-a13d-080fc4241af7",
                    "sense_definition": "Een knop van een computermuis of touchpad indrukken om een digitale actie te starten of een keuze te maken.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "digital_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet twee keer snel op het icoontje <w>klikken</w> om het programma te openen.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно дважды быстро <w>кликнуть</w> по иконке, чтобы открыть программу.",
                                "en": "You have to <w>click</w> the icon twice quickly to open the program."
                            }
                        },
                        {
                            "text": "<w>Klik</w> op de link in de e-mail om je wachtwoord te wijzigen.",
                            "target_lang_translations": {
                                "ru": "<w>Нажмите</w> на ссылку в электронном письме, чтобы изменить пароль.",
                                "en": "<w>Click</w> on the link in the email to change your password."
                            }
                        }
                    ],
                    "synonyms": [
                        "kliks"
                    ],
                    "common_phrases": [
                        "dubbel klikken",
                        "rechts klikken",
                        "op een link klikken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нажимать кнопку компьютерной мыши или тачпада для запуска действия или выбора.",
                        "en": "To press a button on a computer mouse or touchpad to start a digital action or make a choice."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кликать",
                                "target_lang_sense_clarification": "компьютерный термин, разговорное"
                            },
                            {
                                "target_lang_word": "нажимать",
                                "target_lang_sense_clarification": "общее значение"
                            },
                            {
                                "target_lang_word": "щелкать",
                                "target_lang_sense_clarification": "использовать мышь"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "click",
                                "target_lang_sense_clarification": "computer term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd759f36-7ab8-3734-be8e-941ef6b5a956",
                    "sense_definition": "Een goede persoonlijke verstandhouding hebben; goed bij elkaar passen qua karakter.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "social_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vanaf het eerste moment <w>klikte</w> het tussen de twee nieuwe collega's.",
                            "target_lang_translations": {
                                "ru": "С первого момента эти двое новых коллег <w>поладили</w>.",
                                "en": "From the very first moment, it <w>clicked</w> between the two new colleagues."
                            }
                        },
                        {
                            "text": "Ze zijn uit elkaar gegaan omdat het uiteindelijk toch niet bleek te <w>klikken</w>.",
                            "target_lang_translations": {
                                "ru": "Они расстались, потому что в конце концов оказалось, что они не <w>сошлись</w> характерами.",
                                "en": "They broke up because it turned out they didn't <w>click</w> after all."
                            }
                        }
                    ],
                    "antonyms": [
                        "botsen"
                    ],
                    "common_phrases": [
                        "het klikt tussen",
                        "het klikt met"
                    ],
                    "target_lang_definitions": {
                        "ru": "Найти общий язык, поладить; хорошо подходить друг другу по характеру.",
                        "en": "To have a good personal rapport; to fit well together in terms of character."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ладить",
                                "target_lang_sense_clarification": "быть в хороших отношениях"
                            },
                            {
                                "target_lang_word": "сойтись",
                                "target_lang_sense_clarification": "характерами"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "click",
                                "target_lang_sense_clarification": "get along well"
                            },
                            {
                                "target_lang_word": "hit it off",
                                "target_lang_sense_clarification": "idiomatic"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b690cbce-89bf-31b4-8969-bd783d6b9155",
                    "sense_definition": "Een kort, hard en tikkend geluid maken, bijvoorbeeld door twee voorwerpen tegen elkaar te laten komen.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De deur viel dicht en ik hoorde het slot zachtjes <w>klikken</w>.",
                            "target_lang_translations": {
                                "ru": "Дверь захлопнулась, и я услышал, как замок тихо <w>щелкнул</w>.",
                                "en": "The door fell shut and I heard the lock softly <w>click</w>."
                            }
                        },
                        {
                            "text": "Hij zat zenuwachtig met zijn pen te <w>klikken</w> tijdens de vergadering.",
                            "target_lang_translations": {
                                "ru": "Он нервно сидел и <w>щелкал</w> ручкой во время собрания.",
                                "en": "He sat nervously <w>clicking</w> his pen during the meeting."
                            }
                        }
                    ],
                    "common_phrases": [
                        "met de tong klikken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать короткий, резкий тикающий звук.",
                        "en": "To make a short, hard, and ticking sound."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "щелкать",
                                "target_lang_sense_clarification": "издавать звук"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "click",
                                "target_lang_sense_clarification": "make a sound"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "052dead3-63b6-316b-8b05-9303964bd856",
                    "sense_definition": "Iemand verraden door stiekem aan een ander (vaak een autoriteit) te vertellen wat diegene gedaan heeft.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "betrayal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niemand in de klas vond het leuk dat hij bij de meester ging <w>klikken</w>.",
                            "target_lang_translations": {
                                "ru": "Никому в классе не понравилось, что он пошел <w>ябедничать</w> учителю.",
                                "en": "No one in the class liked that he went to <w>snitch</w> to the teacher."
                            }
                        },
                        {
                            "text": "Ze hebben beloofd niet te <w>klikken</w> over wat ze in de pauze hebben gezien.",
                            "target_lang_translations": {
                                "ru": "Они пообещали не <w>стучать</w> о том, что видели на перемене.",
                                "en": "They promised not to <w>snitch</w> about what they saw during the break."
                            }
                        }
                    ],
                    "synonyms": [
                        "kliks",
                        "kliks"
                    ],
                    "common_phrases": [
                        "klikken bij",
                        "niet klikken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ябедничать, доносить на кого-то (обычно авторитетному лицу).",
                        "en": "To betray someone by secretly telling another person (often an authority) what they have done."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ябедничать",
                                "target_lang_sense_clarification": "разговорное, детское"
                            },
                            {
                                "target_lang_word": "доносить",
                                "target_lang_sense_clarification": "формальное"
                            },
                            {
                                "target_lang_word": "стучать",
                                "target_lang_sense_clarification": "слэнг"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "snitch",
                                "target_lang_sense_clarification": "informal"
                            },
                            {
                                "target_lang_word": "tattle",
                                "target_lang_sense_clarification": "childish"
                            },
                            {
                                "target_lang_word": "tell on",
                                "target_lang_sense_clarification": "phrasal verb"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67311f60-8bc3-3cdc-a756-4d436f2753ca",
                    "sense_definition": "Plotseling duidelijk worden; ineens begrepen worden (vaak gebruikt met 'het').",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mental_realization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na uren studeren <w>klikte</w> het ineens en begreep ik de wiskundesom.",
                            "target_lang_translations": {
                                "ru": "После часов учебы в голове вдруг <w>щелкнуло</w>, и я понял задачу по математике.",
                                "en": "After hours of studying, it suddenly <w>clicked</w> and I understood the math problem."
                            }
                        },
                        {
                            "text": "Het duurde even voordat de grap bij iedereen <w>klikte</w>.",
                            "target_lang_translations": {
                                "ru": "Прошло время, прежде чем шутка до всех <w>дошла</w>.",
                                "en": "It took a while before the joke <w>clicked</w> with everyone."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно стать понятным; дойти (о смысле).",
                        "en": "To suddenly become clear; to be understood suddenly."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "доходить",
                                "target_lang_sense_clarification": "становиться понятным"
                            },
                            {
                                "target_lang_word": "щелкнуть",
                                "target_lang_sense_clarification": "переносное значение"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "click",
                                "target_lang_sense_clarification": "mental realization"
                            },
                            {
                                "target_lang_word": "fall into place",
                                "target_lang_sense_clarification": "idiomatic"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4be7397f-c2d5-34c3-ab46-001415348cc2",
                    "sense_definition": "Meervoud van <w>klik</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "form_plural",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je hoorde twee duidelijke <w>klikken</w> toen de machine werd ingeschakeld.",
                            "target_lang_translations": {
                                "ru": "Было слышно два четких <w>щелчка</w>, когда машину включили.",
                                "en": "You heard two distinct <w>clicks</w> when the machine was switched on."
                            }
                        },
                        {
                            "text": "Op school vormden zich al snel verschillende <w>klikken</w> die niet met elkaar omgingen.",
                            "target_lang_translations": {
                                "ru": "В школе быстро сформировались разные <w>компании</w>, которые не общались друг с другом.",
                                "en": "At school, different <w>cliques</w> quickly formed that didn't interact with each other."
                            }
                        }

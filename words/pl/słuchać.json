    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "363af4be-bf1c-3f1c-aaeb-d26799ab7c51",
                    "sense_definition": "Odbierać dźwięki, muzykę lub czyjąś mowę za pomocą zmysłu słuchu, zwracając na nie uwagę.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano przy śniadaniu lubię <w>słuchać</w> wiadomości w radiu.",
                            "target_lang_translations": {
                                "ru": "Каждое утро за завтраком я люблю <w>слушать</w> новости по радио."
                            }
                        },
                        {
                            "text": "Uczniowie powinni uważnie <w>słuchać</w> tego, co mówi nauczyciel na lekcji.",
                            "target_lang_translations": {
                                "ru": "Ученики должны внимательно <w>слушать</w> то, что говорит учитель на уроке."
                            }
                        }
                    ],
                    "synonyms": [
                        "halo"
                    ],
                    "antonyms": [
                        "ignorować"
                    ],
                    "common_phrases": [
                        "słuchać muzyki",
                        "słuchać radia",
                        "słuchać uważnie",
                        "słuchać z uwagą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Воспринимать звуки, музыку или чью-то речь с помощью слуха, обращая на них внимание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слушать",
                                "target_lang_sense_clarification": "воспринимать на слух"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7ebb772-10fe-395e-9680-8f601087a1da",
                    "sense_definition": "Postępować zgodnie z czyimiś poleceniami, radami lub zakazami; być posłusznym.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "obedience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Małe dzieci nie zawsze chcą <w>słuchać</w> swoich rodziców i sprzątać pokój.",
                            "target_lang_translations": {
                                "ru": "Маленькие дети не всегда хотят <w>слушаться</w> своих родителей и убирать комнату."
                            }
                        },
                        {
                            "text": "Warto <w>słuchać</w> dobrych rad osób, które mają większe doświadczenie.",
                            "target_lang_translations": {
                                "ru": "Стоит <w>слушать</w> добрые советы людей, у которых больше опыта."
                            }
                        }
                    ],
                    "synonyms": [
                        "halo"
                    ],
                    "antonyms": [
                        "buntować się",
                        "lekceważyć"
                    ],
                    "common_phrases": [
                        "słuchać rodziców",
                        "słuchać rad",
                        "słuchać poleceń"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действовать в соответствии с чьими-то приказами, советами или запретами; быть послушным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слушаться",
                                "target_lang_sense_clarification": "проявлять послушание"
                            },
                            {
                                "target_lang_word": "слушать",
                                "target_lang_sense_clarification": "следовать советам"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "01c0d7ce-f40c-31e2-98ec-64b69c1d06ae",
                    "sense_definition": "Słowo używane jako uprzejma forma powitania przy odbieraniu połączenia telefonicznego.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "phone_response",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Halo, <w>słucham</w>, z kim mam przyjemność rozmawiać?",
                            "target_lang_translations": {
                                "ru": "Алло, <w>слушаю</w>, с кем имею честь говорить?"
                            }
                        },
                        {
                            "text": "Odebrała telefon i powiedziała krótko: <w>słucham</w>.",
                            "target_lang_translations": {
                                "ru": "Она взяла трубку и коротко сказала: <w>слушаю</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "halo"
                    ],
                    "common_phrases": [
                        "halo słucham"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, используемое как вежливая форма приветствия при ответе на телефонный звонок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слушаю",
                                "target_lang_sense_clarification": "ответ на звонок"
                            },
                            {
                                "target_lang_word": "алло",
                                "target_lang_sense_clarification": "телефонное приветствие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17f010e3-32cd-3291-ac26-02f47ac3e1fc",
                    "sense_definition": "Pytanie o powtórzenie wypowiedzi, której się nie usłyszało lub nie zrozumiało.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "request_repetition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Słucham</w>? Czy możesz powtórzyć ostatnie zdanie, bo było głośno?",
                            "target_lang_translations": {
                                "ru": "<w>Простите</w>? Ты можешь повторить последнее предложение, а то было шумно?"
                            }
                        },
                        {
                            "text": "Przepraszam, <w>słucham</w>? Nie dosłyszałem twojego pytania.",
                            "target_lang_translations": {
                                "ru": "Извините, <w>что</w>? Я не расслышал вашего вопроса."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przepraszam słucham"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вопрос с просьбой повторить высказывание, которое не было услышано или понято."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "простите?",
                                "target_lang_sense_clarification": "вежливая просьба повторить"
                            },
                            {
                                "target_lang_word": "что?",
                                "target_lang_sense_clarification": "разговорный вопрос"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26ada475-6282-30a9-8941-098cc9b2b6aa",
                    "sense_definition": "Wyrażenie zdziwienia lub oburzenia na czyjeś niestosowne lub zaskakujące słowa.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "indignation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Słucham</w>? Jak śmiesz oskarżać mnie o kłamstwo w tej sprawie!",
                            "target_lang_translations": {
                                "ru": "<w>Что</w>? Как ты смеешь обвинять меня во лжи в этом деле!"
                            }
                        },
                        {
                            "text": "<w>Słucham</w>? To chyba jakiś nieśmieszny żart z twojej strony.",
                            "target_lang_translations": {
                                "ru": "<w>Простите</w>? Это, наверное, какая-то несмешная шутка с твоей стороны."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to express shock or offense"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражение удивления или возмущения чьими-то неуместными или неожиданными словами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "что?",
                                "target_lang_sense_clarification": "резкое удивление"
                            },
                            {
                                "target_lang_word": "простите?",
                                "target_lang_sense_clarification": "возмущение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e63b4609-c374-3595-b517-f97ebc525a1e",
                    "sense_definition": "Czynność polegająca na unieważnieniu czegoś, co zostało wcześniej zaplanowane, na przykład spotkania lub wydarzenia.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cancellation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z powodu gwałtownej burzy nastąpiło <w>odwołanie</w> wszystkich lotów.",
                            "target_lang_translations": {
                                "ru": "Из-за сильной грозы произошла <w>отмена</w> всех рейсов."
                            }
                        },
                        {
                            "text": "Sklep będzie nieczynny aż do <w>odwołania</w>.",
                            "target_lang_translations": {
                                "ru": "Магазин будет закрыт до <w>отмены</w> (этого распоряжения)."
                            }
                        }
                    ],
                    "common_phrases": [
                        "do odwołania",
                        "odwołanie wizyty",
                        "odwołanie spotkania",
                        "odwołanie rezerwacji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие, заключающееся в аннулировании чего-либо запланированного ранее, например, встречи или события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отмена",
                                "target_lang_sense_clarification": "события, встречи"
                            },
                            {
                                "target_lang_word": "аннулирование",
                                "target_lang_sense_clarification": "официальное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b14cd1ef-3ef1-36b0-afa3-d080f37fd681",
                    "sense_definition": "Oficjalna prośba skierowana do urzędu lub sądu o ponowne rozpatrzenie sprawy i zmianę wydanej decyzji.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "legal_appeal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Masz prawo złożyć <w>odwołanie</w> od tej decyzji w ciągu czternastu dni.",
                            "target_lang_translations": {
                                "ru": "У тебя есть право подать <w>апелляцию</w> на это решение в течение четырнадцати дней."
                            }
                        },
                        {
                            "text": "Sąd odrzucił <w>odwołanie</w> oskarżonego, uznając je za bezzasadne.",
                            "target_lang_translations": {
                                "ru": "Суд отклонил <w>апелляцию</w> обвиняемого, признав её необоснованной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wnieść odwołanie",
                        "złożyć odwołanie",
                        "odwołanie od decyzji",
                        "odwołanie od wyroku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальная просьба, направленная в учреждение или суд, о пересмотре дела и изменении вынесенного решения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "апелляция",
                                "target_lang_sense_clarification": "юридическая"
                            },
                            {
                                "target_lang_word": "обжалование",
                                "target_lang_sense_clarification": "процесс оспаривания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c39e0ad3-ca2d-3dc7-a79f-206a21830fbc",
                    "sense_definition": "Pozbawienie kogoś pełnionego stanowiska lub funkcji.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "dismissal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rada nadzorcza przegłosowała natychmiastowe <w>odwołanie</w> prezesa spółki.",
                            "target_lang_translations": {
                                "ru": "Наблюдательный совет проголосовал за немедленное <w>отстранение</w> председателя компании."
                            }
                        },
                        {
                            "text": "Jego niespodziewane <w>odwołanie</w> ze stanowiska wywołało wiele plotek w firmie.",
                            "target_lang_translations": {
                                "ru": "Его неожиданное <w>снятие</w> с должности вызвало много слухов в фирме."
                            }
                        }
                    ],
                    "synonyms": [
                        "dymisja",
                        "zwolnienie",
                        "odstawka"
                    ],
                    "antonyms": [
                        "powołanie"
                    ],
                    "common_phrases": [
                        "odwołanie ze stanowiska",
                        "odwołanie zarządu",
                        "wniosek o odwołanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишение кого-либо занимаемой должности или функции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отстранение",
                                "target_lang_sense_clarification": "от должности"
                            },
                            {
                                "target_lang_word": "снятие",
                                "target_lang_sense_clarification": "с поста"
                            },
                            {
                                "target_lang_word": "отзыв",
                                "target_lang_sense_clarification": "должностного лица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8df50030-5398-3618-82fb-7c54ede4a1f9",
                    "sense_definition": "Świadome nawiązanie w wypowiedzi lub tekście do innego dzieła, osoby lub wydarzenia.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "reference",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym filmie pojawia się wyraźne <w>odwołanie</w> do klasyki kina lat pięćdziesiątych.",
                            "target_lang_translations": {
                                "ru": "В этом фильме появляется явная <w>отсылка</w> к классике кино пятидесятых годов."
                            }
                        },
                        {
                            "text": "Autor zastosował w swoim eseju <w>odwołanie</w> do mitologii greckiej.",
                            "target_lang_translations": {
                                "ru": "Автор использовал в своем эссе <w>отсылку</w> к греческой мифологии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "odwołanie do tradycji",
                        "odwołanie do historii",
                        "liczne odwołania"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сознательное упоминание в высказывании или тексте другого произведения, человека или события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отсылка",
                                "target_lang_sense_clarification": "к источнику, контексту"
                            },
                            {
                                "target_lang_word": "ссылка",
                                "target_lang_sense_clarification": "упоминание"
                            }
                        ]
                    }
                }
            ]
        }
    ]

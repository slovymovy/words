            "senses": [
                {
                    "sense_id": "cd0a8c43-dd12-3bdf-a311-5f90ea9f32af",
                    "sense_definition": "Present participle of <w>flow</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The river is <w>flowing</w> faster than usual after the rain.",
                            "target_lang_translations": {
                                "ru": "После дождя река <w>течет</w> быстрее обычного."
                            }
                        },
                        {
                            "text": "Traffic was <w>flowing</w> smoothly until the accident occurred.",
                            "target_lang_translations": {
                                "ru": "Транспортный поток <w>двигался</w> плавно, пока не произошла авария."
                            }
                        }
                    ],
                    "common_phrases": [
                        "flowing freely",
                        "flowing into",
                        "flowing through"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени от глагола flow (течь)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "текущий",
                                "target_lang_sense_clarification": "причастие от «течь»"
                            },
                            {
                                "target_lang_word": "льющийся",
                                "target_lang_sense_clarification": "о жидкости, льющейся потоком"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "08a9871f-3746-3ce7-8c0f-2fb5a21d051e",
                    "sense_definition": "Moving continuously in a current or stream, like a liquid.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We sat by the <w>flowing</w> water and enjoyed the peaceful sound.",
                            "target_lang_translations": {
                                "ru": "Мы сидели у <w>текущей</w> воды и наслаждались умиротворяющим звуком."
                            }
                        },
                        {
                            "text": "The <w>flowing</w> lava moved slowly down the side of the volcano.",
                            "target_lang_translations": {
                                "ru": "<w>Текущая</w> лава медленно двигалась по склону вулкана."
                            }
                        }
                    ],
                    "synonyms": [
                        "running",
                        "streaming",
                        "onrushing"
                    ],
                    "antonyms": [
                        "stagnant",
                        "standing"
                    ],
                    "common_phrases": [
                        "flowing water",
                        "flowing river",
                        "flowing stream"
                    ],
                    "target_lang_definitions": {
                        "ru": "Движущийся непрерывным потоком или струей, подобно жидкости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "струящийся",
                                "target_lang_sense_clarification": "движущийся струей"
                            },
                            {
                                "target_lang_word": "текучий",
                                "target_lang_sense_clarification": "обладающий свойством течь"
                            },
                            {
                                "target_lang_word": "проточный",
                                "target_lang_sense_clarification": "о воде в реках или ручьях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ec744b12-fedc-3a86-a483-f53b5f3b76eb",
                    "sense_definition": "Moving or shaped in a smooth, graceful, and continuous way.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wore a beautiful gown with long, <w>flowing</w> sleeves.",
                            "target_lang_translations": {
                                "ru": "На ней было красивое платье с длинными <w>струящимися</w> рукавами."
                            }
                        },
                        {
                            "text": "The author writes in a <w>flowing</w> style that is easy to read.",
                            "target_lang_translations": {
                                "ru": "Автор пишет в <w>плавном</w> стиле, который легко читать."
                            }
                        }
                    ],
                    "synonyms": [
                        "fluid",
                        "free-flowing"
                    ],
                    "common_phrases": [
                        "flowing hair",
                        "flowing robes",
                        "flowing lines",
                        "flowing script"
                    ],
                    "target_lang_definitions": {
                        "ru": "Движущийся или имеющий форму, отличающуюся плавностью, изяществом и непрерывностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "плавный",
                                "target_lang_sense_clarification": "гладкий, без резких переходов"
                            },
                            {
                                "target_lang_word": "ниспадающий",
                                "target_lang_sense_clarification": "об одежде или ткани"
                            },
                            {
                                "target_lang_word": "распущенные",
                                "target_lang_sense_clarification": "о длинных волосах"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7fd70cd7-a26d-3387-9f12-c8a28910efd9",
                    "sense_definition": "The action or process of moving in a stream.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The constant <w>flowing</w> of the tide gradually shaped the coastline.",
                            "target_lang_translations": {
                                "ru": "Постоянное <w>течение</w> прилива постепенно формировало береговую линию."
                            }
                        },
                        {
                            "text": "He found peace in listening to the rhythmic <w>flowing</w> of the creek.",
                            "target_lang_translations": {
                                "ru": "Он находил покой, слушая ритмичное <w>течение</w> ручья."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или процесс движения в потоке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "течение",
                                "target_lang_sense_clarification": "процесс движения воды"
                            },
                            {
                                "target_lang_word": "поток",
                                "target_lang_sense_clarification": "движущаяся масса"
                            }
                        ]
                    }
                }
            ]
        }
    ]

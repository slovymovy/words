            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "47615643-f4b8-308b-8337-c9b02c457b12",
                    "sense_definition": "Aanduiding van een specifiek tijdstip.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_indication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De film begint vanavond <w>om</w> acht uur.",
                            "target_lang_translations": {
                                "en": "The movie starts <w>at</w> eight o'clock tonight.",
                                "ru": "Фильм начинается сегодня вечером <w>в</w> восемь часов."
                            }
                        },
                        {
                            "text": "We hebben <w>om</w> half drie afgesproken bij het station.",
                            "target_lang_translations": {
                                "en": "We agreed to meet <w>at</w> half past two at the station.",
                                "ru": "Мы договорились встретиться на станции <w>в</w> полтретьего."
                            }
                        }
                    ],
                    "synonyms": [
                        "te"
                    ],
                    "common_phrases": [
                        "om ... uur"
                    ],
                    "target_lang_definitions": {
                        "en": "Indication of a specific time.",
                        "ru": "Указание на конкретный момент времени."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "at",
                                "target_lang_sense_clarification": "used for time"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "о времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18de29c2-d97c-3cfe-a44a-24da97ef1056",
                    "sense_definition": "Aan alle kanten van iets of iemand; in een cirkel rondom.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location_around",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze deed een warme sjaal <w>om</w> haar nek.",
                            "target_lang_translations": {
                                "en": "She put a warm scarf <w>around</w> her neck.",
                                "ru": "Она повязала теплый шарф <w>вокруг</w> шеи."
                            }
                        },
                        {
                            "text": "De aarde draait in een jaar tijd <w>om</w> de zon.",
                            "target_lang_translations": {
                                "en": "The earth revolves <w>around</w> the sun in a year's time.",
                                "ru": "Земля делает оборот <w>вокруг</w> Солнца за год."
                            }
                        }
                    ],
                    "synonyms": [
                        "rond",
                        "rondheen",
                        "rond",
                        "omheen"
                    ],
                    "common_phrases": [
                        "om de hoek",
                        "arm om schouder"
                    ],
                    "target_lang_definitions": {
                        "en": "On all sides of something or someone; in a circle around.",
                        "ru": "Со всех сторон чего-либо или кого-либо; по кругу."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "around",
                                "target_lang_sense_clarification": "surrounding"

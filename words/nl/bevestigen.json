                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij is vorige week opnieuw <w>bevestigd</w> in zijn ambt.",
                            "target_lang_translations": {

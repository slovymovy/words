                {
                    "sense_id": "41d5b5cc-d22b-3fd9-9555-b617dba761f0",
                    "sense_definition": "De vlakke achterkant van een schip of boot.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nautical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De naam van het schip was duidelijk leesbaar op de <w>spiegel</w> geschilderd.",
                            "target_lang_translations": {

{
    "word_family": [
        "warmte",
        "warmen",
        "verwarmen",
        "opwarmen",
        "hartverwarmend"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "13fdcbea-3499-34e2-8bca-36af40b984ba",
                    "sense_definition": "Met een hoge temperatuur; niet koud.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "temperature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de zomer is het vaak erg <w>warm</w> buiten.",
                            "target_lang_translations": {
                                "ru": "Летом на улице часто очень <w>тепло</w>."
                            }
                        },
                        {
                            "text": "Ik was mijn handen met <w>warm</w> water en zeep.",
                            "target_lang_translations": {
                                "ru": "Я мою руки <w>тёплой</w> водой с мылом."
                            }
                        }
                    ],
                    "antonyms": [
                        "koud"
                    ],
                    "common_phrases": [
                        "lekker warm",
                        "warm water",
                        "warm weer"
                    ],
                    "target_lang_definitions": {
                        "ru": "С высокой температурой; не холодный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тёплый",
                                "target_lang_sense_clarification": "о температуре воздуха, воды или предметов"
                            },
                            {
                                "target_lang_word": "жаркий",
                                "target_lang_sense_clarification": "очень тёплый, обычно о погоде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a269d1a-ede4-37ce-8e3f-2a988168b5de",
                    "sense_definition": "Gemaakt van materiaal dat de lichaamswarmte goed vasthoudt.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "temperature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trek een <w>warme</w> trui aan als je naar buiten gaat.",
                            "target_lang_translations": {
                                "ru": "Надень <w>тёплый</w> свитер, если идёшь на улицу."
                            }
                        },
                        {
                            "text": "Onder deze <w>warme</w> deken heb je het niet koud.",
                            "target_lang_translations": {
                                "ru": "Под этим <w>тёплым</w> одеялом тебе не будет холодно."
                            }
                        }
                    ],
                    "antonyms": [
                        "koud"
                    ],
                    "common_phrases": [
                        "warme kleren",
                        "warme jas",
                        "warme trui"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделанный из материала, который хорошо сохраняет тепло тела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тёплый",
                                "target_lang_sense_clarification": "об одежде или материалах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "803dbf73-af31-33b7-803c-ae012cefe1dd",
                    "sense_definition": "Vriendelijk, hartelijk en een gevoel van vertrouwen gevend.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze gaf hem een <w>warm</w> welkom toen hij aankwam.",
                            "target_lang_translations": {
                                "ru": "Она оказала ему <w>тёплый</w> приём, когда он приехал."
                            }
                        },
                        {
                            "text": "Ik heb een <w>warm</w> gevoel bij deze herinnering.",
                            "target_lang_translations": {
                                "ru": "У меня <w>тёплое</w> чувство от этого воспоминания."
                            }
                        }
                    ],
                    "antonyms": [
                        "koud"
                    ],
                    "common_phrases": [
                        "warm welkom",
                        "warm hart",
                        "warm gevoel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дружелюбный, сердечный и вызывающий чувство доверия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тёплый",
                                "target_lang_sense_clarification": "о приёме или чувствах"
                            },
                            {
                                "target_lang_word": "сердечный",
                                "target_lang_sense_clarification": "искренний и добрый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a6fca0c4-a71c-307f-8ada-4593ee06f69b",
                    "sense_definition": "Aangenaam en sfeervol overkomend (vooral van kleuren of geluiden).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De woonkamer is geschilderd in <w>warme</w> kleuren zoals rood en oranje.",
                            "target_lang_translations": {
                                "ru": "Гостиная покрашена в <w>тёплые</w> тона, такие как красный и оранжевый."
                            }
                        },
                        {
                            "text": "De zangeres heeft een heel <w>warme</w> stem.",
                            "target_lang_translations": {
                                "ru": "У певицы очень <w>тёплый</w> голос."
                            }
                        }
                    ],
                    "antonyms": [
                        "koud"
                    ],
                    "common_phrases": [
                        "warme kleuren",
                        "warme stem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приятный и атмосферный (особенно о цветах или звуках)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тёплый",
                                "target_lang_sense_clarification": "о цветовой гамме или тембре голоса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "183931b0-8362-3b3a-b08a-8d96ad4b4fa0",
                    "sense_definition": "Enthousiast of zeer geïnteresseerd in iets.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik kan niet echt <w>warm</w> lopen voor dat saaie plan.",
                            "target_lang_translations": {
                                "ru": "Я не могу по-настоящему <w>заинтересоваться</w> этим скучным планом."
                            }
                        },
                        {
                            "text": "Niemand werd echt <w>warm</w> van zijn voorstel.",
                            "target_lang_translations": {
                                "ru": "Никого по-настоящему не <w>вдохновило</w> его предложение."
                            }
                        }
                    ],
                    "common_phrases": [
                        "warm lopen voor",
                        "warm maken voor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полный энтузиазма или очень заинтересованный в чём-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заинтересованный",
                                "target_lang_sense_clarification": "проявляющий интерес"
                            },
                            {
                                "target_lang_word": "воодушевлённый",
                                "target_lang_sense_clarification": "полный энтузиазма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5fc33b95-7773-36fe-8fbb-2e4653de6dc4",
                    "sense_definition": "Waarbij een oven of hitte wordt gebruikt (vooral in beroepsnamen).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij koopt zijn brood altijd bij de <w>warme</w> bakker.",
                            "target_lang_translations": {
                                "ru": "Он всегда покупает хлеб в <w>пекарне</w>."
                            }
                        },
                        {
                            "text": "Een <w>warme</w> bakker bakt zijn brood zelf in de winkel.",
                            "target_lang_translations": {
                                "ru": "<w>Пекарь</w> сам печёт хлеб в магазине."
                            }
                        }
                    ],
                    "common_phrases": [
                        "warme bakker",
                        "warme maaltijd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с использованием печи или нагрева (особенно в названиях профессий)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "горячий",
                                "target_lang_sense_clarification": "о еде или процессе"
                            },
                            {
                                "target_lang_word": "свежий",
                                "target_lang_sense_clarification": "о выпечке"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c52d3a51-dd43-3e07-9fed-68fb521f7318",
                    "sense_definition": "Op een hartelijke, vriendelijke of temperatuur-gerelateerde manier.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "manner",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gasten werden <w>warm</w> onthaald door de gastvrouw.",
                            "target_lang_translations": {
                                "ru": "Гостей <w>тепло</w> встретила хозяйка."
                            }
                        },
                        {
                            "text": "Je moet je <w>warm</w> aankleden als het vriest.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно <w>тепло</w> одеться, когда на улице мороз."
                            }
                        }
                    ],
                    "common_phrases": [
                        "warm onthalen",
                        "warm aankleden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сердечно, дружелюбно или с ощущением тепла."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тепло",
                                "target_lang_sense_clarification": "наречие"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d1d2d007-d7de-3ad9-9c3a-fd1e7d5d8ace",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>warmen</w>.",
                    "learner_level": "A2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>warm</w> mijn koude handen bij het vuur.",
                            "target_lang_translations": {
                                "ru": "Я <w>грею</w> свои холодные руки у огня."
                            }
                        },
                        {
                            "text": "Ik <w>warm</w> de soep even op in de magnetron.",
                            "target_lang_translations": {
                                "ru": "Я немного <w>подогрею</w> суп в микроволновке."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Первое лицо единственного числа настоящего времени от глагола 'греть'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грею",
                                "target_lang_sense_clarification": "делаю тёплым"
                            },
                            {
                                "target_lang_word": "подогреваю",
                                "target_lang_sense_clarification": "разогреваю пищу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9f863465-ff11-314b-9572-5429b8fa12a8",
                    "sense_definition": "Gebiedende wijs van <w>warmen</w>.",
                    "learner_level": "B1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Warm</w> je handen even op aan deze beker chocolademelk.",
                            "target_lang_translations": {
                                "ru": "<w>Погрей</w> руки об эту кружку с горячим шоколадом."
                            }
                        },
                        {
                            "text": "<w>Warm</w> de oven voor op tweehonderd graden.",
                            "target_lang_translations": {
                                "ru": "<w>Разогрей</w> духовку до двухсот градусов."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола 'греть'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грей",
                                "target_lang_sense_clarification": "повелительное наклонение"
                            },
                            {
                                "target_lang_word": "разогрей",
                                "target_lang_sense_clarification": "просьба нагреть что-то"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "aeb43905-17b0-3f80-ba09-bdf6d1a53d0a",
                    "sense_definition": "Een buurtschap in de gemeente Oude IJsselstreek, Gelderland.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "toponym",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Mijn familie komt oorspronkelijk uit het plaatsje <w>Warm</w>.",
                            "target_lang_translations": {
                                "ru": "Моя семья родом из местечка <w>Варм</w>."
                            }
                        },
                        {
                            "text": "We reden door <w>Warm</w> toen we in de Achterhoek waren.",
                            "target_lang_translations": {
                                "ru": "Мы проезжали через <w>Варм</w>, когда были в Ахтерхуке."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поселение в общине Ауде-Эйсселстрек, провинция Гелдерланд."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Варм",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
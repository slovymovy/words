{
    "word_family": [
        "opkomst"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "794ae527-91ca-3cd3-b441-0cdeab576f26",
                    "sense_definition": "Omhooggaan of zichtbaar worden boven de horizon, specifiek gezegd van hemellichamen zoals de zon en de maan.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de zomer zie je de zon al heel vroeg <opkomen>.",
                            "target_lang_translations": {
                                "en": "In summer you can see the sun <rise> very early.",
                                "ru": "Летом можно увидеть, как солнце <w>всходит</w> очень рано."
                            }
                        },
                        {
                            "text": "Het wordt langzaam licht zodra de zon <opkomt>.",
                            "target_lang_translations": {
                                "en": "It slowly becomes light as soon as the sun <comes up>.",
                                "ru": "Становится светло, как только <w>всходит</w> солнце."
                            }
                        }
                    ],
                    "synonyms": [
                        "opgaan",
                        "stijgen",
                        "oprijzen",
                        "klimmen"
                    ],
                    "antonyms": [
                        "ondergaan",
                        "neerdalen",
                        "neergaan",
                        "afzakken"
                    ],
                    "common_phrases": [
                        "de zon komt op",
                        "de maan komt op"
                    ],
                    "target_lang_definitions": {
                        "en": "To rise or become visible above the horizon, specifically said of celestial bodies like the sun and moon.",
                        "ru": "Подниматься или становиться видимым над горизонтом (о небесных телах, таких как солнце и луна)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rise",
                                "target_lang_sense_clarification": "of the sun or moon"
                            },
                            {
                                "target_lang_word": "come up",
                                "target_lang_sense_clarification": "rise above the horizon"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "всходить",
                                "target_lang_sense_clarification": "о небесных телах"
                            },
                            {
                                "target_lang_word": "подниматься",
                                "target_lang_sense_clarification": "появляться над горизонтом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5dde693-7819-303f-8ca5-ba190e6840ab",
                    "sense_definition": "Iemand of iets verdedigen, steunen of beschermen tegen kritiek of onrecht.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <kwam op> voor zijn vriend die werd gepest.",
                            "target_lang_translations": {
                                "en": "He <stood up> for his friend who was being bullied.",
                                "ru": "Он <w>заступился</w> за своего друга, над которым издевались."
                            }
                        },
                        {
                            "text": "Je moet leren om voor jezelf <op> te <komen>.",
                            "target_lang_translations": {
                                "en": "You must learn to <stand up> for yourself.",
                                "ru": "Ты должен научиться <w>постоять</w> за себя."
                            }
                        }
                    ],
                    "synonyms": [
                        "verdedigen",
                        "bijstaan",
                        "te hulp komen",
                        "te hulp schieten",
                        "verweren"
                    ],
                    "common_phrases": [
                        "opkomen voor jezelf",
                        "opkomen voor anderen",
                        "opkomen voor rechten"
                    ],
                    "target_lang_definitions": {
                        "en": "To defend, support, or protect someone or something against criticism or injustice.",
                        "ru": "Защищать, поддерживать кого-либо или отстаивать чьи-либо интересы перед лицом критики или несправедливости."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stand up for",
                                "target_lang_sense_clarification": "defend or support someone"
                            },
                            {
                                "target_lang_word": "defend",
                                "target_lang_sense_clarification": "protect against attack or criticism"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "заступаться",
                                "target_lang_sense_clarification": "вставать на защиту кого-либо"
                            },
                            {
                                "target_lang_word": "отстаивать",
                                "target_lang_sense_clarification": "защищать права или интересы"
                            },
                            {
                                "target_lang_word": "постоять",
                                "target_lang_sense_clarification": "защитить себя или других"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e73cb75d-ffde-30f7-8ee0-f8ec25265294",
                    "sense_definition": "Plotseling in de gedachten verschijnen of ontstaan.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er <kwam> ineens een geweldig idee in haar <op>.",
                            "target_lang_translations": {
                                "en": "A great idea suddenly <occurred> to her.",
                                "ru": "Ей вдруг <w>пришла в голову</w> отличная идея."
                            }
                        },
                        {
                            "text": "Het is nooit in mij <opgekomen> om zoiets te doen.",
                            "target_lang_translations": {
                                "en": "It never <occurred> to me to do such a thing.",
                                "ru": "Мне никогда не <w>приходило в голову</w> сделать что-то подобное."
                            }
                        }
                    ],
                    "synonyms": [
                        "opwellen"
                    ],
                    "common_phrases": [
                        "in iemand opkomen",
                        "in me opkomen"
                    ],
                    "target_lang_definitions": {
                        "en": "To suddenly appear or originate in one's thoughts.",
                        "ru": "Внезапно появляться в мыслях или приходить в голову."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "occur to",
                                "target_lang_sense_clarification": "come into one's mind"
                            },
                            {
                                "target_lang_word": "come to mind",
                                "target_lang_sense_clarification": "appear in one's thoughts"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "приходить в голову",
                                "target_lang_sense_clarification": "о мыслях или идеях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a745fdc3-c281-3b59-bcf3-e0a0baf6f57c",
                    "sense_definition": "Aanwezig zijn of verschijnen bij een officiële gelegenheid, verkiezing of bijeenkomst.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Slechts de helft van de leden is <opgekomen> voor de vergadering.",
                            "target_lang_translations": {
                                "en": "Only half of the members <showed up> for the meeting.",
                                "ru": "Только половина членов <w>явилась</w> на собрание."
                            }
                        },
                        {
                            "text": "Bij de verkiezingen zijn veel kiezers niet <opgekomen>.",
                            "target_lang_translations": {
                                "en": "Many voters did not <turn up> for the elections.",
                                "ru": "На выборах многие избиратели не <w>пришли</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "verschijnen",
                        "opdagen",
                        "zich melden"
                    ],
                    "antonyms": [
                        "wegblijven",
                        "verstek laten gaan",
                        "forfait geven"
                    ],
                    "common_phrases": [
                        "niet komen opdagen"
                    ],
                    "target_lang_definitions": {
                        "en": "To be present or appear at an official occasion, election, or meeting.",
                        "ru": "Присутствовать или появляться на официальном мероприятии, выборах или собрании."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "show up",
                                "target_lang_sense_clarification": "arrive or attend"
                            },
                            {
                                "target_lang_word": "turn up",
                                "target_lang_sense_clarification": "arrive, often casually"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "являться",
                                "target_lang_sense_clarification": "приходить на официальное мероприятие"
                            },
                            {
                                "target_lang_word": "приходить",
                                "target_lang_sense_clarification": "посещать место сбора"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7837179b-5fb1-3af2-aa54-049902c58324",
                    "sense_definition": "Ontstaan, zich ontwikkelen of aan invloed en populariteit winnen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "China is al jaren een snel <opkomende> economie.",
                            "target_lang_translations": {
                                "en": "China has been a rapidly <emerging> economy for years.",
                                "ru": "Китай уже много лет является быстро <w>развивающейся</w> экономикой."
                            }
                        },
                        {
                            "text": "Er zijn veel nieuwe technologieën die nu snel <opkomen>.",
                            "target_lang_translations": {
                                "en": "There are many new technologies that are <emerging> fast now.",
                                "ru": "Существует много новых технологий, которые сейчас быстро <w>развиваются</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "toenemen",
                        "vooruitgaan"
                    ],
                    "antonyms": [
                        "achteruitgaan",
                        "teloorgaan"
                    ],
                    "common_phrases": [
                        "opkomende markt",
                        "opkomende economie",
                        "opkomende zon"
                    ],
                    "target_lang_definitions": {
                        "en": "To come into existence, develop, or gain influence and popularity.",
                        "ru": "Возникать, развиваться или набирать влияние и популярность."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "emerge",
                                "target_lang_sense_clarification": "become apparent or prominent"
                            },
                            {
                                "target_lang_word": "rise",
                                "target_lang_sense_clarification": "increase in power or status"
                            },
                            {
                                "target_lang_word": "come up",
                                "target_lang_sense_clarification": "develop or arise"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "развиваться",
                                "target_lang_sense_clarification": "об экономике или прогрессе"
                            },
                            {
                                "target_lang_word": "возникать",
                                "target_lang_sense_clarification": "появляться, зарождаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55b57b4e-1019-3d16-9ad6-f87f47820db9",
                    "sense_definition": "Het toneel of podium betreden tijdens een voorstelling.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De hoofdrolspeler moet van rechts <opkomen> in de eerste scène.",
                            "target_lang_translations": {
                                "en": "The protagonist has to <enter> from the right in the first scene.",
                                "ru": "Главный герой должен <w>выходить</w> справа в первой сцене."
                            }
                        },
                        {
                            "text": "Zodra de muziek begint, <komt> het koor <op>.",
                            "target_lang_translations": {
                                "en": "As soon as the music starts, the choir <comes on>.",
                                "ru": "Как только начинается музыка, <w>выходит</w> хор."
                            }
                        }
                    ],
                    "synonyms": [
                        "verschijnen",
                        "optreden"
                    ],
                    "antonyms": [
                        "afgaan"
                    ],
                    "common_phrases": [
                        "links opkomen",
                        "rechts opkomen"
                    ],
                    "target_lang_definitions": {
                        "en": "To enter the stage or podium during a performance.",
                        "ru": "Выходить на сцену или подиум во время представления."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "enter",
                                "target_lang_sense_clarification": "come onto the stage"
                            },
                            {
                                "target_lang_word": "come on",
                                "target_lang_sense_clarification": "appear on stage"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "выходить",
                                "target_lang_sense_clarification": "на сцену"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
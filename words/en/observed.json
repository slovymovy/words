            "senses": [
                {
                    "sense_id": "68e9fb45-0017-355e-897d-bd78367314c2",
                    "sense_definition": "Exceptionally celebrated on a date other than the usual date, often because the actual holiday falls on a weekend.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Since the Fourth of July is on a Sunday this year, Monday will be the holiday <w>observed</w>.",
                            "target_lang_translations": {
                                "ru": "Поскольку Четвертое июля в этом году выпадает на воскресенье, праздник будет <w>отмечаться</w> в понедельник."
                            }
                        },
                        {
                            "text": "Government offices are closed today for Independence Day <w>observed</w>.",
                            "target_lang_translations": {

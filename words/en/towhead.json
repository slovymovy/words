            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8857971b-746f-34e3-82e0-6dce2806af9e",
                    "sense_definition": "A person, especially a child, with very pale, almost white blond hair.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person_desc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The little <towhead> ran across the yard, his hair shining brightly in the sun.",
                            "target_lang_translations": {
                                "ru": "Маленький <белоголовый> бежал по двору, и его волосы ярко сверкали на солнце."
                            }
                        },
                        {
                            "text": "She was a cute <towhead> when she was young, but her hair turned brown later.",
                            "target_lang_translations": {
                                "ru": "В детстве она была милой <белоголовой>, но позже её волосы стали каштановыми."
                            }
                        }
                    ],
                    "synonyms": [
                        "blond"
                    ],
                    "antonyms": [
                        "brunette"
                    ],
                    "common_phrases": [
                        "little towhead"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, особенно ребенок, с очень светлыми, почти белыми волосами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "белоголовый",
                                "target_lang_sense_clarification": "о человеке, особенно ребенке"
                            },
                            {
                                "target_lang_word": "белобрысый",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5c44820-6a9c-3a0a-8187-f0b1720a935e",
                    "sense_definition": "A sandbar or small island in a river, often covered with vegetation.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geography",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boat nearly ran aground on a shallow <towhead> in the middle of the river.",
                            "target_lang_translations": {
                                "ru": "Лодка чуть не села на мелководную <отмель> посреди реки."
                            }
                        },
                        {
                            "text": "We decided to camp on a grassy <towhead> for the night.",
                            "target_lang_translations": {
                                "ru": "Мы решили разбить лагерь на ночь на травянистом <островке>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sandbar",
                        "islet",
                        "ait"
                    ],
                    "traits": [
                        {

                    "sense_id": "7b0f653e-1d29-3af1-955d-d9af4549cfc1",
                    "sense_definition": "Uszkodzenie skóry lub tkanki ciała spowodowane urazem, uderzeniem lub oparzeniem.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_injury",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz oczyścił <ranę> i założył świeży opatrunek.",
                            "target_lang_translations": {
                                "ru": "Врач промыл <рану> и наложил свежую повязку."
                            }
                        },
                        {
                            "text": "To była tylko powierzchowna <rana>, więc szybko się zagoiła.",
                            "target_lang_translations": {
                                "ru": "Это была лишь поверхностная <рана>, поэтому она быстро зажила."
                            }
                        }
                    ],

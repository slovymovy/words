    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2accf6ba-b303-3500-a5fd-87d4c11a1620",
                    "sense_definition": "Involving physical conflict or aggression.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_harm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The protest started peacefully but became <w>violent</w> later in the day.",
                            "target_lang_translations": {
                                "ru": "Протест начался мирно, но позже принял <w>наси́льственный</w> характер."
                            }
                        },
                        {
                            "text": "They decided to use <w>violent</w> means to achieve their political goals.",
                            "target_lang_translations": {
                                "ru": "Они решили использовать <w>наси́льственные</w> методы для достижения своих политических целей."
                            }
                        }
                    ],
                    "synonyms": [
                        "aggressive",
                        "brutal"
                    ],
                    "antonyms": [
                        "peaceful"
                    ],
                    "common_phrases": [
                        "violent crime",
                        "violent conflict",
                        "violent behavior"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный с физическим конфликтом или агрессией"

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5eb37d20-adec-35bf-b642-bad4464cbaad",
                    "sense_definition": "Ulegać procesowi fermentacji, zmieniać smak na kwaśny (zwykle o produktach spożywczych).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "fermentation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zostawione na słońcu mleko szybko <w>kiśnie</w>.",
                            "target_lang_translations": {
                                "ru": "Оставленное на солнце молоко быстро <w>киснет</w>."
                            }
                        },
                        {
                            "text": "W naszej piwnicy w dużych beczkach <w>kisną</w> ogórki.",
                            "target_lang_translations": {
                                "ru": "В нашем подвале в больших бочках <w>квасятся</w> огурцы."
                            }
                        }
                    ],
                    "synonyms": [
                        "kwaśnieć",
                        "fermentować"
                    ],
                    "common_phrases": [
                        "mleko kiśnie",
                        "ogórki kisną",
                        "kapusta kiśnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвергаться процессу брожения, становиться кислым на вкус (обычно о продуктах питания)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "киснуть",
                                "target_lang_sense_clarification": "о молоке, супе или тесте"
                            },
                            {
                                "target_lang_word": "кваситься",
                                "target_lang_sense_clarification": "об овощах: капусте, огурцах"
                            },
                            {
                                "target_lang_word": "скисать",
                                "target_lang_sense_clarification": "портиться, становиться кислым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6a1ac8ed-0e26-341b-b312-cb5fe4cb767a",
                    "sense_definition": "Przebywać długo w jednym miejscu bezczynnie, nudzić się i marnować czas.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "stagnation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie chcę <w>kisnąć</w> w domu przez cały weekend, wolę gdzieś wyjść.",
                            "target_lang_translations": {
                                "ru": "Я не хочу <w>киснуть</w> дома все выходные, предпочитаю куда-нибудь выйти."
                            }
                        },
                        {
                            "text": "Oni od lat <w>kisną</w> w tej samej firmie bez szans na awans.",
                            "target_lang_translations": {
                                "ru": "Они годами <w>киснут</w> в одной и той же фирме без шансов на повышение."
                            }
                        }
                    ],
                    "synonyms": [
                        "nudzić się",
                        "gnić"
                    ],
                    "common_phrases": [
                        "kisnąć w domu",
                        "kisnąć w pracy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Долго находиться в одном месте без дела, скучать и тратить время впустую."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "киснуть",
                                "target_lang_sense_clarification": "перен. изнывать от скуки, бездействовать"
                            },
                            {
                                "target_lang_word": "тухнуть",
                                "target_lang_sense_clarification": "разг. сидеть взаперти, деградировать"
                            }
                        ]
                    }
                }
            ]
        }
    ]

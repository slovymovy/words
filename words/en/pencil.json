{
    "word_family": [
        "penciler",
        "penciller"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e9af2b22-306f-3688-8839-35dd3a631bf4",
                    "sense_definition": "A writing utensil with a graphite (commonly referred to as lead) shaft, usually blended with clay, clad in wood, and sharpened to a taper.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please write your name at the top of the page with a <w>pencil</w>."
                        },
                        {
                            "text": "She keeps a sharp <w>pencil</w> and an eraser in her bag for sketching."
                        }
                    ],
                    "synonyms": [
                        "lead pencil"
                    ],
                    "common_phrases": [
                        "sharpen a pencil",
                        "colored pencil",
                        "pencil case",
                        "pencil sharpener"
                    ]
                },
                {
                    "sense_id": "80036cb0-9329-3631-88dc-05e1b3ceaab7",
                    "sense_definition": "An aggregate or collection of rays of light, especially when diverging from, or converging to, a point.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "geometric_collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A narrow <w>pencil</w> of light streamed through the keyhole into the dark room."
                        },
                        {
                            "text": "The convex lens focuses the incident <w>pencil</w> of rays onto a single focal point."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ]
                },
                {
                    "sense_id": "5c547653-2ada-367d-becc-c40ac0f670d3",
                    "sense_definition": "A family of geometric objects with a common property, such as the set of lines that pass through a given point in a projective plane.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geometric_collection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In projective geometry, a <w>pencil</w> of lines consists of all lines passing through a specific intersection point."
                        },
                        {
                            "text": "The student calculated the equation for the <w>pencil</w> of planes sharing a common axis."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mathematics"
                        }
                    ]
                },
                {
                    "sense_id": "08be960e-ff92-3ae1-b0b7-a2ec78e8164a",
                    "sense_definition": "A small medicated stick used for local application, such as to stop bleeding.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He applied a styptic <w>pencil</w> to the small cut on his chin to stop the bleeding."
                        },
                        {
                            "text": "The doctor used a silver nitrate <w>pencil</w> to treat the affected area of the skin."
                        }
                    ],
                    "common_phrases": [
                        "styptic pencil"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "medicine"
                        }
                    ]
                },
                {
                    "sense_id": "438f4b36-7670-3c2e-9516-a13ecd36bf02",
                    "sense_definition": "A paintbrush, particularly a fine one used by an artist.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The master artist picked up his <w>pencil</w> to add delicate details to the portrait."
                        },
                        {
                            "text": "In ancient texts, the tool used for painting was often referred to as a <w>pencil</w>."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ]
                },
                {
                    "sense_id": "a94240a4-4845-35d2-be6f-7ebdee1bccb4",
                    "sense_definition": "The authority to grant credit, complimentary services, or approve debts, typically in a casino context.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pit boss has the <w>pencil</w> and can authorize a free meal for the high rollers."
                        },
                        {
                            "text": "Without the manager's <w>pencil</w>, no additional credit could be issued to the player."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "gambling"
                        }
                    ]
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "baf4c3ed-9273-3e21-bb28-feb4e7984826",
                    "sense_definition": "To write, draw, or record something using a pencil.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will <w>pencil</w> you in for a meeting next Tuesday afternoon."
                        },
                        {
                            "text": "She quickly <w>penciled</w> a sketch of the garden while sitting on the bench."
                        }
                    ],
                    "common_phrases": [
                        "pencil in"
                    ]
                },
                {
                    "sense_id": "6e3ac8ae-2fcf-37a6-af7f-f0d7edc297c0",
                    "sense_definition": "To mark or color with fine lines or hues, as if using a pencil.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The setting sun <w>pencilled</w> the horizon with streaks of gold and violet."
                        },
                        {
                            "text": "Delicate veins were <w>pencilled</w> across the petals of the exotic flower."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "literary"
                        }
                    ]
                }
            ]
        }
    ]

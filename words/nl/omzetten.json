{
    "word_family": [
        "omzet",
        "omzetting"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cca7612a-337b-3d72-8949-86dea4e782d9",
                    "sense_definition": "Iets veranderen van de ene vorm, toestand of het ene formaat in het andere.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil mijn dollars graag <w>omzetten</w> in euro's.",
                            "target_lang_translations": {
                                "ru": "Я бы хотел <w>обменять</w> свои доллары на евро.",
                                "en": "I would like to <w>convert</w> my dollars into euros."
                            }
                        },
                        {
                            "text": "Met dit programma kun je een Word-bestand <w>omzetten</w> naar een PDF.",
                            "target_lang_translations": {
                                "ru": "С помощью этой программы можно <w>конвертировать</w> файл Word в PDF.",
                                "en": "With this program you can <w>convert</w> a Word file to a PDF."
                            }
                        }
                    ],
                    "synonyms": [
                        "veranderen",
                        "converteren",
                        "transformeren",
                        "verwisselen"
                    ],
                    "antonyms": [
                        "behouden"
                    ],
                    "common_phrases": [
                        "geld omzetten",
                        "bestand omzetten",
                        "energie omzetten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменять форму, состояние или формат чего-либо; превращать одно в другое.",
                        "en": "To change something from one form, state, or format into another."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "преобразовывать",
                                "target_lang_sense_clarification": "менять вид или форму"
                            },
                            {
                                "target_lang_word": "конвертировать",
                                "target_lang_sense_clarification": "валюту или файлы"
                            },
                            {
                                "target_lang_word": "переводить",
                                "target_lang_sense_clarification": "трансформировать (напр. энергию)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "convert",
                                "target_lang_sense_clarification": "currency, files, or units"
                            },
                            {
                                "target_lang_word": "transform",
                                "target_lang_sense_clarification": "change form or appearance"
                            },
                            {
                                "target_lang_word": "exchange",
                                "target_lang_sense_clarification": "swap currency"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4934b15-b31d-3b8d-b6c5-bcd7c35cb5e1",
                    "sense_definition": "Een mechanisme, zoals een schakelaar of knop, in een andere stand plaatsen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet de schakelaar <w>omzetten</w> om de machine te starten.",
                            "target_lang_translations": {
                                "ru": "Нужно <w>переключить</w> выключатель, чтобы запустить машину.",
                                "en": "You have to <w>flip</w> the switch to start the machine."
                            }
                        },
                        {
                            "text": "Hij moest de knop in zijn hoofd even <w>omzetten</w> om weer rustig te worden.",
                            "target_lang_translations": {
                                "ru": "Ему нужно было словно <w>переключить</w> тумблер в голове, чтобы снова успокоиться.",
                                "en": "He had to <w>flip</w> the switch in his head for a moment to calm down again."
                            }
                        }
                    ],
                    "synonyms": [
                        "omdraaien",
                        "omkeren",
                        "overhalen"
                    ],
                    "common_phrases": [
                        "de knop omzetten",
                        "de schakelaar omzetten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переводить механизм, например выключатель или кнопку, в другое положение.",
                        "en": "To move a mechanism, such as a switch or button, to a different position."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переключать",
                                "target_lang_sense_clarification": "менять положение переключателя"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "flip",
                                "target_lang_sense_clarification": "a switch"
                            },
                            {
                                "target_lang_word": "throw",
                                "target_lang_sense_clarification": "a switch or lever"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3cce3cea-6eb8-3449-82d6-f4bbd03ebbd8",
                    "sense_definition": "Goederen of diensten verkopen voor een bepaalde geldwaarde (omzet genereren).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De supermarkt wist vorige maand een recordbedrag <w>om</w> te <w>zetten</w>.",
                            "target_lang_translations": {
                                "ru": "В прошлом месяце супермаркету удалось <w>сделать</w> рекордный <w>оборот</w>.",
                                "en": "The supermarket managed to <w>turn over</w> a record amount last month."
                            }
                        },
                        {
                            "text": "Hoeveel zouden ze per jaar <w>omzetten</w> met die nieuwe winkel?",
                            "target_lang_translations": {
                                "ru": "Сколько они будут <w>выручать</w> в год с этим новым магазином?",
                                "en": "How much would they <w>turn over</w> per year with that new store?"
                            }
                        }
                    ],
                    "synonyms": [
                        "verkopen",
                        "verhandelen"
                    ],
                    "common_phrases": [
                        "veel omzetten",
                        "miljoenen omzetten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продавать товары или услуги на определенную сумму; делать оборот.",
                        "en": "To sell goods or services for a specific monetary value; to generate revenue."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "иметь оборот",
                                "target_lang_sense_clarification": "финансовый показатель"
                            },
                            {
                                "target_lang_word": "выручать",
                                "target_lang_sense_clarification": "получать доход от продаж"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "turn over",
                                "target_lang_sense_clarification": "generate revenue"
                            },
                            {
                                "target_lang_word": "gross",
                                "target_lang_sense_clarification": "earn income"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5e121333-98a3-30ff-a64b-106292f90571",
                    "sense_definition": "Iets aan de randen versieren of bekleden met een ander materiaal.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De baret van de hoofdgriffier is aan de onderrand met zwart fluweel <w>omzet</w>.",
                            "target_lang_translations": {
                                "ru": "Берет главного секретаря по нижнему краю <w>окаймлен</w> черным бархатом.",
                                "en": "The chief clerk's beret is <w>edged</w> with black velvet at the bottom rim."
                            }
                        },
                        {
                            "text": "In het museum zagen we een mantel die met goud was <w>omzet</w>.",
                            "target_lang_translations": {
                                "ru": "В музее мы видели мантию, которая была <w>обшита</w> золотом.",
                                "en": "In the museum we saw a cloak that was <w>trimmed</w> with gold."
                            }
                        }
                    ],
                    "synonyms": [
                        "omranden",
                        "afzetten",
                        "bekleden"
                    ],
                    "common_phrases": [
                        "met goud omzet",
                        "met diamanten omzet"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Voornamelijk gebruikt in literaire of historische contexten."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Украшать или обшивать края чего-либо другим материалом.",
                        "en": "To decorate or cover the edges of something with a different material."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окаймлять",
                                "target_lang_sense_clarification": "обрамлять края"
                            },
                            {
                                "target_lang_word": "обшивать",
                                "target_lang_sense_clarification": "отделывать тканью"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "edge",
                                "target_lang_sense_clarification": "border with material"
                            },
                            {
                                "target_lang_word": "trim",
                                "target_lang_sense_clarification": "decorate edges"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [

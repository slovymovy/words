    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3e70cf60-3c4f-39de-9538-9e5f46d434c1",
                    "sense_definition": "A group of people born in a specific range of years; short for <w>generation</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "generation_clipping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marketing strategies must adapt to the habits of the younger <w>gen</w>.",
                            "target_lang_translations": {
                                "ru": "Маркетинговые стратегии должны адаптироваться к привычкам молодого <w>поколения</w>."
                            }
                        },
                        {
                            "text": "My parents belong to a different <w>gen</w> and do not understand these trends.",
                            "target_lang_translations": {
                                "ru": "Мои родители принадлежат к другому <w>поколению</w> и не понимают этих трендов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Gen Z",
                        "Gen X",
                        "future gens"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "clipping"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, родившихся в определенный период времени; сокращение от \"generation\" (поколение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поколение",
                                "target_lang_sense_clarification": "группа людей одного возраста"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "036b7824-da1e-30dc-87f6-11d0f117e37b",
                    "sense_definition": "A specific version or stage in the development of a product or technology; short for <w>generation</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "generation_clipping",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The next <w>gen</w> of consoles will feature significantly better graphics.",
                            "target_lang_translations": {
                                "ru": "Консоли следующего <w>поколения</w> будут обладать значительно лучшей графикой."
                            }
                        },
                        {
                            "text": "As a first-<w>gen</w> student, she faced unique challenges at the university.",
                            "target_lang_translations": {
                                "ru": "Будучи студенткой в первом <w>поколении</w>, она столкнулась с уникальными трудностями в университете."
                            }
                        }
                    ],
                    "common_phrases": [
                        "next-gen",
                        "first-gen",
                        "current-gen"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "clipping"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретная версия или этап развития продукта или технологии; сокращение от \"generation\" (поколение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поколение",
                                "target_lang_sense_clarification": "версия технологии или продукта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35e20873-04bb-330e-bec8-6d7ee08a3f63",
                    "sense_definition": "A high-ranking military officer; short for <w>General</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "title_or_honorific_name",
                    "examples": [
                        {
                            "text": "<w>Gen</w> Smith is expected to arrive at the base tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "<w>Генерал</w> Смит должен прибыть на базу завтра утром."
                            }
                        },
                        {
                            "text": "The orders were signed by <w>Gen</w> Cavoli himself.",
                            "target_lang_translations": {
                                "ru": "Приказы были подписаны самим <w>генералом</w> Каволи."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Высший офицерский чин; сокращение от \"General\" (генерал)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "генерал",
                                "target_lang_sense_clarification": "воинское звание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9025f026-2b08-3177-ac0d-bb031ecdf759",
                    "sense_definition": "A machine that converts mechanical energy into electricity; short for <w>generator</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to fire up the <w>gen</w> before the power goes out completely.",
                            "target_lang_translations": {
                                "ru": "Нам нужно запустить <w>генератор</w>, пока электричество не отключилось полностью."
                            }
                        },
                        {
                            "text": "The backup <w>gen</w> kicked in as soon as the storm hit.",
                            "target_lang_translations": {
                                "ru": "Резервный <w>генератор</w> включился, как только начался шторм."
                            }
                        }
                    ],
                    "synonyms": [
                        "genny"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "clipping"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Машина, преобразующая механическую энергию в электрическую; сокращение от \"generator\" (генератор)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "генератор",
                                "target_lang_sense_clarification": "устройство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e5265c4-45b9-34fb-af1e-1b88086d6651",
                    "sense_definition": "Information or news about a specific subject.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He went to the club to get the latest <w>gen</w> on the mission.",
                            "target_lang_translations": {
                                "ru": "Он пошел в клуб, чтобы получить последнюю <w>информацию</w> о задании."
                            }
                        },
                        {
                            "text": "Can you give me the <w>gen</w> on what happened at the meeting?",
                            "target_lang_translations": {
                                "ru": "Можешь дать мне <w>сведения</w> о том, что произошло на собрании?"
                            }
                        }
                    ],
                    "traits": [

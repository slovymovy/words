                {
                    "sense_id": "61bef624-c692-3d77-8aa7-60e6843cd9d2",
                    "sense_definition": "Kennis of een vaardigheid verwerven door studie, oefening of ervaring.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "learning_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik wil graag de Nederlandse taal <w>leren</w>.",
                            "target_lang_translations": {
                                "ru": "Я хочу <w>выучить</w> нидерландский язык."
                            }
                        },
                        {
                            "text": "Op school <w>leren</w> kinderen lezen en schrijven.",
                            "target_lang_translations": {
                                "ru": "В школе дети <w>учатся</w> читать и писать."
                            }
                        },
                        {
                            "text": "Kies je eigen weg, maak je eigen keuzes, <w>leer</w> je eigen lessen.",
                            "target_lang_translations": {
                                "ru": "Выбирай свой путь, делай свой выбор, <w>усваивай</w> свои уроки."
                            }
                        }
                    ],
                    "synonyms": [
                        "studeren",
                        "aanleren",
                        "opsteken"
                    ],
                    "antonyms": [
                        "vergeten",
                        "afleren"
                    ],
                    "common_phrases": [
                        "uit het hoofd leren",
                        "leren kennen",
                        "leren van fouten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приобретать знания или навыки посредством учебы, практики или опыта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "учиться",
                                "target_lang_sense_clarification": "приобретать навыки или знания"
                            },
                            {
                                "target_lang_word": "учить",
                                "target_lang_sense_clarification": "запоминать или изучать что-то конкретное"
                            },
                            {
                                "target_lang_word": "изучать",
                                "target_lang_sense_clarification": "исследовать или осваивать предмет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a070cf0c-ae06-33e9-81b2-0998277cf1d1",
                    "sense_definition": "Iemand kennis of vaardigheden bijbrengen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "learning_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn vader <w>leerde</w> mij fietsen toen ik klein was.",
                            "target_lang_translations": {
                                "ru": "Мой отец <w>научил</w> меня кататься на велосипеде, когда я был маленьким."
                            }
                        },
                        {
                            "text": "De leraar <w>leert</w> de leerlingen hoe ze moeten rekenen.",
                            "target_lang_translations": {
                                "ru": "Учитель <w>учит</w> учеников считать."
                            }
                        }
                    ],
                    "synonyms": [
                        "onderwijzen"
                    ],
                    "common_phrases": [
                        "iemand iets leren",
                        "iemand manieren leren"
                    ],
                    "target_lang_definitions": {

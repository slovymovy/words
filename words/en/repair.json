                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переподключа́ть",
                                "target_lang_sense_clarification": "соединять заново"
                            },
                            {
                                "target_lang_word": "сопряга́ть за́ново",
                                "target_lang_sense_clarification": "технический термин для парных устройств"
                            }
                        ]
                    }
                }
            ]
        },
        {

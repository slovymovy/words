{
    "word_family": [
        "recoil",
        "uncoil"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8dd51d8f-4268-3f4b-876b-97baa429156a",
                    "sense_definition": "A length of rope, wire, or other flexible material wound into a series of concentric rings or spirals.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sailor threw a heavy <w>coil</w> of rope onto the dock.",
                            "target_lang_translations": {
                                "ru": "Матрос бросил тяжелый <w>моток</w> веревки на причал."
                            }
                        },
                        {
                            "text": "A <w>coil</w> of smoke rose slowly from the chimney.",
                            "target_lang_translations": {
                                "ru": "<w>Кольцо</w> дыма медленно поднималось из трубы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "coil of rope",
                        "coil of wire",
                        "mosquito coil"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отрезок веревки, проволоки или другого гибкого материала, свернутый в серию концентрических колец или спиралей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "моток",
                                "target_lang_sense_clarification": "свёрнутая верёвка или проволока"
                            },
                            {
                                "target_lang_word": "спираль",
                                "target_lang_sense_clarification": "форма объекта"
                            },
                            {
                                "target_lang_word": "кольцо",
                                "target_lang_sense_clarification": "форма дыма или пара"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1bf98849-3750-36a8-9d65-5e95ceb54b63",
                    "sense_definition": "A wire wound into a spiral, often used to conduct electricity or create a magnetic field.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "electrical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mechanic needed to replace the ignition <w>coil</w> in the engine.",
                            "target_lang_translations": {
                                "ru": "Механику нужно было заменить <w>катушку</w> зажигания в двигателе."
                            }
                        },
                        {
                            "text": "An electric current flows through the copper <w>coil</w> to generate heat.",
                            "target_lang_translations": {
                                "ru": "Электрический ток течет через медную <w>спираль</w> для выработки тепла."
                            }
                        }
                    ],
                    "synonyms": [
                        "inductor"
                    ],
                    "common_phrases": [
                        "ignition coil",
                        "heating coil",
                        "Tesla coil"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проволока, свернутая в спираль, часто используемая для проведения электричества или создания магнитного поля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "катушка",
                                "target_lang_sense_clarification": "электротехнический компонент"
                            },
                            {
                                "target_lang_word": "спираль",
                                "target_lang_sense_clarification": "нагревательный элемент"
                            },
                            {
                                "target_lang_word": "обмотка",
                                "target_lang_sense_clarification": "часть электрической цепи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89e1a2c4-1cd7-3b08-85c2-749ce8569065",
                    "sense_definition": "An intrauterine device (IUD) used for contraception.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She visited her doctor to discuss having a <w>coil</w> fitted.",
                            "target_lang_translations": {
                                "ru": "Она посетила врача, чтобы обсудить установку <w>спирали</w>."
                            }
                        },
                        {
                            "text": "The contraceptive <w>coil</w> is a very effective method of birth control.",
                            "target_lang_translations": {
                                "ru": "Контрацептивная <w>спираль</w> — очень эффективный метод контроля рождаемости."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fitted for a coil"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "Commonly referred to as 'the coil'."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутриматочная спираль (ВМС), используемая для контрацепции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спираль",
                                "target_lang_sense_clarification": "средство контрацепции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f3c3b5c5-6f0a-32fa-a706-2db22271dcb3",
                    "sense_definition": "A state of trouble, turmoil, or entanglement.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The poet wrote about the desire to shuffle off this mortal <w>coil</w>.",
                            "target_lang_translations": {
                                "ru": "Поэт писал о желании сбросить эту бренную <w>оболочку</w>."
                            }
                        },
                        {
                            "text": "He sought to escape the <w>coil</w> of daily worries and responsibilities.",
                            "target_lang_translations": {
                                "ru": "Он стремился уйти от <w>суеты</w> повседневных забот и обязанностей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mortal coil"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "Mostly found in the phrase 'mortal coil'."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние беспокойства, суматохи или запутанности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "суета",
                                "target_lang_sense_clarification": "беспокойство, тщетность"
                            },
                            {
                                "target_lang_word": "суматоха",
                                "target_lang_sense_clarification": "беспорядочная деятельность"
                            },
                            {
                                "target_lang_word": "шум",
                                "target_lang_sense_clarification": "перен. суматоха, скандал"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ffc3b8e5-a5f1-36e1-bcf3-8b2e6ba6adff",
                    "sense_definition": "To wind or arrange something into a series of rings, loops, or a spiral shape.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The snake began to <w>coil</w> itself around the tree branch.",
                            "target_lang_translations": {
                                "ru": "Змея начала <w>обвиваться</w> вокруг ветки дерева."
                            }
                        },
                        {
                            "text": "Please <w>coil</w> the garden hose neatly after you finish watering the plants.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, аккуратно <w>сверни</w> садовый шланг после того, как закончишь поливать растения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "coil up",
                        "coil around"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наматывать или укладывать что-либо кольцами, петлями или в форме спирали."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свёртывать",
                                "target_lang_sense_clarification": "скручивать в кольцо"
                            },
                            {
                                "target_lang_word": "наматывать",
                                "target_lang_sense_clarification": "навивать на что-либо"
                            },
                            {
                                "target_lang_word": "сворачивать",
                                "target_lang_sense_clarification": "укладывать кольцами"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "4623ab6f-095c-3c44-bf7e-18aa75386ba4",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Coil</w> signed the documents at the meeting.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Койл</w> подписал документы на встрече."
                            }
                        },
                        {
                            "text": "We are looking for a record of a family named <w>Coil</w> in the archives.",
                            "target_lang_translations": {
                                "ru": "Мы ищем в архивах запись о семье по фамилии <w>Койл</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Койл",
                                "target_lang_sense_clarification": "транслитерация фамилии"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
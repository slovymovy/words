                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "припи́сывать ло́жные показа́ния",
                                "target_lang_sense_clarification": "police context"
                            },
                            {
                                "target_lang_word": "фальсифици́ровать призна́ние",

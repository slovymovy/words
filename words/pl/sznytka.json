    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "bad9ed42-e6e0-3b51-b903-4b1f9132b26c",
                    "sense_definition": "Kromka chleba, kawałek pieczywa odkrojony z bochenka, często posmarowany czymś lub obłożony dodatkami; kanapka (używane głównie w gwarze poznańskiej).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "slice_of_bread",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na drugie śniadanie przygotowałem sobie <w>sznytkę</w> z serem i pomidorem.",
                            "target_lang_translations": {
                                "ru": "На второй завтрак я приготовил себе <w>бутерброд</w> с сыром и помидором."
                            }
                        },
                        {
                            "text": "Babcia zawsze częstowała nas świeżą <w>sznytkę</w> chleba z domowym smalcem.",
                            "target_lang_translations": {
                                "ru": "Бабушка всегда угощала нас свежим <w>ломтиком</w> хлеба с домашним смальцем."
                            }
                        }
                    ],
                    "synonyms": [
                        "kromka",
                        "sznitka"
                    ],
                    "common_phrases": [
                        "sznytka chleba",
                        "sznytka z masłem",
                        "sznytka z serem"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Poznań"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ломтик хлеба, кусок выпечки, отрезанный от буханки, часто намазанный чем-либо или с добавками; бутерброд (используется в основном в познанском диалекте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ломтик хлеба",
                                "target_lang_sense_clarification": "отрезанный кусок"
                            },
                            {
                                "target_lang_word": "бутерброд",
                                "target_lang_sense_clarification": "хлеб с добавками"
                            },
                            {
                                "target_lang_word": "кусок хлеба",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "656e0e60-100c-3d02-bbdc-6262575c2665",
                    "sense_definition": "Rodzaj niewielkiej bułki pszennej, która posiada charakterystyczne nacięcie pośrodku.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "type_of_roll",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W lokalnej piekarni kupiłem chrupiącą <w>sznytkę</w> prosto z gorącego pieca.",
                            "target_lang_translations": {
                                "ru": "В местной пекарне я купил хрустящую <w>булочку</w> прямо из горячей печи."
                            }
                        },
                        {
                            "text": "Ta pszenna <w>sznytka</w> smakuje najlepiej, gdy jest jeszcze ciepła i posmarowana masłem.",
                            "target_lang_translations": {
                                "ru": "Эта пшеничная <w>булочка</w> вкуснее всего, когда она еще теплая и намазана маслом."
                            }
                        }
                    ],
                    "synonyms": [
                        "sznitka"
                    ],
                    "common_phrases": [
                        "świeża sznytka",
                        "chrupiąca sznytka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разновидность небольшой пшеничной булочки, имеющей характерный надрез посередине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "булочка",
                                "target_lang_sense_clarification": "хлебобулочное изделие"
                            },
                            {
                                "target_lang_word": "булка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        }
    ]

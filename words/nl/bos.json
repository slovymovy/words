                },
                {
                    "sense_id": "f92097f6-2324-356f-a700-4139a73dca72",
                    "sense_definition": "Het oerwoud of tropisch regenwoud.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "nature_area",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn oom werkt diep in het <w>bos</w> van Suriname.",
                            "target_lang_translations": {

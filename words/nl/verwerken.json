{
    "word_family": [
        "verwerking",
        "verwerker",
        "onverwerkt"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fc7fe0b3-b79c-3035-bf41-53516077cf11",
                    "sense_definition": "Informatie, gegevens of administratieve taken behandelen en in een systeem invoeren of afhandelen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "processing_handling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De computer heeft even tijd nodig om alle nieuwe gegevens te <w>verwerken</w>.",
                            "target_lang_translations": {
                                "ru": "Компьютеру нужно немного времени, чтобы <w>обработать</w> все новые данные.",
                                "en": "The computer needs some time to <w>process</w> all the new data."
                            }
                        },
                        {
                            "text": "De administratie moet de binnengekomen facturen nog <w>verwerken</w>.",
                            "target_lang_translations": {
                                "ru": "Администрация еще должна <w>обработать</w> поступившие счета.",
                                "en": "The administration still has to <w>process</w> the received invoices."
                            }
                        }
                    ],
                    "synonyms": [
                        "afhandelen",
                        "behandelen",
                        "registreren"
                    ],
                    "common_phrases": [
                        "gegevens verwerken",
                        "orders verwerken",
                        "informatie verwerken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обрабатывать информацию, данные или выполнять административные задачи; вводить в систему.",
                        "en": "To handle information, data, or administrative tasks and enter them into a system or complete them."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обрабатывать",
                                "target_lang_sense_clarification": "данные, информацию, заказы"
                            },
                            {
                                "target_lang_word": "оформлять",
                                "target_lang_sense_clarification": "документы, счета"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "process",
                                "target_lang_sense_clarification": "data, information, orders"
                            },
                            {
                                "target_lang_word": "handle",
                                "target_lang_sense_clarification": "administrative tasks, paperwork"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3503afa4-6281-3d49-925d-bd54461be258",
                    "sense_definition": "Grondstoffen, ingrediënten of materialen gebruiken om er iets nieuws van te maken of het ergens in op te nemen.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "processing_handling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In deze fabriek <w>verwerken</w> ze oud papier tot nieuw karton.",
                            "target_lang_translations": {
                                "ru": "На этой фабрике макулатуру <w>перерабатывают</w> в новый картон.",
                                "en": "In this factory, they <w>process</w> old paper into new cardboard."
                            }
                        },
                        {
                            "text": "De kok heeft de restjes groenten <w>verwerkt</w> in een heerlijke soep.",
                            "target_lang_translations": {
                                "ru": "Повар <w>использовал</w> остатки овощей для приготовления вкусного супа.",
                                "en": "The cook <w>incorporated</w> the leftover vegetables into a delicious soup."
                            }
                        }
                    ],
                    "synonyms": [
                        "omzetten"
                    ],
                    "common_phrases": [
                        "in een product verwerken",
                        "tot iets verwerken",
                        "afval verwerken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать сырье или ингредиенты для создания чего-то нового; перерабатывать материал.",
                        "en": "To use raw materials or ingredients to create something new or include them in something."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перерабатывать",
                                "target_lang_sense_clarification": "превращать сырье в продукт"
                            },
                            {
                                "target_lang_word": "использовать",
                                "target_lang_sense_clarification": "включать в состав чего-либо"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "process",
                                "target_lang_sense_clarification": "raw materials into a product"
                            },
                            {
                                "target_lang_word": "incorporate",
                                "target_lang_sense_clarification": "ingredients or parts into a whole"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7294f10d-4f8d-388c-9461-7d59a592d0e3",
                    "sense_definition": "Geestelijk in het reine komen met een moeilijke gebeurtenis, verlies of emotie; accepteren en een plek geven.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het duurde lang voordat hij het overlijden van zijn vader kon <w>verwerken</w>.",
                            "target_lang_translations": {
                                "ru": "Прошло много времени, прежде чем он смог <w>пережить</w> смерть отца.",
                                "en": "It took a long time before he could <w>process</w> his father's death."
                            }
                        },
                        {
                            "text": "Ze heeft tijd nodig om het slechte nieuws te <w>verwerken</w>.",
                            "target_lang_translations": {
                                "ru": "Ей нужно время, чтобы <w>переварить</w> плохие новости.",
                                "en": "She needs time to <w>process</w> the bad news."
                            }
                        }
                    ],
                    "synonyms": [
                        "accepteren"
                    ],
                    "antonyms": [
                        "verdringen",
                        "opkroppen"
                    ],
                    "common_phrases": [
                        "een trauma verwerken",
                        "verdriet verwerken",
                        "het verlies verwerken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Психологически справляться с трудным событием или эмоциями; принять и пережить случившееся.",
                        "en": "To psychologically come to terms with a difficult event, loss, or emotion."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пережить",
                                "target_lang_sense_clarification": "справиться с горем или утратой"
                            },
                            {
                                "target_lang_word": "смириться",
                                "target_lang_sense_clarification": "принять ситуацию"
                            },
                            {
                                "target_lang_word": "переварить",
                                "target_lang_sense_clarification": "осмыслить информацию или новости (разг.)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "process",
                                "target_lang_sense_clarification": "emotions, trauma, news"
                            },
                            {
                                "target_lang_word": "come to terms with",
                                "target_lang_sense_clarification": "loss, grief"
                            },
                            {
                                "target_lang_word": "deal with",
                                "target_lang_sense_clarification": "difficult situations"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8aff9c8-4f3a-3091-84e8-2740007e1595",
                    "sense_definition": "Voedsel in het lichaam afbreken en opnemen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "processing_handling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn maag kan zware maaltijden 's avonds laat niet goed <w>verwerken</w>.",
                            "target_lang_translations": {
                                "ru": "Мой желудок не может нормально <w>переваривать</w> тяжелую пищу поздно вечером.",
                                "en": "My stomach cannot <w>digest</w> heavy meals well late at night."
                            }
                        },
                        {
                            "text": "Het lichaam moet hard werken om al dat vet te <w>verwerken</w>.",
                            "target_lang_translations": {
                                "ru": "Организму приходится тяжело работать, чтобы <w>переработать</w> весь этот жир.",
                                "en": "The body has to work hard to <w>process</w> all that fat."
                            }
                        }
                    ],
                    "synonyms": [
                        "verteren"
                    ],
                    "common_phrases": [
                        "voedsel verwerken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расщеплять и усваивать пищу в организме.",
                        "en": "To break down and absorb food in the body."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переваривать",
                                "target_lang_sense_clarification": "пищу"
                            },
                            {
                                "target_lang_word": "усваивать",
                                "target_lang_sense_clarification": "питательные вещества"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "digest",
                                "target_lang_sense_clarification": "food"
                            },
                            {
                                "target_lang_word": "process",
                                "target_lang_sense_clarification": "substances in the body"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
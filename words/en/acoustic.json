                    "semantic_group_id": "instrument_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He picked up his <w>acoustic</w> and began to strum a gentle melody.",
                            "target_lang_translations": {
                                "ru": "Он взял свою <w>акустику</w> и начал наигрывать нежную мелодию."
                            }

{
    "word_family": [
        "ochrona",
        "ochroniarz",
        "ochronny",
        "chronić",
        "schronisko",
        "schron"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7e644888-5dd8-3516-9a70-530484a0ee64",
                    "sense_definition": "Zabezpieczyć kogoś lub coś przed atakiem, zniszczeniem, niebezpieczeństwem lub szkodliwym działaniem.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "protection_transitive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten krem pozwoli skutecznie <w>ochronić</w> twoją skórę przed słońcem.",
                            "target_lang_translations": {
                                "ru": "Этот крем позволит эффективно <w>защитить</w> твою кожу от солнца."
                            }
                        },
                        {
                            "text": "Rodzice chcieli za wszelką cenę <w>ochronić</w> swoje dziecko przed stresem.",
                            "target_lang_translations": {
                                "ru": "Родители хотели любой ценой <w>уберечь</w> своего ребенка от стресса."
                            }
                        }
                    ],
                    "synonyms": [
                        "zabezpieczyć",
                        "osłonić",
                        "ustrzec",
                        "ocalić"
                    ],
                    "antonyms": [
                        "narazić",
                        "zagrozić"
                    ],
                    "common_phrases": [
                        "ochronić przed niebezpieczeństwem",
                        "ochronić przed zimnem",
                        "ochronić środowisko",
                        "ochronić prywatność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обезопасить кого-либо или что-либо от нападения, уничтожения, опасности или вредного воздействия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "защитить",
                                "target_lang_sense_clarification": "обезопасить от угрозы"
                            },
                            {
                                "target_lang_word": "уберечь",
                                "target_lang_sense_clarification": "сохранить в целости, не дать пострадать"
                            },
                            {
                                "target_lang_word": "оградить",
                                "target_lang_sense_clarification": "отделить от опасности или неприятностей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "287dda10-90c8-39d3-aa1c-a7c7d1daea28",
                    "sense_definition": "Zabezpieczyć samego siebie przed czymś groźnym lub szkodliwym; znaleźć schronienie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "protection_reflexive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podróżni weszli do jaskini, aby <w>ochronić</w> się przed nadchodzącą burzą.",
                            "target_lang_translations": {
                                "ru": "Путешественники вошли в пещеру, чтобы <w>укрыться</w> от надвигающейся бури."
                            }
                        },
                        {
                            "text": "Nie zdołasz <w>ochronić</w> się przed konsekwencjami swoich złych decyzji.",
                            "target_lang_translations": {
                                "ru": "Ты не сможешь <w>уберечься</w> от последствий своих плохих решений."
                            }
                        }
                    ],
                    "synonyms": [
                        "zabezpieczyć się",
                        "schronić się",
                        "ukryć się",
                        "osłonić się"
                    ],
                    "antonyms": [
                        "narazić się"
                    ],
                    "common_phrases": [
                        "ochronić się przed deszczem",
                        "ochronić się przed atakiem",
                        "ochronić się przed krytyką"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обезопасить самого себя от чего-либо угрожающего или вредного; найти убежище."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "защититься",
                                "target_lang_sense_clarification": "обезопасить себя"
                            },
                            {
                                "target_lang_word": "укрыться",
                                "target_lang_sense_clarification": "спрятаться, найти убежище"
                            },
                            {
                                "target_lang_word": "уберечься",
                                "target_lang_sense_clarification": "избежать опасности или вреда"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
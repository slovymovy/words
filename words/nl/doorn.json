                {
                    "sense_id": "eeb29748-a886-363f-a2ac-f28cc7453bba",
                    "sense_definition": "Een scherp, puntig uitsteeksel aan de stengel of tak van een plant.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "sharp_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pas op dat je je niet prikt aan de scherpe <w>doornen</w> van de roos.",
                            "target_lang_translations": {
                                "ru": "Осторожно, не уколись об острые <w>шипы</w> розы."
                            }
                        },
                        {
                            "text": "Dat lelijke gebouw is mij al jaren een <w>doorn</w> in het oog.",
                            "target_lang_translations": {
                                "ru": "Это уродливое здание уже много лет для меня как <w>бельмо</w> на глазу."
                            }
                        }
                    ],
                    "synonyms": [
                        "stekel",
                        "doren"
                    ],
                    "common_phrases": [
                        "een doorn in het oog",
                        "geen rozen zonder doornen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Острый, колючий выступ на стебле или ветке растения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шип",
                                "target_lang_sense_clarification": "ботанический термин"
                            },
                            {
                                "target_lang_word": "колючка",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c0b751e4-bbac-3de2-bf4a-68818d61acd9",
                    "sense_definition": "Kształt ciała ludzkiego, wygląd zewnętrzny sylwetki.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "shape_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ona ćwiczy codziennie, aby mieć wysportowaną <w>figurę</w>.",
                            "target_lang_translations": {
                                "ru": "Она тренируется каждый день, чтобы иметь спортивную <w>фигуру</w>."
                            }
                        },
                        {
                            "text": "Po ciąży bardzo szybko wróciła do swojej dawnej <w>figury</w>.",
                            "target_lang_translations": {
                                "ru": "После беременности она очень быстро вернулась к своей прежней <w>фигуре</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sylwetka"
                    ],
                    "common_phrases": [
                        "zgrabna figura",
                        "dbać o figurę",
                        "trzymać figurę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очертания человеческого тела, внешний вид силуэта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фигура",
                                "target_lang_sense_clarification": "телосложение"
                            },
                            {
                                "target_lang_word": "телосложение",
                                "target_lang_sense_clarification": "конституция тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ece13dd2-d316-3a44-b173-5a8d8293c21b",
                    "sense_definition": "Obiekt geometryczny, zbiór punktów na płaszczyźnie lub w przestrzeni (np. trójkąt, koło, sześcian).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "shape_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Trójkąt to podstawowa <w>figura</w> geometryczna poznawana w szkole.",
                            "target_lang_translations": {
                                "ru": "Треугольник — это базовая геометрическая <w>фигура</w>, которую изучают в школе."
                            }
                        },
                        {
                            "text": "Nauczyciel kazał nam obliczyć pole powierzchni tej <w>figury</w>.",
                            "target_lang_translations": {
                                "ru": "Учитель велел нам вычислить площадь поверхности этой <w>фигуры</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "figura geometryczna",
                        "figura płaska",
                        "figura przestrzenna",
                        "pole figury"
                    ],
                    "target_lang_definitions": {
                        "ru": "Геометрический объект, множество точек на плоскости или в пространстве (например, треугольник, круг, куб)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фигура",
                                "target_lang_sense_clarification": "геометрическая форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f26d44b6-af0f-397a-806e-5d5957e6bfaa",
                    "sense_definition": "Rzeźba przedstawiająca postać człowieka, zwierzęcia lub istoty fantastycznej.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sculpture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W parku stała kamienna <w>figura</w> lwa, na którą wspinały się dzieci.",
                            "target_lang_translations": {
                                "ru": "В парке стояла каменная <w>фигура</w> льва, на которую взбирались дети."
                            }
                        },
                        {
                            "text": "Muzeum zamówiło nową <w>figurę</w> woskową znanego aktora.",
                            "target_lang_translations": {
                                "ru": "Музей заказал новую восковую <w>фигуру</w> известного актера."
                            }
                        }
                    ],
                    "synonyms": [
                        "posąg",
                        "statua"
                    ],
                    "common_phrases": [
                        "figura woskowa",
                        "figura z brązu",
                        "stać jak figura"
                    ],
                    "target_lang_definitions": {
                        "ru": "Скульптура, изображающая человека, животное или фантастическое существо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "статуя",
                                "target_lang_sense_clarification": "скульптура"
                            },
                            {
                                "target_lang_word": "фигура",
                                "target_lang_sense_clarification": "скульптурное изображение"
                            },
                            {
                                "target_lang_word": "изваяние",
                                "target_lang_sense_clarification": "книжн."

{
    "word_family": [
        "hoop",
        "hopen",
        "ophopen"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4a61a0f7-be66-3478-a03c-e1e4102a6c72",
                    "sense_definition": "In een toestand van grote wanorde of chaos.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben het hele huis <w>overhoop</w> gehaald om het document te vinden.",
                            "target_lang_translations": {
                                "en": "We turned the whole house <w>upside down</w> to find the document.",
                                "ru": "Мы перевернули весь дом <w>вверх дном</w>, чтобы найти документ."
                            }
                        },
                        {
                            "text": "Door de onverwachte storm lagen al onze reisplannen <w>overhoop</w>.",
                            "target_lang_translations": {
                                "en": "Due to the unexpected storm, all our travel plans were <w>in a mess</w>.",
                                "ru": "Из-за неожиданного шторма все наши планы на поездку пошли <w>кувырком</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wanordelijk",
                        "rommelig",
                        "chaotisch"
                    ],
                    "antonyms": [
                        "ordelijk",
                        "opgeruimd",
                        "overzichtelijk"
                    ],
                    "common_phrases": [
                        "overhoop halen",
                        "overhoop gooien",
                        "alles overhoop"
                    ],
                    "target_lang_definitions": {
                        "en": "In a state of great disorder or chaos.",
                        "ru": "В состоянии сильного беспорядка или хаоса."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "upside down",
                                "target_lang_sense_clarification": "physical disorder, often used with 'turn'"
                            },
                            {
                                "target_lang_word": "in a mess",
                                "target_lang_sense_clarification": "general state of disorder"
                            },
                            {
                                "target_lang_word": "in disarray",
                                "target_lang_sense_clarification": "chaotic state"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вверх дном",
                                "target_lang_sense_clarification": "о физическом беспорядке"
                            },
                            {
                                "target_lang_word": "кувырком",
                                "target_lang_sense_clarification": "о планах или ситуации"
                            },
                            {
                                "target_lang_word": "в беспорядке",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c4206f63-8e2a-338d-af78-29fbaae12b66",
                    "sense_definition": "In een toestand van ruzie of onenigheid met iemand.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die twee politieke partijen liggen altijd met elkaar <w>overhoop</w>.",
                            "target_lang_translations": {
                                "en": "Those two political parties are always <w>at odds</w> with each other.",
                                "ru": "Эти две политические партии всегда <w>в ссоре</w> друг с другом."
                            }
                        },
                        {
                            "text": "Hij ligt al jaren <w>overhoop</w> met zijn buren over de schutting.",
                            "target_lang_translations": {
                                "en": "He has been <w>at odds</w> with his neighbors about the fence for years.",
                                "ru": "Он уже много лет <w>в ссоре</w> со своими соседями из-за забора."
                            }
                        }
                    ],
                    "synonyms": [
                        "gebrouilleerd"
                    ],
                    "antonyms": [
                        "eensgezind",
                        "unaniem"
                    ],
                    "common_phrases": [
                        "overhoop liggen",
                        "met elkaar overhoop"
                    ],
                    "target_lang_definitions": {
                        "en": "In a state of conflict or disagreement with someone.",
                        "ru": "В состоянии ссоры или разногласий с кем-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "at odds",
                                "target_lang_sense_clarification": "in disagreement"
                            },
                            {
                                "target_lang_word": "in conflict",
                                "target_lang_sense_clarification": "fighting or arguing"
                            },
                            {
                                "target_lang_word": "at loggerheads",
                                "target_lang_sense_clarification": "strong disagreement"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "в ссоре",
                                "target_lang_sense_clarification": "состояние конфликта"
                            },
                            {
                                "target_lang_word": "не в ладах",
                                "target_lang_sense_clarification": "иметь разногласия"
                            },
                            {
                                "target_lang_word": "на ножах",
                                "target_lang_sense_clarification": "идиома: сильная вражда"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
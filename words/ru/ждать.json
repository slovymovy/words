    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9051f15f-f2d7-31a5-8a56-36b060eb9865",
                    "sense_definition": "Находиться где-либо или проводить время в бездействии до появления кого-либо или наступления чего-либо.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_wait",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы будем <w>ждать</w> вас завтра вечером около входа в театр.",
                            "target_lang_translations": {
                                "en": "We will <w>wait</w> for you tomorrow evening near the theater entrance."
                            }
                        },
                        {
                            "text": "Пассажиры вынуждены были долго <w>ждать</w> своего поезда на холодном перроне.",
                            "target_lang_translations": {
                                "en": "The passengers were forced to <w>wait</w> a long time for their train on the cold platform."
                            }
                        }
                    ],
                    "synonyms": [
                        "дожидаться",
                        "поджидать",
                        "ожидать"
                    ],
                    "common_phrases": [
                        "ждать автобуса",
                        "ждать в гости",
                        "заставить себя ждать",
                        "ждать своей очереди"
                    ],
                    "target_lang_definitions": {
                        "en": "To stay somewhere or spend time doing nothing until someone appears or something happens."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wait",
                                "target_lang_sense_clarification": "general action"
                            },
                            {
                                "target_lang_word": "wait for",
                                "target_lang_sense_clarification": "until someone or something arrives"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c72077b-0d7b-39e8-8a90-066f83d07cda",
                    "sense_definition": "Надеяться на что-либо, предполагать наступление чего-либо или рассчитывать на это.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "expectation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <w>жду</w> от него искренних извинений за вчерашний поступок.",
                            "target_lang_translations": {
                                "en": "I <w>expect</w> sincere apologies from him for yesterday's action."
                            }
                        },
                        {
                            "text": "Мы не можем просто сидеть и <w>ждать</w>, что проблема решится сама собой.",
                            "target_lang_translations": {
                                "en": "We cannot just sit and <w>wait</w> for the problem to resolve itself."
                            }
                        }
                    ],
                    "synonyms": [
                        "ожидать",
                        "чаять"
                    ],
                    "common_phrases": [
                        "ждать помощи",
                        "ждать звонка",
                        "ждать у моря погоды",
                        "ждать чуда"
                    ],
                    "target_lang_definitions": {
                        "en": "To hope for something, anticipate that it will happen, or count on it."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "expect",
                                "target_lang_sense_clarification": "to anticipate or count on"
                            },
                            {
                                "target_lang_word": "wait for",
                                "target_lang_sense_clarification": "to look forward to"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a00d4509-b39d-3c9e-bac7-bd13349f7272",
                    "sense_definition": "Быть уготованным, предстоять кому-либо (о событиях, судьбе или будущем).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "destiny",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Победителей этого сложного конкурса <w>ждут</w> ценные призы и всеобщее признание.",
                            "target_lang_translations": {
                                "en": "Valuable prizes and universal recognition <w>await</w> the winners of this difficult competition."
                            }
                        },
                        {
                            "text": "Никто из путешественников не знал, что <w>ждёт</w> их за поворотом горной дороги.",
                            "target_lang_translations": {
                                "en": "None of the travelers knew what <w>awaited</w> them around the bend of the mountain road."
                            }
                        }
                    ],
                    "synonyms": [
                        "ожидать"
                    ],
                    "common_phrases": [
                        "тебя ждёт успех",
                        "нас ждут перемены",
                        "впереди ждёт неизвестность"
                    ],
                    "target_lang_definitions": {
                        "en": "To be in store for or lie ahead of someone (regarding events, fate, or the future)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "await",
                                "target_lang_sense_clarification": "formal, regarding future events"
                            },
                            {
                                "target_lang_word": "be in store for",
                                "target_lang_sense_clarification": "regarding destiny"
                            },
                            {
                                "target_lang_word": "wait for",
                                "target_lang_sense_clarification": "general usage"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc4bba33-a69a-3bc5-acb1-0597fd6cc1e9",
                    "sense_definition": "Медлить с действием, откладывать что-либо на потом.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "delay",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хватит <w>ждать</w> идеального момента, нужно начинать действовать прямо сейчас.",
                            "target_lang_translations": {
                                "en": "Stop <w>waiting</w> for the perfect moment, we need to start acting right now."
                            }
                        },
                        {
                            "text": "Время не <w>ждёт</w>, поэтому мы должны принять важное решение немедленно.",
                            "target_lang_translations": {
                                "en": "Time does not <w>wait</w>, so we must make an important decision immediately."
                            }
                        }
                    ],
                    "synonyms": [
                        "медлить",
                        "мешкать",
                        "тянуть время",
                        "погодить"
                    ],
                    "antonyms": [
                        "спешить",
                        "торопиться"
                    ],
                    "common_phrases": [
                        "время не ждёт",
                        "дело не ждёт",
                        "хватит ждать"
                    ],
                    "target_lang_definitions": {
                        "en": "To delay an action or put something off for later."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wait",
                                "target_lang_sense_clarification": "to delay acting"
                            },
                            {
                                "target_lang_word": "delay",
                                "target_lang_sense_clarification": "to postpone"
                            },
                            {
                                "target_lang_word": "hold off",
                                "target_lang_sense_clarification": "to pause before acting"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ed324bef-753b-3dff-9a65-2b0dcdfbc8bd",
                    "sense_definition": "Форма прошедшего времени мужского рода единственного числа изъявительного наклонения глагола <w>смочь</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он очень старался и в итоге <смог> сдать этот трудный экзамен.",
                            "target_lang_translations": {
                                "en": "He tried very hard and eventually <w>managed</w> to pass this difficult exam."
                            }
                        },
                        {
                            "text": "К сожалению, я не <смог> прийти на нашу встречу вовремя.",
                            "target_lang_translations": {
                                "en": "Unfortunately, I <w>could</w> not come to our meeting on time."
                            }
                        }
                    ],
                    "synonyms": [
                        "сумел"
                    ],
                    "common_phrases": [
                        "я не смог",
                        "он смог сделать",
                        "едва смог",
                        "кто смог"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense masculine singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Past tense form of the verb 'to be able'; indicates ability or successful completion of an action in the past."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "could",
                                "target_lang_sense_clarification": "general ability in the past"
                            },
                            {
                                "target_lang_word": "was able to",
                                "target_lang_sense_clarification": "ability on a specific occasion"
                            },
                            {
                                "target_lang_word": "managed to",
                                "target_lang_sense_clarification": "implies success despite difficulty"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d81c6e2b-7a9d-3eb9-acd3-349e6459339e",
                    "sense_definition": "Густая пелена тумана в сочетании с дымом, копотью и выхлопными газами, характерная для больших городов.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за лесных пожаров город накрыл едкий и густой <смог>.",
                            "target_lang_translations": {
                                "en": "Due to forest fires, the city was covered by acrid and thick <w>smog</w>."
                            }
                        },
                        {
                            "text": "Наряду с информацией о погоде во многих городах сейчас сообщают и прогноз уровня <смога>.",
                            "target_lang_translations": {
                                "en": "Along with weather information, many cities now report the <w>smog</w> level forecast."
                            }
                        }
                    ],
                    "common_phrases": [
                        "густой смог",
                        "смог над городом",
                        "ядовитый смог",
                        "сильный смог"
                    ],
                    "target_lang_definitions": {
                        "en": "A type of intense air pollution; a mixture of smoke and fog."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "smog",
                                "target_lang_sense_clarification": "air pollution"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
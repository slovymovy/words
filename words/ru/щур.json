    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "66643792-9a32-3b74-98f8-a021f7ebf724",
                    "sense_definition": "Певчая птица семейства вьюрковых с ярким оперением (малиновым у самцов), обитающая в хвойных лесах.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бесшумно пролетела вещая птица <w>щур</w>, которую я так давно хотел увидеть.",
                            "target_lang_translations": {
                                "en": "The prophetic <w>pine grosbeak</w>, which I had wanted to see for so long, flew by silently."
                            }
                        },
                        {
                            "text": "Появление <w>щуров</w> в наших краях часто считается верным признаком суровой зимы.",
                            "target_lang_translations": {
                                "en": "The appearance of <w>pine grosbeaks</w> in our region is often considered a sure sign of a harsh winter."
                            }
                        }
                    ],
                    "synonyms": [
                        "финский попугай"
                    ],
                    "common_phrases": [
                        "стая щуров",
                        "пение щура"
                    ],
                    "target_lang_definitions": {
                        "en": "A songbird of the finch family with bright plumage (crimson in males), found in coniferous forests."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pine grosbeak",
                                "target_lang_sense_clarification": "bird species"
                            },
                            {
                                "target_lang_word": "grosbeak",
                                "target_lang_sense_clarification": "bird family"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fbffbc1-e717-3f37-8c60-0c6821cba8c5",
                    "sense_definition": "Далёкий предок, родоначальник рода.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "ancestor_myth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый <w>щур</w> нашего рода почитался потомками как хранитель традиций.",
                            "target_lang_translations": {
                                "en": "Every <w>ancestor</w> of our lineage was revered by descendants as a keeper of traditions."
                            }
                        },
                        {
                            "text": "В древних сказаниях упоминается великий <w>щур</w>, основавший это поселение.",
                            "target_lang_translations": {
                                "en": "Ancient legends mention a great <w>forefather</w> who founded this settlement."
                            }
                        }
                    ],
                    "synonyms": [
                        "предок",
                        "пращур"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A distant ancestor or progenitor of a lineage."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ancestor",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "forefather",
                                "target_lang_sense_clarification": "progenitor"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d9b79599-f85a-3dd3-bc5d-5b1e4c97d548",
                    "sense_definition": "В славянской мифологии: дух предка, охраняющий домашний очаг и семью.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "ancestor_myth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Славяне верили, что <w>щур</w> незримо присутствует в доме и бережет его от бед.",
                            "target_lang_translations": {
                                "en": "Slavs believed that the <w>ancestral spirit</w> is invisibly present in the house and protects it from troubles."
                            }
                        },
                        {
                            "text": "Во дворе, по поверьям, хозяйничал <w>щур</w>, которого нужно было уважать.",
                            "target_lang_translations": {
                                "en": "According to beliefs, a <w>spirit</w> ruled in the courtyard, who had to be respected."
                            }
                        }
                    ],
                    "synonyms": [
                        "домовой",
                        "домовик",
                        "кутный бог"
                    ],
                    "target_lang_definitions": {
                        "en": "In Slavic mythology, an ancestral spirit that protects the home and family."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ancestral spirit",
                                "target_lang_sense_clarification": "mythology"
                            },
                            {
                                "target_lang_word": "chur",
                                "target_lang_sense_clarification": "Slavic mythology term"
                            }
                        ]
                    }
                }
            ]
        },
        {

                {
                    "sense_id": "12993a46-32de-342e-9290-c7fe6d40ffb8",
                    "sense_definition": "Рот зверя, рыбы или другого животного.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [

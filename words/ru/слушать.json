    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0e3c40af-4f5c-350d-825f-cbc7607ebae6",
                    "sense_definition": "Воспринимать звуки, речь или музыку, сосредотачивая на них внимание.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "perception",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я люблю <слушать> классическую музыку по вечерам.",
                            "target_lang_translations": {
                                "en": "I like to <listen> to classical music in the evenings."
                            }
                        },
                        {
                            "text": "Студенты внимательно <слушают> объяснения преподавателя.",
                            "target_lang_translations": {
                                "en": "The students are <listening> attentively to the teacher's explanations."
                            }
                        }
                    ],
                    "antonyms": [
                        "пропускать"
                    ],
                    "common_phrases": [
                        "слушать музыку",
                        "слушать радио",
                        "внимательно слушать",
                        "слушать новости"
                    ],
                    "target_lang_definitions": {
                        "en": "To perceive sounds, speech, or music by focusing attention on them."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "listen",
                                "target_lang_sense_clarification": "to pay attention to sound"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cd67eff-8dfc-3be9-8691-db85c4f289e2",
                    "sense_definition": "Следовать чьим-то советам, доверять чьему-то мнению или подчиняться.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "obedience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Дети должны <слушать> своих родителей и учителей.",
                            "target_lang_translations": {
                                "en": "Children must <listen> to their parents and teachers."
                            }
                        },
                        {
                            "text": "Он никогда не <слушает> полезных советов и делает всё по-своему.",
                            "target_lang_translations": {
                                "en": "He never <listens> to useful advice and does everything his own way."
                            }
                        }
                    ],
                    "synonyms": [
                        "слушаться",
                        "доверять"
                    ],
                    "common_phrases": [
                        "слушать родителей",
                        "слушать советы",
                        "слушать старших"
                    ],
                    "target_lang_definitions": {
                        "en": "To follow someone's advice, trust their opinion, or obey instructions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "listen",
                                "target_lang_sense_clarification": "to follow advice or instructions"
                            },
                            {
                                "target_lang_word": "obey",
                                "target_lang_sense_clarification": "to do what one is told"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fcc31d81-c7da-399f-a72f-c78d024614be",
                    "sense_definition": "Используется в повелительном наклонении для привлечения внимания собеседника к своей речи.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "discourse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Слушай>, давай встретимся завтра в парке?",
                            "target_lang_translations": {
                                "en": "<Listen>, let's meet in the park tomorrow?"
                            }
                        },
                        {
                            "text": "<Слушайте>, я совсем забыл рассказать вам эту историю!",
                            "target_lang_translations": {
                                "en": "<Listen>, I completely forgot to tell you this story!"
                            }
                        }
                    ],
                    "common_phrases": [
                        "слушай сюда",
                        "послушай меня"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used as a discourse marker"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used in the imperative mood to draw the listener's attention to what is being said."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "listen",
                                "target_lang_sense_clarification": "used to attract attention"
                            },
                            {
                                "target_lang_word": "look",
                                "target_lang_sense_clarification": "colloquial attention getter"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16f1effa-3107-3e57-a833-c9e119f4ec8c",
                    "sense_definition": "Изучать какой-либо предмет, посещая лекции или занятия.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "study",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В этом семестре мы будем <слушать> курс лекций по философии.",
                            "target_lang_translations": {
                                "en": "This semester we will <attend> a course of lectures on philosophy."
                            }
                        },
                        {
                            "text": "Мой друг поехал в университет <слушать> лекции известных профессоров.",
                            "target_lang_translations": {
                                "en": "My friend went to the university to <listen> to lectures by famous professors."
                            }
                        }
                    ],
                    "synonyms": [
                        "изучать"
                    ],
                    "antonyms": [
                        "пропускать"
                    ],
                    "common_phrases": [
                        "слушать курс",
                        "слушать лекции"
                    ],
                    "target_lang_definitions": {
                        "en": "To study a subject by attending lectures or classes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "attend",
                                "target_lang_sense_clarification": "lectures or courses"
                            },
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "a course or class"
                            },
                            {
                                "target_lang_word": "audit",
                                "target_lang_sense_clarification": "to attend a class without credit"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4bfc6e5-2a08-39c5-85e4-6866987f2f88",
                    "sense_definition": "Выражение согласия или готовности исполнить приказ (обычно в ответе на распоряжение).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "obedience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "— Принесите мне документы. — <Слушаю>, Иван Петрович.",
                            "target_lang_translations": {
                                "en": "— Bring me the documents. — <Yes, sir>, Ivan Petrovich."
                            }
                        },
                        {
                            "text": "Офицер отдал приказ, и солдат громко ответил: «<Слушаю>!»",
                            "target_lang_translations": {
                                "en": "The officer gave the order, and the soldier answered loudly: \"<Yes, sir>!\""
                            }
                        }
                    ],
                    "common_phrases": [
                        "слушаю и повинуюсь"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "military or formal context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An expression of agreement or readiness to execute an order (usually in response to a command)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "yes, sir",
                                "target_lang_sense_clarification": "military or formal response"
                            },
                            {
                                "target_lang_word": "at your command",
                                "target_lang_sense_clarification": "formal expression of readiness"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5f7de3d6-1cf6-3b05-8fd2-39f690d7209f",
                    "sense_definition": "Прослушивать работу внутренних органов для медицинской диагностики.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач взял стетоскоп, чтобы <слушать> сердце пациента.",
                            "target_lang_translations": {
                                "en": "The doctor took a stethoscope to <listen> to the patient's heart."
                            }
                        },
                        {
                            "text": "Доктор внимательно <слушал> дыхание больного и делал записи.",
                            "target_lang_translations": {
                                "en": "The doctor carefully <listened> to the patient's breathing and made notes."
                            }
                        }
                    ],
                    "synonyms": [
                        "выслушивать"
                    ],
                    "common_phrases": [
                        "слушать сердце",
                        "слушать легкие",
                        "слушать дыхание"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "medical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To examine the sounds of internal organs for medical diagnosis."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "listen to",
                                "target_lang_sense_clarification": "examine sounds of the body"
                            },
                            {
                                "target_lang_word": "auscultate",
                                "target_lang_sense_clarification": "medical term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "58f66b33-7970-333e-a0e3-bb3d114e162f",
                    "sense_definition": "Официально рассматривать и обсуждать судебное дело.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Городской суд будет <слушать> это сложное дело на следующей неделе.",
                            "target_lang_translations": {
                                "en": "The city court will <hear> this complex case next week."
                            }
                        },
                        {
                            "text": "Судья начал <слушать> показания свидетелей защиты.",
                            "target_lang_translations": {
                                "en": "The judge began to <hear> the testimony of the defense witnesses."
                            }
                        }
                    ],
                    "antonyms": [
                        "откладывать"
                    ],
                    "common_phrases": [
                        "слушать дело"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "legal context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To officially examine and discuss a legal case."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hear",
                                "target_lang_sense_clarification": "a legal case or testimony"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "317a2486-37bf-3d8e-a890-c7b1632c8ab7",
                    "sense_definition": "Een opleiding of reeks lessen die overdag wordt gegeven, in tegenstelling tot in de avond.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij heeft besloten om een <w>dagcursus</w> te volgen in plaats van te werken.",
                            "target_lang_translations": {
                                "en": "She decided to take a <w>day course</w> instead of working.",
                                "ru": "Она решила записаться на <w>дневной курс</w> вместо того, чтобы работать."
                            }
                        },
                        {
                            "text": "De <w>dagcursus</w> begint elke ochtend om negen uur en eindigt om vier uur.",
                            "target_lang_translations": {
                                "en": "The <w>day course</w> starts every morning at nine o'clock and ends at four o'clock.",
                                "ru": "<w>Дневной курс</w> начинается каждое утро в девять часов и заканчивается в четыре."
                            }
                        }
                    ],
                    "antonyms": [
                        "avondcursus"
                    ],
                    "common_phrases": [
                        "een dagcursus volgen",
                        "inschrijven voor een dagcursus"
                    ],
                    "target_lang_definitions": {
                        "en": "A course or series of lessons given during the day, as opposed to in the evening.",
                        "ru": "Обучение или цикл занятий, проводимых в дневное время, в отличие от вечерних курсов."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "day course",
                                "target_lang_sense_clarification": "classes held during daytime hours"
                            },
                            {
                                "target_lang_word": "daytime course",
                                "target_lang_sense_clarification": "educational program during the day"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "дневной курс",
                                "target_lang_sense_clarification": "обучение в дневное время"
                            },
                            {
                                "target_lang_word": "дневное обучение",
                                "target_lang_sense_clarification": "форма обучения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5702b3f-fc58-3c6d-9cdd-8151c0758ec2",
                    "sense_definition": "Een korte opleiding of training die in één enkele dag wordt afgerond.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de <w>dagcursus</w> leerden we de basisbeginselen van eerste hulp.",
                            "target_lang_translations": {
                                "en": "During the <w>one-day course</w> we learned the basics of first aid.",
                                "ru": "Во время <w>однодневного курса</w> мы изучили основы оказания первой помощи."
                            }
                        },
                        {
                            "text": "Is een <w>dagcursus</w> voldoende om dit ingewikkelde computerprogramma te begrijpen?",
                            "target_lang_translations": {
                                "en": "Is a <w>one-day course</w> sufficient to understand this complicated computer program?",
                                "ru": "Достаточно ли <w>однодневного курса</w>, чтобы понять эту сложную компьютерную программу?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "een intensieve dagcursus",
                        "korte dagcursus"
                    ],
                    "target_lang_definitions": {
                        "en": "A short course or training that is completed in a single day.",
                        "ru": "Краткий курс или тренинг, который завершается за один день."
                    },
                    "translations": {
                        "en": [

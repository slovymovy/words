    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e49be5c3-5be0-37d9-8e94-3b50af9b5ccd",
                    "sense_definition": "Flexible material in a roll with a sticky surface on one or both sides, used to stick things together.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "adhesive_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hand me some <w>tape</w> so I can fix this torn paper.",
                            "target_lang_translations": {
                                "ru": "Передай мне <w>скотч</w>, чтобы я мог починить эту порванную бумагу."
                            }
                        },
                        {
                            "text": "She sealed the cardboard box with clear shipping <w>tape</w>.",
                            "target_lang_translations": {
                                "ru": "Она заклеила картонную коробку прозрачным упаковочным <w>скотчем</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "adhesive tape",
                        "sticky tape"
                    ],
                    "common_phrases": [
                        "duct tape",
                        "masking tape",
                        "scotch tape",
                        "electrical tape"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гибкая лента с липким слоем, используемая для склеивания предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скотч",
                                "target_lang_sense_clarification": "самое распространенное разговорное название"
                            },
                            {
                                "target_lang_word": "клейкая лента",
                                "target_lang_sense_clarification": "формальное название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a175179d-94fe-3c43-8b4d-8d60afeb8ab9",
                    "sense_definition": "A cassette or reel containing a magnetic strip used for recording audio or video; also, the recording itself.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "recording_media",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Did you manage to get the interview on <w>tape</w>?",
                            "target_lang_translations": {
                                "ru": "Тебе удалось записать интервью на <w>плёнку</w>?"
                            }
                        },
                        {
                            "text": "I found a box of old music <w>tapes</w> in the attic.",
                            "target_lang_translations": {
                                "ru": "Я нашел коробку со старыми музыкальными <w>кассетами</w> на чердаке."
                            }
                        }
                    ],
                    "synonyms": [
                        "cassette",
                        "recording",
                        "videotape"
                    ],
                    "common_phrases": [
                        "on tape",
                        "mix tape",
                        "sex tape",
                        "tape recorder"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кассета или катушка с магнитной лентой для записи звука или видео; также сама запись."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кассета",
                                "target_lang_sense_clarification": "носитель информации"
                            },
                            {
                                "target_lang_word": "плёнка",
                                "target_lang_sense_clarification": "магнитная лента"
                            },
                            {
                                "target_lang_word": "запись",
                                "target_lang_sense_clarification": "результат записи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72e03e47-e13e-3a96-bbff-b4b639217c12",
                    "sense_definition": "Thin and flat paper, plastic, or similar flexible material, usually produced in the form of a long strip or roll.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "strip_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The machine reads data from a long strip of magnetic <w>tape</w>.",
                            "target_lang_translations": {
                                "ru": "Машина считывает данные с длинной полосы магнитной <w>ленты</w>."
                            }
                        },
                        {
                            "text": "We used colorful paper <w>tape</w> to decorate the room for the party.",
                            "target_lang_translations": {
                                "ru": "Мы использовали цветную бумажную <w>ленту</w>, чтобы украсить комнату к вечеринке."
                            }
                        }
                    ],
                    "synonyms": [
                        "strip",
                        "band",
                        "ribbon"
                    ],
                    "common_phrases": [
                        "magnetic tape",
                        "ticker tape",
                        "measuring tape"
                    ],
                    "target_lang_definitions": {
                        "ru": "Узкая длинная полоса из бумаги, пластика или ткани."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лента",
                                "target_lang_sense_clarification": "полоса материала"
                            },
                            {
                                "target_lang_word": "тесьма",
                                "target_lang_sense_clarification": "тканевая полоска"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fe14952-f210-3f55-911f-33dc648ab22f",
                    "sense_definition": "A finishing strip stretched across a track to mark the end of a race.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sports_equipment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The runner sprinted forward to break the <w>tape</w> first.",
                            "target_lang_translations": {
                                "ru": "Бегун рванул вперед, чтобы первым порвать <w>финишную ленту</w>."
                            }
                        },
                        {
                            "text": "Officials stretched the white <w>tape</w> across the finish line.",
                            "target_lang_translations": {
                                "ru": "Судьи натянули белую <w>ленту</w> поперек финишной черты."
                            }
                        }
                    ],
                    "synonyms": [
                        "finish line"
                    ],
                    "common_phrases": [
                        "break the tape"
                    ],
                    "target_lang_definitions": {
                        "ru": "Финишная лента, которую разрывает победитель забега."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "финишная лента",
                                "target_lang_sense_clarification": "спорт"
                            },
                            {
                                "target_lang_word": "ленточка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "22d76791-2986-363b-bdca-488670e4c67b",
                    "sense_definition": "Time-consuming bureaucratic procedures that delay progress.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "bureaucracy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to cut through the <w>tape</w> to get this project approved quickly.",
                            "target_lang_translations": {
                                "ru": "Нам нужно преодолеть <w>волокиту</w>, чтобы быстро утвердить этот проект."
                            }
                        },
                        {
                            "text": "Government <w>tape</w> often slows down small business growth.",
                            "target_lang_translations": {
                                "ru": "Правительственная <w>бюрократия</w> часто замедляет рост малого бизнеса."
                            }
                        }
                    ],
                    "synonyms": [
                        "red tape",
                        "bureaucracy"
                    ],
                    "common_phrases": [
                        "cut through the tape"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for 'red tape'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бюрократическая волокита, замедляющая процессы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волокита",
                                "target_lang_sense_clarification": "бюрократическая задержка"
                            },
                            {
                                "target_lang_word": "бюрократия",
                                "target_lang_sense_clarification": "система управления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce2b9038-2ea4-37c8-b6c6-816b528b40f1",
                    "sense_definition": "The series of prices at which a financial instrument trades, historically printed on a paper strip.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "finance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Traders watched the <w>tape</w> closely as the market crashed.",
                            "target_lang_translations": {
                                "ru": "Трейдеры внимательно следили за <w>лентой</w> котировок, когда рынок рухнул."
                            }
                        },
                        {
                            "text": "Don't fight the <w>tape</w> if you want to make a profit in this market.",
                            "target_lang_translations": {
                                "ru": "Не идите против <w>рынка</w>, если хотите получить прибыль."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ticker tape"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "refers to ticker tape"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Лента котировок, показывающая цены на бирже."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лента",
                                "target_lang_sense_clarification": "биржевая лента (тикер)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77d4ba9e-314c-3c8a-8341-6590aab0b7aa",
                    "sense_definition": "An unthinking, patterned mental response or behavior triggered by a particular stimulus.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He just plays the same old <w>tapes</w> whenever we argue about money.",
                            "target_lang_translations": {
                                "ru": "Он просто заводит ту же старую <w>пластинку</w>, когда мы спорим о деньгах."
                            }
                        },
                        {
                            "text": "She needs to erase those negative mental <w>tapes</w> to move forward.",
                            "target_lang_translations": {
                                "ru": "Ей нужно стереть эти негативные ментальные <w>программы</w>, чтобы двигаться дальше."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Шаблонное поведение или навязчивые мысли (метафора)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пластинка",
                                "target_lang_sense_clarification": "в идиоме 'сменить пластинку'"
                            },
                            {
                                "target_lang_word": "программа",
                                "target_lang_sense_clarification": "психологическая установка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fec47f5-d574-383c-8e6b-d5eae0db6dc7",
                    "sense_definition": "The wrapping on the blade of a hockey stick to improve puck control.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sports_equipment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His pass landed right on the <w>tape</w> of his teammate's stick.",
                            "target_lang_translations": {
                                "ru": "Его пас пришелся прямо в <w>обмотку</w> клюшки товарища по команде."
                            }
                        },
                        {
                            "text": "He needs to replace the worn <w>tape</w> on his blade before the game.",
                            "target_lang_translations": {
                                "ru": "Ему нужно заменить изношенную <w>обмотку</w> на крюке перед игрой."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Специальная лента для обмотки клюшки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обмотка",
                                "target_lang_sense_clarification": "материал на клюшке"
                            },
                            {
                                "target_lang_word": "лента",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "904649c2-bffa-3f72-92ef-484944c63f7f",
                    "sense_definition": "To bind, fasten, or attach something using adhesive tape.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "action_bind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>tape</w> the box securely before mailing it.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, надежно <w>заклей</w> коробку перед отправкой."
                            }

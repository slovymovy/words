            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1ac7cada-4b26-36dd-8439-f9d05b577818",
                    "sense_definition": "Устанавливать время, место или срок для встречи или события.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "set_fix",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы обычно <назначаем> встречи в уютном кафе в центре города.",
                            "target_lang_translations": {
                                "en": "We usually <schedule> meetings in a cozy cafe in the city center."
                            }
                        },
                        {
                            "text": "Я не люблю <назначать> важные дела на понедельник.",
                            "target_lang_translations": {
                                "en": "I don't like to <schedule> important tasks for Monday."
                            }
                        }
                    ],
                    "synonyms": [
                        "устанавливать",
                        "намечать",
                        "определять"
                    ],
                    "antonyms": [
                        "отменять"
                    ],
                    "common_phrases": [
                        "назначать встречу",
                        "назначать свидание",
                        "назначать срок",
                        "назначать дату"
                    ],
                    "target_lang_definitions": {
                        "en": "To establish a time, place, or deadline for a meeting or event."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "set",
                                "target_lang_sense_clarification": "a date, time, or place"
                            },
                            {
                                "target_lang_word": "schedule",
                                "target_lang_sense_clarification": "an event or meeting"
                            },
                            {
                                "target_lang_word": "fix",
                                "target_lang_sense_clarification": "a specific time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5bfc99e9-63d6-31d2-b92e-c807bb27ef3d",
                    "sense_definition": "Определять кого-либо на должность или поручать выполнение обязанностей.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "appoint",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Совет директоров <назначает> нового управляющего компанией.",
                            "target_lang_translations": {
                                "en": "The board of directors is <appointing> a new company manager."
                            }
                        },
                        {
                            "text": "Его планируют <назначать> ответственным за этот сложный проект.",
                            "target_lang_translations": {
                                "en": "They plan to <appoint> him as the person in charge of this complex project."
                            }
                        }
                    ],
                    "synonyms": [
                        "определять",
                        "ставить",
                        "утверждать"
                    ],
                    "antonyms": [
                        "увольнять",
                        "снимать"
                    ],
                    "common_phrases": [
                        "назначать на должность",
                        "назначать директором",
                        "назначать ответственного"
                    ],
                    "target_lang_definitions": {
                        "en": "To assign someone to a position or entrust them with duties."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "appoint",
                                "target_lang_sense_clarification": "to a position"
                            },
                            {
                                "target_lang_word": "name",
                                "target_lang_sense_clarification": "to a role"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5143dde-98a7-3cd0-a89b-12f3cb778ad3",
                    "sense_definition": "Выписывать лекарство или рекомендовать определенное лечение (о враче).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "prescribe",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач внимательно осматривает пациента, прежде чем <назначать> лекарство.",
                            "target_lang_translations": {
                                "en": "The doctor examines the patient carefully before <prescribing> medication."
                            }
                        },
                        {
                            "text": "При легкой простуде не стоит сразу <назначать> антибиотики.",
                            "target_lang_translations": {
                                "en": "For a mild cold, one shouldn't <prescribe> antibiotics immediately."
                            }
                        }
                    ],
                    "synonyms": [
                        "прописывать",
                        "выписывать"
                    ],
                    "antonyms": [
                        "отменять"
                    ],
                    "common_phrases": [
                        "назначать лечение",
                        "назначать лекарство",
                        "назначать таблетки",
                        "назначать процедуру"
                    ],
                    "target_lang_definitions": {
                        "en": "To write a prescription for medicine or recommend a specific treatment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "prescribe",
                                "target_lang_sense_clarification": "medicine or treatment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5abfb557-7ace-31df-83c0-3d9d8f0f565c",
                    "sense_definition": "Официально устанавливать денежную выплату (пенсию, пособие) или меру наказания.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "assign_grant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Комиссия каждый год <назначает> стипендию лучшим студентам университета.",
                            "target_lang_translations": {
                                "en": "The committee <awards> a scholarship to the best university students every year."
                            }
                        },
                        {
                            "text": "Судья имеет право <назначать> штраф за нарушение правил дорожного движения.",
                            "target_lang_translations": {
                                "en": "The judge has the right to <impose> a fine for traffic violations."
                            }
                        }
                    ],
                    "synonyms": [
                        "устанавливать",
                        "присуждать"
                    ],
                    "common_phrases": [
                        "назначать пенсию",
                        "назначать штраф",
                        "назначать пособие",
                        "назначать наказание"
                    ],
                    "target_lang_definitions": {
                        "en": "To officially establish a monetary payment or a penalty."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "assign",
                                "target_lang_sense_clarification": "a pension or allowance"
                            },
                            {
                                "target_lang_word": "impose",
                                "target_lang_sense_clarification": "a fine or punishment"
                            },
                            {
                                "target_lang_word": "award",
                                "target_lang_sense_clarification": "a scholarship or grant"
                            }
                        ]
                    }
                }
            ]
        }
    ]

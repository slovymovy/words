        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "780d7c38-a17d-340f-97b6-4a94ec1b6159",
                    "sense_definition": "Of, from, or pertaining to Egypt, the Egyptian people or the Egyptian language.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The museum has a large collection of ancient <w>Egyptian</w> art.",
                            "target_lang_translations": {
                                "ru": "В музее собрана большая коллекция древнего <w>египетского</w> искусства."
                            }
                        },
                        {
                            "text": "We bought some high-quality sheets made of pure <w>Egyptian</w> cotton.",
                            "target_lang_translations": {
                                "ru": "Мы купили качественное постельное белье из чистого <w>египетского</w> хлопка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Egyptian cotton",
                        "Egyptian pyramids",
                        "ancient Egyptian"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к Египту, египетскому народу или египетскому языку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "египетский",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "afa26eee-b7b4-3d5a-844e-b7290485e182",
                    "sense_definition": "A person from Egypt or of Egyptian descent.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "An <w>Egyptian</w> won the international squash tournament last year.",
                            "target_lang_translations": {
                                "ru": "В прошлом году международный турнир по сквошу выиграл <w>египтянин</w>."
                            }
                        },
                        {
                            "text": "She married an <w>Egyptian</w> and moved to Cairo to live with his family.",
                            "target_lang_translations": {
                                "ru": "Она вышла замуж за <w>египтянина</w> и переехала в Каир жить с его семьей."
                            }
                        }
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "bb4e2dc2-7186-36c4-9f41-65686e8c944f",
                    "sense_definition": "Uszkodzony fizycznie, pęknięty lub rozdzielony na części pod wpływem siły.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary, <w>złamany</w> kij leżał na środku leśnej ścieżki.",
                            "target_lang_translations": {
                                "ru": "Эта старая <w>сломанная</w> палка лежала посреди лесной тропинки."
                            }
                        },
                        {
                            "text": "Mój brat wrócił ze szkoły ze <w>złamanym</w> ołówkiem w piórniku.",
                            "target_lang_translations": {
                                "ru": "Мой брат вернулся из школы со <w>сломанным</w> карандашом в пенале."
                            }
                        }
                    ],
                    "synonyms": [
                        "połamany",
                        "przełamany"
                    ],
                    "antonyms": [
                        "cały",
                        "nienaruszony"
                    ],
                    "common_phrases": [
                        "złamana ręka",
                        "złamany nos",
                        "złamany ołówek",
                        "złamany obcas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физически поврежденный, треснувший или разделенный на части под воздействием силы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сломанный",
                                "target_lang_sense_clarification": "поврежденный физически"
                            },
                            {
                                "target_lang_word": "переломленный",
                                "target_lang_sense_clarification": "разделенный надвое (о палке, кости)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4889f523-546c-30d9-a2df-b9131dfd86ba",
                    "sense_definition": "Odczuwający silny smutek, przygnębienie lub brak nadziei z powodu trudnych przeżyć.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po stracie pracy siedział w domu całkowicie <w>złamany</w> i smutny.",
                            "target_lang_translations": {
                                "ru": "Потеряв работу, он сидел дома совершенно <w>сломленный</w> и грустный."
                            }
                        },
                        {
                            "text": "Mężczyzna opowiadał o swojej tragedii cichym, <w>złamanym</w> głosem.",
                            "target_lang_translations": {
                                "ru": "Мужчина рассказывал о своей трагедии тихим, <w>надломленным</w> голосом."
                            }
                        }
                    ],
                    "synonyms": [
                        "przybity",
                        "przygnębiony",
                        "załamany",
                        "zgnębiony",
                        "podłamany",
                        "rozbity",
                        "smutny"
                    ],
                    "antonyms": [
                        "radosny",
                        "szczęśliwy"
                    ],
                    "common_phrases": [
                        "złamane serce",
                        "złamany głos",
                        "złamany życiem",
                        "złamany duch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувствующий сильную грусть, подавленность или безнадежность из-за тяжелых переживаний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сломленный",
                                "target_lang_sense_clarification": "о душевном состоянии"
                            },
                            {
                                "target_lang_word": "разбитый",
                                "target_lang_sense_clarification": "о сердце или общем состоянии"
                            },
                            {
                                "target_lang_word": "надломленный",
                                "target_lang_sense_clarification": "о голосе или человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b58f5288-8e59-3b8a-9202-03d518172a49",
                    "sense_definition": "Taki, którego opór, wola walki lub zasady zostały pokonane siłą lub presją.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Więzień, choć początkowo twardy, został w końcu <w>złamany</w> przez długie przesłuchania.",
                            "target_lang_translations": {
                                "ru": "Заключенный, хоть поначалу и стойкий, был в конце концов <w>сломлен</w> долгими допросами."
                            }
                        },
                        {
                            "text": "Jego <w>złamany</w> opór pozwolił przeciwnikom na przejęcie kontroli nad miastem.",
                            "target_lang_translations": {
                                "ru": "Его <w>сломленное</w> сопротивление позволило противникам захватить контроль над городом."
                            }
                        }
                    ],
                    "synonyms": [
                        "przełamany"
                    ],
                    "antonyms": [
                        "nieugięty"
                    ],
                    "common_phrases": [
                        "złamany opór",
                        "złamany charakter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, чье сопротивление, воля к борьбе или принципы были подавлены силой или давлением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сломленный",
                                "target_lang_sense_clarification": "о воле или сопротивлении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8957f28e-d3d3-340f-a9de-162a3141eb31",
                    "sense_definition": "O szyfrze lub kodzie: taki, którego treść została odczytana przez osoby niepowołane.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dzięki pracy wybitnych matematyków skomplikowany szyfr wroga został <w>złamany</w>.",
                            "target_lang_translations": {
                                "ru": "Благодаря работе выдающихся математиков сложный шифр врага был <w>взломан</w>."
                            }
                        },
                        {
                            "text": "To jest <w>złamany</w> kod, więc nie możemy go już używać do bezpiecznej komunikacji.",
                            "target_lang_translations": {
                                "ru": "Это <w>взломанный</w> код, поэтому мы больше не можем использовать его для безопасной связи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "złamany szyfr",
                        "złamany kod"
                    ],
                    "target_lang_definitions": {
                        "ru": "О шифре или коде: такой, содержание которого было прочитано посторонними лицами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взломанный",
                                "target_lang_sense_clarification": "о коде или системе"
                            },
                            {
                                "target_lang_word": "разгаданный",
                                "target_lang_sense_clarification": "о шифре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7e58ff3-cda3-3844-8c8e-66e1266fdc3d",
                    "sense_definition": "O kolorze: nieczysty, zawierający domieszkę innej barwy, zwykle szarości lub barwy dopełniającej.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ściany w salonie pomalowaliśmy na elegancki odcień <w>złamanej</w> bieli.",
                            "target_lang_translations": {
                                "ru": "Стены в гостиной мы покрасили в элегантный оттенок <w>приглушённого</w> белого."
                            }
                        },
                        {
                            "text": "Artysta użył <w>złamanej</w> zieleni, aby oddać ponury nastrój krajobrazu.",
                            "target_lang_translations": {
                                "ru": "Художник использовал <w>приглушённый</w> зеленый, чтобы передать мрачное настроение пейзажа."
                            }
                        }
                    ],
                    "antonyms": [
                        "czysty"
                    ],
                    "common_phrases": [
                        "złamana biel",
                        "złamany kolor"
                    ],
                    "target_lang_definitions": {
                        "ru": "О цвете: нечистый, содержащий примесь другого цвета, обычно серого или дополнительного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приглушённый",
                                "target_lang_sense_clarification": "неяркий, с примесью"
                            },
                            {
                                "target_lang_word": "смешанный",
                                "target_lang_sense_clarification": "сложный цвет"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ed171a7d-e552-358a-9d8c-b5b71dd46b6d",
                    "sense_definition": "Imiesłów przymiotnikowy bierny od czasownika <w>złamać</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przepis ruchu drogowego został <w>złamany</w> przez kierowcę ciężarówki.",
                            "target_lang_translations": {
                                "ru": "Правило дорожного движения было <w>нарушено</w> водителем грузовика."
                            }
                        },
                        {
                            "text": "Klucz został <w>złamany</w> w zamku, więc musieliśmy wezwać ślusarza.",
                            "target_lang_translations": {
                                "ru": "Ключ был <w>сломан</w> в замке, поэтому нам пришлось вызвать слесаря."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zostać złamanym"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Страдательное причастие от глагола złamać (сломать/нарушить)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нарушенный",
                                "target_lang_sense_clarification": "о законе или правиле"
                            },
                            {
                                "target_lang_word": "сломанный",
                                "target_lang_sense_clarification": "о предмете"
                            }
                        ]
                    }
                }
            ]
        }
    ]

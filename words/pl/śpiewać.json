            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5ab6c30f-e2fb-3d0c-9164-ead1b69fba2f",
                    "sense_definition": "Wykonywać melodię głosem, wydawać z siebie melodyjne dźwięki, często łącząc je ze słowami.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "music_performance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja młodsza siostra uwielbia <w>śpiewać</w> wesołe piosenki każdego dnia.",
                            "target_lang_translations": {
                                "ru": "Моя младшая сестра обожает <w>петь</w> весёлые песни каждый день."
                            }
                        },
                        {
                            "text": "Wszyscy goście zaczęli głośno <w>śpiewać</w> sto lat dla solenizanta.",
                            "target_lang_translations": {
                                "ru": "Все гости начали громко <w>петь</w> «Сто лет» имениннику."
                            }
                        }
                    ],
                    "synonyms": [
                        "wokalizować"
                    ],
                    "common_phrases": [
                        "śpiewać piosenkę",
                        "śpiewać w chórze",
                        "ładnie śpiewać",
                        "śpiewać pod prysznicem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исполнять мелодию голосом, издавать мелодичные звуки, часто в сочетании со словами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "петь",
                                "target_lang_sense_clarification": "исполнять вокально"
                            },
                            {
                                "target_lang_word": "напевать",
                                "target_lang_sense_clarification": "тихо петь мелодию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "622eb006-aa3a-32ad-a177-9cd13cd02acd",
                    "sense_definition": "Zdradzać tajemnice lub informować o wspólnikach, zazwyczaj podczas przesłuchania przez policję.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "betrayal_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Aresztowany złodziej zaczął <w>śpiewać</w> i podał nazwiska wszystkich wspólników.",
                            "target_lang_translations": {
                                "ru": "Арестованный вор начал <w>колоться</w> и назвал имена всех сообщников."
                            }
                        },
                        {
                            "text": "Gangster bał się, że jego kolega zacznie <w>śpiewać</w> podczas długiego przesłuchania.",
                            "target_lang_translations": {
                                "ru": "Гангстер боялся, что его коллега начнет <w>сдавать</w> во время долгого допроса."
                            }
                        }
                    ],
                    "synonyms": [
                        "sypać"
                    ],
                    "common_phrases": [
                        "zacząć śpiewać",
                        "śpiewać na przesłuchaniu"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "używane w kontekście kryminalnym lub policyjnym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выдавать тайны или сообщать о сообщниках, обычно во время полицейского допроса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стучать",
                                "target_lang_sense_clarification": "доносить (жарг.)"
                            },
                            {
                                "target_lang_word": "сдавать",
                                "target_lang_sense_clarification": "выдавать кого-либо"
                            },
                            {
                                "target_lang_word": "колоться",
                                "target_lang_sense_clarification": "признаваться (жарг.)"
                            },
                            {
                                "target_lang_word": "петь",
                                "target_lang_sense_clarification": "давать показания (жарг.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3a14da57-e3db-3791-8f94-215fc773f469",
                    "sense_definition": "Małe ogniwo elektryczne służące do zasilania przenośnych urządzeń, takich jak piloty, zegarki czy latarki.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "energy_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę kupić nowe <w>baterie</w> do pilota od telewizora.",
                            "target_lang_translations": {
                                "ru": "Мне нужно купить новые <w>батарейки</w> для пульта от телевизора."
                            }
                        },
                        {
                            "text": "Mój telefon się wyłączył, ponieważ padła w nim <w>bateria</w>.",
                            "target_lang_translations": {
                                "ru": "Мой телефон выключился, так как в нем села <w>батарея</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bateria alkaliczna",
                        "bateria słoneczna",
                        "wymiana baterii",
                        "ładować baterie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Маленький источник электричества для питания переносных устройств, таких как пульты, часы или фонарики."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "батарейка",
                                "target_lang_sense_clarification": "маленький элемент питания"
                            },
                            {
                                "target_lang_word": "батарея",
                                "target_lang_sense_clarification": "источник питания"
                            },
                            {
                                "target_lang_word": "аккумулятор",
                                "target_lang_sense_clarification": "перезаряжаемый элемент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5fb6241-863d-3eca-b62c-50b76b47454a",
                    "sense_definition": "Urządzenie hydrauliczne z zaworami, służące do regulowania przepływu wody w zlewie, umywalce lub wannie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "plumbing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hydraulik naprawił cieknącą <w>baterię</w> w naszej łazience.",
                            "target_lang_translations": {
                                "ru": "Сантехник починил протекающий <w>смеситель</w> в нашей ванной."
                            }
                        },
                        {
                            "text": "Do nowej kuchni wybraliśmy nowoczesną, chromowaną <w>baterię</w>.",
                            "target_lang_translations": {
                                "ru": "Для новой кухни мы выбрали современный хромированный <w>смеситель</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kran"
                    ],
                    "common_phrases": [
                        "bateria umywalkowa",
                        "bateria kuchenna",
                        "bateria prysznicowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сантехническое устройство для смешивания горячей и холодной воды и регулирования её потока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смеситель",
                                "target_lang_sense_clarification": "сантехнический прибор"
                            },
                            {
                                "target_lang_word": "кран",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3afa740a-1304-3ee3-a7b2-6ace723e4ab6",
                    "sense_definition": "Pododdział artylerii wojskowej, składający się z kilku dział lub wyrzutni rakietowych wraz z obsługą.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wroga <w>bateria</w> dział została zniszczona przez lotnictwo.",
                            "target_lang_translations": {
                                "ru": "Вражеская <w>батарея</w> орудий была уничтожена авиацией."
                            }
                        },
                        {
                            "text": "Dowódca rozkazał, aby <w>bateria</w> zajęła pozycje na wzgórzu.",
                            "target_lang_translations": {
                                "ru": "Командир приказал, чтобы <w>батарея</w> заняла позиции на холме."

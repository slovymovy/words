    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d3fa4da7-97d5-3eb2-a35b-4fd022d2a65b",
                    "sense_definition": "Czynność zestawiania ze sobą osób, rzeczy lub zjawisk w celu oceny ich podobieństw i różnic.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "comparison_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W <w>porównaniu</w> z zeszłym rokiem ceny znacznie wzrosły.",
                            "target_lang_translations": {
                                "ru": "По <w>сравнению</w> с прошлым годом цены значительно выросли."
                            }
                        },
                        {
                            "text": "To <w>porównanie</w> dwóch modeli samochodów pomogło mi w wyborze.",
                            "target_lang_translations": {
                                "ru": "Это <w>сравнение</w> двух моделей автомобилей помогло мне в выборе."
                            }
                        },
                        {
                            "text": "Jego osiągnięcia są bez <w>porównania</w> lepsze od moich.",
                            "target_lang_translations": {
                                "ru": "Его достижения без <w>сравнения</w> лучше моих."
                            }
                        }
                    ],
                    "synonyms": [
                        "równość",
                        "równonoc"
                    ],
                    "common_phrases": [
                        "w porównaniu z",
                        "w porównaniu do",
                        "bez porównania",
                        "punkt odniesienia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по сопоставлению людей, предметов или явлений для оценки их сходств и различий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сравнение",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "сопоставление",
                                "target_lang_sense_clarification": "процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b3a3e7c-4487-3fba-b8ef-2d5357b1789b",
                    "sense_definition": "Figura retoryczna, w której cechy jednego przedmiotu lub zjawiska są zestawione z cechami innego, zazwyczaj za pomocą spójników takich jak „jak”, „niby”.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "literary_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Poeta zastosował w wierszu bardzo malownicze <w>porównanie</w>.",
                            "target_lang_translations": {
                                "ru": "Поэт использовал в стихотворении очень живописное <w>сравнение</w>."
                            }
                        },
                        {
                            "text": "To rozbudowane <w>porównanie</w> homeryckie zajmuje kilka wersów utworu.",
                            "target_lang_translations": {
                                "ru": "Это развернутое гомеровское <w>сравнение</w> занимает несколько строк произведения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "porównanie homeryckie",
                        "użyć porównania"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literacki"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Риторическая фигура, в которой свойства одного предмета или явления сопоставляются со свойствами другого, обычно с помощью союзов «как», «словно»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сравнение",
                                "target_lang_sense_clarification": "литературный приём"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50b56eb2-79fc-340f-967c-a2d5d72d371f",
                    "sense_definition": "Jeden z dwóch momentów w roku, kiedy dzień i noc mają taką samą długość.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "archaic_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dawniej świętowano wiosenne <w>porównanie</w> dnia z nocą.",
                            "target_lang_translations": {
                                "ru": "Раньше праздновали весеннее <w>равноденствие</w>."
                            }
                        },
                        {
                            "text": "Stare kroniki wspominają o jesiennym <w>porównaniu</w>.",
                            "target_lang_translations": {
                                "ru": "Старые хроники упоминают об осеннем <w>равноденствии</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "równonoc"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "przestarzałe"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Один из двух моментов в году, когда день и ночь имеют одинаковую продолжительность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "равноденствие",
                                "target_lang_sense_clarification": "астрономия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e10faa64-68dd-307a-96d2-1ca80c851b7c",
                    "sense_definition": "Stan bycia równym pod jakimś względem.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "archaic_meaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Walczyli o pełne <w>porównanie</w> praw wszystkich obywateli.",
                            "target_lang_translations": {
                                "ru": "Они боролись за полное <w>уравнивание</w> прав всех граждан."
                            }
                        },
                        {
                            "text": "W tym kontekście słowo to oznaczało <w>porównanie</w> stanów społecznych.",
                            "target_lang_translations": {
                                "ru": "В этом контексте слово означало <w>уравнивание</w> социальных сословий."
                            }
                        }
                    ],
                    "synonyms": [
                        "równość"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "przestarzałe"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние равенства в каком-либо отношении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уравнивание",
                                "target_lang_sense_clarification": "процесс"
                            },
                            {
                                "target_lang_word": "равенство",

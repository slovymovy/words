                },
                {
                    "sense_id": "5be5e1d9-4292-37cd-9b56-656afdebb4ab",
                    "sense_definition": "Rood in het gezicht, veroorzaakt door sterke emoties zoals schaamte of woede.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "facial_color",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze werd <w>vuurrood</w> toen ze haar fout ontdekte.",
                            "target_lang_translations": {
                                "en": "She turned <w>bright red</w> when she discovered her mistake.",
                                "ru": "Она стала <w>пунцовой</w>, когда обнаружила свою ошибку."
                            }
                        },
                        {
                            "text": "Van pure woede liep zijn gezicht <w>vuurrood</w> aan.",
                            "target_lang_translations": {
                                "en": "His face turned <w>scarlet</w> with pure anger.",
                                "ru": "От ярости его лицо стало <w>багровым</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "vuurrood worden",
                        "vuurrood aanlopen"
                    ],
                    "target_lang_definitions": {
                        "en": "Red in the face, caused by strong emotions such as shame or anger.",
                        "ru": "Красный цвет лица, вызванный сильными эмоциями, такими как стыд или гнев."
                    },

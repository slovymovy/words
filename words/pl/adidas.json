                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сленговое название синдрома приобретенного иммунодефицита (СПИД)."
                    },
                    "translations": {
                        "ru": []

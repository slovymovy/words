                {
                    "sense_id": "87894796-e0d4-3df6-84ed-324ed82aaffb",
                    "sense_definition": "Zich in een positie bevinden waarbij men aan de bovenkant bevestigd is en de grond niet raakt",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "state_of_suspension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jas <w>hangt</w> aan de kapstok in de gang.",
                            "target_lang_translations": {

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "578df519-8a90-38b6-87df-b626f68d5a27",
                    "sense_definition": "Grupa urządzeń, produktów lub technologii stworzonych na tym samym etapie rozwoju technicznego.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na rynku pojawiła się nowa <w>generacja</w> smartfonów z lepszymi aparatami.",
                            "target_lang_translations": {
                                "ru": "На рынке появилось новое <w>поколение</w> смартфонов с лучшими камерами."
                            }
                        },
                        {
                            "text": "Konsole obecnej <w>generacji</w> oferują niezwykle realistyczną grafikę.",
                            "target_lang_translations": {
                                "ru": "Консоли текущего <w>поколения</w> предлагают невероятно реалистичную графику."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nowa generacja",
                        "kolejna generacja",
                        "sieć piątej generacji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа устройств или технологий, созданных на одном этапе технического развития."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поколение",
                                "target_lang_sense_clarification": "этап развития техники"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c7c1c3a-2895-3555-9c15-a69c2e107814",
                    "sense_definition": "Ogół ludzi żyjących w tym samym czasie i będących w zbliżonym wieku, często dzielących podobne doświadczenia kulturowe.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta <w>generacja</w> dorastała w świecie pełnym cyfrowych technologii.",
                            "target_lang_translations": {
                                "ru": "Это <w>поколение</w> росло в мире, полном цифровых технологий."
                            }
                        },
                        {
                            "text": "Różnice poglądów często prowadzą do konfliktów między starszą a młodszą <w>generacją</w>.",
                            "target_lang_translations": {
                                "ru": "Различия во взглядах часто приводят к конфликтам между старшим и младшим <w>поколением</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "pokolenie"
                    ],
                    "common_phrases": [
                        "młoda generacja",
                        "starsza generacja",
                        "stracona generacja"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность людей, живущих в одно время и имеющих близкий возраст."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поколение",
                                "target_lang_sense_clarification": "люди одного возраста"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abe7ed20-bea7-3f14-8c3e-a958c85f610e",
                    "sense_definition": "Proces wytwarzania czegoś, w szczególności energii elektrycznej lub sygnałów.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lokalna <w>generacja</w> energii pozwala uniezależnić się od dużych dostawców.",
                            "target_lang_translations": {
                                "ru": "Локальная <w>выработка</w> энергии позволяет не зависеть от крупных поставщиков."
                            }
                        },
                        {
                            "text": "System odpowiada za automatyczną <w>generację</w> raportów finansowych.",
                            "target_lang_translations": {
                                "ru": "Система отвечает за автоматическую <w>генерацию</w> финансовых отчетов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "generacja energii",
                        "generacja mocy",
                        "generacja sygnału"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс производства чего-либо, в частности электроэнергии или сигналов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "генерация",
                                "target_lang_sense_clarification": "создание, формирование"
                            },
                            {
                                "target_lang_word": "выработка",
                                "target_lang_sense_clarification": "производство энергии"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ab427b13-697f-3525-a782-108557f5eed8",
                    "sense_definition": "Установить связь, войти в контакт с кем-либо (по телефону, почте, интернету и т. п.)",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я попытаюсь <w>связаться</w> с вашим менеджером завтра утром.",
                            "target_lang_translations": {
                                "en": "I will try to <w>contact</w> your manager tomorrow morning."
                            }
                        },
                        {
                            "text": "Если у вас возникнут вопросы, вы можете <w>связаться</w> с нами по электронной почте.",
                            "target_lang_translations": {
                                "en": "If you have any questions, you can <w>contact</w> us by email."
                            }
                        }
                    ],
                    "synonyms": [
                        "соединиться",
                        "созвониться",
                        "списаться",
                        "сконтактироваться"
                    ],
                    "common_phrases": [
                        "связаться по телефону",
                        "связаться с администрацией",
                        "связаться по скайпу",
                        "легко связаться"
                    ],
                    "target_lang_definitions": {
                        "en": "To establish a connection or make contact with someone (by phone, mail, internet, etc.)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "contact",
                                "target_lang_sense_clarification": "general term for establishing communication"
                            },
                            {
                                "target_lang_word": "get in touch",
                                "target_lang_sense_clarification": "common phrase for initiating contact"
                            },
                            {
                                "target_lang_word": "reach",
                                "target_lang_sense_clarification": "to succeed in contacting someone"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1c580a64-4641-3b3c-b9e1-adcafca333fe",
                    "sense_definition": "Начать иметь дело с кем-либо или чем-либо (часто с оттенком неодобрения, риска или нежелательности)",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "interaction_conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тебе не стоило <w>связываться</w> с этой подозрительной компанией.",
                            "target_lang_translations": {
                                "en": "You shouldn't have <w>gotten involved</w> with that suspicious crowd."
                            }
                        },
                        {
                            "text": "Лучше не <w>связываться</w> с ним, когда он в плохом настроении.",
                            "target_lang_translations": {
                                "en": "It's better not to <w>mess</w> with him when he's in a bad mood."
                            }
                        }
                    ],
                    "synonyms": [
                        "спутаться"
                    ],
                    "antonyms": [
                        "избегать"
                    ],
                    "common_phrases": [
                        "не стоит связываться",
                        "зря связался",
                        "связаться с дураком",
                        "связаться на свою голову"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To start dealing with someone or something, often implying risk, trouble, or disapproval"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mess with",
                                "target_lang_sense_clarification": "colloquial, implies provoking or dealing with something dangerous"
                            },
                            {
                                "target_lang_word": "get involved",
                                "target_lang_sense_clarification": "to become connected with a situation or person, often negatively"
                            },
                            {
                                "target_lang_word": "get mixed up",
                                "target_lang_sense_clarification": "to become entangled in a bad situation"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cba6394b-85bc-3a75-8c8c-f2fd99bf572a",
                    "sense_definition": "Оказаться внутренне соединённым, логически ассоциированным или соотнесённым с чем-либо",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "association",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В моей памяти этот старый дом навсегда <w>связался</w> с детством.",
                            "target_lang_translations": {
                                "en": "In my memory, this old house has forever <w>become associated</w> with childhood."
                            }
                        },
                        {
                            "text": "Эти разрозненные факты наконец <w>связались</w> в единую картину преступления.",
                            "target_lang_translations": {
                                "en": "These scattered facts finally <w>came together</w> into a single picture of the crime."
                            }
                        }
                    ],
                    "synonyms": [
                        "ассоциироваться",
                        "соотнестись"
                    ],
                    "common_phrases": [
                        "тесно связаться",
                        "неразрывно связаться"
                    ],
                    "target_lang_definitions": {
                        "en": "To become internally connected, logically associated, or correlated with something"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "become associated",
                                "target_lang_sense_clarification": "to form a mental connection"
                            },
                            {
                                "target_lang_word": "be linked",
                                "target_lang_sense_clarification": "to have a logical connection"
                            },
                            {
                                "target_lang_word": "come together",
                                "target_lang_sense_clarification": "for facts or ideas connecting to form a whole"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7e72d56-a1f7-3723-8fbe-81ff7fe73d4b",
                    "sense_definition": "Страдательный залог к глаголу <w>связать</w> (быть скреплённым, соединённым)",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Концы верёвки должны крепко <w>связаться</w> узлом, чтобы груз не упал.",
                            "target_lang_translations": {
                                "en": "The ends of the rope must <w>be tied</w> tightly in a knot so the load doesn't fall."
                            }
                        },
                        {
                            "text": "Нити узора могут <w>связаться</w> неправильно, если не следовать схеме.",
                            "target_lang_translations": {
                                "en": "The threads of the pattern might <w>be knit</w> incorrectly if you don't follow the diagram."
                            }
                        }
                    ],
                    "synonyms": [
                        "соединиться",
                        "скрепиться"
                    ],
                    "antonyms": [
                        "развязаться"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "passive voice"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Passive form of 'to tie' or 'to knit'; to be fastened or joined together"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be tied",
                                "target_lang_sense_clarification": "passive state of being fastened with a knot"
                            },
                            {
                                "target_lang_word": "be knit",
                                "target_lang_sense_clarification": "passive state of being formed by knitting"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                },
                {
                    "sense_id": "05de34dd-9327-34a0-ba9a-ceb7ba1ce9fa",
                    "sense_definition": "Een blijvende invloed of een kenmerkend karakter dat iemand of iets ergens aan geeft.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphorical_influence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De nieuwe directeur heeft duidelijk zijn <w>stempel</w> op het beleid gedrukt.",
                            "target_lang_translations": {
                                "en": "The new director has clearly put his <w>stamp</w> on the policy.",
                                "ru": "Новый директор явно наложил свой <w>отпечаток</w> на политику."
                            }
                        },
                        {
                            "text": "De architectuur uit die tijd draagt een zwaar religieus <w>stempel</w>.",
                            "target_lang_translations": {
                                "en": "The architecture from that time bears a heavy religious <w>stamp</w>.",
                                "ru": "Архитектура того времени несет на себе сильный религиозный <w>отпечаток</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "invloed",
                        "merk",
                        "karakter"
                    ],
                    "common_phrases": [
                        "zijn stempel drukken op",
                        "een zwaar stempel drukken"
                    ],
                    "target_lang_definitions": {
                        "en": "A lasting influence or characteristic character that someone or something gives to something.",
                        "ru": "Устойчивое влияние или характерная черта, которую кто-то или что-то придает чему-либо."
                    },
                    "translations": {

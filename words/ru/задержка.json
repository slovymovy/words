    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cebd7685-77a4-366c-836b-9ff1f38e2943",
                    "sense_definition": "Промедление, временная приостановка какого-либо действия или процесса.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "delay_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за сильного снегопада произошла <w>задержка</w> всех рейсов.",
                            "target_lang_translations": {
                                "en": "Due to heavy snowfall, there was a <w>delay</w> of all flights."
                            }
                        },
                        {
                            "text": "Мы приносим извинения за небольшую <w>задержку</w> с доставкой вашего заказа.",
                            "target_lang_translations": {
                                "en": "We apologize for the slight <w>delay</w> in the delivery of your order."
                            }
                        }
                    ],
                    "synonyms": [
                        "промедление",
                        "приостановка",
                        "пауза"
                    ],
                    "antonyms": [
                        "ускорение"
                    ],
                    "common_phrases": [
                        "задержка рейса",
                        "без задержки",
                        "задержка зарплаты",
                        "временная задержка"
                    ],
                    "target_lang_definitions": {
                        "en": "A period of time by which something is late or postponed; a temporary suspension of an action or process."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "delay",
                                "target_lang_sense_clarification": "time lag or postponement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e1070f4-2bca-3ef0-948d-b4ab5250c1ba",
                    "sense_definition": "Действие по глаголу «задерживать»; остановка, удержание или препятствование движению чего-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_stoppage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач попросил сделать глубокий вдох и <w>задержку</w> дыхания на несколько секунд.",
                            "target_lang_translations": {
                                "en": "The doctor asked to take a deep breath and <w>hold</w> it for a few seconds."
                            }
                        },
                        {
                            "text": "Механическая <w>задержка</w> в устройстве предотвращает случайное срабатывание.",
                            "target_lang_translations": {
                                "en": "A mechanical <w>delay</w> in the device prevents accidental triggering."
                            }
                        }
                    ],
                    "synonyms": [
                        "приостановка",
                        "задержание"
                    ],
                    "antonyms": [
                        "высвобождение",
                        "освобождение"
                    ],
                    "common_phrases": [
                        "задержка дыхания",
                        "задержка воды",
                        "задержка развития"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of stopping, holding back, or hindering the movement or progress of something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "holding",
                                "target_lang_sense_clarification": "act of keeping something back (e.g., breath)"
                            },
                            {
                                "target_lang_word": "retention",
                                "target_lang_sense_clarification": "holding fluids or substances"
                            },
                            {
                                "target_lang_word": "detention",
                                "target_lang_sense_clarification": "confinement or custody"
                            },
                            {
                                "target_lang_word": "stoppage",
                                "target_lang_sense_clarification": "halt in activity or movement"
                            }
                        ]
                    }
                }
            ]
        }

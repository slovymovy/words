{
    "word_family": [
        "pożreć",
        "pożeracz",
        "pożarcie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ab16d2da-9b9d-3183-b0c8-e396294ee1d6",
                    "sense_definition": "Jeść coś bardzo szybko, łapczywie i dużymi kęsami (o ludziach) lub zjadać ofiarę (o zwierzętach).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "eating_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Głodny wilk błyskawicznie <pożerał> upolowaną sarnę.",
                            "target_lang_translations": {
                                "ru": "Голодный волк молниеносно <пожирал> пойманную косулю."
                            }
                        },
                        {
                            "text": "Mój brat zawsze <pożera> obiad w kilka minut, zamiast jeść spokojnie.",
                            "target_lang_translations": {
                                "ru": "Мой брат всегда <пожирает> обед за несколько минут, вместо того чтобы есть спокойно."
                            }
                        }
                    ],
                    "synonyms": [
                        "żreć",
                        "pałaszować",
                        "wsuwać"
                    ],
                    "antonyms": [
                        "delektować się",
                        "skubać"
                    ],
                    "common_phrases": [
                        "pożerać w całości"
                    ],
                    "target_lang_definitions": {
                        "ru": "Есть что-то очень быстро, жадно и большими кусками (о людях) или съедать жертву (о животных)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пожирать",
                                "target_lang_sense_clarification": "есть с жадностью"
                            },
                            {
                                "target_lang_word": "уплетать",
                                "target_lang_sense_clarification": "есть с аппетитом (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6c78c55-abf0-31c5-8b57-82e9337c6280",
                    "sense_definition": "Szybko zużywać zasoby, energię lub pieniądze; niszczyć coś całkowicie (często o ogniu).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "resource_consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stary silnik w tym samochodzie <pożera> ogromne ilości paliwa.",
                            "target_lang_translations": {
                                "ru": "Старый двигатель в этой машине <пожирает> огромное количество топлива."
                            }
                        },
                        {
                            "text": "Szalejący pożar <pożerał> kolejne hektary lasu.",
                            "target_lang_translations": {
                                "ru": "Бушующий пожар <пожирал> гектар за гектаром леса."
                            }
                        }
                    ],
                    "synonyms": [
                        "pochłaniać",
                        "zużywać",
                        "trawić",
                        "marnować"
                    ],
                    "antonyms": [
                        "oszczędzać",
                        "gromadzić"
                    ],
                    "common_phrases": [
                        "pożerać paliwo",
                        "pożerać pieniądze",
                        "pożerać czas",
                        "pożerać prąd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро расходовать ресурсы, энергию или деньги; полностью уничтожать что-то (часто об огне)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пожирать",
                                "target_lang_sense_clarification": "уничтожать или тратить в большом количестве"
                            },
                            {
                                "target_lang_word": "поглощать",
                                "target_lang_sense_clarification": "впитывать или расходовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa69179a-fdbd-33bc-9fc9-11946653cafc",
                    "sense_definition": "Intensywnie chłonąć coś zmysłami (np. czytać, patrzeć) lub być niszczonym psychicznie przez silne emocje.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopiec <pożerał> kolejne tomy przygód swojego ulubionego bohatera.",
                            "target_lang_translations": {
                                "ru": "Мальчик <глотал> очередные тома приключений своего любимого героя."
                            }
                        },
                        {
                            "text": "Zazdrość powoli go <pożerała> i niszczyła ich relację.",
                            "target_lang_translations": {
                                "ru": "Ревность медленно его <сжирала> и разрушала их отношения."
                            }
                        }
                    ],
                    "synonyms": [
                        "chłonąć",
                        "dręczyć",
                        "nurtować"
                    ],
                    "common_phrases": [
                        "pożerać wzrokiem",
                        "pożerać oczami",
                        "pożerać książki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Интенсивно воспринимать что-то чувствами (например, читать, смотреть) или быть мучимым сильными эмоциями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пожирать",
                                "target_lang_sense_clarification": "жадно смотреть или читать"
                            },
                            {
                                "target_lang_word": "сжирать",
                                "target_lang_sense_clarification": "мучить (о чувствах)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
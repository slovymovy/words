    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "fb969397-d652-3f9b-afda-95855d579233",
                    "sense_definition": "Difficult to understand or deal with because of many parts or details.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The instructions for the new game were too <w>complicated</w> for us.",
                            "target_lang_translations": {
                                "ru": "Инструкции к новой игре были для нас слишком <w>сложными</w>."
                            }
                        },
                        {
                            "text": "She found herself in a <w>complicated</w> situation that was hard to explain.",
                            "target_lang_translations": {
                                "ru": "Она оказалась в <w>запутанной</w> ситуации, которую было трудно объяснить."
                            }
                        }
                    ],
                    "synonyms": [
                        "complex",
                        "intricate",
                        "difficult"
                    ],
                    "antonyms": [
                        "simple"
                    ],
                    "common_phrases": [
                        "complicated situation",
                        "complicated relationship",
                        "complicated process",
                        "complicated system"
                    ],
                    "target_lang_definitions": {
                        "ru": "Трудный для понимания или решения из-за множества частей или деталей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сложный",
                                "target_lang_sense_clarification": "трудный для понимания или выполнения"
                            },
                            {
                                "target_lang_word": "запутанный",
                                "target_lang_sense_clarification": "неясный, имеющий сложную структуру"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef79a6e2-b407-3418-b9e6-c7c91b40b71f",
                    "sense_definition": "Folded lengthwise, specifically referring to the wings of certain insects.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The beetle has <w>complicated</w> wings that fit neatly under its shell.",
                            "target_lang_translations": {
                                "ru": "У жука <w>складчатые</w> крылья, которые аккуратно помещаются под панцирем."
                            }
                        },
                        {
                            "text": "Scientists studied the <w>complicated</w> structure of the insect's folded wings.",
                            "target_lang_translations": {
                                "ru": "Ученые изучали <w>складчатую</w> структуру сложенных крыльев насекомого."
                            }
                        }
                    ],
                    "antonyms": [
                        "simple"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сложенный продольно (обычно о крыльях некоторых насекомых)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "складчатый",
                                "target_lang_sense_clarification": "биологический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "fe2c9541-eafe-3034-874d-fd718d287fdf",
                    "sense_definition": "Simple past and past participle of <w>complicate</w>.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sudden rain <w>complicated</w> our travel plans significantly.",
                            "target_lang_translations": {
                                "ru": "Внезапный дождь значительно <w>усложнил</w> наши планы на путешествие."
                            }

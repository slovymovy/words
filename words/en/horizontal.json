                        {
                            "text": "The movie implied that the characters enjoyed some <w>horizontal</w> exercise.",
                            "target_lang_translations": {
                                "ru": "В фильме намекалось, что персонажи наслаждались <w>горизонтальными</w> упражнениями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "horizontal tango",
                        "horizontal dancing"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "humorous or euphemistic"

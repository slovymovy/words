    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "6a08f126-7793-3d1c-8436-51996acb9d83",
                    "sense_definition": "Więcej niż jeden; spora liczba kogoś lub czegoś.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "more_than_one",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ostatnia z tych podróży trwała ponad rok i dzięki temu miałem <w>niejedną</w> sposobność, by z bliska przyjrzeć się politycznej rzeczywistości.",
                            "target_lang_translations": {
                                "ru": "Последнее из этих путешествий длилось больше года, и благодаря этому у меня была <w>не одна</w> возможность вблизи присмотреться к политической реальности."
                            }
                        },
                        {
                            "text": "Już <w>niejeden</w> raz prosiłem cię o zrobienie zakupów w drodze do domu.",
                            "target_lang_translations": {
                                "ru": "Я уже <w>не один</w> раз просил тебя сделать покупки по дороге домой."
                            }
                        }
                    ],
                    "synonyms": [
                        "wielu",
                        "sporo"
                    ],
                    "common_phrases": [
                        "niejeden raz",
                        "niejedna osoba"
                    ],
                    "target_lang_definitions": {
                        "ru": "Больше чем один; значительное количество кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не один",
                                "target_lang_sense_clarification": "в значении «многие» или «несколько»"
                            },
                            {
                                "target_lang_word": "многие",
                                "target_lang_sense_clarification": "большое количество"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9178ab74-e00e-354f-ba69-f1647078c9a7",
                    "sense_definition": "Więcej niż jedna osoba; wielu ludzi.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "more_than_one",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Niejeden</w> chciałby zwiedzić cały świat i poznać obce kultury.",
                            "target_lang_translations": {
                                "ru": "<w>Многие</w> хотели бы объехать весь мир и познакомиться с чужими культурами."
                            }
                        },
                        {
                            "text": "To trudne zadanie przerosło już <w>niejednego</w>, ale wierzę, że tobie się uda.",
                            "target_lang_translations": {
                                "ru": "Эта трудная задача оказалась не под силу уже <w>не одному</w>, но я верю, что у тебя получится."
                            }
                        }
                    ],
                    "synonyms": [
                        "wielu"
                    ],
                    "common_phrases": [
                        "niejeden z nas"
                    ],
                    "target_lang_definitions": {
                        "ru": "Больше чем один человек; многие люди."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "многие",
                                "target_lang_sense_clarification": "о людях"
                            },
                            {
                                "target_lang_word": "не один",
                                "target_lang_sense_clarification": "о людях, в значении «многие»"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
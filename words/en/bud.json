    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "19c22244-64ed-3c2e-803b-aacd8dfbbc9f",
                    "sense_definition": "A newly sprouted leaf or blossom that has not yet unfolded.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "botany_growth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After a long, cold winter, the trees finally began to produce <w>buds</w>.",
                            "target_lang_translations": {
                                "ru": "После долгой холодной зимы на деревьях наконец начали появляться <w>почки</w>."
                            }
                        },
                        {
                            "text": "The rose <w>bud</w> slowly opened to reveal a beautiful red flower.",
                            "target_lang_translations": {
                                "ru": "<w>Бутон</w> розы медленно раскрылся, обнажив прекрасный красный цветок."
                            }
                        }
                    ],
                    "synonyms": [
                        "sprout",
                        "shoot"
                    ],
                    "common_phrases": [
                        "in bud",
                        "flower bud",
                        "rose bud"
                    ],
                    "target_lang_definitions": {
                        "ru": "Недавно появившийся листок или цветок, который еще не распустился"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почка",
                                "target_lang_sense_clarification": "зачаток побега или листка"
                            },
                            {
                                "target_lang_word": "бутон",
                                "target_lang_sense_clarification": "нераспустившийся цветок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "896ffeee-27f2-33d1-af1f-d2a9519ee7d3",
                    "sense_definition": "A friend; an informal term of address for a man.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "friendship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I like to hang out with my <w>buds</w> on Saturday night.",
                            "target_lang_translations": {
                                "ru": "Мне нравится зависать с <w>приятелями</w> субботними вечерами."
                            }
                        },
                        {
                            "text": "Hey <w>bud</w>, do you know what time the game starts?",
                            "target_lang_translations": {
                                "ru": "Эй, <w>дружище</w>, не знаешь, во сколько начинается игра?"
                            }
                        }
                    ],
                    "synonyms": [
                        "buddy",
                        "pal",
                        "mate",
                        "friend",
                        "dude"
                    ],
                    "common_phrases": [
                        "best bud"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Друг; неформальное обращение к мужчине"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приятель",
                                "target_lang_sense_clarification": "друг, знакомый"
                            },
                            {
                                "target_lang_word": "дружище",
                                "target_lang_sense_clarification": "обращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c96db38b-1e6b-3ba6-8a8f-419d4b4968db",
                    "sense_definition": "Marijuana, specifically the flowering part of the plant.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "cannabis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was caught by the police with a small bag of <w>bud</w>.",
                            "target_lang_translations": {
                                "ru": "Полиция поймала его с маленьким пакетиком <w>шишек</w>."
                            }
                        },
                        {
                            "text": "Hey bro, want to smoke some <w>bud</w>?",
                            "target_lang_translations": {
                                "ru": "Эй, братан, хочешь покурить <w>травки</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "weed",
                        "pot",
                        "marijuana",
                        "grass",
                        "cannabis"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Марихуана, особенно цветущая часть растения"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шишка",
                                "target_lang_sense_clarification": "сленг: соцветие конопли"
                            },
                            {
                                "target_lang_word": "травка",
                                "target_lang_sense_clarification": "сленг: марихуана"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f34fe358-5439-300d-96b0-a39783f6c109",
                    "sense_definition": "A small rounded body on an organism that grows into a new genetically identical organism.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "botany_growth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In this slide, you can see a yeast cell forming <w>buds</w>.",
                            "target_lang_translations": {
                                "ru": "На этом слайде видно, как дрожжевая клетка образует <w>почки</w>."
                            }
                        },
                        {
                            "text": "The hydra reproduces asexually by developing <w>buds</w> on its body.",
                            "target_lang_translations": {
                                "ru": "Гидра размножается бесполым путем, образуя <w>почки</w> на своем теле."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical (biology)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшое образование на теле организма, из которого развивается новый организм"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почка",
                                "target_lang_sense_clarification": "биология"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f6648309-031c-3386-bb33-ec184302fc1e",
                    "sense_definition": "To form or put forth buds.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "botany_growth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The trees are finally starting to <w>bud</w> after the long winter.",
                            "target_lang_translations": {
                                "ru": "После долгой зимы деревья наконец начинают <w>распускаться</w>."
                            }
                        },
                        {
                            "text": "My roses <w>budded</w> early this year because of the warm weather.",
                            "target_lang_translations": {
                                "ru": "Мои розы в этом году рано <w>дали бутоны</w> из-за теплой погоды."
                            }
                        }
                    ],
                    "synonyms": [
                        "sprout"
                    ],
                    "common_phrases": [
                        "start to bud"
                    ],
                    "target_lang_definitions": {
                        "ru": "Образовывать почки или бутоны"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распускаться",
                                "target_lang_sense_clarification": "о листьях или цветах"
                            },
                            {
                                "target_lang_word": "давать почки",
                                "target_lang_sense_clarification": "процесс роста"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "040f682a-6631-3988-9c5b-db437c967f58",
                    "sense_definition": "To graft a plant by inserting a bud from one plant under the bark of another.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "botany_growth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gardeners often <w>bud</w> fruit trees to ensure they produce high-quality fruit.",
                            "target_lang_translations": {
                                "ru": "Садоводы часто <w>прививают</w> фруктовые деревья, чтобы гарантировать высокое качество плодов."
                            }
                        },
                        {
                            "text": "He learned how to <w>bud</w> roses onto wild rootstocks.",
                            "target_lang_translations": {
                                "ru": "Он научился <w>окулировать</w> розы на дикие подвои."
                            }
                        }
                    ],
                    "synonyms": [
                        "graft"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical (horticulture)"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прививать растение, вставляя почку одного растения под кору другого"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окулировать",
                                "target_lang_sense_clarification": "садоводство: прививать глазком"
                            },
                            {
                                "target_lang_word": "прививать",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                }
            ]
        },

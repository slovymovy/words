    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4b4c5717-08dd-3946-975c-bf1dddb2370d",
                    "sense_definition": "A written promise from a company to repair or replace a product if it breaks or has a fault within a specific period of time.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "guarantee_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new washing machine comes with a two-year <w>warranty</w> on parts and labor."
                        },
                        {
                            "text": "If you try to fix the device yourself, you might void the <w>warranty</w>."
                        },
                        {
                            "text": "I decided to buy an extended <w>warranty</w> for my laptop just in case something goes wrong."
                        }
                    ],
                    "synonyms": [
                        "guarantee"
                    ],
                    "common_phrases": [
                        "under warranty",
                        "extended warranty",
                        "void the warranty",
                        "warranty period",
                        "lifetime warranty"
                    ]
                },
                {
                    "sense_id": "b2f757f4-a1a7-3b61-a471-97b9dd983b36",
                    "sense_definition": "A formal agreement or pledge, often implied by law, stating that goods or property are in the condition promised.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "guarantee_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The buyer sued the manufacturer for breach of <w>warranty</w> after the equipment failed immediately."
                        },
                        {
                            "text": "There is an implied <w>warranty</w> that the food sold in the restaurant is safe to eat."
                        }
                    ],
                    "synonyms": [
                        "assurance",
                        "pledge",
                        "guarantee"
                    ],
                    "common_phrases": [
                        "implied warranty",
                        "breach of warranty",
                        "express warranty"
                    ]
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6736e6ab-4262-3cf1-bd12-106ac50f5c8c",
                    "sense_definition": "To provide a formal guarantee for a product.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "guarantee_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company <w>warranties</w> its engines for five years or fifty thousand miles."
                        },
                        {
                            "text": "They refused to <w>warranty</w> the item because the damage was caused by misuse."
                        }
                    ],
                    "synonyms": [
                        "guarantee",
                        "warrant"
                    ]
                }
            ]
        }
    ]

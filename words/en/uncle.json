    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c51fc848-8c8d-3f06-94ec-2cc5f03acb40",
                    "sense_definition": "The brother of one's parent, or the husband of one's aunt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "kinship_biological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My <w>uncle</w> lives in a small house near the river."
                        },
                        {
                            "text": "We visited our <w>uncle</w> and aunt during the summer holidays."
                        }
                    ],
                    "antonyms": [
                        "aunt",
                        "nephew",
                        "niece"
                    ],
                    "common_phrases": [
                        "maternal uncle",
                        "paternal uncle",
                        "great-uncle"
                    ]
                },
                {
                    "sense_id": "c61be84a-e2fc-3549-ad2f-1029ed86907a",
                    "sense_definition": "A respectful or affectionate title for an older man, often a close friend of one's parents.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "kinship_fictive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have always called my father's best friend <w>Uncle</w> Joe."
                        },
                        {
                            "text": "The children politely addressed the shopkeeper as <w>uncle</w>."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used to address older men"
                        }
                    ]
                },
                {
                    "sense_id": "5254ae35-42f4-3302-b7a1-aef736475853",
                    "sense_definition": "A slang term for a pawnbroker.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He had to leave his watch at his <w>uncle</w>'s to get some quick cash."
                        },
                        {
                            "text": "In the past, people often joked about visiting their <w>uncle</w> when they needed money."
                        }
                    ],
                    "traits": [

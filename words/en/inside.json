{
    "word_family": [
        "insider"
    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "5b668bcd-1e7d-3b43-89e7-27b801aacde8",
                    "sense_definition": "Within the interior of something, closest to the center or to a specific point of reference.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He placed the letter <w>inside</w> the envelope.",
                            "target_lang_translations": {
                                "ru": "Он положил письмо <w>внутрь</w> конверта."
                            }
                        },
                        {
                            "text": "Is there anything <w>inside</w> the box?",
                            "target_lang_translations": {
                                "ru": "Есть ли что-нибудь <w>внутри</w> коробки?"
                            }
                        }
                    ],
                    "antonyms": [
                        "outside"
                    ],
                    "common_phrases": [
                        "inside the house",
                        "inside the box"
                    ],
                    "target_lang_definitions": {
                        "ru": "В пределах внутренней части чего-либо, ближе к центру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внутри",
                                "target_lang_sense_clarification": "нахождение в пределах чего-либо"
                            },
                            {
                                "target_lang_word": "в",
                                "target_lang_sense_clarification": "общий предлог места"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "314dfe04-a8a3-3b6e-aee2-d853e350d769",
                    "sense_definition": "Within a period of time.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The job was finished <w>inside</w> two weeks.",
                            "target_lang_translations": {
                                "ru": "Работа была закончена <w>за</w> две недели."
                            }
                        },
                        {
                            "text": "We need to get there <w>inside</w> an hour.",
                            "target_lang_translations": {
                                "ru": "Нам нужно добраться туда <w>за</w> час."
                            }
                        }
                    ],
                    "synonyms": [
                        "within"
                    ],
                    "common_phrases": [
                        "inside an hour",
                        "inside a week"
                    ],
                    "target_lang_definitions": {
                        "ru": "В течение определённого периода времени; не дольше, чем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в течение",
                                "target_lang_sense_clarification": "на протяжении периода"
                            },
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "в пределах срока"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "23a4fd43-ea4b-3cdb-9ec7-b410a711de9b",
                    "sense_definition": "Within or towards the interior of something; within the scope or limits of something (a place), especially a building.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_adv",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It started raining, so I went <w>inside</w>.",
                            "target_lang_translations": {
                                "ru": "Начался дождь, поэтому я пошёл <w>внутрь</w>."
                            }
                        },
                        {
                            "text": "Please step <w>inside</w> and close the door.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, проходите <w>внутрь</w> и закройте дверь."
                            }
                        }
                    ],
                    "synonyms": [
                        "indoors"
                    ],
                    "antonyms": [
                        "outside"
                    ],
                    "common_phrases": [
                        "go inside",
                        "stay inside",
                        "come inside"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутрь или в помещении (о здании)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внутрь",
                                "target_lang_sense_clarification": "направление движения"
                            },
                            {
                                "target_lang_word": "внутри",
                                "target_lang_sense_clarification": "местонахождение в помещении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a5b3dd36-a017-33a4-a774-a33ba451477d",
                    "sense_definition": "Intimately, secretly; without expressing what one is feeling or thinking.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "mental",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was smiling, but crying <w>inside</w>.",
                            "target_lang_translations": {
                                "ru": "Она улыбалась, но плакала <w>в душе</w>."
                            }
                        },
                        {
                            "text": "I felt dead <w>inside</w> after hearing the bad news.",
                            "target_lang_translations": {
                                "ru": "Я чувствовал себя мёртвым <w>внутри</w> после плохих новостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "inwardly"
                    ],
                    "common_phrases": [
                        "dead inside",
                        "dying inside"
                    ],
                    "target_lang_definitions": {
                        "ru": "В душе, мысленно; скрывая чувства от других."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в душе",
                                "target_lang_sense_clarification": "о чувствах и мыслях"
                            },
                            {
                                "target_lang_word": "внутри",
                                "target_lang_sense_clarification": "внутреннее состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd86eb36-ec35-3cbe-a1f5-bf15626c65fb",
                    "sense_definition": "In or to prison.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "prison",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He spent ten years <w>inside</w> for robbery.",
                            "target_lang_translations": {
                                "ru": "Он провёл десять лет <w>в тюрьме</w> за ограбление."
                            }
                        },
                        {
                            "text": "Life is hard when you are <w>inside</w>.",
                            "target_lang_translations": {
                                "ru": "Жизнь тяжела, когда ты <w>за решёткой</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В тюрьме (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в тюрьме",
                                "target_lang_sense_clarification": "место заключения"
                            },
                            {
                                "target_lang_word": "за решёткой",
                                "target_lang_sense_clarification": "идиома"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "64970111-6408-3c6c-be48-ebd391a3fa6e",
                    "sense_definition": "The interior or inner part of something.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>inside</w> of the building has been extensively restored.",
                            "target_lang_translations": {
                                "ru": "<w>Внутренняя часть</w> здания была значительно отреставрирована."
                            }
                        },
                        {
                            "text": "You should clean the <w>inside</w> of the car.",
                            "target_lang_translations": {
                                "ru": "Тебе следует почистить <w>салон</w> машины."
                            }
                        }
                    ],
                    "synonyms": [
                        "interior"
                    ],
                    "antonyms": [
                        "outside"
                    ],
                    "common_phrases": [
                        "on the inside",
                        "from the inside"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутренняя часть или сторона чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внутренняя часть",
                                "target_lang_sense_clarification": "составная часть предмета"
                            },
                            {
                                "target_lang_word": "внутренность",
                                "target_lang_sense_clarification": "пространство внутри"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6771cb6-d875-3409-87e2-a345e0110b26",
                    "sense_definition": "The interior organs of the body, especially the guts.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Eating that spoiled food will damage your <w>insides</w>.",
                            "target_lang_translations": {
                                "ru": "Употребление этой испорченной еды повредит твои <w>внутренности</w>."
                            }
                        },
                        {
                            "text": "The doctor checked his <w>insides</w> for any internal bleeding.",
                            "target_lang_translations": {
                                "ru": "Врач проверил его <w>внутренности</w> на наличие внутреннего кровотечения."
                            }
                        }
                    ],
                    "synonyms": [
                        "entrails",
                        "guts"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "usually plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутренние органы тела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внутренности",
                                "target_lang_sense_clarification": "органы"
                            },
                            {
                                "target_lang_word": "нутро",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ede3d587-dd87-3603-acf8-14e8c06e6ea4",
                    "sense_definition": "The side of a curved road or racetrack that has the shorter arc length.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "road",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He overtook the other car on the <w>inside</w>.",
                            "target_lang_translations": {
                                "ru": "Он обогнал другую машину по <w>внутренней стороне</w>."
                            }
                        },
                        {
                            "text": "Stick to the <w>inside</w> on this turn to save time.",
                            "target_lang_translations": {
                                "ru": "Держись <w>внутренней стороны</w> на этом повороте, чтобы сэкономить время."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on the inside"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сторона изогнутой дороги или трека с меньшим радиусом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внутренняя сторона",
                                "target_lang_sense_clarification": "дороги или поворота"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "acd61a13-6cc5-3e7b-9908-47d4b26ff05d",
                    "sense_definition": "Of or pertaining to the inner surface, limit or boundary.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "attr_loc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>inside</w> pocket is safer for your wallet.",
                            "target_lang_translations": {
                                "ru": "<w>Внутренний</w> карман безопаснее для твоего кошелька."
                            }
                        },
                        {
                            "text": "He ran in the <w>inside</w> lane to win the race.",
                            "target_lang_translations": {
                                "ru": "Он бежал по <w>внутренней</w> дорожке, чтобы выиграть гонку."
                            }
                        }
                    ],
                    "synonyms": [
                        "inner"
                    ],
                    "antonyms": [
                        "outside"
                    ],
                    "common_phrases": [
                        "inside pocket",
                        "inside lane"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к внутренней поверхности или границе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внутренний",
                                "target_lang_sense_clarification": "прилагательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4c56f1b4-cf26-380f-8316-754c8b59c7b9",
                    "sense_definition": "Originating from, arranged by, or being someone inside an organisation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "attr_org",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The robbery was planned by a guard, so it was an <w>inside</w> job.",
                            "target_lang_translations": {
                                "ru": "Ограбление было спланировано охранником, так что это было <w>внутреннее</w> дело."
                            }
                        },
                        {
                            "text": "She has <w>inside</w> information about the forthcoming takeover.",
                            "target_lang_translations": {
                                "ru": "У неё есть <w>внутренняя</w> информация о предстоящем поглощении."
                            }
                        }
                    ],
                    "common_phrases": [
                        "inside job",
                        "inside information",
                        "inside story"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исходящий от кого-то внутри организации; инсайдерский."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "внутренний",
                                "target_lang_sense_clarification": "относящийся к организации"
                            },
                            {
                                "target_lang_word": "инсайдерский",
                                "target_lang_sense_clarification": "о секретной информации"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
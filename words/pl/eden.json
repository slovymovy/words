                {
                    "sense_id": "17b20349-ec99-309b-b3a5-495c037d4195",
                    "sense_definition": "Biblijny ogród stworzony przez Boga dla pierwszych ludzi, Adama i Ewy; kraina wiecznego szczęścia",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Według Biblii Adam i Ewa zostali wygnani z <w>Edenu</w> za nieposłuszeństwo.",
                            "target_lang_translations": {
                                "ru": "Согласно Библии, Адам и Ева были изгнаны из <w>Эдема</w> за непослушание."
                            }
                        },
                        {
                            "text": "Malarze renesansu często przedstawiali biblijny <w>Eden</w> pełen zwierząt i roślin.",
                            "target_lang_translations": {
                                "ru": "Художники эпохи Возрождения часто изображали библейский <w>Эдем</w>, полный животных и растений."
                            }
                        }
                    ],
                    "synonyms": [
                        "raj"
                    ],
                    "common_phrases": [
                        "ogród Eden",
                        "bramy Edenu",
                        "wygnanie z Edenu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Библейский сад, созданный Богом для первых людей, Адама и Евы; страна вечного счастья"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Эдем",
                                "target_lang_sense_clarification": "библейский сад"
                            },

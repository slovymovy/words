                {
                    "sense_id": "dc1337ac-33a2-3672-a63c-2c517860fda6",
                    "sense_definition": "A city in Lancashire, North West England, which gives its name to the county and historically to the House of Lancaster.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "places",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We visited the medieval castle in <w>Lancaster</w> during our tour of England.",
                            "target_lang_translations": {
                                "ru": "Мы посетили средневековый замок в <w>Ланкастере</w> во время нашего тура по Англии."
                            }
                        },
                        {
                            "text": "The university in <w>Lancaster</w> attracts students from all over the world.",
                            "target_lang_translations": {
                                "ru": "Университет в <w>Ланкастере</w> привлекает студентов со всего мира."
                            }
                        }
                    ],
                    "common_phrases": [
                        "City of Lancaster",
                        "Duchy of Lancaster"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "UK"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Город в графстве Ланкашир на северо-западе Англии, давший название графству и историческому дому Ланкастеров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ланкастер",
                                "target_lang_sense_clarification": "город в Англии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76742cb9-a694-3d27-a78f-6efde30d5d94",
                    "sense_definition": "A royal dynasty of English kings in the 15th century, symbolized by a red rose, that fought against the House of York in the Wars of the Roses.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "history",
                    "name_type": "historical_name",
                    "examples": [
                        {
                            "text": "Henry IV was the first king to rule from the House of <w>Lancaster</w>.",
                            "target_lang_translations": {
                                "ru": "Генрих IV был первым королем из династии <w>Ланкастеров</w>."
                            }
                        },
                        {
                            "text": "The conflict between <w>Lancaster</w> and York shaped the history of medieval England.",
                            "target_lang_translations": {
                                "ru": "Конфликт между <w>Ланкастерами</w> и Йорками определил историю средневековой Англии."
                            }
                        }
                    ],
                    "synonyms": [
                        "House of Lancaster"
                    ],
                    "antonyms": [
                        "York",
                        "House of York"
                    ],
                    "common_phrases": [
                        "House of Lancaster",
                        "Duke of Lancaster"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "historical context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Королевская династия английских королей XV века, символом которой была алая роза; сражалась против дома Йорков в Войне Алой и Белой розы."
                    },
                    "translations": {
                        "ru": [
                            {

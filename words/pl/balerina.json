    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5267bf74-70c9-37ff-9cca-dae884ea85af",
                    "sense_definition": "Lekki, damski but na płaskiej podeszwie, wzorowany na obuwiu do tańca klasycznego.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "footwear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłam sobie nowe, skórzane <w>baleriny</w>, które pasują do każdej sukienki.",
                            "target_lang_translations": {
                                "ru": "Я купила себе новые кожаные <w>балетки</w>, которые подходят к любому платью."
                            }
                        },
                        {
                            "text": "<w>Baleriny</w> są co prawda wygodne, ale trudno w nich wyglądać niczym długonoga piękność.",
                            "target_lang_translations": {
                                "ru": "<w>Балетки</w>, правда, удобные, но в них трудно выглядеть как длинноногая красавица."
                            }
                        }
                    ],
                    "synonyms": [
                        "balerinka"
                    ],
                    "common_phrases": [
                        "wygodne baleriny",
                        "czarne baleriny",
                        "skórzane baleriny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкая женская обувь на плоской подошве, созданная по образцу обуви для классического танца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "балетки",
                                "target_lang_sense_clarification": "обувь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd986bc3-64c6-3305-8e0e-1d1735dc5586",
                    "sense_definition": "Tancerka baletowa, zwłaszcza solistka w zespole baletowym.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "dancer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młoda <w>balerina</w> spędzała całe dnie na sali treningowej, ćwicząc piruety.",
                            "target_lang_translations": {
                                "ru": "Молодая <w>балерина</w> проводила целые дни в тренировочном зале, отрабатывая пируэты."
                            }
                        },
                        {
                            "text": "Każda mała dziewczynka marzy czasem, by zostać sławną <w>baleriną</w>.",
                            "target_lang_translations": {
                                "ru": "Каждая маленькая девочка иногда мечтает стать известной <w>балериной</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "baletnica"
                    ],
                    "common_phrases": [
                        "zostać baleriną",
                        "słynna balerina",
                        "strój baleriny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Артистка балета, профессиональная танцовщица."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "балерина",
                                "target_lang_sense_clarification": "танцовщица"
                            }
                        ]
                    }
                }
            ]
        }
    ]

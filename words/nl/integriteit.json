    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "626c17ef-e70c-3568-b4e7-f8678c05240a",
                    "sense_definition": "De eigenschap dat iemand eerlijk en betrouwbaar is en zich houdt aan morele waarden.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "moral_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De journalist trok de <w>integriteit</w> van de minister publiekelijk in twijfel.",
                            "target_lang_translations": {
                                "ru": "Журналист публично поставил под сомнение <w>порядочность</w> министра.",
                                "en": "The journalist publicly questioned the minister's <w>integrity</w>."
                            }
                        },
                        {
                            "text": "Wij zoeken een kandidaat met een hoge mate van persoonlijke <w>integriteit</w>.",
                            "target_lang_translations": {
                                "ru": "Мы ищем кандидата с высоким уровнем личной <w>честности</w>.",
                                "en": "We are looking for a candidate with a high level of personal <w>integrity</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "onkreukbaarheid",
                        "rechtschapenheid"
                    ],
                    "antonyms": [
                        "corruptie",
                        "onbetrouwbaarheid"
                    ],
                    "common_phrases": [
                        "persoonlijke integriteit",
                        "integriteit schenden",
                        "integriteit in twijfel trekken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество человека, заключающееся в честности, надежности и соблюдении моральных ценностей.",
                        "en": "The quality of being honest, reliable, and adhering to moral values."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порядочность",
                                "target_lang_sense_clarification": "высокие моральные качества"
                            },
                            {
                                "target_lang_word": "честность",
                                "target_lang_sense_clarification": "правдивость и искренность"
                            },
                            {
                                "target_lang_word": "неподкупность",
                                "target_lang_sense_clarification": "способность не поддаваться коррупции"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "integrity",
                                "target_lang_sense_clarification": "adherence to moral principles"
                            },
                            {
                                "target_lang_word": "honesty",
                                "target_lang_sense_clarification": "quality of being truthful"
                            },
                            {
                                "target_lang_word": "probity",
                                "target_lang_sense_clarification": "strong moral principles; incorruptibility"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "72df924d-7186-32ab-bdc4-516cf44b87e3",
                    "sense_definition": "De toestand waarin iets geheel, volledig en onbeschadigd is.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "state_of_being",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De territoriale <w>integriteit</w> van het land moet te allen tijde worden beschermd.",
                            "target_lang_translations": {
                                "ru": "Территориальная <w>целостность</w> страны должна быть защищена в любое время.",
                                "en": "The country's territorial <w>integrity</w> must be protected at all times."
                            }
                        },
                        {
                            "text": "De arts beloofde de lichamelijke <w>integriteit</w> van de patiënt te respecteren.",
                            "target_lang_translations": {
                                "ru": "Врач пообещал уважать физическую <w>неприкосновенность</w> пациента.",
                                "en": "The doctor promised to respect the patient's physical <w>integrity</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "heelheid"
                    ],
                    "common_phrases": [
                        "territoriale integriteit",
                        "lichamelijke integriteit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние, при котором что-либо является целым, полным и неповрежденным.",
                        "en": "The state of being whole, complete, and undamaged."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "целостность",
                                "target_lang_sense_clarification": "состояние полноты и неразрывности"
                            },
                            {
                                "target_lang_word": "неприкосновенность",
                                "target_lang_sense_clarification": "защищенность от посягательств (например, границ или тела)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "integrity",
                                "target_lang_sense_clarification": "state of being whole and undivided"
                            },
                            {
                                "target_lang_word": "inviolability",
                                "target_lang_sense_clarification": "state of being protected from violation"
                            },
                            {
                                "target_lang_word": "wholeness",
                                "target_lang_sense_clarification": "state of being complete"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e70ce032-cbc1-3bcb-a37d-289e1854f644",
                    "sense_definition": "De zekerheid dat digitale gegevens juist en volledig zijn en niet onbevoegd zijn gewijzigd.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het bedrijf moet de <w>integriteit</w> van de klantgegevens kunnen garanderen.",
                            "target_lang_translations": {
                                "ru": "Компания должна быть в состоянии гарантировать <w>целостность</w> данных клиентов.",
                                "en": "The company must be able to guarantee the <w>integrity</w> of the customer data."
                            }
                        },
                        {
                            "text": "Door een technische storing is de <w>integriteit</w> van de database aangetast.",
                            "target_lang_translations": {
                                "ru": "Из-за технического сбоя <w>целостность</w> базы данных была нарушена.",
                                "en": "Due to a technical failure, the <w>integrity</w> of the database was compromised."
                            }
                        }
                    ],
                    "common_phrases": [
                        "integriteit van gegevens",
                        "data-integriteit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Гарантия того, что цифровые данные верны, полны и не были изменены без разрешения.",
                        "en": "The assurance that digital data is accurate, complete, and has not been altered without authorization."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "целостность",
                                "target_lang_sense_clarification": "неизменность и полнота информации"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "integrity",
                                "target_lang_sense_clarification": "accuracy and consistency of data"
                            }
                        ]
                    }
                }
            ]
        }
    ]

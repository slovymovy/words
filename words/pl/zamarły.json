    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4edf2e43-84f0-3049-aa96-8af9ae5adc9a",
                    "sense_definition": "Taki, który przestał się ruszać, znieruchomiał; zastygły w bezruchu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "motionless",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pies stał <zamarły> w bezruchu, wpatrując się uważnie w gęste zarośla.",
                            "target_lang_translations": {
                                "ru": "Собака стояла, <застывшая> в неподвижности, внимательно вглядываясь в густые заросли."
                            }
                        },
                        {
                            "text": "Był całkowicie <zamarły> ze strachu, gdy zobaczył wchodzącego do pokoju niedźwiedzia.",
                            "target_lang_translations": {
                                "ru": "Он совершенно <оцепенел> от страха, когда увидел входящего в комнату медведя."
                            }
                        }
                    ],
                    "synonyms": [
                        "nieruchomy",
                        "zastygły",
                        "znieruchomiały"
                    ],
                    "common_phrases": [
                        "zamarły w bezruchu",
                        "zamarły z przerażenia",
                        "zamarły wzrok"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто перестал двигаться, оцепенел; застывший в неподвижности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "застывший",
                                "target_lang_sense_clarification": "о человеке или животном"
                            },
                            {
                                "target_lang_word": "оцепенелый",
                                "target_lang_sense_clarification": "от страха или напряжения"
                            },
                            {
                                "target_lang_word": "замерший",
                                "target_lang_sense_clarification": "переставший двигаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79308ec6-32bf-3ff1-b384-c7d407494b9b",
                    "sense_definition": "Taki, który jest pusty, cichy i pozbawiony życia lub ruchu (zwykle o miejscu).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "empty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szliśmy powoli przez <zamarłe> miasto, w którym nie było widać żywego ducha.",
                            "target_lang_translations": {
                                "ru": "Мы медленно шли через <вымерший> город, в котором не было видно ни одной живой души."
                            }
                        },
                        {
                            "text": "To <zamarłe> miasteczko ożywa tylko latem, gdy przyjeżdżają do niego turyści.",
                            "target_lang_translations": {
                                "ru": "Этот <замерший> городок оживает только летом, когда туда приезжают туристы."
                            }
                        }
                    ],
                    "synonyms": [
                        "opustoszały",
                        "wymarły"
                    ],
                    "common_phrases": [
                        "zamarłe miasto",
                        "zamarła ulica",
                        "zamarła cisza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пустой, тихий, лишенный жизни или движения (обычно о месте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вымерший",
                                "target_lang_sense_clarification": "безлюдный"
                            },
                            {
                                "target_lang_word": "замерший",
                                "target_lang_sense_clarification": "погруженный в тишину"
                            },
                            {
                                "target_lang_word": "мёртвый",
                                "target_lang_sense_clarification": "перен. безжизненный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02612c92-fa31-38b6-8c10-0242f3a69441",
                    "sense_definition": "Taki, który przestał żyć, rosnąć lub funkcjonować; obumarły lub nieaktywny.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "dead_inactive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W starym lesie widzieliśmy liczne <zamarłe> pnie drzew, które przewrócił wiatr.",
                            "target_lang_translations": {
                                "ru": "В старом лесу мы видели многочисленные <мёртвые> стволы деревьев, которые повалил ветер."
                            }
                        },
                        {
                            "text": "Ten <zamarły> wulkan nie wybuchł od tysięcy lat i jest teraz atrakcją turystyczną.",
                            "target_lang_translations": {
                                "ru": "Этот <потухший> вулкан не извергался тысячи лет и теперь является туристической достопримечательностью."
                            }
                        }
                    ],
                    "synonyms": [
                        "wymarły"
                    ],
                    "common_phrases": [
                        "zamarły wulkan",
                        "zamarłe drzewo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переставший жить, расти или функционировать; отмерший или неактивный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отмерший",
                                "target_lang_sense_clarification": "о растениях или тканях"
                            },
                            {
                                "target_lang_word": "потухший",
                                "target_lang_sense_clarification": "о вулкане"
                            },
                            {
                                "target_lang_word": "мёртвый",
                                "target_lang_sense_clarification": "неживой"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7d4e733c-608f-32f6-9101-6ff77acf2641",
                    "sense_definition": "Trzecia osoba liczby mnogiej (rodzaj niemęskoosobowy) czasu przeszłego od słowa <w>zamrzeć</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystkie rozmowy przy stole nagle <zamarły>, gdy do sali wszedł dyrektor.",
                            "target_lang_translations": {
                                "ru": "Все разговоры за столом внезапно <замерли>, когда в зал вошел директор."
                            }
                        },
                        {
                            "text": "Jej dłonie <zamarły> w połowie gestu, jakby nie wiedziała, co dalej zrobić.",
                            "target_lang_translations": {
                                "ru": "Её ладони <замерли> на полпути, словно она не знала, что делать дальше."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rozmowy zamarły",
                        "oczy zamarły",
                        "usta zamarły"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма прошедшего времени глагола \"zamrzeć\" (замереть) для третьего лица множественного числа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "замерли",
                                "target_lang_sense_clarification": "перестали двигаться"
                            },
                            {
                                "target_lang_word": "умолкли",
                                "target_lang_sense_clarification": "о звуках или разговорах"
                            }
                        ]
                    }

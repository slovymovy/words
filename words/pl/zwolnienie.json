            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3db3c21a-15a8-328d-9e43-1f423dd855c5",
                    "sense_definition": "Dokument potwierdzający usprawiedliwioną nieobecność lub zwalniający z jakiegoś obowiązku, często z powodów zdrowotnych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "exemption_doc",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę zanieść do szkoły <zwolnienie> od lekarza, bo byłem chory przez trzy dni.",
                            "target_lang_translations": {
                                "ru": "Я должен отнести в школу <справку> от врача, так как болел три дня."
                            }
                        },
                        {
                            "text": "Nauczyciel poprosił ucznia o pisemne <zwolnienie> z lekcji wychowania fizycznego.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил у ученика письменное <освобождение> от уроков физкультуры."
                            }
                        }
                    ],
                    "synonyms": [
                        "usprawiedliwienie"
                    ],
                    "common_phrases": [
                        "zwolnienie lekarskie",
                        "zwolnienie z zajęć",
                        "zwolnienie z WF-u"
                    ],
                    "target_lang_definitions": {
                        "ru": "Документ, подтверждающий уважительную причину отсутствия или освобождающий от какой-либо обязанности, часто по состоянию здоровья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "справка",
                                "target_lang_sense_clarification": "документ от врача или учреждения"
                            },
                            {
                                "target_lang_word": "освобождение",
                                "target_lang_sense_clarification": "от занятий или обязанностей"
                            },
                            {
                                "target_lang_word": "больничный",
                                "target_lang_sense_clarification": "листок нетрудоспособности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "104e1c69-52ba-33ae-ac06-6d214ec7220c",
                    "sense_definition": "Zakończenie współpracy z pracownikiem przez pracodawcę; pozbawienie kogoś stanowiska.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "dismissal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego nagłe <zwolnienie> z pracy zaskoczyło cały zespół w biurze.",
                            "target_lang_translations": {
                                "ru": "Его внезапное <увольнение> с работы удивило весь коллектив в офисе."
                            }
                        },
                        {
                            "text": "Firma ogłosiła grupowe <zwolnienie> pracowników z powodu trudnej sytuacji finansowej.",
                            "target_lang_translations": {
                                "ru": "Компания объявила о массовом <увольнении> сотрудников из-за сложной финансовой ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "dymisja",
                        "wypowiedzenie",
                        "redukcja"
                    ],
                    "antonyms": [
                        "zatrudnienie",
                        "angaż",
                        "przyjęcie"
                    ],
                    "common_phrases": [
                        "zwolnienie dyscyplinarne",
                        "zwolnienie grupowe",
                        "wręczyć zwolnienie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прекращение трудовых отношений с работником по инициативе работодателя; лишение кого-либо должности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "увольнение",
                                "target_lang_sense_clarification": "прекращение работы"
                            },
                            {
                                "target_lang_word": "сокращение",
                                "target_lang_sense_clarification": "уменьшение штата"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "814cd219-a8b0-3b7b-b462-5ff8d4141876",
                    "sense_definition": "Rzeczownik odczasownikowy od <w>zwolnić</w>: czynność zmniejszenia prędkości poruszania się lub tempa działania.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "action_slow",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed ostrym zakrętem konieczne jest znaczne <zwolnienie> prędkości samochodu.",
                            "target_lang_translations": {
                                "ru": "Перед крутым поворотом необходимо значительное <снижение> скорости автомобиля."
                            }
                        },
                        {
                            "text": "Kierowca zauważył znak drogowy nakazujący <zwolnienie> do pięćdziesięciu kilometrów na godzinę.",
                            "target_lang_translations": {
                                "ru": "Водитель заметил дорожный знак, предписывающий <снижение скорости> до пятидесяти километров в час."
                            }
                        }
                    ],
                    "synonyms": [

                {
                    "sense_id": "4cfc497a-7301-3025-bf88-bfc6fdd71b98",
                    "sense_definition": "Having lost some of the original brightness, vividness, or color.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "visual_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She decided to throw away the <w>faded</w> curtains that had hung in the window for years.",
                            "target_lang_translations": {
                                "ru": "Она решила выбросить <w>выцветшие</w> шторы, которые висели на окне годами."
                            }
                        },
                        {
                            "text": "He wore a pair of <w>faded</w> blue jeans to the casual meeting.",
                            "target_lang_translations": {
                                "ru": "Он надел пару <w>полинявших</w> синих джинсов на неформальную встречу."
                            }
                        }
                    ],
                    "synonyms": [
                        "pale",
                        "washed-out",
                        "discolored",
                        "dim"
                    ],
                    "antonyms": [
                        "bright",
                        "vivid",
                        "colorful"
                    ],
                    "common_phrases": [
                        "faded jeans",
                        "faded colors",
                        "faded photograph"
                    ],
                    "target_lang_definitions": {
                        "ru": "Потерявший первоначальную яркость, насыщенность или цвет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выцветший",
                                "target_lang_sense_clarification": "потерявший цвет от солнца или времени"
                            },
                            {
                                "target_lang_word": "полинявший",
                                "target_lang_sense_clarification": "потерявший окраску, обычно после стирки"
                            },
                            {
                                "target_lang_word": "поблекший",
                                "target_lang_sense_clarification": "ставший менее ярким, тусклым"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11d388cc-e7fe-3e93-b1ad-f2736479fbd4",
                    "sense_definition": "Having lost former strength, vitality, importance, or clarity.",

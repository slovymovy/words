            "senses": [
                {
                    "sense_id": "0d2e39cc-f7ed-3566-ae07-9ec62c1f3e34",
                    "sense_definition": "Een onvoorziene gebeurtenis met negatieve gevolgen, zoals schade of verwondingen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er is een ernstig <w>ongeluk</w> gebeurd op de snelweg.",
                            "target_lang_translations": {
                                "en": "A serious <w>accident</w> has happened on the highway.",
                                "ru": "На шоссе произошла серьезная <w>авария</w>."
                            }
                        },
                        {
                            "text": "Ik heb per <w>ongeluk</w> jouw glas omgestoten.",
                            "target_lang_translations": {
                                "en": "I knocked over your glass by <w>accident</w>.",
                                "ru": "Я <w>случайно</w> опрокинул твой стакан."
                            }
                        }
                    ],
                    "synonyms": [
                        "ongeval"
                    ],
                    "common_phrases": [
                        "per ongeluk",
                        "een dodelijk ongeluk",
                        "een ongeluk krijgen",
                        "een ongeluk veroorzaken"
                    ],
                    "target_lang_definitions": {
                        "en": "An unforeseen event with negative consequences, such as damage or injuries.",
                        "ru": "Непредвиденное событие с негативными последствиями, такими как ущерб или травмы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "accident",
                                "target_lang_sense_clarification": "An unexpected event causing damage or injury"
                            },

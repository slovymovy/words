                {
                    "sense_id": "4052685d-a29c-3507-ba14-a85ef7fe7ffc",
                    "sense_definition": "The round yellow or orange fruit of a vine plant, often used as food or for decoration.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "plant_and_fruit",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We carved a scary face into the <w>pumpkin</w> for Halloween.",
                            "target_lang_translations": {
                                "ru": "Мы вырезали страшную рожицу на <w>тыкве</w> для Хэллоуина."
                            }
                        },
                        {
                            "text": "She baked a delicious <w>pumpkin</w> pie for Thanksgiving dinner.",
                            "target_lang_translations": {
                                "ru": "Она испекла вкусный пирог из <w>тыквы</w> к ужину на День благодарения."
                            }
                        }
                    ],
                    "synonyms": [
                        "squash",
                        "gourd"
                    ],
                    "common_phrases": [
                        "pumpkin pie",
                        "pumpkin patch",
                        "carve a pumpkin",
                        "pumpkin spice"
                    ],
                    "target_lang_definitions": {
                        "ru": "Круглый желтый или оранжевый плод вьющегося растения, часто используемый в пищу или для украшения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тыква",
                                "target_lang_sense_clarification": "плод"
                            }
                        ]

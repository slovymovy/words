{
    "word_family": [
        "adoracja",
        "adorować",
        "adoratorka"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0fc07398-83bf-3a2c-b2d1-bb76e6057e09",
                    "sense_definition": "Mężczyzna, który zabiega o względy kobiety, okazując jej zainteresowanie i podziw.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "romantic_suitor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej tajemniczy <w>adorator</w> przysyłał bukiety czerwonych róż każdego dnia.",
                            "target_lang_translations": {
                                "ru": "Её таинственный <w>поклонник</w> присылал букеты красных роз каждый день."
                            }
                        },
                        {
                            "text": "Wokół pięknej aktorki zawsze kręcił się wianuszek <w>adoratorów</w>.",
                            "target_lang_translations": {
                                "ru": "Вокруг красивой актрисы всегда крутилась толпа <w>ухажёров</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wielbiciel",
                        "absztyfikant",
                        "amant",
                        "fatygant"
                    ],
                    "common_phrases": [
                        "tajemniczy adorator",
                        "wianuszek adoratorów",
                        "namolny adorator"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужчина, который ухаживает за женщиной, проявляя к ней интерес и восхищение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поклонник",
                                "target_lang_sense_clarification": "мужчина, проявляющий романтический интерес"
                            },
                            {
                                "target_lang_word": "ухажёр",
                                "target_lang_sense_clarification": "мужчина, который активно ухаживает за женщиной"
                            },
                            {
                                "target_lang_word": "воздыхатель",
                                "target_lang_sense_clarification": "влюбленный мужчина, часто тайно или безответно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a94748e-2759-3d88-875c-e629d193c5c3",
                    "sense_definition": "Osoba, która darzy kogoś lub coś wielkim podziwem i uznaniem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "general_admirer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek był wielkim <w>adoratorem</w> muzyki klasycznej i opery.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка был большим <w>почитателем</w> классической музыки и оперы."
                            }
                        },
                        {
                            "text": "To znany <w>adorator</w> kobiecego piękna oraz sztuki współczesnej.",
                            "target_lang_translations": {
                                "ru": "Это известный <w>ценитель</w> женской красоты и современного искусства."
                            }
                        }
                    ],
                    "synonyms": [
                        "wielbiciel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который относится к кому-либо или чему-либо с большим восхищением и признанием."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "почитатель",
                                "target_lang_sense_clarification": "тот, кто глубоко уважает чей-то талант или творчество"
                            },
                            {
                                "target_lang_word": "ценитель",
                                "target_lang_sense_clarification": "знаток, способный оценить достоинства чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9cb31166-b80b-36df-a989-d063d945dc01",
                    "sense_definition": "Osoba oddająca cześć Bogu, trwająca na modlitwie przed przedmiotem kultu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "religious_worshiper",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy <w>adorator</w> w kaplicy zachowywał absolutną ciszę i skupienie.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>поклоняющийся</w> в часовне хранил абсолютную тишину и сосредоточенность."
                            }
                        },
                        {
                            "text": "Jako wierny <w>adorator</w>, spędzał wiele godzin na kontemplacji w kościele.",
                            "target_lang_translations": {
                                "ru": "Как верный <w>поклоняющийся</w>, он проводил много часов в созерцании в церкви."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wierny adorator"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, воздающий почести Богу, пребывающий в молитве перед святыней."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поклоняющийся",
                                "target_lang_sense_clarification": "участник религиозного поклонения"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "976d94c0-085a-3360-b64a-1210cba5e378",
                    "sense_definition": "Poddawać produkt żywnościowy działaniu wysokiej temperatury na rozgrzanym tłuszczu (np. oleju, maśle) w celu przygotowania go do spożycia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cooking_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja mama często <smaży> naleśniki na śniadanie.",
                            "target_lang_translations": {
                                "ru": "Моя мама часто <жарит> блины на завтрак."
                            }
                        },
                        {
                            "text": "Kucharz <smaży> rybę na małym ogniu, żeby się nie przypaliła.",
                            "target_lang_translations": {
                                "ru": "Повар <жарит> рыбу на медленном огне, чтобы она не пригорела."
                            }
                        }
                    ],
                    "antonyms": [
                        "gotować"
                    ],
                    "common_phrases": [
                        "smażyć na oleju",
                        "smażyć na patelni",
                        "smażyć jajecznicę",
                        "smażyć rybę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подвергать продукты термической обработке на разогретом жире (например, масле) для приготовления к употреблению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жарить",
                                "target_lang_sense_clarification": "готовить пищу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1c91553-9002-3be8-ab9b-9a40dcb7dd1a",
                    "sense_definition": "O potrawie: ulegać procesowi obróbki termicznej na gorącym tłuszczu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cooking_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Cebula <smaży się> na złoty kolor.",
                            "target_lang_translations": {
                                "ru": "Лук <жарится> до золотистого цвета."
                            }
                        },
                        {
                            "text": "W kuchni ładnie pachnie, bo mięso <smaży się> na patelni.",
                            "target_lang_translations": {
                                "ru": "На кухне вкусно пахнет, потому что мясо <жарится> на сковороде."
                            }
                        }
                    ],
                    "common_phrases": [
                        "smażyć się na wolnym ogniu",
                        "mięso się smaży"
                    ],
                    "target_lang_definitions": {
                        "ru": "О блюде: подвергаться термической обработке в горячем жире."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жариться",
                                "target_lang_sense_clarification": "о еде"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0e05340e-70f5-3c84-9a2c-5d388c0aecdd",
                    "sense_definition": "Opalać się bardzo intensywnie, wystawiając ciało na długotrwałe działanie słońca.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sunbathing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Turyści całymi dniami <smażą się> na plaży nad morzem.",
                            "target_lang_translations": {
                                "ru": "Туристы целыми днями <жарятся> на пляже у моря."
                            }
                        },
                        {
                            "text": "Nie lubię <smażyć się> na słońcu, wolę siedzieć w cieniu.",

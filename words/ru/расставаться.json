    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "af1969e7-d709-3eae-a9a9-093dcb0d3a16",
                    "sense_definition": "Уходить друг от друга, прощаться при разлуке.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "separation_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы долго <расставались> на вокзале перед отходом поезда.",
                            "target_lang_translations": {
                                "en": "We spent a long time <saying goodbye> at the station before the train departed."
                            }
                        },
                        {
                            "text": "Друзьям было грустно <расставаться> после весёлых каникул.",
                            "target_lang_translations": {
                                "en": "The friends were sad to <part> after the fun holidays."
                            }
                        }
                    ],
                    "synonyms": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e25dea44-e57d-378d-af08-1b535bfabc5f",
                    "sense_definition": "Een cel in een gevangenis waarin een gevangene apart van de anderen wordt opgesloten, meestal als strafmaatregel.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "afzonderingsruimte",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De bewaker bracht de agressieve gevangene direct naar de <w>isolatiecel</w>.",
                            "target_lang_translations": {
                                "ru": "Охранник отвел агрессивного заключенного прямо в <w>карцер</w>."
                            }
                        },
                        {
                            "text": "Hij moest voor straf drie dagen in de <w>isolatiecel</w> doorbrengen.",
                            "target_lang_translations": {
                                "ru": "В наказание ему пришлось провести три дня в <w>карцере</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "isoleercel"
                    ],
                    "common_phrases": [
                        "in de isolatiecel",
                        "naar de isolatiecel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Камера в тюрьме, где заключенного содержат отдельно от других, обычно в качестве меры наказания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "карцер",
                                "target_lang_sense_clarification": "тюремная камера для наказаний"
                            },
                            {
                                "target_lang_word": "одиночная камера",
                                "target_lang_sense_clarification": "камера для одиночного заключения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97d095b1-e77e-3999-b9ce-02831ad13419",
                    "sense_definition": "Een beveiligde ruimte in een psychiatrisch ziekenhuis waar een patiënt tijdelijk wordt afgezonderd ter bescherming.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "afzonderingsruimte",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verpleging probeerde de patiënt te kalmeren voordat ze de <w>isolatiecel</w> overwogen.",
                            "target_lang_translations": {
                                "ru": "Медперсонал пытался успокоить пациента, прежде чем рассматривать перевод в <w>изолятор</w>."
                            }
                        },
                        {
                            "text": "In moderne klinieken wordt de <w>isolatiecel</w> steeds minder vaak gebruikt.",
                            "target_lang_translations": {
                                "ru": "В современных клиниках <w>изолятор</w> используется все реже."
                            }
                        }
                    ],
                    "synonyms": [
                        "isoleercel"
                    ],
                    "common_phrases": [
                        "in de isolatiecel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Защищенное помещение в психиатрической больнице, где пациента временно изолируют для защиты."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изолятор",
                                "target_lang_sense_clarification": "в медицинском учреждении"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
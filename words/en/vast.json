{
    "word_family": [
        "vastly",
        "vastness"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d1bf7b01-885e-3ecb-9168-ca8099e041c6",
                    "sense_definition": "Very great in size, amount, degree, intensity, or especially extent.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>vast</w> majority of the students decided to join the school trip.",
                            "target_lang_translations": {
                                "ru": "<w>Подавляющее</w> большинство студентов решило поехать на школьную экскурсию."
                            }
                        },
                        {
                            "text": "There is a <w>vast</w> difference between the two proposed plans for the project.",
                            "target_lang_translations": {
                                "ru": "Между двумя предложенными планами проекта существует <w>огромная</w> разница."
                            }
                        },
                        {
                            "text": "We gazed out across the <w>vast</w> ocean, unable to see land in any direction.",
                            "target_lang_translations": {
                                "ru": "Мы смотрели на <w>бескрайний</w> океан, не видя земли ни в одной стороне."
                            }
                        }
                    ],
                    "synonyms": [
                        "huge",
                        "enormous",
                        "immense",
                        "massive",
                        "extensive"
                    ],
                    "antonyms": [
                        "tiny",
                        "small",
                        "minute",
                        "limited"
                    ],
                    "common_phrases": [
                        "vast majority",
                        "vast amount",
                        "vast number",
                        "vast area",
                        "vast difference"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень большой по размеру, количеству, степени, интенсивности или, особенно, протяженности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "огромный",
                                "target_lang_sense_clarification": "по размеру, количеству или степени"
                            },
                            {
                                "target_lang_word": "обширный",
                                "target_lang_sense_clarification": "по площади, охвату или знаниям"
                            },
                            {
                                "target_lang_word": "бескрайний",
                                "target_lang_sense_clarification": "не имеющий видимых границ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "389dd74e-84aa-3f8d-a809-5fe8f5c59586",
                    "sense_definition": "Waste; desert; desolate; lonely.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The travelers felt small and vulnerable in the <w>vast</w> and empty wilderness.",
                            "target_lang_translations": {
                                "ru": "Путешественники чувствовали себя маленькими и уязвимыми в <w>пустынной</w> и пустой глуши."
                            }
                        },
                        {
                            "text": "He wandered through the <w>vast</w> lands where no other soul could be found.",
                            "target_lang_translations": {
                                "ru": "Он бродил по <w>безлюдным</w> землям, где нельзя было встретить ни одной живой души."
                            }
                        }
                    ],
                    "synonyms": [
                        "desolate",
                        "empty"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "used in older literature to mean desolate or waste"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пустынный, необитаемый, заброшенный (устаревшее или литературное значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пустынный",
                                "target_lang_sense_clarification": "необитаемый, пустой"
                            },
                            {
                                "target_lang_word": "безлюдный",
                                "target_lang_sense_clarification": "лишенный людей"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dfc3abc0-3c64-3fbf-a6cd-161dd66fe188",
                    "sense_definition": "A vast space.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rocket launched upward and disappeared into the dark <w>vast</w> of space.",
                            "target_lang_translations": {
                                "ru": "Ракета взмыла вверх и исчезла в темном <w>просторе</w> космоса."
                            }
                        },
                        {
                            "text": "She stood on the cliff edge, looking out over the watery <w>vast</w> below.",
                            "target_lang_translations": {
                                "ru": "Она стояла на краю обрыва, глядя на водную <w>ширь</w> внизу."
                            }
                        }
                    ],
                    "synonyms": [
                        "expanse",
                        "void"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "literary or poetic usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Огромное пространство, простор (существительное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "простор",
                                "target_lang_sense_clarification": "свободное обширное пространство"
                            },
                            {
                                "target_lang_word": "ширь",
                                "target_lang_sense_clarification": "широкое пространство, раздолье"
                            },
                            {
                                "target_lang_word": "бескрайность",
                                "target_lang_sense_clarification": "отсутствие границ"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
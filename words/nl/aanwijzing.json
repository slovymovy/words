                    "sense_id": "a3c674af-c42d-376f-a314-a3bacc341487",
                    "sense_definition": "Een inlichting of voorschrift over hoe men moet handelen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar gaf duidelijke <w>aanwijzingen</w> over wat we moesten leren.",
                            "target_lang_translations": {
                                "en": "The teacher gave clear <w>instructions</w> about what we had to learn.",
                                "ru": "Учитель дал чёткие <w>указания</w> насчёт того, что нам нужно выучить."
                            }
                        },
                        {
                            "text": "Je moet de <w>aanwijzingen</w> van de politie altijd direct opvolgen.",
                            "target_lang_translations": {
                                "en": "You must always follow the police's <w>instructions</w> immediately.",
                                "ru": "Вы должны всегда немедленно следовать <w>указаниям</w> полиции."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aanwijzingen geven",
                        "aanwijzingen opvolgen",
                        "volgens de aanwijzingen"
                    ],
                    "target_lang_definitions": {
                        "en": "Information or a rule on how to act.",
                        "ru": "Информация или предписание о том, как следует действовать."
                    },
                    "translations": {

                {
                    "sense_id": "2abfa3ce-3452-3ed9-b26d-9f014a09070f",
                    "sense_definition": "Having lost one's job; having had one's employment ended.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "employment_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>dismissed</w> workers were offered support to find new employment.",
                            "target_lang_translations": {
                                "ru": "<w>Уволенным</w> рабочим предложили помощь в поиске новой работы."
                            }
                        },
                        {
                            "text": "He felt angry about being a <w>dismissed</w> employee without a valid reason.",
                            "target_lang_translations": {
                                "ru": "Он злился из-за того, что оказался <w>уволенным</w> сотрудником без уважительной причины."
                            }
                        }
                    ],
                    "synonyms": [
                        "fired",
                        "discharged",
                        "terminated"
                    ],
                    "antonyms": [
                        "hired",
                        "employed"
                    ],
                    "common_phrases": [
                        "dismissed workers",
                        "dismissed employees"
                    ],
                    "target_lang_definitions": {
                        "ru": "Потерявший работу; тот, с кем расторгли трудовой договор."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уволенный",
                                "target_lang_sense_clarification": "лишившийся работы"

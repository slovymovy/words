    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d3ad5619-64e9-3204-8ed5-16becf519067",
                    "sense_definition": "Частный предприниматель, владелец мелкого предприятия или торговец, работающий на себя.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На городском рынке торгуют в основном мелкие <w>частники</w>, предлагающие свежие продукты.",
                            "target_lang_translations": {
                                "en": "At the city market, mostly small <w>private traders</w> sell fresh produce."
                            }
                        },
                        {
                            "text": "Этот <w>частник</w> открыл свою пекарню и теперь продает хлеб всему району.",
                            "target_lang_translations": {
                                "en": "This <w>private entrepreneur</w> opened his own bakery and now sells bread to the whole district."
                            }
                        }
                    ],
                    "synonyms": [
                        "предприниматель",
                        "бизнесмен",
                        "собственник"
                    ],
                    "target_lang_definitions": {
                        "en": "A private entrepreneur, owner of a small business, or a trader working for themselves."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "private entrepreneur",
                                "target_lang_sense_clarification": "business owner"
                            },
                            {
                                "target_lang_word": "private trader",
                                "target_lang_sense_clarification": "market seller"
                            },
                            {
                                "target_lang_word": "private owner",
                                "target_lang_sense_clarification": "general ownership"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a190da88-1e4c-31ce-b94a-2d14c2a5e56e",
                    "sense_definition": "Водитель личного автомобиля, который занимается частным извозом пассажиров за плату.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Поздно вечером автобусы уже не ходили, поэтому нам пришлось ловить <w>частника</w>.",
                            "target_lang_translations": {
                                "en": "Late in the evening buses were no longer running, so we had to catch a <w>private taxi</w>."
                            }
                        },
                        {
                            "text": "<w>Частник</w> согласился довезти нас до вокзала быстрее и дешевле, чем официальное такси.",
                            "target_lang_translations": {
                                "en": "The <w>private driver</w> agreed to take us to the station faster and cheaper than an official taxi."
                            }
                        }
                    ],
                    "synonyms": [
                        "бомбила"
                    ],
                    "common_phrases": [
                        "ловить частника",
                        "поймать частника",
                        "ехать на частнике"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "refers to unofficial taxi drivers"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A driver of a private car who offers unofficial taxi services for a fee."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "private taxi driver",
                                "target_lang_sense_clarification": "unofficial taxi service"
                            },
                            {
                                "target_lang_word": "private motorist",
                                "target_lang_sense_clarification": "driver giving rides for money"
                            }
                        ]
                    }
                }
            ]
        }
    ]

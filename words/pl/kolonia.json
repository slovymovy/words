    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a582210e-f314-3cc3-8b9f-3b1dd1bb57f2",
                    "sense_definition": "Zorganizowany wyjazd wypoczynkowy dla dzieci i młodzieży, odbywający się zazwyczaj w czasie wakacji pod opieką wychowawców (często używane w liczbie mnogiej).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "recreation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja młodsza siostra po raz pierwszy jedzie na <kolonie> nad morze.",
                            "target_lang_translations": {
                                "ru": "Моя младшая сестра в первый раз едет в <летний лагерь> на море."
                            }
                        },
                        {
                            "text": "Rodzice zapisali syna na <kolonię> językową w górach.",
                            "target_lang_translations": {
                                "ru": "Родители записали сына в языковой <лагерь> в горах."
                            }
                        }
                    ],
                    "synonyms": [
                        "wybudowanie"
                    ],
                    "common_phrases": [
                        "jechać na kolonie",
                        "kolonie letnie",
                        "wychowawca kolonijny"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "often used in plural: kolonie"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Организованный отдых для детей и молодежи, обычно проходящий во время каникул под присмотром воспитателей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "летний лагерь",
                                "target_lang_sense_clarification": "детский отдых"
                            },
                            {
                                "target_lang_word": "детский лагерь",
                                "target_lang_sense_clarification": "место отдыха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0bea1e48-b9d8-3e91-bdde-c88678cefb21",
                    "sense_definition": "Terytorium zamorskie lub odległy obszar podległy politycznie i gospodarczo innemu państwu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "politics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wiele państw afrykańskich było kiedyś europejskimi <koloniami>.",
                            "target_lang_translations": {
                                "ru": "Многие африканские страны когда-то были европейскими <колониями>."
                            }
                        },
                        {
                            "text": "Mieszkańcy <kolonii> domagali się niepodległości od metropolii.",
                            "target_lang_translations": {
                                "ru": "Жители <колонии> требовали независимости от метрополии."
                            }
                        }
                    ],
                    "antonyms": [
                        "metropolia"
                    ],
                    "common_phrases": [
                        "była kolonia",
                        "zakładać kolonie",
                        "kolonia karna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заморская территория или отдаленная область, находящаяся в политической и экономической зависимости от другого государства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колония",
                                "target_lang_sense_clarification": "зависимая территория"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2d79285-a9ab-38d1-ba67-b1b601aa4ac6",
                    "sense_definition": "Skupisko organizmów jednego gatunku żyjących blisko siebie, często połączonych fizycznie lub współpracujących.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pod mikroskopem widać, jak szybko rozrasta się <kolonia> bakterii.",
                            "target_lang_translations": {
                                "ru": "Под микроскопом видно, как быстро разрастается <колония> бактерий."
                            }
                        },
                        {
                            "text": "Na tej wyspie znajduje się ogromna <kolonia> pingwinów.",
                            "target_lang_translations": {
                                "ru": "На этом острове находится огромная <колония> пингвинов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kolonia bakterii",
                        "kolonia mrówek",
                        "kolonia lęgowa"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "scientific context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Скопление организмов одного вида, живущих близко друг к другу, часто физически соединенных или взаимодействующих."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колония",
                                "target_lang_sense_clarification": "биология"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "29eccae1-f9b7-368d-95fb-f491bde037cb",
                    "sense_definition": "Zespół domów mieszkalnych, często robotniczych, położony z dala od centrum miasta lub stanowiący wyodrębnioną część osady.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "urbanization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mieszkamy w starej górniczej <kolonii> na obrzeżach miasta.",
                            "target_lang_translations": {
                                "ru": "Мы живем в старом шахтерском <посёлке> на окраине города."
                            }
                        },
                        {
                            "text": "Deweloper planuje wybudować nową <kolonię> domków jednorodzinnych.",
                            "target_lang_translations": {
                                "ru": "Застройщик планирует построить новый <посёлок> частных домов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kolonia domków",
                        "kolonia górnicza",
                        "kolonia robotnicza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа жилых домов, часто для рабочих, расположенная вдали от центра города или составляющая обособленную часть поселения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посёлок",
                                "target_lang_sense_clarification": "жилой массив"
                            },
                            {
                                "target_lang_word": "рабочий посёлок",
                                "target_lang_sense_clarification": "тип поселения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9889d733-00d4-36d2-898a-d714254f78e2",
                    "sense_definition": "Miejsce przymusowego odosobnienia i ciężkiej pracy dla więźniów, zazwyczaj w odległym rejonie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "law",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Więźniowie zostali zesłani do <kolonii> karnej na wiele lat.",
                            "target_lang_translations": {
                                "ru": "Заключенные были сосланы в исправительную <колонию> на много лет."
                            }
                        },
                        {
                            "text": "Ucieczka z pilnie strzeżonej <kolonii> była praktycznie niemożliwa.",
                            "target_lang_translations": {
                                "ru": "Побег из строго охраняемой <колонии> был практически невозможен."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kolonia karna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место принудительного содержания и тяжелого труда для заключенных, обычно в отдаленном районе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "колония",
                                "target_lang_sense_clarification": "место заключения"
                            },
                            {
                                "target_lang_word": "исправительная колония",
                                "target_lang_sense_clarification": "официальное название"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "d5ab4f59-4aeb-3a19-80da-e6debbb4f9cf",
                    "sense_definition": "Duże miasto w zachodnich Niemczech, położone nad rzeką Ren, słynące z zabytkowej katedry.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "geography",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "W przyszłym tygodniu jadę pociągiem do <Kolonii>.",
                            "target_lang_translations": {
                                "ru": "На следующей неделе я еду на поезде в <Кёльн>."
                            }
                        },
                        {
                            "text": "Katedra w <Kolonii> jest jednym z najważniejszych zabytków w Niemczech.",
                            "target_lang_translations": {
                                "ru": "Собор в <Кёльне> — один из важнейших памятников в Германии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "katedra w Kolonii",
                        "woda z Kolonii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупный город на западе Германии, расположенный на реке Рейн, известный своим историческим собором."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Кёльн",
                                "target_lang_sense_clarification": "город в Германии"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0739a204-3a63-3d3f-bf40-4ccb54fb33b1",
                    "sense_definition": "Zdrownienie od słowa <w>nos</w>; mały, często zgrabny nos.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziecko miało czerwony <nosek> od mroźnego wiatru.",
                            "target_lang_translations": {
                                "ru": "У ребенка был красный <носик> от морозного ветра."
                            }
                        },
                        {
                            "text": "Ona ma bardzo zgrabny <nosek>, który pasuje do jej twarzy.",
                            "target_lang_translations": {
                                "ru": "У нее очень изящный <носик>, который подходит к ее лицу."
                            }
                        }
                    ],
                    "synonyms": [
                        "noseczek",
                        "nosunio"
                    ],
                    "common_phrases": [
                        "zgrabny nosek",
                        "zadarty nosek",
                        "pudrować nosek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от слова <w>нос</w>; маленький, часто изящный нос."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "носик",
                                "target_lang_sense_clarification": "уменьшительно-ласкательное от нос"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee65b008-2144-3b62-a03a-43c50f65428f",
                    "sense_definition": "Przednia część buta, która osłania palce stopy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiłam eleganckie czółenka, które mają otwarty <nosek>.",
                            "target_lang_translations": {
                                "ru": "Я купила элегантные туфли, у которых открытый <мысок>."
                            }
                        },
                        {
                            "text": "Ten but ma wzmocniony <nosek>, aby chronić palce przed uderzeniami.",
                            "target_lang_translations": {
                                "ru": "У этого ботинка усиленный <носок>, чтобы защитить пальцы от ударов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "buty z noskiem",
                        "otwarty nosek",
                        "okrągły nosek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передняя часть обуви, закрывающая пальцы ног."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мысок",
                                "target_lang_sense_clarification": "передняя часть обуви"
                            },
                            {
                                "target_lang_word": "носок",
                                "target_lang_sense_clarification": "часть обуви"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f54572b2-d5ac-3ddc-b13a-cb58a96bdbe9",
                    "sense_definition": "Skrzydlaty owoc klonu, który wiruje podczas spadania z drzewa.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jesienią dzieci zbierają każdy spadający <nosek> i przyklejają go sobie do czoła.",
                            "target_lang_translations": {
                                "ru": "Осенью дети подбирают каждый упавший <носик> и приклеивают его себе на лоб."
                            }
                        },
                        {
                            "text": "Wiatr porwał suchy <nosek> klonu i poniósł go na drugą stronę ulicy.",
                            "target_lang_translations": {
                                "ru": "Ветер подхватил сухой <носик> клёна и понес его на другую сторону улицы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nosek klonu",
                        "przykleić sobie nosek"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "common name for maple samara"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Крылатый плод клёна, который вращается при падении с дерева."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "носик",
                                "target_lang_sense_clarification": "разг. семя клёна"

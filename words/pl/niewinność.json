                    "target_lang_definitions": {
                        "ru": "Свойство того, что является безвредным, незначительным и не имеет негативных последствий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безобидность",
                                "target_lang_sense_clarification": "отсутствие злого умысла (о шутках, словах)"
                            },
                            {

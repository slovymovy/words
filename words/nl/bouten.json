    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e5cd1230-e2d4-33f6-b6f9-a4b59ae4217a",
                    "sense_definition": "Meervoud van <w>bout</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Normaal gesproken was dat geen enkel probleem geweest, ze gebruikten een eenvoudige en beproefde techniek met platen en <w>bouten</w> voor de samenvoeging.",
                            "target_lang_translations": {
                                "ru": "Обычно это не составило бы никаких проблем, они использовали простую и проверенную технику с пластинами и <w>болтами</w> для соединения."
                            }
                        },
                        {
                            "text": "Heb jij de <w>bouten</w> en moeren bij de hand om dit rek in elkaar te zetten?",
                            "target_lang_translations": {
                                "ru": "У тебя есть под рукой <w>болты</w> и гайки, чтобы собрать этот стеллаж?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Множественное число слова <w>bout</w> (болт)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "болты",
                                "target_lang_sense_clarification": "крепежные изделия"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b115d5ba-c04c-341a-b0b2-5bb50e161ac2",
                    "sense_definition": "Iets stevig vastmaken met behulp van bouten.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "technical_fastening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De stalen segmenten kunnen snel aan elkaar worden <w>gebout</w>.",
                            "target_lang_translations": {
                                "ru": "Стальные сегменты могут быть быстро <w>скреплены болтами</w> друг с другом."
                            }
                        },
                        {
                            "text": "We hebben de machine stevig aan de vloer <w>gebout</w> zodat hij niet kan verschuiven.",
                            "target_lang_translations": {
                                "ru": "Мы надежно <w>прикрутили болтами</w> станок к полу, чтобы он не сдвинулся."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aan elkaar bouten",
                        "vast bouten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прочно прикреплять что-либо с помощью болтов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скреплять болтами",
                                "target_lang_sense_clarification": "соединять"
                            },
                            {
                                "target_lang_word": "привинчивать",
                                "target_lang_sense_clarification": "крепить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0368bac6-17cd-37e8-b390-2d04d6d42913",
                    "sense_definition": "Zich ontlasten; naar de wc gaan voor de grote boodschap.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "slang_bodily_function",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij zit al een half uur op de wc te <w>bouten</w>.",
                            "target_lang_translations": {
                                "ru": "Он уже полчаса сидит в туалете и <w>срёт</w>."
                            }
                        },
                        {
                            "text": "Na het eten van die bedorven vis moest hij de hele nacht <w>bouten</w>.",
                            "target_lang_translations": {
                                "ru": "После того как он съел ту тухлую рыбу, ему пришлось всю ночь <w>срать</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "scheten"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Испражняться; ходить в туалет «по-большому»."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "срать",
                                "target_lang_sense_clarification": "грубое"
                            },
                            {
                                "target_lang_word": "гадить",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d0dac04-7448-335f-a404-17e1fbbba6d4",
                    "sense_definition": "Winden laten; gassen uit de darmen laten ontsnappen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "slang_bodily_function",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het stinkt hier verschrikkelijk, wie heeft er <w>gebout</w>?",
                            "target_lang_translations": {
                                "ru": "Здесь ужасно воняет, кто <w>напердел</w>?"
                            }
                        },
                        {
                            "text": "Je moet niet zo <w>bouten</w> in een kleine ruimte.",
                            "target_lang_translations": {
                                "ru": "Не стоит так <w>пердеть</w> в тесном помещении."
                            }
                        }
                    ],
                    "synonyms": [
                        "ruften",
                        "scheten"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пукать; выпускать газы из кишечника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пердеть",
                                "target_lang_sense_clarification": "грубое"
                            },
                            {
                                "target_lang_word": "бздеть",
                                "target_lang_sense_clarification": "грубое"
                            }
                        ]

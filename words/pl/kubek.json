{
    "word_family": [
        "kubeczek",
        "kubas",
        "kubkowy"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "82c02b93-7469-36c3-a8a1-b6a6627ffaa5",
                    "sense_definition": "Naczynie do picia, zwykle z uchem, większe od filiżanki, wykonane najczęściej z ceramiki, szkła lub plastiku.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano wypijam duży <kubek> gorącej kawy.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я выпиваю большую <кружку> горячего кофе."
                            }
                        },
                        {
                            "text": "Postaw ten brudny <kubek> do zlewu w kuchni.",
                            "target_lang_translations": {
                                "ru": "Поставь эту грязную <кружку> в раковину на кухне."
                            }
                        }
                    ],
                    "synonyms": [
                        "garnuszek"
                    ],
                    "common_phrases": [
                        "kubek kawy",
                        "kubek herbaty",
                        "kubek termiczny",
                        "kubek w kubek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сосуд для питья, обычно с ручкой, больше чашки, чаще всего изготовленный из керамики, стекла или пластика."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кружка",
                                "target_lang_sense_clarification": "посуда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ed1b77b-351e-3692-b865-86a85438f036",
                    "sense_definition": "Struktura anatomiczna znajdująca się głównie na języku, odpowiedzialna za odczuwanie smaku.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zbyt gorące potrawy mogą poparzyć twoje <kubki> smakowe.",
                            "target_lang_translations": {
                                "ru": "Слишком горячие блюда могут обжечь твои вкусовые <рецепторы>."
                            }
                        },
                        {
                            "text": "Z wiekiem <kubki> smakowe stają się mniej wrażliwe na bodźce.",
                            "target_lang_translations": {
                                "ru": "С возрастом вкусовые <рецепторы> становятся менее чувствительными к раздражителям."
                            }
                        }
                    ],
                    "synonyms": [
                        "receptor smakowy"
                    ],
                    "common_phrases": [
                        "kubki smakowe"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "anatomia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Анатомическая структура, расположенная в основном на языке и отвечающая за восприятие вкуса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вкусовая почка",
                                "target_lang_sense_clarification": "анатомия"
                            },
                            {
                                "target_lang_word": "вкусовой рецептор",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85e9f59b-aa0f-368f-a057-0cb3541c3f66",
                    "sense_definition": "Metalowy przyrząd służący do asekuracji i zjazdów na linie, używany we wspinaczce.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "climbing_gear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Instruktor pokazał nam, jak poprawnie wpiąć linę w <kubek>.",
                            "target_lang_translations": {
                                "ru": "Инструктор показал нам, как правильно заправить веревку в <страховочное устройство>."
                            }
                        },
                        {
                            "text": "Do bezpiecznej asekuracji na ściance niezbędny jest atestowany <kubek>.",
                            "target_lang_translations": {
                                "ru": "Для безопасной страховки на скалодроме необходимо сертифицированное <страховочное устройство>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kubek asekuracyjny"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "wspinaczka"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Металлическое устройство для страховки и спуска по веревке, используемое в скалолазании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "страховочное устройство",
                                "target_lang_sense_clarification": "альпинизм"
                            },
                            {
                                "target_lang_word": "стакан",
                                "target_lang_sense_clarification": "сленг альпинистов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a61181f6-8a9a-346a-85ad-d9f34d77360f",
                    "sense_definition": "Grzyb z rodzaju Cyathus, charakteryzujący się owocnikiem w kształcie małego kieliszka lub kubeczka.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "fungi",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas spaceru po lesie biolog znalazł rzadki okaz <kubka>.",
                            "target_lang_translations": {
                                "ru": "Во время прогулки по лесу биолог нашел редкий экземпляр <бокальчика>."
                            }
                        },
                        {
                            "text": "Owocniki tego <kubka> przypominają miniaturowe gniazda z jajami.",
                            "target_lang_translations": {
                                "ru": "Плодовые тела этого <бокальчика> напоминают миниатюрные гнезда с яйцами."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mykologia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Гриб из рода Cyathus, отличающийся плодовым телом в форме маленького бокала или чашечки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бокальчик",
                                "target_lang_sense_clarification": "гриб"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                        {
                            "text": "Our local library is celebrating its tenth <w>birthday</w> next month.",
                            "target_lang_translations": {
                                "ru": "Наша местная библиотека отмечает свой десятый <w>день рождения</w> в следующем месяце."
                            }
                        }
                    ],
                    "common_phrases": [
                        "company birthday",
                        "official birthday"
                    ],
                    "target_lang_definitions": {

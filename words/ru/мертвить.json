{
    "word_family": [
        "смерть",
        "мёртвый",
        "мертвец",
        "умертвить",
        "омертветь",
        "смертный",
        "бессмертие",
        "мертвенный"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9efdd25c-4125-368c-bf7f-5d4797db3d3d",
                    "sense_definition": "Лишать жизненных сил, способности жить и развиваться; губить (обычно о природных явлениях).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_deadening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Палящее солнце <мертвит> траву и цветы в поле.",
                            "target_lang_translations": {
                                "en": "The scorching sun <w>kills</w> the grass and flowers in the field."
                            }
                        },
                        {
                            "text": "Сильный мороз зимой <мертвит> всё живое в лесу.",
                            "target_lang_translations": {
                                "en": "Severe frost in winter <w>kills</w> everything living in the forest."
                            }
                        }
                    ],
                    "synonyms": [
                        "губить",
                        "убивать"
                    ],
                    "antonyms": [
                        "оживлять",
                        "возрождать"
                    ],
                    "common_phrases": [
                        "зной мертвит",
                        "мертвить природу",
                        "мертвить всё живое"
                    ],
                    "target_lang_definitions": {
                        "en": "To deprive of vitality, ability to live and develop; to destroy (usually regarding natural phenomena)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "kill",
                                "target_lang_sense_clarification": "general sense of causing death or destruction"
                            },
                            {
                                "target_lang_word": "deaden",
                                "target_lang_sense_clarification": "literary or poetic usage"
                            },
                            {
                                "target_lang_word": "blight",
                                "target_lang_sense_clarification": "specifically regarding plants or nature"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b3a607d-56da-3d6b-9388-6105063ad77e",
                    "sense_definition": "Делать вялым и безжизненным, лишать бодрости или душевных сил.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_deadening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Глубокая печаль <мертвит> его душу и лишает надежды.",
                            "target_lang_translations": {
                                "en": "Deep sorrow <w>deadens</w> his soul and deprives him of hope."
                            }
                        },
                        {
                            "text": "Постоянный страх <мертвит> волю человека.",
                            "target_lang_translations": {
                                "en": "Constant fear <w>paralyzes</w> a person's will."
                            }
                        }
                    ],
                    "synonyms": [
                        "убивать"
                    ],
                    "antonyms": [
                        "оживлять"
                    ],
                    "common_phrases": [
                        "тоска мертвит",
                        "страх мертвит",
                        "мертвит душу"
                    ],
                    "target_lang_definitions": {
                        "en": "To make listless and lifeless; to deprive of vigor or spiritual strength"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deaden",
                                "target_lang_sense_clarification": "to make feelings or the soul less intense"
                            },
                            {
                                "target_lang_word": "numb",
                                "target_lang_sense_clarification": "to deprive of feeling or responsiveness"
                            },
                            {
                                "target_lang_word": "paralyze",
                                "target_lang_sense_clarification": "to render the will or spirit unable to act"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0401946f-18f8-3324-b1ce-78d37fce8fbf",
                    "sense_definition": "Приводить к застою, останавливать развитие чего-либо (общества, искусства, мысли).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "metaphorical_deadening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Строгая цензура часто <мертвит> современную литературу.",
                            "target_lang_translations": {
                                "en": "Strict censorship often <w>stifles</w> modern literature."
                            }
                        },
                        {
                            "text": "Излишний бюрократический контроль <мертвит> любую инициативу.",
                            "target_lang_translations": {
                                "en": "Excessive bureaucratic control <w>kills</w> any initiative."
                            }
                        }
                    ],
                    "synonyms": [
                        "губить"
                    ],
                    "antonyms": [
                        "возрождать",
                        "оживлять"
                    ],
                    "common_phrases": [
                        "мертвить искусство",
                        "мертвить мысль",
                        "мертвить общество"
                    ],
                    "target_lang_definitions": {
                        "en": "To bring to stagnation; to stop the development of something (society, art, thought)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stifle",
                                "target_lang_sense_clarification": "to suppress development or creativity"
                            },
                            {
                                "target_lang_word": "suffocate",
                                "target_lang_sense_clarification": "metaphorical sense of preventing growth"
                            },
                            {
                                "target_lang_word": "kill",
                                "target_lang_sense_clarification": "to put an end to an initiative or idea"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
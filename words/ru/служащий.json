    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a5211192-2a2f-3f22-b708-6969a94714a1",
                    "sense_definition": "Человек, работающий по найму в государственном учреждении, офисе или частной компании, обычно занимающийся нефизическим трудом.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "employee_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждый <служащий> банка должен носить строгий костюм.",
                            "target_lang_translations": {
                                "en": "Every bank <w>employee</w> must wear a formal suit."
                            }
                        },
                        {
                            "text": "Государственные <служащие> работают с девяти до шести.",
                            "target_lang_translations": {
                                "en": "Civil <w>servants</w> work from nine to six."
                            }
                        }
                    ],
                    "synonyms": [
                        "клерк"
                    ],
                    "antonyms": [
                        "работодатель",
                        "фрилансер",
                        "неслужащий"
                    ],
                    "common_phrases": [
                        "государственный служащий",
                        "банковский служащий",
                        "гражданский служащий"
                    ],
                    "target_lang_definitions": {
                        "en": "A person employed in a government institution, office, or private company, usually engaged in non-physical labor."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "employee",
                                "target_lang_sense_clarification": "general term for a worker"
                            },
                            {
                                "target_lang_word": "office worker",
                                "target_lang_sense_clarification": "specifically non-manual labor"
                            },
                            {
                                "target_lang_word": "clerk",
                                "target_lang_sense_clarification": "administrative or bank worker"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "41c0d1a3-358b-32fb-bcd3-e33426dee0b8",
                    "sense_definition": "Действительное причастие настоящего времени от глагола <w>служить</w>; исполняющий обязанности, работающий где-либо.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "action_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Офицер, верно <служащий> своей стране, получил медаль.",
                            "target_lang_translations": {
                                "en": "The officer faithfully <w>serving</w> his country received a medal."
                            }
                        },
                        {
                            "text": "Парень, <служащий> у нас водителем, сегодня заболел.",
                            "target_lang_translations": {
                                "en": "The guy <w>serving</w> as our driver got sick today."
                            }
                        }
                    ],
                    "common_phrases": [
                        "верно служащий",
                        "служащий в армии"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Present active participle of the verb 'to serve'; performing duties or working somewhere."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "serving",
                                "target_lang_sense_clarification": "performing a duty or working"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32c998c3-92fc-3a4c-a895-2314550617b4",
                    "sense_definition": "Предназначенный для выполнения определенной функции; используемый в качестве чего-либо.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "function_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это специальный инструмент, <служащий> для открытия замков.",
                            "target_lang_translations": {
                                "en": "This is a special tool <w>serving</w> to open locks."
                            }
                        },
                        {
                            "text": "Мы нашли камень, <служащий> опорой для старого моста.",
                            "target_lang_translations": {
                                "en": "We found a stone <w>serving</w> as a support for the old bridge."
                            }
                        }
                    ],
                    "synonyms": [
                        "предназначенный",
                        "используемый",
                        "применяемый"
                    ],
                    "antonyms": [
                        "непригодный"
                    ],
                    "common_phrases": [
                        "служащий для чего-либо",
                        "служащий доказательством",
                        "служащий примером"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Intended to perform a specific function; used as something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "serving",
                                "target_lang_sense_clarification": "functioning or acting as"
                            }
                        ]
                    }
                }
            ]
        }
    ]

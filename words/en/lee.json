{
    "word_family": [
        "leeward",
        "leeway"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "511e0b1c-13f1-3c81-b5cd-40a0b5750318",
                    "sense_definition": "A sheltered place, especially an area protected from the wind by an object; the side of something that is away from the wind.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "shelter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We pitched our tent in the <w>lee</w> of a large rock to escape the storm.",
                            "target_lang_translations": {
                                "ru": "Мы поставили палатку с <w>подветренной стороны</w> большой скалы, чтобы укрыться от шторма."
                            }
                        },
                        {
                            "text": "The small boat anchored in the <w>lee</w> of the island for the night.",
                            "target_lang_translations": {
                                "ru": "Маленькая лодка встала на якорь с <w>подветренной стороны</w> острова на ночь."
                            }
                        }
                    ],
                    "antonyms": [
                        "windward",
                        "weather"
                    ],
                    "common_phrases": [
                        "in the lee of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Укрытое место, защищенное от ветра каким-либо объектом; сторона, противоположная той, откуда дует ветер."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подветренная сторона",
                                "target_lang_sense_clarification": "сторона, защищенная от ветра"
                            },
                            {
                                "target_lang_word": "укрытие",
                                "target_lang_sense_clarification": "место, защищенное от непогоды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b67a4ba-71e5-3731-850c-26200230b0fb",
                    "sense_definition": "Sediment or dregs that settle at the bottom of a liquid, such as wine.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sediment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The winemaker carefully separated the clear liquid from the <w>lee</w>.",
                            "target_lang_translations": {
                                "ru": "Винодел осторожно отделил чистую жидкость от <w>осадка</w>."
                            }
                        },
                        {
                            "text": "Old resentments settled like <w>lee</w> at the bottom of his memory.",
                            "target_lang_translations": {
                                "ru": "Старые обиды осели, как <w>осадок</w>, на дне его памяти."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "usually plural: lees"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Осадок или гуща, оседающие на дне жидкости, например, вина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осадок",
                                "target_lang_sense_clarification": "твердые частицы на дне жидкости"
                            },
                            {
                                "target_lang_word": "гуща",
                                "target_lang_sense_clarification": "плотный осадок"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "13c007bd-4393-3f0e-9935-9431be8a53e6",
                    "sense_definition": "Facing away from the wind or the flow of a fluid.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moss tends to grow thicker on the <w>lee</w> side of the trees.",
                            "target_lang_translations": {
                                "ru": "Мох обычно растет гуще на <w>подветренной</w> стороне деревьев."
                            }
                        },
                        {
                            "text": "The captain ordered the crew to keep a close watch on the <w>lee</w> shore.",
                            "target_lang_translations": {
                                "ru": "Капитан приказал команде внимательно следить за <w>подветренным</w> берегом."
                            }
                        }
                    ],
                    "antonyms": [
                        "windward"
                    ],
                    "common_phrases": [
                        "lee side",
                        "lee shore"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся с той стороны, куда дует ветер; защищенный от ветра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подветренный",
                                "target_lang_sense_clarification": "расположенный с защищенной от ветра стороны"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "64b3d212-6d25-37d5-8c2d-a94e619e5f62",
                    "sense_definition": "A very common surname of English, Chinese, or Korean origin.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "surname",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Lee</w> has been our neighbor for over twenty years.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Ли</w> был нашим соседом более двадцати лет."
                            }
                        },
                        {
                            "text": "Bruce <w>Lee</w> is a legendary figure in martial arts cinema.",
                            "target_lang_translations": {
                                "ru": "Брюс <w>Ли</w> — легендарная фигура в кино о боевых искусствах."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень распространенная фамилия английского, китайского или корейского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ли",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7e9120a-26b4-3cab-bf86-bda32700601a",
                    "sense_definition": "A male or unisex given name.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "given_name",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My cousin <w>Lee</w> is coming to visit us this weekend.",
                            "target_lang_translations": {
                                "ru": "Мой кузен <w>Ли</w> приедет навестить нас в эти выходные."
                            }
                        },
                        {
                            "text": "<w>Lee</w> and Sarah went to the park to play tennis.",
                            "target_lang_translations": {
                                "ru": "<w>Ли</w> и Сара пошли в парк поиграть в теннис."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Мужское или унисекс имя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ли",
                                "target_lang_sense_clarification": "имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d8c2531-bbd1-3cd7-9c08-847d14edfebb",
                    "sense_definition": "A river in Ireland that flows through the city of Cork.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "river",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "The River <w>Lee</w> is a central feature of the landscape in Cork.",
                            "target_lang_translations": {
                                "ru": "Река <w>Ли</w> является центральной особенностью ландшафта в Корке."
                            }
                        },
                        {
                            "text": "Several historic bridges cross the <w>Lee</w> as it winds through the city.",
                            "target_lang_translations": {
                                "ru": "Несколько исторических мостов пересекают <w>Ли</w>, пока она петляет через город."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Река в Ирландии, протекающая через город Корк."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ли",
                                "target_lang_sense_clarification": "река"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea9b75b5-88d5-301d-843d-ddca6db481eb",
                    "sense_definition": "A name for various towns, villages, and places in English-speaking countries.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "place",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They stopped in the small town of <w>Lee</w> during their road trip.",
                            "target_lang_translations": {
                                "ru": "Они остановились в маленьком городке <w>Ли</w> во время своего автопутешествия."
                            }
                        },
                        {
                            "text": "He lives in <w>Lee</w>, which is a quiet suburb of London.",
                            "target_lang_translations": {
                                "ru": "Он живет в <w>Ли</w>, тихом пригороде Лондона."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Название различных городов, деревень и мест в англоязычных странах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Ли",
                                "target_lang_sense_clarification": "географическое название"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "muteness",
        "mutely",
        "unmute"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "7bdd5537-5d4d-3609-ae31-dca13c5ad14b",
                    "sense_definition": "To turn off the sound of an electronic device or to stop a person from being heard.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "silence_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>mute</w> your microphone when you are not speaking.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>выключите</w> микрофон, когда вы не говорите."
                            }
                        },
                        {
                            "text": "He <w>muted</w> the television to answer his phone.",
                            "target_lang_translations": {
                                "ru": "Он <w>выключил звук</w> телевизора, чтобы ответить на звонок."
                            }
                        }
                    ],
                    "antonyms": [
                        "unmute"
                    ],
                    "common_phrases": [
                        "mute the volume",
                        "mute the microphone",
                        "mute a call"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выключить звук электронного устройства или лишить человека возможности быть услышанным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выключать звук",
                                "target_lang_sense_clarification": "общее действие для устройств"
                            },
                            {
                                "target_lang_word": "отключать микрофон",
                                "target_lang_sense_clarification": "в контексте связи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9dbda138-8863-3afd-b780-027ea912c8ef",
                    "sense_definition": "To make something quiet or less intense.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "silence_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The thick curtains <w>muted</w> the noise from the street.",
                            "target_lang_translations": {
                                "ru": "Плотные шторы <w>приглушали</w> шум с улицы."
                            }
                        },
                        {
                            "text": "She tried to <w>mute</w> her laughter so the teacher would not hear.",
                            "target_lang_translations": {
                                "ru": "Она пыталась <w>приглушить</w> смех, чтобы учитель не услышал."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать что-то тихим или менее интенсивным; приглушить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приглушать",
                                "target_lang_sense_clarification": "делать тише или мягче"
                            },
                            {
                                "target_lang_word": "заглушать",
                                "target_lang_sense_clarification": "подавлять звук или чувство"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "352a91c9-90f8-3eb4-a208-e907d933b6a5",
                    "sense_definition": "Unable to speak, either from birth or due to injury or shock.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "speechless",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The child was born deaf and <w>mute</w>.",
                            "target_lang_translations": {
                                "ru": "Ребенок родился глухим и <w>немым</w>."
                            }
                        },
                        {
                            "text": "She stood <w>mute</w> with shock after hearing the bad news.",
                            "target_lang_translations": {
                                "ru": "Она стояла <w>немая</w> от шока, услышав плохие новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "voiceless",
                        "unspeaking",
                        "dumb"
                    ],
                    "antonyms": [
                        "talkative",
                        "vocal"
                    ],
                    "common_phrases": [
                        "deaf and mute",
                        "stand mute"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неспособный говорить от рождения, из-за травмы или сильного потрясения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "немой",
                                "target_lang_sense_clarification": "физически неспособный говорить"
                            },
                            {
                                "target_lang_word": "бессловесный",
                                "target_lang_sense_clarification": "лишенный дара речи"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8746dc2b-c521-39cb-8015-bb2843f19225",
                    "sense_definition": "Silent; not making any sound.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "speechless",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crowd remained <w>mute</w> as the ceremony began.",
                            "target_lang_translations": {
                                "ru": "Толпа оставалась <w>безмолвной</w>, когда началась церемония."
                            }
                        },
                        {
                            "text": "He gave a <w>mute</w> nod to show that he understood the instructions.",
                            "target_lang_translations": {
                                "ru": "Он сделал <w>молчаливый</w> кивок, чтобы показать, что понял инструкции."
                            }
                        }
                    ],
                    "synonyms": [
                        "aphonic",
                        "aphonous"
                    ],
                    "antonyms": [
                        "audible"
                    ],
                    "common_phrases": [
                        "mute appeal",
                        "mute witness"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тихий; не издающий ни звука."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безмолвный",
                                "target_lang_sense_clarification": "хранящий молчание"
                            },
                            {
                                "target_lang_word": "молчаливый",
                                "target_lang_sense_clarification": "неразговорчивый или тихий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd0945d1-1152-32ad-bb7a-4d0545538fd0",
                    "sense_definition": "Used to describe a letter in a word that is written but not pronounced.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "linguistic_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The letter 'b' in the word 'debt' is <w>mute</w>.",
                            "target_lang_translations": {
                                "ru": "Буква 'b' в слове 'debt' — <w>непроизносимая</w>."
                            }
                        },
                        {
                            "text": "English spelling contains many <w>mute</w> letters that confuse learners.",
                            "target_lang_translations": {
                                "ru": "Английская орфография содержит много <w>немых</w> букв, которые путают учащихся."
                            }
                        }
                    ],
                    "common_phrases": [
                        "mute letter",
                        "mute e"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для описания буквы в слове, которая пишется, но не произносится."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "непроизносимый",
                                "target_lang_sense_clarification": "о звуке или букве"
                            },
                            {
                                "target_lang_word": "немой",
                                "target_lang_sense_clarification": "лингвистический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b406eef9-7934-3353-a5c2-0cc5c150ad37",
                    "sense_definition": "A control or switch on an electronic device that turns off the sound.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "silence_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I think you are on <w>mute</w> because we cannot hear you.",
                            "target_lang_translations": {
                                "ru": "Я думаю, вы на <w>беззвучном</w>, потому что мы вас не слышим."
                            }
                        },
                        {
                            "text": "He pressed the <w>mute</w> on the remote control instantly.",
                            "target_lang_translations": {
                                "ru": "Он мгновенно нажал <w>кнопку выключения звука</w> на пульте."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on mute",
                        "mute button"
                    ],
                    "target_lang_definitions": {
                        "ru": "Элемент управления или режим на электронном устройстве, отключающий звук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беззвучный режим",
                                "target_lang_sense_clarification": "состояние устройства"
                            },
                            {
                                "target_lang_word": "выключение звука",
                                "target_lang_sense_clarification": "функция или кнопка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b033f58a-ca77-3743-b915-91c02946f784",
                    "sense_definition": "A device used to soften or change the tone of a musical instrument.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "music_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The trumpet player inserted a <w>mute</w> to create a jazzy sound.",
                            "target_lang_translations": {
                                "ru": "Трубач вставил <w>сурдину</w>, чтобы создать джазовое звучание."
                            }
                        },
                        {
                            "text": "She used a practice <w>mute</w> on her violin to avoid disturbing the neighbors.",
                            "target_lang_translations": {
                                "ru": "Она использовала тренировочную <w>сурдину</w> на скрипке, чтобы не беспокоить соседей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "trumpet mute",
                        "practice mute"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устройство, используемое для смягчения или изменения тембра музыкального инструмента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сурдина",
                                "target_lang_sense_clarification": "музыкальное приспособление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45939b57-bb07-37ab-89f9-9250b8d07d45",
                    "sense_definition": "A person who does not have the power of speech.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The main character in the novel is a <w>mute</w> who communicates through writing.",
                            "target_lang_translations": {
                                "ru": "Главный герой романа — <w>немой</w>, который общается письменно."
                            }
                        },
                        {
                            "text": "He worked as a <w>mute</w> servant in the royal palace.",
                            "target_lang_translations": {
                                "ru": "Он работал <w>немым</w> слугой в королевском дворце."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "Modern usage often prefers terms like 'non-verbal' or 'speech impaired'."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, не обладающий даром речи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "немой",
                                "target_lang_sense_clarification": "существительное"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
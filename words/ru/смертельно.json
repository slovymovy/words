                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "fatal",
                                "target_lang_sense_clarification": "causing death"
                            },
                            {
                                "target_lang_word": "deadly",
                                "target_lang_sense_clarification": "capable of causing death"
                            }
                        ]
                    }
                }
            ]
        }
    ]

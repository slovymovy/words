            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5b7faa81-d772-35b6-9234-2399fa5a058a",
                    "sense_definition": "De toestand van geestelijke verwarring, waanzin of dwaasheid.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is pure <w>gekte</w> om met dit slechte weer de zee op te gaan.",
                            "target_lang_translations": {
                                "en": "It is pure <w>madness</w> to go out to sea in this bad weather.",
                                "ru": "Это чистое <w>безумие</w> — выходить в море в такую плохую погоду."
                            }
                        },
                        {
                            "text": "Zijn plan grensde aan totale <w>gekte</w>, maar toch lukte het hem.",
                            "target_lang_translations": {
                                "en": "His plan bordered on total <w>insanity</w>, but he succeeded anyway.",
                                "ru": "Его план граничил с полным <w>сумасшествием</w>, но всё же ему это удалось."
                            }
                        }
                    ],
                    "synonyms": [
                        "waanzin",
                        "dwaasheid",
                        "gekheid",
                        "gekkigheid",
                        "mafheid",
                        "leipheid"
                    ],
                    "antonyms": [
                        "wijsheid",
                        "verstand"
                    ],
                    "common_phrases": [
                        "pure gekte",
                        "totale gekte"
                    ],
                    "target_lang_definitions": {
                        "en": "The state of mental confusion, madness, or folly.",
                        "ru": "Состояние душевного расстройства, безумие или крайняя глупость."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "madness",
                                "target_lang_sense_clarification": "state of being mentally ill or extremely foolish"
                            },
                            {
                                "target_lang_word": "insanity",
                                "target_lang_sense_clarification": "severe mental illness or extreme foolishness"
                            },
                            {
                                "target_lang_word": "folly",
                                "target_lang_sense_clarification": "lack of good sense; foolishness"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "безумие",
                                "target_lang_sense_clarification": "состояние психики или крайне неразумный поступок"
                            },
                            {
                                "target_lang_word": "сумасшествие",
                                "target_lang_sense_clarification": "потеря рассудка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6b518ce5-98bf-38b5-a3cd-99767a037f4f",
                    "sense_definition": "Een situatie van overdreven drukte, chaos of opwinding.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "situation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De volgende ochtend verliet ik in alle vroegte de <w>gekte</w> van de stad.",
                            "target_lang_translations": {
                                "en": "The next morning, I left the <w>madness</w> of the city very early.",
                                "ru": "На следующее утро я рано покинул городскую <w>суматоху</w>."
                            }
                        },
                        {
                            "text": "Tijdens de uitverkoop heerste er een enorme <w>gekte</w> in de winkelstraat.",
                            "target_lang_translations": {
                                "en": "During the sale, there was a huge <w>frenzy</w> in the shopping street.",
                                "ru": "Во время распродажи на торговой улице царило настоящее <w>безумие</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dwaasheid",
                        "waanzin"
                    ],
                    "antonyms": [
                        "rust",
                        "kalmte"
                    ],
                    "common_phrases": [
                        "de gekte van de dag"
                    ],
                    "target_lang_definitions": {
                        "en": "A situation of excessive busyness, chaos, or excitement.",
                        "ru": "Ситуация чрезмерной суеты, хаоса или возбуждения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "madness",
                                "target_lang_sense_clarification": "chaotic or frenetic activity"
                            },
                            {
                                "target_lang_word": "frenzy",
                                "target_lang_sense_clarification": "wild excitement or uncontrolled activity"
                            },
                            {
                                "target_lang_word": "craziness",
                                "target_lang_sense_clarification": "informal; a chaotic situation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "суматоха",

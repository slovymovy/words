    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "e147548d-9a0d-38b9-9bf1-e8ef9244018d",
                    "sense_definition": "To look at and understand the meaning of written or printed words or symbols.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "decode_text",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I like to <w>read</w> a book before going to sleep.",
                            "target_lang_translations": {
                                "ru": "Я люблю <w>читать</w> книгу перед сном."
                            }
                        },
                        {
                            "text": "Can you <w>read</w> the sign from this distance?",
                            "target_lang_translations": {
                                "ru": "Ты можешь <w>прочитать</w> вывеску с этого расстояния?"
                            }
                        }
                    ],
                    "synonyms": [
                        "understand",
                        "make out",
                        "scan"
                    ],
                    "common_phrases": [
                        "read a book",
                        "read the news",
                        "read carefully"
                    ],
                    "target_lang_definitions": {
                        "ru": "Смотреть на написанные или напечатанные слова или символы и понимать их значение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "читать",
                                "target_lang_sense_clarification": "процесс чтения"
                            },
                            {
                                "target_lang_word": "прочитать",
                                "target_lang_sense_clarification": "законченное действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ce4aad7c-d0ba-39af-8a53-18d1ca77ea3d",
                    "sense_definition": "To speak written words aloud so that others can hear them.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "decode_text",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher asked the student to <w>read</w> the story to the class.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил ученика <w>прочитать</w> рассказ классу."
                            }
                        },
                        {
                            "text": "Please <w>read</w> out loud so everyone can hear you.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>читай</w> вслух, чтобы все могли тебя слышать."
                            }
                        }
                    ],
                    "synonyms": [
                        "read aloud",
                        "read out",
                        "speak"
                    ],
                    "common_phrases": [
                        "read out loud",
                        "read to someone",
                        "read a story"
                    ],
                    "target_lang_definitions": {
                        "ru": "Произносить написанные слова вслух, чтобы их могли услышать другие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "читать",
                                "target_lang_sense_clarification": "вслух"
                            },
                            {
                                "target_lang_word": "зачитывать",
                                "target_lang_sense_clarification": "оглашать написанное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ed2ac69-bea6-3f34-9649-f35d69dd73fe",
                    "sense_definition": "To consist of specific words or text; to convey a specific message in writing.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "text_content",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sign on the door <w>reads</w> \"Do Not Enter\".",
                            "target_lang_translations": {
                                "ru": "Табличка на двери <w>гласит</w>: «Не входить»."
                            }
                        },
                        {
                            "text": "The label <w>reads</w> as if it were written in a hurry.",
                            "target_lang_translations": {
                                "ru": "Надпись на этикетке <w>читается</w> так, будто она была сделана в спешке."
                            }
                        }
                    ],
                    "synonyms": [
                        "say",
                        "run"
                    ],
                    "common_phrases": [
                        "the sign reads",
                        "the label reads",
                        "the message reads"
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержать определенные слова или текст; передавать определенное сообщение в письменном виде."

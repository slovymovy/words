                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pipe nipple",
                                "target_lang_sense_clarification": "plumbing fitting"
                            },
                            {
                                "target_lang_word": "nipple",
                                "target_lang_sense_clarification": "short threaded pipe"
                            }
                        ]
                    }
                },
                {

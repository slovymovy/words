{
    "word_family": [
        "disrupt",
        "disruptive",
        "disruptively",
        "disruptor"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "eede226f-649e-3433-9f27-31268fdaf727",
                    "sense_definition": "An interruption to the regular flow or sequence of something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "interruption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The strike caused a major <w>disruption</w> to train services across the country.",
                            "target_lang_translations": {
                                "ru": "Забастовка привела к серьезному <w>сбою</w> в железнодорожном сообщении по всей стране."
                            }
                        },
                        {
                            "text": "We apologize for the service <w>disruption</w> caused by the severe weather conditions.",
                            "target_lang_translations": {
                                "ru": "Мы приносим извинения за <w>перебои</w> в обслуживании, вызванные суровыми погодными условиями."
                            }
                        }
                    ],
                    "synonyms": [
                        "interruption",
                        "disturbance",
                        "stoppage"
                    ],
                    "antonyms": [
                        "continuity",
                        "continuation"
                    ],
                    "common_phrases": [
                        "cause disruption",
                        "major disruption",
                        "travel disruption",
                        "service disruption",
                        "minimize disruption"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нарушение нормального хода или последовательности чего-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбой",
                                "target_lang_sense_clarification": "в работе систем, графике или обслуживании"
                            },
                            {
                                "target_lang_word": "нарушение",
                                "target_lang_sense_clarification": "порядка, нормального течения дел"
                            },
                            {
                                "target_lang_word": "срыв",
                                "target_lang_sense_clarification": "планов, сроков, договоренностей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "207a5b59-762c-3d4a-94fd-beb58e64a53b",
                    "sense_definition": "A continuing act of disorder or confusion.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "disorder",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There was great <w>disruption</w> in the classroom when the teacher left the room.",
                            "target_lang_translations": {
                                "ru": "Когда учитель вышел из комнаты, в классе начался сильный <w>беспорядок</w>."
                            }
                        },
                        {
                            "text": "The loud construction work created a constant <w>disruption</w> for the office workers.",
                            "target_lang_translations": {
                                "ru": "Громкие строительные работы создавали постоянную <w>помеху</w> для офисных сотрудников."
                            }
                        }
                    ],
                    "synonyms": [
                        "disorder",
                        "turmoil",
                        "chaos",
                        "commotion"
                    ],
                    "antonyms": [
                        "order",
                        "peace",
                        "calm"
                    ],
                    "common_phrases": [
                        "social disruption",
                        "political disruption",
                        "create disruption"
                    ],
                    "target_lang_definitions": {
                        "ru": "Продолжающееся состояние беспорядка или неразберихи"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "беспорядок",
                                "target_lang_sense_clarification": "отсутствие порядка, хаос"
                            },
                            {
                                "target_lang_word": "дезорганизация",
                                "target_lang_sense_clarification": "нарушение организованности, развал"
                            },
                            {
                                "target_lang_word": "помеха",
                                "target_lang_sense_clarification": "то, что мешает действию или процессу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bfbe05f0-ed63-3cf0-ba3d-e991080bd585",
                    "sense_definition": "A breaking or bursting apart; a breach.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_break",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The sudden <w>disruption</w> of the vessel's hull caused it to sink rapidly.",
                            "target_lang_translations": {
                                "ru": "Внезапное <w>разрушение</w> корпуса судна привело к тому, что оно быстро затонуло."
                            }
                        },
                        {
                            "text": "Geologists studied the <w>disruption</w> of the rock layers caused by the earthquake.",
                            "target_lang_translations": {
                                "ru": "Геологи изучали <w>разрыв</w> пластов горных пород, вызванный землетрясением."
                            }
                        }
                    ],
                    "synonyms": [
                        "rupture",
                        "breach",
                        "break"
                    ],
                    "antonyms": [
                        "union",
                        "connection"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разламывание или разрыв на части; пролом"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разрыв",
                                "target_lang_sense_clarification": "нарушение целостности, разделение на части"
                            },
                            {
                                "target_lang_word": "разрушение",
                                "target_lang_sense_clarification": "повреждение структуры"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
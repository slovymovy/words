    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "27729568-9bbd-320b-8f21-2a40b285d800",
                    "sense_definition": "Совершаясь по принуждению, против собственного желания; не добровольно.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он <вынужденно> согласился на их условия, так как другого выхода просто не было.",
                            "target_lang_translations": {
                                "en": "He agreed to their terms <under duress>, as there was simply no other way out."
                            }
                        },
                        {
                            "text": "Многие жители <вынужденно> покинули свои дома из-за стихийного бедствия.",
                            "target_lang_translations": {
                                "en": "Many residents left their homes <involuntarily> due to the natural disaster."
                            }
                        }
                    ],
                    "synonyms": [
                        "принудительно",
                        "насильно",
                        "поневоле"
                    ],
                    "antonyms": [
                        "добровольно",
                        "охотно"
                    ],
                    "common_phrases": [
                        "вынужденно согласиться",
                        "действовать вынужденно",
                        "вынужденно признать"
                    ],
                    "target_lang_definitions": {
                        "en": "Done under coercion or against one's own desire; not voluntarily."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "involuntarily",
                                "target_lang_sense_clarification": "not done of one's own free will"
                            },
                            {
                                "target_lang_word": "under duress",
                                "target_lang_sense_clarification": "under pressure or threat"
                            },
                            {
                                "target_lang_word": "forcedly",
                                "target_lang_sense_clarification": "in a forced or constrained manner"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95072964-c71e-316b-9c00-4021e641d214",
                    "sense_definition": "Будучи вызванным какими-либо внешними условиями или обстоятельствами; в силу необходимости.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за сильного снегопада мы <вынужденно> задержались в пути на несколько часов.",
                            "target_lang_translations": {
                                "en": "Due to heavy snowfall, we were <unavoidably> delayed on the road for several hours."
                            }
                        },
                        {
                            "text": "«Да, у нас возникли небольшие трудности», — <вынужденно> объяснил он причину опоздания.",
                            "target_lang_translations": {
                                "en": "\"Yes, we had some minor difficulties,\" he explained <out of necessity> regarding the delay."
                            }
                        }
                    ],
                    "synonyms": [
                        "по необходимости",
                        "в силу обстоятельств"
                    ],
                    "common_phrases": [
                        "вынужденно остановиться",
                        "вынужденно бездействовать"
                    ],
                    "target_lang_definitions": {
                        "en": "Caused by external conditions or circumstances; due to necessity."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "of necessity",
                                "target_lang_sense_clarification": "because circumstances demand it"
                            },
                            {
                                "target_lang_word": "unavoidably",
                                "target_lang_sense_clarification": "impossible to avoid due to conditions"
                            },
                            {
                                "target_lang_word": "perforce",
                                "target_lang_sense_clarification": "by force of circumstances (formal)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
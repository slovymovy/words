            "senses": [
                {
                    "sense_id": "89812b4b-0fd7-362d-9fab-4aeaf0be18c7",
                    "sense_definition": "To put forward something in opposition or to oppose an action.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The general decided to <w>obtend</w> a strong defense against the invading army.",
                            "target_lang_translations": {
                                "ru": "Генерал решил <w>противопоставить</w> вторгшейся армии мощную оборону."
                            }
                        },
                        {
                            "text": "They attempted to <w>obtend</w> their rights against the unjust seizure of property.",
                            "target_lang_translations": {
                                "ru": "Они попытались <w>выдвинуть</w> свои права против несправедливого изъятия собственности."
                            }
                        }
                    ],
                    "synonyms": [
                        "oppose",
                        "resist"
                    ],
                    "antonyms": [
                        "yield",
                        "submit"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "This word is rarely used in modern English."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выдвигать что-либо в качестве возражения или для противодействия действию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "противопоставлять",
                                "target_lang_sense_clarification": "выставлять против чего-либо"
                            },
                            {
                                "target_lang_word": "оказывать сопротивление",
                                "target_lang_sense_clarification": "противодействовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "135a8f82-f0b3-3a00-a927-9000236d268f",
                    "sense_definition": "To offer something as a reason, excuse, or pretext for an action.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He tried to <w>obtend</w> his sudden illness as the reason for his absence.",
                            "target_lang_translations": {
                                "ru": "Он попытался <w>сослаться на</w> свою внезапную болезнь как на причину отсутствия."
                            }
                        },
                        {
                            "text": "The suspect sought to <w>obtend</w> a false alibi to avoid prosecution.",
                            "target_lang_translations": {
                                "ru": "Подозреваемый пытался <w>представить</w> ложное алиби, чтобы избежать судебного преследования."
                            }
                        }
                    ],
                    "synonyms": [
                        "allege",
                        "plead"
                    ],
                    "traits": [

            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "81a015e2-0935-3507-a2e8-32a4fe39616e",
                    "sense_definition": "Secured by a lock so that it cannot be opened.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "secured_restricted",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A <w>locked</w> door prevented us from entering the empty house.",
                            "target_lang_translations": {
                                "ru": "<w>Запертая</w> дверь помешала нам войти в пустой дом."
                            }
                        },
                        {
                            "text": "She keeps her valuable jewelry in a <w>locked</w> box.",
                            "target_lang_translations": {
                                "ru": "Она хранит свои драгоценности в <w>запертой</w> шкатулке."
                            }
                        }
                    ],
                    "antonyms": [
                        "unlocked"
                    ],
                    "common_phrases": [
                        "locked door",
                        "locked room",
                        "locked gate",
                        "locked out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запертый на замок или закрытый так, что его нельзя открыть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "запертый",
                                "target_lang_sense_clarification": "на ключ"
                            },
                            {
                                "target_lang_word": "закрытый",
                                "target_lang_sense_clarification": "на замок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "844ec9b5-e3e5-32a5-bb2f-3403445dc07a",
                    "sense_definition": "Protected by a digital restriction or password that prevents access to a computer resource or file.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "secured_restricted",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You cannot edit or delete a <w>locked</w> computer file without permission.",
                            "target_lang_translations": {
                                "ru": "Вы не можете редактировать или удалять <w>заблокированный</w> компьютерный файл без разрешения."
                            }
                        },
                        {
                            "text": "The system administrator reset the password for the <w>locked</w> account.",
                            "target_lang_translations": {
                                "ru": "Системный администратор сбросил пароль для <w>заблокированной</w> учетной записи."
                            }
                        }
                    ],
                    "antonyms": [
                        "unlocked"
                    ],
                    "common_phrases": [
                        "locked file",
                        "locked screen",
                        "locked account"
                    ],
                    "target_lang_definitions": {
                        "ru": "Защищенный цифровым ограничением или паролем, предотвращающим доступ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заблокированный",
                                "target_lang_sense_clarification": "о доступе или файле"
                            },
                            {
                                "target_lang_word": "запароленный",
                                "target_lang_sense_clarification": "защищенный паролем"
                            }
                        ]
                    }
                },
                {

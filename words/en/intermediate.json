                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "промежуточный",
                                "target_lang_sense_clarification": "находящийся между этапами"
                            },
                            {
                                "target_lang_word": "средний",
                                "target_lang_sense_clarification": "об уровне или качестве"
                            }

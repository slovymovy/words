{
    "word_family": [],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "c188af51-2219-3c63-ab5f-021af6607167",
                    "sense_definition": "Wskazuje na miejsce znajdujące się po tylnej stronie kogoś lub czegoś.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location_static",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój kot często śpi <za> kanapą w salonie.",
                            "target_lang_translations": {
                                "ru": "Мой кот часто спит <за> диваном в гостиной."
                            }
                        },
                        {
                            "text": "Dzieci schowały się <za> murem, żeby ich nie znaleziono.",
                            "target_lang_translations": {
                                "ru": "Дети спрятались <за> стеной, чтобы их не нашли."
                            }
                        }
                    ],
                    "antonyms": [
                        "przed"
                    ],
                    "common_phrases": [
                        "za domem",
                        "za drzwiami",
                        "za rogiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на место, находящееся с задней стороны кого-либо или чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "предлог места (с творительным падежом): позади, сзади"
                            },
                            {
                                "target_lang_word": "позади",
                                "target_lang_sense_clarification": "наречие или предлог: сзади"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b4ac76f-a4ea-3829-b066-3038e873f637",
                    "sense_definition": "Wskazuje na odcinek czasu, po upływie którego coś ma nastąpić.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "time_future",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pociąg do Warszawy odjeżdża <za> dziesięć minut.",
                            "target_lang_translations": {
                                "ru": "Поезд в Варшаву отправляется <через> десять минут."
                            }
                        },
                        {
                            "text": "<Za> miesiąc będę miał dwadzieścia pięć lat.",
                            "target_lang_translations": {
                                "ru": "<Через> месяц мне исполнится двадцать пять лет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "za chwilę",
                        "za moment",
                        "za godzinę",
                        "za rok"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на отрезок времени, по истечении которого что-то должно произойти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "через",
                                "target_lang_sense_clarification": "предлог времени: спустя какой-то срок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b243b235-d16e-3b7b-b459-2b2ecc6367e8",
                    "sense_definition": "Wskazuje na to, co jest przedmiotem wymiany, zapłaty lub powodem wdzięczności.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "exchange_gratitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jestem ci bardzo wdzięczny <za> twoją pomoc.",
                            "target_lang_translations": {
                                "ru": "Я очень благодарен тебе <за> твою помощь."
                            }
                        },
                        {
                            "text": "Ile zapłaciłeś <za> te nowe buty?",
                            "target_lang_translations": {
                                "ru": "Сколько ты заплатил <за> эти новые туфли?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "dziękować za",
                        "płacić za",
                        "przepraszać za"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на предмет обмена, оплаты или причину благодарности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "предлог причины или обмена (с винительным падежом)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7e7fe33-9267-3bcc-a631-6ec3c53e2f82",
                    "sense_definition": "Wskazuje na ruch skierowany na drugą stronę czegoś lub w kierunku tyłu czegoś.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W te wakacje wyjeżdżamy <za> granicę na kilka dni.",
                            "target_lang_translations": {
                                "ru": "В эти каникулы мы выезжаем <за> границу на несколько дней."
                            }
                        },
                        {
                            "text": "Pies wybiegł <za> bramę i zaczął szczekać.",
                            "target_lang_translations": {
                                "ru": "Собака выбежала <за> ворота и начала лаять."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wyjechać za granicę",
                        "iść za mąż",
                        "wyrzucić za drzwi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на движение, направленное на другую сторону чего-либо или в сторону задней части чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "предлог направления (с винительным падежом): по ту сторону, за пределы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf75b2c9-a531-37e8-9a26-6de9e0c20473",
                    "sense_definition": "Wskazuje na występowanie kogoś lub czegoś później w kolejności (w przestrzeni lub czasie).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijał dzień <za> dniem, a on wciąż nie wracał.",
                            "target_lang_translations": {
                                "ru": "Проходил день <за> днём, а он всё не возвращался."
                            }
                        },
                        {
                            "text": "Proszę powtarzać słowa jedno <za> drugim.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, повторяйте слова одно <за> другим."
                            }
                        }
                    ],
                    "synonyms": [
                        "po"
                    ],
                    "antonyms": [
                        "przed"
                    ],
                    "common_phrases": [
                        "krok za krokiem",
                        "jeden za drugim",
                        "raz za razem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на следование кого-либо или чего-либо позже в очередности (в пространстве или времени)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "предлог последовательности: следом, после"
                            },
                            {
                                "target_lang_word": "вслед за",
                                "target_lang_sense_clarification": "указывает на следование непосредственно после"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b58cac5b-0c8b-3a32-ba48-2ac0f6bf5f56",
                    "sense_definition": "Wskazuje na popieranie kogoś lub czegoś; bycie zwolennikiem.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "support",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy pracownicy są <za> nowym pomysłem dyrektora.",
                            "target_lang_translations": {
                                "ru": "Все сотрудники <за> новую идею директора."
                            }
                        },
                        {
                            "text": "Zagłosowałem <za> przyjęciem tej ustawy w parlamencie.",
                            "target_lang_translations": {
                                "ru": "Я проголосовал <за> принятие этого закона в парламенте."
                            }
                        }
                    ],
                    "common_phrases": [
                        "być za",
                        "głosować za"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на поддержку кого-либо или чего-либо; бытие сторонником."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "выражение поддержки или согласия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8232db12-ca75-3de2-a2b0-791650e077f1",
                    "sense_definition": "Wskazuje na część ciała lub przedmiotu, która jest chwytana lub trzymana.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "contact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Matka trzymała małe dziecko <za> rękę podczas spaceru.",
                            "target_lang_translations": {
                                "ru": "Мать держала маленького ребенка <за> руку во время прогулки."
                            }
                        },
                        {
                            "text": "Złapał go <za> kołnierz i wyrzucił z pokoju.",
                            "target_lang_translations": {
                                "ru": "Он схватил его <за> воротник и вышвырнул из комнаты."
                            }
                        }
                    ],
                    "common_phrases": [
                        "trzymać za rękę",
                        "ciągnąć za język",
                        "złapać za słuchawkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на часть тела или предмета, которую хватают или держат."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "указывает на часть объекта, за которую держат"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2addd78-1dc5-38e4-8618-e9b3b221f783",
                    "sense_definition": "Wskazuje na przyczynę jakiegoś działania, stanu lub kary.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dostał wysoki mandat <za> szybką jazdę samochodem.",
                            "target_lang_translations": {
                                "ru": "Он получил большой штраф <за> быструю езду на машине."
                            }
                        },
                        {
                            "text": "<Za> karę nie wolno ci dzisiaj oglądać telewizji.",
                            "target_lang_translations": {
                                "ru": "<В> наказание тебе сегодня нельзя смотреть телевизор."
                            }
                        }
                    ],
                    "common_phrases": [
                        "za karę",
                        "za zasługi",
                        "za niewinność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на причину какого-либо действия, состояния или наказания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "за",
                                "target_lang_sense_clarification": "указывает на причину или основание (с винительным падежом)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc88ca4c-8f9d-37f6-bada-a60aeb91776a",
                    "sense_definition": "Wskazuje na okres w przeszłości, w którym coś się działo (często w odniesieniu do panowania władców lub minionych epok).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "time_past",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To piękne miasto zostało zbudowane <za> króla Kazimierza.",
                            "target_lang_translations": {
                                "ru": "Этот красивый город был построен <при> короле Казимире."
                            }
                        },
                        {
                            "text": "<Za> starych czasów ludzie żyli znacznie spokojniej.",
                            "target_lang_translations": {
                                "ru": "<В> старые времена люди жили гораздо спокойнее."
                            }
                        }
                    ],
                    "common_phrases": [
                        "za dawnych czasów",
                        "za króla",
                        "za młodu"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "używane głównie w kontekście historycznym lub wspomnieniowym"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Указывает на период в прошлом, когда что-то происходило (часто в отношении правления правителей или минувших эпох)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "при",
                                "target_lang_sense_clarification": "указывает на эпоху или время правления"
                            },
                            {
                                "target_lang_word": "во времена",
                                "target_lang_sense_clarification": "в период существования чего-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "article",
            "senses": [
                {
                    "sense_id": "760a8fb9-b49f-34c5-8032-122579689105",
                    "sense_definition": "Słowo oznaczające, że czegoś jest więcej niż potrzeba lub niż jest to pożądane; wskazuje na nadmiar cechy.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety ta koszula jest dla mnie <za> mała.",
                            "target_lang_translations": {
                                "ru": "К сожалению, эта рубашка мне <слишком> мала."
                            }
                        },
                        {
                            "text": "Nie idź tak szybko, bo dla mnie to <za> duże tempo.",
                            "target_lang_translations": {
                                "ru": "Не иди так быстро, для меня это <слишком> высокий темп."
                            }
                        }
                    ],
                    "synonyms": [
                        "zbyt"
                    ],
                    "common_phrases": [
                        "za dużo",
                        "za bardzo",
                        "za mało",
                        "za drogo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, означающее, что чего-то больше, чем нужно или желательно; указывает на избыток качества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слишком",
                                "target_lang_sense_clarification": "наречие меры и степени: чересчур"
                            },
                            {
                                "target_lang_word": "чересчур",
                                "target_lang_sense_clarification": "излишне, более чем нужно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b2738afe-f841-3a98-bde0-f9d4fd31b040",
                    "sense_definition": "Skrót od: Zespół Aspergera (całościowe zaburzenie rozwoju mieszczące się w spektrum autyzmu).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "medical_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie jest prawdą, że osoby z <ZA> cechuje zupełny brak poczucia humoru.",
                            "target_lang_translations": {
                                "ru": "Неправда, что людей с <синдромом Аспергера> отличает полное отсутствие чувства юмора."
                            }
                        },
                        {
                            "text": "Wczesna diagnoza <ZA> pomaga w lepszym dostosowaniu terapii.",
                            "target_lang_translations": {
                                "ru": "Ранняя диагностика <синдрома Аспергера> помогает лучше подобрать терапию."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от: Синдром Аспергера (общее расстройство развития, входящее в спектр аутизма)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "синдром Аспергера",
                                "target_lang_sense_clarification": "медицинский диагноз"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
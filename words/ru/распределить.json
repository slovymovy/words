    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5e350441-1ac6-35f6-a923-73c561021dd4",
                    "sense_definition": "Разделить что-либо на части и раздать их разным людям или направить в разные места.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "distribution_sharing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Начальник отдела должен грамотно <w>распределить</w> обязанности между сотрудниками.",
                            "target_lang_translations": {
                                "en": "The department head must competently <w>distribute</w> duties among the employees."
                            }
                        },
                        {
                            "text": "Мы решили <w>распределить</w> полученную прибыль поровну между всеми партнёрами.",
                            "target_lang_translations": {
                                "en": "We decided to <w>distribute</w> the profit received equally among all partners."
                            }
                        }
                    ],
                    "synonyms": [
                        "раздать",
                        "поделить",
                        "разделить"
                    ],
                    "antonyms": [
                        "собрать",
                        "объединить"
                    ],
                    "common_phrases": [
                        "распределить обязанности",
                        "распределить бюджет",
                        "распределить нагрузку",
                        "распределить ресурсы"
                    ],
                    "target_lang_definitions": {
                        "en": "To divide something into parts and give them to different people or send them to different places."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "distribute",
                                "target_lang_sense_clarification": "general meaning: to give shares of something"
                            },
                            {
                                "target_lang_word": "allocate",
                                "target_lang_sense_clarification": "to set apart for a specific purpose or person"
                            },
                            {
                                "target_lang_word": "assign",
                                "target_lang_sense_clarification": "to give out tasks or duties"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fbb33f93-13a1-3bd8-9008-4b4be8f7952a",
                    "sense_definition": "Спланировать использование ограниченных ресурсов (времени, сил, внимания) для достижения цели.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "distribution_planning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студенту очень важно правильно <w>распределить</w> время на экзамене, чтобы успеть ответить на все вопросы.",
                            "target_lang_translations": {
                                "en": "It is very important for a student to correctly <w>allocate</w> time during the exam to answer all questions in time."
                            }
                        },
                        {
                            "text": "Опытный марафонец умеет <w>распределить</w> силы на всю дистанцию.",
                            "target_lang_translations": {
                                "en": "An experienced marathon runner knows how to <w>manage</w> their energy over the entire distance."
                            }
                        }
                    ],
                    "synonyms": [
                        "спланировать",
                        "организовать"
                    ],
                    "common_phrases": [
                        "распределить время",
                        "распределить силы",
                        "распределить внимание"
                    ],
                    "target_lang_definitions": {
                        "en": "To plan the use of limited resources (time, energy, attention) to achieve a goal."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "allocate",
                                "target_lang_sense_clarification": "to designate time or resources for a task"
                            },
                            {
                                "target_lang_word": "budget",
                                "target_lang_sense_clarification": "to plan the expenditure of time or money"
                            },
                            {
                                "target_lang_word": "manage",
                                "target_lang_sense_clarification": "to handle resources or strength efficiently"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9605cd41-5338-337f-8dcf-8abae2d19ad2",
                    "sense_definition": "Разместить кого-либо или что-либо в определённом порядке, рассортировать по группам или категориям.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "distribution_sorting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учитель физкультуры помог <w>распределить</w> учеников по двум командам.",
                            "target_lang_translations": {
                                "en": "The PE teacher helped <w>divide</w> the students into two teams."
                            }
                        },
                        {
                            "text": "Все полученные данные необходимо <w>распределить</w> по соответствующим папкам.",
                            "target_lang_translations": {
                                "en": "All received data must be <w>sorted</w> into the appropriate folders."
                            }
                        }
                    ],
                    "synonyms": [
                        "рассортировать",
                        "разместить",
                        "расставить",
                        "сгруппировать"
                    ],
                    "antonyms": [
                        "смешать"
                    ],
                    "common_phrases": [
                        "распределить по группам",
                        "распределить по категориям",
                        "распределить по порядку"
                    ],
                    "target_lang_definitions": {
                        "en": "To place someone or something in a specific order, or sort into groups or categories."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sort",
                                "target_lang_sense_clarification": "to arrange according to class or category"
                            },
                            {
                                "target_lang_word": "categorize",
                                "target_lang_sense_clarification": "to place into categories"
                            },
                            {
                                "target_lang_word": "group",
                                "target_lang_sense_clarification": "to put into groups"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e13309de-f12f-3efe-9239-13bdaf7ab90a",
                    "sense_definition": "Официально назначить выпускника учебного заведения на конкретное место работы.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "distribution_assignment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "После окончания медицинского института молодого врача могут <w>распределить</w> в сельскую больницу.",
                            "target_lang_translations": {
                                "en": "After graduating from medical school, a young doctor can be <w>assigned</w> to a rural hospital."
                            }
                        },
                        {
                            "text": "Комиссия решила <w>распределить</w> его на завод в другом городе.",
                            "target_lang_translations": {
                                "en": "The commission decided to <w>assign</w> him to a factory in another city."
                            }
                        }
                    ],
                    "synonyms": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "520761e1-6f6c-329b-b76c-ce8cafec0181",
                    "sense_definition": "Быть признанным кем-либо, восприниматься в каком-либо качестве или иметь определённую репутацию.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "reputation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В нашей культуре опоздание <считается> проявлением неуважения."
                        },
                        {
                            "text": "Этот роман по праву <считается> лучшим произведением автора."
                        }
                    ],
                    "synonyms": [
                        "слыть",
                        "являться"
                    ],
                    "common_phrases": [
                        "считаться лучшим",
                        "считаться погибшим",
                        "считаться правильным"
                    ]
                },
                {
                    "sense_id": "c843e422-e1b8-31a9-84c0-66bd50dc1074",
                    "sense_definition": "Принимать во внимание, уважать чьё-либо мнение, желания или сложившиеся обстоятельства.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "respect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Тебе придётся <считаться> с моим решением, нравится оно тебе или нет."
                        },
                        {
                            "text": "Мы должны <считаться> с возможностью дождя и взять с собой зонты."
                        }
                    ],
                    "common_phrases": [
                        "считаться с мнением",
                        "считаться с обстоятельствами",
                        "считаться с фактами"
                    ]
                },
                {
                    "sense_id": "4e17c2ef-71f7-37cd-ac94-4f3e670a6792",
                    "sense_definition": "Быть таким, который можно или нужно сосчитать; идти в зачёт (быть действительным).",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "counting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот гол не <считается>, потому что судья уже остановил игру."
                        },
                        {
                            "text": "Цыплята по осени <считаются>, поэтому рано делать выводы."
                        }
                    ],
                    "antonyms": [
                        "быть не в счёт"
                    ],
                    "common_phrases": [
                        "это не считается"
                    ]
                },
                {
                    "sense_id": "80aa7f56-5f9f-3bdb-bb1b-7fdbd0973e79",
                    "sense_definition": "Сводить счёты, выяснять отношения (обычно о взаимных обидах) или производить взаимные денежные расчёты.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "settling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Давай не будем <считаться>, кто сколько заплатил за обед, мы же друзья."
                        },
                        {
                            "text": "Он был слишком горд, чтобы <считаться> с обидчиками по мелочам."
                        }
                    ],
                    "common_phrases": [
                        "считаться с врагами"
                    ]
                },
                {
                    "sense_id": "f02a4afd-6fe8-3894-8aba-171610f46d87",
                    "sense_definition": "Быть распознанным, скопированным или воспринятым с носителя информации (о данных).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "data",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Информация успешно <считалась> с повреждённого диска."
                        },
                        {
                            "text": "Штрихкод товара не <считался> сканером с первого раза."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ]
                }
            ]
        }
    ]

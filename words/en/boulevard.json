    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fc63f72e-b8e7-3c26-aa1d-1e9fe3517e2f",
                    "sense_definition": "A broad, well-paved and landscaped thoroughfare.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "street_type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We enjoyed a long walk down the tree-lined <w>boulevard</w>.",
                            "target_lang_translations": {
                                "ru": "Мы наслаждались долгой прогулкой по обсаженному деревьями <w>бульвару</w>."
                            }
                        },
                        {
                            "text": "The city is famous for its wide <w>boulevard</w> filled with shops and cafes.",
                            "target_lang_translations": {
                                "ru": "Город славится своим широким <w>бульваром</w>, полным магазинов и кафе."
                            }
                        }
                    ],
                    "synonyms": [
                        "avenue",
                        "thoroughfare",
                        "street"
                    ],
                    "antonyms": [
                        "alley",
                        "lane"
                    ],
                    "common_phrases": [
                        "grand boulevard",
                        "tree-lined boulevard",
                        "wide boulevard",
                        "main boulevard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Широкая, благоустроенная улица, обычно с аллеями деревьев по бокам или посередине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бульвар",
                                "target_lang_sense_clarification": "широкая аллея или улица с зелеными насаждениями"
                            },
                            {
                                "target_lang_word": "проспект",
                                "target_lang_sense_clarification": "большая широкая улица в городе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c7094a3-983c-32bc-b10f-1c503670d6df",
                    "sense_definition": "A strip of land between a street and sidewalk.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "street_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He planted colorful flowers on the <w>boulevard</w> in front of his house.",
                            "target_lang_translations": {
                                "ru": "Он посадил разноцветные цветы на <w>газоне</w> перед своим домом."
                            }
                        },
                        {
                            "text": "Please do not park your car on the grass of the <w>boulevard</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, не паркуйте машину на траве <w>газона</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "verge",
                        "nature strip"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полоса земли (обычно с травой) между проезжей частью и тротуаром."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "газон",
                                "target_lang_sense_clarification": "участок с травой вдоль дороги"
                            },
                            {
                                "target_lang_word": "обочина",
                                "target_lang_sense_clarification": "край дороги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a733b392-ef47-382e-9691-5dcbdd171439",
                    "sense_definition": "The grassy area in the middle of some streets; A refuge island.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "street_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pedestrians waited on the <w>boulevard</w> for the traffic to clear.",
                            "target_lang_translations": {
                                "ru": "Пешеходы ждали на <w>островке безопасности</w>, пока поток машин не поредеет."
                            }
                        },
                        {
                            "text": "A line of oak trees grows along the central <w>boulevard</w>.",
                            "target_lang_translations": {
                                "ru": "Вдоль центральной <w>разделительной полосы</w> растет ряд дубов."
                            }
                        }
                    ],
                    "synonyms": [
                        "median",
                        "central reservation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Озелененный участок посередине улицы или островок безопасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разделительная полоса",
                                "target_lang_sense_clarification": "зона, разделяющая транспортные потоки"
                            },
                            {
                                "target_lang_word": "островок безопасности",
                                "target_lang_sense_clarification": "место для пешеходов посреди дороги"
                            }
                        ]
                    }
                }
            ]
        },

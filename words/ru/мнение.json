                        "общественное мнение",
                        "личное мнение",
                        "высказывать мнение",
                        "менять мнение"
                    ],
                    "target_lang_definitions": {
                        "en": "A judgment expressing an evaluation, attitude, or view regarding something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "opinion",
                                "target_lang_sense_clarification": "personal belief or judgment"
                            },
                            {

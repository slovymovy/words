    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e04f76f1-dc35-33fc-9861-813aa95f0e1f",
                    "sense_definition": "Предпринимательская экономическая деятельность, приносящая доход или прибыль.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "activity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой брат решил открыть свой собственный <w>бизнес</w>.",
                            "target_lang_translations": {
                                "en": "My brother decided to open his own <w>business</w>."
                            }
                        },
                        {
                            "text": "В этой стране очень выгодно вести малый <w>бизнес</w>.",
                            "target_lang_translations": {
                                "en": "It is very profitable to run a small <w>business</w> in this country."
                            }
                        }
                    ],
                    "synonyms": [
                        "предпринимательство"
                    ],
                    "common_phrases": [
                        "малый бизнес",
                        "открыть свой бизнес",
                        "вести бизнес",
                        "семейный бизнес"
                    ],
                    "target_lang_definitions": {
                        "en": "Entrepreneurial economic activity generating income or profit."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "business",
                                "target_lang_sense_clarification": "commercial activity"
                            },
                            {
                                "target_lang_word": "entrepreneurship",
                                "target_lang_sense_clarification": "activity of setting up a business"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55b6fdde-5857-330b-a3b9-404baaa4ab0b",
                    "sense_definition": "Отдельное коммерческое предприятие, фирма или компания.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он удачно продал свой <w>бизнес</w> и переехал за границу.",
                            "target_lang_translations": {
                                "en": "He successfully sold his <w>business</w> and moved abroad."
                            }
                        },
                        {
                            "text": "Этот ресторан — очень успешный и прибыльный <w>бизнес</w>.",
                            "target_lang_translations": {
                                "en": "This restaurant is a very successful and profitable <w>business</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "предприятие",
                        "компания"
                    ],
                    "common_phrases": [
                        "продать бизнес",
                        "готовый бизнес",
                        "купить бизнес"
                    ],
                    "target_lang_definitions": {
                        "en": "A separate commercial enterprise, firm, or company."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "business",
                                "target_lang_sense_clarification": "company or firm"
                            },
                            {
                                "target_lang_word": "company",
                                "target_lang_sense_clarification": "commercial organization"
                            },
                            {
                                "target_lang_word": "enterprise",
                                "target_lang_sense_clarification": "business organization"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81f57661-fd5e-3757-86cf-22ae0c04104d",
                    "sense_definition": "Деловое сообщество, совокупность предпринимателей и коммерческих организаций.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "community",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Крупный <w>бизнес</w> должен платить налоги в полном объеме.",
                            "target_lang_translations": {
                                "en": "Big <w>business</w> must pay taxes in full."
                            }
                        },
                        {
                            "text": "Государство и <w>бизнес</w> ищут пути для сотрудничества.",
                            "target_lang_translations": {
                                "en": "The state and <w>business</w> are looking for ways to cooperate."
                            }
                        }
                    ],
                    "synonyms": [
                        "деловое сообщество"
                    ],
                    "common_phrases": [
                        "крупный бизнес",
                        "российский бизнес",
                        "представители бизнеса",
                        "средний бизнес"
                    ],
                    "target_lang_definitions": {
                        "en": "The business community; the aggregate of entrepreneurs and commercial organizations."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "business",
                                "target_lang_sense_clarification": "commercial sector collectively"
                            },
                            {
                                "target_lang_word": "the business community",
                                "target_lang_sense_clarification": "people involved in business"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b7b2123c-37b6-3621-a0c8-053809600100",
                    "sense_definition": "Деятельность, направленная исключительно на получение наживы или коммерческой выгоды.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gain",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для них это просто <w>бизнес</w>, и их не волнуют чувства людей.",
                            "target_lang_translations": {
                                "en": "For them it is just <w>business</w>, and they don't care about people's feelings."
                            }
                        },
                        {
                            "text": "Они делают грязный <w>бизнес</w> на проблемах других граждан.",
                            "target_lang_translations": {
                                "en": "They are doing dirty <w>business</w> on the problems of other citizens."
                            }
                        }
                    ],
                    "common_phrases": [
                        "делать бизнес на",
                        "чистый бизнес"
                    ],
                    "target_lang_definitions": {
                        "en": "Activity aimed exclusively at making a profit or commercial gain."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "business",
                                "target_lang_sense_clarification": "actions for profit"
                            }
                        ]
                    }
                }
            ]
        }

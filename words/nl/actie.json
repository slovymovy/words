                    ],
                    "synonyms": [
                        "daad",
                        "handeling"
                    ],
                    "antonyms": [
                        "sloomheid"
                    ],
                    "common_phrases": [
                        "in actie komen",
                        "actie ondernemen"
                    ],
                    "target_lang_definitions": {

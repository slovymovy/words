    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ef95912d-b43b-35d5-bdd5-37d30ad98b9d",
                    "sense_definition": "Очень долгий промежуток времени, который кажется бесконечным.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы не виделись целую <w>вечность</w>, и я очень соскучился.",
                            "target_lang_translations": {
                                "en": "We haven't seen each other for <w>ages</w>, and I really missed you."
                            }
                        },
                        {
                            "text": "Мне показалось, что прошла <w>вечность</w>, прежде чем поезд наконец прибыл.",
                            "target_lang_translations": {
                                "en": "It seemed like an <w>eternity</w> passed before the train finally arrived."
                            }
                        }
                    ],
                    "antonyms": [
                        "мгновение",
                        "миг",
                        "момент"
                    ],
                    "common_phrases": [
                        "целая вечность",
                        "прошла вечность",
                        "ждать вечность"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "используется как гипербола"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A very long period of time that seems endless."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "eternity",
                                "target_lang_sense_clarification": "a very long time"
                            },
                            {
                                "target_lang_word": "ages",
                                "target_lang_sense_clarification": "informal, a long time"
                            },
                            {
                                "target_lang_word": "forever",
                                "target_lang_sense_clarification": "a very long time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8426271c-69f2-3286-8591-c6194bbf9731",
                    "sense_definition": "Бесконечный период времени, не имеющий ни начала, ни конца.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Многие поэты и писатели мечтали оставить свой след в <w>вечности</w>.",
                            "target_lang_translations": {
                                "en": "Many poets and writers dreamed of leaving their mark in <w>eternity</w>."
                            }
                        },
                        {
                            "text": "Если время конечно, значит, <w>вечности</w> в физическом смысле не существует.",
                            "target_lang_translations": {
                                "en": "If time is finite, then <w>eternity</w> in the physical sense does not exist."
                            }
                        }
                    ],
                    "synonyms": [
                        "бесконечность"
                    ],
                    "antonyms": [
                        "мгновение",
                        "миг"
                    ],
                    "common_phrases": [
                        "кануть в вечность",
                        "думать о вечности",
                        "порог вечности"
                    ],
                    "target_lang_definitions": {
                        "en": "An infinite period of time, having neither beginning nor end."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "eternity",
                                "target_lang_sense_clarification": "infinite time"
                            }
                        ]
                    }
                }
            ]
        }
    ]

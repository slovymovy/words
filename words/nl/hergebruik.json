                    "sense_id": "c55d95d5-0444-3ab1-b607-d95afd7d8411",
                    "sense_definition": "Het opnieuw gebruiken van materialen of producten.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het <w>hergebruik</w> van plastic tassen is beter voor het milieu.",
                            "target_lang_translations": {
                                "en": "The <w>reuse</w> of plastic bags is better for the environment.",
                                "ru": "<w>Повторное использование</w> пластиковых пакетов лучше для окружающей среды."
                            }
                        },
                        {
                            "text": "Wij stimuleren het <w>hergebruik</w> van oude kleding in onze winkel.",
                            "target_lang_translations": {
                                "en": "We encourage the <w>reuse</w> of old clothing in our store.",
                                "ru": "Мы поощряем <w>повторное использование</w> старой одежды в нашем магазине."
                            }
                        }
                    ],
                    "synonyms": [
                        "recycling"
                    ],
                    "antonyms": [
                        "verspilling"
                    ],
                    "common_phrases": [
                        "hergebruik van materialen",
                        "geschikt voor hergebruik"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of using materials or products again.",
                        "ru": "Процесс повторного использования материалов или продуктов."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "reuse",

{
    "word_family": [
        "notować",
        "zanotować",
        "notatka",
        "notes"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "97800714-8c41-3b57-87f3-c878d94de12b",
                    "sense_definition": "Zapisać informację w celu jej utrwalenia, aby o niej pamiętać lub mieć dowód.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "recording",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <odnotować> to ważne wydarzenie w moim prywatnym kalendarzu.",
                            "target_lang_translations": {
                                "ru": "Я должен <записать> это важное событие в своем личном календаре."
                            }
                        },
                        {
                            "text": "Sekretarka dokładnie <odnotowała> wszystkie uwagi klienta podczas spotkania.",
                            "target_lang_translations": {
                                "ru": "Секретарь точно <записала> все замечания клиента во время встречи."
                            }
                        }
                    ],
                    "synonyms": [
                        "zapisać",
                        "zanotować",
                        "spisać",
                        "wynotować"
                    ],
                    "common_phrases": [
                        "odnotować w pamięci",
                        "odnotować w raporcie",
                        "starannie odnotować",
                        "odnotować wynik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Записать информацию, чтобы сохранить её, запомнить или иметь доказательство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "записать",
                                "target_lang_sense_clarification": "сохранить информацию в письменном виде"
                            },
                            {
                                "target_lang_word": "зафиксировать",
                                "target_lang_sense_clarification": "закрепить сведения документально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f95eebc-f51e-3c70-a033-0d973f745144",
                    "sense_definition": "Zauważyć lub stwierdzić wystąpienie jakiegoś faktu, zjawiska lub zmiany.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "observation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym roku policja <odnotowała> znaczny spadek przestępczości w mieście.",
                            "target_lang_translations": {
                                "ru": "В этом году полиция <зафиксировала> значительное снижение преступности в городе."
                            }
                        },
                        {
                            "text": "Lekarze <odnotowali> nagłą poprawę stanu zdrowia u pacjenta.",
                            "target_lang_translations": {
                                "ru": "Врачи <отметили> внезапное улучшение состояния здоровья пациента."
                            }
                        }
                    ],
                    "synonyms": [
                        "zauważyć",
                        "zaobserwować",
                        "dostrzec",
                        "spostrzec",
                        "skonstatować"
                    ],
                    "common_phrases": [
                        "odnotować wzrost",
                        "odnotować spadek",
                        "odnotować stratę",
                        "odnotować sukces"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заметить или констатировать факт, явление или изменение (часто в статистике или наблюдениях)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зафиксировать",
                                "target_lang_sense_clarification": "установить наличие факта или изменения"
                            },
                            {
                                "target_lang_word": "отметить",
                                "target_lang_sense_clarification": "обратить внимание на явление"
                            },
                            {
                                "target_lang_word": "зарегистрировать",
                                "target_lang_sense_clarification": "внести факт в статистику или учет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "faabaf20-0602-3355-a575-8d50d671a421",
                    "sense_definition": "Zaznaczyć na liście lub w dokumencie, że dana czynność została wykonana lub element został sprawdzony.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "recording",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel <odnotował> obecność wszystkich uczniów w dzienniku lekcyjnym.",
                            "target_lang_translations": {
                                "ru": "Учитель <отметил> присутствие всех учеников в классном журнале."
                            }
                        },
                        {
                            "text": "Kierownik magazynu <odnotował> przyjęcie towaru w systemie komputerowym.",
                            "target_lang_translations": {
                                "ru": "Заведующий складом <отметил> прием товара в компьютерной системе."
                            }
                        }
                    ],
                    "synonyms": [
                        "odhaczyć",
                        "odfajkować"
                    ],
                    "common_phrases": [
                        "odnotować obecność",
                        "odnotować wykonanie",
                        "odnotować wpłatę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать пометку в списке или документе о том, что действие выполнено или элемент проверен."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отметить",
                                "target_lang_sense_clarification": "поставить метку в списке"
                            },
                            {
                                "target_lang_word": "проставить",
                                "target_lang_sense_clarification": "сделать запись (например, об отсутствии)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f36af6d-d764-3452-86b6-48df99e9cbba",
                    "sense_definition": "Wspomnieć o czymś krótko w wypowiedzi lub tekście, zwracając na to uwagę odbiorcy.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Autor artykułu <odnotował> również negatywny wpływ diety na zdrowie.",
                            "target_lang_translations": {
                                "ru": "Автор статьи также <отметил> негативное влияние диеты на здоровье."
                            }
                        },
                        {
                            "text": "Warto <odnotować>, że ten zabytkowy budynek przetrwał wojnę bez zniszczeń.",
                            "target_lang_translations": {
                                "ru": "Стоит <отметить>, что это историческое здание пережило войну без повреждений."
                            }
                        }
                    ],
                    "synonyms": [
                        "wspomnieć",
                        "nadmienić",
                        "wzmiankować",
                        "napomknąć"
                    ],
                    "common_phrases": [
                        "warto odnotować",
                        "należy odnotować fakt",
                        "trzeba odnotować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Кратко упомянуть о чем-либо в речи или тексте, обращая на это внимание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отметить",
                                "target_lang_sense_clarification": "указать на важный момент"
                            },
                            {
                                "target_lang_word": "упомянуть",
                                "target_lang_sense_clarification": "коснуться темы в разговоре"
                            }
                        ]
                    }
                }
            ]
        }
    ]

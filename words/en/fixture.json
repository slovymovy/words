                {
                    "sense_id": "75e23223-2f13-3ada-bc64-9395a1fc41a1",
                    "sense_definition": "A fixed state or environment used as a consistent baseline for running software tests.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "software_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The test <w>fixture</w> ensures that the database is in a known state before the code is executed.",
                            "target_lang_translations": {
                                "ru": "Тестовая <w>фикстура</w> гарантирует, что база данных находится в известном состоянии перед выполнением кода."
                            }
                        },
                        {
                            "text": "Developers set up a <w>fixture</w> to simulate user data for the automated tests.",
                            "target_lang_translations": {
                                "ru": "Разработчики настроили <w>тестовое окружение</w> для имитации пользовательских данных для автоматических тестов."
                            }
                        }

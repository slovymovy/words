    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "83d0f663-67e1-3241-9375-8ed3908a1bf5",
                    "sense_definition": "Представитель народа, традиционно ведущего кочевой образ жизни и рассеянного по многим странам мира.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В наш город приехали <цыгане> и устроили концерт народной музыки.",
                            "target_lang_translations": {
                                "en": "<w>Gypsies</w> came to our city and organized a folk music concert."
                            }
                        },
                        {
                            "text": "На окраине села остановился большой табор <цыган>, привлекая внимание местных жителей.",
                            "target_lang_translations": {
                                "en": "A large camp of <w>Gypsies</w> stopped on the outskirts of the village, attracting the attention of the locals."
                            }
                        }
                    ],
                    "synonyms": [
                        "ро́ма"
                    ],
                    "common_phrases": [
                        "табор цыган",
                        "кочевые цыгане",
                        "песни цыган"
                    ],
                    "target_lang_definitions": {
                        "en": "A member of a people, traditionally leading a nomadic lifestyle and scattered across many countries of the world."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Gypsy",
                                "target_lang_sense_clarification": "common term, sometimes considered offensive"
                            },
                            {
                                "target_lang_word": "Roma",
                                "target_lang_sense_clarification": "preferred polite term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1d338771-5625-398e-8b8f-6c0afc8b5594",
                    "sense_definition": "Хитрый человек, склонный к обману или плутовству.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот торговец настоящий <цыган>, с ним нужно быть очень осторожным при сделке.",
                            "target_lang_translations": {
                                "en": "This merchant is a real <w>gypsy</w>; you need to be very careful with him during the deal."
                            }
                        },
                        {
                            "text": "Мой сосед хитрый как <цыган>, он всегда ищет свою выгоду в любом деле.",
                            "target_lang_translations": {
                                "en": "My neighbor is as cunning as a <w>gypsy</w>; he is always looking for his own benefit in any matter."
                            }
                        }
                    ],
                    "synonyms": [
                        "обманщик",
                        "плут"
                    ],
                    "common_phrases": [
                        "хитрый как цыган"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used figuratively to describe a cunning person"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A cunning person, prone to deception or trickery."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "gypsy",
                                "target_lang_sense_clarification": "figurative, implying cunning"
                            },
                            {
                                "target_lang_word": "swindler",
                                "target_lang_sense_clarification": "synonym for the figurative meaning"
                            }
                        ]
                    }
                }
            ]
        }
    ]

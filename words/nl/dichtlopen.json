{
    "word_family": [
        "loper",
        "loop",
        "aanloop",
        "verloop",
        "inloop",
        "uitloop",
        "leegloop"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cd648bf6-04cb-339d-b714-b36b761be45a",
                    "sense_definition": "Verstopt of geblokkeerd raken waardoor doorstroming niet meer mogelijk is",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "blockage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De afvoer in de keuken is helemaal <w>dichtgelopen</w> door etensresten.",
                            "target_lang_translations": {
                                "ru": "Слив на кухне полностью <w>забился</w> остатками еды."
                            }
                        },
                        {
                            "text": "Als je het filter niet regelmatig schoonmaakt, zal het snel <w>dichtlopen</w>.",
                            "target_lang_translations": {
                                "ru": "Если не чистить фильтр регулярно, он быстро <w>засорится</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "verstoppen",
                        "dichtslippen",
                        "blokkeren"
                    ],
                    "antonyms": [
                        "ontstoppen",
                        "openen"
                    ],
                    "common_phrases": [
                        "de afvoer loopt dicht",
                        "de weg loopt dicht"
                    ],
                    "target_lang_definitions": {
                        "ru": "Засориться или заблокироваться, в результате чего поток жидкости или газа больше не может проходить"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "засориться",
                                "target_lang_sense_clarification": "о трубах, фильтрах или каналах"
                            },
                            {
                                "target_lang_word": "забиться",
                                "target_lang_sense_clarification": "разговорный вариант; о плотном засорении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cb53557-913c-3cb9-916d-4a4a97710ae4",
                    "sense_definition": "De afstand tot een voorligger verkleinen door sneller te rennen",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "sports_gap",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De atleet wist het gat met de kopgroep in de laatste ronde <w>dichtgelopen</w> te hebben.",
                            "target_lang_translations": {
                                "ru": "Атлету удалось <w>сократить</w> разрыв с лидирующей группой на последнем круге."
                            }
                        },
                        {
                            "text": "Hij moet versnellen om de achterstand op de leider te <w>dichtlopen</w>.",
                            "target_lang_translations": {
                                "ru": "Ему нужно ускориться, чтобы <w>ликвидировать</w> отставание от лидера."
                            }
                        }
                    ],
                    "synonyms": [
                        "inlopen",
                        "naderen"
                    ],
                    "antonyms": [
                        "uitlopen",
                        "lossen"
                    ],
                    "common_phrases": [
                        "het gat dichtlopen",
                        "een achterstand dichtlopen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сократить дистанцию до впереди идущего участника, двигаясь быстрее (в спорте)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сократить",
                                "target_lang_sense_clarification": "обычно в сочетании с «разрыв» или «дистанцию»"
                            },
                            {
                                "target_lang_word": "ликвидировать",
                                "target_lang_sense_clarification": "обычно в сочетании с «отставание»"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3081832e-f264-3e73-817a-493a5e54171e",
                    "sense_definition": "Ondoorgaanbaar worden door dichte begroeiing of ophoping van materiaal",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "blockage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het oude bospad is in de loop der jaren volledig <w>dichtgelopen</w> met struikgewas.",
                            "target_lang_translations": {
                                "ru": "Старая лесная тропа с годами полностью <w>заросла</w> кустарником."
                            }
                        },
                        {
                            "text": "Zonder onderhoud zullen de smalle sloten langzaam <w>dichtlopen</w> met modder.",
                            "target_lang_translations": {
                                "ru": "Без обслуживания узкие канавы медленно <w>заплывут</w> грязью."
                            }
                        }
                    ],
                    "synonyms": [
                        "dichtgroeien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать непроходимым или исчезнуть из-за густой растительности или скопления грязи"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зарасти",
                                "target_lang_sense_clarification": "покрыться растительностью (о тропах, дорогах)"
                            },
                            {
                                "target_lang_word": "заплыть",
                                "target_lang_sense_clarification": "заполниться грязью или илом (о канавах, руслах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb0a30b2-55a1-3f7a-82ee-e8acbdcb2d0b",
                    "sense_definition": "Onleesbaar of vlekkerig worden doordat inkt de open ruimtes in of tussen letters vult",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bij gebruik van het verkeerde papier kunnen de kleine letters <w>dichtlopen</w>.",
                            "target_lang_translations": {
                                "ru": "При использовании неподходящей бумаги мелкие буквы могут <w>заплыть</w>."
                            }
                        },
                        {
                            "text": "De details in de illustratie zijn <w>dichtgelopen</w> door een teveel aan inkt.",
                            "target_lang_translations": {
                                "ru": "Детали на иллюстрации <w>слились</w> из-за избытка чернил."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Стать неразборчивым или смазанным из-за того, что чернила заполняют просветы (в печати или письме)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заплыть",
                                "target_lang_sense_clarification": "о буквах или деталях изображения, теряющих четкость"
                            },
                            {
                                "target_lang_word": "слиться",
                                "target_lang_sense_clarification": "стать единым пятном, потерять контуры"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "16e4c527-130f-304f-ae8b-16c22463e083",
                    "sense_definition": "Functioning properly; in a condition to be used or worked.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "functional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old machinery is surprisingly still <w>operative</w> after all these years.",
                            "target_lang_translations": {
                                "ru": "Старое оборудование на удивление всё ещё <w>исправно</w> спустя столько лет."
                            }
                        },
                        {
                            "text": "We need to ensure the alarm system is fully <w>operative</w> before leaving.",
                            "target_lang_translations": {
                                "ru": "Нам нужно убедиться, что система сигнализации полностью <w>работоспособна</w> перед уходом."
                            }
                        }
                    ],
                    "synonyms": [
                        "functional",
                        "working",
                        "operational",
                        "active"
                    ],
                    "antonyms": [
                        "inoperative",
                        "broken",
                        "defective"
                    ],
                    "common_phrases": [
                        "fully operative",
                        "become operative",
                        "remain operative"
                    ],
                    "target_lang_definitions": {
                        "ru": "Функционирующий должным образом; находящийся в рабочем состоянии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исправный",
                                "target_lang_sense_clarification": "в хорошем состоянии, не сломанный"
                            },
                            {
                                "target_lang_word": "действующий",
                                "target_lang_sense_clarification": "функционирующий, активный"
                            },
                            {
                                "target_lang_word": "рабочий",
                                "target_lang_sense_clarification": "готовый к использованию"
                            },
                            {
                                "target_lang_word": "работающий",
                                "target_lang_sense_clarification": "выполняющий свои функции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39ffcf2a-8f60-3e96-af38-137b3ace9b6a",
                    "sense_definition": "Most significant or key to the meaning or outcome of a situation; having the most relevance.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "key",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He said he might help us, but 'might' is the <w>operative</w> word.",
                            "target_lang_translations": {
                                "ru": "Он сказал, что, может быть, поможет нам, но «может быть» — это <w>ключевое</w> слово."
                            }
                        },
                        {
                            "text": "Speed is the <w>operative</w> factor in this emergency response plan.",
                            "target_lang_translations": {
                                "ru": "Скорость — это <w>решающий</w> фактор в этом плане аварийного реагирования."
                            }
                        }
                    ],
                    "synonyms": [
                        "key",
                        "crucial",
                        "significant",
                        "pivotal"
                    ],
                    "antonyms": [
                        "insignificant",
                        "irrelevant"
                    ],
                    "common_phrases": [
                        "the operative word",
                        "operative factor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наиболее значимый или ключевой для смысла или исхода ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ключевой",

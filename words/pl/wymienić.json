                {
                    "sense_id": "244fc9fe-da5b-3a5e-80a0-5bcd7e3e6a60",
                    "sense_definition": "Podać po kolei nazwy, nazwiska, liczby lub przykłady; wyliczyć coś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "speak_list",
                    "name_type": "no",
                    "examples": [

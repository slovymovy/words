                        "terugzenden",
                        "terugbrengen"
                    ],
                    "antonyms": [
                        "behouden"
                    ],
                    "common_phrases": [
                        "artikel retourneren",
                        "gratis retourneren",
                        "pakket retourneren"
                    ],
                    "target_lang_definitions": {
                        "en": "To send or bring something back to the sender or place of origin",
                        "ru": "Отправить или вернуть что-либо отправителю или в место происхождения"
                    },
                    "translations": {

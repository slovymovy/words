{
    "word_family": [
        "opstand",
        "opstanding",
        "opstandig"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5fc4c325-f603-37bf-b6bb-ad2f01deb565",
                    "sense_definition": "Wakker worden en het bed verlaten om de dag te beginnen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "fysieke_actie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik moet morgenochtend heel vroeg <w>opstaan</w> om de trein te halen.",
                            "target_lang_translations": {
                                "en": "I have to <w>get up</w> very early tomorrow morning to catch the train.",
                                "ru": "Завтра утром мне нужно очень рано <w>встать</w>, чтобы успеть на поезд."
                            }
                        },
                        {
                            "text": "In het weekend vind ik het heerlijk om laat te <w>opstaan</w>.",
                            "target_lang_translations": {
                                "en": "On the weekend I love to <w>get up</w> late.",
                                "ru": "В выходные я обожаю поздно <w>вставать</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "uit bed komen"
                    ],
                    "antonyms": [
                        "naar bed gaan",
                        "gaan slapen"
                    ],
                    "common_phrases": [
                        "vroeg opstaan",
                        "laat opstaan",
                        "met het verkeerde been opstaan"
                    ],
                    "target_lang_definitions": {
                        "en": "To wake up and leave one's bed to start the day.",
                        "ru": "Просыпаться и вставать с постели, чтобы начать день."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "get up",
                                "target_lang_sense_clarification": "leave one's bed"
                            },
                            {
                                "target_lang_word": "rise",
                                "target_lang_sense_clarification": "formal or literary"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вставать",
                                "target_lang_sense_clarification": "подниматься с постели после сна"
                            },
                            {
                                "target_lang_word": "просыпаться",
                                "target_lang_sense_clarification": "пробуждаться ото сна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d220e8f-19d2-3f6a-9961-0db2e4ace651",
                    "sense_definition": "Vanuit een zittende of liggende houding op de voeten gaan staan.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "fysieke_actie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet in de bus <w>opstaan</w> voor oudere mensen.",
                            "target_lang_translations": {
                                "en": "You have to <w>stand up</w> for elderly people on the bus.",
                                "ru": "В автобусе нужно <w>вставать</w>, чтобы уступить место пожилым людям."
                            }
                        },
                        {
                            "text": "Na de lange film wilde iedereen even <w>opstaan</w> en bewegen.",
                            "target_lang_translations": {
                                "en": "After the long movie, everyone wanted to <w>stand up</w> and move around for a bit.",
                                "ru": "После долгого фильма всем хотелось <w>встать</w> и размяться."
                            }
                        }
                    ],
                    "synonyms": [
                        "gaan staan",
                        "rechtstaan"
                    ],
                    "antonyms": [
                        "gaan zitten",
                        "gaan liggen"
                    ],
                    "common_phrases": [
                        "opstaan voor iemand",
                        "opstaan uit een stoel"
                    ],
                    "target_lang_definitions": {
                        "en": "To rise to a standing position from sitting or lying down.",
                        "ru": "Вставать на ноги из сидячего или лежачего положения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stand up",
                                "target_lang_sense_clarification": "assume a standing position"
                            },
                            {
                                "target_lang_word": "get up",
                                "target_lang_sense_clarification": "rise from sitting"
                            },
                            {
                                "target_lang_word": "rise",
                                "target_lang_sense_clarification": "formal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вставать",
                                "target_lang_sense_clarification": "принимать вертикальное положение"
                            },
                            {
                                "target_lang_word": "подниматься",
                                "target_lang_sense_clarification": "вставать с места"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "deded99f-52d9-3ec5-ae54-16ef67bb69fc",
                    "sense_definition": "Op het fornuis staan om gekookt of opgewarmd te worden.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "proces_actief",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zie dat de aardappels al <w>opstaan</w> voor het avondeten.",
                            "target_lang_translations": {
                                "en": "I see that the potatoes <w>are on</w> for dinner already.",
                                "ru": "Я вижу, что картошка уже <w>варится</w> к ужину."
                            }
                        },
                        {
                            "text": "Zorg ervoor dat de soep op tijd <w>opstaat</w> als de gasten komen.",
                            "target_lang_translations": {
                                "en": "Make sure the soup <w>is on</w> in time when the guests arrive.",
                                "ru": "Позаботься о том, чтобы суп <w>варился</w> вовремя, когда придут гости."
                            }
                        }
                    ],
                    "synonyms": [
                        "op het vuur staan",
                        "koken"
                    ],
                    "common_phrases": [
                        "het eten staat op",
                        "de aardappels staan op"
                    ],
                    "target_lang_definitions": {
                        "en": "To be on the stove to be cooked or heated.",
                        "ru": "Стоять на плите (о еде), готовясь или разогреваясь."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be on",
                                "target_lang_sense_clarification": "be on the stove"
                            },
                            {
                                "target_lang_word": "be cooking",
                                "target_lang_sense_clarification": "undergo cooking"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вариться",
                                "target_lang_sense_clarification": "готовиться на огне"
                            },
                            {
                                "target_lang_word": "стоять на плите",
                                "target_lang_sense_clarification": "быть в процессе готовки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ffe4278-790d-38f7-ac0a-06f058694004",
                    "sense_definition": "Afgespeeld worden, zoals muziek op de radio of een film op televisie.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "proces_actief",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Weet jij welk liedje er nu <w>opstaat</w> op de radio?",
                            "target_lang_translations": {
                                "en": "Do you know which song <w>is on</w> the radio right now?",
                                "ru": "Ты знаешь, какая песня сейчас <w>играет</w> по радио?"
                            }
                        },
                        {
                            "text": "Als mijn favoriete programma <w>opstaat</w>, mag niemand mij storen.",
                            "target_lang_translations": {
                                "en": "When my favorite program <w>is on</w>, no one is allowed to disturb me.",
                                "ru": "Когда <w>идет</w> моя любимая передача, меня нельзя отвлекать."
                            }
                        }
                    ],
                    "synonyms": [
                        "draaien",
                        "spelen"
                    ],
                    "common_phrases": [
                        "een liedje staat op",
                        "de radio staat op"
                    ],
                    "target_lang_definitions": {
                        "en": "To be played or broadcast, such as music on the radio or a film on television.",
                        "ru": "Воспроизводиться или транслироваться (о музыке, передачах)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be on",
                                "target_lang_sense_clarification": "be playing or broadcast"
                            },
                            {
                                "target_lang_word": "play",
                                "target_lang_sense_clarification": "of music or media"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "играть",
                                "target_lang_sense_clarification": "о музыке или радио"
                            },
                            {
                                "target_lang_word": "идти",
                                "target_lang_sense_clarification": "о фильме или телепередаче"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1971ce26-d9a7-37de-b4c2-6c3c0b2fe324",
                    "sense_definition": "Weer levend worden na de dood.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het verhaal zullen de doden uiteindelijk uit hun graf <w>opstaan</w>.",
                            "target_lang_translations": {
                                "en": "In the story, the dead will eventually <w>rise</w> from their graves.",
                                "ru": "В этой истории мертвые в конце концов <w>восстанут</w> из своих могил."
                            }
                        },
                        {
                            "text": "Veel gelovigen hopen dat ze na dit leven zullen <w>opstaan</w>.",
                            "target_lang_translations": {
                                "en": "Many believers hope that they will <w>rise</w> after this life.",
                                "ru": "Многие верующие надеются, что после этой жизни они <w>воскреснут</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "herrijzen",
                        "wederopstaan"
                    ],
                    "antonyms": [
                        "sterven",
                        "overlijden"
                    ],
                    "common_phrases": [
                        "opstaan uit de dood"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "religious"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To come back to life after death.",
                        "ru": "Оживать, возвращаться к жизни после смерти."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "rise",
                                "target_lang_sense_clarification": "return to life"
                            },
                            {
                                "target_lang_word": "resurrect",
                                "target_lang_sense_clarification": "religious context"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "воскресать",
                                "target_lang_sense_clarification": "возвращаться к жизни"
                            },
                            {
                                "target_lang_word": "восставать",
                                "target_lang_sense_clarification": "подниматься из мертвых (книжн.)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
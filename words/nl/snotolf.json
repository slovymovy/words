            "pos": "noun",
            "senses": [
                {
                    "sense_id": "97295756-95d1-3e13-bdac-6249354c637c",
                    "sense_definition": "Een bepaalde soort zeevis (*Cyclopterus lumpus*) die voorkomt in de kustgebieden van de Noordelijke Atlantische Oceaan.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De visser haalde een grote <w>snotolf</w> uit zijn netten.",
                            "target_lang_translations": {
                                "en": "The fisherman pulled a large <w>lumpsucker</w> from his nets.",
                                "ru": "Рыбак вытащил большого <w>пинагора</w> из своих сетей."
                            }
                        },
                        {
                            "text": "In het zeeaquarium zwom de <w>snotolf</w> traag langs de rotsen.",
                            "target_lang_translations": {
                                "en": "In the sea aquarium, the <w>lumpfish</w> swam slowly past the rocks.",
                                "ru": "В морском аквариуме <w>пинагор</w> медленно проплывал мимо скал."
                            }
                        }
                    ],
                    "synonyms": [
                        "lompvis",
                        "steenkruiper"
                    ],
                    "common_phrases": [
                        "gevangen snotolf",
                        "grote snotolf"
                    ],
                    "target_lang_definitions": {
                        "en": "A specific type of marine fish (*Cyclopterus lumpus*) found in the coastal areas of the North Atlantic Ocean.",
                        "ru": "Определенный вид морской рыбы (*Cyclopterus lumpus*), обитающий в прибрежных водах Северной Атлантики."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lumpsucker",
                                "target_lang_sense_clarification": "fish species"
                            },
                            {
                                "target_lang_word": "lumpfish",
                                "target_lang_sense_clarification": "common name"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пинагор",
                                "target_lang_sense_clarification": "рыба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6250e0df-681f-3035-a84c-45fe9175cfa6",
                    "sense_definition": "Een persoon die als vies, smerig of onverzorgd wordt beschouwd.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "insult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Was je handen eens, jij vieze <w>snotolf</w>!",
                            "target_lang_translations": {
                                "en": "Wash your hands, you dirty <w>slob</w>!",
                                "ru": "А ну помой руки, ты грязный <w>неряха</w>!"
                            }
                        },
                        {
                            "text": "Hij gedraagt zich aan tafel als een echte <w>snotolf</w>.",
                            "target_lang_translations": {
                                "en": "He behaves like a real <w>slob</w> at the table.",
                                "ru": "За столом он ведет себя как настоящий <w>грязнуля</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "smeerlap",
                        "viespeuk",
                        "vuilak"
                    ],
                    "common_phrases": [
                        "vieze snotolf",
                        "vuile snotolf"
                    ],
                    "target_lang_definitions": {
                        "en": "A person regarded as dirty, filthy, or unkempt.",
                        "ru": "Человек, который считается грязным, неопрятным или неухоженным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slob",
                                "target_lang_sense_clarification": "dirty person"
                            },
                            {
                                "target_lang_word": "filthy person",
                                "target_lang_sense_clarification": "literal description"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "грязнуля",
                                "target_lang_sense_clarification": "неопрятный человек"
                            },
                            {
                                "target_lang_word": "неряха",
                                "target_lang_sense_clarification": "неухоженный человек"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6318a661-6468-3813-9997-f10d299e0af2",
                    "sense_definition": "Een persoon die jong is en weinig ervaring heeft.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "insult",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die jonge <w>snotolf</w> moet nog veel leren over het vak.",
                            "target_lang_translations": {
                                "en": "That young <w>whippersnapper</w> still has a lot to learn about the profession.",
                                "ru": "Этому юному <w>сопляку</w> еще многому нужно научиться в профессии."
                            }
                        },
                        {
                            "text": "Luister niet naar die <w>snotolf</w>, hij weet van toeten noch blazen.",
                            "target_lang_translations": {
                                "en": "Don't listen to that <w>snot-nose</w>, he doesn't know the first thing about it.",
                                "ru": "Не слушай этого <w>сопляка</w>, он ничего не смыслит."
                            }
                        }
                    ],
                    "synonyms": [
                        "snotneus"
                    ],
                    "common_phrases": [
                        "jonge snotolf"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who is young and has little experience.",
                        "ru": "Молодой человек, не имеющий опыта."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "snot-nose",
                                "target_lang_sense_clarification": "young, annoying person"
                            },
                            {
                                "target_lang_word": "whippersnapper",
                                "target_lang_sense_clarification": "young, inexperienced person"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "сопляк",
                                "target_lang_sense_clarification": "пренебрежительно о молодом, неопытном человеке"
                            },
                            {
                                "target_lang_word": "молокосос",
                                "target_lang_sense_clarification": "разговорное, пренебрежительное"
                            }
                        ]
                    }
                }
            ]
        }

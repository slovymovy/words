    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c96f5823-46e0-356d-9607-e85976df5b59",
                    "sense_definition": "Zeer verbaasd over iets dat je niet had verwacht.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "emotion_surprise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik stond echt <w>versteld</w> van zijn plotselinge vertrek.",
                            "target_lang_translations": {
                                "en": "I was truly <w>amazed</w> by his sudden departure.",
                                "ru": "Я был искренне <w>поражён</w> его внезапным отъездом."
                            }
                        },
                        {
                            "text": "De goochelaar deed het hele publiek <w>versteld</w> staan met zijn trucs.",
                            "target_lang_translations": {
                                "en": "The magician left the whole audience <w>astounded</w> with his tricks.",
                                "ru": "Фокусник заставил всю публику <w>изумиться</w> своими трюками."
                            }
                        }
                    ],
                    "synonyms": [
                        "verbaasd",
                        "verbluft",
                        "verrast",
                        "overdonderd"
                    ],
                    "common_phrases": [
                        "versteld staan",
                        "doen versteld staan"
                    ],
                    "target_lang_definitions": {
                        "en": "Very surprised about something you did not expect.",
                        "ru": "Очень удивлен чем-то, чего вы не ожидали."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "amazed",
                                "target_lang_sense_clarification": "filled with wonder or astonishment"
                            },
                            {
                                "target_lang_word": "astounded",
                                "target_lang_sense_clarification": "shocked or greatly surprised"
                            },
                            {
                                "target_lang_word": "stunned",
                                "target_lang_sense_clarification": "so shocked that one is temporarily unable to react"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "изумлённый",
                                "target_lang_sense_clarification": "испытывающий сильное удивление"
                            },
                            {
                                "target_lang_word": "поражённый",
                                "target_lang_sense_clarification": "сильно удивленный, потрясенный"
                            },
                            {
                                "target_lang_word": "ошеломлённый",
                                "target_lang_sense_clarification": "настолько удивленный, что не может реагировать"
                            }

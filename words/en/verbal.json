                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "владе́ющий ре́чью",
                                "target_lang_sense_clarification": "able to speak"
                            },
                            {
                                "target_lang_word": "говоря́щий",
                                "target_lang_sense_clarification": "using speech"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38213b4a-ee2f-3fef-81b6-003b1632dd25",
                    "sense_definition": "Corresponding word for word.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "literal_translation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The diplomat insisted on a <w>verbal</w> translation of the treaty to avoid ambiguity.",
                            "target_lang_translations": {
                                "ru": "Дипломат настоял на <w>дословном</w> переводе договора, чтобы избежать двусмысленности."
                            }
                        },
                        {
                            "text": "This is a <w>verbal</w> copy of the original text, preserving every error.",
                            "target_lang_translations": {
                                "ru": "Это <w>дословная</w> копия оригинального текста, сохраняющая каждую ошибку."
                            }

{
    "word_family": [
        "achteruitgang",
        "achteruitkijkspiegel",
        "achteruitrijlicht"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "d9ed4ad8-b2a8-32b9-b3d2-0bada6f36b5b",
                    "sense_definition": "Naar achteren gericht; in de richting van de rug of de achterkant.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij zette voorzichtig een stap <w>achteruit</w> toen de hond begon te blaffen.",
                            "target_lang_translations": {
                                "ru": "Он осторожно сделал шаг <w>назад</w>, когда собака начала лаять."
                            }
                        },
                        {
                            "text": "De auto reed langzaam <w>achteruit</w> om uit het parkeervak te komen.",
                            "target_lang_translations": {
                                "ru": "Машина медленно ехала <w>назад</w>, чтобы выехать с парковочного места."
                            }
                        }
                    ],
                    "synonyms": [
                        "achterwaarts",
                        "terug"
                    ],
                    "antonyms": [
                        "vooruit"
                    ],
                    "common_phrases": [
                        "stap achteruit",
                        "achteruit rijden",
                        "achteruit kijken",
                        "achteruit lopen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направленный назад; в сторону спины или задней части."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "назад",
                                "target_lang_sense_clarification": "направление движения"
                            },
                            {
                                "target_lang_word": "задним ходом",
                                "target_lang_sense_clarification": "способ движения транспорта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c0d64a8-8ad6-386b-befb-d61135df717d",
                    "sense_definition": "In een slechtere toestand komen; in kwaliteit of gezondheid verminderen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "deterioration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gezondheid van de patiënt ging na de operatie snel <w>achteruit</w>.",
                            "target_lang_translations": {
                                "ru": "Здоровье пациента после операции быстро пошло <w>на спад</w>."
                            }
                        },
                        {
                            "text": "Sinds de economische crisis gaat het bedrijf hard <w>achteruit</w>.",
                            "target_lang_translations": {
                                "ru": "С момента экономического кризиса дела компании идут <w>к худшему</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "vooruit"
                    ],
                    "common_phrases": [
                        "snel achteruit",
                        "hard achteruit",
                        "erop achteruit gaan"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приходить в худшее состояние; снижение качества или здоровья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "на спад",
                                "target_lang_sense_clarification": "об ухудшении состояния"
                            },
                            {
                                "target_lang_word": "к худшему",
                                "target_lang_sense_clarification": "негативное изменение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4f086f76-bb31-30cb-b278-d62eaee522ae",
                    "sense_definition": "De stand van de versnellingsbak waarmee een voertuig naar achteren kan rijden.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "gear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet de auto in zijn <w>achteruit</w> zetten om te kunnen parkeren.",
                            "target_lang_translations": {
                                "ru": "Нужно включить <w>заднюю передачу</w>, чтобы припарковаться."
                            }
                        },
                        {
                            "text": "De bestuurder schakelde naar de <w>achteruit</w> en reed de oprit af.",
                            "target_lang_translations": {
                                "ru": "Водитель переключился на <w>задний ход</w> и выехал с подъездной дорожки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in zijn achteruit",
                        "in de achteruit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Положение коробки передач, позволяющее транспортному средству двигаться назад."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задний ход",
                                "target_lang_sense_clarification": "режим движения"
                            },
                            {
                                "target_lang_word": "задняя передача",
                                "target_lang_sense_clarification": "механизм"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cbe0065-fd72-3ec2-88ad-5ac152a4847c",
                    "sense_definition": "Een uitgang of gedeelte aan de achterzijde van een gebouw of ruimte.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leveranciers gebruiken meestal de <w>achteruit</w> om goederen te lossen.",
                            "target_lang_translations": {
                                "ru": "Поставщики обычно используют <w>задний выход</w> для разгрузки товаров."
                            }
                        },
                        {
                            "text": "We verlieten het pand stilletjes via de donkere <w>achteruit</w>.",
                            "target_lang_translations": {
                                "ru": "Мы тихо покинули здание через темный <w>черный ход</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "via de achteruit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выход или часть здания с задней стороны."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "задний выход",
                                "target_lang_sense_clarification": "дверь с тыльной стороны"
                            },
                            {
                                "target_lang_word": "черный ход",
                                "target_lang_sense_clarification": "служебный вход"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
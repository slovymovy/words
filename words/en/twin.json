                },
                {
                    "sense_id": "04ad139a-dabd-3f76-95ef-8e185b90e6d3",
                    "sense_definition": "A narrow bed intended for one person, or a mattress of this size.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "furniture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The child has outgrown his crib and needs a <w>twin</w>.",
                            "target_lang_translations": {
                                "ru": "Ребенок вырос из своей колыбели, и ему нужна <w>односпальная кровать</w>."
                            }
                        },
                        {
                            "text": "These sheets are designed to fit a <w>twin</w> perfectly.",
                            "target_lang_translations": {
                                "ru": "Эти простыни идеально подходят для <w>односпальной кровати</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "twin bed",
                        "twin mattress",
                        "twin size"
                    ],
                    "target_lang_definitions": {
                        "ru": "Узкая кровать, предназначенная для одного человека, или матрас такого размера."

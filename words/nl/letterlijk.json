                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "literally",
                                "target_lang_sense_clarification": "in a literal manner or sense"
                            },
                            {
                                "target_lang_word": "word for word",
                                "target_lang_sense_clarification": "verbatim"

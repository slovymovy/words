{
    "word_family": [
        "apprehend",
        "apprehension",
        "apprehensive",
        "misapprehension",
        "reapprehend"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7e560f61-5e69-3162-be72-86d22110cd1a",
                    "sense_definition": "The act of arresting or capturing someone again, usually after an escape.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "capture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police issued a warrant for the immediate <w>reapprehension</w> of the escaped convict.",
                            "target_lang_translations": {
                                "ru": "Полиция выдала ордер на немедленный <w>повторный арест</w> сбежавшего осужденного."
                            }
                        },
                        {
                            "text": "His <w>reapprehension</w> occurred only two days after he fled from custody.",
                            "target_lang_translations": {
                                "ru": "Его <w>повторное задержание</w> произошло всего через два дня после того, как он сбежал из-под стражи."
                            }
                        }
                    ],
                    "synonyms": [
                        "rearrest",
                        "recapture"
                    ],
                    "antonyms": [
                        "release",
                        "liberation"
                    ],
                    "common_phrases": [
                        "warrant for reapprehension",
                        "avoid reapprehension"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по повторному аресту или поимке кого-либо, как правило, после побега."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "повторный арест",
                                "target_lang_sense_clarification": "юридический термин"
                            },
                            {
                                "target_lang_word": "повторное задержание",
                                "target_lang_sense_clarification": "действие правоохранительных органов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92234fab-913a-310e-9d49-8760f7fd6b7a",
                    "sense_definition": "A new or renewed understanding of a concept, situation, or idea.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "understanding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The philosopher's latest book offers a <w>reapprehension</w> of ancient ethical theories.",
                            "target_lang_translations": {
                                "ru": "Последняя книга философа предлагает <w>переосмысление</w> древних этических теорий."
                            }
                        },
                        {
                            "text": "After years of study, she arrived at a <w>reapprehension</w> of the complex historical events.",
                            "target_lang_translations": {
                                "ru": "После многих лет исследований она пришла к <w>новому пониманию</w> сложных исторических событий."
                            }
                        }
                    ],
                    "synonyms": [
                        "reconsideration",
                        "reinterpretation",
                        "review"
                    ],
                    "antonyms": [
                        "misunderstanding"
                    ],
                    "common_phrases": [
                        "reapprehension of reality",
                        "sudden reapprehension"
                    ],
                    "target_lang_definitions": {
                        "ru": "Новое или обновленное понимание концепции, ситуации или идеи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переосмысление",
                                "target_lang_sense_clarification": "интеллектуальный процесс"
                            },
                            {
                                "target_lang_word": "новое понимание",
                                "target_lang_sense_clarification": "результат размышлений"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
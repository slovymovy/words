    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cf5d14e0-9ef2-3d0c-b965-7cc4072f0099",
                    "sense_definition": "Mocno chwycić i ścisnąć dłoń innej osoby, zazwyczaj jako gest powitania, pożegnania, gratulacji lub zawarcia umowy.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "gestures",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chwyciła go za rękę i <w>uścisnęła</w>, uśmiechając się przy tym ciepło.",
                            "target_lang_translations": {
                                "ru": "Она взяла его за руку и <w>сжала</w> её, тепло улыбаясь при этом."
                            }
                        },
                        {
                            "text": "Prezydent <w>uścisnął</w> dłoń każdemu z zaproszonych na uroczystość gości.",
                            "target_lang_translations": {
                                "ru": "Президент <w>пожал</w> руку каждому из приглашенных на торжество гостей."
                            }
                        }
                    ],
                    "synonyms": [
                        "ścisnąć"
                    ],
                    "common_phrases": [
                        "uścisnąć dłoń",
                        "uścisnąć rękę",
                        "mocno uścisnąć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крепко взять и сжать ладонь другого человека, обычно в качестве жеста приветствия, прощания, поздравления или заключения договора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пожать",
                                "target_lang_sense_clarification": "руку (приветствие, прощание)"
                            },
                            {
                                "target_lang_word": "сжать",
                                "target_lang_sense_clarification": "крепко сдавить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20d24381-3bdd-3e0b-9333-97b724bf1402",
                    "sense_definition": "Objąć kogoś ramionami, przytulić w celu wyrażenia sympatii, miłości lub pocieszenia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "affection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Matka <w>uścisnęła</w> córkę na pożegnanie przed jej długim wyjazdem.",
                            "target_lang_translations": {
                                "ru": "Мать <w>обняла</w> дочь на прощание перед её долгим отъездом."
                            }
                        },
                        {
                            "text": "Chciałbym cię serdecznie <w>uścisnąć</w> i podziękować za twoją pomoc.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы тебя сердечно <w>обнять</w> и поблагодарить за твою помощь."
                            }
                        }
                    ],
                    "synonyms": [
                        "przytulić",
                        "objąć",
                        "uściskać"
                    ],
                    "antonyms": [
                        "odepchnąć"
                    ],
                    "common_phrases": [
                        "uścisnąć serdecznie",
                        "uścisnąć na pożegnanie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обхватить кого-либо руками, прижать к себе для выражения симпатии, любви или утешения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обнять",
                                "target_lang_sense_clarification": "заключить в объятия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
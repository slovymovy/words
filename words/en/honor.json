                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "context of card games like bridge"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старшая карта (туз, король, дама или валет) в бридже и других карточных играх."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "онер",
                                "target_lang_sense_clarification": "карточный термин"
                            },
                            {
                                "target_lang_word": "старшая карта",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d98bdec3-c763-39f6-850c-28eb7b385858",
                    "sense_definition": "To show great respect or public admiration for someone.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_respect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We gather today to <w>honor</w> the memory of our ancestors.",

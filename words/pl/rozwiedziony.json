                    "sense_id": "23ead850-ccb1-3108-a2bd-6eb781397313",
                    "sense_definition": "Taki, którego małżeństwo zostało oficjalnie rozwiązane przez sąd.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "marital_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój sąsiad jest <w>rozwiedziony</w> i mieszka teraz sam.",
                            "target_lang_translations": {
                                "ru": "Мой сосед <w>разведён</w> и живёт теперь один."
                            }
                        },
                        {
                            "text": "Każdy świeżo <w>rozwiedziony</w> człowiek potrzebuje czasu dla siebie.",
                            "target_lang_translations": {
                                "ru": "Каждому недавно <w>разведённому</w> человеку нужно время для себя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "świeżo rozwiedziony",
                        "prawnie rozwiedziony",
                        "rozwiedziony mężczyzna",
                        "rozwiedziony rodzic"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, чей брак был официально расторгнут судом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разведённый",
                                "target_lang_sense_clarification": "семейное положение"
                            }
                        ]
                    }
                }
            ]
        },
        {

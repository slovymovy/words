    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "78f01a80-3104-341c-a165-688ac0dee093",
                    "sense_definition": "To break something into small pieces or powder by crushing it.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_crush",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should <w>grind</w> the coffee beans just before brewing for the best flavor.",
                            "target_lang_translations": {
                                "ru": "Вам следует <w>помолоть</w> кофейные зерна прямо перед варкой для лучшего вкуса."
                            }
                        },
                        {
                            "text": "The ancient mill was used to <w>grind</w> wheat into flour for the village.",
                            "target_lang_translations": {
                                "ru": "Древняя мельница использовалась, чтобы <w>молоть</w> пшеницу в муку для всей деревни."
                            }
                        }
                    ],
                    "synonyms": [
                        "chore",
                        "swot",
                        "grindadráp"
                    ],
                    "common_phrases": [
                        "grind coffee",
                        "grind corn",
                        "grind pepper"
                    ],
                    "target_lang_definitions": {
                        "ru": "Размельчать что-либо на мелкие кусочки или порошок путем давления или дробления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молоть",
                                "target_lang_sense_clarification": "превращать в муку или порошок (кофе, зерно)"
                            },
                            {
                                "target_lang_word": "измельчать",
                                "target_lang_sense_clarification": "делать мелким"
                            },
                            {
                                "target_lang_word": "перемалывать",
                                "target_lang_sense_clarification": "тщательно молоть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ee9d607-5011-323d-bc23-acb7ecfe4356",
                    "sense_definition": "To rub two surfaces together harshly, often making an unpleasant noise.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "friction_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I often <w>grind</w> my teeth in my sleep when I am stressed.",
                            "target_lang_translations": {
                                "ru": "Я часто <w>скрежещу</w> зубами во сне, когда испытываю стресс."
                            }
                        },
                        {
                            "text": "The old truck's gears began to <w>grind</w> loudly as he shifted.",
                            "target_lang_translations": {
                                "ru": "Шестеренки старого грузовика начали громко <w>скрежетать</w>, когда он переключал передачу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "grind one's teeth",
                        "gears grind"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильно тереть поверхности друг о друга, часто издавая неприятный звук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скрежетать",
                                "target_lang_sense_clarification": "издавать резкий звук трения (зубами, металлом)"
                            },
                            {
                                "target_lang_word": "тереть",
                                "target_lang_sense_clarification": "с силой водить по поверхности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6f526f90-80b6-3e03-84ad-2056c7779510",
                    "sense_definition": "To make something smooth or sharp by rubbing it against a hard surface.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "sharpen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The blacksmith began to <w>grind</w> the sword to ensure it was razor-sharp.",
                            "target_lang_translations": {
                                "ru": "Кузнец начал <w>точить</w> меч, чтобы убедиться, что он острый как бритва."
                            }
                        },
                        {
                            "text": "He used a special tool to <w>grind</w> the rough edges off the glass.",
                            "target_lang_translations": {
                                "ru": "Он использовал специальный инструмент, чтобы <w>отшлифовать</w> неровные края стекла."
                            }
                        }
                    ],
                    "common_phrases": [
                        "grind a knife",
                        "grind metal",
                        "grind a lens"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-либо гладким или острым путем трения о твердую поверхность."

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3be3f266-39cd-310a-aa9f-ff69461497ae",
                    "sense_definition": "Posiłek wieczorny, ostatni posiłek w ciągu dnia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "meal_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na <kolację> zjadłem kanapkę z serem i pomidorem.",
                            "target_lang_translations": {
                                "ru": "На <ужин> я съел бутерброд с сыром и помидором."
                            }
                        },
                        {
                            "text": "Cała rodzina usiadła wspólnie do wigilijnej <kolacji>.",
                            "target_lang_translations": {
                                "ru": "Вся семья вместе села за рождественский <ужин>."
                            }
                        },
                        {
                            "text": "O której godzinie zazwyczaj jesz <kolację>?",
                            "target_lang_translations": {
                                "ru": "Во сколько ты обычно ешь <ужин>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "wieczerza",
                        "reg. śl. kolacyjo"
                    ],
                    "antonyms": [
                        "śniadanie"
                    ],
                    "common_phrases": [
                        "jeść kolację",
                        "przygotować kolację",
                        "zaprosić na kolację",
                        "kolacja wigilijna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вечерний приём пищи, последняя еда в течение дня."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ужин",
                                "target_lang_sense_clarification": "вечерняя еда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b1639a20-f5c7-3343-9119-414312789c90",
                    "sense_definition": "Uroczyste wieczorne przyjęcie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "meal_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Prezydent z małżonką ugoszczeni zostali <kolacją> w siedzibie nuncjatury w Warszawie.",
                            "target_lang_translations": {
                                "ru": "Президента с супругой угостили <ужином> в здании нунциатуры в Варшаве."
                            }
                        },
                        {
                            "text": "Firma zorganizowała uroczystą <kolację> dla swoich kluczowych klientów.",
                            "target_lang_translations": {
                                "ru": "Фирма организовала торжественный <ужин> для своих ключевых клиентов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "uroczysta kolacja",
                        "wydać kolację",
                        "kolacja pożegnalna",
                        "kolacja biznesowa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Торжественный вечерний приём."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ужин",
                                "target_lang_sense_clarification": "торжественный приём"
                            },
                            {
                                "target_lang_word": "званный ужин",
                                "target_lang_sense_clarification": "официальное мероприятие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cdea73ea-bc42-32e4-ac84-c23ff98f6282",
                    "sense_definition": "Zestawienie odpisu z oryginałem lub dwóch różnych tekstów w celu ich porównania.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Badacz przeprowadził szczegółową <kolację> rękopisów, aby znaleźć różnice w tekście.",
                            "target_lang_translations": {
                                "ru": "Исследователь провёл детальную <сверку> рукописей, чтобы найти различия в тексте."
                            }
                        },
                        {
                            "text": "Wymagana jest dokładna <kolacja> dokumentów przed ich ostatecznym zatwierdzeniem.",
                            "target_lang_translations": {
                                "ru": "Требуется тщательная <сверка> документов перед их окончательным утверждением."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dokonać kolacji tekstu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сопоставление копии с оригиналом или двух разных текстов для их сравнения."

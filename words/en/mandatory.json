                },
                {
                    "sense_id": "5c5a34d7-541e-3511-97bb-a2e850c2b654",
                    "sense_definition": "A specific obstacle or boundary in disc golf that a player must navigate around as required.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If you miss the <w>mandatory</w>, you must take a penalty stroke.",
                            "target_lang_translations": {

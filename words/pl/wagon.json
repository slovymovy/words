    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ebda00b2-459c-3616-8662-90d8ae80d538",
                    "sense_definition": "Pojazd szynowy, zazwyczaj bez własnego napędu, przeznaczony do przewozu pasażerów lub towarów.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "rail_transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pociąg składał się z lokomotywy i dziesięciu <wagonów>.",
                            "target_lang_translations": {
                                "ru": "Поезд состоял из локомотива и десяти <вагонов>."
                            }
                        },
                        {
                            "text": "Wsiadłem do ostatniego <wagonu>, ponieważ było tam najwięcej wolnych miejsc.",
                            "target_lang_translations": {
                                "ru": "Я сел в последний <вагон>, так как там было больше всего свободных мест."
                            }
                        }
                    ],
                    "antonyms": [
                        "lokomotywa"
                    ],
                    "common_phrases": [
                        "wagon pasażerski",
                        "wagon towarowy",
                        "wagon sypialny",
                        "wagon restauracyjny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рельсовое транспортное средство, обычно без собственного двигателя, предназначенное для перевозки пассажиров или грузов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вагон",
                                "target_lang_sense_clarification": "железнодорожный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c25c356e-e096-35c5-8a62-6d9da13abd9e",
                    "sense_definition": "Kryty pojazd kolejki linowej poruszający się nad ziemią.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "cable_transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Widok z <wagonu> kolejki linowej na ośnieżone szczyty był zachwycający.",
                            "target_lang_translations": {
                                "ru": "Вид из <вагончика> канатной дороги на заснеженные вершины был восхитительным."
                            }
                        },
                        {
                            "text": "Turyści musieli czekać w kolejce na kolejny <wagon>, aby zjechać na dół.",
                            "target_lang_translations": {
                                "ru": "Туристам пришлось ждать в очереди следующий <вагончик>, чтобы спуститься вниз."
                            }
                        }
                    ],
                    "synonyms": [
                        "gondola"
                    ],
                    "common_phrases": [
                        "wagon kolejki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крытое транспортное средство канатной дороги, движущееся над землей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вагончик",
                                "target_lang_sense_clarification": "канатной дороги"
                            },
                            {
                                "target_lang_word": "кабинка",
                                "target_lang_sense_clarification": "канатной дороги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8077693b-2568-3f9b-8055-9d1c3c9190c6",
                    "sense_definition": "Opakowanie zbiorcze towaru, najczęściej karton papierosów zawierający 10 paczek.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "packaging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek palił tak dużo, że kupował <wagon> papierosów tygodniowo.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка курил так много, что покупал <блок> сигарет в неделю."
                            }
                        },
                        {
                            "text": "W strefie bezcłowej można kupić <wagon> ulubionych papierosów znacznie taniej.",
                            "target_lang_translations": {
                                "ru": "В зоне дьюти-фри можно купить <блок> любимых сигарет значительно дешевле."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wagon papierosów"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Групповая упаковка товара, чаще всего картонная коробка сигарет, содержащая 10 пачек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "блок",
                                "target_lang_sense_clarification": "сигарет"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d1a31e83-4a7a-3fec-81db-bf115d9fd9db",
                    "sense_definition": "De gezamenlijke bemanning van een vliegtuig of schip",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De piloot en de rest van de <w>crew</w> heetten de passagiers welkom aan boord.",
                            "target_lang_translations": {
                                "ru": "Пилот и остальной <w>экипаж</w> приветствовали пассажиров на борту."
                            }
                        },
                        {
                            "text": "Na de lange vlucht ging de hele <w>crew</w> naar het hotel om uit te rusten.",
                            "target_lang_translations": {
                                "ru": "После долгого перелета весь <w>экипаж</w> отправился в отель отдыхать."
                            }
                        }
                    ],
                    "synonyms": [
                        "bemanning"
                    ],
                    "antonyms": [
                        "passagiers"
                    ],
                    "common_phrases": [
                        "de hele crew",
                        "lid van de crew"
                    ],
                    "target_lang_definitions": {
                        "ru": "Весь персонал, работающий на борту самолета или корабля"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экипаж",
                                "target_lang_sense_clarification": "самолета или судна"
                            },
                            {
                                "target_lang_word": "команда",
                                "target_lang_sense_clarification": "группа людей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad64efc4-d690-3f61-a6e7-0796819e21c6",
                    "sense_definition": "Het technische en ondersteunende personeel dat meewerkt aan een film- of televisieproductie, in tegenstelling tot de acteurs",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "entertainment_staff",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De regisseur bedankte de cast en <w>crew</w> voor hun inzet tijdens de opnames.",
                            "target_lang_translations": {
                                "ru": "Режиссер поблагодарил актеров и <w>съемочную группу</w> за их усилия во время съемок."
                            }
                        },
                        {
                            "text": "Zonder een ervaren technische <w>crew</w> is het onmogelijk om zo'n grote film te maken.",
                            "target_lang_translations": {
                                "ru": "Без опытной технической <w>команды</w> невозможно снять такой масштабный фильм."
                            }
                        }
                    ],
                    "synonyms": [
                        "filmploeg"
                    ],
                    "antonyms": [
                        "cast"
                    ],
                    "common_phrases": [
                        "cast en crew",
                        "technische crew"
                    ],
                    "target_lang_definitions": {
                        "ru": "Технический и вспомогательный персонал кино- или телепроизводства"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "съемочная группа",
                                "target_lang_sense_clarification": "в кино и на ТВ"
                            },
                            {
                                "target_lang_word": "команда",
                                "target_lang_sense_clarification": "коллектив"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aac62c50-5376-32e9-ba8d-9e0bed663998",
                    "sense_definition": "Een groep medewerkers die verantwoordelijk is voor de opbouw en afbraak van podia en techniek bij evenementen of concerten",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "entertainment_staff",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>crew</w> werkte de hele nacht door om het podium voor het festival op tijd klaar te krijgen.",
                            "target_lang_translations": {
                                "ru": "<w>Команда</w> работала всю ночь, чтобы вовремя подготовить сцену к фестивалю."
                            }
                        },
                        {
                            "text": "Als onderdeel van de backstage <w>crew</w> zorg je ervoor dat alle instrumenten goed staan.",
                            "target_lang_translations": {
                                "ru": "Как часть закулисной <w>команды</w>, ты следишь за тем, чтобы все инструменты стояли правильно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "backstage crew",
                        "lokale crew"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа сотрудников, отвечающая за техническое обеспечение мероприятий и концертов"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "техническая команда",
                                "target_lang_sense_clarification": "на мероприятиях"
                            },
                            {
                                "target_lang_word": "персонал",
                                "target_lang_sense_clarification": "обслуживающий"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
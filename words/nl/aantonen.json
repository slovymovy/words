            "senses": [
                {
                    "sense_id": "fbd402f4-890d-39e6-acac-a4994eae9b1e",
                    "sense_definition": "Met feiten, redeneringen of bewijsmateriaal vaststellen dat iets waar of juist is.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "bewijsvoering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De wetenschappers konden met dit experiment <w>aantonen</w> dat de theorie klopte.",
                            "target_lang_translations": {
                                "en": "The scientists were able to <w>prove</w> with this experiment that the theory was correct.",
                                "ru": "Ученые смогли с помощью этого эксперимента <w>доказать</w>, что теория была верна."
                            }
                        },
                        {
                            "text": "Het onderzoek moet <w>aantonen</w> of de nieuwe maatregelen effectief zijn.",
                            "target_lang_translations": {
                                "en": "The study must <w>demonstrate</w> whether the new measures are effective.",
                                "ru": "Исследование должно <w>продемонстрировать</w>, эффективны ли новые меры."
                            }
                        }
                    ],
                    "synonyms": [
                        "bewijzen"
                    ],
                    "antonyms": [
                        "weerleggen",
                        "ontkrachten"
                    ],
                    "common_phrases": [
                        "wetenschappelijk aantonen",
                        "duidelijk aantonen",
                        "verband aantonen",
                        "zwart op wit aantonen"
                    ],
                    "target_lang_definitions": {
                        "en": "To establish that something is true or correct using facts, reasoning, or evidence.",
                        "ru": "Устанавливать с помощью фактов, рассуждений или доказательств, что что-то является истинным или правильным."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "prove",
                                "target_lang_sense_clarification": "to establish the truth of"
                            },
                            {
                                "target_lang_word": "demonstrate",
                                "target_lang_sense_clarification": "to show clearly by evidence"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "доказывать",
                                "target_lang_sense_clarification": "подтверждать истинность фактами"
                            },
                            {
                                "target_lang_word": "демонстрировать",
                                "target_lang_sense_clarification": "наглядно показывать наличие чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1730e289-7899-3c89-88b3-db632498ce26",
                    "sense_definition": "Iets duidelijk maken of laten zien, vaak om inzicht te geven in de gevolgen of werking van iets.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "demonstratie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De praktijk zal <w>aantonen</w> wat de werkelijke gevolgen van deze beslissing zijn.",
                            "target_lang_translations": {
                                "en": "Practice will <w>show</w> what the actual consequences of this decision are.",
                                "ru": "Практика <w>покажет</w>, каковы истинные последствия этого решения."
                            }
                        },
                        {
                            "text": "Hij <w>toonde aan</w> dat er grote risico's verbonden waren aan het project.",
                            "target_lang_translations": {
                                "en": "He <w>demonstrated</w> that there were major risks associated with the project.",
                                "ru": "Он <w>показал</w>, что с проектом связаны большие риски."
                            }
                        }
                    ],
                    "synonyms": [
                        "bewijzen",
                        "bewijzen",
                        "bewijzen"
                    ],
                    "common_phrases": [
                        "gevolgen aantonen"
                    ],
                    "target_lang_definitions": {
                        "en": "To make something clear or visible, often to reveal consequences or effects.",
                        "ru": "Делать что-то ясным или очевидным, часто для того, чтобы показать последствия или принцип действия."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "show",

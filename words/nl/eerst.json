{
    "word_family": [
        "eerste",
        "allereerst",
        "vooreerst"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "526ff4c6-fdce-3733-8228-e66377c2e980",
                    "sense_definition": "Voordat iets anders gebeurt of als begin van een reeks handelingen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet <eerst> je handen wassen voordat je aan tafel gaat.",
                            "target_lang_translations": {
                                "ru": "Ты должен <w>сначала</w> вымыть руки, прежде чем садиться за стол.",
                                "en": "You must wash your hands <w>first</w> before going to the table."
                            }
                        },
                        {
                            "text": "We gaan <eerst> en vooral even rustig zitten.",
                            "target_lang_translations": {
                                "ru": "Мы <w>прежде всего</w> спокойно присядем.",
                                "en": "We are going to sit down quietly <w>first</w> of all."
                            }
                        }
                    ],
                    "antonyms": [
                        "laatst"
                    ],
                    "common_phrases": [
                        "eerst en vooral",
                        "wie het eerst komt",
                        "eerst zien dan geloven"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перед тем, как произойдет что-то другое, или как начало последовательности действий.",
                        "en": "Before something else happens or as the beginning of a series of actions."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сначала",
                                "target_lang_sense_clarification": "указывает на порядок действий"
                            },
                            {
                                "target_lang_word": "сперва",
                                "target_lang_sense_clarification": "разговорный вариант"
                            },
                            {
                                "target_lang_word": "прежде всего",
                                "target_lang_sense_clarification": "в первую очередь"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "first",
                                "target_lang_sense_clarification": "indicating order or priority"
                            },
                            {
                                "target_lang_word": "first of all",
                                "target_lang_sense_clarification": "emphasizing priority"
                            },
                            {
                                "target_lang_word": "firstly",
                                "target_lang_sense_clarification": "used in enumerations"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19ab819b-7ee7-3d1c-940d-e299806de3fe",
                    "sense_definition": "In het begin of in een eerdere periode, vaak in tegenstelling tot de huidige situatie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_past",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het deed <eerst> veel pijn, maar nu gaat het beter.",
                            "target_lang_translations": {
                                "ru": "<w>Сначала</w> было очень больно, но теперь стало лучше.",
                                "en": "It hurt a lot <w>at first</w>, but now it is better."
                            }
                        },
                        {
                            "text": "Zij woonden <eerst> in Amsterdam en verhuisden later naar Rotterdam.",
                            "target_lang_translations": {
                                "ru": "<w>Сначала</w> они жили в Амстердаме, а позже переехали в Роттердам.",
                                "en": "They lived in Amsterdam <w>at first</w> and later moved to Rotterdam."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В начале или в более ранний период, часто в отличие от текущей ситуации.",
                        "en": "In the beginning or during an earlier period, often in contrast to the current situation."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сначала",
                                "target_lang_sense_clarification": "в первое время"
                            },
                            {
                                "target_lang_word": "поначалу",
                                "target_lang_sense_clarification": "в начальный период"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "at first",
                                "target_lang_sense_clarification": "in the beginning"
                            },
                            {
                                "target_lang_word": "initially",
                                "target_lang_sense_clarification": "formal"
                            },
                            {
                                "target_lang_word": "originally",
                                "target_lang_sense_clarification": "referring to the origin"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ce6f614-3b95-338c-9460-5eb08a43b92a",
                    "sense_definition": "Niet eerder dan een bepaald moment; pas.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "not_until",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De trein naar Brussel vertrekt <eerst> over een uur.",
                            "target_lang_translations": {
                                "ru": "Поезд в Брюссель отправится <w>только</w> через час.",
                                "en": "The train to Brussels leaves <w>only</w> in an hour."
                            }
                        },
                        {
                            "text": "Hij kwam <eerst> om twee uur 's nachts thuis.",
                            "target_lang_translations": {
                                "ru": "Он пришел домой <w>только</w> в два часа ночи.",
                                "en": "He came home <w>only</w> at two o'clock at night."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in België"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Не раньше определенного момента; только.",
                        "en": "Not earlier than a specific moment; only."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "только",
                                "target_lang_sense_clarification": "в значении «не раньше чем»"
                            },
                            {
                                "target_lang_word": "лишь",
                                "target_lang_sense_clarification": "синоним «только»"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "only",
                                "target_lang_sense_clarification": "indicating a delay"
                            },
                            {
                                "target_lang_word": "not until",
                                "target_lang_sense_clarification": "indicating a time limit"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
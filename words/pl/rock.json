    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3bb4cd54-5c5f-3552-afe9-e4ec5e06e915",
                    "sense_definition": "Gatunek silnie rytmicznej muzyki rozrywkowej, granej głównie z użyciem instrumentów nagłośnionych elektrycznie, w szczególności gitar.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "music_genre",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój starszy brat całymi dniami słucha głośnego <w>rocka</w> w swoim pokoju.",
                            "target_lang_translations": {
                                "ru": "Мой старший брат целыми днями слушает громкий <w>рок</w> в своей комнате."
                            }
                        },
                        {
                            "text": "Ten znany zespół gra klasyczny <w>rock</w>, który przyciąga tłumy na koncerty.",
                            "target_lang_translations": {
                                "ru": "Эта известная группа играет классический <w>рок</w>, который привлекает толпы на концерты."
                            }
                        }
                    ],
                    "synonyms": [
                        "muzyka rockowa"
                    ],
                    "common_phrases": [
                        "gwiazda rocka",
                        "zespół rockowy",
                        "słuchać rocka",
                        "grać rocka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жанр ритмичной популярной музыки, исполняемой преимущественно на электроинструментах, в частности на гитарах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рок",
                                "target_lang_sense_clarification": "музыкальный жанр"
                            },
                            {
                                "target_lang_word": "рок-музыка",
                                "target_lang_sense_clarification": "синоним"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bef565c5-0aa6-3fd6-9ab7-3c10bc14cca7",
                    "sense_definition": "Skrócona nazwa stylu muzycznego lub tańca <w>rock and roll</w>.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "music_genre",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na weselu wszyscy goście chętnie tańczyli szybkiego <w>rocka</w>.",
                            "target_lang_translations": {
                                "ru": "На свадьбе все гости охотно танцевали быстрый <w>рок-н-ролл</w>."
                            }
                        },
                        {
                            "text": "To był stary, dobry <w>rock</w>, przy którym nikt nie mógł usiedzieć w miejscu.",
                            "target_lang_translations": {
                                "ru": "Это был старый добрый <w>рок</w>, под который никто не мог усидеть на месте."
                            }
                        }
                    ],
                    "synonyms": [
                        "rock and roll"
                    ],
                    "common_phrases": [
                        "tańczyć rocka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращенное название музыкального стиля или танца рок-н-ролл."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рок-н-ролл",
                                "target_lang_sense_clarification": "танец или стиль"
                            },
                            {
                                "target_lang_word": "рок",
                                "target_lang_sense_clarification": "разговорное сокращение"
                            }
                        ]
                    }
                }
            ]
        }

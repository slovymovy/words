{
    "word_family": [
        "goed",
        "beter",
        "opperbest",
        "bestwil"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "1881fcee-1311-3b5a-bad1-b0b13bb85bec",
                    "sense_definition": "De overtreffende trap van <w>goed</w>; beter dan alle anderen.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "superlative_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is het <w>beste</w> boek dat ik in jaren heb gelezen.",
                            "target_lang_translations": {
                                "ru": "Это <w>лучшая</w> книга, которую я читал за последние годы."
                            }
                        },
                        {
                            "text": "Zij is de <w>beste</w> leerling van de hele school.",
                            "target_lang_translations": {
                                "ru": "Она <w>лучшая</w> ученица во всей школе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de beste wensen",
                        "het beste",
                        "beste vriend"
                    ],
                    "target_lang_definitions": {
                        "ru": "Превосходная степень от \"хороший\"; лучше всех остальных."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лучший",
                                "target_lang_sense_clarification": "превосходная степень"
                            },
                            {
                                "target_lang_word": "самый лучший",
                                "target_lang_sense_clarification": "усиленная форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "35dfba30-4d39-380f-be20-f79f363aac08",
                    "sense_definition": "Een beleefde en vriendelijke aanhef, meestal gebruikt in brieven of e-mails.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "salutation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Beste</w> meneer Jansen, bedankt voor uw snelle reactie.",
                            "target_lang_translations": {
                                "ru": "<w>Уважаемый</w> господин Янсен, спасибо за ваш быстрый ответ."
                            }
                        },
                        {
                            "text": "<w>Beste</w> collega's, morgen hebben we een belangrijke vergadering.",
                            "target_lang_translations": {
                                "ru": "<w>Дорогие</w> коллеги, завтра у нас важное собрание."
                            }
                        }
                    ],
                    "common_phrases": [
                        "beste mensen",
                        "mijn beste"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вежливое и дружелюбное обращение, обычно используемое в письмах или электронных сообщениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дорогой",
                                "target_lang_sense_clarification": "неформальное или полуформальное обращение"
                            },
                            {
                                "target_lang_word": "уважаемый",
                                "target_lang_sense_clarification": "формальное обращение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dfc2572b-f661-3c0e-a641-971735646009",
                    "sense_definition": "Goed of acceptabel; wordt gebruikt om instemming of goedkeuring te geven.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "agreement_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik vind het <w>best</w> als je vanavond wat later thuiskomt.",
                            "target_lang_translations": {
                                "ru": "Я <w>не против</w>, если ты сегодня придешь домой попозже."
                            }
                        },
                        {
                            "text": "Is het <w>best</w> als ik hier even ga zitten?",
                            "target_lang_translations": {
                                "ru": "<w>Ничего</w>, если я здесь присяду?"
                            }
                        }
                    ],
                    "synonyms": [
                        "prima"
                    ],
                    "common_phrases": [
                        "ik vind het best",
                        "alles best"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хорошо или приемлемо; используется для выражения согласия или одобрения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хорошо",
                                "target_lang_sense_clarification": "выражение согласия"
                            },
                            {
                                "target_lang_word": "ладно",
                                "target_lang_sense_clarification": "разговорное согласие"
                            },
                            {
                                "target_lang_word": "не против",
                                "target_lang_sense_clarification": "отсутствие возражений"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "7d4db445-d8b8-3780-a66d-88b7c8100ce7",
                    "sense_definition": "Op de allerbeste manier; de overtreffende trap van <w>goed</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "superlative_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik weet niet wie van mijn vrienden het <w>best</w> kan koken.",
                            "target_lang_translations": {
                                "ru": "Я не знаю, кто из моих друзей готовит <w>лучше всех</w>."
                            }
                        },
                        {
                            "text": "Deze jas past mij het <w>best</w> van allemaal.",
                            "target_lang_translations": {
                                "ru": "Эта куртка сидит на мне <w>лучше всего</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het best",
                        "op zijn best"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наилучшим образом; превосходная степень от \"хорошо\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лучше всего",
                                "target_lang_sense_clarification": "о предметах или действиях"
                            },
                            {
                                "target_lang_word": "лучше всех",
                                "target_lang_sense_clarification": "о людях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "95c71467-79a2-3165-873a-286db9ecc031",
                    "sense_definition": "In redelijke mate; tamelijk of behoorlijk.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "degree_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is vandaag <w>best</w> koud voor de tijd van het jaar.",
                            "target_lang_translations": {
                                "ru": "Сегодня <w>довольно</w> холодно для этого времени года."
                            }
                        },
                        {
                            "text": "Dat zou <w>best</w> kunnen kloppen wat je daar zegt.",
                            "target_lang_translations": {
                                "ru": "То, что ты говоришь, <w>вполне</w> может быть правдой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "best wel",
                        "best mogelijk"
                    ],
                    "target_lang_definitions": {
                        "ru": "В значительной степени; довольно или вполне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "довольно",
                                "target_lang_sense_clarification": "в достаточной степени"
                            },
                            {
                                "target_lang_word": "вполне",
                                "target_lang_sense_clarification": "полностью, совершенно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ba06412f-af72-3d05-863a-52f6559f720b",
                    "sense_definition": "De grootst mogelijke inspanning die iemand kan leveren.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "effort_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik zal mijn <w>best</w> doen om de opdracht op tijd af te krijgen.",
                            "target_lang_translations": {
                                "ru": "Я сделаю <w>всё возможное</w>, чтобы закончить задание вовремя."
                            }
                        },
                        {
                            "text": "Ondanks dat iedereen zijn <w>best</w> had gedaan, verloren ze de wedstrijd.",
                            "target_lang_translations": {
                                "ru": "Несмотря на то, что все приложили <w>максимум усилий</w>, они проиграли матч."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zijn best doen",
                        "mijn best doen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Максимально возможное усилие, которое кто-то может приложить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "всё возможное",
                                "target_lang_sense_clarification": "максимальные усилия"
                            },
                            {
                                "target_lang_word": "максимум",
                                "target_lang_sense_clarification": "предел возможностей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb0bf690-ffda-387d-89f0-c8795a23fffb",
                    "sense_definition": "Een oudere vrouw.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "person_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude <w>best</w> zat stilletjes bij de haard te breien.",
                            "target_lang_translations": {
                                "ru": "Старая <w>бабушка</w> тихо сидела у камина и вязала."
                            }
                        },
                        {
                            "text": "Vroeger noemde men een grootmoeder soms liefkozend een <w>best</w>.",
                            "target_lang_translations": {
                                "ru": "Раньше бабушку иногда ласково называли <w>старушкой</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "wordt in modern Nederlands zelden meer gebruikt"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пожилая женщина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "старушка",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "бабушка",
                                "target_lang_sense_clarification": "пожилая женщина"
                            }
                        ]
                    }
                }
            ]
        },
        {

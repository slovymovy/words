                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перелётный",
                                "target_lang_sense_clarification": "о ловчей птице"
                            },
                            {
                                "target_lang_word": "дикий",
                                "target_lang_sense_clarification": "не выращенный в неволе"
                            }
                        ]
                    }
                }
            ]
        },
        {

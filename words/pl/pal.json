                    "sense_definition": "Komenda wojskowa wydawana żołnierzom, nakazująca oddanie strzału z broni palnej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Oficer krzyknął głośno: cel, <pal>!",
                            "target_lang_translations": {
                                "ru": "Офицер громко крикнул: цельсь, <пли>!"
                            }
                        },
                        {
                            "text": "Na komendę <pal> wszyscy żołnierze wystrzelili jednocześnie.",
                            "target_lang_translations": {
                                "ru": "По команде «<пли>» все солдаты выстрелили одновременно."
                            }
                        }
                    ],
                    "synonyms": [
                        "ognia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Военная команда для открытия огня из огнестрельного оружия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пли",
                                "target_lang_sense_clarification": "команда"
                            },
                            {
                                "target_lang_word": "огонь",
                                "target_lang_sense_clarification": "команда"
                            }
                        ]
                    }

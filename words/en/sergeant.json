    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "257bf5bd-695d-32d4-bfcc-0cf048ad2245",
                    "sense_definition": "A non-commissioned officer in the army, air force, or marine corps, ranking above a corporal; also a police officer ranking just below a lieutenant or inspector.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>sergeant</w> ordered the platoon to march forward immediately.",
                            "target_lang_translations": {
                                "ru": "<w>Сержант</w> приказал взводу немедленно выдвигаться вперёд."
                            }
                        },
                        {
                            "text": "She was promoted to police <w>sergeant</w> after five years of dedicated service.",
                            "target_lang_translations": {
                                "ru": "Она получила звание <w>сержанта</w> полиции после пяти лет преданной службы."
                            }
                        }
                    ],
                    "synonyms": [
                        "sarge",
                        "sarnt"
                    ],
                    "common_phrases": [
                        "drill sergeant",
                        "police sergeant",
                        "staff sergeant",
                        "sergeant major"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звание младшего командного состава в армии, ВВС или морской пехоте, выше капрала; также офицер полиции рангом ниже лейтенанта или инспектора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сержант",
                                "target_lang_sense_clarification": "воинское или полицейское звание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a30b0fa3-7e4b-357f-802e-18d297809300",
                    "sense_definition": "A lawyer of the highest rank in former times, historically having exclusive rights to practice in certain courts.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "legal_historical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the medieval court, the <w>sergeant</w> presented the argument with great skill.",
                            "target_lang_translations": {
                                "ru": "В средневековом суде <w>серджент</w> с большим мастерством представил свои доводы."
                            }
                        },
                        {
                            "text": "The title of <w>sergeant</w> at law is no longer used in the modern legal system.",
                            "target_lang_translations": {
                                "ru": "Титул <w>серджента</w> права больше не используется в современной правовой системе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sergeant at law"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historical legal title"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Юрист высшего ранга в прежние времена, исторически имевший исключительные права на практику в определенных судах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "серджент",
                                "target_lang_sense_clarification": "исторический юридический титул"
                            },
                            {
                                "target_lang_word": "сержант права",
                                "target_lang_sense_clarification": "устаревший юридический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {

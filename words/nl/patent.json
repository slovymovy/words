                        "ru": [
                            {
                                "target_lang_word": "патент",
                                "target_lang_sense_clarification": "документ на изобретение"
                            },
                            {
                                "target_lang_word": "авторское свидетельство",
                                "target_lang_sense_clarification": "устаревший термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d280410b-dfd2-355d-a6b2-032739f88270",
                    "sense_definition": "Wettelijke verklaring dat men een beroep, bedrijf of handwerk mag uitvoeren.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal_permission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De handelaar moest zijn <w>patent</w> tonen aan de controleur om te bewijzen dat hij mocht verkopen.",
                            "target_lang_translations": {
                                "en": "The trader had to show his <w>license</w> to the inspector to prove he was allowed to sell.",
                                "ru": "Торговец должен был показать свой <w>патент</w> инспектору, чтобы доказать право на продажу."
                            }
                        },
                        {
                            "text": "Zonder het juiste <w>patent</w> was het in die tijd verboden om een ambacht uit te oefenen.",
                            "target_lang_translations": {
                                "en": "Without the proper <w>license</w>, it was forbidden to practice a craft in those days.",
                                "ru": "Без надлежащего <w>патента</w> в то время было запрещено заниматься ремеслом."
                            }
                        }
                    ],
                    "synonyms": [
                        "vergunning",
                        "licentie"

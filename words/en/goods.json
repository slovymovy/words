                        {
                            "text": "Philosophy often distinguishes between material and spiritual <w>goods</w>.",
                            "target_lang_translations": {
                                "ru": "Философия часто различает материальные и духовные <w>блага</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "public goods"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {

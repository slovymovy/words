{
    "word_family": [
        "caller",
        "calling",
        "recall",
        "miscall",
        "overcall",
        "outcall"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "807f3ce0-3e32-3614-819d-e372300c715e",
                    "sense_definition": "To give a name to someone or something; to address or refer to someone by a specific name.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "naming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>call</w> me Sarah.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>зовите</w> меня Сара."
                            }
                        },
                        {
                            "text": "They decided to <w>call</w> their new puppy Max.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>назвать</w> своего нового щенка Макс."
                            }
                        },
                        {
                            "text": "What do you <w>call</w> this flower in English?",
                            "target_lang_translations": {
                                "ru": "Как вы <w>называете</w> этот цветок по-английски?"
                            }
                        }
                    ],
                    "synonyms": [
                        "name",
                        "dub",
                        "title",
                        "designate"
                    ],
                    "common_phrases": [
                        "call by name",
                        "be called",
                        "what do you call"
                    ],
                    "target_lang_definitions": {
                        "ru": "Давать имя кому-либо или чему-либо; обращаться к кому-либо по имени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "называть",
                                "target_lang_sense_clarification": "давать имя или характеризовать"
                            },
                            {
                                "target_lang_word": "звать",
                                "target_lang_sense_clarification": "обращаться по имени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69c29ee3-cfe1-34d5-a2a2-a5217de454d5",
                    "sense_definition": "To contact or attempt to contact someone by telephone.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "telephony",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will <w>call</w> you when I arrive at the station.",
                            "target_lang_translations": {
                                "ru": "Я <w>позвоню</w> тебе, когда приеду на станцию."
                            }
                        },
                        {
                            "text": "She <w>called</w> her mother to wish her a happy birthday.",
                            "target_lang_translations": {
                                "ru": "Она <w>позвонила</w> маме, чтобы поздравить её с днём рождения."
                            }
                        }
                    ],
                    "synonyms": [
                        "phone",
                        "ring",
                        "telephone"
                    ],
                    "common_phrases": [
                        "call back",
                        "call up",
                        "give someone a call"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связываться или пытаться связаться с кем-либо по телефону."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звонить",
                                "target_lang_sense_clarification": "по телефону"
                            },
                            {
                                "target_lang_word": "позвонить",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18711f6c-32e5-32cc-9eea-50bf6c5d4f0d",
                    "sense_definition": "To speak or shout in a loud voice, often to attract attention or to be heard from a distance.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "vocalization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>called</w> out to his friend across the street.",
                            "target_lang_translations": {
                                "ru": "Он <w>крикнул</w> своему другу через улицу."
                            }
                        },
                        {
                            "text": "If you need any help, just <w>call</w>.",
                            "target_lang_translations": {
                                "ru": "Если тебе понадобится помощь, просто <w>позови</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "shout",
                        "yell",
                        "cry",
                        "holler"
                    ],
                    "antonyms": [
                        "whisper",
                        "mutter"
                    ],
                    "common_phrases": [
                        "call out",
                        "call for help",
                        "call aloud"
                    ],
                    "target_lang_definitions": {
                        "ru": "Говорить или кричать громким голосом, часто чтобы привлечь внимание."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кричать",
                                "target_lang_sense_clarification": "громко говорить"
                            },
                            {
                                "target_lang_word": "звать",
                                "target_lang_sense_clarification": "призывать громким голосом"
                            },
                            {
                                "target_lang_word": "позвать",
                                "target_lang_sense_clarification": "окликнуть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "16c64848-42c6-3864-ae6e-d4e8323bbd50",
                    "sense_definition": "To ask, order, or invite someone to come to a particular place.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "summoning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The manager <w>called</w> the employee into his office for a meeting.",
                            "target_lang_translations": {
                                "ru": "Менеджер <w>вызвал</w> сотрудника в свой кабинет на совещание."
                            }
                        },
                        {
                            "text": "We had to <w>call</w> a plumber to fix the leaking pipe.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось <w>вызвать</w> сантехника, чтобы починить протекающую трубу."
                            }
                        }
                    ],
                    "synonyms": [
                        "summon",
                        "send for"
                    ],
                    "antonyms": [
                        "dismiss",
                        "send away"
                    ],
                    "common_phrases": [
                        "call a meeting",
                        "call the police",
                        "call a doctor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Просить, приказывать или приглашать кого-либо прийти в определённое место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вызывать",
                                "target_lang_sense_clarification": "официально приглашать или требовать явиться"
                            },
                            {
                                "target_lang_word": "позвать",
                                "target_lang_sense_clarification": "попросить прийти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f82053e4-d97b-33d5-a724-0a90588e2fbc",
                    "sense_definition": "To make a short visit to a person or place.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "visiting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We plan to <w>call</w> on our grandmother this weekend.",
                            "target_lang_translations": {
                                "ru": "Мы планируем <w>навестить</w> нашу бабушку в эти выходные."
                            }
                        },
                        {
                            "text": "The train <w>calls</w> at every station on this line.",
                            "target_lang_translations": {
                                "ru": "Поезд <w>останавливается</w> на каждой станции этой линии."
                            }
                        }
                    ],
                    "synonyms": [
                        "visit",
                        "stop by",
                        "drop by"
                    ],
                    "common_phrases": [
                        "call on",
                        "call at"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нанести короткий визит человеку или посетить место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "навещать",
                                "target_lang_sense_clarification": "приходить в гости"
                            },
                            {
                                "target_lang_word": "посещать",
                                "target_lang_sense_clarification": "бывать где-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3bd82e01-5df5-304b-8ec7-482009c051a1",
                    "sense_definition": "To consider, describe, or characterize something as being of a particular type or quality.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "judgement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I wouldn't <w>call</w> him a friend, just an acquaintance.",
                            "target_lang_translations": {
                                "ru": "Я бы не <w>назвал</w> его другом, просто знакомым."
                            }
                        },
                        {
                            "text": "Let's <w>call</w> it a day and go home.",
                            "target_lang_translations": {
                                "ru": "Давайте <w>закончим</w> на сегодня и пойдём домой."
                            }
                        }
                    ],
                    "synonyms": [
                        "describe",
                        "label",
                        "consider",
                        "term"
                    ],
                    "common_phrases": [
                        "call it a day",
                        "call it quits"
                    ],
                    "target_lang_definitions": {
                        "ru": "Считать, описывать или характеризовать что-либо определённым образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "называть",
                                "target_lang_sense_clarification": "давать определение или характеристику"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8dde1308-d6e9-3208-8b91-a1af7a9f76f5",
                    "sense_definition": "To make a formal decision, declaration, or prediction about an event or result.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "judgement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The referee <w>called</w> a foul against the defending team.",
                            "target_lang_translations": {
                                "ru": "Судья <w>зафиксировал</w> нарушение у защищающейся команды."
                            }
                        },
                        {
                            "text": "It is too early to <w>call</w> the results of the election.",
                            "target_lang_translations": {
                                "ru": "Слишком рано <w>объявлять</w> результаты выборов."
                            }
                        }
                    ],
                    "synonyms": [
                        "declare",
                        "rule",
                        "announce",
                        "predict"
                    ],
                    "common_phrases": [
                        "call the shots",
                        "call an election"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать официальное решение, делать заявление или прогноз о событии."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "99059de7-7f70-3aaa-ad65-3fdc459ff313",
                    "sense_definition": "An act of speaking to someone on the telephone or attempting to contact them.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "telephony",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to make a quick phone <w>call</w>.",
                            "target_lang_translations": {
                                "ru": "Мне нужно сделать быстрый телефонный <w>звонок</w>."
                            }
                        },
                        {
                            "text": "She missed a <w>call</w> from her boss this morning.",
                            "target_lang_translations": {
                                "ru": "Она пропустила <w>звонок</w> от начальника сегодня утром."
                            }
                        }
                    ],
                    "synonyms": [
                        "phone call",
                        "ring"
                    ],
                    "common_phrases": [
                        "make a call",
                        "take a call",
                        "missed call",
                        "give someone a call"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговор по телефону или попытка связаться с кем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звонок",
                                "target_lang_sense_clarification": "телефонный вызов"
                            },
                            {
                                "target_lang_word": "разговор",
                                "target_lang_sense_clarification": "беседа по телефону"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21e49b16-8b28-3c75-9a36-54d0839cde8b",
                    "sense_definition": "A loud cry or shout; the characteristic sound made by a bird or animal.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "vocalization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We heard the <w>call</w> of a wild bird in the forest.",
                            "target_lang_translations": {
                                "ru": "Мы услышали <w>крик</w> дикой птицы в лесу."
                            }
                        },
                        {
                            "text": "He gave a loud <w>call</w> to warn the others of the danger.",
                            "target_lang_translations": {
                                "ru": "Он издал громкий <w>клич</w>, чтобы предупредить остальных об опасности."
                            }
                        }
                    ],
                    "synonyms": [
                        "cry",
                        "shout",
                        "yell",
                        "scream"
                    ],
                    "antonyms": [
                        "whisper"
                    ],
                    "common_phrases": [
                        "bird call",
                        "distress call",
                        "wake-up call"
                    ],
                    "target_lang_definitions": {
                        "ru": "Громкий крик или возглас; характерный звук, издаваемый птицей или животным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крик",
                                "target_lang_sense_clarification": "громкий звук голоса"
                            },
                            {
                                "target_lang_word": "зов",
                                "target_lang_sense_clarification": "призыв"
                            },
                            {
                                "target_lang_word": "клич",
                                "target_lang_sense_clarification": "громкий возглас"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dacee4e8-58f5-3c9e-9284-844914f25596",
                    "sense_definition": "A decision or judgement, often made quickly or in sports.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "judgement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "That was a good <w>call</w> to bring umbrellas today.",
                            "target_lang_translations": {
                                "ru": "Это было хорошее <w>решение</w> взять сегодня зонты."
                            }
                        },
                        {
                            "text": "It was a tough <w>call</w>, but we decided to cancel the event.",
                            "target_lang_translations": {
                                "ru": "Это был трудный <w>выбор</w>, но мы решили отменить мероприятие."
                            }
                        }
                    ],
                    "synonyms": [
                        "decision",
                        "judgement",
                        "ruling"
                    ],
                    "common_phrases": [
                        "good call",
                        "bad call",
                        "tough call"
                    ],
                    "target_lang_definitions": {
                        "ru": "Решение или суждение, часто принимаемое быстро или в спорте."
                    },
                    "translations": {
                        "ru": []
                    }
                },
                {
                    "sense_id": "663e32db-481b-36f8-905c-7bf9f156766e",
                    "sense_definition": "A period of being available to work if required, especially for doctors or emergency workers.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The surgeon is on <w>call</w> this weekend.",
                            "target_lang_translations": {
                                "ru": "Хирург на <w>дежурстве</w> в эти выходные."
                            }
                        },
                        {
                            "text": "I hate being on <w>call</w> during the holidays because I can't travel.",
                            "target_lang_translations": {
                                "ru": "Я ненавижу быть на <w>дежурстве</w> во время праздников, потому что не могу путешествовать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "on call"
                    ],
                    "target_lang_definitions": {
                        "ru": "Период доступности для работы по требованию, особенно для врачей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дежурство",
                                "target_lang_sense_clarification": "рабочая смена по вызову"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f175f7bb-1c06-342c-bb96-4ce8befcacf0",
                    "sense_definition": "A need or reason for a particular action, usually used in negative contexts.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "necessity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is no <w>call</w> for such rude behavior.",
                            "target_lang_translations": {
                                "ru": "Нет <w>причин</w> для такого грубого поведения."
                            }
                        },
                        {
                            "text": "I don't see the <w>call</w> for buying such expensive equipment.",
                            "target_lang_translations": {
                                "ru": "Я не вижу <w>необходимости</w> покупать такое дорогое оборудование."
                            }
                        }
                    ],
                    "synonyms": [
                        "need",
                        "necessity",
                        "justification",
                        "reason"
                    ],
                    "common_phrases": [
                        "no call for"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нужда или причина для определённого действия, обычно используется в отрицательном контексте."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "479f66a6-2c88-37b4-98ba-c202b15be7e0",
                    "sense_definition": "A surname.",
                    "learner_level": "A1",
                    "frequency": "Low",
                    "semantic_group_id": "naming",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Call</w> signed the documents.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Колл</w> подписал документы."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": "Used as a family name."
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": []
                    }
                }
            ]
        }
    ]
}
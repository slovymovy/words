            "senses": [
                {
                    "sense_id": "00f4cdb5-ab07-39a7-ac22-f72f9b3ebfad",
                    "sense_definition": "Lacking a specific physical or mental ability; often used in compound terms to describe disabilities politely.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "disability_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The organization raises money to support physically <w>challenged</w> athletes.",
                            "target_lang_translations": {
                                "ru": "Организация собирает деньги для поддержки спортсменов с физическими <w>нарушениями</w>."
                            }
                        },
                        {
                            "text": "Teachers use specialized methods for mentally <w>challenged</w> students.",
                            "target_lang_translations": {
                                "ru": "Учителя используют специальные методики для учеников с ментальными <w>нарушениями</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "disabled",
                        "impaired"
                    ],
                    "antonyms": [
                        "able-bodied"
                    ],
                    "common_phrases": [
                        "mentally challenged",
                        "physically challenged",
                        "learning challenged"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишенный определенных физических или умственных способностей; часто используется в составных терминах для вежливого описания инвалидности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с ограниченными возможностями",
                                "target_lang_sense_clarification": "политкорректный термин"
                            },
                            {
                                "target_lang_word": "имеющий нарушения",
                                "target_lang_sense_clarification": "формальное описание наличия расстройств"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7b9d15de-cdb6-3629-a004-1758f905ab90",
                    "sense_definition": "Used humorously after an adverb to describe a lack of a certain quality, imitating polite terms for disabilities.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "disability_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He jokingly refers to himself as vertically <w>challenged</w> instead of short.",
                            "target_lang_translations": {
                                "ru": "Он в шутку называет себя вертикально <w>обделенным</w> вместо того, чтобы сказать «низкий»."
                            }
                        },
                        {
                            "text": "I am somewhat directionally <w>challenged</w> and always need a map.",
                            "target_lang_translations": {
                                "ru": "Я испытываю некоторые <w>трудности</w> с ориентированием, и мне всегда нужна карта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "vertically challenged",
                        "follically challenged",
                        "financially challenged"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "humorous or ironic usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в шутку после наречия для описания отсутствия определенного качества, имитируя вежливые термины для обозначения инвалидности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обделенный",
                                "target_lang_sense_clarification": "иронично об отсутствии качества"

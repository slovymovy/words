    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "200d54bf-f1cd-33c3-9795-9c65e0b5facf",
                    "sense_definition": "A supporting, lower or bottom component of a structure or object.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "foundation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The lamp has a heavy wooden <w>base</w> to keep it stable.",
                            "target_lang_translations": {
                                "ru": "У лампы тяжелое деревянное <w>основание</w> для устойчивости."
                            }
                        },
                        {
                            "text": "We sat at the <w>base</w> of the cliff and watched the waves.",
                            "target_lang_translations": {
                                "ru": "Мы сидели у <w>подножия</w> утеса и смотрели на волны."
                            }
                        }
                    ],
                    "synonyms": [
                        "substructure",
                        "ground"
                    ],
                    "antonyms": [
                        "superstructure",
                        "apex"
                    ],
                    "common_phrases": [
                        "at the base of",
                        "base of the pyramid",
                        "concrete base"
                    ],
                    "target_lang_definitions": {
                        "ru": "Опорная, нижняя часть конструкции или объекта."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "основание",
                                "target_lang_sense_clarification": "общая часть предмета"
                            },
                            {
                                "target_lang_word": "база",
                                "target_lang_sense_clarification": "архитектура, колонны"
                            },
                            {
                                "target_lang_word": "подножие",
                                "target_lang_sense_clarification": "горы, скалы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a79bd283-50b9-362d-91f6-0bc27694b3a4",
                    "sense_definition": "A site, structure, or both, usually durable and often permanent, for housing military personnel and materiel.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers returned to the <w>base</w> after their patrol.",
                            "target_lang_translations": {
                                "ru": "Солдаты вернулись на <w>базу</w> после патрулирования."
                            }
                        },
                        {
                            "text": "Access to the air <w>base</w> is restricted to authorized personnel.",
                            "target_lang_translations": {
                                "ru": "Доступ на авиа<w>базу</w> разрешен только уполномоченному персоналу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "military base",
                        "air base",
                        "naval base",
                        "army base"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место или сооружение для размещения военных сил и техники."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "база",
                                "target_lang_sense_clarification": "военная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b612527b-e1fa-3e34-bcf7-d2a90587c386",
                    "sense_definition": "The place where decisions for an organization are made; headquarters.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "location_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company's European <w>base</w> is located in Frankfurt.",
                            "target_lang_translations": {
                                "ru": "Европейская <w>штаб-квартира</w> компании находится во Франкфурте."
                            }
                        },
                        {
                            "text": "They used the hotel as a <w>base</w> for their travels around the region.",
                            "target_lang_translations": {
                                "ru": "Они использовали отель как <w>базу</w> для путешествий по региону."
                            }
                        }
                    ],
                    "common_phrases": [
                        "home base",
                        "base of operations"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место принятия решений организации; штаб-квартира."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "база",
                                "target_lang_sense_clarification": "центр операций"
                            },
                            {
                                "target_lang_word": "штаб-квартира",
                                "target_lang_sense_clarification": "главный офис"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "916a0853-d0f2-3b21-b7ee-c4ec3dc2e7d6",
                    "sense_definition": "One of the four places that a runner can stand without being subject to being tagged out when the ball is in play.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sports_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The player slid into second <w>base</w> just before the ball arrived.",
                            "target_lang_translations": {
                                "ru": "Игрок скользнул на вторую <w>базу</w> прямо перед тем, как мяч попал туда."
                            }
                        },
                        {
                            "text": "If you hit the ball over the fence, you can run around every <w>base</w>.",
                            "target_lang_translations": {
                                "ru": "Если вы перебьете мяч через забор, вы можете обежать каждую <w>базу</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "first base",
                        "second base",
                        "third base",
                        "on base"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "baseball"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одно из четырех мест на поле, где бегущий может находиться в безопасности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "база",
                                "target_lang_sense_clarification": "бейсбол"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fb9a3a4-c75c-3200-84e9-b52378f10bfd",
                    "sense_definition": "The lowest side of a triangle or other polygon, or the lowest face of a cone, pyramid or other polyhedron laid flat.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "math_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To calculate the area of a triangle, you need to know its <w>base</w> and height.",
                            "target_lang_translations": {
                                "ru": "Чтобы вычислить площадь треугольника, нужно знать его <w>основание</w> и высоту."
                            }
                        },
                        {
                            "text": "The <w>base</w> of the pyramid is a perfect square.",
                            "target_lang_translations": {
                                "ru": "<w>Основание</w> пирамиды представляет собой идеальный квадрат."
                            }
                        }
                    ],
                    "common_phrases": [
                        "base and height",
                        "length of the base"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "geometry"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Нижняя сторона многоугольника или грань многогранника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "основание",
                                "target_lang_sense_clarification": "геометрия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "88293dee-91d3-3e70-8526-6db4774ebec5",
                    "sense_definition": "The starting point of a logical deduction or thought; basis.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "foundation_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This theory has no scientific <w>base</w> and should be ignored.",
                            "target_lang_translations": {
                                "ru": "Эта теория не имеет научной <w>основы</w> и ее следует игнорировать."
                            }
                        },
                        {
                            "text": "We need a solid <w>base</w> of evidence before we can proceed.",
                            "target_lang_translations": {
                                "ru": "Нам нужна прочная доказательная <w>база</w>, прежде чем мы сможем продолжить."
                            }
                        }
                    ],
                    "synonyms": [
                        "radix"
                    ],
                    "common_phrases": [
                        "base for",
                        "knowledge base",
                        "customer base"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отправная точка логического вывода или мысли; основа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "основа",
                                "target_lang_sense_clarification": "суть, начало"
                            },
                            {
                                "target_lang_word": "база",
                                "target_lang_sense_clarification": "данных, знаний"
                            },
                            {
                                "target_lang_word": "фундамент",
                                "target_lang_sense_clarification": "перен. прочная основа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "357e0f5b-cbc3-330d-906e-84c808728126",
                    "sense_definition": "Any of a class of generally water-soluble compounds that turn red litmus blue and react with acids to form salts.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "science_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ammonia is a common weak <w>base</w> used in cleaning products.",
                            "target_lang_translations": {
                                "ru": "Аммиак — это распространенное слабое <w>основание</w>, используемое в чистящих средствах."
                            }
                        },
                        {
                            "text": "In this reaction, the acid neutralizes the <w>base</w> to produce water and salt.",
                            "target_lang_translations": {
                                "ru": "В этой реакции кислота нейтрализует <w>основание</w>, образуя воду и соль."
                            }
                        }
                    ],
                    "synonyms": [
                        "alkali"
                    ],
                    "antonyms": [
                        "acid"
                    ],
                    "common_phrases": [
                        "acid and base",
                        "weak base",
                        "strong base"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "chemistry"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Класс соединений, реагирующих с кислотами с образованием солей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "основание",
                                "target_lang_sense_clarification": "химия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "721b95d8-6790-3818-8f4b-42acb60f5147",
                    "sense_definition": "A group of voters who almost always support a single party's candidates for elected office.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "politics_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The candidate gave a speech designed to energize his political <w>base</w>.",
                            "target_lang_translations": {
                                "ru": "Кандидат произнес речь, призванную активизировать его политическую <w>базу</w>."
                            }
                        },
                        {
                            "text": "The party needs to expand its support beyond its traditional <w>base</w>.",
                            "target_lang_translations": {
                                "ru": "Партии необходимо расширить поддержку за пределы своей традиционной <w>базы</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "electoral base",
                        "political base"
                    ],
                    "common_phrases": [
                        "voter base",
                        "support base",
                        "party base"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "politics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа избирателей, постоянно поддерживающих определенную партию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "база",
                                "target_lang_sense_clarification": "электоральная"
                            },
                            {
                                "target_lang_word": "электорат",
                                "target_lang_sense_clarification": "избиратели"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "aff4f7f2-6af6-326c-888a-79cbca2dd3fc",
                    "sense_definition": "To give as its foundation or starting point; to lay the foundation of.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_foundation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The director decided to <w>base</w> the movie on a true story.",
                            "target_lang_translations": {
                                "ru": "Режиссер решил <w>основать</w> фильм на реальных событиях."
                            }
                        },
                        {
                            "text": "You should <w>base</w> your decision on facts, not assumptions.",
                            "target_lang_translations": {
                                "ru": "Вам следует <w>основывать</w> свое решение на фактах, а не на предположениях."
                            }
                        }
                    ],
                    "common_phrases": [
                        "base on",
                        "base upon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать что-либо в качестве основы или отправной точки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "основывать",
                                "target_lang_sense_clarification": "на фактах, событиях"
                            },
                            {
                                "target_lang_word": "базировать",
                                "target_lang_sense_clarification": "обосновывать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d32adecf-9414-395d-89a9-e53923437a9e",
                    "sense_definition": "To be located (at a particular place).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verb_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The charity is <w>based</w> in London but operates globally.",
                            "target_lang_translations": {
                                "ru": "Благотворительная организация <w>базируется</w> в Лондоне, но работает по всему миру."
                            }
                        },
                        {
                            "text": "Where will the new project team be <w>based</w>?",
                            "target_lang_translations": {
                                "ru": "Где будет <w>базироваться</w> новая проектная группа?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "be based in",
                        "be based at"
                    ],
                    "target_lang_definitions": {
                        "ru": "Располагаться или иметь штаб-квартиру в определенном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "базироваться",
                                "target_lang_sense_clarification": "располагаться"
                            },
                            {
                                "target_lang_word": "размещаться",
                                "target_lang_sense_clarification": "находиться"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b416ba9d-a94d-33db-a3f5-9bba78308f2f",
                    "sense_definition": "Morally reprehensible, immoral; cowardly.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "adj_moral",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stealing from the elderly is a <w>base</w> and shameful act.",
                            "target_lang_translations": {
                                "ru": "Кража у пожилых людей — это <w>низкий</w> и постыдный поступок."
                            }
                        },
                        {
                            "text": "He acted out of <w>base</w> motives like greed and jealousy.",
                            "target_lang_translations": {
                                "ru": "Он действовал из <w>низменных</w> побуждений, таких как жадность и зависть."
                            }
                        }
                    ],
                    "synonyms": [
                        "despicable",
                        "evil",
                        "vulgar"
                    ],
                    "antonyms": [
                        "noble",
                        "admirable"
                    ],
                    "common_phrases": [
                        "base motives",
                        "base instincts"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "often literary or formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Морально предосудительный, безнравственный; низкий."
                    },
                    "translations": {

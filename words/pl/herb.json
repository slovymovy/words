                {
                    "sense_id": "1eb097af-ba55-3434-87e7-f2827ea5ee98",
                    "sense_definition": "Oficjalny znak symboliczny państwa, miasta, regionu lub organizacji, ustalony według określonych reguł.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "heraldic_sign",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na budynku urzędu wisi <w>herb</w> naszego miasta.",
                            "target_lang_translations": {
                                "ru": "На здании администрации висит <w>герб</w> нашего города."
                            }
                        },
                        {
                            "text": "Biały Orzeł to oficjalny <w>herb</w> Polski.",
                            "target_lang_translations": {
                                "ru": "Белый орёл — это официальный <w>герб</w> Польши."
                            }
                        }
                    ],
                    "synonyms": [
                        "godło"
                    ],
                    "common_phrases": [
                        "herb miasta",
                        "herb państwowy",
                        "herb klubu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальный символический знак государства, города, региона или организации, установленный по определённым правилам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "герб",
                                "target_lang_sense_clarification": "символ государства или города"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ccd2b17b-1b53-35ec-910d-0f8009603c51",
                    "sense_definition": "Dziedziczny znak rodu szlacheckiego, umieszczany tradycyjnie na tarczy, pieczęciach i przedmiotach własności.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "heraldic_sign",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rycerz miał namalowany swój <w>herb</w> na tarczy.",
                            "target_lang_translations": {
                                "ru": "У рыцаря на щите был нарисован его <w>герб</w>."
                            }
                        },
                        {
                            "text": "Ta rodzina od wieków pieczętuje się <w>herbem</w> Jastrzębiec.",
                            "target_lang_translations": {
                                "ru": "Эта семья веками пользуется <w>гербом</w> Ястшембец."
                            }
                        }
                    ],
                    "synonyms": [
                        "klejnot herbowy"
                    ],
                    "common_phrases": [
                        "herb szlachecki",
                        "pieczętować się herbem",
                        "ród herbowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наследственный знак дворянского рода, традиционно изображаемый на щите, печатях и предметах собственности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "герб",
                                "target_lang_sense_clarification": "родовой знак"
                            }
                        ]
                    }
                }
            ]
        }
    ]

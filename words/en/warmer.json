                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гетры",
                                "target_lang_sense_clarification": "для ног"
                            },
                            {
                                "target_lang_word": "теплые наушники",
                                "target_lang_sense_clarification": "для ушей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cb6f1daa-f909-33b6-9678-db0ad96cc4c6",
                    "sense_definition": "A short introductory activity used to engage people at the beginning of a lesson or meeting.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher started the class with a fun <w>warmer</w> to wake everyone up.",
                            "target_lang_translations": {
                                "ru": "Учитель начал урок с веселой <w>разминки</w>, чтобы всех взбодрить."
                            }

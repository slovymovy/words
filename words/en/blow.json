                        }
                    ],
                    "synonyms": [
                        "gale",
                        "storm"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильный ветер или шторм."
                    },
                    "translations": {
                        "ru": [

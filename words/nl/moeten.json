{
    "word_family": [
        "moetje"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "abfe84c2-33e1-341d-bd42-3bf0163a9ecb",
                    "sense_definition": "Verplicht zijn iets te doen door een regel, bevel of noodzaak; geen andere keuze hebben.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "obligation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je <w>moet</w> je huiswerk maken voordat je naar buiten mag.",
                            "target_lang_translations": {
                                "en": "You <w>have to</w> do your homework before you are allowed outside.",
                                "ru": "Ты <w>должен</w> сделать домашнее задание, прежде чем тебе разрешат пойти на улицу."
                            }
                        },
                        {
                            "text": "Wij <w>moesten</w> gisteren lang wachten op de bus.",
                            "target_lang_translations": {
                                "en": "We <w>had to</w> wait a long time for the bus yesterday.",
                                "ru": "Вчера нам <w>пришлось</w> долго ждать автобуса."
                            }
                        }
                    ],
                    "synonyms": [
                        "verplicht zijn te",
                        "hebben te"
                    ],
                    "antonyms": [
                        "niet hoeven"
                    ],
                    "common_phrases": [
                        "ik moet weg",
                        "je moet luisteren",
                        "we moeten opschieten"
                    ],
                    "target_lang_definitions": {
                        "en": "To be required to do something by a rule, order, or necessity; to have no other choice.",
                        "ru": "Быть обязанным сделать что-то из-за правила, приказа или необходимости; не иметь другого выбора."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "have to",
                                "target_lang_sense_clarification": "general obligation or external necessity"
                            },
                            {
                                "target_lang_word": "must",
                                "target_lang_sense_clarification": "strong obligation or internal necessity"
                            },
                            {
                                "target_lang_word": "need to",
                                "target_lang_sense_clarification": "necessity"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "быть должным",
                                "target_lang_sense_clarification": "обязанность или долг"
                            },
                            {
                                "target_lang_word": "нужно",
                                "target_lang_sense_clarification": "необходимость"
                            },
                            {
                                "target_lang_word": "приходиться",
                                "target_lang_sense_clarification": "вынужденная необходимость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "023906df-aca0-3cd0-8611-95be1fa0ef6f",
                    "sense_definition": "Zeer waarschijnlijk waar zijn of wenselijk geacht worden; een logische conclusie of dringend advies.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "deduction_advice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat <w>moet</w> een vergissing zijn, want ik heb al betaald.",
                            "target_lang_translations": {
                                "en": "That <w>must</w> be a mistake, because I have already paid.",
                                "ru": "Это, <w>должно быть</w>, ошибка, так как я уже заплатил."
                            }
                        },
                        {
                            "text": "Je <w>moet</w> echt eens naar die nieuwe film gaan kijken.",
                            "target_lang_translations": {
                                "en": "You really <w>must</w> go see that new movie sometime.",
                                "ru": "Тебе действительно <w>стоит</w> как-нибудь посмотреть этот новый фильм."
                            }
                        }
                    ],
                    "synonyms": [
                        "zou moeten",
                        "horen"
                    ],
                    "common_phrases": [
                        "dat moet wel",
                        "je moet niet vergeten",
                        "het moet haast wel"
                    ],
                    "target_lang_definitions": {
                        "en": "To be very likely true or considered desirable; a logical conclusion or urgent advice.",
                        "ru": "Быть очень вероятным или желательным; логический вывод или настоятельный совет."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "must",
                                "target_lang_sense_clarification": "logical deduction or certainty"
                            },
                            {
                                "target_lang_word": "should",
                                "target_lang_sense_clarification": "advice or recommendation"
                            },
                            {
                                "target_lang_word": "have to",
                                "target_lang_sense_clarification": "logical conclusion"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "должно быть",
                                "target_lang_sense_clarification": "логическое предположение"
                            },
                            {
                                "target_lang_word": "стоить",
                                "target_lang_sense_clarification": "совет или рекомендация"
                            },
                            {
                                "target_lang_word": "нужно",
                                "target_lang_sense_clarification": "настоятельный совет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60592ea8-81a5-36d9-b226-61d4b88642bf",
                    "sense_definition": "Genoodzaakt zijn ergens naartoe te gaan (vaak met een voorzetsel van richting).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>moet</w> nu echt naar huis om te eten.",
                            "target_lang_translations": {
                                "en": "I really <w>have to go</w> home now to eat.",
                                "ru": "Мне правда сейчас <w>нужно</w> домой, чтобы поесть."
                            }
                        },
                        {
                            "text": "Morgen <w>moeten</w> we vroeg naar het vliegveld vertrekken.",
                            "target_lang_translations": {
                                "en": "Tomorrow we <w>have to</w> leave for the airport early.",
                                "ru": "Завтра мы <w>должны</w> рано выехать в аэропорт."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ik moet naar huis",
                        "hij moet naar bed",
                        "wij moeten naar school"
                    ],
                    "target_lang_definitions": {
                        "en": "To be forced or required to go somewhere (often used with a preposition of direction).",
                        "ru": "Быть вынужденным отправиться куда-либо (часто используется с предлогом направления)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "have to go",
                                "target_lang_sense_clarification": "movement implied by context"
                            },
                            {
                                "target_lang_word": "must go",
                                "target_lang_sense_clarification": "movement implied by context"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "нужно",
                                "target_lang_sense_clarification": "необходимость идти/ехать"
                            },
                            {
                                "target_lang_word": "пора",
                                "target_lang_sense_clarification": "время уходить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3dcf7dba-b8fd-3216-891c-1a9b731b785c",
                    "sense_definition": "Een dringende lichamelijke behoefte voelen om naar het toilet te gaan.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "biological_need",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stop de auto even, want ik <w>moet</w> heel nodig.",
                            "target_lang_translations": {
                                "en": "Stop the car for a moment, because I really <w>need to go</w>.",
                                "ru": "Останови машину на минутку, потому что мне очень <w>нужно</w>."
                            }
                        },
                        {
                            "text": "Het kindje gaf aan dat het <w>moest</w> en rende naar de wc.",
                            "target_lang_translations": {
                                "en": "The child indicated that it <w>had to go</w> and ran to the toilet.",
                                "ru": "Ребенок показал, что <w>хочет в туалет</w>, и побежал в уборную."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ik moet nodig",
                        "hij moet plassen"
                    ],
                    "target_lang_definitions": {
                        "en": "To feel an urgent physical need to go to the toilet.",
                        "ru": "Испытывать острую физическую потребность сходить в туалет."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "need to go",
                                "target_lang_sense_clarification": "euphemism for using the toilet"
                            },
                            {
                                "target_lang_word": "have to go",
                                "target_lang_sense_clarification": "urgent need"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "нужно",
                                "target_lang_sense_clarification": "эвфемизм для посещения туалета"
                            },
                            {
                                "target_lang_word": "хотеть в туалет",
                                "target_lang_sense_clarification": "физическая потребность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44331cbe-f62c-356b-a6f4-33a24e71279c",
                    "sense_definition": "Indien het geval zou zijn dat; drukt een voorwaarde uit.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "conditional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Moest</w> je hem nog spreken, doe hem dan de groeten.",
                            "target_lang_translations": {
                                "en": "<w>Should</w> you speak to him, give him my regards.",
                                "ru": "<w>Если</w> ты будешь с ним разговаривать, передавай ему привет."
                            }
                        },
                        {
                            "text": "<w>Moest</w> het morgen regenen, dan gaat het feest niet door.",
                            "target_lang_translations": {
                                "en": "<w>Should</w> it rain tomorrow, the party won't go on.",
                                "ru": "<w>Если</w> завтра пойдет дождь, вечеринка не состоится."
                            }
                        }
                    ],
                    "common_phrases": [
                        "moest het kunnen",
                        "moest je willen"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "België"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used to express a condition; if it were the case that.",
                        "ru": "Используется для выражения условия; если бы случилось так, что."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "should",
                                "target_lang_sense_clarification": "conditional usage"
                            },
                            {
                                "target_lang_word": "if ... happen to",
                                "target_lang_sense_clarification": "conditional"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "если",
                                "target_lang_sense_clarification": "условие"
                            },
                            {
                                "target_lang_word": "случись",
                                "target_lang_sense_clarification": "если вдруг произойдет"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ed8a4945-b188-3c1d-8728-1102df50363e",
                    "sense_definition": "Meervoud van het zelfstandig naamwoord <w>moet</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er zaten diepe <w>moeten</w> in het hout door de strakke touwen.",
                            "target_lang_translations": {
                                "en": "There were deep <w>indentations</w> in the wood from the tight ropes.",
                                "ru": "На дереве остались глубокие <w>вмятины</w> от туго натянутых веревок."
                            }
                        },
                        {
                            "text": "Na het slapen had hij rode <w>moeten</w> in zijn gezicht van het kussen.",
                            "target_lang_translations": {
                                "en": "After sleeping, he had red <w>marks</w> on his face from the pillow.",
                                "ru": "После сна у него на лице были красные <w>следы</w> от подушки."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural of the noun 'moet'; an impression, mark, or dent left in a surface.",
                        "ru": "Множественное число существительного 'moet'; отпечаток, след или вмятина на поверхности."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "marks",
                                "target_lang_sense_clarification": "visible impressions"
                            },
                            {
                                "target_lang_word": "indentations",
                                "target_lang_sense_clarification": "depressions in a surface"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "следы",
                                "target_lang_sense_clarification": "видимые отпечатки"
                            },
                            {
                                "target_lang_word": "вмятины",
                                "target_lang_sense_clarification": "углубления в поверхности"
                            }
                        ]
                    }
                }
            ]
        }
    ]

        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "448f203a-5cdf-3f43-ae1f-fc24978f928a",
                    "sense_definition": "To mend a hole or worn area by covering it with a piece of material.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "repair_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>patch</w> these old jeans before I can wear them again.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>залатать</w> эти старые джинсы, прежде чем я смогу снова их надеть."
                            }
                        },
                        {
                            "text": "They <w>patched</w> the roof to stop the rain from leaking in.",
                            "target_lang_translations": {
                                "ru": "Они <w>залатали</w> крышу, чтобы дождь не протекал внутрь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "patch a hole",
                        "patch up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чинить дыру или изношенное место, закрывая его куском материала."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "латать",
                                "target_lang_sense_clarification": "чинить одежду"
                            },
                            {
                                "target_lang_word": "ставить заплаты",
                                "target_lang_sense_clarification": "процесс починки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ae86645f-d3a9-37d1-9c6e-6fc9e3b6cc09",
                    "sense_definition": "To repair a relationship, situation, or quarrel, often hastily.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "repair_relationship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They managed to <w>patch</w> up their differences after the argument.",
                            "target_lang_translations": {
                                "ru": "Им удалось <w>уладить</w> свои разногласия после ссоры."
                            }
                        },
                        {
                            "text": "The team tried to <w>patch</w> up the problem before the deadline.",
                            "target_lang_translations": {
                                "ru": "Команда пыталась <w>устранить</w> проблему до дедлайна."
                            }
                        }
                    ],
                    "common_phrases": [
                        "patch things up",
                        "patch up a quarrel"
                    ],
                    "target_lang_definitions": {
                        "ru": "Восстанавливать отношения, ситуацию или улаживать ссору, часто поспешно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "налаживать",
                                "target_lang_sense_clarification": "отношения"
                            },
                            {
                                "target_lang_word": "улаживать",
                                "target_lang_sense_clarification": "конфликт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fd42cc0-8075-32f3-8a9e-df45cc8e6f0b",
                    "sense_definition": "To update or fix a computer program by applying a small piece of code.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "software_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The IT department will <w>patch</w> the server tonight to improve security.",
                            "target_lang_translations": {
                                "ru": "IT-отдел будет <w>патчить</w> сервер сегодня вечером для улучшения безопасности."
                            }
                        },
                        {
                            "text": "The game was <w>patched</w> to fix several bugs reported by players.",
                            "target_lang_translations": {
                                "ru": "Игру <w>пропатчили</w>, чтобы исправить несколько ошибок, о которых сообщили игроки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обновлять или исправлять компьютерную программу путем применения небольшого фрагмента кода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "патчить",
                                "target_lang_sense_clarification": "компьютерный жаргон"
                            },
                            {
                                "target_lang_word": "обновлять",
                                "target_lang_sense_clarification": "программное обеспечение"
                            }
                        ]

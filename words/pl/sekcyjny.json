    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6048ff4e-f7f4-37de-a4c8-4634ba0790ad",
                    "sense_definition": "Dotyczący sekcji (wyodrębnionej części organizacji, grupy tematycznej lub zabiegu otwarcia zwłok).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Student medycyny po raz pierwszy wszedł na salę <w>sekcyjną</w>.",
                            "target_lang_translations": {
                                "ru": "Студент-медик впервые вошел в <w>секционный</w> зал."
                            }
                        },
                        {
                            "text": "Zebranie <w>sekcyjne</w> poświęcono omówieniu nowych projektów badawczych.",
                            "target_lang_translations": {
                                "ru": "<w>Секционное</w> собрание посвятили обсуждению новых исследовательских проектов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stół sekcyjny",
                        "protokół sekcyjny",
                        "sala sekcyjna",
                        "zebranie sekcyjne"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к секции (части организации, тематической группе) или к процедуре вскрытия (аутопсии)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "секционный",
                                "target_lang_sense_clarification": "относящийся к секции или вскрытию"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "23c1b689-a7d7-3b02-acc6-0b4a9fab0459",
                    "sense_definition": "Osoba pełniąca funkcję dowódcy sekcji (małego pododdziału w wojsku lub straży pożarnej).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "commander_rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasz <w>sekcyjny</w> wydał rozkaz do natychmiastowego wymarszu.",
                            "target_lang_translations": {
                                "ru": "Наш <w>командир отделения</w> отдал приказ к немедленному выступлению."
                            }
                        },
                        {
                            "text": "Młody <w>sekcyjny</w> świetnie poradził sobie z dowodzeniem akcją ratunkową.",
                            "target_lang_translations": {
                                "ru": "Молодой <w>командир отделения</w> отлично справился с руководством спасательной операцией."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, выполняющий функции командира секции (небольшого подразделения в армии или пожарной охране)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "командир отделения",
                                "target_lang_sense_clarification": "должность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73571702-5428-3fcb-a0c5-7356f8b0e6f0",
                    "sense_definition": "Pierwszy stopień w korpusie podoficerów Państwowej Straży Pożarnej.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "commander_rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Za wzorową służbę strażak otrzymał awans na stopień starszego <w>sekcyjnego</w>.",
                            "target_lang_translations": {
                                "ru": "За образцовую службу пожарный получил повышение до звания старшего <w>секционного</w>."
                            }
                        },
                        {
                            "text": "Mój brat jest <w>sekcyjnym</w> w jednostce ratowniczo-gaśniczej.",
                            "target_lang_translations": {
                                "ru": "Мой брат служит <w>секционным</w> в спасательно-пожарной части."
                            }
                        }
                    ],
                    "common_phrases": [
                        "starszy sekcyjny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Первое звание в корпусе унтер-офицеров Государственной пожарной службы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "секционный",
                                "target_lang_sense_clarification": "звание в пожарной службе Польши"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
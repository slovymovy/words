                        "ulegać pokusie",
                        "ulegać namowom",
                        "ulegać emocjom",
                        "ulegać wpływom"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переставать сопротивляться и подчиняться чьей-либо воле, давлению или сильным эмоциям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поддаваться",
                                "target_lang_sense_clarification": "переставать сопротивляться давлению или эмоциям"
                            },
                            {
                                "target_lang_word": "уступать",

{
    "word_family": [
        "virtualiteit",
        "virtualisatie",
        "virtualiseren"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "56c1543d-edee-374d-9028-1e672a90d13a",
                    "sense_definition": "Gemaakt of gesimuleerd door een computer of aanwezig op het internet, in tegenstelling tot de fysieke werkelijkheid.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "digital",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Veel jongeren brengen dagelijks uren door in een <w>virtuele</w> wereld.",
                            "target_lang_translations": {
                                "ru": "Многие молодые люди ежедневно проводят часы в <w>виртуальном</w> мире."
                            }
                        },
                        {
                            "text": "Door de pandemie moesten we overschakelen op een <w>virtuele</w> vergadering.",
                            "target_lang_translations": {
                                "ru": "Из-за пандемии нам пришлось переключиться на <w>виртуальное</w> собрание."
                            }
                        }
                    ],
                    "antonyms": [
                        "echt",
                        "feitelijk"
                    ],
                    "common_phrases": [
                        "virtuele werkelijkheid",
                        "virtuele wereld",
                        "virtueel geheugen",
                        "virtuele assistent"
                    ],
                    "target_lang_definitions": {
                        "ru": "Созданный или смоделированный компьютером, либо существующий в интернете, в отличие от физической реальности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "виртуальный",
                                "target_lang_sense_clarification": "компьютерный, цифровой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "603a6ca1-e9a2-31d0-acb5-834bfd141705",
                    "sense_definition": "In wezen of in effect aanwezig, maar (nog) niet feitelijk of fysiek bestaand.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De wielrenner reed tijdens de klim lange tijd <w>virtueel</w> in de gele trui.",
                            "target_lang_translations": {
                                "ru": "Во время подъема велогонщик долгое время <w>виртуально</w> ехал в желтой майке."
                            }
                        },
                        {
                            "text": "In de optica spreekt men van een <w>virtueel</w> beeld als het licht niet werkelijk samenkomt.",
                            "target_lang_translations": {
                                "ru": "В оптике говорят о <w>мнимом</w> изображении, когда свет на самом деле не сходится."
                            }
                        }
                    ],
                    "synonyms": [
                        "onwerkelijk"
                    ],
                    "antonyms": [
                        "feitelijk",
                        "echt"
                    ],
                    "common_phrases": [
                        "virtueel beeld",
                        "virtueel leider"
                    ],
                    "target_lang_definitions": {
                        "ru": "Существующий по сути или производящий эффект, но не являющийся фактом или физическим объектом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "виртуальный",
                                "target_lang_sense_clarification": "потенциальный, условный"
                            },
                            {
                                "target_lang_word": "мнимый",
                                "target_lang_sense_clarification": "в оптике"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4add1fb5-6355-30cc-8aae-939fbd2555df",
                    "sense_definition": "Bijna volledig; in de praktijk op hetzelfde neerkomend als.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "degree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het is <w>virtueel</w> onmogelijk om dit probleem zonder hulp op te lossen.",
                            "target_lang_translations": {
                                "ru": "Решить эту проблему без помощи <w>практически</w> невозможно."
                            }
                        },
                        {
                            "text": "De kans dat dit gebeurt is <w>virtueel</w> nul.",
                            "target_lang_translations": {
                                "ru": "Вероятность того, что это произойдет, <w>практически</w> равна нулю."
                            }
                        }
                    ],
                    "synonyms": [
                        "onwerkelijk"
                    ],
                    "common_phrases": [
                        "virtueel onmogelijk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Почти полный; на практике означающий то же самое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "практически",
                                "target_lang_sense_clarification": "почти"
                            },
                            {
                                "target_lang_word": "фактически",
                                "target_lang_sense_clarification": "по сути"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
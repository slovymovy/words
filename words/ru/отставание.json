    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a72d050f-37b9-3cb5-9c90-09530a76d601",
                    "sense_definition": "Действие по значению глагола <w>отставать</w>; нахождение позади кого-либо или чего-либо при движении или выполнении работы.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за сильного снегопада произошло значительное <w>отставание</w> поезда от расписания.",
                            "target_lang_translations": {
                                "en": "Due to heavy snowfall, there was a significant <w>lag</w> in the train's schedule."
                            }
                        },
                        {
                            "text": "Спортсмен приложил все усилия, чтобы сократить <w>отставание</w> от лидера гонки.",
                            "target_lang_translations": {
                                "en": "The athlete made every effort to reduce the <w>gap</w> behind the race leader."
                            }
                        }
                    ],
                    "antonyms": [
                        "опережение",
                        "обгон"
                    ],
                    "common_phrases": [
                        "отставание от графика",
                        "сократить отставание",
                        "небольшое отставание"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of falling behind or the state of being behind someone or something in movement, time, or progress."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "lag",
                                "target_lang_sense_clarification": "general state of being behind"
                            },
                            {
                                "target_lang_word": "gap",
                                "target_lang_sense_clarification": "distance behind a leader"
                            },
                            {
                                "target_lang_word": "delay",
                                "target_lang_sense_clarification": "regarding schedules or time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a2c94f52-1517-315f-9eaf-058863857a7c",
                    "sense_definition": "Недостаточный уровень или недостаточно быстрый темп развития чего-либо по сравнению с нормой или другими показателями.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Правительство разработало план, чтобы преодолеть экономическое <w>отставание</w> страны.",
                            "target_lang_translations": {
                                "en": "The government developed a plan to overcome the country's economic <w>backwardness</w>."
                            }
                        },
                        {
                            "text": "В этой отрасли наблюдается серьёзное техническое <w>отставание</w> от мировых стандартов.",
                            "target_lang_translations": {
                                "en": "A serious technical <w>lag</w> behind world standards is observed in this industry."
                            }
                        }
                    ],
                    "antonyms": [
                        "лидерство",
                        "опережение"
                    ],
                    "common_phrases": [
                        "ликвидировать отставание",
                        "техническое отставание",
                        "экономическое отставание"
                    ],
                    "target_lang_definitions": {
                        "en": "A lack of development or progress compared to a standard, norm, or competitors."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "backwardness",
                                "target_lang_sense_clarification": "state of being less developed"
                            },
                            {
                                "target_lang_word": "lag",
                                "target_lang_sense_clarification": "slower pace of development"
                            }
                        ]
                    }
                }
            ]
        }
    ]

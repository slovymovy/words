    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "1205747f-a7f6-3fb7-86d4-d5f694af094b",
                    "sense_definition": "Делать заявку на изготовление, доставку товара или выполнение услуги.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я обычно <заказываю> такси через мобильное приложение.",
                            "target_lang_translations": {
                                "en": "I usually <w>order</w> a taxi via a mobile app."
                            }
                        },
                        {
                            "text": "На день рождения мы будем <заказывать> большой торт.",
                            "target_lang_translations": {
                                "en": "For the birthday, we will <w>order</w> a big cake."
                            }
                        }
                    ],
                    "common_phrases": [
                        "заказывать еду",
                        "заказывать столик",
                        "заказывать билеты",
                        "заказывать такси"
                    ],
                    "target_lang_definitions": {
                        "en": "To request goods or services to be supplied, made, or delivered."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to order",
                                "target_lang_sense_clarification": "general meaning for goods, food, or services"
                            },
                            {
                                "target_lang_word": "to book",
                                "target_lang_sense_clarification": "for tickets, tables, or appointments"
                            },
                            {
                                "target_lang_word": "to reserve",
                                "target_lang_sense_clarification": "synonym for booking tables or rooms"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "345ecd57-6532-3cd4-aeff-b48355607a99",
                    "sense_definition": "Нанимать убийцу для устранения кого-либо.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В детективном романе герой решает <заказывать> своего конкурента.",
                            "target_lang_translations": {
                                "en": "In the detective novel, the hero decides to <w>put a hit on</w> his competitor."
                            }
                        },
                        {
                            "text": "Преступники не стали <заказывать> свидетеля, а просто запугали его.",
                            "target_lang_translations": {
                                "en": "The criminals did not <w>put a hit on</w> the witness, but simply intimidated him."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "criminal context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To hire a contract killer to murder someone (criminal slang)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to put a hit on",
                                "target_lang_sense_clarification": "idiomatic slang"
                            },
                            {
                                "target_lang_word": "to order a hit on",
                                "target_lang_sense_clarification": "explicit criminal context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d0f9baac-c0aa-3510-a589-f16fda173fc7",
                    "sense_definition": "Запрещать что-либо, делать недоступным.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Совесть <заказывает> мне поступать бесчестно в этой ситуации.",
                            "target_lang_translations": {
                                "en": "Conscience <w>forbids</w> me to act dishonestly in this situation."
                            }
                        },
                        {
                            "text": "Древний закон <заказывал> чужеземцам приближаться к священному храму.",
                            "target_lang_translations": {
                                "en": "Ancient law <w>forbade</w> strangers to approach the sacred temple."
                            }
                        }
                    ],
                    "synonyms": [
                        "запрещать"
                    ],
                    "antonyms": [
                        "разрешать"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "literary or obsolete usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To prohibit or prevent someone from doing something (archaic/literary)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to forbid",
                                "target_lang_sense_clarification": "formal or archaic context"
                            },
                            {
                                "target_lang_word": "to bar",

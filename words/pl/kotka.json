            "senses": [
                {
                    "sense_id": "a220d051-7556-3579-80e7-c3b249d15942",
                    "sense_definition": "Samica kota.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na balkonie pięć kocurów zaleca się do mojej <kotki>!",
                            "target_lang_translations": {
                                "ru": "На балконе пять котов ухаживают за моей <кошкой>!"
                            }
                        },
                        {
                            "text": "Nasza <kotka> lubi spać na miękkiej poduszce w salonie.",
                            "target_lang_translations": {
                                "ru": "Наша <кошка> любит спать на мягкой подушке в гостиной."
                            }
                        }
                    ],
                    "antonyms": [
                        "kocur"
                    ],
                    "common_phrases": [
                        "mała kotka",
                        "dzika kotka",
                        "domowa kotka"
                    ],
                    "target_lang_definitions": {
                        "ru": "Самка кота."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кошка",
                                "target_lang_sense_clarification": "самка животного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "425325c5-47d0-3eec-867b-b49b0edebe0c",
                    "sense_definition": "Forma słowa <w>kotek</w> (dopełniacz lub biernik liczby pojedynczej).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zdechł kotek i nie ma już <kotka>.",
                            "target_lang_translations": {
                                "ru": "Сдох котик, и нет уже <котика>."
                            }
                        },
                        {
                            "text": "Dziecko zobaczyło małego <kotka> i chciało go pogłaskać.",
                            "target_lang_translations": {
                                "ru": "Ребёнок увидел маленького <котёнка> и захотел его погладить."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive/accusative singular"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма слова <w>kotek</w> (родительный или винительный падеж единственного числа)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "котёнок",
                                "target_lang_sense_clarification": "детёныш"
                            },
                            {
                                "target_lang_word": "котик",
                                "target_lang_sense_clarification": "ласкательное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8cc1c401-4eaa-30d2-8623-10e3917c3b01",
                    "sense_definition": "Kwiatostan niektórych drzew, np. wierzby, przypominający puszystą kulkę.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "plant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na drzewach w parku pojawiły się już pierwsze wiosenne <kotki>.",
                            "target_lang_translations": {
                                "ru": "На деревьях в парке уже появились первые весенние <котики>."
                            }
                        },
                        {
                            "text": "Włożyliśmy do wazonu gałązki wierzby, na których rosły szare <kotki>.",
                            "target_lang_translations": {
                                "ru": "Мы поставили в вазу ветки ивы, на которых росли серые <котики>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bazia"
                    ],
                    "common_phrases": [
                        "wierzbowe kotki",
                        "szare kotki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соцветие некоторых деревьев, например ивы, напоминающее пушистый шарик."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "серёжка",
                                "target_lang_sense_clarification": "соцветие"
                            },
                            {
                                "target_lang_word": "котик",
                                "target_lang_sense_clarification": "соцветие ивы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13710212-892a-347a-b76f-bca0701c822f",
                    "sense_definition": "Pieszczotliwe lub potoczne określenie kobiety, która zachowuje się przymilnie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A gdzie to moja <kotka> mnie dziś zabiera na kolację?",
                            "target_lang_translations": {
                                "ru": "А куда это моя <кошечка> меня сегодня ведёт на ужин?"
                            }
                        },
                        {
                            "text": "Zachowywała się jak prawdziwa <kotka>, mrużąc oczy i uśmiechając się.",
                            "target_lang_translations": {
                                "ru": "Она вела себя как настоящая <кошка>, щуря глаза и улыбаясь."
                            }
                        }
                    ],
                    "common_phrases": [
                        "moja kotka",
                        "drapieżna kotka"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ласковое или разговорное название женщины, которая ведёт себя игриво или вкрадчиво."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кошечка",
                                "target_lang_sense_clarification": "ласковое обращение"
                            },
                            {
                                "target_lang_word": "киса",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        }

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "92ce7f3c-2314-308a-8e7d-a677f7efc332",
                    "sense_definition": "Oglądać coś pobieżnie, przewracając strony lub przewijając treść, często żeby wstępnie zapoznać się z informacjami.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lubię rano <przeglądać> prasę, pijąc gorącą kawę.",
                            "target_lang_translations": {
                                "ru": "Я люблю по утрам <просматривать> прессу, попивая горячий кофе."
                            }
                        },
                        {
                            "text": "Ona godzinami <przegląda> strony internetowe o modzie.",
                            "target_lang_translations": {
                                "ru": "Она часами <просматривает> веб-сайты о моде."
                            }
                        }
                    ],
                    "synonyms": [
                        "wertować",
                        "kartkować"
                    ],
                    "common_phrases": [
                        "przeglądać prasę",
                        "przeglądać internet",
                        "przeglądać gazetę",
                        "przeglądać zdjęcia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бегло осматривать что-либо, перелистывая страницы или прокручивая контент, часто для ознакомления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просматривать",
                                "target_lang_sense_clarification": "бегло знакомиться с содержанием"
                            },
                            {
                                "target_lang_word": "листать",
                                "target_lang_sense_clarification": "переворачивать страницы или прокручивать ленту"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90d63fad-0d4e-3864-9fd3-03d69e81eb33",
                    "sense_definition": "Patrzeć na swoje odbicie, zazwyczaj w lustrze, szybie lub tafli wody.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziewczyna <przeglądała się> w lustrze, poprawiając makijaż.",
                            "target_lang_translations": {
                                "ru": "Девушка <смотрелась> в зеркало, поправляя макияж."
                            }
                        },
                        {
                            "text": "Nie <przeglądaj się> w każdej wystawie sklepowej, bo się spóźnimy.",
                            "target_lang_translations": {
                                "ru": "Не <глядись> в каждую витрину магазина, а то мы опоздаем."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przeglądać się w lustrze",
                        "przeglądać się w witrynie"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "czasownik zwrotny z 'się'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Смотреть на свое отражение, обычно в зеркале, стекле или воде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смотреться",
                                "target_lang_sense_clarification": "видеть свое отражение"
                            },
                            {
                                "target_lang_word": "глядеться",
                                "target_lang_sense_clarification": "разглядывать себя (разговорное)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee3ef84d-3383-3634-ac0f-5c27e0940e23",
                    "sense_definition": "Dokładnie oglądać lub sprawdzać rzeczy jedna po drugiej, na przykład w poszukiwaniu czegoś lub w celu kontroli.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Celnik dokładnie <przeglądał> bagaże wszystkich podróżnych.",
                            "target_lang_translations": {
                                "ru": "Таможенник тщательно <осматривал> багаж всех путешественников."
                            }
                        },
                        {
                            "text": "Muszę <przeglądać> te dokumenty, zanim je podpiszę.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <просмотреть> эти документы, прежде чем я их подпишу."
                            }
                        }
                    ],
                    "synonyms": [
                        "sprawdzać",
                        "kontrolować",
                        "rewidować"
                    ],
                    "common_phrases": [
                        "przeglądać dokumenty",
                        "przeglądać pliki",
                        "przeglądać rzeczy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тщательно осматривать или проверять вещи одну за другой, например, при поиске или контроле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осматривать",
                                "target_lang_sense_clarification": "внимательно обследовать"
                            },
                            {
                                "target_lang_word": "проверять",
                                "target_lang_sense_clarification": "контролировать правильность или содержимое"
                            },
                            {
                                "target_lang_word": "перебирать",
                                "target_lang_sense_clarification": "рассматривать вещи по очереди"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
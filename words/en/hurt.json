                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вред",
                                "target_lang_sense_clarification": "порча, ущерб"
                            },
                            {
                                "target_lang_word": "уще́рб",
                                "target_lang_sense_clarification": "убыток, потеря"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf1ff260-4de4-377d-a47c-f88bfeb91a84",

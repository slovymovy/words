            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "b5246ab9-31b4-39fb-866e-0da1651d9758",
                    "sense_definition": "Of low quality, cheap, or worthless; resembling junk.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I refuse to buy <w>junky</w> toys that break after one day of use.",
                            "target_lang_translations": {
                                "ru": "Я отказываюсь покупать <w>дрянные</w> игрушки, которые ломаются через день использования."
                            }
                        },
                        {
                            "text": "She made a bracelet using the clasp from a <w>junky</w> old necklace.",
                            "target_lang_translations": {
                                "ru": "Она сделала браслет, используя застёжку от <w>барахляного</w> старого ожерелья."
                            }
                        }
                    ],
                    "synonyms": [
                        "trashy",
                        "shoddy",
                        "cheap",
                        "rubbishy"
                    ],
                    "antonyms": [
                        "high-quality",
                        "premium",
                        "valuable"
                    ],
                    "common_phrases": [
                        "junky car",
                        "junky furniture",
                        "junky jewelry"
                    ],
                    "target_lang_definitions": {

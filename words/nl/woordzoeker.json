            "senses": [
                {
                    "sense_id": "f6245a67-23da-37d8-800f-a7e0f448ed16",
                    "sense_definition": "Een puzzel bestaande uit een rooster met letters waarin verborgen woorden gevonden moeten worden.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn oma lost elke dag een <w>woordzoeker</w> op in de krant.",
                            "target_lang_translations": {
                                "ru": "Моя бабушка каждый день разгадывает <w>филворд</w> в газете."
                            }
                        },
                        {
                            "text": "Kun jij alle verstopte namen in deze <w>woordzoeker</w> vinden?",
                            "target_lang_translations": {
                                "ru": "Ты можешь найти все спрятанные имена в этом <w>филворде</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "speurpuzzel"
                    ],
                    "common_phrases": [
                        "een woordzoeker oplossen",
                        "een moeilijke woordzoeker"
                    ],
                    "target_lang_definitions": {
                        "ru": "Головоломка, состоящая из сетки с буквами, в которой нужно найти скрытые слова."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "филворд",
                                "target_lang_sense_clarification": "тип кроссворда"
                            },
                            {
                                "target_lang_word": "головоломка «поиск слов»",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b5e460e-ad4c-3f16-921d-4edc4ca5abff",
                    "sense_definition": "Iemand die actief naar woorden of de juiste formulering zoekt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als schrijver ben je eigenlijk altijd een <w>woordzoeker</w>.",
                            "target_lang_translations": {
                                "ru": "Как писатель, ты, по сути, всегда <w>искатель слов</w>."
                            }
                        },
                        {
                            "text": "De <w>woordzoeker</w> bladerde urenlang door dikke woordenboeken.",
                            "target_lang_translations": {
                                "ru": "<w>Искатель слов</w> часами листал толстые словари."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто активно ищет слова или правильную формулировку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искатель слов",
                                "target_lang_sense_clarification": "буквально"
                            }
                        ]
                    }
                }
            ]
        }

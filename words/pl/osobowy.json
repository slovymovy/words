            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3bdbbfa9-5fb2-3248-ba53-54dfe8b29564",
                    "sense_definition": "Dotyczący konkretnej osoby, jej tożsamości lub cech indywidualnych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "general_personal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz pokazać dowód <w>osobowy</w> przy wejściu do budynku.",
                            "target_lang_translations": {
                                "ru": "Ты должен показать <w>удостоверение личности</w> при входе в здание."
                            }
                        },
                        {
                            "text": "Wypełniłem formularz moimi danymi <w>osobowymi</w>.",
                            "target_lang_translations": {
                                "ru": "Я заполнил форму своими <w>персональными</w> данными."
                            }
                        }
                    ],
                    "synonyms": [
                        "personalny"
                    ],
                    "antonyms": [
                        "bezosobowy"
                    ],
                    "common_phrases": [
                        "dowód osobowy",
                        "dane osobowe",
                        "godność osobowa",
                        "majątek osobowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к конкретному человеку, его личности или индивидуальным чертам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "личный",
                                "target_lang_sense_clarification": "принадлежащий конкретному лицу"
                            },
                            {
                                "target_lang_word": "персональный",
                                "target_lang_sense_clarification": "касающийся данных или специфики человека"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "86e914fe-72c5-3f34-aa35-d528c117927c",
                    "sense_definition": "Służący do przewozu ludzi, a nie towarów.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na parkingu stoi mój nowy samochód <w>osobowy</w>.",
                            "target_lang_translations": {
                                "ru": "На парковке стоит мой новый <w>легковой</w> автомобиль."
                            }
                        },
                        {
                            "text": "Ruch <w>osobowy</w> na granicy odbywa się bez zakłóceń.",
                            "target_lang_translations": {
                                "ru": "<w>Пассажирское</w> движение на границе проходит без помех."
                            }
                        }
                    ],
                    "synonyms": [
                        "pasażerski"
                    ],
                    "antonyms": [
                        "ciężarowy"
                    ],
                    "common_phrases": [
                        "samochód osobowy",
                        "pociąg osobowy",
                        "transport osobowy",
                        "ruch osobowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предназначенный для перевозки людей, а не грузов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "легковой",
                                "target_lang_sense_clarification": "об автомобиле"
                            },
                            {
                                "target_lang_word": "пассажирский",
                                "target_lang_sense_clarification": "о поезде, транспорте, движении"
                            }

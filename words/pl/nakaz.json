            "senses": [
                {
                    "sense_id": "0318ce52-5dfa-31b8-ab44-acf2192ba7a2",
                    "sense_definition": "Stanowcze polecenie wykonania czegoś, wydane przez osobę lub instytucję mającą władzę.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "order_command",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor wydał pracownikom <nakaz> powrotu do biura.",
                            "target_lang_translations": {
                                "ru": "Директор издал <w>приказ</w> сотрудникам вернуться в офис."
                            }
                        },
                        {
                            "text": "To nie była prośba, lecz wyraźny <nakaz>.",
                            "target_lang_translations": {
                                "ru": "Это была не просьба, а явный <w>приказ</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozkaz",
                        "polecenie",
                        "zarządzenie"
                    ],
                    "antonyms": [
                        "zakaz"
                    ],
                    "common_phrases": [
                        "wydać nakaz",
                        "otrzymać nakaz",
                        "nakaz chwili"
                    ],
                    "target_lang_definitions": {
                        "ru": "Решительное требование выполнить что-либо, исходящее от лица или учреждения, обладающего властью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приказ",
                                "target_lang_sense_clarification": "официальное распоряжение"
                            },
                            {
                                "target_lang_word": "распоряжение",
                                "target_lang_sense_clarification": "указание к действию"
                            },
                            {
                                "target_lang_word": "предписание",
                                "target_lang_sense_clarification": "официальное письменное требование"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25bca790-fdca-3912-9a92-198d006a0ae6",
                    "sense_definition": "Oficjalny dokument sądowy lub administracyjny upoważniający do wykonania określonej czynności prawnej lub wymuszający określone zachowanie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_document",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja weszła do mieszkania, okazując <nakaz> przeszukania.",
                            "target_lang_translations": {
                                "ru": "Полиция вошла в квартиру, предъявив <w>ордер</w> на обыск."
                            }
                        },
                        {
                            "text": "Sąd wystawił <nakaz> aresztowania podejrzanego o kradzież.",
                            "target_lang_translations": {
                                "ru": "Суд выдал <w>ордер</w> на арест подозреваемого в краже."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nakaz aresztowania",
                        "nakaz przeszukania",
                        "nakaz zapłaty",
                        "nakaz sądowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальный судебный или административный документ, уполномочивающий на совершение определенного юридического действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ордер",
                                "target_lang_sense_clarification": "на арест или обыск"
                            },
                            {
                                "target_lang_word": "судебный приказ",
                                "target_lang_sense_clarification": "постановление судьи (напр. о взыскании долга)"
                            },
                            {
                                "target_lang_word": "постановление",
                                "target_lang_sense_clarification": "решение органа власти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "217f0d6a-0eec-31da-9f9d-1594a3cad7f3",
                    "sense_definition": "Wewnętrzna lub społeczna norma postępowania, wynikająca z zasad moralnych, religijnych lub etycznych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "moral_norm",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pomoc potrzebującym to dla wielu ludzi <nakaz> moralny.",
                            "target_lang_translations": {
                                "ru": "Помощь нуждающимся — это для многих людей моральный <w>долг</w>."
                            }
                        },
                        {
                            "text": "Zawsze starał się postępować zgodnie z <nakazem> sumienia.",
                            "target_lang_translations": {
                                "ru": "Он всегда старался поступать по <w>велению</w> совести."
                            }
                        }
                    ],
                    "synonyms": [
                        "przykazanie",
                        "powinność",
                        "obowiązek"
                    ],
                    "common_phrases": [
                        "nakaz sumienia",
                        "nakaz moralny",
                        "nakaz religijny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутренняя или общественная норма поведения, вытекающая из моральных, религиозных или этических принципов."
                    },
                    "translations": {

{
    "word_family": [
        "granitic",
        "granitoid"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "28ad3c49-3f10-316d-9fec-e2c5dc1aaabd",
                    "sense_definition": "A very hard type of rock used in building, usually gray or pink in color.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The kitchen counter was made of polished black <w>granite</w>.",
                            "target_lang_translations": {
                                "ru": "Кухонная столешница была сделана из полированного черного <w>гранита</w>."
                            }
                        },
                        {
                            "text": "Workers extracted large blocks of <w>granite</w> from the quarry for the new monument.",
                            "target_lang_translations": {
                                "ru": "Рабочие извлекали большие блоки <w>гранита</w> из карьера для нового памятника."
                            }
                        }
                    ],
                    "common_phrases": [
                        "granite quarry",
                        "granite slab",
                        "polished granite",
                        "granite rock"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень твердая горная порода, используемая в строительстве, обычно серого или розового цвета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гранит",
                                "target_lang_sense_clarification": "горная порода"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "641200a7-51aa-36e1-8a87-5f2017d3bab2",
                    "sense_definition": "Great hardness, firmness, or lack of feeling; unyielding toughness.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The general stared at his enemies with a face of <w>granite</w>.",
                            "target_lang_translations": {
                                "ru": "Генерал смотрел на своих врагов с лицом из <w>гранита</w>."
                            }
                        },
                        {
                            "text": "She showed a will of <w>granite</w> throughout the difficult negotiations.",
                            "target_lang_translations": {
                                "ru": "Она демонстрировала волю из <w>гранита</w> на протяжении всех сложных переговоров."
                            }
                        }
                    ],
                    "synonyms": [
                        "toughness",
                        "firmness",
                        "hardness"
                    ],
                    "antonyms": [
                        "softness"
                    ],
                    "common_phrases": [
                        "face of granite",
                        "will of granite"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "figurative"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Большая твердость, стойкость или отсутствие чувств; непреклонность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гранит",
                                "target_lang_sense_clarification": "перен. твердость, стойкость"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "a4622d93-a5b9-39e2-ac02-c719eb7b399c",
                    "sense_definition": "The name of several places, including towns in the United States and an island in Australia.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We stopped in the small town of <w>Granite</w> to refuel our car.",
                            "target_lang_translations": {
                                "ru": "Мы остановились в небольшом городке <w>Гранит</w>, чтобы заправить машину."
                            }
                        },
                        {
                            "text": "The map indicates that <w>Granite</w> is located near the mountain range.",
                            "target_lang_translations": {
                                "ru": "Карта указывает, что <w>Гранит</w> расположен недалеко от горного хребта."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Название нескольких мест, включая города в США и остров в Австралии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Гранит",
                                "target_lang_sense_clarification": "топоним"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7b3cecf3-ea2e-3998-b600-11bbde703b7d",
                    "sense_definition": "Great respect and admiration for someone, often because of their achievements or moral character.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "respect_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They held a banquet in <w>honor</w> of the visiting president.",
                            "target_lang_translations": {
                                "ru": "Они устроили банкет в <w>честь</w> визита президента."
                            }
                        },
                        {
                            "text": "The soldiers were buried with full military <w>honors</w>.",
                            "target_lang_translations": {
                                "ru": "Солдаты были похоронены с полными воинскими <w>почестями</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "glory"
                    ],
                    "antonyms": [
                        "dishonor",
                        "contempt"
                    ],
                    "common_phrases": [
                        "in honor of",
                        "guest of honor",
                        "maid of honor",
                        "place of honor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Глубокое уважение и восхищение кем-либо, часто из-за их достижений или моральных качеств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "честь",
                                "target_lang_sense_clarification": "уважение, почет"
                            },
                            {
                                "target_lang_word": "почести",
                                "target_lang_sense_clarification": "церемониальные знаки уважения (обычно во мн. ч.)"
                            },
                            {
                                "target_lang_word": "почёт",
                                "target_lang_sense_clarification": "общественное уважение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2de86ac9-9efb-3cde-bc44-241a398dccd1",
                    "sense_definition": "The quality of knowing and doing what is morally right.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "integrity_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is a man of <w>honor</w> who always keeps his promises.",
                            "target_lang_translations": {
                                "ru": "Он человек <w>чести</w>, который всегда держит свои обещания."
                            }
                        },
                        {
                            "text": "She defended her family's <w>honor</w> against the false accusations.",
                            "target_lang_translations": {
                                "ru": "Она защищала <w>честь</w> своей семьи от ложных обвинений."
                            }
                        }
                    ],
                    "synonyms": [
                        "chivalry",
                        "gentlemanliness"
                    ],
                    "antonyms": [
                        "dishonor"
                    ],
                    "common_phrases": [
                        "word of honor",
                        "code of honor",
                        "man of honor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Качество, заключающееся в знании и выполнении того, что морально правильно; моральный кодекс."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "честь",
                                "target_lang_sense_clarification": "внутреннее нравственное достоинство"
                            },
                            {
                                "target_lang_word": "благородство",
                                "target_lang_sense_clarification": "высокая нравственность"
                            },
                            {
                                "target_lang_word": "порядочность",
                                "target_lang_sense_clarification": "честность, неспособность к низким поступкам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "077a8e9b-37ba-3f3d-95df-e4fef3d424fe",
                    "sense_definition": "A special privilege or pleasure.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "privilege_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I had the <w>honor</w> of meeting the famous scientist last week.",
                            "target_lang_translations": {
                                "ru": "Я имел <w>честь</w> встретиться с известным ученым на прошлой неделе."
                            }
                        },
                        {
                            "text": "Would you do the <w>honors</w> and cut the cake for everyone?",
                            "target_lang_translations": {
                                "ru": "Не окажешь ли ты <w>честь</w> разрезать торт для всех?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "have the honor",
                        "do the honors"
                    ],
                    "target_lang_definitions": {
                        "ru": "Особая привилегия или удовольствие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "честь",
                                "target_lang_sense_clarification": "почетное право или привилегия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc91acb7-ea7d-381b-9fd9-d17d30aae873",
                    "sense_definition": "An award, official title, or rank given to someone as a reward for their achievements.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "award_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She received the highest <w>honor</w> the university could bestow.",
                            "target_lang_translations": {
                                "ru": "Она получила высшую <w>награду</w>, которую мог присудить университет."
                            }
                        },
                        {
                            "text": "The writer has won many national <w>honors</w> for his poetry.",
                            "target_lang_translations": {
                                "ru": "Писатель получил множество национальных <w>наград</w> за свою поэзию."
                            }
                        }
                    ],
                    "common_phrases": [
                        "with honors",
                        "roll of honor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Награда, официальный титул или звание, присуждаемые кому-либо в качестве поощрения за достижения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "награда",
                                "target_lang_sense_clarification": "знак отличия"
                            },
                            {
                                "target_lang_word": "почесть",
                                "target_lang_sense_clarification": "знак общественного признания"
                            },
                            {
                                "target_lang_word": "звание",
                                "target_lang_sense_clarification": "официальный титул"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2b67a25-cf0b-3665-b746-10f2deec5197",
                    "sense_definition": "A university degree of a higher standard than a basic pass degree.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "education_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He graduated with first-class <w>honours</w> in economics.",
                            "target_lang_translations": {
                                "ru": "Он окончил университет с <w>отличием</w> первой степени по экономике."
                            }

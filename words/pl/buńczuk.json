    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5cc01c89-bb38-3a8c-81a7-4e5d7fa770cc",
                    "sense_definition": "Drzewce zakończone kulą lub grotem, ozdobione końskim ogonem, będące dawnym symbolem władzy wojskowej u ludów wschodnich i w dawnej Polsce.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "symbol_wladzy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W muzeum wojskowym oglądaliśmy oryginalny turecki <w>buńczuk</w> zdobyty podczas bitwy.",
                            "target_lang_translations": {
                                "ru": "В военном музее мы рассматривали подлинный турецкий <w>бунчук</w>, захваченный во время битвы."
                            }
                        },
                        {
                            "text": "Hetman rozkazał podnieść <w>buńczuk</w>, dając tym samym sygnał do ataku na nieprzyjaciela.",
                            "target_lang_translations": {
                                "ru": "Гетман приказал поднять <w>бунчук</w>, подавая тем самым сигнал к атаке на неприятеля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "buńczuk hetmański",
                        "turecki buńczuk",
                        "pod buńczukiem"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "odnosi się do dawnych czasów"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "kresy wschodnie, Turcja"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Древко, увенчанное шаром или острием и украшенное конским хвостом, являвшееся старинным символом военной власти у восточных народов и в древней Польше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бунчук",
                                "target_lang_sense_clarification": "символ власти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "65faad0b-13e2-352a-9f6c-741ae1f1ea2d",
                    "sense_definition": "Pęk włosia służący jako ozdoba hełmu rycerskiego lub element dekoracyjny końskiej uprzęży.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "ozdoba",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego lśniący hełm zdobił czarny <w>buńczuk</w>, który powiewał na wietrze.",
                            "target_lang_translations": {
                                "ru": "Его сверкающий шлем украшал черный <w>султан</w>, который развевался на ветру."
                            }
                        },
                        {
                            "text": "Koń paradował dumnie, mając przy uzdzie kolorowy <w>buńczuk</w>.",
                            "target_lang_translations": {
                                "ru": "Конь гордо гарцевал с цветным <w>бунчуком</w> у уздечки."
                            }
                        }
                    ],
                    "synonyms": [
                        "kita"
                    ],
                    "common_phrases": [
                        "buńczuk przy hełmie",
                        "koński buńczuk"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "element dawnego uzbrojenia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пучок волос, служащий украшением рыцарского шлема или декоративным элементом конской упряжи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "султан",
                                "target_lang_sense_clarification": "украшение на шлеме"
                            },
                            {
                                "target_lang_word": "бунчук",
                                "target_lang_sense_clarification": "украшение упряжи"
                            }
                        ]
                    }
                }
            ]
        }
    ]

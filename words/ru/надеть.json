            "senses": [
                {
                    "sense_id": "c16fc261-272b-3d35-87fc-e4013eec4e35",
                    "sense_definition": "Покрыть тело или часть тела одеждой, обувью или аксессуарами.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На улице холодно, поэтому нужно обязательно <w>надеть</w> шапку.",
                            "target_lang_translations": {
                                "en": "It's cold outside, so you definitely need to <w>put on</w> a hat."
                            }
                        },
                        {
                            "text": "Она решила <w>надеть</w> своё лучшее платье на праздничный ужин.",
                            "target_lang_translations": {
                                "en": "She decided to <w>put on</w> her best dress for the festive dinner."
                            }
                        }
                    ],
                    "antonyms": [
                        "снять"
                    ],
                    "common_phrases": [
                        "надеть пальто",
                        "надеть очки",
                        "надеть маску",
                        "надеть обувь",
                        "надеть кольцо"
                    ],
                    "target_lang_definitions": {
                        "en": "To cover the body or part of the body with clothes, shoes, or accessories."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put on",
                                "target_lang_sense_clarification": "clothing or accessories"
                            },
                            {
                                "target_lang_word": "don",
                                "target_lang_sense_clarification": "formal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67f5871b-1742-32ab-be5a-66170ad1bf80",
                    "sense_definition": "Укрепить, насадить или приладить какой-либо предмет на чём-либо.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рыбак показал сыну, как правильно <w>надеть</w> наживку на крючок.",
                            "target_lang_translations": {
                                "en": "The fisherman showed his son how to properly <w>put</w> the bait <w>on</w> the hook."
                            }
                        },
                        {
                            "text": "Чтобы защитить мебель от пыли, мы решили <w>надеть</w> чехол на диван.",
                            "target_lang_translations": {
                                "en": "To protect the furniture from dust, we decided to <w>put</w> a cover <w>on</w> the sofa."
                            }
                        }
                    ],
                    "synonyms": [
                        "насадить",
                        "нацепить"
                    ],
                    "antonyms": [
                        "снять"
                    ],
                    "common_phrases": [
                        "надеть наживку",
                        "надеть чехол",
                        "надеть шину",
                        "надеть наконечник"
                    ],
                    "target_lang_definitions": {
                        "en": "To fasten, mount, or fit an object onto something."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put on",
                                "target_lang_sense_clarification": "an object onto something"
                            },
                            {
                                "target_lang_word": "fit",
                                "target_lang_sense_clarification": "a cover or part"
                            }
                        ]
                    }
                }

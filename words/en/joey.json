    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "dc913597-9530-3791-80d3-5a3f933fd11b",
                    "sense_definition": "A diminutive of the male given names Joe or Joseph.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "name_male",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "My brother <w>Joey</w> plays football every Saturday.",
                            "target_lang_translations": {
                                "ru": "Мой брат <w>Джоуи</w> играет в футбол каждую субботу."
                            }
                        },
                        {
                            "text": "We are waiting for <w>Joey</w> to arrive before we start dinner.",
                            "target_lang_translations": {
                                "ru": "Мы ждем приезда <w>Джоуи</w>, прежде чем начать ужин."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма мужских имен Джо или Джозеф."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Джоуи",
                                "target_lang_sense_clarification": "мужское имя"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc278077-a517-33f6-88da-c2cb8007a9a8",
                    "sense_definition": "A diminutive of the female given name Josephine.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "name_female",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Her aunt <w>Joey</w> sent her a beautiful birthday card.",
                            "target_lang_translations": {
                                "ru": "Её тетя <w>Джоуи</w> прислала ей красивую поздравительную открытку."
                            }
                        },
                        {
                            "text": "Although her name is Josephine, everyone calls her <w>Joey</w>.",
                            "target_lang_translations": {
                                "ru": "Хотя её зовут Жозефина, все называют её <w>Джоуи</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма женского имени Жозефина."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Джоуи",
                                "target_lang_sense_clarification": "женское имя"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "91daed91-9242-301d-a64b-53d8783781e3",
                    "sense_definition": "The immature young of a marsupial, notably a junior kangaroo, but also a young wallaby or koala.",

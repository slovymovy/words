    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "e51475bd-bd70-3d73-a40c-cdcef71c6ff5",
                    "sense_definition": "In or from the nearby area or neighborhood.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We prefer to buy vegetables that are grown <w>locally</w>.",
                            "target_lang_translations": {
                                "ru": "Мы предпочитаем покупать овощи, выращенные <w>в данной местности</w>."
                            }
                        },
                        {
                            "text": "The new restaurant has become very popular <w>locally</w>.",
                            "target_lang_translations": {
                                "ru": "Новый ресторан стал очень популярен <w>в округе</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "nearby",
                        "close by"
                    ],
                    "antonyms": [
                        "globally",
                        "nationally",
                        "internationally",
                        "distantly"
                    ],
                    "common_phrases": [
                        "grown locally",
                        "sourced locally",
                        "produced locally",
                        "shop locally",
                        "available locally"
                    ],
                    "target_lang_definitions": {
                        "ru": "В близлежащем районе или окрестностях; на местном уровне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в данной местности",
                                "target_lang_sense_clarification": "в этом районе"
                            },
                            {
                                "target_lang_word": "локально",
                                "target_lang_sense_clarification": "в определенном месте"
                            },
                            {
                                "target_lang_word": "местно",
                                "target_lang_sense_clarification": "не глобально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a38c7180-28e9-341e-a81f-50bf8154c33f",
                    "sense_definition": "In a restricted part of the body.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The cream should be applied <w>locally</w> to the rash.",
                            "target_lang_translations": {
                                "ru": "Крем следует наносить <w>местно</w> на сыпь."
                            }
                        },
                        {
                            "text": "This medication acts <w>locally</w> in the lungs rather than affecting the whole system.",
                            "target_lang_translations": {
                                "ru": "Это лекарство действует <w>местно</w> в легких, а не влияет на всю систему."
                            }
                        }
                    ],
                    "synonyms": [
                        "topically"
                    ],
                    "antonyms": [
                        "systemically",
                        "generally"
                    ],
                    "common_phrases": [
                        "applied locally",
                        "treat locally"
                    ],
                    "target_lang_definitions": {
                        "ru": "На ограниченном участке тела."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "местно",
                                "target_lang_sense_clarification": "медицинский термин"
                            },
                            {
                                "target_lang_word": "локально",
                                "target_lang_sense_clarification": "точечно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6fec3a65-e5fb-3923-85ad-c344142d5797",
                    "sense_definition": "Within a sufficiently small sphere or interval around a given point.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The surface of the earth appears <w>locally</w> flat to an observer.",
                            "target_lang_translations": {
                                "ru": "Поверхность Земли кажется наблюдателю <w>локально</w> плоской."
                            }
                        },
                        {
                            "text": "We analyzed how the mathematical function behaves <w>locally</w>.",
                            "target_lang_translations": {
                                "ru": "Мы проанализировали, как математическая функция ведет себя <w>локально</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "globally"
                    ],
                    "common_phrases": [
                        "locally Euclidean",
                        "locally compact"
                    ],
                    "target_lang_definitions": {
                        "ru": "В пределах достаточно малой сферы или интервала вокруг данной точки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "локально",
                                "target_lang_sense_clarification": "математический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30b0e0d5-ee0c-3370-955a-6cd23734c125",
                    "sense_definition": "With respect to place or location.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two events were <w>locally</w> distinct even if they happened at the same time.",
                            "target_lang_translations": {
                                "ru": "Эти два события были <w>территориально</w> различны, даже если они произошли в одно и то же время."
                            }
                        },
                        {
                            "text": "These items are <w>locally</w> separated within the storage unit.",
                            "target_lang_translations": {
                                "ru": "Эти предметы <w>пространственно</w> разделены внутри складского блока."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В отношении места или местоположения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "территориально",
                                "target_lang_sense_clarification": "по местоположению"
                            },
                            {
                                "target_lang_word": "пространственно",
                                "target_lang_sense_clarification": "в пространстве"
                            }
                        ]
                    }
                }
            ]
        }
    ]

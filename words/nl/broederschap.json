            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1aa92be5-8647-3c88-af3b-6956ed588b1d",
                    "sense_definition": "Een vriendschappelijke relatie of band tussen mensen die voelt als familie, vergelijkbaar met de band tussen broers.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_relation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de lange reis ontstond er een hechte <w>broederschap</w> tussen de avonturiers.",
                            "target_lang_translations": {
                                "ru": "Во время долгого путешествия между искателями приключений возникло крепкое <w>братство</w>."
                            }
                        },
                        {
                            "text": "Zij geloven in de universele <w>broederschap</w> van alle mensen op aarde.",
                            "target_lang_translations": {
                                "ru": "Они верят во всеобщее <w>братство</w> всех людей на земле."
                            }
                        }
                    ],
                    "synonyms": [
                        "verbroedering",
                        "fraterniteit"
                    ],
                    "common_phrases": [
                        "hechte broederschap",
                        "universele broederschap"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дружеские отношения или связь между людьми, напоминающая семейные узы, подобно связи между братьями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "братство",
                                "target_lang_sense_clarification": "отношения, узы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "20171005-79dc-37df-bac9-c169a91537d4",
                    "sense_definition": "Een vereniging van mensen die een gemeenschappelijk doel, beroep of geloof delen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "organization",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij werd lid van een religieuze <w>broederschap</w> die zorgt voor de armen.",
                            "target_lang_translations": {
                                "ru": "Он стал членом религиозного <w>братства</w>, которое заботится о бедных."
                            }
                        },
                        {
                            "text": "De leden van de <w>broederschap</w> komen elke maand samen voor een vergadering.",
                            "target_lang_translations": {
                                "ru": "Члены <w>братства</w> собираются каждый месяц на собрание."
                            }
                        }
                    ],
                    "synonyms": [
                        "confrerie",
                        "congregatie"
                    ],
                    "common_phrases": [
                        "geheime broederschap",
                        "religieuze broederschap"
                    ],
                    "target_lang_definitions": {
                        "ru": "Объединение людей, разделяющих общую цель, профессию или веру."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "братство",
                                "target_lang_sense_clarification": "организация, сообщество"
                            }
                        ]
                    }
                }
            ]
        }
    ]

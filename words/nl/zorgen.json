                },
                {
                    "sense_id": "4c1f2f64-2cb8-3ce6-934e-a25d341c8362",
                    "sense_definition": "De oorzaak zijn van iets; iets teweegbrengen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cause_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De zware storm heeft voor veel schade aan de huizen <w>gezorgd</w>.",
                            "target_lang_translations": {
                                "en": "The heavy storm <w>caused</w> a lot of damage to the houses.",
                                "ru": "Сильный шторм <w>вызвал</w> значительный ущерб домам."
                            }
                        },
                        {
                            "text": "Zijn onverwachte opmerking <w>zorgde</w> voor een ongemakkelijke stilte in de kamer.",
                            "target_lang_translations": {
                                "en": "His unexpected remark <w>caused</w> an awkward silence in the room.",
                                "ru": "Его неожиданное замечание <w>вызвало</w> неловкое молчание в комнате."
                            }
                        }
                    ],
                    "synonyms": [

                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "служебный",
                                "target_lang_sense_clarification": "относящийся к исполнению обязанностей"
                            },
                            {
                                "target_lang_word": "рабочий",
                                "target_lang_sense_clarification": "используемый для работы (напр. телефон)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5065bbd-74d8-34e0-b164-3629d81d5098",
                    "sense_definition": "Przeznaczony wyłącznie dla personelu lub funkcjonariuszy; związany z pełnieniem służby w formacjach mundurowych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Klienci restauracji nie mogą korzystać z wejścia <w>służbowego</w>.",
                            "target_lang_translations": {
                                "ru": "Клиенты ресторана не могут пользоваться <w>служебным</w> входом."
                            }

{
    "word_family": [
        "muilkorf",
        "muilpeer",
        "muildier",
        "muilezel",
        "muilen"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4c8874ba-8c24-3447-9738-1f502200d0da",
                    "sense_definition": "De bek of snuit van een dier, vooral gebruikt bij grote dieren of roofdieren.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "mouth_face",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leeuw hield de prooi stevig vast in zijn <w>muil</w>.",
                            "target_lang_translations": {
                                "en": "The lion held the prey firmly in its <w>jaws</w>.",
                                "ru": "Лев крепко держал добычу в своей <w>пасти</w>."
                            }
                        },
                        {
                            "text": "De hond kauwde tevreden op het bot met zijn <w>muil</w>.",
                            "target_lang_translations": {
                                "en": "The dog chewed contentedly on the bone with its <w>mouth</w>.",
                                "ru": "Собака довольно грызла кость своей <w>пастью</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "snuit",
                        "smoel"
                    ],
                    "target_lang_definitions": {
                        "en": "The mouth, jaws, or snout of an animal, especially used for large animals or predators.",
                        "ru": "Рот или морда животного, особенно крупного или хищного."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "jaws",
                                "target_lang_sense_clarification": "referring to the mouth and biting mechanism of a predator"
                            },
                            {
                                "target_lang_word": "muzzle",
                                "target_lang_sense_clarification": "the projecting part of an animal's face, including nose and mouth"
                            },
                            {
                                "target_lang_word": "maw",
                                "target_lang_sense_clarification": "the jaws or throat of a voracious animal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пасть",
                                "target_lang_sense_clarification": "рот зверя, хищника"
                            },
                            {
                                "target_lang_word": "морда",
                                "target_lang_sense_clarification": "передняя часть головы животного"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f593d6b7-19fe-3d86-ac8b-1b4067ca2759",
                    "sense_definition": "De mond of het gezicht van een persoon.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "mouth_face",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je nu je <w>muil</w> niet houdt, krijg je problemen.",
                            "target_lang_translations": {
                                "en": "If you don't shut your <w>trap</w> now, you'll get into trouble.",
                                "ru": "Если ты сейчас не закроешь свою <w>пасть</w>, у тебя будут проблемы."
                            }
                        },
                        {
                            "text": "Hij ging hard op zijn <w>muil</w> toen hij over het stoeprandje struikelde.",
                            "target_lang_translations": {
                                "en": "He fell hard on his <w>face</w> when he tripped over the curb.",
                                "ru": "Он сильно упал на <w>морду</w>, когда споткнулся о бордюр."
                            }
                        }
                    ],
                    "synonyms": [
                        "smoel"
                    ],
                    "common_phrases": [
                        "hou je muil",
                        "op je muil gaan",
                        "grote muil"
                    ],
                    "target_lang_definitions": {
                        "en": "The mouth or face of a person (informal, often rude or aggressive).",
                        "ru": "Рот или лицо человека (грубо, неформально)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trap",
                                "target_lang_sense_clarification": "slang for mouth, often used in 'shut your trap'"
                            },
                            {
                                "target_lang_word": "gob",
                                "target_lang_sense_clarification": "British slang for mouth"
                            },
                            {
                                "target_lang_word": "mug",
                                "target_lang_sense_clarification": "slang for face"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пасть",
                                "target_lang_sense_clarification": "грубое название рта (напр. «закрой пасть»)"
                            },
                            {
                                "target_lang_word": "морда",
                                "target_lang_sense_clarification": "грубое название лица"
                            },
                            {
                                "target_lang_word": "харя",
                                "target_lang_sense_clarification": "очень грубое сленговое название лица"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6adba182-7b59-332c-8aff-0a0ead9edf42",
                    "sense_definition": "Een soort schoeisel met een open hiel dat de voet niet volledig omsluit, vaak binnenshuis gedragen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "footwear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De sprookjesprins vond alleen het glazen <w>muiltje</w> op de trap.",
                            "target_lang_translations": {
                                "en": "The fairy tale prince found only the glass <w>slipper</w> on the stairs.",
                                "ru": "Сказочный принц нашел на лестнице только хрустальную <w>туфельку</w>."
                            }
                        },
                        {
                            "text": "Ze draagt graag comfortabele <w>muiltjes</w> als ze thuis is.",
                            "target_lang_translations": {
                                "en": "She likes wearing comfortable <w>mules</w> when she is at home.",
                                "ru": "Ей нравится носить удобные <w>тапочки</w>, когда она дома."
                            }
                        }
                    ],
                    "common_phrases": [
                        "glazen muiltje"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "often used in the diminutive form 'muiltje'"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A type of shoe with an open heel that does not fully enclose the foot, often worn indoors.",
                        "ru": "Тип обуви с открытой пяткой, часто для ношения дома."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mule",
                                "target_lang_sense_clarification": "a backless shoe or slipper"
                            },
                            {
                                "target_lang_word": "slipper",
                                "target_lang_sense_clarification": "light indoor footwear (common translation for 'muiltje' in fairy tales)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "туфля",
                                "target_lang_sense_clarification": "легкая обувь (используется в контексте «хрустальная туфелька»)"
                            },
                            {
                                "target_lang_word": "мюли",
                                "target_lang_sense_clarification": "вид обуви без задника"
                            },
                            {
                                "target_lang_word": "тапок",
                                "target_lang_sense_clarification": "домашняя обувь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "959c23fd-0b4e-38bc-bce1-beafe844f0f4",
                    "sense_definition": "Een kruising tussen een paard en een ezel.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "animal_hybrid",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een <w>muil</w> is vaak sterker dan een paard en koppiger dan een ezel.",
                            "target_lang_translations": {
                                "en": "A <w>mule</w> is often stronger than a horse and more stubborn than a donkey.",
                                "ru": "<w>Мул</w> часто сильнее лошади и упрямее осла."
                            }
                        },
                        {
                            "text": "De boer gebruikte een <w>muil</w> om de zware lasten door de bergen te dragen.",
                            "target_lang_translations": {
                                "en": "The farmer used a <w>mule</w> to carry the heavy loads through the mountains.",
                                "ru": "Фермер использовал <w>мула</w>, чтобы переносить тяжелые грузы через горы."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A hybrid offspring of a horse and a donkey.",
                        "ru": "Гибридное потомство лошади и осла."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mule",
                                "target_lang_sense_clarification": "hybrid animal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "мул",
                                "target_lang_sense_clarification": "животное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7811704b-8655-3a4d-821a-250ab8f4a86a",
                    "sense_definition": "Een grote, donkere opening die doet denken aan de bek van een groot dier.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "mouth_face",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We keken in de gapende <w>muil</w> van de donkere grot.",
                            "target_lang_translations": {
                                "en": "We looked into the gaping <w>maw</w> of the dark cave.",
                                "ru": "Мы смотрели в зияющую <w>пасть</w> темной пещеры."
                            }
                        },
                        {
                            "text": "De trein verdween langzaam in de zwarte <w>muil</w> van de tunnel.",
                            "target_lang_translations": {
                                "en": "The train slowly disappeared into the black <w>mouth</w> of the tunnel.",
                                "ru": "Поезд медленно исчез в черной <w>пасти</w> туннеля."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A large, dark opening resembling the mouth of a large animal.",
                        "ru": "Большое темное отверстие, напоминающее пасть крупного животного."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "maw",
                                "target_lang_sense_clarification": "a gaping, often threatening opening"
                            },
                            {
                                "target_lang_word": "mouth",
                                "target_lang_sense_clarification": "the opening or entrance of a cave or tunnel"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пасть",
                                "target_lang_sense_clarification": "метафорически о темном, пугающем входе"
                            },
                            {
                                "target_lang_word": "зев",
                                "target_lang_sense_clarification": "отверстие, вход (напр. пещеры)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0e21bb0e-30ac-335d-87b7-2301186dc1a9",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>muilen</w> (tongzoenen of pruilen).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>muil</w> niet graag in het openbaar met iemand.",
                            "target_lang_translations": {
                                "en": "I don't like to <w>make out</w> with anyone in public.",
                                "ru": "Я не люблю <w>целоваться взасос</w> с кем-то на людях."
                            }
                        },
                        {
                            "text": "Als ik mijn zin niet krijg, <w>muil</w> ik soms een beetje.",
                            "target_lang_translations": {
                                "en": "If I don't get my way, I sometimes <w>pout</w> a little.",
                                "ru": "Если я не получаю желаемого, я иногда немного <w>дуюсь</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        },
                        {
                            "trait_type": "regional",
                            "comment": "meaning 'to french kiss' is primarily Belgian/Flemish"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of 'muilen' (to french kiss or to pout).",
                        "ru": "Первое лицо единственного числа настоящего времени от 'muilen' (целоваться взасос или дуться)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "make out",
                                "target_lang_sense_clarification": "to kiss passionately (Belgian/Flemish slang)"
                            },
                            {
                                "target_lang_word": "pout",
                                "target_lang_sense_clarification": "to push one's lips forward in annoyance"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "целоваться взасос",
                                "target_lang_sense_clarification": "страстно целоваться (сленг)"
                            },
                            {
                                "target_lang_word": "дуться",
                                "target_lang_sense_clarification": "выражать недовольство выражением лица"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "common_phrases": [
                        "de digitale printknop",
                        "klikken op de printknop"
                    ],
                    "target_lang_definitions": {
                        "ru": "Виртуальная кнопка или значок в компьютерной программе или приложении, с помощью которых можно распечатать документ."
                    },
                    "translations": {

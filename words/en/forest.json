                        {
                            "text": "He has been a fan of <w>Forest</w> since he was a child.",
                            "target_lang_translations": {
                                "ru": "Он болеет за <w>Форест</w> с детства."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Футбольный клуб «Ноттингем Форест»."
                    },
                    "translations": {

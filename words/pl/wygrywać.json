            "pos": "verb",
            "senses": [
                {
                    "sense_id": "470381e8-f032-3f45-a7be-512ce5c4dfe2",
                    "sense_definition": "Odnosić zwycięstwo w rywalizacji sportowej, grze, walce lub sporze; być lepszym od przeciwnika.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "victory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nasza drużyna narodowa rzadko <w>wygrywa</w> z tak silnym przeciwnikiem.",
                            "target_lang_translations": {
                                "ru": "Наша национальная сборная редко <w>выигрывает</w> у такого сильного противника."
                            }
                        },
                        {
                            "text": "Musimy <w>wygrać</w> ten mecz, aby awansować do finału.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>выиграть</w> этот матч, чтобы выйти в финал."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwyciężać",
                        "triumfować"
                    ],
                    "antonyms": [
                        "przegrywać"
                    ],
                    "common_phrases": [
                        "wygrywać mecz",
                        "wygrywać wybory",
                        "wygrywać wojnę",
                        "wygrywać z czasem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Одерживать победу в спортивном состязании, игре, борьбе или споре; быть лучше соперника."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выигрывать",
                                "target_lang_sense_clarification": "в состязании, споре"
                            },
                            {
                                "target_lang_word": "побеждать",
                                "target_lang_sense_clarification": "соперника"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abfdd030-8e09-31e4-b62f-42068ec30e4f",
                    "sense_definition": "Zdobywać nagrodę, pieniądze lub korzyść materialną w konkursie, grze losowej lub zakładzie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "prize",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój wujek <w>wygrał</w> milion złotych na loterii.",
                            "target_lang_translations": {
                                "ru": "Мой дядя <w>выиграл</w> миллион злотых в лотерею."
                            }
                        },
                        {
                            "text": "Mam nadzieję, że <w>wygram</w> główną nagrodę w tym konkursie.",
                            "target_lang_translations": {
                                "ru": "Я надеюсь, что <w>выиграю</w> главный приз в этом конкурсе."
                            }
                        }
                    ],
                    "synonyms": [
                        "zyskiwać"
                    ],
                    "antonyms": [
                        "przegrywać"
                    ],
                    "common_phrases": [
                        "wygrywać na loterii",
                        "wygrywać pieniądze",
                        "wygrywać nagrodę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Получать приз, деньги или материальную выгоду в конкурсе, лотерее или пари."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выигрывать",
                                "target_lang_sense_clarification": "приз, деньги"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6436ff31-8ab8-3122-a29e-84423273f624",
                    "sense_definition": "O losie, bilecie lub numerach: okazywać się zwycięskim, przynosić wygraną.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "prize",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sprawdź w internecie, które numery dzisiaj <w>wygrywają</w>.",
                            "target_lang_translations": {
                                "ru": "Проверь в интернете, какие номера сегодня <w>выигрывают</w>."
                            }
                        },
                        {
                            "text": "W tej zdrapce co trzeci los <w>wygrywa</w>.",
                            "target_lang_translations": {
                                "ru": "В этой моментальной лотерее каждый третий билет <w>выигрывает</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "los wygrywa",
                        "numer wygrywa"
                    ],
                    "target_lang_definitions": {
                        "ru": "О лотерейном билете или числах: оказываться выигрышным, приносить победу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выигрывать",
                                "target_lang_sense_clarification": "о билете, числах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6db4773-a202-3470-851d-17e355ff4fe2",
                    "sense_definition": "Grać konkretną melodię lub utwór na instrumencie muzycznym.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uliczny grajek <w>wygrywał</w> na akordeonie wesołe piosenki.",
                            "target_lang_translations": {
                                "ru": "Уличный музыкант <w>наигрывал</w> на аккордеоне веселые песни."
                            }
                        },
                        {
                            "text": "Zegar na wieży ratuszowej co godzinę <w>wygrywa</w> hejnał.",
                            "target_lang_translations": {
                                "ru": "Часы на башне ратуши каждый час <w>играют</w> сигнал трубы."
                            }
                        }
                    ],
                    "synonyms": [
                        "grać",
                        "wykonywać"
                    ],
                    "common_phrases": [
                        "wygrywać melodię",
                        "wygrywać rytm",
                        "wygrywać marsza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Играть определенную мелодию или произведение на музыкальном инструменте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "наигрывать",
                                "target_lang_sense_clarification": "мелодию"
                            },
                            {
                                "target_lang_word": "играть",
                                "target_lang_sense_clarification": "на инструменте"
                            }
                        ]
                    }
                }
            ]
        }

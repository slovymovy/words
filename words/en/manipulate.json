    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "369bf027-7e05-3cb3-bab1-73c7123e921f",
                    "sense_definition": "To influence or control someone cleverly or unfairly to get what you want.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "influence_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She knows exactly how to <w>manipulate</w> people to get what she wants.",
                            "target_lang_translations": {
                                "ru": "Она точно знает, как <w>манипулировать</w> людьми, чтобы получить желаемое."
                            }
                        },
                        {
                            "text": "The article was written to <w>manipulate</w> public opinion before the election.",
                            "target_lang_translations": {
                                "ru": "Статья была написана, чтобы <w>манипулировать</w> общественным мнением перед выборами."
                            }
                        }
                    ],
                    "synonyms": [
                        "influence",
                        "control",
                        "exploit"
                    ],
                    "common_phrases": [
                        "manipulate people",
                        "manipulate public opinion",
                        "manipulate the media"
                    ],
                    "target_lang_definitions": {
                        "ru": "Влиять на кого-либо или управлять кем-либо ловким или нечестным образом для достижения своих целей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "манипулировать",
                                "target_lang_sense_clarification": "людьми, сознанием"
                            },
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "скрыто влиять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2f6d0435-afcf-33a2-ae21-0e33920ed0fa",
                    "sense_definition": "To change, edit, or control information, data, or systems, often for a specific purpose.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "influence_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This software allows you to <w>manipulate</w> images easily.",
                            "target_lang_translations": {
                                "ru": "Это программное обеспечение позволяет легко <w>обрабатывать</w> изображения."
                            }
                        },
                        {
                            "text": "The accountant was fired because he tried to <w>manipulate</w> data in the report.",
                            "target_lang_translations": {
                                "ru": "Бухгалтера уволили, потому что он пытался <w>подтасовать</w> данные в отчете."
                            }
                        }
                    ],
                    "synonyms": [
                        "alter",
                        "modify",
                        "edit",
                        "falsify"
                    ],
                    "common_phrases": [
                        "manipulate data",
                        "manipulate images",
                        "manipulate the market"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменять, редактировать или контролировать информацию, данные или системы, часто с определенной целью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подтасовывать",
                                "target_lang_sense_clarification": "факты, данные (негативн.)"
                            },
                            {
                                "target_lang_word": "обрабатывать",
                                "target_lang_sense_clarification": "изображения, информацию"
                            },
                            {
                                "target_lang_word": "манипулировать",
                                "target_lang_sense_clarification": "цифрами, данными"
                            },
                            {
                                "target_lang_word": "искажать",
                                "target_lang_sense_clarification": "сведения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7feb3261-68b1-3a1a-9b01-ebcbd6ef84cc",
                    "sense_definition": "To handle, move, or operate an object skillfully using the hands.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_handling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The surgeon had to <w>manipulate</w> the tiny instruments with great care.",
                            "target_lang_translations": {
                                "ru": "Хирургу приходилось с большой осторожностью <w>манипулировать</w> крошечными инструментами."
                            }
                        },
                        {
                            "text": "He learned to <w>manipulate</w> the controls of the machine very quickly.",
                            "target_lang_translations": {
                                "ru": "Он очень быстро научился <w>управлять</w> рычагами машины."
                            }
                        }
                    ],
                    "synonyms": [
                        "handle",
                        "maneuver",
                        "operate"
                    ],
                    "common_phrases": [
                        "manipulate tools",
                        "carefully manipulate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Умело обращаться с предметами, перемещать или использовать их с помощью рук."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "манипулировать",
                                "target_lang_sense_clarification": "предметами"
                            },
                            {
                                "target_lang_word": "управлять",
                                "target_lang_sense_clarification": "механизмами"
                            },
                            {
                                "target_lang_word": "орудовать",
                                "target_lang_sense_clarification": "инструментами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3e557874-66ef-3353-a024-054223ceacbf",
                    "sense_definition": "To move a person's bones or muscles into the correct position using the hands for medical treatment.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_handling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor needed to <w>manipulate</w> the broken bone back into place.",
                            "target_lang_translations": {
                                "ru": "Врачу нужно было <w>вправить</w> сломанную кость на место."
                            }
                        },
                        {
                            "text": "A chiropractor can <w>manipulate</w> the spine to help relieve back pain.",
                            "target_lang_translations": {
                                "ru": "Мануальный терапевт может <w>воздействовать</w> на позвоночник, чтобы помочь облегчить боль в спине."
                            }
                        }
                    ],
                    "common_phrases": [
                        "manipulate the spine",
                        "manipulate a joint"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "medical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Воздействовать руками на кости или мышцы пациента для их правильного расположения или лечения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вправлять",
                                "target_lang_sense_clarification": "кость, сустав"
                            },
                            {
                                "target_lang_word": "проводить манипуляции",
                                "target_lang_sense_clarification": "медицинские"
                            }
                        ]
                    }
                }
            ]
        }
    ]

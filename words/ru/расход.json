    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "aae60bca-7534-3c37-93af-c8221e58d2df",
                    "sense_definition": "Затрата денежных средств или материальных ресурсов на какие-либо нужды.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "financial_spending",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы должны сократить <w>расходы</w> на развлечения в этом месяце.",
                            "target_lang_translations": {
                                "en": "We must cut <w>expenses</w> on entertainment this month."
                            }
                        },
                        {
                            "text": "Этот <w>расход</w> оказался слишком большим для нашего семейного бюджета.",
                            "target_lang_translations": {
                                "en": "This <w>expense</w> turned out to be too big for our family budget."
                            }
                        }
                    ],
                    "synonyms": [
                        "потребность",
                        "утрачивание"
                    ],
                    "antonyms": [
                        "доход",
                        "приход",
                        "накопление"
                    ],
                    "common_phrases": [
                        "сократить расходы",
                        "денежный расход",
                        "непредвиденные расходы",
                        "расходы на питание"
                    ],
                    "target_lang_definitions": {
                        "en": "Money or resources spent to satisfy specific needs."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "expense",
                                "target_lang_sense_clarification": "cost incurred"
                            },
                            {
                                "target_lang_word": "spending",
                                "target_lang_sense_clarification": "act of using money"
                            },
                            {
                                "target_lang_word": "expenditure",
                                "target_lang_sense_clarification": "formal spending"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c710548b-8f4f-3db9-b90b-a654b44bf5ad",
                    "sense_definition": "Количество энергии, топлива или материалов, затрачиваемое на работу механизма или процесса.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У этой машины очень экономичный <w>расход</w> топлива на трассе.",
                            "target_lang_translations": {
                                "en": "This car has very economical fuel <w>consumption</w> on the highway."
                            }
                        },
                        {
                            "text": "Зимой <w>расход</w> электроэнергии в доме значительно увеличивается.",
                            "target_lang_translations": {
                                "en": "In winter, electricity <w>consumption</w> in the house increases significantly."
                            }
                        }
                    ],
                    "antonyms": [
                        "накопление"
                    ],
                    "common_phrases": [
                        "расход топлива",
                        "средний расход",
                        "расход энергии",
                        "расход материалов"
                    ],
                    "target_lang_definitions": {
                        "en": "The amount of energy, fuel, or materials used by a mechanism or process."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "consumption",
                                "target_lang_sense_clarification": "usage of fuel or energy"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "721b4e71-c8f2-3f56-8814-b564bc0b6c63",
                    "sense_definition": "Часть бухгалтерского учёта, в которой фиксируются все издержки и траты; противоположность приходу.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "financial_spending",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Бухгалтер внимательно занёс эту сумму в <w>расход</w>.",
                            "target_lang_translations": {
                                "en": "The accountant carefully entered this amount into <w>expenses</w>."
                            }
                        },
                        {
                            "text": "В конце каждого месяца мы сверяем приход и <w>расход</w>.",
                            "target_lang_translations": {
                                "en": "At the end of each month, we reconcile income and <w>expenditure</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "приход",
                        "доход"
                    ],
                    "common_phrases": [
                        "книга расходов",
                        "записать в расход",
                        "приход и расход"
                    ],
                    "target_lang_definitions": {
                        "en": "The part of accounting where costs are recorded; the opposite of income."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "expenditure",
                                "target_lang_sense_clarification": "accounting term"
                            },
                            {
                                "target_lang_word": "expense",
                                "target_lang_sense_clarification": "recorded cost"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed8e7c7f-7a37-3d7b-855d-29e34d72264d",
                    "sense_definition": "Величина, показывающая количество вещества (жидкости, газа), проходящее через сечение потока за единицу времени.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Прибор точно фиксирует <w>расход</w> воды в главной трубе.",
                            "target_lang_translations": {
                                "en": "The device accurately records the water <w>flow rate</w> in the main pipe."
                            }
                        },
                        {
                            "text": "Минимальный <w>расход</w> жидкости составляет десять литров в секунду.",
                            "target_lang_translations": {
                                "en": "The minimum liquid <w>flow rate</w> is ten liters per second."
                            }
                        }
                    ],
                    "synonyms": [
                        "поток"
                    ],
                    "antonyms": [
                        "аккумуляция"
                    ],
                    "common_phrases": [
                        "расход воды",
                        "расход газа",
                        "расход воздуха"
                    ],
                    "target_lang_definitions": {
                        "en": "The quantity of a substance (liquid or gas) passing through a specific area per unit of time."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flow rate",
                                "target_lang_sense_clarification": "technical term for fluids"
                            },
                            {
                                "target_lang_word": "discharge",
                                "target_lang_sense_clarification": "volume of flow"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "47904583-8178-3008-84ef-bf10fa8afd2b",
                    "sense_definition": "Schoonmaken of vuil verwijderen met behulp van een bezem of borstel.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cleaning_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Vergeet niet de vloer nog te <w>vegen</w>!",
                            "target_lang_translations": {
                                "ru": "Не забудь еще <w>подмести</w> пол!"
                            }
                        },
                        {
                            "text": "De winkelier <w>veegt</w> elke ochtend de stoep voor zijn zaak.",
                            "target_lang_translations": {
                                "ru": "Владелец магазина каждое утро <w>подметает</w> тротуар перед своим заведением."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de vloer vegen",
                        "de straat vegen",
                        "schoon vegen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чистить или удалять грязь с помощью метлы или щетки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подметать",
                                "target_lang_sense_clarification": "убирать метлой"
                            },
                            {
                                "target_lang_word": "мести",
                                "target_lang_sense_clarification": "сметать мусор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a94b1ce5-b8d3-3e7d-81d2-b7e223624e79",
                    "sense_definition": "Iets verplaatsen, weghalen of schoonmaken door er met de hand, een doek of een ander voorwerp langs te strijken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cleaning_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij <w>veegde</w> de kruimels van haar shirt voordat ze opstond.",
                            "target_lang_translations": {
                                "ru": "Она <w>смахнула</w> крошки со своей рубашки, прежде чем встать."
                            }
                        },
                        {
                            "text": "Je moet goed je voeten <w>vegen</w> op de deurmat als je binnenkomt.",
                            "target_lang_translations": {
                                "ru": "Нужно хорошо <w>вытирать</w> ноги о коврик при входе."
                            }
                        }
                    ],
                    "synonyms": [
                        "wissen"
                    ],
                    "common_phrases": [
                        "voeten vegen",
                        "tranen vegen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещать, удалять что-либо или чистить, проводя по поверхности рукой, тряпкой или другим предметом."
                    },
                    "translations": {
                        "ru": [
                            {

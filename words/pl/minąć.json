    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "aeb960ff-bfc4-3eea-9496-a8f80f68193f",
                    "sense_definition": "Upłynąć (o czasie) lub przestać istnieć, skończyć się (o stanie, uczuciu).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szybko <minęło> dwadzieścia minut, zanim przyjechała karetka.",
                            "target_lang_translations": {
                                "ru": "Быстро <прошли> двадцать минут, прежде чем приехала скорая."
                            }
                        },
                        {
                            "text": "Ból głowy nareszcie <minął> po zażyciu silnego leku.",
                            "target_lang_translations": {
                                "ru": "Головная боль наконец <прошла> после приёма сильного лекарства."
                            }
                        },
                        {
                            "text": "Młodość <minęła> im na ciągłych podróżach i zabawie.",
                            "target_lang_translations": {
                                "ru": "Молодость <прошла> у них в постоянных путешествиях и развлечениях."
                            }
                        }
                    ],
                    "synonyms": [
                        "upłynąć",
                        "skończyć się",
                        "ustąpić"
                    ],
                    "antonyms": [
                        "trwać"
                    ],
                    "common_phrases": [
                        "czas minął",
                        "ból minął",
                        "lata minęły"
                    ],
                    "target_lang_definitions": {
                        "ru": "Протечь, пройти (о времени) или прекратиться, исчезнуть (о состоянии, чувстве)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пройти",
                                "target_lang_sense_clarification": "о времени, боли или состоянии"
                            },
                            {
                                "target_lang_word": "миновать",
                                "target_lang_sense_clarification": "о времени или опасности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a1e5096-01f8-3bb4-9f87-77df08bd3642",
                    "sense_definition": "Przemieścić się obok kogoś lub czegoś, zostawiając to za sobą.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Właśnie <minąłem> dużą ciężarówkę na autostradzie.",
                            "target_lang_translations": {
                                "ru": "Я только что <проехал> большой грузовик на автостраде."
                            }
                        },
                        {
                            "text": "Codziennie rano <mijam> ten stary budynek w drodze do pracy.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я <прохожу> мимо этого старого здания по дороге на работу."
                            }
                        },
                        {
                            "text": "Samochód szybko <minął> przystanek autobusowy bez zatrzymywania się.",
                            "target_lang_translations": {
                                "ru": "Автомобиль быстро <проехал> автобусную остановку, не останавливаясь."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyminąć",
                        "wyprzedzić"
                    ],
                    "common_phrases": [
                        "minąć cel",
                        "minąć bez słowa",
                        "minąć kogoś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переместиться мимо кого-либо или чего-либо, оставив это позади."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миновать",
                                "target_lang_sense_clarification": "пройти или проехать мимо"
                            },
                            {
                                "target_lang_word": "проехать",
                                "target_lang_sense_clarification": "мимо чего-либо на транспорте"
                            },
                            {
                                "target_lang_word": "пройти",
                                "target_lang_sense_clarification": "мимо чего-либо пешком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "76172681-a8b5-3f02-9ca8-0338e5a821a3",
                    "sense_definition": "Przejść lub przejechać obok siebie nawzajem w przeciwnych kierunkach.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "reciprocal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rozpędzone samochody <minęły> się o włos na wąskiej drodze.",
                            "target_lang_translations": {
                                "ru": "Мчащиеся машины <разминулись> на волосок друг от друга на узкой дороге."
                            }
                        },
                        {
                            "text": "W drzwiach wejściowych <minęliśmy> się z sąsiadem bez słowa.",
                            "target_lang_translations": {
                                "ru": "В дверях подъезда мы молча <разминулись> с соседом."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyminąć się"
                    ],
                    "antonyms": [
                        "zderzyć się",
                        "spotkać się"
                    ],
                    "common_phrases": [
                        "minąć się z prawdą",
                        "minąć się z powołaniem",
                        "minąć się w drzwiach"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "używane z zaimkiem \"się\""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пройти или проехать мимо друг друга во встречных направлениях."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разминуться",
                                "target_lang_sense_clarification": "пройти мимо друг друга, не столкнувшись"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
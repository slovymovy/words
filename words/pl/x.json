    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f029bf7f-3ad4-30c7-a1c5-903bb20d2b19",
                    "sense_definition": "Siedemnasta litera alfabetu łacińskiego, w języku polskim występująca głównie w wyrazach zapożyczonych.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "letter_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Słowo \"taxi\" jest jednym z niewielu polskich słów zawierających literę <w>x</w>.",
                            "target_lang_translations": {
                                "ru": "Слово \"taxi\" — одно из немногих польских слов, содержащих букву <w>икс</w>."
                            }
                        },
                        {
                            "text": "W polskim alfabecie <w>x</w> znajduje się między w a y.",
                            "target_lang_translations": {
                                "ru": "В польском алфавите <w>икс</w> находится между w и y."
                            }
                        }
                    ],
                    "synonyms": [
                        "iks"
                    ],
                    "common_phrases": [
                        "litera x",
                        "promienie x"
                    ],
                    "target_lang_definitions": {
                        "ru": "Семнадцатая буква латинского алфавита, встречающаяся в польском языке в основном в заимствованных словах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "икс",
                                "target_lang_sense_clarification": "название буквы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0cd9a6f0-76c7-3fed-b1d3-3dd5740695c0",
                    "sense_definition": "Znak graficzny w kształcie dwóch przecinających się linii, używany jako symbol, podpis lub oznaczenie wyboru.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "graphic_sign",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zamiast pełnego podpisu, na dokumencie widniał tylko koślawy <w>x</w>.",
                            "target_lang_translations": {
                                "ru": "Вместо полной подписи на документе стоял только корявый <w>крестик</w>."
                            }
                        },
                        {
                            "text": "Proszę postawić <w>x</w> w kratce obok poprawnej odpowiedzi.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, поставьте <w>крестик</w> в квадратике рядом с правильным ответом."
                            }
                        }
                    ],
                    "synonyms": [
                        "iks"
                    ],
                    "common_phrases": [
                        "postawić x"
                    ],
                    "target_lang_definitions": {
                        "ru": "Графический знак в форме двух пересекающихся линий, используемый как символ, подпись или отметка выбора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крестик",
                                "target_lang_sense_clarification": "знак, отметка"
                            },
                            {
                                "target_lang_word": "икс",
                                "target_lang_sense_clarification": "символ"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "numeral",
            "senses": [
                {
                    "sense_id": "cc45bf14-b31d-3146-99c7-ce8089712439",
                    "sense_definition": "Symbol matematyczny oznaczający niewiadomą wartość lub zmienną w równaniu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "math_variable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciel poprosił nas, abyśmy obliczyli <w>x</w> w tym trudnym równaniu.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил нас вычислить <w>икс</w> в этом сложном уравнении."
                            }
                        },
                        {
                            "text": "Jeśli <w>x</w> jest liczbą dodatnią, to wynik funkcji rośnie.",
                            "target_lang_translations": {
                                "ru": "Если <w>икс</w> — положительное число, то значение функции растет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oś x",
                        "wartość x",
                        "znaleźć x"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mathematics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Математический символ, обозначающий неизвестную величину или переменную в уравнении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "икс",
                                "target_lang_sense_clarification": "математическая переменная"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "898c50f6-b980-372d-b0a4-5f4718a38f99",
                    "sense_definition": "Słowo zastępujące konkretną liczbę, oznaczające nieokreśloną, często dużą ilość.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "indefinite_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Powtarzałem ci to już <w>x</w> razy, a ty nadal nie pamiętasz.",
                            "target_lang_translations": {
                                "ru": "Я повторял тебе это уже <w>икс</w> раз, а ты всё равно не помнишь."
                            }
                        },
                        {
                            "text": "W tym raporcie brakuje jeszcze <w>x</w> danych, żeby był kompletny.",
                            "target_lang_translations": {
                                "ru": "В этом отчете не хватает еще <w>икс</w> данных, чтобы он был полным."
                            }
                        }
                    ],
                    "synonyms": [
                        "n"
                    ],
                    "common_phrases": [
                        "x razy",
                        "godzina x"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in speech to imply 'many' or 'unknown number'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, заменяющее конкретное число, обозначающее неопределенное, часто большое количество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "икс",
                                "target_lang_sense_clarification": "неизвестное число"
                            },
                            {
                                "target_lang_word": "эн",
                                "target_lang_sense_clarification": "неопределенное количество"
                            }
                        ]
                    }
                }
            ]
        },
        {

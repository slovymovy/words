{
    "word_family": [
        "increasingly",
        "increment",
        "decrease"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3d1ef8ce-4dec-3f29-9b36-ee256c7fbfa2",
                    "sense_definition": "To become larger or greater in amount, number, or degree.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "general_growth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The population of the city continues to <w>increase</w> every year.",
                            "target_lang_translations": {
                                "ru": "Население города продолжает <w>увеличиваться</w> с каждым годом."
                            }
                        },
                        {
                            "text": "Prices will likely <w>increase</w> if the demand for oil goes up.",
                            "target_lang_translations": {
                                "ru": "Цены, вероятно, <w>вырастут</w>, если спрос на нефть поднимется."
                            }
                        }
                    ],
                    "synonyms": [
                        "grow",
                        "rise",
                        "go up",
                        "soar",
                        "shoot up"
                    ],
                    "antonyms": [
                        "decrease",
                        "drop",
                        "fall",
                        "go down",
                        "shrink",
                        "plummet",
                        "plunge",
                        "sink"
                    ],
                    "common_phrases": [
                        "increase rapidly",
                        "increase steadily",
                        "increase in value",
                        "increase in size"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться больше по количеству, числу или степени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "увеличиваться",
                                "target_lang_sense_clarification": "становиться больше по величине или объему"
                            },
                            {
                                "target_lang_word": "расти",
                                "target_lang_sense_clarification": "увеличиваться в размерах или количестве"
                            },
                            {
                                "target_lang_word": "возрастать",
                                "target_lang_sense_clarification": "становиться больше, усиливаться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "270e1a86-b894-396f-8f56-bf7b02d19b9f",
                    "sense_definition": "To make something larger or greater in amount, number, or degree.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "general_growth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company plans to <w>increase</w> its sales by ten percent.",
                            "target_lang_translations": {
                                "ru": "Компания планирует <w>увеличить</w> свои продажи на десять процентов."
                            }
                        },
                        {
                            "text": "You should <w>increase</w> your intake of water during hot weather.",
                            "target_lang_translations": {
                                "ru": "Вам следует <w>увеличить</w> потребление воды в жаркую погоду."
                            }
                        }
                    ],
                    "synonyms": [
                        "raise",
                        "augment",
                        "up"
                    ],
                    "antonyms": [
                        "reduce",
                        "lower",
                        "cut",
                        "minimize",
                        "lessen"
                    ],
                    "common_phrases": [
                        "increase the risk",
                        "increase production",
                        "increase awareness",
                        "increase the pressure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать что-либо больше по количеству, числу или степени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "увеличивать",
                                "target_lang_sense_clarification": "делать большим по количеству или величине"
                            },
                            {
                                "target_lang_word": "повышать",
                                "target_lang_sense_clarification": "увеличивать уровень или степень"
                            },
                            {
                                "target_lang_word": "наращивать",
                                "target_lang_sense_clarification": "постепенно увеличивать или добавлять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4af7041e-dd0f-3d19-b330-e3d9274e4fa2",
                    "sense_definition": "To become more nearly full; to show more of the surface (of the moon).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "astronomical_phase",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As the moon <w>increases</w>, the nights become brighter.",
                            "target_lang_translations": {
                                "ru": "По мере того как луна <w>растет</w>, ночи становятся светлее."
                            }
                        },
                        {
                            "text": "We watched the moon <w>increase</w> towards its full phase over the week.",
                            "target_lang_translations": {
                                "ru": "Мы наблюдали, как луна <w>растет</w> до полной фазы в течение недели."
                            }
                        }
                    ],
                    "synonyms": [
                        "wax"
                    ],
                    "antonyms": [
                        "wane"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться более полным; показывать большую часть поверхности (о Луне)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расти",
                                "target_lang_sense_clarification": "о фазе Луны"
                            },
                            {
                                "target_lang_word": "прибывать",
                                "target_lang_sense_clarification": "увеличиваться (о воде или Луне)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2a941e13-1339-325d-95b7-b093ecab077c",
                    "sense_definition": "To multiply by the production of young; to be fertile or prolific.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "biological_reproduction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rabbits <w>increased</w> rapidly in the absence of predators.",
                            "target_lang_translations": {
                                "ru": "Кролики быстро <w>размножились</w> в отсутствие хищников."
                            }
                        },
                        {
                            "text": "They were told to be fruitful and <w>increase</w> in number.",
                            "target_lang_translations": {
                                "ru": "Им было сказано плодиться и <w>размножаться</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "proliferate",
                        "propagate",
                        "teem"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern usage prefers 'multiply' or 'reproduce'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Размножаться, производить потомство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "размножаться",
                                "target_lang_sense_clarification": "производить потомство"
                            },
                            {
                                "target_lang_word": "плодиться",
                                "target_lang_sense_clarification": "размножаться в большом количестве"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "36c443ef-289e-3320-b4cb-508152f2da75",
                    "sense_definition": "An amount by which a quantity is added to or made larger.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "noun_amount",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The workers demanded a significant wage <w>increase</w> this year.",
                            "target_lang_translations": {
                                "ru": "Рабочие потребовали значительной <w>прибавки</w> к зарплате в этом году."
                            }
                        },
                        {
                            "text": "There was a slight <w>increase</w> in temperature during the afternoon.",
                            "target_lang_translations": {
                                "ru": "Днем наблюдалось небольшое <w>повышение</w> температуры."
                            }
                        }
                    ],
                    "synonyms": [
                        "rise",
                        "gain",
                        "increment",
                        "uptick"
                    ],
                    "antonyms": [
                        "decrease",
                        "drop",
                        "reduction",
                        "loss",
                        "cut"
                    ],
                    "common_phrases": [
                        "price increase",
                        "tax increase",
                        "wage increase",
                        "sharp increase"
                    ],
                    "target_lang_definitions": {
                        "ru": "Величина, на которую что-либо прибавилось или стало больше."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прибавка",
                                "target_lang_sense_clarification": "дополнительное количество (обычно о деньгах)"
                            },
                            {
                                "target_lang_word": "повышение",
                                "target_lang_sense_clarification": "увеличение уровня или цены"
                            },
                            {
                                "target_lang_word": "прирост",
                                "target_lang_sense_clarification": "величина увеличения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0aad74b-8884-3524-90fc-39be244da2bf",
                    "sense_definition": "The act or process of becoming larger.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "noun_process",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rapid <w>increase</w> of the city's population caused housing shortages.",
                            "target_lang_translations": {
                                "ru": "Быстрый <w>рост</w> населения города вызвал нехватку жилья."
                            }
                        },
                        {
                            "text": "We are seeing a steady <w>increase</w> in the use of renewable energy.",
                            "target_lang_translations": {
                                "ru": "Мы наблюдаем устойчивый <w>рост</w> использования возобновляемых источников энергии."
                            }
                        }
                    ],
                    "synonyms": [
                        "expansion",
                        "enlargement"
                    ],
                    "antonyms": [
                        "decline",
                        "diminishment",
                        "shrinkage"
                    ],
                    "common_phrases": [
                        "on the increase",
                        "rate of increase"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие или процесс становления большим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рост",
                                "target_lang_sense_clarification": "процесс увеличения"
                            },
                            {
                                "target_lang_word": "увеличение",
                                "target_lang_sense_clarification": "действие по увеличению размера или числа"
                            },
                            {
                                "target_lang_word": "возрастание",
                                "target_lang_sense_clarification": "процесс усиления или роста"
                            }
                        ]
                    }
                },
                {

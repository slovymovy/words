{
    "word_family": [
        "tune",
        "tuner",
        "tuning",
        "tunability",
        "untunable"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cf1c710b-df58-3466-a9a9-d43a248c9d58",
                    "sense_definition": "Capable of being adjusted to a particular pitch, frequency, or setting.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "adjustable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The laboratory uses a <w>tunable</w> laser to study different wavelengths of light.",
                            "target_lang_translations": {
                                "ru": "Лаборатория использует <w>перестраиваемый</w> лазер для изучения света с различной длиной волны."
                            }
                        },
                        {
                            "text": "This radio receiver is <w>tunable</w> across a wide range of frequencies.",
                            "target_lang_translations": {
                                "ru": "Этот радиоприемник является <w>перестраиваемым</w> в широком диапазоне частот."
                            }
                        }
                    ],
                    "synonyms": [
                        "adjustable",
                        "variable"
                    ],
                    "antonyms": [
                        "fixed"
                    ],
                    "common_phrases": [
                        "tunable laser",
                        "tunable filter",
                        "tunable light"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способный быть настроенным на определенную высоту тона, частоту или режим работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "настраиваемый",
                                "target_lang_sense_clarification": "общий термин для устройств или параметров"
                            },
                            {
                                "target_lang_word": "перестраиваемый",
                                "target_lang_sense_clarification": "технический термин (о лазерах, частотах, приемниках)"
                            },
                            {
                                "target_lang_word": "регулируемый",
                                "target_lang_sense_clarification": "поддающийся регулировке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7562fa25-84eb-3a1f-9e5d-4d5f020c7a0c",
                    "sense_definition": "Having a pleasant, harmonious, or melodious sound.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "melodious",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The choir sang with a <w>tunable</w> and sweet voice that filled the hall.",
                            "target_lang_translations": {
                                "ru": "Хор пел <w>мелодичными</w> и нежными голосами, которые наполнили зал."
                            }
                        },
                        {
                            "text": "He played a <w>tunable</w> melody on his wooden flute.",
                            "target_lang_translations": {
                                "ru": "Он сыграл <w>благозвучную</w> мелодию на своей деревянной флейте."
                            }
                        }
                    ],
                    "synonyms": [
                        "melodious",
                        "tuneful",
                        "harmonious"
                    ],
                    "antonyms": [
                        "discordant",
                        "dissonant"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "usage is largely obsolete in modern English, replaced by 'tuneful' or 'melodious'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий приятным, гармоничным или мелодичным звучанием (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мелодичный",
                                "target_lang_sense_clarification": "приятный для слуха"
                            },
                            {
                                "target_lang_word": "благозвучный",
                                "target_lang_sense_clarification": "гармоничный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c5c1a1f9-2aeb-3ce5-94df-1e69d26c4064",
                    "sense_definition": "A parameter or setting in a computer system that can be configured to adjust performance.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The administrator adjusted several kernel <w>tunables</w> to improve the server's speed.",
                            "target_lang_translations": {
                                "ru": "Администратор изменил несколько <w>настраиваемых параметров</w> ядра, чтобы увеличить скорость работы сервера."
                            }
                        },
                        {
                            "text": "There are many <w>tunables</w> available in the advanced configuration menu.",
                            "target_lang_translations": {
                                "ru": "В меню расширенной конфигурации доступно множество <w>настроек</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "parameter",
                        "setting",
                        "configuration"
                    ],
                    "common_phrases": [
                        "kernel tunables",
                        "system tunables"
                    ],
                    "target_lang_definitions": {
                        "ru": "Параметр или настройка в компьютерной системе, которую можно изменить для регулировки производительности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "настраиваемый параметр",
                                "target_lang_sense_clarification": "компьютерный термин"
                            },
                            {
                                "target_lang_word": "настройка",
                                "target_lang_sense_clarification": "параметр конфигурации"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "156bd043-c408-3a0b-b824-0e483373a03d",
                    "sense_definition": "Het oppervlak van de aarde of de vloer waarop men staat en loopt.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "fysieke_laag",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zet die zware tas maar even op de <w>grond</w>.",
                            "target_lang_translations": {
                                "ru": "Поставь эту тяжелую сумку на <w>пол</w>."
                            }
                        },
                        {
                            "text": "Na de lange vlucht was ik blij om weer op de <w>grond</w> te staan.",
                            "target_lang_translations": {
                                "ru": "После долгого перелета я был рад снова стоять на <w>земле</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "op de grond",
                        "naar de grond",
                        "van de grond"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поверхность земли или пол, на котором стоят и ходят."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "земля",
                                "target_lang_sense_clarification": "поверхность планеты"
                            },
                            {
                                "target_lang_word": "пол",
                                "target_lang_sense_clarification": "покрытие в помещении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8a569fa-03f0-3096-8add-16603120f89c",
                    "sense_definition": "Het materiaal waaruit de bovenste laag van de aarde bestaat en waarin planten groeien.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "fysieke_laag",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De tuinman stopt de plantjes voorzichtig in de vruchtbare <w>grond</w>.",
                            "target_lang_translations": {
                                "ru": "Садовник осторожно сажает растения в плодородную <w>почву</w>."
                            }
                        },
                        {
                            "text": "Het kind speelde buiten en zat met zijn handen in de <w>grond</w>.",
                            "target_lang_translations": {
                                "ru": "Ребенок играл на улице и копался руками в <w>земле</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "aarde"
                    ],
                    "common_phrases": [
                        "in de grond",
                        "vruchtbare grond",
                        "goede grond"
                    ],
                    "target_lang_definitions": {
                        "ru": "Материал, составляющий верхний слой земли, в котором растут растения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "земля",
                                "target_lang_sense_clarification": "вещество"
                            },
                            {
                                "target_lang_word": "почва",
                                "target_lang_sense_clarification": "плодородный слой"
                            },
                            {
                                "target_lang_word": "грунт",
                                "target_lang_sense_clarification": "основа для посадки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e68fea7f-470e-34f5-80f5-d0ca1358f1cc",
                    "sense_definition": "Een begrensd stuk land dat eigendom is van iemand of voor een bepaald doel wordt gebruikt.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "fysieke_laag",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De gemeente heeft deze <w>grond</w> verkocht aan een projectontwikkelaar.",
                            "target_lang_translations": {
                                "ru": "Муниципалитет продал эту <w>землю</w> застройщику."
                            }
                        },
                        {
                            "text": "Wij zoeken een mooi stuk <w>grond</w> om ons eigen huis op te bouwen.",
                            "target_lang_translations": {
                                "ru": "Мы ищем хороший участок <w>земли</w>, чтобы построить свой дом."
                            }
                        }
                    ],
                    "synonyms": [
                        "terrein"
                    ],
                    "common_phrases": [
                        "stuk grond",
                        "grond kopen",
                        "eigen grond"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ограниченный участок земли, который кому-то принадлежит или используется для определенной цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "земля",
                                "target_lang_sense_clarification": "земельная собственность"
                            },
                            {
                                "target_lang_word": "участок",
                                "target_lang_sense_clarification": "кусок земли"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "752283a0-66cf-3bf7-936d-d50673b5fce9",
                    "sense_definition": "De reden of het argument dat als basis dient voor een besluit, mening of handeling.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstracte_basis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op welke <w>grond</w> heeft de commissie deze beslissing genomen?",
                            "target_lang_translations": {
                                "ru": "На каком <w>основании</w> комиссия приняла это решение?"
                            }
                        },
                        {
                            "text": "Er is geen wettelijke <w>grond</w> om zijn verzoek te weigeren.",
                            "target_lang_translations": {
                                "ru": "Нет никаких законных <w>оснований</w>, чтобы отказать в его просьбе."
                            }
                        }
                    ],
                    "synonyms": [
                        "reden",
                        "basis",
                        "argument",
                        "fundering",
                        "motivatie",
                        "beweegreden",
                        "onderbouwing"

                    "semantic_group_id": "direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Окна старой избы выходили на <w>лето</w>, поэтому там всегда было светло.",
                            "target_lang_translations": {
                                "en": "The windows of the old hut faced <w>south</w>, so it was always light there."

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "13575977-a841-3192-bc1e-f8d4dfd35565",
                    "sense_definition": "Real or perceived movement of atmospheric air usually caused by convection or differences in air pressure.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "atmospheric_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A cold <w>wind</w> blew from the north, making everyone shiver.",
                            "target_lang_translations": {
                                "ru": "Холодный <w>ветер</w> дул с севера, заставляя всех дрожать."
                            }
                        },
                        {
                            "text": "The strong <w>wind</w> knocked over the garden chairs.",
                            "target_lang_translations": {
                                "ru": "Сильный <w>ветер</w> опрокинул садовые стулья."
                            }
                        }
                    ],
                    "synonyms": [
                        "breeze",
                        "gale"
                    ],
                    "common_phrases": [
                        "strong wind",
                        "gust of wind",
                        "blow in the wind"
                    ],
                    "target_lang_definitions": {
                        "ru": "Движение атмосферного воздуха, обычно вызванное конвекцией или разницей давления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ве́тер",
                                "target_lang_sense_clarification": "движение воздуха"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "44ce914b-2868-3697-8d3b-abbb0c551d3e",
                    "sense_definition": "The ability to breathe easily, often used in the context of physical exertion or impact.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physiological_air",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy fall knocked the <w>wind</w> out of him for a moment.",
                            "target_lang_translations": {
                                "ru": "Тяжёлое падение на мгновение выбило из него <w>дух</w>."
                            }
                        },
                        {
                            "text": "He stopped running to catch his <w>wind</w> before continuing up the hill.",
                            "target_lang_translations": {
                                "ru": "Он перестал бежать, чтобы перевести <w>дух</w>, прежде чем продолжить подъём на холм."
                            }
                        }
                    ],
                    "common_phrases": [
                        "knock the wind out",
                        "catch one's wind",
                        "out of wind"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность свободно дышать, дыхание (часто в контексте физических нагрузок или ударов)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дыха́ние",
                                "target_lang_sense_clarification": "способность дышать"
                            },
                            {
                                "target_lang_word": "дух",
                                "target_lang_sense_clarification": "разг. дыхание, силы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4f370f8-4ece-39eb-a9cc-9e1a674b0e0e",
                    "sense_definition": "Gas in the stomach or intestines.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physiological_air",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The baby was crying because she had some <w>wind</w> after feeding.",
                            "target_lang_translations": {
                                "ru": "Ребёнок плакал, потому что у него были <w>газы</w> после кормления."
                            }
                        },
                        {
                            "text": "He felt embarrassed to pass <w>wind</w> during the quiet meeting.",
                            "target_lang_translations": {
                                "ru": "Ему было стыдно пускать <w>ветры</w> во время тихой встречи."
                            }
                        }
                    ],
                    "synonyms": [
                        "gas"
                    ],
                    "common_phrases": [
                        "pass wind",
                        "break wind"
                    ],
                    "target_lang_definitions": {
                        "ru": "Газы в желудке или кишечнике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "га́зы",
                                "target_lang_sense_clarification": "скопление газов в кишечнике"
                            },
                            {
                                "target_lang_word": "ве́тры",
                                "target_lang_sense_clarification": "мед. устар. газы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6216ffe4-eb27-30b5-8b16-63ecf3f8d40c",
                    "sense_definition": "The section of an orchestra consisting of woodwind and sometimes brass instruments.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "music_instruments",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>wind</w> section played a beautiful melody during the second movement.",
                            "target_lang_translations": {
                                "ru": "Секция <w>духовых</w> сыграла прекрасную мелодию во второй части."
                            }
                        },
                        {
                            "text": "She plays the flute in the <w>wind</w> ensemble at school.",
                            "target_lang_translations": {
                                "ru": "Она играет на флейте в школьном ансамбле <w>духовых</w> инструментов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wind instrument",
                        "wind section"
                    ],
                    "target_lang_definitions": {
                        "ru": "Секция оркестра, состоящая из деревянных и иногда медных духовых инструментов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "духовы́е",
                                "target_lang_sense_clarification": "духовые инструменты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83006880-4c08-3738-ae8c-a1e8b0146660",
                    "sense_definition": "News or information about an event, especially obtained through hearsay or gossip.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "news_rumor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police got <w>wind</w> of the plan and arrived before the robbers could escape.",
                            "target_lang_translations": {
                                "ru": "Полиция пронюхала о плане и прибыла до того, как грабители успели скрыться."
                            }
                        },
                        {
                            "text": "I hope the boss doesn't catch <w>wind</w> of our surprise party for him.",
                            "target_lang_translations": {
                                "ru": "Надеюсь, босс не узнает о вечеринке-сюрпризе для него."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get wind of",
                        "catch wind of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Новости или информация о событии, особенно полученные через слухи или сплетни."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "слух",
                                "target_lang_sense_clarification": "молва, непроверенная информация"
                            },
                            {
                                "target_lang_word": "сведе́ния",
                                "target_lang_sense_clarification": "информация"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d9066b0a-ada4-3dd2-88cf-5f001575b357",
                    "sense_definition": "To turn coils of a cord, string, or similar material around something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "circular_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>wind</w> the wool into a neat ball so we can use it later.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>смотай</w> шерсть в аккуратный клубок, чтобы мы могли использовать её позже."
                            }
                        },
                        {
                            "text": "She <w>wound</w> the long scarf around her neck to keep warm.",
                            "target_lang_translations": {
                                "ru": "Она <w>обмотала</w> длинный шарф вокруг шеи, чтобы согреться."
                            }
                        }
                    ],
                    "synonyms": [
                        "flatus",
                        "gas"
                    ],
                    "common_phrases": [
                        "wind up",
                        "wind around"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наматывать витки веревки, нити или подобного материала вокруг чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нама́тывать",
                                "target_lang_sense_clarification": "навивать на что-то"
                            },
                            {
                                "target_lang_word": "обвива́ть",
                                "target_lang_sense_clarification": "обматывать вокруг"
                            },
                            {
                                "target_lang_word": "сматывать",
                                "target_lang_sense_clarification": "сворачивать в клубок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06bd42f8-54c6-3aa2-bbea-ba1351fe58f3",
                    "sense_definition": "To travel or follow a path with numerous curves and turns.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "circular_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The river <w>winds</w> through the green valley before reaching the sea.",
                            "target_lang_translations": {
                                "ru": "Река <w>извивается</w> по зелёной долине, прежде чем достичь моря."
                            }
                        },
                        {
                            "text": "A narrow dirt road <w>winds</w> up the steep mountain side.",
                            "target_lang_translations": {
                                "ru": "Узкая грунтовая дорога <w>петляет</w> вверх по крутому склону горы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wind through",
                        "wind down"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться или следовать по пути с многочисленными изгибами и поворотами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "извива́ться",
                                "target_lang_sense_clarification": "о реке или дороге"
                            },
                            {
                                "target_lang_word": "петля́ть",
                                "target_lang_sense_clarification": "делать петли, повороты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a814f280-6280-3659-8b16-a3511541e7c3",
                    "sense_definition": "To tighten the spring of a clockwork mechanism to make it operate.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "circular_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't forget to <w>wind</w> the alarm clock before you go to sleep.",
                            "target_lang_translations": {
                                "ru": "Не забудь <w>завести</w> будильник перед сном."
                            }
                        },
                        {
                            "text": "He carefully <w>wound</w> his grandfather's old watch every morning.",
                            "target_lang_translations": {
                                "ru": "Он осторожно <w>заводил</w> старые дедушкины часы каждое утро."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wind a clock",
                        "wind a watch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Затягивать пружину часового механизма, чтобы привести его в действие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заводи́ть",
                                "target_lang_sense_clarification": "механизм или часы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0b69a351-68e8-3836-9bec-1bc6ba8551c7",
                    "sense_definition": "To cause something to move up or down by turning a handle or using a mechanism.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "circular_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Could you please <w>wind</w> the car window down to let in some fresh air?",
                            "target_lang_translations": {
                                "ru": "Не мог бы ты <w>опустить</w> стекло машины, чтобы впустить немного свежего воздуха?"
                            }
                        },
                        {
                            "text": "She <w>wound</w> the bucket up from the bottom of the well.",
                            "target_lang_translations": {
                                "ru": "Она <w>подняла</w> ведро со дна колодца."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wind the window",
                        "wind up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заставлять что-либо двигаться вверх или вниз, вращая ручку или используя механизм."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крути́ть",
                                "target_lang_sense_clarification": "вращать ручку"
                            },
                            {
                                "target_lang_word": "поднима́ть",
                                "target_lang_sense_clarification": "с помощью механизма"
                            },
                            {
                                "target_lang_word": "опуска́ть",
                                "target_lang_sense_clarification": "с помощью механизма"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "561016d6-3be0-3cb7-946c-58a16c141af4",
                    "sense_definition": "To cause someone to become breathless, usually by a blow to the body or physical exertion.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physiological_air",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The football player was <w>winded</w> by the tackle but soon recovered.",
                            "target_lang_translations": {
                                "ru": "Футболисту <w>сбило дыхание</w> при захвате, но он вскоре пришёл в себя."
                            }
                        },
                        {
                            "text": "Running up the stairs so quickly completely <w>winded</w> me.",
                            "target_lang_translations": {
                                "ru": "Быстрый бег вверх по лестнице совершенно <w>лишил меня сил</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "get winded"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишить кого-либо возможности дышать, обычно ударом по телу или физическим напряжением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбить дыха́ние",
                                "target_lang_sense_clarification": "заставить задыхаться ударом"
                            },
                            {
                                "target_lang_word": "лиши́ть сил",
                                "target_lang_sense_clarification": "утомить до одышки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3637e56d-5eb6-3e4f-94a3-679e812156c3",
                    "sense_definition": "To cause a baby to bring up gas from the stomach by patting its back.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physiological_air",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should gently <w>wind</w> the baby after every bottle.",
                            "target_lang_translations": {
                                "ru": "Вам следует осторожно помочь ребёнку <w>срыгнуть</w> после каждой бутылочки."
                            }
                        },
                        {
                            "text": "The nurse showed the new parents how to <w>wind</w> their infant correctly.",
                            "target_lang_translations": {
                                "ru": "Медсестра показала молодым родителям, как правильно держать младенца, чтобы он <w>срыгнул</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wind the baby"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Помочь младенцу выпустить газы из желудка, похлопывая его по спине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "помо́чь срыгну́ть",
                                "target_lang_sense_clarification": "удалить воздух из желудка младенца"
                            }
                        ]
                    }
                }
            ]
        },

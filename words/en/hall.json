{
    "word_family": [
        "hallway",
        "hallmark"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9c4d0110-56ad-3790-a963-2698f8b79904",
                    "sense_definition": "A passage inside a building that connects rooms or leads to the entrance.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "corridor_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please leave your muddy boots in the <w>hall</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, оставьте грязные ботинки в <w>прихожей</w>."
                            }
                        },
                        {
                            "text": "The bathroom is just down the <w>hall</w> on the left.",
                            "target_lang_translations": {
                                "ru": "Ванная прямо по <w>коридору</w> слева."
                            }
                        }
                    ],
                    "synonyms": [
                        "corridor",
                        "hallway",
                        "passage",
                        "passageway"
                    ],
                    "common_phrases": [
                        "down the hall",
                        "entrance hall",
                        "front hall"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проход внутри здания, соединяющий комнаты или ведущий к входу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коридор",
                                "target_lang_sense_clarification": "узкий проход, соединяющий части здания"
                            },
                            {
                                "target_lang_word": "прихожая",
                                "target_lang_sense_clarification": "помещение сразу после входа в квартиру или дом"
                            },
                            {
                                "target_lang_word": "холл",
                                "target_lang_sense_clarification": "просторное помещение для ожидания или отдыха, обычно у входа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78c66a1f-10e7-3fc1-9bee-f18330be86d1",
                    "sense_definition": "A large room or building used for public events, meetings, concerts, or official business.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "large_room_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The orchestra played beautifully in the concert <w>hall</w>.",
                            "target_lang_translations": {
                                "ru": "Оркестр прекрасно играл в концертном <w>зале</w>."
                            }
                        },
                        {
                            "text": "Local residents gathered at the town <w>hall</w> to discuss the new park.",
                            "target_lang_translations": {
                                "ru": "Местные жители собрались в <w>ратуше</w>, чтобы обсудить новый парк."
                            }
                        }
                    ],
                    "synonyms": [
                        "auditorium",
                        "assembly room"
                    ],
                    "common_phrases": [
                        "town hall",
                        "city hall",
                        "concert hall",
                        "dance hall",
                        "mess hall"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое помещение или здание, используемое для общественных мероприятий, собраний, концертов или официальных дел."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зал",
                                "target_lang_sense_clarification": "большое помещение для публики или собраний"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06431617-5bbb-3cad-8569-21b72fbfbb38",
                    "sense_definition": "A building where university students live during the term.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Most first-year students choose to live in <w>halls</w> to make friends easily.",
                            "target_lang_translations": {
                                "ru": "Большинство первокурсников предпочитают жить в <w>общежитии</w>, чтобы быстрее найти друзей."
                            }
                        },
                        {
                            "text": "She moved out of the <w>hall</w> and into a private apartment.",
                            "target_lang_translations": {
                                "ru": "Она съехала из <w>общежития</w> в частную квартиру."
                            }
                        }
                    ],
                    "synonyms": [
                        "dormitory",
                        "residence hall"
                    ],
                    "common_phrases": [
                        "halls of residence"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "common in British English; US English typically uses 'dormitory'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Здание, где живут студенты университета во время семестра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "общежитие",
                                "target_lang_sense_clarification": "место проживания студентов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "11f195ee-3ef2-3634-ae6a-8f06ef5655c5",
                    "sense_definition": "A large country house or mansion, often the main house of an estate.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The duke lived in a great <w>hall</w> overlooking the sea.",
                            "target_lang_translations": {
                                "ru": "Герцог жил в огромной <w>усадьбе</w> с видом на море."
                            }
                        },
                        {
                            "text": "Tourists can visit the historic <w>hall</w> and its beautiful gardens.",
                            "target_lang_translations": {
                                "ru": "Туристы могут посетить историческое <w>поместье</w> и его прекрасные сады."
                            }
                        }
                    ],
                    "synonyms": [
                        "manor",
                        "mansion",
                        "estate"
                    ],
                    "common_phrases": [
                        "great hall"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большой загородный дом или особняк, часто главный дом поместья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усадьба",
                                "target_lang_sense_clarification": "господский дом с прилегающими строениями"
                            },
                            {
                                "target_lang_word": "поместье",
                                "target_lang_sense_clarification": "земельное владение с домом"
                            },
                            {
                                "target_lang_word": "особняк",
                                "target_lang_sense_clarification": "большой частный дом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e2490d1-f9bd-3695-86d3-ad9fe7c2c66d",
                    "sense_definition": "A dining room in a college or university, or the formal meal served there.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Students are expected to wear formal gowns when attending <w>hall</w>.",
                            "target_lang_translations": {
                                "ru": "Студенты должны быть в официальных мантиях при посещении <w>трапезной</w>."
                            }
                        },
                        {
                            "text": "We met for breakfast in the college <w>hall</w>.",
                            "target_lang_translations": {
                                "ru": "Мы встретились за завтраком в <w>столовой</w> колледжа."
                            }
                        }
                    ],
                    "synonyms": [
                        "refectory",
                        "canteen"
                    ],
                    "common_phrases": [
                        "formal hall"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British English, particularly Oxbridge context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Столовая в колледже или университете, или официальный обед, который там подают."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "столовая",
                                "target_lang_sense_clarification": "помещение для приема пищи"
                            },
                            {
                                "target_lang_word": "трапезная",
                                "target_lang_sense_clarification": "столовая в старинных учебных заведениях или монастырях"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "498b1721-f108-3447-ab80-8e1f4f128e22",
                    "sense_definition": "A common surname of English or Scandinavian origin.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Hall</w> teaches mathematics at the local high school.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Холл</w> преподает математику в местной средней школе."
                            }
                        },
                        {
                            "text": "I have an appointment with Dr. <w>Hall</w> tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "У меня назначена встреча с доктором <w>Холлом</w> завтра утром."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Распространенная фамилия английского или скандинавского происхождения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Холл",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2bdb272d-7908-3b00-bd1f-89aef1957388",
                    "sense_definition": "A name used for various places and communities in countries like the US, UK, and Australia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "They visited the small village of <w>Hall</w> during their trip.",
                            "target_lang_translations": {
                                "ru": "Во время поездки они посетили небольшую деревню <w>Холл</w>."
                            }
                        },
                        {
                            "text": "My cousin lives in a community called <w>Hall</w> in New York.",
                            "target_lang_translations": {
                                "ru": "Мой двоюродный брат живет в поселении под названием <w>Холл</w> в Нью-Йорке."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Название различных мест и населенных пунктов в таких странах, как США, Великобритания и Австралия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Холл",
                                "target_lang_sense_clarification": "географическое название"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fa2a13cc-c6e7-33fb-aa6b-8f596200299c",
                    "sense_definition": "The length of a straight line passing through the center of a circle or sphere.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "geometry_measure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The storm is over five hundred miles in <w>diameter</w>.",
                            "target_lang_translations": {
                                "ru": "Шторм достигает более пятисот миль в <w>диаметре</w>."
                            }
                        },
                        {
                            "text": "You need to measure the <w>diameter</w> of the pipe before buying a cap.",
                            "target_lang_translations": {
                                "ru": "Нужно измерить <w>диаметр</w> трубы перед покупкой заглушки."
                            }
                        }
                    ],
                    "synonyms": [
                        "width",
                        "breadth"
                    ],
                    "common_phrases": [
                        "in diameter",
                        "measure the diameter",
                        "large diameter",
                        "small diameter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длина прямой линии, проходящей через центр круга или сферы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "диаметр",
                                "target_lang_sense_clarification": "длина линии"
                            },
                            {
                                "target_lang_word": "поперечник",
                                "target_lang_sense_clarification": "ширина круглого объекта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d75d543-6eb7-3352-8281-a54bd78d1f8f",
                    "sense_definition": "A straight line segment that passes through the center of a circle or sphere and connects two points on its surface.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "geometry_line",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teacher asked us to draw a <w>diameter</w> on the circle.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил нас начертить <w>диаметр</w> окружности."
                            }
                        },
                        {
                            "text": "A <w>diameter</w> divides a circle into two equal semicircles.",
                            "target_lang_translations": {
                                "ru": "<w>Диаметр</w> делит круг на два равных полукруга."
                            }
                        }
                    ],
                    "common_phrases": [
                        "draw a diameter"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отрезок прямой, проходящий через центр окружности и соединяющий две точки на её поверхности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "диаметр",
                                "target_lang_sense_clarification": "геометрический отрезок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb252509-f7af-3256-adb2-8a8810a330ce",
                    "sense_definition": "The maximum distance between any two points in a specific mathematical space.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "math_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In this metric space, the <w>diameter</w> is finite.",
                            "target_lang_translations": {
                                "ru": "В этом метрическом пространстве <w>диаметр</w> конечен."
                            }
                        },
                        {
                            "text": "We calculated the <w>diameter</w> of the set to determine its bounds.",
                            "target_lang_translations": {
                                "ru": "Мы вычислили <w>диаметр</w> множества, чтобы определить его границы."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Максимальное расстояние между двумя точками множества в метрическом пространстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "диаметр",
                                "target_lang_sense_clarification": "математическая величина"
                            }
                        ]
                    }
                }
            ]
        }
    ]

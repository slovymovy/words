                },
                {
                    "sense_id": "3d319218-50f0-3bde-96b6-d66ab4f1a2cc",
                    "sense_definition": "A place in the United States: A borough, the county seat of Delaware County, Pennsylvania.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geography",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We visited the courthouse in Media to look up some old property records.",
                            "target_lang_translations": {
                                "ru": "Мы посетили здание суда в <w>Медиа</w>, чтобы найти старые записи о собственности."
                            }

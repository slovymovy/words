    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "da6970bb-300d-3636-93c0-9dea941f005b",
                    "sense_definition": "Образовывать пену или покрываться ею (о жидкости).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Шампунь хорошо <пенится> и легко смывается водой."
                        },
                        {
                            "text": "В бокале красиво <пенилось> холодное шампанское."
                        }
                    ],
                    "synonyms": [
                        "вспениваться",
                        "бурлить"
                    ],
                    "common_phrases": [
                        "мыло пенится",
                        "море пенится",
                        "пиво пенится"
                    ]
                },
                {
                    "sense_id": "b3b598c2-59bf-303a-9f33-ce9ff02844e7",
                    "sense_definition": "Виднеться белой, пышной массой, напоминающей пену (о цветах, облаках, тканях).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вдали <пенились> цветущие яблоневые сады."
                        },
                        {
                            "text": "Кружева на её старинном платье <пенились> при ходьбе."
                        }
                    ],
                    "synonyms": [
                        "белеть"
                    ],
                    "common_phrases": [
                        "сады пенятся"
                    ],
                    "traits": [
                        {
                            "trait_type": "literary",
                            "comment": "often used in descriptions of nature or clothing"
                        }
                    ]
                },
                {
                    "sense_id": "26c0ee06-4045-35ff-a99a-d0b60a25732e",
                    "sense_definition": "Покрываться слюной с пеной от сильного утомления, болезни или гнева (о животных или людях).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Загнанная лошадь тяжело дышала и <пенилась>."
                        },
                        {
                            "text": "В приступе ярости он кричал и буквально <пенился>."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "especially when referring to people in anger"
                        }
                    ]
                },
                {
                    "sense_id": "36a344a9-8436-36dc-9597-749267563177",
                    "sense_definition": "Протекать очень активно, бурно и с избытком энергии (о жизни, событиях, чувствах).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вокруг нас <пенилась> весёлая курортная жизнь."
                        },
                        {
                            "text": "Радость в его душе <пенилась> через край."
                        }
                    ],
                    "synonyms": [
                        "бурлить",
                        "кипеть"
                    ],
                    "traits": [
                        {
                            "trait_type": "figurative",
                            "comment": ""
                        }
                    ]
                }
            ]
        }
    ]

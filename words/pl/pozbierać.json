                    "common_phrases": [
                        "pozbierać myśli",
                        "pozbierać siły"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сконцентрировать что-то нематериальное, например мысли или силы, для совершения действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собрать",
                                "target_lang_sense_clarification": "мысли, силы"
                            },
                            {
                                "target_lang_word": "собраться",
                                "target_lang_sense_clarification": "с мыслями"
                            }
                        ]
                    }
                }

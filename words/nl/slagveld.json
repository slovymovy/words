                    }
                },
                {
                    "sense_id": "30060027-5acc-31f3-95ff-aa93799c15bd",
                    "sense_definition": "Een plaats waar een grote chaos, wanorde of verwoesting is aangericht.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "figuurlijk",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na het wilde feestje was de woonkamer veranderd in een waar <w>slagveld</w>.",
                            "target_lang_translations": {
                                "en": "After the wild party, the living room had turned into a true <w>battlefield</w>.",
                                "ru": "После бурной вечеринки гостиная превратилась в настоящее <w>побоище</w>."
                            }
                        },
                        {
                            "text": "De kettingbotsing op de snelweg liet een <w>slagveld</w> van autowrakken achter.",
                            "target_lang_translations": {
                                "en": "The pile-up on the highway left behind a <w>battlefield</w> of car wrecks.",
                                "ru": "Цепная авария на шоссе оставила после себя <w>поле битвы</w> из автомобильных обломков."
                            }
                        }
                    ],
                    "synonyms": [
                        "strijdperk"
                    ],
                    "common_phrases": [
                        "een waar slagveld",
                        "een compleet slagveld"
                    ],
                    "target_lang_definitions": {
                        "en": "A place of great chaos, disorder, or destruction.",
                        "ru": "Место, где царит хаос, беспорядок или разруха."
                    },

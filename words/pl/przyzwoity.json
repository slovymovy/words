    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cd503ae8-efdd-3217-9e53-4a5f2e715db3",
                    "sense_definition": "Taki, który postępuje zgodnie z zasadami moralnymi, jest uczciwy, szlachetny i dobrze wychowany; także: zgodny z dobrymi obyczajami.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "moral_conduct",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To bardzo <w>przyzwoity</w> człowiek, któremu można bez obaw zaufać.",
                            "target_lang_translations": {
                                "ru": "Это очень <w>порядочный</w> человек, которому можно без опасений довериться."
                            }
                        },
                        {
                            "text": "Jej <w>przyzwoite</w> zachowanie podczas uroczystości zostało przez wszystkich docenione.",
                            "target_lang_translations": {
                                "ru": "Её <w>пристойное</w> поведение во время церемонии было оценено всеми."
                            }
                        }
                    ],
                    "synonyms": [
                        "porządny",
                        "uczciwy",
                        "moralny",
                        "cnotliwy"
                    ],
                    "antonyms": [
                        "nieprzyzwoity"
                    ],
                    "common_phrases": [
                        "przyzwoity człowiek",
                        "przyzwoite zachowanie",
                        "przyzwoita rodzina"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто поступает в соответствии с моральными принципами, честный, благородный и воспитанный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "порядочный",
                                "target_lang_sense_clarification": "честный, неспособный на низкие поступки"
                            },
                            {
                                "target_lang_word": "благовоспитанный",
                                "target_lang_sense_clarification": "имеющий хорошее воспитание, умеющий вести себя в обществе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dae07a2c-7269-3155-a288-9271737a62a6",
                    "sense_definition": "Taki, który jest dość dobry, wystarczający lub zadowalający pod względem jakości, wielkości lub poziomu.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "quality_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W nowej firmie zaoferowali mu całkiem <w>przyzwoite</w> zarobki.",
                            "target_lang_translations": {
                                "ru": "В новой фирме ему предложили вполне <w>приличный</w> заработок."
                            }
                        },
                        {
                            "text": "Mimo niskiego budżetu, udało im się nakręcić całkiem <w>przyzwoity</w> film.",
                            "target_lang_translations": {
                                "ru": "Несмотря на низкий бюджет, им удалось снять вполне <w>приличный</w> фильм."
                            }
                        }
                    ],
                    "synonyms": [
                        "niezły",
                        "zadowalający",
                        "godziwy",
                        "poprawny"
                    ],
                    "common_phrases": [
                        "przyzwoite zarobki",
                        "przyzwoity poziom",
                        "całkiem przyzwoity",
                        "przyzwoite warunki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Достаточно хороший, удовлетворительный по качеству, размеру или уровню."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приличный",
                                "target_lang_sense_clarification": "хорошего качества или значительный по размеру"
                            },
                            {
                                "target_lang_word": "неплохой",
                                "target_lang_sense_clarification": "довольно хороший, приемлемый"
                            },
                            {
                                "target_lang_word": "сносный",
                                "target_lang_sense_clarification": "удовлетворительный, терпимый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7fe8a823-1f04-3a6d-9bd3-0801365ae632",
                    "sense_definition": "Właściwy i stosowny w danej sytuacji, zgodny z przyjętymi konwencjami lub normami społecznymi.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "appropriateness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wypadałoby wrócić do domu o <w>przyzwoitej</w> porze, żeby nie budzić domowników.",
                            "target_lang_translations": {
                                "ru": "Следовало бы вернуться домой в <w>приличное</w> время, чтобы не будить домашних."
                            }
                        },
                        {
                            "text": "Na rozmowę kwalifikacyjną należy założyć <w>przyzwoity</w> i schludny strój.",
                            "target_lang_translations": {
                                "ru": "На собеседование следует надеть <w>подобающий</w> и опрятный костюм."
                            }
                        }
                    ],
                    "synonyms": [
                        "stosowny",
                        "odpowiedni",
                        "właściwy"
                    ],
                    "antonyms": [
                        "nieprzyzwoity"
                    ],
                    "common_phrases": [
                        "o przyzwoitej porze",
                        "przyzwoity strój"
                    ],
                    "target_lang_definitions": {
                        "ru": "Уместный и подобающий в данной ситуации, соответствующий принятым нормам приличия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пристойный",
                                "target_lang_sense_clarification": "соответствующий правилам приличия, не вызывающий осуждения"
                            },
                            {
                                "target_lang_word": "подобающий",
                                "target_lang_sense_clarification": "соответствующий обстоятельствам или положению"
                            },
                            {
                                "target_lang_word": "приличный",
                                "target_lang_sense_clarification": "уместный, не вульгарный"
                            }
                        ]
                    }
                }
            ]
        }
    ]

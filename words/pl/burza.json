    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2a70e0db-ede6-3471-bb89-0ad0cc868412",
                    "sense_definition": "Gwałtowne zjawisko atmosferyczne, któremu towarzyszą silny wiatr, opady deszczu oraz wyładowania elektryczne (błyskawice i grzmoty).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nadchodzi wielka <w>burza</w>, więc musimy szybko zamknąć wszystkie okna w domu.",
                            "target_lang_translations": {
                                "ru": "Надвигается сильная <w>гроза</w>, поэтому мы должны быстро закрыть все окна в доме."
                            }
                        },
                        {
                            "text": "Podczas wczorajszej <w>burzy</w> piorun uderzył w stare drzewo rosnące w parku.",
                            "target_lang_translations": {
                                "ru": "Во время вчерашней <w>грозы</w> молния ударила в старое дерево, растущее в парке."
                            }
                        }
                    ],
                    "synonyms": [
                        "nawałnica",
                        "ulewa"
                    ],
                    "antonyms": [
                        "pogoda",
                        "spokój"
                    ],
                    "common_phrases": [
                        "burza z piorunami",
                        "cisza przed burzą",
                        "burza piaskowa",
                        "nadciąga burza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бурное атмосферное явление, сопровождающееся сильным ветром, дождем и электрическими разрядами (молнией и громом)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гроза",
                                "target_lang_sense_clarification": "атмосферное явление с громом и молнией"
                            },
                            {
                                "target_lang_word": "буря",
                                "target_lang_sense_clarification": "сильный ветер, ураган"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a45fd8e3-29ef-3a64-ac2b-c10d7ddfdab6",
                    "sense_definition": "Gwałtowna reakcja ludzi, nagłe wydarzenie o burzliwym przebiegu lub stan silnego wzburzenia emocjonalnego.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "metaphor_event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego kontrowersyjna wypowiedź wywołała prawdziwą <w>burzę</w> w mediach społecznościowych.",
                            "target_lang_translations": {
                                "ru": "Его спорное высказывание вызвало настоящую <w>бурю</w> в социальных сетях."
                            }
                        },
                        {
                            "text": "Po zakończeniu spektaklu na widowni wybuchła <w>burza</w> oklasków.",
                            "target_lang_translations": {
                                "ru": "После окончания спектакля в зрительном зале разразилась <w>буря</w> аплодисментов."
                            }
                        }
                    ],
                    "synonyms": [
                        "awantura",
                        "zamieszanie",
                        "wstrząs"
                    ],
                    "antonyms": [
                        "harmonia",
                        "zgoda"
                    ],
                    "common_phrases": [
                        "burza mózgów",
                        "burza w szklance wody",
                        "burza oklasków",
                        "burza uczuć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бурная реакция людей, внезапное событие или состояние сильного эмоционального возбуждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "буря",
                                "target_lang_sense_clarification": "перен. сильное волнение, споры"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f73a86c6-503c-3b20-bf23-1a0e078611cb",
                    "sense_definition": "Bardzo gęste, bujne i często nieuporządkowane włosy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphor_hair",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jej głowę zdobiła ruda <w>burza</w> kręconych loków, której nie dało się ujarzmić.",
                            "target_lang_translations": {
                                "ru": "Её голову украшала рыжая <w>копна</w> вьющихся локонов, которую невозможно было укротить."
                            }
                        },
                        {
                            "text": "Spod czapki wystawała mu siwa <w>burza</w> włosów.",
                            "target_lang_translations": {
                                "ru": "Из-под шапки у него выбивалась седая <w>копна</w> волос."
                            }
                        }
                    ],
                    "synonyms": [
                        "gąszcz",
                        "szopa"
                    ],
                    "antonyms": [
                        "łysina"
                    ],
                    "common_phrases": [
                        "burza włosów",
                        "burza loków"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень густые, пышные и часто непослушные волосы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "копна",
                                "target_lang_sense_clarification": "о густых волосах"
                            },
                            {
                                "target_lang_word": "грива",
                                "target_lang_sense_clarification": "о длинных, густых волосах"
                            }
                        ]
                    }
                }
            ]
        },
        {

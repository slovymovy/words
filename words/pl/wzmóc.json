    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "efe52c78-c6d9-3c89-bd52-c321b958b2ff",
                    "sense_definition": "Sprawić, że działanie, zjawisko lub uczucie staje się silniejsze, bardziej intensywne lub występuje w większym stopniu.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "intensification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rząd zapowiedział, że zamierza <w>wzmóc</w> kontrolę na granicach w związku z nowymi przepisami.",
                            "target_lang_translations": {
                                "ru": "Правительство объявило, что намерено <w>усилить</w> контроль на границах в связи с новыми правилами."
                            }
                        },
                        {
                            "text": "Musimy <w>wzmóc</w> nasze wysiłki, aby zakończyć ten ważny projekt przed upływem terminu.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>усилить</w> наши усилия, чтобы завершить этот важный проект до истечения срока."
                            }
                        }
                    ],
                    "synonyms": [
                        "nasilić",
                        "zintensyfikować",
                        "zwiększyć",
                        "spotęgować"
                    ],
                    "antonyms": [
                        "osłabić",
                        "zmniejszyć",
                        "uśmierzyć"
                    ],
                    "common_phrases": [
                        "wzmóc czujność",
                        "wzmóc wysiłki",
                        "wzmóc kontrolę",
                        "wzmóc nacisk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать действие, явление или чувство более сильным, интенсивным или проявляющимся в большей степени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усилить",
                                "target_lang_sense_clarification": "сделать более сильным или мощным"
                            },
                            {
                                "target_lang_word": "интенсифицировать",
                                "target_lang_sense_clarification": "сделать более напряженным или действенным (книжн.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be495901-f555-3702-8da6-48bca3d4841d",
                    "sense_definition": "O zjawiskach, uczuciach lub stanach: stać się silniejszym, gwałtowniejszym lub bardziej odczuwalnym.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "intensification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wieczorem wiatr gwałtownie <w>wzmógł się</w>, łamiąc gałęzie starych drzew w parku.",
                            "target_lang_translations": {
                                "ru": "Вечером ветер резко <w>усилился</w>, ломая ветки старых деревьев в парке."
                            }
                        },
                        {
                            "text": "Jego niepokój <w>wzmógł się</w>, gdy zauważył, że drzwi do mieszkania są otwarte.",
                            "target_lang_translations": {
                                "ru": "Его беспокойство <w>усилилось</w>, когда он заметил, что дверь в квартиру открыта."
                            }
                        }
                    ],
                    "synonyms": [
                        "nasilić się",
                        "spotęgować się",
                        "przybrać na sile"
                    ],
                    "antonyms": [
                        "osłabnąć",
                        "zelżeć",
                        "ustąpić",
                        "ucichnąć"
                    ],
                    "common_phrases": [
                        "wiatr się wzmógł",
                        "ból się wzmógł",
                        "hałas się wzmógł",
                        "apel się wzmógł"
                    ],
                    "target_lang_definitions": {
                        "ru": "О явлениях, чувствах или состояниях: стать более сильным, резким или ощутимым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "усилиться",
                                "target_lang_sense_clarification": "стать более сильным (о ветре, боли, чувствах)"
                            },
                            {
                                "target_lang_word": "возрасти",
                                "target_lang_sense_clarification": "увеличиться по силе или степени"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
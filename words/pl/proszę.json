    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "e77736ce-c665-3ee0-bb4d-ec4c156a9b75",
                    "sense_definition": "Zwrot grzecznościowy używany przy wyrażaniu prośby, życzenia lub polecenia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "politeness_request",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Proszę</w> zamknąć okno, bo jest zimno.",
                            "target_lang_translations": {
                                "ru": "<w>Пожалуйста</w>, закройте окно, потому что холодно."
                            }
                        },
                        {
                            "text": "Czy może mi pan <w>proszę</w> podać sól?",
                            "target_lang_translations": {
                                "ru": "Не могли бы вы, <w>пожалуйста</w>, передать мне соль?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "proszę bardzo",
                        "poproszę",
                        "proszę chwilę zaczekać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вежливое слово, используемое при выражении просьбы, желания или приказа."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пожалуйста",
                                "target_lang_sense_clarification": "вежливая просьба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef23a617-6851-3981-8292-b23e4011e92e",
                    "sense_definition": "Słowo wypowiadane w momencie wręczania lub podawania komuś jakiegoś przedmiotu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "giving",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Proszę</w>, to twoja reszta z zakupów.",
                            "target_lang_translations": {
                                "ru": "<w>Пожалуйста</w>, это ваша сдача с покупок."
                            }
                        },
                        {
                            "text": "<w>Proszę</w>, oto dokumenty, o które pytałeś.",
                            "target_lang_translations": {
                                "ru": "<w>Пожалуйста</w>, вот документы, о которых ты спрашивал."
                            }
                        }
                    ],
                    "synonyms": [
                        "masz"
                    ],
                    "common_phrases": [
                        "proszę bardzo",
                        "oto proszę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, произносимое при вручении или передаче кому-либо какого-либо предмета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пожалуйста",
                                "target_lang_sense_clarification": "при передаче предмета"
                            },
                            {
                                "target_lang_word": "вот",
                                "target_lang_sense_clarification": "указание на предмет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e5c78dd-5a61-3d4b-a782-77fc43a07666",
                    "sense_definition": "Grzeczna odpowiedź na czyjeś podziękowanie.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "politeness_response",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziękuję za pomoc przy naprawie samochodu. – <w>Proszę</w> bardzo.",
                            "target_lang_translations": {
                                "ru": "Спасибо за помощь с ремонтом машины. – <w>Пожалуйста</w>."
                            }
                        },
                        {
                            "text": "Nie ma za co, <w>proszę</w> uprzejmie.",
                            "target_lang_translations": {
                                "ru": "Не за что, <w>пожалуйста</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "proszę bardzo",
                        "ależ proszę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вежливый ответ на чью-либо благодарность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пожалуйста",
                                "target_lang_sense_clarification": "ответ на спасибо"
                            },
                            {
                                "target_lang_word": "не за что",
                                "target_lang_sense_clarification": "разговорный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "06566fea-5d36-37e5-a213-8aec26fd8383",
                    "sense_definition": "Zwrot grzecznościowy łączony z tytułem lub nazwą osoby, do której się zwracamy, służący do wzmocnienia uwagi.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "addressing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Proszę</w> pana, czy ten autobus jedzie do centrum?",
                            "target_lang_translations": {
                                "ru": "<w>Скажите, пожалуйста</w>, этот автобус идет в центр?"
                            }
                        },
                        {
                            "text": "Nie zrobiłem zadania domowego, <w>proszę</w> pani.",
                            "target_lang_translations": {
                                "ru": "Я не сделал домашнее задание, <w>извините</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "proszę pana",
                        "proszę pani",
                        "proszę państwa",
                        "proszę wycieczki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вежливое обращение (часто с титулом) для привлечения внимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скажите, пожалуйста",
                                "target_lang_sense_clarification": "обращение с вопросом"
                            },
                            {
                                "target_lang_word": "извините",
                                "target_lang_sense_clarification": "привлечение внимания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afe6e0f3-8be2-3d6c-98cc-cb12d4d1c82d",
                    "sense_definition": "Zachęta do wejścia do środka, zajęcia miejsca lub poczęstowania się czymś.",
                    "learner_level": "A1",
                    "frequency": "Middle",
                    "semantic_group_id": "invitation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pukają do drzwi, więc głośno wołam: <w>proszę</w>!",
                            "target_lang_translations": {
                                "ru": "Стучат в дверь, поэтому я громко кричу: <w>войдите</w>!"
                            }
                        },
                        {
                            "text": "<w>Proszę</w>, niech pan usiądzie wygodnie w fotelu.",
                            "target_lang_translations": {
                                "ru": "<w>Пожалуйста</w>, садитесь поудобнее в кресло."
                            }
                        }
                    ],
                    "common_phrases": [
                        "proszę wejść",
                        "proszę siadać"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приглашение войти, сесть или угоститься чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прошу",
                                "target_lang_sense_clarification": "приглашение"
                            },
                            {
                                "target_lang_word": "пожалуйста",
                                "target_lang_sense_clarification": "вежливое предложение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b776f936-e6f5-3d76-93aa-2119a8f1765c",
                    "sense_definition": "Pytanie zadawane, gdy nie usłyszało się lub nie zrozumiało wypowiedzi rozmówcy, oznaczające prośbę o powtórzenie.",
                    "learner_level": "A1",
                    "frequency": "Middle",
                    "semantic_group_id": "clarification",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Proszę</w>? Możesz powtórzyć, bo nie dosłyszałem?",
                            "target_lang_translations": {
                                "ru": "<w>Простите</w>? Можешь повторить, я не расслышал?"
                            }
                        },
                        {
                            "text": "Jak <w>proszę</w>? Muzyka gra za głośno.",
                            "target_lang_translations": {
                                "ru": "Что, <w>простите</w>? Музыка играет слишком громко."
                            }
                        }
                    ],
                    "synonyms": [
                        "słucham"
                    ],
                    "common_phrases": [
                        "słucham proszę",
                        "jak proszę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вопрос, задаваемый, когда не расслышали или не поняли собеседника; просьба повторить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "простите?",
                                "target_lang_sense_clarification": "вежливый переспрос"
                            },
                            {
                                "target_lang_word": "что?",
                                "target_lang_sense_clarification": "разговорный переспрос"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60cd3d93-3016-3429-8781-6b6a425cb5ad",
                    "sense_definition": "Słowo wypowiadane przy odbieraniu połączenia telefonicznego.",
                    "learner_level": "A1",
                    "frequency": "Middle",
                    "semantic_group_id": "phone",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Proszę</w>, tu mieszkanie Kowalskich.",
                            "target_lang_translations": {
                                "ru": "<w>Алло</w>, это квартира Ковальских."
                            }
                        },
                        {
                            "text": "Halo, <w>proszę</w>, kto mówi?",
                            "target_lang_translations": {
                                "ru": "Алло, <w>слушаю</w>, кто говорит?"
                            }
                        }
                    ],
                    "synonyms": [
                        "halo",
                        "słucham"
                    ],
                    "common_phrases": [
                        "tak proszę",
                        "halo proszę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, произносимое при ответе на телефонный звонок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "алло",
                                "target_lang_sense_clarification": "телефонное приветствие"
                            },
                            {
                                "target_lang_word": "слушаю",
                                "target_lang_sense_clarification": "готовность слушать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b315c270-8f40-3294-a98c-3315d2315e09",
                    "sense_definition": "Wykrzyknienie wyrażające zdziwienie, zaskoczenie, a czasem ironię lub satysfakcję, że coś się stało.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "surprise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "No <w>proszę</w>, jednak udało ci się przyjść na czas.",
                            "target_lang_translations": {
                                "ru": "Ну <w>надо же</w>, тебе все-таки удалось прийти вовремя."
                            }
                        },
                        {
                            "text": "<w>Proszę</w>, <w>proszę</w>! Kogo my tu mamy!",
                            "target_lang_translations": {
                                "ru": "<w>Надо же</w>! Кто у нас тут!"
                            }
                        }
                    ],
                    "synonyms": [
                        "no proszę",
                        "o!"
                    ],
                    "common_phrases": [
                        "proszę proszę",
                        "no proszę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Восклицание, выражающее удивление, неожиданность или иронию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надо же",
                                "target_lang_sense_clarification": "удивление"
                            },
                            {
                                "target_lang_word": "ну и ну",
                                "target_lang_sense_clarification": "ирония/сюрприз"
                            }
                        ]
                    }
                }
            ]
        },

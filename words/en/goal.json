                    "common_phrases": [
                        "score a goal",
                        "winning goal",
                        "own goal"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очко, полученное в игре в результате попадания мяча или предмета в сетку или зону ворот."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гол",

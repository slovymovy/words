                {
                    "sense_id": "c74bd459-d83f-34a0-b556-f585ac539881",
                    "sense_definition": "Słowo używane, aby wyróżnić lub podkreślić jeden element spośród innych; zwłaszcza.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emphasis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lubię owoce, a <szczególnie> jabłka.",
                            "target_lang_translations": {
                                "ru": "Я люблю фрукты, а <особенно> яблоки."
                            }
                        },
                        {
                            "text": "Musisz uważać na drodze, <szczególnie> w nocy.",
                            "target_lang_translations": {
                                "ru": "Нужно быть осторожным на дороге, <особенно> ночью."
                            }
                        }
                    ],
                    "synonyms": [

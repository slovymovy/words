            "pos": "verb",
            "senses": [
                {
                    "sense_id": "463ec7c2-122c-3ebe-9635-da31f9e604a4",
                    "sense_definition": "Относиться к определённому сроку или событию, совпадать с ними по времени; быть назначенным на конкретную дату.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Торжественное открытие памятника <w>приурочивается</w> к празднованию Дня города.",
                            "target_lang_translations": {
                                "en": "The ceremonial opening of the monument <w>is timed</w> to coincide with the City Day celebration."
                            }
                        },
                        {
                            "text": "Это мероприятие традиционно <w>приурочивается</w> к началу учебного года.",
                            "target_lang_translations": {
                                "en": "This event <w>is</w> traditionally <w>timed</w> to the start of the academic year."
                            }
                        }
                    ],
                    "synonyms": [
                        "приноравливаться"
                    ],
                    "common_phrases": [
                        "приурочиваться к дате",
                        "приурочиваться к празднику",
                        "приурочиваться к юбилею",
                        "приурочиваться к событию"
                    ],
                    "target_lang_definitions": {
                        "en": "To be timed to coincide with a specific date or event; to happen at the same time as something else by design."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be timed (to)",
                                "target_lang_sense_clarification": "to be arranged to happen at a specific time"
                            },
                            {
                                "target_lang_word": "coincide (with)",
                                "target_lang_sense_clarification": "to happen at the same time"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6149b82-cf8d-3e46-81e4-3fdc71cf83b9",
                    "sense_definition": "Страдательный залог к глаголу <w>приурочивать</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Выпуск новой серии марок <w>приурочивается</w> к предстоящему чемпионату мира.",

            "senses": [
                {
                    "sense_id": "3dff6d0e-8fb4-371d-91d2-535fec40998f",
                    "sense_definition": "Onderdeel van het spijsverteringsstelsel tussen de maag en de anus.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dokter luisterde met een stethoscoop naar mijn <w>darmen</w>.",
                            "target_lang_translations": {
                                "ru": "Врач послушал мой <w>кишечник</w> стетоскопом."
                            }
                        },
                        {
                            "text": "De dunne <w>darm</w> speelt een belangrijke rol bij de opname van voedingsstoffen.",
                            "target_lang_translations": {
                                "ru": "<w>Тонкая кишка</w> играет важную роль в усвоении питательных веществ."
                            }
                        }
                    ],
                    "synonyms": [
                        "spijsverteringskanaal"
                    ],
                    "common_phrases": [
                        "dunne darm",
                        "dikke darm",
                        "blinde darm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть пищеварительной системы между желудком и анусом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кишка",
                                "target_lang_sense_clarification": "часть органа"
                            },
                            {
                                "target_lang_word": "кишечник",
                                "target_lang_sense_clarification": "вся система"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d864382-d491-3df2-acfc-a31143baf916",
                    "sense_definition": "Flexibele buis of slang waar vloeistof of gas doorheen kan stromen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij sloot de <w>darm</w> aan op de kraan om de tuin te sproeien.",
                            "target_lang_translations": {
                                "ru": "Он подсоединил <w>шланг</w> к крану, чтобы полить сад."
                            }
                        },
                        {
                            "text": "Er zit een knik in de <w>darm</w>, waardoor het water niet goed doorstroomt.",
                            "target_lang_translations": {
                                "ru": "На <w>шланге</w> образовался перегиб, из-за чего вода плохо проходит."
                            }
                        }
                    ],
                    "synonyms": [
                        "spijsverteringskanaal"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "common in Belgium"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Гибкая трубка или шланг, по которому может течь жидкость или газ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "шланг",
                                "target_lang_sense_clarification": "гибкая трубка"
                            }
                        ]
                    }
                }
            ]
        }
    ]

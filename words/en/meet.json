{
    "word_family": [
        "meeting"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "836979a9-ae31-33a1-9fd8-19940755b937",
                    "sense_definition": "To be introduced to someone and get to know them for the first time.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "social_contact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is a pleasure to <w>meet</w> you.",
                            "target_lang_translations": {
                                "ru": "Приятно с вами <w>познакомиться</w>."
                            }
                        },
                        {
                            "text": "I would like you to <w>meet</w> my brother.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <w>познакомить</w> вас с моим братом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nice to meet you",
                        "pleased to meet you"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть представленным кому-то и узнать его впервые."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "знакомиться",
                                "target_lang_sense_clarification": "узнавать кого-то впервые"
                            },
                            {
                                "target_lang_word": "познакомиться",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45a2ee8a-32c8-39f9-9128-9fa8a0d75860",
                    "sense_definition": "To come together with someone at a specific time and place by arrangement.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "social_contact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Let's <w>meet</w> at the station at nine o'clock.",
                            "target_lang_translations": {
                                "ru": "Давай <w>встретимся</w> на вокзале в девять часов."
                            }
                        },
                        {
                            "text": "We agreed to <w>meet</w> for lunch tomorrow.",
                            "target_lang_translations": {
                                "ru": "Мы договорились <w>встретиться</w> завтра за обедом."
                            }
                        }
                    ],
                    "synonyms": [
                        "join"
                    ],
                    "common_phrases": [
                        "meet up",
                        "meet for lunch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Встретиться с кем-то в определенное время и в определенном месте по договоренности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встречаться",
                                "target_lang_sense_clarification": "видеться по плану"
                            },
                            {
                                "target_lang_word": "встретиться",
                                "target_lang_sense_clarification": "совершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af2534a5-9001-3964-9446-c6d683b0f98d",
                    "sense_definition": "To see and speak to someone by chance; to encounter.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "social_contact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Guess who I <w>met</w> at the supermarket today?",
                            "target_lang_translations": {
                                "ru": "Угадай, кого я сегодня <w>встретил</w> в супермаркете?"
                            }
                        },
                        {
                            "text": "I <w>met</w> an old friend in the park while walking the dog.",
                            "target_lang_translations": {
                                "ru": "Я <w>встретил</w> старого друга в парке, когда выгуливал собаку."
                            }
                        }
                    ],
                    "synonyms": [
                        "encounter",
                        "bump into",
                        "run into"
                    ],
                    "antonyms": [
                        "pass",
                        "pass"
                    ],
                    "common_phrases": [
                        "meet by chance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Случайно увидеть кого-то и поговорить; столкнуться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встретить",
                                "target_lang_sense_clarification": "случайно увидеть"
                            },
                            {
                                "target_lang_word": "столкнуться",
                                "target_lang_sense_clarification": "неожиданно пересечься"
                            },
                            {
                                "target_lang_word": "встречать",
                                "target_lang_sense_clarification": "несовершенный вид"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2c0a676-cc4d-3afb-b1f0-07e498bf09a3",
                    "sense_definition": "To satisfy a requirement, need, or demand; to fulfill.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fulfillment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company is working hard to <w>meet</w> the deadline.",
                            "target_lang_translations": {
                                "ru": "Компания усердно работает, чтобы <w>уложиться</w> в срок."
                            }
                        },
                        {
                            "text": "This proposal does not <w>meet</w> our requirements.",
                            "target_lang_translations": {
                                "ru": "Это предложение не <w>отвечает</w> нашим требованиям."
                            }
                        }
                    ],
                    "synonyms": [
                        "satisfy",
                        "fulfill"
                    ],
                    "antonyms": [
                        "pass"
                    ],
                    "common_phrases": [
                        "meet requirements",
                        "meet needs",
                        "meet a deadline",
                        "meet the cost"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удовлетворять требованию, потребности или запросу; выполнять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соответствовать",
                                "target_lang_sense_clarification": "требованиям или стандартам"
                            },
                            {
                                "target_lang_word": "удовлетворять",
                                "target_lang_sense_clarification": "потребности или запросы"
                            },
                            {
                                "target_lang_word": "отвечать",
                                "target_lang_sense_clarification": "быть подходящим"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0446cdde-be8a-35af-ac1c-acfcdeabe9a1",
                    "sense_definition": "To gather together as a group for a formal discussion or purpose.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "social_contact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The committee will <w>meet</w> next week to decide on the budget.",
                            "target_lang_translations": {
                                "ru": "Комитет <w>соберется</w> на следующей неделе, чтобы принять решение по бюджету."
                            }
                        },
                        {
                            "text": "Leaders from both countries <w>met</w> to discuss the peace treaty.",
                            "target_lang_translations": {
                                "ru": "Лидеры обеих стран <w>встретились</w>, чтобы обсудить мирный договор."
                            }
                        }
                    ],
                    "synonyms": [
                        "gather",
                        "assemble",
                        "convene"
                    ],
                    "common_phrases": [
                        "meet with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собраться группой для официального обсуждения или цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собираться",
                                "target_lang_sense_clarification": "сходиться вместе"
                            },
                            {
                                "target_lang_word": "встречаться",
                                "target_lang_sense_clarification": "проводить собрание"
                            },
                            {
                                "target_lang_word": "заседать",
                                "target_lang_sense_clarification": "о комитетах и органах власти"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ce0cb54-a566-3d27-9827-f6d7435047ce",
                    "sense_definition": "To come together and join or touch at a specific point (used for roads, rivers, lines, etc.).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_convergence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two rivers <w>meet</w> just south of the city.",
                            "target_lang_translations": {
                                "ru": "Две реки <w>сливаются</w> к югу от города."
                            }
                        },
                        {
                            "text": "We stood where the forest <w>meets</w> the sea.",
                            "target_lang_translations": {
                                "ru": "Мы стояли там, где лес <w>встречается</w> с морем."
                            }
                        }
                    ],
                    "synonyms": [
                        "join",
                        "converge",
                        "intersect",
                        "touch"
                    ],
                    "antonyms": [
                        "pass"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединяться или соприкасаться в определенной точке (о дорогах, реках, линиях и т. д.)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соединяться",
                                "target_lang_sense_clarification": "сливаться воедино"
                            },
                            {
                                "target_lang_word": "сходиться",
                                "target_lang_sense_clarification": "приближаться друг к другу"
                            },
                            {
                                "target_lang_word": "пересекаться",
                                "target_lang_sense_clarification": "проходить через одну точку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2048c768-462e-3285-a13c-37c699d3715c",
                    "sense_definition": "To play against an opponent in a sports competition.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "competition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Brazil will <w>meet</w> France in the final match.",
                            "target_lang_translations": {
                                "ru": "Бразилия <w>встретится</w> с Францией в финальном матче."
                            }
                        },
                        {
                            "text": "The two boxers will <w>meet</w> in the ring tonight.",
                            "target_lang_translations": {
                                "ru": "Два боксера <w>сойдутся</w> на ринге сегодня вечером."
                            }
                        }
                    ],
                    "synonyms": [
                        "face",
                        "play"
                    ],
                    "target_lang_definitions": {
                        "ru": "Играть против соперника в спортивном соревновании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встречаться",
                                "target_lang_sense_clarification": "играть против кого-то"
                            },
                            {
                                "target_lang_word": "сразиться",
                                "target_lang_sense_clarification": "вступить в бой или поединок"
                            },
                            {
                                "target_lang_word": "сойтись",
                                "target_lang_sense_clarification": "в поединке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0008e66b-9df9-347c-bba9-b81c38ebb586",
                    "sense_definition": "To experience or suffer something, often something unpleasant like fate or death.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "experience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The brave soldier <w>met</w> his death on the battlefield.",
                            "target_lang_translations": {
                                "ru": "Храбрый солдат <w>встретил</w> свою смерть на поле боя."
                            }
                        },
                        {
                            "text": "He <w>met</w> his fate with courage and dignity.",
                            "target_lang_translations": {
                                "ru": "Он <w>встретил</w> свою судьбу с мужеством и достоинством."
                            }
                        }
                    ],
                    "synonyms": [
                        "experience",
                        "face",
                        "undergo"
                    ],
                    "common_phrases": [
                        "meet one's fate",
                        "meet one's death",
                        "meet with opposition"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often literary or dramatic"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Испытать или пережить что-то, часто неприятное, например судьбу или смерть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встретить",
                                "target_lang_sense_clarification": "столкнуться с чем-то неизбежным"
                            },
                            {
                                "target_lang_word": "найти",
                                "target_lang_sense_clarification": "обрести (смерть, конец)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c6c65efe-ee0e-36d4-9172-eba29dfec3dd",
                    "sense_definition": "A sports competition, especially for track and field or swimming.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She won three races at the track <w>meet</w>.",
                            "target_lang_translations": {
                                "ru": "Она выиграла три забега на <w>соревнованиях</w> по легкой атлетике."
                            }
                        },
                        {
                            "text": "Our school is hosting a swim <w>meet</w> on Saturday.",
                            "target_lang_translations": {
                                "ru": "В субботу наша школа проводит <w>соревнования</w> по плаванию."
                            }
                        }
                    ],
                    "synonyms": [
                        "competition",
                        "contest"
                    ],
                    "common_phrases": [
                        "track meet",
                        "swim meet"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "chiefly US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Спортивное соревнование, особенно по легкой атлетике или плаванию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соревнования",
                                "target_lang_sense_clarification": "спортивное состязание"
                            },
                            {
                                "target_lang_word": "встреча",
                                "target_lang_sense_clarification": "матч или поединок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4e1f396c-61fb-304f-8216-69524a649f3e",
                    "sense_definition": "A meeting, often arranged for a specific purpose.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "event",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Let's set up a <w>meet</w> to discuss the details.",
                            "target_lang_translations": {
                                "ru": "Давай устроим <w>встречу</w>, чтобы обсудить детали."
                            }
                        },
                        {
                            "text": "The spy arranged a secret <w>meet</w> in the park.",
                            "target_lang_translations": {
                                "ru": "Шпион организовал тайную <w>встречу</w> в парке."
                            }
                        }
                    ],
                    "synonyms": [
                        "meeting",
                        "gathering"
                    ],
                    "common_phrases": [
                        "arrange a meet",
                        "swap meet"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Встреча, часто организованная с определенной целью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встреча",
                                "target_lang_sense_clarification": "свидание или собрание"
                            },
                            {
                                "target_lang_word": "сбор",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "08522f02-c0e8-38f1-9772-6bba492659cc",
                    "sense_definition": "Suitable, right, or proper for the occasion.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "appropriateness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is <w>meet</w> and right so to do.",
                            "target_lang_translations": {
                                "ru": "Это <w>достойно</w> и праведно."
                            }
                        },
                        {
                            "text": "He did what he thought was <w>meet</w> for the situation.",
                            "target_lang_translations": {
                                "ru": "Он сделал то, что счел <w>уместным</w> в данной ситуации."
                            }
                        }
                    ],
                    "synonyms": [
                        "suitable",
                        "proper",
                        "fitting"
                    ],
                    "antonyms": [
                        "pass",
                        "pass"
                    ],
                    "common_phrases": [
                        "meet and right",
                        "help meet"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        },
                        {
                            "trait_type": "form",
                            "comment": "often used in religious or literary contexts"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Подходящий, правильный или уместный для данного случая."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подобающий",
                                "target_lang_sense_clarification": "устаревшее или высокое"
                            },
                            {
                                "target_lang_word": "уместный",
                                "target_lang_sense_clarification": "соответствующий обстановке"
                            },
                            {
                                "target_lang_word": "достойный",
                                "target_lang_sense_clarification": "в религиозном контексте"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
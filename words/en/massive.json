{
    "word_family": [
        "mass",
        "massively",
        "massiveness"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "77f8a92d-c876-36e9-b4fa-dc2bc6c4a05b",
                    "sense_definition": "Very large in size, quantity, or extent; heavy and solid.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "size_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The castle was surrounded by <w>massive</w> stone walls that were impossible to breach.",
                            "target_lang_translations": {
                                "ru": "Замок был окружен <w>массивными</w> каменными стенами, которые невозможно было пробить."
                            }
                        },
                        {
                            "text": "The company has seen a <w>massive</w> increase in sales over the last quarter.",
                            "target_lang_translations": {
                                "ru": "В прошлом квартале компания наблюдала <w>огромный</w> рост продаж."
                            }
                        }
                    ],
                    "synonyms": [
                        "huge",
                        "enormous",
                        "gigantic",
                        "colossal",
                        "substantial",
                        "bulky",
                        "heavy"
                    ],
                    "antonyms": [
                        "tiny",
                        "little",
                        "insubstantial",
                        "minuscule",
                        "microscopic"
                    ],
                    "common_phrases": [
                        "massive amount",
                        "massive scale",
                        "massive structure",
                        "massive increase"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень большой по размеру, количеству или степени; тяжелый и цельный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "массивный",
                                "target_lang_sense_clarification": "о физических объектах, тяжелый и крупный"
                            },
                            {
                                "target_lang_word": "огромный",
                                "target_lang_sense_clarification": "о размере или количестве"
                            },
                            {
                                "target_lang_word": "колоссальный",
                                "target_lang_sense_clarification": "чрезвычайно большой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3284a75-0c31-3bbb-8991-48c8a95a33bd",
                    "sense_definition": "Very severe, serious, or affecting a large portion of the body.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "medical_severity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He suffered a <w>massive</w> heart attack and was rushed to the emergency room immediately.",
                            "target_lang_translations": {
                                "ru": "У него случился <w>обширный</w> инфаркт, и его немедленно доставили в реанимацию."
                            }
                        },
                        {
                            "text": "The patient experienced <w>massive</w> bleeding before the doctors could stabilize the wound.",
                            "target_lang_translations": {
                                "ru": "У пациента началось <w>массивное</w> кровотечение, прежде чем врачи смогли стабилизировать рану."
                            }
                        }
                    ],
                    "synonyms": [
                        "overwhelming",
                        "significant"
                    ],
                    "common_phrases": [
                        "massive heart attack",
                        "massive stroke",
                        "massive trauma",
                        "massive hemorrhage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень тяжелый, серьезный или затрагивающий большую часть тела (о болезни или травме)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обширный",
                                "target_lang_sense_clarification": "об инфаркте, инсульте или поражении тканей"
                            },
                            {
                                "target_lang_word": "массивный",
                                "target_lang_sense_clarification": "о кровотечении или травме"
                            },
                            {
                                "target_lang_word": "тяжелый",
                                "target_lang_sense_clarification": "о состоянии здоровья"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9261fa9-fa91-3066-9db1-54049a1fbf32",
                    "sense_definition": "Used to emphasize that something is very great or extreme, often in a negative context.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I felt like a <w>massive</w> idiot when I realized I had forgotten her name.",
                            "target_lang_translations": {
                                "ru": "Я чувствовал себя <w>полным</w> идиотом, когда понял, что забыл её имя."
                            }
                        },
                        {
                            "text": "Buying that old car turned out to be a <w>massive</w> mistake that cost us thousands.",
                            "target_lang_translations": {
                                "ru": "Покупка этой старой машины оказалась <w>огромной</w> ошибкой, которая стоила нам тысячи."
                            }
                        }
                    ],
                    "synonyms": [
                        "consequential",
                        "overwhelming",
                        "significant",
                        "meaningful"
                    ],
                    "antonyms": [
                        "insignificant",
                        "trivial",
                        "unimportant"
                    ],
                    "common_phrases": [
                        "massive idiot",
                        "massive mistake",
                        "massive failure",
                        "massive fan"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used for emphasis"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для усиления значения, часто в негативном контексте (полный, страшный)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "полный",
                                "target_lang_sense_clarification": "об идиоте, дураке"
                            },
                            {
                                "target_lang_word": "огромный",
                                "target_lang_sense_clarification": "об ошибке, проблеме"
                            },
                            {
                                "target_lang_word": "страшный",
                                "target_lang_sense_clarification": "разг., для усиления (напр., страшный зануда)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1ebb87df-affc-35a3-a445-7ee5fe3d7a95",
                    "sense_definition": "Extremely good, exciting, or popular.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "quality_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The concert last night was absolutely <w>massive</w>, with thousands of fans cheering.",
                            "target_lang_translations": {
                                "ru": "Вчерашний концерт был абсолютно <w>грандиозным</w>, тысячи фанатов ликовали."
                            }
                        },
                        {
                            "text": "We are planning a <w>massive</w> party to celebrate the end of the exams.",
                            "target_lang_translations": {
                                "ru": "Мы планируем <w>масштабную</w> вечеринку, чтобы отпраздновать окончание экзаменов."
                            }
                        }
                    ],
                    "synonyms": [
                        "awesome",
                        "excellent",
                        "super",
                        "stupendous"
                    ],
                    "antonyms": [
                        "lame",
                        "disappointing",
                        "stale",
                        "crappy"
                    ],
                    "common_phrases": [
                        "massive party",
                        "massive night out",
                        "massive tune"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "common in youth culture"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Чрезвычайно хороший, захватывающий или популярный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "грандиозный",
                                "target_lang_sense_clarification": "о событии, вечеринке"
                            },
                            {
                                "target_lang_word": "крутой",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "масштабный",
                                "target_lang_sense_clarification": "о крупном мероприятии"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f30d9971-cfff-3cfa-902b-a2663691bf47",
                    "sense_definition": "Possessing physical mass; not massless.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In particle physics, some bosons are <w>massive</w> while others have no mass at all.",
                            "target_lang_translations": {
                                "ru": "В физике элементарных частиц некоторые бозоны <w>массивные</w>, в то время как другие вообще не имеют массы."
                            }
                        },
                        {
                            "text": "A <w>massive</w> object causes a curvature in spacetime according to the theory of relativity.",
                            "target_lang_translations": {
                                "ru": "<w>Массивный</w> объект вызывает искривление пространства-времени согласно теории относительности."
                            }
                        }
                    ],
                    "antonyms": [
                        "massless"
                    ],
                    "common_phrases": [
                        "massive particle",
                        "massive object",
                        "massive star"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий физической массой; не безмассовый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "массивный",
                                "target_lang_sense_clarification": "физический термин"
                            },
                            {
                                "target_lang_word": "обладающий массой",
                                "target_lang_sense_clarification": "описательный перевод"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "602f88b7-2243-341e-95aa-65946ec566cf",
                    "sense_definition": "A group of people from a particular place or sharing a common interest.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "people_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The DJ gave a shout-out to the London <w>massive</w> listening to the show.",
                            "target_lang_translations": {
                                "ru": "Диджей передал привет лондонской <w>тусовке</w>, слушающей шоу."
                            }
                        },
                        {
                            "text": "We went to the club with the whole <w>massive</w> to celebrate his birthday.",
                            "target_lang_translations": {
                                "ru": "Мы пошли в клуб всей <w>толпой</w>, чтобы отпраздновать его день рождения."
                            }
                        }
                    ],
                    "synonyms": [
                        "clique",
                        "gang",
                        "set"
                    ],
                    "common_phrases": [
                        "London massive",
                        "junglist massive"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "originally Jamaican, common in UK urban dialects"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей из определенного места или объединенных общим интересом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тусовка",
                                "target_lang_sense_clarification": "сленг, группа по интересам"
                            },
                            {
                                "target_lang_word": "братва",
                                "target_lang_sense_clarification": "сленг, приятели"
                            },
                            {
                                "target_lang_word": "компания",
                                "target_lang_sense_clarification": "группа друзей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f75bce0c-3517-37f6-bc03-50d4860ca78e",
                    "sense_definition": "A large body of rock that is uniform in structure and not layered.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "geology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Geologists studied the granite <w>massive</w> to understand the formation of the mountain range.",
                            "target_lang_translations": {
                                "ru": "Геологи изучали гранитный <w>массив</w>, чтобы понять формирование горного хребта."
                            }
                        },
                        {
                            "text": "The landscape is dominated by a limestone <w>massive</w> that rises sharply from the valley.",
                            "target_lang_translations": {
                                "ru": "В ландшафте доминирует известняковый <w>массив</w>, резко возвышающийся над долиной."
                            }
                        }
                    ],
                    "common_phrases": [
                        "granite massive",
                        "rock massive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупное геологическое образование, однородное по структуре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "массив",
                                "target_lang_sense_clarification": "геологический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

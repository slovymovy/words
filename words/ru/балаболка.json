    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "cd94d196-4246-3eba-89f6-09237106144b",
                    "sense_definition": "Человек, который много и попусту говорит; любитель поболтать.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "person_chatter",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта <балаболка> может часами рассказывать о своих покупках по телефону.",
                            "target_lang_translations": {
                                "en": "This <chatterbox> can talk about her purchases on the phone for hours."
                            }
                        },
                        {
                            "text": "Ну ты у меня и <балаболка>, ни на минуту не замолкаешь!",
                            "target_lang_translations": {
                                "en": "You really are a <chatterbox>, you don't stop talking for a minute!"
                            }
                        }
                    ],
                    "synonyms": [
                        "болтун",
                        "болтушка"
                    ],
                    "antonyms": [
                        "молчун"
                    ],
                    "common_phrases": [
                        "страшная балаболка",
                        "просто балаболка",
                        "такая балаболка"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person who talks a lot and to no purpose; a chatterbox."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "chatterbox",
                                "target_lang_sense_clarification": "a person who talks at length about trivial matters"
                            },
                            {
                                "target_lang_word": "blabbermouth",
                                "target_lang_sense_clarification": "a person who talks excessively or indiscreetly"
                            },
                            {
                                "target_lang_word": "windbag",
                                "target_lang_sense_clarification": "a person who talks at length but says little of value"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b9c5ef5-6626-3bfc-af22-fbb13cc2d09a",
                    "sense_definition": "Небольшое висячее украшение в виде подвески, шарика или кисточки.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "ornament",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На старинной шали по краям висели разноцветные <балаболки>.",
                            "target_lang_translations": {
                                "en": "Multicolored <tassels> hung along the edges of the antique shawl."
                            }
                        },
                        {
                            "text": "Смешная <балаболка> на его шапке прыгала при каждом шаге.",
                            "target_lang_translations": {
                                "en": "The funny <pom-pom> on his hat bounced with every step."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "often replaced by помпон or кисточка in modern usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A small hanging ornament, such as a pendant, ball, or tassel."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bauble",
                                "target_lang_sense_clarification": "a small, showy trinket or decoration"
                            },
                            {
                                "target_lang_word": "pom-pom",
                                "target_lang_sense_clarification": "a decorative ball of fabric or wool, often on hats"
                            },
                            {
                                "target_lang_word": "tassel",
                                "target_lang_sense_clarification": "a tuft of loosely hanging threads used as decoration"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d628d41b-a806-307b-a12a-9ccf5623c913",
                    "sense_definition": "Плод картофеля в виде маленькой зелёной ягоды.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "botany_regional",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Балаболки> у картошки не ешь — они ядовитые.",
                            "target_lang_translations": {
                                "en": "Don't eat the potato <berries> — they are poisonous."
                            }
                        },
                        {
                            "text": "Осенью на кустах появились зелёные <балаболки>, похожие на помидоры.",
                            "target_lang_translations": {
                                "en": "In autumn, green <berries> appeared on the bushes, looking like tomatoes."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "dialectal usage"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The fruit of a potato plant in the form of a small green berry."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "potato berry",
                                "target_lang_sense_clarification": "the toxic fruit produced by a potato plant"
                            },
                            {
                                "target_lang_word": "seed ball",
                                "target_lang_sense_clarification": "agricultural term for the fruit containing potato seeds"
                            }
                        ]
                    }
                }
            ]
        }
    ]

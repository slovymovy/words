                    ],
                    "common_phrases": [
                        "фотографировать действительность",
                        "фотографировать жизнь"
                    ],
                    "target_lang_definitions": {
                        "en": "To reproduce or describe something with great precision, copying reality without creative alteration."
                    },
                    "translations": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d1569239-0ba1-3b2f-9a00-7bb778064a12",
                    "sense_definition": "A type of knot with two loops, used to tie together two cords such as shoelaces or apron strings, and frequently used as decoration.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "shape_knot",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She tied a pink <w>bow</w> in her hair to match her dress.",
                            "target_lang_translations": {
                                "ru": "Она завязала розовый <w>бант</w> в волосах под цвет платья."
                            }
                        },
                        {
                            "text": "He struggled to tie the <w>bow</w> on his shoelaces.",
                            "target_lang_translations": {
                                "ru": "Он с трудом завязывал <w>бантик</w> на шнурках."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tie a bow",
                        "ribbon bow"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тип узла с двумя петлями, используемый для связывания шнурков или лент, часто применяемый как украшение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бант",
                                "target_lang_sense_clarification": "украшение из ленты"
                            },
                            {
                                "target_lang_word": "бантик",
                                "target_lang_sense_clarification": "маленький узел (напр. на шнурках)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7064418-89df-3f08-a89d-6a38f1d5f208",
                    "sense_definition": "A weapon made of a curved piece of wood or other flexible material whose ends are connected by a string, used for shooting arrows.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "weapon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The archer lifted his <w>bow</w> and aimed carefully at the target.",
                            "target_lang_translations": {
                                "ru": "Лучник поднял свой <w>лук</w> и тщательно прицелился в мишень."
                            }
                        },
                        {
                            "text": "Hunting with a <w>bow</w> and arrow requires great skill and patience.",
                            "target_lang_translations": {
                                "ru": "Охота с <w>луком</w> и стрелами требует большого мастерства и терпения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bow and arrow",
                        "draw a bow"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оружие, изготовленное из гибкого изогнутого материала с натянутой тетивой, используемое для стрельбы стрелами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лук",
                                "target_lang_sense_clarification": "оружие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "375cb05d-68f2-3e3a-947d-bc823051c46a",
                    "sense_definition": "A gesture, usually showing respect, made by inclining the head or bending forward at the waist.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "gesture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The actors returned to the stage to take a final <w>bow</w>.",
                            "target_lang_translations": {
                                "ru": "Актеры вернулись на сцену, чтобы выйти на финальный <w>поклон</w>."
                            }
                        },
                        {
                            "text": "He made a polite <w>bow</w> to the guests as he entered the room.",
                            "target_lang_translations": {
                                "ru": "Войдя в комнату, он сделал вежливый <w>поклон</w> гостям."
                            }
                        }
                    ],
                    "common_phrases": [
                        "take a bow",
                        "deep bow"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жест, обычно выражающий уважение, совершаемый наклоном головы или корпуса вперед."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поклон",
                                "target_lang_sense_clarification": "жест уважения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83185520-ff6a-36a5-ab60-0d80bbd8feb9",
                    "sense_definition": "A rod with horsehair stretched between the ends, used for playing various stringed musical instruments.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The violinist moved the <w>bow</w> smoothly across the strings.",
                            "target_lang_translations": {
                                "ru": "Скрипач плавно вел <w>смычком</w> по струнам."
                            }
                        },
                        {
                            "text": "She needs to buy a new <w>bow</w> for her cello.",
                            "target_lang_translations": {
                                "ru": "Ей нужно купить новый <w>смычок</w> для виолончели."
                            }
                        }
                    ],
                    "synonyms": [
                        "fiddlestick"
                    ],
                    "common_phrases": [
                        "violin bow"
                    ],
                    "target_lang_definitions": {
                        "ru": "Трость с натянутым конским волосом, используемая для игры на струнных музыкальных инструментах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "смычок",
                                "target_lang_sense_clarification": "музыкальный инструмент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0a5cc88-7acf-348b-b6cd-6a96fbeb97de",
                    "sense_definition": "The front of a boat or ship.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "ship",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Huge waves crashed over the <w>bow</w> of the ship during the storm.",
                            "target_lang_translations": {
                                "ru": "Во время шторма огромные волны перекатывались через <w>нос</w> корабля."
                            }
                        },
                        {
                            "text": "The captain stood at the <w>bow</w> scanning the horizon for land.",
                            "target_lang_translations": {
                                "ru": "Капитан стоял на <w>носу</w>, вглядываясь в горизонт в поисках земли."
                            }
                        }
                    ],
                    "synonyms": [
                        "prow",
                        "stem"
                    ],
                    "antonyms": [
                        "stern"
                    ],
                    "common_phrases": [
                        "across the bow",
                        "bow to stern"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передняя часть лодки или корабля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нос",
                                "target_lang_sense_clarification": "судна"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f307c09c-691a-36ed-a109-4f2ed0821fd4",
                    "sense_definition": "Anything bent or curved, such as a rainbow or a bend in a river.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "shape_curve",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The river forms a gentle <w>bow</w> around the base of the hill.",
                            "target_lang_translations": {
                                "ru": "Река образует плавную <w>излучину</w> у подножия холма."
                            }
                        },
                        {
                            "text": "The architect designed the window with a slight <w>bow</w> to let in more light.",
                            "target_lang_translations": {
                                "ru": "Архитектор спроектировал окно с небольшим <w>изгибом</w>, чтобы впустить больше света."
                            }
                        }
                    ],
                    "synonyms": [
                        "arc",
                        "bend",
                        "curve"
                    ],
                    "target_lang_definitions": {
                        "ru": "Что-либо изогнутое или имеющее форму дуги, например, изгиб реки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "изгиб",
                                "target_lang_sense_clarification": "форма"
                            },
                            {
                                "target_lang_word": "излучина",
                                "target_lang_sense_clarification": "реки"
                            },
                            {
                                "target_lang_word": "дуга",
                                "target_lang_sense_clarification": "геометрическая форма"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b4158d77-6ec4-3c68-a933-8796e6628125",
                    "sense_definition": "To bend oneself as a gesture of respect or deference.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "gesture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The performers <w>bowed</w> low to thank the audience for their applause.",
                            "target_lang_translations": {
                                "ru": "Исполнители низко <w>поклонились</w>, благодаря публику за аплодисменты."
                            }
                        },
                        {
                            "text": "He <w>bowed</w> politely when he was introduced to the queen.",
                            "target_lang_translations": {
                                "ru": "Он вежливо <w>поклонился</w>, когда его представили королеве."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bow down",
                        "bow out"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наклоняться в знак уважения или почтения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кланяться",
                                "target_lang_sense_clarification": "совершать поклон"
                            },
                            {
                                "target_lang_word": "поклониться",
                                "target_lang_sense_clarification": "совершить поклон (сов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "37876f3c-e058-33ac-b948-3c422dc2f6c3",
                    "sense_definition": "To defer to something; to submit or yield.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gesture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government refused to <w>bow</w> to pressure from the opposition.",
                            "target_lang_translations": {
                                "ru": "Правительство отказалось <w>подчиняться</w> давлению оппозиции."
                            }
                        },
                        {
                            "text": "I must <w>bow</w> to your superior knowledge on this subject.",
                            "target_lang_translations": {
                                "ru": "Я должен <w>склониться</w> перед вашими превосходящими знаниями в этом вопросе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "bow to pressure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Подчиняться чему-либо; уступать или сдаваться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подчиняться",
                                "target_lang_sense_clarification": "давлению или силе"
                            },
                            {
                                "target_lang_word": "уступать",
                                "target_lang_sense_clarification": "соглашаться против воли"
                            },
                            {
                                "target_lang_word": "склоняться",
                                "target_lang_sense_clarification": "перед авторитетом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8f7e690-061a-34c1-b4d2-760a8cd9cd00",
                    "sense_definition": "To become bent or curved under weight or pressure.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "shape_curve",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old wooden shelf <w>bowed</w> in the middle under the weight of the books.",
                            "target_lang_translations": {
                                "ru": "Старая деревянная полка <w>прогнулась</w> посередине под тяжестью книг."
                            }
                        },
                        {
                            "text": "The tall trees <w>bowed</w> in the fierce wind.",
                            "target_lang_translations": {
                                "ru": "Высокие деревья <w>гнулись</w> под сильным ветром."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Искривляться или прогибаться под весом или давлением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "гнуться",
                                "target_lang_sense_clarification": "менять форму"
                            },
                            {
                                "target_lang_word": "прогибаться",
                                "target_lang_sense_clarification": "под тяжестью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d813f35f-027f-35a1-b6f6-d9e79881964b",
                    "sense_definition": "To play music on a stringed instrument using a bow.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>bowed</w> the cello with great emotion during the solo.",
                            "target_lang_translations": {
                                "ru": "Во время соло он с большим чувством <w>вел смычком</w> по виолончели."
                            }
                        },
                        {
                            "text": "The musician <w>bowed</w> the strings gently to produce a soft sound.",
                            "target_lang_translations": {
                                "ru": "Музыкант мягко <w>касался смычком</w> струн, чтобы извлечь нежный звук."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Играть на струнном инструменте с помощью смычка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "играть смычком",
                                "target_lang_sense_clarification": "способ игры"
                            },
                            {
                                "target_lang_word": "водить смычком",
                                "target_lang_sense_clarification": "действие"
                            }
                        ]
                    }
                }
            ]
        },
        {

                                "target_lang_sense_clarification": "sudden attack"
                            },
                            {
                                "target_lang_word": "hold-up",
                                "target_lang_sense_clarification": "armed robbery"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ограбление",
                                "target_lang_sense_clarification": "хищение имущества"
                            },

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "21fe1b1d-455f-3b8c-9a52-0adfe5455a76",
                    "sense_definition": "Zich voortbewegen als een vloeistof; op een soepele en continue manier bewegen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het water <vloeide> langzaam vanuit de bergen naar de zee.",
                            "target_lang_translations": {
                                "ru": "Вода медленно <текла> с гор к морю."
                            }
                        },
                        {
                            "text": "Tijdens het feest liet de gastheer de wijn rijkelijk <vloeien>.",
                            "target_lang_translations": {
                                "ru": "Во время праздника хозяин позволил вину <литься> рекой."
                            }
                        }
                    ],
                    "synonyms": [
                        "stromen"
                    ],
                    "antonyms": [
                        "stollen"
                    ],
                    "common_phrases": [
                        "rijkelijk vloeien",
                        "in elkaar vloeien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Двигаться подобно жидкости; перемещаться плавно и непрерывно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "течь",
                                "target_lang_sense_clarification": "о движении жидкости"
                            },
                            {
                                "target_lang_word": "литься",
                                "target_lang_sense_clarification": "плавно струиться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1194dc7a-42d8-35bd-ad22-87b940885692",
                    "sense_definition": "Bloed verliezen uit de baarmoeder, bijvoorbeeld tijdens de menstruatie of na een bevalling.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "medical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij bleef na de bevalling nog enkele dagen licht <vloeien>.",
                            "target_lang_translations": {
                                "ru": "У нее продолжало слегка <кровить> еще несколько дней после родов."
                            }
                        },
                        {
                            "text": "Als je hevig blijft <vloeien>, moet je contact opnemen met de huisarts.",
                            "target_lang_translations": {
                                "ru": "Если вы продолжаете сильно <кровоточить>, нужно обратиться к врачу."
                            }
                        }
                    ],
                    "synonyms": [
                        "bloeden",
                        "menstrueren"
                    ],
                    "common_phrases": [
                        "hevig vloeien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Терять кровь из матки, например, во время менструации или после родов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кровоточить",
                                "target_lang_sense_clarification": "терять кровь"
                            },
                            {
                                "target_lang_word": "истекать кровью",
                                "target_lang_sense_clarification": "сильное кровотечение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8128853-f648-3236-aa33-4fa2260821b8",
                    "sense_definition": "(van papier) Inkt te snel opzuigen en laten uitlopen, waardoor geschreven lijnen onscherp worden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "writing_ink",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt beter niet met een vulpen schrijven omdat dit goedkope papier <vloeit>.",
                            "target_lang_translations": {
                                "ru": "Лучше не писать перьевой ручкой, потому что эта дешевая бумага <промокает>."
                            }
                        },
                        {
                            "text": "De inkt is onleesbaar geworden doordat het papier zo sterk <vloeit>.",
                            "target_lang_translations": {
                                "ru": "Чернила стали нечитаемыми, так как бумага сильно <промокает>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "(о бумаге) Слишком быстро впитывать чернила, из-за чего написанное расплывается."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "промокать",
                                "target_lang_sense_clarification": "пропускать чернила"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c3558066-0aeb-3b38-be0d-deaba9912e1b",
                    "sense_definition": "Natte inkt droogmaken door er absorberend papier op te drukken.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "writing_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <vloeide> zijn handtekening voorzichtig zodat er geen vlekken ontstonden.",
                            "target_lang_translations": {
                                "ru": "Он осторожно <промакнул> свою подпись, чтобы не было пятен."
                            }
                        },
                        {
                            "text": "Vroeger <vloeide> men brieven standaard voordat ze in de envelop gingen.",
                            "target_lang_translations": {
                                "ru": "Раньше люди стандартно <промакивали> письма перед тем, как положить их в конверт."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "gebruik van vloeipapier is zeldzaam geworden"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Высушивать мокрые чернила, прижимая к ним впитывающую бумагу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "промакивать",
                                "target_lang_sense_clarification": "сушить промокашкой"
                            }
                        ]
                    }
                }
            ]
        },

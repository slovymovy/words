{
    "word_family": [
        "pozostawać",
        "pozostawić",
        "pozostałość",
        "pozostawienie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "575ecd76-3538-38d7-a7ea-e0768a75e6c6",
                    "sense_definition": "Nie opuścić miejsca, w którym się przebywało; nie wyjechać.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <pozostać> w łóżku, dopóki nie wyzdrowiejesz.",
                            "target_lang_translations": {
                                "ru": "Ты должен <оставаться> в постели, пока не выздоровеешь."
                            }
                        },
                        {
                            "text": "Wszyscy goście wyszli, ale on postanowił <pozostać> w domu.",
                            "target_lang_translations": {
                                "ru": "Все гости ушли, но он решил <остаться> дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "zostać"
                    ],
                    "common_phrases": [
                        "pozostać w domu",
                        "pozostać na miejscu",
                        "pozostać w tyle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не покинуть место, в котором находился; не уехать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остаться",
                                "target_lang_sense_clarification": "не уходить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "51f5fe4c-f233-3e15-bf16-6eb84189ceaa",
                    "sense_definition": "Utrzymać dotychczasowy stan, cechy lub właściwości; nadal znajdować się w określonej sytuacji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo wielu lat rozłąki, oni <pozostali> najlepszymi przyjaciółmi.",
                            "target_lang_translations": {
                                "ru": "Несмотря на много лет разлуки, они <остались> лучшими друзьями."
                            }
                        },
                        {
                            "text": "Ta sprawa musi <pozostać> naszą tajemnicą.",
                            "target_lang_translations": {
                                "ru": "Это дело должно <остаться> нашей тайной."
                            }
                        }
                    ],
                    "synonyms": [
                        "zostać"
                    ],
                    "common_phrases": [
                        "pozostać w kontakcie",
                        "pozostać przy życiu",
                        "pozostać bez zmian",
                        "pozostać sobą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сохранить прежнее состояние, черты или свойства; продолжать находиться в определенной ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остаться",
                                "target_lang_sense_clarification": "сохранить состояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b349e9e8-2db7-32ee-b0e4-fc2b8b915055",
                    "sense_definition": "Być tym, co zostało z większej ilości, liczby lub całości; uchować się.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do końca egzaminu <pozostało> tylko piętnaście minut.",
                            "target_lang_translations": {
                                "ru": "До конца экзамена <осталось> всего пятнадцать минут."
                            }
                        },
                        {
                            "text": "Z całego ciasta <pozostał> tylko jeden mały kawałek.",
                            "target_lang_translations": {
                                "ru": "От всего пирога <остался> только один маленький кусочек."
                            }
                        }
                    ],
                    "synonyms": [
                        "zostać"
                    ],
                    "common_phrases": [
                        "nic nie pozostało",
                        "pozostało niewiele czasu",
                        "pozostało do zrobienia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть тем, что сохранилось от большего количества или целого; уцелеть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остаться",
                                "target_lang_sense_clarification": "быть в остатке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "17ed3a0d-2663-3a0e-b15f-d68b1ec7205f",
                    "sense_definition": "Przetrwać lub być zachowanym po czyimś odejściu, śmierci lub po minionym zdarzeniu.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "legacy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jego słowa na zawsze <pozostaną> w mojej pamięci.",
                            "target_lang_translations": {
                                "ru": "Его слова навсегда <останутся> в моей памяти."
                            }
                        },
                        {
                            "text": "Po dawnej fabryce <pozostały> dzisiaj tylko ruiny.",
                            "target_lang_translations": {
                                "ru": "От старой фабрики сегодня <остались> только руины."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pozostać w pamięci",
                        "pozostać po przodkach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сохраниться после чьего-либо ухода, смерти или прошедшего события."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "остаться",
                                "target_lang_sense_clarification": "сохраниться"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
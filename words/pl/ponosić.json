    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "eb88f153-504c-355f-8ac1-0f15d22aefec",
                    "sense_definition": "Brać na siebie ciężar czegoś negatywnego, trudnego lub wymagającego, takiego jak odpowiedzialność, koszty, kara lub skutki działań.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "burden_responsibility",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Każdy pracownik musi <ponosić> odpowiedzialność za swoje decyzje zawodowe.",
                            "target_lang_translations": {
                                "ru": "Каждый сотрудник должен <нести> ответственность за свои профессиональные решения."
                            }
                        },
                        {
                            "text": "Firma zaczęła <ponosić> straty z powodu nagłego spadku sprzedaży.",
                            "target_lang_translations": {
                                "ru": "Компания начала <нести> убытки из-за внезапного падения продаж."
                            }
                        },
                        {
                            "text": "Nie chcę <ponosić> konsekwencji twojego nieprzemyślanego zachowania.",
                            "target_lang_translations": {
                                "ru": "Я не хочу <отвечать> за последствия твоего необдуманного поведения."
                            }
                        }
                    ],
                    "synonyms": [
                        "doświadczać",
                        "doznawać",
                        "płacić"
                    ],
                    "antonyms": [
                        "unikać",
                        "zrzucać"
                    ],
                    "common_phrases": [
                        "ponosić odpowiedzialność",
                        "ponosić koszty",
                        "ponosić konsekwencje",
                        "ponosić winę",
                        "ponosić stratę",
                        "ponosić klęskę",
                        "ponosić śmierć"
                    ],
                    "target_lang_definitions": {
                        "ru": "Брать на себя бремя чего-то негативного, трудного или требующего усилий, например, ответственности, расходов или последствий действий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нести",
                                "target_lang_sense_clarification": "ответственность, расходы"
                            },
                            {
                                "target_lang_word": "терпеть",
                                "target_lang_sense_clarification": "убытки, поражение"
                            },
                            {
                                "target_lang_word": "отвечать",
                                "target_lang_sense_clarification": "за последствия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7abb95d9-810b-31dc-a447-69a97b90377f",
                    "sense_definition": "Trzymać kogoś lub coś i przemieszczać się z tym przez pewien, zwykle krótki, czas; także mieć na sobie ubranie przez jakiś czas (np. w celu rozchodzenia butów).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_carrying",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz trochę <ponosić> te nowe buty po domu, żeby przestały cię obcierać.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно немного <поносить> эти новые туфли дома, чтобы они перестали натирать."
                            }
                        },
                        {
                            "text": "Ojciec obiecał <ponosić> zmęczoną córkę na rękach do samego domu.",
                            "target_lang_translations": {
                                "ru": "Отец обещал <поносить> уставшую дочь на руках до самого дома."
                            }
                        }
                    ],
                    "synonyms": [
                        "nosić",
                        "dźwigać",
                        "trzymać"
                    ],
                    "antonyms": [
                        "odłożyć",
                        "postawić"
                    ],
                    "common_phrases": [
                        "ponosić na rękach",
                        "ponosić buty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Держать кого-то или что-то и перемещаться с этим в течение некоторого времени; также носить одежду некоторое время (например, чтобы разносить обувь)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поносить",
                                "target_lang_sense_clarification": "на руках, одежду (недолго)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c265259f-ad47-3c92-9cb8-cfc58b3456bd",
                    "sense_definition": "Ulegać silnym emocjom, impulsom lub wyobraźni, tracąc chwilowo panowanie nad swoim zachowaniem.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czasami <ponosi> go fantazja i opowiada historie, które nigdy się nie wydarzyły.",
                            "target_lang_translations": {
                                "ru": "Иногда его <заносит> в фантазиях, и он рассказывает истории, которых никогда не было."
                            }
                        },
                        {
                            "text": "Kiedy <ponoszą> go nerwy, zaczyna krzyczeć na wszystkich dookoła.",
                            "target_lang_translations": {
                                "ru": "Когда у него <сдают> нервы, он начинает кричать на всех вокруг."
                            }
                        }
                    ],
                    "synonyms": [
                        "ulegać"
                    ],
                    "antonyms": [
                        "panować"
                    ],
                    "common_phrases": [
                        "ponoszą nerwy",
                        "ponosi fantazja",
                        "ponosi gniew"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поддаваться сильным эмоциям, импульсам или воображению, временно теряя контроль над своим поведением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заносить",
                                "target_lang_sense_clarification": "разг., о потере контроля"
                            },
                            {
                                "target_lang_word": "увлекать",
                                "target_lang_sense_clarification": "о фантазии, чувствах"
                            },
                            {
                                "target_lang_word": "сдавать",
                                "target_lang_sense_clarification": "о нервах"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
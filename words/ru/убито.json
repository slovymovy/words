    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dca04b7b-6eb2-38f4-8345-a585ba56f474",
                    "sense_definition": "Краткая форма причастия прошедшего времени среднего рода единственного числа от глагола <w>убить</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Животное было <w>убито</w> одним точным выстрелом.",
                            "target_lang_translations": {
                                "en": "The animal was <w>killed</w> by a single precise shot."
                            }
                        },
                        {
                            "text": "На этот бессмысленный спор было <w>убито</w> слишком много времени.",
                            "target_lang_translations": {
                                "en": "Too much time was <w>wasted</w> on this meaningless argument."
                            }
                        }
                    ],
                    "common_phrases": [
                        "было убито"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Short form of the past participle of the verb \"to kill\" (neuter gender)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "killed",
                                "target_lang_sense_clarification": "caused to die"
                            },
                            {
                                "target_lang_word": "wasted",
                                "target_lang_sense_clarification": "spent uselessly (of time)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4786ecb7-8bcf-3de8-9858-1e8825bd80d4",
                    "sense_definition": "С выражением крайней степени горя, отчаяния или подавленности.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он <w>убито</w> молчал, не зная, что ответить на обвинения.",
                            "target_lang_translations": {
                                "en": "He remained <w>dejectedly</w> silent, not knowing how to answer the accusations."
                            }
                        },
                        {
                            "text": "Девушка <w>убито</w> посмотрела на разбитую вазу и заплакала.",
                            "target_lang_translations": {
                                "en": "The girl looked <w>despairingly</w> at the broken vase and started crying."
                            }
                        }
                    ],
                    "synonyms": [
                        "подавленно",
                        "удручённо",
                        "безнадёжно"
                    ],
                    "antonyms": [
                        "радостно",
                        "оживлённо",
                        "бодро"
                    ],
                    "common_phrases": [
                        "убито молчать",
                        "убито смотреть",
                        "убито ответить"
                    ],
                    "target_lang_definitions": {
                        "en": "In a manner expressing extreme grief, despair, or depression."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "dejectedly",
                                "target_lang_sense_clarification": "in a depressed manner"
                            },
                            {
                                "target_lang_word": "despairingly",
                                "target_lang_sense_clarification": "showing loss of hope"
                            },
                            {
                                "target_lang_word": "brokenly",
                                "target_lang_sense_clarification": "with emotional collapse"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6b293836-33d1-3e4b-8286-9d1f4cb48d11",
                    "sense_definition": "Краткая форма среднего рода единственного числа прилагательного <w>убитый</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Его лицо было <w>убито</w> и выражало глубокую печаль.",
                            "target_lang_translations": {
                                "en": "His face was <w>crushed</w> and expressed deep sorrow."
                            }
                        },
                        {
                            "text": "Всё живое в нём казалось <w>убито</w> горем.",
                            "target_lang_translations": {
                                "en": "Everything alive in him seemed <w>destroyed</w> by grief."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "adjective"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Short form of the adjective describing a state of being crushed, exhausted, or destroyed (neuter gender)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crushed",
                                "target_lang_sense_clarification": "emotionally overwhelmed"
                            },
                            {
                                "target_lang_word": "destroyed",
                                "target_lang_sense_clarification": "ruined or annihilated"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
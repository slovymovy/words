                },
                {
                    "sense_id": "870f2552-3d52-3239-83de-f6019d47a027",
                    "sense_definition": "Stan całkowitego upadku moralnego, finansowego lub życiowego; sytuacja krytyczna.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_decline",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po utracie pracy czuł, że sięgnął <dna>.",
                            "target_lang_translations": {
                                "ru": "Потеряв работу, он почувствовал, что достиг <дна>."
                            }
                        },
                        {
                            "text": "Musisz odbić się od <dna> i zacząć wszystko od nowa.",
                            "target_lang_translations": {
                                "ru": "Ты должен оттолкнуться от <дна> и начать всё сначала."
                            }
                        }
                    ],
                    "synonyms": [
                        "upadek"
                    ],
                    "antonyms": [
                        "szczyt"
                    ],
                    "common_phrases": [
                        "sięgnąć dna",
                        "stoczyć się na dno",
                        "odbić się od dna"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние полного морального, финансового или жизненного упадка; критическая ситуация."

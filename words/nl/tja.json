            "senses": [
                {
                    "sense_id": "2341b351-1537-330c-a5dc-21f301120ec7",
                    "sense_definition": "Wordt gebruikt om aan te geven dat men nadenkt, aarzelt of niet direct een antwoord heeft.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Waar liggen de autosleutels eigenlijk? — <w>Tja</w>, daar vraag je me wat."
                        },
                        {
                            "text": "<w>Tja</w>, ik weet eigenlijk niet zeker of ik wel tijd heb."
                        }
                    ],
                    "synonyms": [
                        "hmm",
                        "eh",
                        "nou"
                    ],
                    "common_phrases": [
                        "tja, even denken",
                        "tja, wie weet"
                    ],
                    "traits": [

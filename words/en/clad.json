    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4b2814ca-bd6b-3dcc-a577-c436ea612f53",
                    "sense_definition": "Wearing clothing or covered with armor.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "wearing_clothes",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mysterious stranger arrived at the party <w>clad</w> entirely in black.",
                            "target_lang_translations": {
                                "ru": "Таинственный незнакомец прибыл на вечеринку, <w>облачённый</w> во всё чёрное."
                            }
                        },
                        {
                            "text": "Despite the cold weather, the runners were scantily <w>clad</w> in shorts and vests.",
                            "target_lang_translations": {
                                "ru": "Несмотря на холодную погоду, бегуны были легко <w>одеты</w> в шорты и майки."
                            }
                        }
                    ],
                    "synonyms": [
                        "dressed",
                        "clothed",
                        "attired"
                    ],
                    "antonyms": [
                        "unclad"
                    ],
                    "common_phrases": [
                        "clad in",
                        "scantily clad",
                        "fully clad",
                        "armor-clad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Носящий одежду или покрытый доспехами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одетый",
                                "target_lang_sense_clarification": "носящий одежду"
                            },
                            {
                                "target_lang_word": "облачённый",
                                "target_lang_sense_clarification": "торжественно или полностью одетый (например, в доспехи)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c03cbc8b-5426-37bd-b7b8-c05a07dec80c",
                    "sense_definition": "Covered or enveloped by a specified material or substance.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "covered_surface",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We took a photograph of the beautiful snow-<w>clad</w> mountains.",
                            "target_lang_translations": {
                                "ru": "Мы сфотографировали красивые, <w>покрытые</w> снегом горы."
                            }
                        },
                        {
                            "text": "The old cottage was charming with its ivy-<w>clad</w> walls.",
                            "target_lang_translations": {
                                "ru": "Старый коттедж был очарователен своими стенами, <w>увитыми</w> плющом."
                            }
                        }
                    ],
                    "synonyms": [
                        "clothed"
                    ],
                    "common_phrases": [
                        "snow-clad",
                        "ivy-clad",
                        "iron-clad",
                        "copper-clad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрытый или окутанный определённым материалом или веществом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "покрытый",
                                "target_lang_sense_clarification": "о поверхности, закрытой чем-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0128185c-ab00-3c1f-824e-d01f3616411c",
                    "sense_definition": "Simple past and past participle of <w>clothe</w>.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hills were <w>clad</w> in a thick layer of mist this morning.",
                            "target_lang_translations": {
                                "ru": "Этим утром холмы были <w>окутаны</w> плотным слоем тумана."
                            }
                        },
                        {
                            "text": "She <w>clad</w> herself in warm layers before going out into the storm.",
                            "target_lang_translations": {
                                "ru": "Она <w>облачилась</w> в тёплую многослойную одежду перед тем, как выйти в бурю."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past form of clothe"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола clothe (одевать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одевать",
                                "target_lang_sense_clarification": "облекать в одежду"
                            },
                            {
                                "target_lang_word": "облачать",
                                "target_lang_sense_clarification": "наряжать, одевать торжественно"
                            },
                            {
                                "target_lang_word": "окутывать",
                                "target_lang_sense_clarification": "перен. покрывать (о тумане, дымке)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "39722ef2-1d35-31e8-93f8-92cb78cf3bed",
                    "sense_definition": "To cover a structure or material with a protective or decorative layer.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "technical_covering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The architects decided to <w>clad</w> the building in glass panels.",
                            "target_lang_translations": {
                                "ru": "Архитекторы решили <w>облицевать</w> здание стеклянными панелями."
                            }
                        },
                        {
                            "text": "Workers will <w>clad</w> the pipes with insulation to prevent freezing.",
                            "target_lang_translations": {
                                "ru": "Рабочие <w>обшиют</w> трубы изоляцией для предотвращения замерзания."
                            }
                        }
                    ],
                    "common_phrases": [
                        "clad with",
                        "clad in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрывать конструкцию или материал защитным или декоративным слоем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "облицовывать",
                                "target_lang_sense_clarification": "покрывать поверхность отделочным материалом"
                            },
                            {
                                "target_lang_word": "обшивать",
                                "target_lang_sense_clarification": "покрывать слоем материала (досками, металлом)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c8159043-f13a-3b54-a50d-e2c90d5347de",
                    "sense_definition": "To dress or cover someone with clothing.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "archaic_dressing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The queen <w>clad</w> her knights in silver armor for the ceremony.",
                            "target_lang_translations": {
                                "ru": "Королева <w>облачила</w> своих рыцарей в серебряные доспехи для церемонии."
                            }
                        },
                        {
                            "text": "Nature <w>clads</w> the fields in green when spring arrives.",
                            "target_lang_translations": {
                                "ru": "Природа <w>одевает</w> поля в зелень, когда приходит весна."
                            }
                        }
                    ],
                    "synonyms": [
                        "dressed",
                        "clothed"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "modern usage prefers 'clothe' or 'dress'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одевать или покрывать кого-либо одеждой (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "облачать",
                                "target_lang_sense_clarification": "высокий стиль или устаревшее"
                            },
                            {
                                "target_lang_word": "одевать",
                                "target_lang_sense_clarification": "общее значение"
                            }
                        ]
                    }
                }
            ]
        }
    ]

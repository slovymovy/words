    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "d0cacf51-ce6c-36aa-a7b6-8cecbf910123",
                    "sense_definition": "Anders dan wat gebruikelijk of oorspronkelijk is, vaak als vervanging bedoeld.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_option",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door de wegwerkzaamheden moesten we een <w>alternatieve</w> route nemen.",
                            "target_lang_translations": {
                                "ru": "Из-за дорожных работ нам пришлось выбрать <w>альтернативный</w> маршрут.",
                                "en": "Due to the roadworks, we had to take an <w>alternative</w> route."
                            }
                        },
                        {
                            "text": "Er wordt veel onderzoek gedaan naar <w>alternatieve</w> energiebronnen zoals wind en zon.",
                            "target_lang_translations": {
                                "ru": "Проводится много исследований <w>альтернативных</w> источников энергии, таких как ветер и солнце.",
                                "en": "Much research is being done into <w>alternative</w> energy sources such as wind and sun."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitwijkmogelijkheid",
                        "mogelijkheid"
                    ],
                    "common_phrases": [
                        "alternatieve route",
                        "alternatieve geneeswijze",
                        "alternatieve energie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отличающийся от обычного или первоначального, часто предназначенный для замены.",
                        "en": "Different from what is usual or original, often intended as a replacement."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "альтернативный",
                                "target_lang_sense_clarification": "другой, заменяющий"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "alternative",
                                "target_lang_sense_clarification": "different, replacement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a9709844-4758-3da1-aae6-ab4548042f63",
                    "sense_definition": "Afwijkend van de gevestigde normen of de massa, vaak met betrekking tot cultuur of levensstijl.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn zus luistert graag naar <w>alternatieve</w> muziek die je niet op de radio hoort.",
                            "target_lang_translations": {
                                "ru": "Моя сестра любит слушать <w>альтернативную</w> музыку, которую не услышишь по радио.",
                                "en": "My sister likes listening to <w>alternative</w> music that you don't hear on the radio."
                            }
                        },
                        {
                            "text": "Zij hebben een <w>alternatieve</w> levensstijl en wonen in een commune.",
                            "target_lang_translations": {
                                "ru": "Они ведут <w>альтернативный</w> образ жизни и живут в коммуне.",
                                "en": "They have an <w>alternative</w> lifestyle and live in a commune."
                            }
                        }
                    ],
                    "antonyms": [
                        "conventioneel",
                        "mainstream"
                    ],
                    "common_phrases": [
                        "alternatieve muziek",
                        "alternatieve scene",
                        "alternatieve kleding"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отклоняющийся от установленных норм или массовой культуры.",
                        "en": "Deviating from established norms or the masses, often regarding culture or lifestyle."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "альтернативный",
                                "target_lang_sense_clarification": "неформальный, андеграундный"
                            },
                            {
                                "target_lang_word": "нетрадиционный",
                                "target_lang_sense_clarification": "отличающийся от принятых норм"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "alternative",
                                "target_lang_sense_clarification": "non-mainstream, underground"
                            },
                            {
                                "target_lang_word": "unconventional",
                                "target_lang_sense_clarification": "not conforming to norms"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1c1d4530-1dd1-3d95-bde4-929373cf84f3",
                    "sense_definition": "Een andere mogelijkheid of keuze die men kan maken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_option",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heeft u daar misschien een goed <w>alternatief</w> voor?",
                            "target_lang_translations": {
                                "ru": "У вас, может быть, есть хорошая <w>альтернатива</w> этому?",
                                "en": "Do you perhaps have a good <w>alternative</w> for that?"
                            }
                        },
                        {
                            "text": "Omdat de trein niet reed, was de bus het enige <w>alternatief</w>.",
                            "target_lang_translations": {
                                "ru": "Так как поезд не ходил, автобус был единственной <w>альтернативой</w>.",
                                "en": "Because the train wasn't running, the bus was the only <w>alternative</w>."
                            }
                        }
                    ],
                    "synonyms": [

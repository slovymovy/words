                            },
                            {
                                "target_lang_word": "удрать",
                                "target_lang_sense_clarification": "разг. быстро убежать, скрыться"
                            },
                            {
                                "target_lang_word": "смыться",
                                "target_lang_sense_clarification": "разг. незаметно уйти, исчезнуть"

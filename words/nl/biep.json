{
    "word_family": [
        "biepen",
        "bieper"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "551dc4bc-3b6e-3611-acb4-b3dd55eebebc",
                    "sense_definition": "Een kort, hoog geluid, vaak gemaakt door een elektronisch apparaat als signaal.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_signal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Spreek uw bericht in na de <w>biep</w>.",
                            "target_lang_translations": {
                                "ru": "Оставьте сообщение после <w>звукового сигнала</w>.",
                                "en": "Leave your message after the <w>beep</w>."
                            }
                        },
                        {
                            "text": "De magnetron gaf een luide <w>biep</w> toen het eten klaar was.",
                            "target_lang_translations": {
                                "ru": "Микроволновка издала громкий <w>писк</w>, когда еда была готова.",
                                "en": "The microwave gave a loud <w>beep</w> when the food was ready."
                            }
                        }
                    ],
                    "synonyms": [
                        "bieptoon"
                    ],
                    "common_phrases": [
                        "na de biep",
                        "een korte biep"
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткий высокий звук, часто издаваемый электронным устройством в качестве сигнала.",
                        "en": "A short, high-pitched sound, often made by an electronic device as a signal."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звуковой сигнал",
                                "target_lang_sense_clarification": "общий термин, часто используется для автоответчиков"
                            },
                            {
                                "target_lang_word": "гудок",
                                "target_lang_sense_clarification": "телефонный или предупреждающий сигнал"
                            },
                            {
                                "target_lang_word": "писк",
                                "target_lang_sense_clarification": "описание высокого звука"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "beep",
                                "target_lang_sense_clarification": "short electronic sound"
                            },
                            {
                                "target_lang_word": "tone",
                                "target_lang_sense_clarification": "audio signal, e.g., for voicemail"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "bc6b647d-b7c6-31d8-b5b5-6326a91b502c",
                    "sense_definition": "Woord dat een kort, elektronisch geluid nabootst.",
                    "learner_level": "A2",
                    "frequency": "Low",
                    "semantic_group_id": "sound_imitation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Biep</w>, <w>biep</w>, daar gaat de wekker alweer.",
                            "target_lang_translations": {
                                "ru": "<w>Бип</w>, <w>бип</w>, вот и будильник опять звонит.",
                                "en": "<w>Beep</w>, <w>beep</w>, there goes the alarm clock again."
                            }
                        },
                        {
                            "text": "De auto deed <w>biep</w> toen ik hem op slot deed.",
                            "target_lang_translations": {
                                "ru": "Машина сделала <w>бип</w>, когда я её запер.",
                                "en": "The car went <w>beep</w> when I locked it."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Слово, имитирующее короткий электронный звук.",
                        "en": "Word imitating a short, electronic sound."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бип",
                                "target_lang_sense_clarification": "звукоподражание"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "beep",
                                "target_lang_sense_clarification": "onomatopoeia"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ec6b7eb3-47b7-319b-ac56-63198efccee4",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>biepen</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>biep</w> de boodschappen snel bij de zelfscankassa.",
                            "target_lang_translations": {
                                "ru": "Я быстро <w>пробиваю</w> продукты на кассе самообслуживания.",
                                "en": "I quickly <w>scan</w> the groceries at the self-checkout."
                            }
                        },
                        {
                            "text": "Ik <w>biep</w> je wel even als ik onderweg ben.",
                            "target_lang_translations": {
                                "ru": "Я <w>маякну</w> тебе, когда буду в пути.",
                                "en": "I'll <w>buzz</w> you when I'm on my way."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма первого лица единственного числа настоящего времени глагола biepen (пищать, сканировать).",
                        "en": "First person singular present tense of 'biepen' (to beep, to scan)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пробиваю",
                                "target_lang_sense_clarification": "сканировать товары"
                            },
                            {
                                "target_lang_word": "маякну",
                                "target_lang_sense_clarification": "дать знать, сделать короткий звонок"
                            },
                            {
                                "target_lang_word": "пикаю",
                                "target_lang_sense_clarification": "разг. издавать звук или сканировать"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "scan",
                                "target_lang_sense_clarification": "to register items or cards"
                            },
                            {
                                "target_lang_word": "buzz",
                                "target_lang_sense_clarification": "informal: to contact or signal someone"
                            },
                            {
                                "target_lang_word": "beep",
                                "target_lang_sense_clarification": "to make a sound"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffcc2cf6-2ce4-3aad-a3f1-8f1bf9e10b0e",
                    "sense_definition": "Gebiedende wijs van <w>biepen</w>.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Biep</w> even als je voor de deur staat.",
                            "target_lang_translations": {
                                "ru": "<w>Посигналь</w> (или позвони), когда будешь у двери.",
                                "en": "<w>Buzz</w> when you are at the door."
                            }
                        },
                        {
                            "text": "<w>Biep</w> die pas nog eens voor de lezer.",
                            "target_lang_translations": {
                                "ru": "<w>Пикни</w> этим пропуском ещё раз перед считывателем.",
                                "en": "<w>Scan</w> that card in front of the reader again."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повелительное наклонение глагола biepen.",
                        "en": "Imperative mood of 'biepen'."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посигналь",
                                "target_lang_sense_clarification": "издай звук, нажми на звонок"
                            },
                            {
                                "target_lang_word": "пикни",
                                "target_lang_sense_clarification": "разг. приложи карту/сканируй"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "scan",
                                "target_lang_sense_clarification": "command: register an item"
                            },
                            {
                                "target_lang_word": "buzz",
                                "target_lang_sense_clarification": "command: give a signal"
                            },
                            {
                                "target_lang_word": "beep",
                                "target_lang_sense_clarification": "command: make a sound"
                            }
                        ]
                    }
                }
            ]
        }
    ]

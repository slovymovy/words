    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "02166ede-07f9-332f-b653-c4bc4c72d582",
                    "sense_definition": "Procedura kontroli dokumentów i bagażu przed podróżą samolotem lub przy przekraczaniu granicy państwowej.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy być na lotnisku dwie godziny wcześniej, żeby zdążyć na <w>odprawę</w>.",
                            "target_lang_translations": {
                                "ru": "Мы должны быть в аэропорту за два часа, чтобы успеть на <w>регистрацию</w>."
                            }
                        },
                        {
                            "text": "Pasażerowie proszeni są o przygotowanie paszportów, ponieważ <w>odprawa</w> celna zaraz się rozpocznie.",
                            "target_lang_translations": {
                                "ru": "Пассажиров просят подготовить паспорта, так как сейчас начнется таможенный <w>контроль</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "komunia"
                    ],
                    "common_phrases": [
                        "odprawa paszportowa",
                        "odprawa celna",
                        "odprawa biletowa",
                        "przejść odprawę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процедура проверки документов и багажа перед вылетом или при пересечении государственной границы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "регистрация",
                                "target_lang_sense_clarification": "на рейс"
                            },
                            {
                                "target_lang_word": "таможенный контроль",
                                "target_lang_sense_clarification": "проверка на границе"
                            },
                            {
                                "target_lang_word": "оформление",
                                "target_lang_sense_clarification": "документов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e984446e-6c8a-387d-b4d5-d546c1b55421",
                    "sense_definition": "Krótkie spotkanie służbowe, na którym przełożony przekazuje pracownikom zadania lub informacje.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano w biurze odbywa się krótka <w>odprawa</w> zespołu.",
                            "target_lang_translations": {
                                "ru": "Каждое утро в офисе проходит короткая <w>планёрка</w> команды."
                            }
                        },
                        {
                            "text": "Na <w>odprawie</w> szef rozdzielił zadania na cały nadchodzący tydzień.",
                            "target_lang_translations": {
                                "ru": "На <w>планёрке</w> начальник распределил задачи на всю предстоящую неделю."
                            }
                        }
                    ],
                    "synonyms": [
                        "komunia"
                    ],
                    "common_phrases": [
                        "poranna odprawa",
                        "odprawa techniczna",
                        "odprawa z pracownikami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Краткое служебное совещание, на котором руководитель распределяет задачи или передает информацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "планёрка",
                                "target_lang_sense_clarification": "короткое совещание"
                            },
                            {
                                "target_lang_word": "инструктаж",
                                "target_lang_sense_clarification": "получение указаний"
                            },
                            {
                                "target_lang_word": "летучка",
                                "target_lang_sense_clarification": "быстрое собрание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68c2419b-fb1c-3eb0-97d5-4deb2ad9a824",
                    "sense_definition": "Dodatkowe pieniądze wypłacane pracownikowi przez pracodawcę w momencie zwolnienia z pracy lub przejścia na emeryturę.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po zwolnieniu z firmy każdy pracownik otrzymał wysoką <w>odprawę</w>.",
                            "target_lang_translations": {
                                "ru": "После увольнения из фирмы каждый сотрудник получил крупное <w>выходное пособие</w>."
                            }
                        },
                        {
                            "text": "Przepisy określają, komu przysługuje <w>odprawa</w> emerytalna po wielu latach pracy.",
                            "target_lang_translations": {
                                "ru": "Законодательство определяет, кому полагается <w>выходное пособие</w> при выходе на пенсию после многих лет работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "komunia"
                    ],
                    "common_phrases": [
                        "odprawa emerytalna",
                        "odprawa pośmiertna",
                        "wysokość odprawy",
                        "otrzymać odprawę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Денежная выплата работнику при увольнении или выходе на пенсию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выходное пособие",
                                "target_lang_sense_clarification": "выплата при увольнении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "33bdb9eb-6540-3367-a8e1-b2a3bfcff7d7",
                    "sense_definition": "Stanowcza, często odmowna odpowiedź na czyjeś żądania lub propozycje; odesłanie kogoś z kwitkiem.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Poseł otrzymał surową <w>odprawę</w> i musiał wrócić do swojego kraju bez sukcesu.",
                            "target_lang_translations": {
                                "ru": "Посол получил суровую <w>отповедь</w> и был вынужден вернуться в свою страну, не добившись успеха."
                            }
                        },
                        {
                            "text": "Jego niegrzeczne zachowanie spotkało się z natychmiastową i ostrą <w>odprawą</w>.",
                            "target_lang_translations": {
                                "ru": "Его грубое поведение встретило немедленный и резкий <w>отпор</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "komunia"
                    ],
                    "antonyms": [
                        "akceptacja"
                    ],
                    "common_phrases": [
                        "dać komuś odprawę"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "rare in modern spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Решительный, часто негативный ответ на чьи-либо требования или предложения; отказ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отповедь",
                                "target_lang_sense_clarification": "резкий ответ"
                            },
                            {
                                "target_lang_word": "отпор",
                                "target_lang_sense_clarification": "решительный отказ"
                            }
                        ]
                    }
                }
            ]

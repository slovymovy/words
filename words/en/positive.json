    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "288a4e20-13df-3f89-a3d8-9c917ef8712f",
                    "sense_definition": "Good, useful, or leading to a good result; characterized by affirmation or acceptance.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "good_favorable",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The movie received mostly <w>positive</w> reviews from critics.",
                            "target_lang_translations": {
                                "ru": "Фильм получил в основном <w>положительные</w> отзывы от критиков."
                            }
                        },
                        {
                            "text": "Regular exercise has a very <w>positive</w> effect on your overall health.",
                            "target_lang_translations": {
                                "ru": "Регулярные упражнения оказывают очень <w>положительный</w> эффект на ваше общее здоровье."
                            }
                        }
                    ],
                    "synonyms": [
                        "good",
                        "favorable",
                        "beneficial",
                        "constructive",
                        "excellent"
                    ],
                    "antonyms": [
                        "negative",
                        "bad"
                    ],
                    "common_phrases": [
                        "positive feedback",
                        "positive impact",
                        "positive effect",
                        "positive response"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хороший, полезный или ведущий к хорошему результату; выражающий согласие или одобрение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "положительный",
                                "target_lang_sense_clarification": "благоприятный, одобряющий"
                            },
                            {
                                "target_lang_word": "позитивный",
                                "target_lang_sense_clarification": "конструктивный, хороший"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2ec0347e-83e9-3049-9a54-7d1a1c426930",
                    "sense_definition": "Hopeful and confident about the future; thinking about the good aspects of a situation.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "optimistic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He tries to keep a <w>positive</w> attitude even when things are difficult.",
                            "target_lang_translations": {
                                "ru": "Он старается сохранять <w>позитивный</w> настрой, даже когда дела идут плохо."
                            }
                        },
                        {
                            "text": "She has a <w>positive</w> outlook on life that inspires everyone around her.",
                            "target_lang_translations": {
                                "ru": "У неё <w>позитивный</w> взгляд на жизнь, который вдохновляет всех вокруг."
                            }
                        }
                    ],
                    "synonyms": [
                        "optimistic"
                    ],
                    "antonyms": [
                        "negative"
                    ],
                    "common_phrases": [
                        "positive attitude",
                        "positive outlook",
                        "stay positive",
                        "think positive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исполненный надежд и уверенности в будущем; видящий хорошие стороны в ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "позитивный",
                                "target_lang_sense_clarification": "оптимистичный настрой"
                            },
                            {
                                "target_lang_word": "оптимистичный",
                                "target_lang_sense_clarification": "полный надежд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "61234404-2591-304d-8966-65744fc4d48c",
                    "sense_definition": "Completely sure about something; having no doubt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "certainty",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am absolutely <w>positive</w> that I locked the front door.",
                            "target_lang_translations": {
                                "ru": "Я абсолютно <w>уверен</w>, что запер входную дверь."
                            }
                        },
                        {
                            "text": "Are you <w>positive</w> you saw him at the station yesterday?",
                            "target_lang_translations": {
                                "ru": "Ты <w>уверен</w>, что видел его вчера на станции?"
                            }
                        }
                    ],
                    "synonyms": [
                        "certain",
                        "sure"
                    ],
                    "antonyms": [
                        "uncertain",
                        "unsure"
                    ],
                    "common_phrases": [
                        "absolutely positive",
                        "hundred percent positive"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью уверенный в чём-либо; не имеющий сомнений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уверенный",
                                "target_lang_sense_clarification": "не сомневающийся"
                            },
                            {
                                "target_lang_word": "абсолютно уверенный",
                                "target_lang_sense_clarification": "полностью убежденный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3dbd2b3f-f4e2-3350-9a5c-61f2cb1dfc05",
                    "sense_definition": "Greater than zero.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "math",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Seven is a <w>positive</w> number, while minus seven is negative.",
                            "target_lang_translations": {
                                "ru": "Семь — это <w>положительное</w> число, а минус семь — отрицательное."
                            }
                        },
                        {
                            "text": "The temperature finally became <w>positive</w> after a week of freezing cold.",
                            "target_lang_translations": {
                                "ru": "Температура наконец стала <w>положительной</w> после недели морозов."
                            }
                        }
                    ],
                    "antonyms": [
                        "negative"
                    ],
                    "common_phrases": [
                        "positive number",
                        "positive integer",
                        "positive value"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "mathematics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Больше нуля (о числе или величине)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "положительный",
                                "target_lang_sense_clarification": "математика"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77cbf281-00e8-3627-a789-aa5356c783ad",
                    "sense_definition": "Showing the presence of a specific disease, condition, or substance in a medical or scientific test.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "test_result",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The athlete was banned after his drug test came back <w>positive</w>.",
                            "target_lang_translations": {
                                "ru": "Спортсмена дисквалифицировали после того, как его тест на допинг оказался <w>положительным</w>."
                            }
                        },
                        {
                            "text": "If the result is <w>positive</w>, you will need to undergo further treatment.",
                            "target_lang_translations": {
                                "ru": "Если результат будет <w>положительным</w>, вам нужно будет пройти дальнейшее лечение."
                            }
                        }
                    ],
                    "antonyms": [
                        "negative"
                    ],
                    "common_phrases": [
                        "test positive",
                        "false positive",
                        "HIV positive"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "medicine"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Показывающий наличие определённого заболевания, состояния или вещества (о медицинском тесте)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "положительный",
                                "target_lang_sense_clarification": "результат анализа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d71bb5b-e50a-320c-a641-36d6fc51d7df",
                    "sense_definition": "Having an electrical charge of the type carried by protons; opposite to negative.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "physics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A cation is a <w>positive</w> ion because it has lost electrons.",
                            "target_lang_translations": {
                                "ru": "Катион — это <w>положительный</w> ион, потому что он потерял электроны."
                            }
                        },
                        {
                            "text": "Connect the red cable to the <w>positive</w> terminal of the battery.",
                            "target_lang_translations": {
                                "ru": "Подсоедините красный кабель к <w>положительной</w> клемме аккумулятора."
                            }
                        }
                    ],
                    "antonyms": [
                        "negative"
                    ],
                    "common_phrases": [
                        "positive charge",
                        "positive pole",
                        "positive terminal"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "physics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий электрический заряд того же типа, что и протон; противоположный отрицательному."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "положительный",
                                "target_lang_sense_clarification": "физика: заряд"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffff432a-d43c-3e2a-bb72-a6d11cfad6ce",
                    "sense_definition": "Complete or absolute; used to emphasize a noun, often a negative one.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "intensifier",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was a <w>positive</w> nightmare getting home in that heavy traffic.",
                            "target_lang_translations": {
                                "ru": "Добраться домой в таком плотном потоке было <w>настоящим</w> кошмаром."
                            }
                        },
                        {
                            "text": "The room was a <w>positive</w> mess after the children's party.",
                            "target_lang_translations": {
                                "ru": "После детского праздника в комнате творился <w>сущий</w> беспорядок."
                            }
                        }
                    ],
                    "synonyms": [
                        "absolute",
                        "complete"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Полный, абсолютный; используется для усиления значения существительного (часто с негативным оттенком)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "настоящий",
                                "target_lang_sense_clarification": "усилительная частица"
                            },
                            {
                                "target_lang_word": "сущий",
                                "target_lang_sense_clarification": "полный (о чем-то плохом)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60e40290-32b7-37f6-ad92-da1a3076d798",
                    "sense_definition": "Describing the basic form of an adjective or adverb, not the comparative or superlative form.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the phrase 'a good book', the adjective is in the <w>positive</w> degree.",
                            "target_lang_translations": {
                                "ru": "Во фразе «хорошая книга» прилагательное стоит в <w>положительной</w> степени."
                            }
                        },
                        {
                            "text": "Better is the comparative form, while good is the <w>positive</w> form.",
                            "target_lang_translations": {
                                "ru": "«Лучше» — это сравнительная форма, а «хороший» — <w>положительная</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "positive degree",
                        "positive form"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "grammar"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к исходной форме прилагательного или наречия (не сравнительной и не превосходной)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "положительная степень",
                                "target_lang_sense_clarification": "грамматика"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "343f1b12-26c9-360a-ba0d-e59c1d482454",

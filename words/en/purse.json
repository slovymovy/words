                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "refers to historical currency units"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Историческая денежная единица или сумма денег, использовавшаяся ранее в таких странах, как Турция или Персия."

{
    "word_family": [
        "wkład",
        "wkładka",
        "wkładanie"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a8c40673-db87-3f89-b95d-4d7d545b9e50",
                    "sense_definition": "Umieszczać coś wewnątrz czegoś innego, pomiędzy czymś lub w określonym miejscu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_placement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę <wkładać> brudne naczynia prosto do zmywarki.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <складывайте> грязную посуду прямо в посудомоечную машину."
                            }
                        },
                        {
                            "text": "Zawsze <wkładam> klucze do tej samej kieszeni w plecaku.",
                            "target_lang_translations": {
                                "ru": "Я всегда <кладу> ключи в один и тот же карман рюкзака."
                            }
                        },
                        {
                            "text": "Nie <wkładaj> palców między drzwi, bo zrobisz sobie krzywdę.",
                            "target_lang_translations": {
                                "ru": "Не <суй> пальцы в дверь, а то поранишься."
                            }
                        }
                    ],
                    "synonyms": [
                        "umieszczać",
                        "wsadzać",
                        "wtykać",
                        "pakować",
                        "ładować"
                    ],
                    "antonyms": [
                        "wyjmować",
                        "wyciągać"
                    ],
                    "common_phrases": [
                        "wkładać do środka",
                        "wkładać do kieszeni",
                        "wkładać ręce",
                        "wkładać do pieca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Помещать что-либо внутрь чего-то другого, между чем-то или в определенное место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "класть",
                                "target_lang_sense_clarification": "помещать внутрь или на поверхность"
                            },
                            {
                                "target_lang_word": "вкладывать",
                                "target_lang_sense_clarification": "помещать внутрь чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd1d64da-d661-3b07-9aaf-10f94128267b",
                    "sense_definition": "Zakładać na siebie ubranie, buty lub dodatki.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kiedy jest zimno, musisz <wkładać> czapkę i szalik.",
                            "target_lang_translations": {
                                "ru": "Когда холодно, нужно <надевать> шапку и шарф."
                            }
                        },
                        {
                            "text": "Ona lubi <wkładać> eleganckie sukienki na rodzinne uroczystości.",
                            "target_lang_translations": {
                                "ru": "Она любит <надевать> элегантные платья на семейные торжества."
                            }
                        }
                    ],
                    "synonyms": [
                        "ubrać się",
                        "ubrać się"
                    ],
                    "antonyms": [
                        "zdejmować"
                    ],
                    "common_phrases": [
                        "wkładać buty",
                        "wkładać płaszcz",
                        "wkładać okulary",
                        "wkładać na siebie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Надевать на себя одежду, обувь или аксессуары."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надевать",
                                "target_lang_sense_clarification": "об одежде и аксессуарах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4418ea51-f8a9-34d3-bae6-b2445bdc50ec",
                    "sense_definition": "Angażować wysiłek, pracę, czas lub pieniądze w realizację jakiegoś celu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "investment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nauczyciele <wkładają> dużo pracy w przygotowanie ciekawych lekcji.",
                            "target_lang_translations": {
                                "ru": "Учителя <вкладывают> много труда в подготовку интересных уроков."
                            }
                        },
                        {
                            "text": "Nie warto <wkładać> pieniędzy w tak ryzykowny interes.",
                            "target_lang_translations": {
                                "ru": "Не стоит <вкладывать> деньги в такое рискованное дело."
                            }
                        }
                    ],
                    "synonyms": [
                        "inwestować",
                        "wnosić",
                        "lokować",
                        "wpłacać"
                    ],
                    "common_phrases": [
                        "wkładać wysiłek",
                        "wkładać serce",
                        "wkładać pracę",
                        "wkładać pieniądze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тратить силы, время или деньги для достижения какой-либо цели."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вкладывать",
                                "target_lang_sense_clarification": "инвестировать деньги или усилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
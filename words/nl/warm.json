                {
                    "sense_id": "13fdcbea-3499-34e2-8bca-36af40b984ba",
                    "sense_definition": "Met een hoge temperatuur; niet koud.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "temperature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de zomer is het vaak erg <w>warm</w> buiten.",

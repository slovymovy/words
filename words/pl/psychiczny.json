                    "semantic_group_id": "insane_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ludzie we wsi omijali go szerokim łukiem, mówiąc, że to <w>psychiczny</w>.",
                            "target_lang_translations": {
                                "ru": "Люди в деревне обходили его стороной, говоря, что он <w>псих</w>."
                            }
                        },
                        {
                            "text": "Nie zachowuj się jak <w>psychiczny</w>, uspokój się wreszcie.",
                            "target_lang_translations": {

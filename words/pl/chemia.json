    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dd9d40f6-4307-38cd-9377-7b13da5c8571",
                    "sense_definition": "Nauka przyrodnicza zajmująca się badaniem składu, budowy, właściwości i przemian substancji.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "science_education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój brat studiuje <chemię> na uniwersytecie.",
                            "target_lang_translations": {
                                "ru": "Мой брат изучает <химию> в университете."
                            }
                        },
                        {
                            "text": "Współczesna <chemia> pozwala tworzyć nowe, bardzo wytrzymałe materiały.",
                            "target_lang_translations": {
                                "ru": "Современная <химия> позволяет создавать новые, очень прочные материалы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chemia organiczna",
                        "chemia nieorganiczna",
                        "nagroda Nobla z chemii"
                    ],
                    "target_lang_definitions": {
                        "ru": "Естественная наука о составе, строении, свойствах и превращениях веществ."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "химия",
                                "target_lang_sense_clarification": "наука"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1f5641d4-bcef-3e19-9f9d-12f6167c0c76",
                    "sense_definition": "Lekcja w szkole, podczas której uczniowie poznają zagadnienia chemiczne.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "science_education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jutro mam trudny sprawdzian z <chemii>, więc muszę się uczyć.",
                            "target_lang_translations": {
                                "ru": "Завтра у меня сложная контрольная по <химии>, поэтому мне нужно учиться."
                            }
                        },
                        {
                            "text": "Na lekcji <chemii> robiliśmy ciekawe doświadczenia z kwasami.",
                            "target_lang_translations": {
                                "ru": "На уроке <химии> мы проводили интересные опыты с кислотами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sprawdzian z chemii",
                        "nauczyciel chemii",
                        "sala od chemii"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "skrót myślowy od 'lekcja chemii'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Урок в школе, на котором ученики изучают химию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "химия",
                                "target_lang_sense_clarification": "урок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f35de70a-f5df-349a-930a-6a25f498f93d",
                    "sense_definition": "Potoczne określenie szkodliwych, sztucznych dodatków w żywności, kosmetykach lub środkach czystości.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tych tanich warzywach nie ma żadnych witamin, tylko sama <chemia>.",
                            "target_lang_translations": {
                                "ru": "В этих дешёвых овощах нет никаких витаминов, одна сплошная <химия>."
                            }
                        },
                        {
                            "text": "Staram się unikać <chemii> w produktach, które kupuję dla dzieci.",
                            "target_lang_translations": {
                                "ru": "Я стараюсь избегать <химии> в продуктах, которые покупаю детям."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sama chemia",
                        "chemia w jedzeniu",
                        "bez chemii"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "wydźwięk negatywny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название вредных искусственных добавок в пище, косметике или бытовой химии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "химия",
                                "target_lang_sense_clarification": "искусственные добавки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1da10179-317f-33dc-8f18-0fffd19f403a",
                    "sense_definition": "Silne, instynktowne porozumienie, napięcie lub pociąg fizyczny między ludźmi.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na koncercie jazzowym między muzykami a słuchaczami musi działać <chemia>.",
                            "target_lang_translations": {
                                "ru": "На джазовом концерте между музыкантами и слушателями должна быть <химия>."
                            }
                        },
                        {
                            "text": "Od pierwszego spotkania była między nimi niesamowita <chemia>.",
                            "target_lang_translations": {
                                "ru": "С первой встречи между ними была невероятная <химия>."
                            }
                        }
                    ],
                    "synonyms": [
                        "fluidy"
                    ],
                    "common_phrases": [
                        "chemia między ludźmi",
                        "czuć chemię",
                        "brak chemii"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "użycie metaforyczne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильное инстинктивное взаимопонимание, напряжение или физическое влечение между людьми."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "химия",
                                "target_lang_sense_clarification": "связь между людьми"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eedd8af2-0d33-3efb-a867-26af09f221ae",
                    "sense_definition": "Potoczne określenie chemioterapii, metody leczenia nowotworów.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Lekarz zalecił pacjentowi przyjmowanie <chemii> przez sześć miesięcy.",
                            "target_lang_translations": {
                                "ru": "Врач назначил пациенту курс <химии> на шесть месяцев."
                            }
                        },
                        {
                            "text": "Mimo że brał silną <chemię>, starał się żyć normalnie i pracować.",
                            "target_lang_translations": {
                                "ru": "Несмотря на то, что он проходил сильную <химию>, он старался жить нормально и работать."
                            }
                        }
                    ],
                    "synonyms": [
                        "chemioterapia"
                    ],
                    "common_phrases": [
                        "brać chemię",
                        "cykl chemii",
                        "wlew chemii"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "żargon medyczny/pacjencki"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное название химиотерапии, метода лечения рака."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "химия",
                                "target_lang_sense_clarification": "химиотерапия"
                            }
                        ]
                    }
                }
            ]
        }

            "senses": [
                {
                    "sense_id": "8f909771-bd81-3400-a56d-40b39b0af6d4",
                    "sense_definition": "Человек, который выдвигается для избрания на выборах или претендует на вакантную должность.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На выборах победил <кандидат> от нашей партии.",
                            "target_lang_translations": {
                                "en": "The <w>candidate</w> from our party won the election."
                            }
                        },
                        {
                            "text": "Компания рассматривает вашего друга как перспективного <кандидата> на эту должность.",
                            "target_lang_translations": {
                                "en": "The company is considering your friend as a promising <w>candidate</w> for this position."
                            }
                        }
                    ],
                    "synonyms": [
                        "претендент",
                        "соискатель"
                    ],
                    "common_phrases": [
                        "кандидат в президенты",
                        "кандидат на должность",
                        "выдвижение кандидата",
                        "список кандидатов"
                    ],
                    "target_lang_definitions": {
                        "en": "A person who is nominated for election or applies for a vacant position."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "candidate",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "applicant",
                                "target_lang_sense_clarification": "for a job"
                            },
                            {
                                "target_lang_word": "nominee",
                                "target_lang_sense_clarification": "for an election"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "609489ab-173d-31db-a9ad-1d946b2a25a7",
                    "sense_definition": "Первая учёная степень (кандидат наук) в России и ряде других стран, а также лицо, имеющее эту степень.",

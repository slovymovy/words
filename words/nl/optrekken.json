                        "ru": [
                            {
                                "target_lang_word": "трогаться",
                                "target_lang_sense_clarification": "начинать движение"
                            },
                            {
                                "target_lang_word": "разгоняться",
                                "target_lang_sense_clarification": "увеличивать скорость"

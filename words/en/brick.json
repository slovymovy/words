    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f36145a9-5b36-3702-9f8d-7b02d3452c4a",
                    "sense_definition": "A hardened rectangular block of mud or clay, used for building walls and houses.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "building_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The construction workers laid every red <w>brick</w> carefully to ensure the wall was straight.",
                            "target_lang_translations": {
                                "ru": "Строители аккуратно укладывали каждый красный <w>кирпич</w>, чтобы стена была ровной."
                            }
                        },
                        {
                            "text": "He picked up a heavy <w>brick</w> from the pile and placed it on the garden path.",
                            "target_lang_translations": {
                                "ru": "Он взял тяжелый <w>кирпич</w> из кучи и положил его на садовую дорожку."
                            }
                        }
                    ],
                    "synonyms": [
                        "block"
                    ],
                    "common_phrases": [
                        "red brick",
                        "brick wall",
                        "lay bricks"
                    ],
                    "target_lang_definitions": {
                        "ru": "Затвердевший прямоугольный брусок из глины или грязи, используемый для строительства стен и домов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кирпич",
                                "target_lang_sense_clarification": "строительный блок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8003fcee-00ff-388b-9c16-bf204e35034b",
                    "sense_definition": "Hardened clay considered collectively as a material for construction.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "building_material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They decided to buy a house made of <w>brick</w> because it is more durable than wood.",
                            "target_lang_translations": {
                                "ru": "Они решили купить дом из <w>кирпича</w>, потому что он долговечнее деревянного."
                            }
                        },
                        {
                            "text": "The old factory was a massive structure built of red <w>brick</w> and steel.",
                            "target_lang_translations": {
                                "ru": "Старая фабрика представляла собой массивное сооружение из красного <w>кирпича</w> и стали."
                            }
                        }
                    ],
                    "common_phrases": [
                        "made of brick",
                        "brick house"
                    ],
                    "target_lang_definitions": {
                        "ru": "Затвердевшая глина как строительный материал."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кирпич",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d94c91bd-cf31-3431-8ff5-447166a3bd6e",
                    "sense_definition": "An object that has the rectangular shape and approximate size of a building block.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "shape",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She bought a solid <w>brick</w> of cheese from the market to serve at the party.",
                            "target_lang_translations": {
                                "ru": "Она купила целый <w>брусок</w> сыра на рынке, чтобы подать его на вечеринке."
                            }
                        },
                        {
                            "text": "The police found a <w>brick</w> of gold hidden inside the secret safe.",
                            "target_lang_translations": {
                                "ru": "Полиция нашла <w>слиток</w> золота, спрятанный внутри потайного сейфа."
                            }
                        }
                    ],
                    "synonyms": [
                        "block",
                        "slab"
                    ],
                    "common_phrases": [
                        "gold brick",
                        "brick of cheese"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет, имеющий прямоугольную форму и примерный размер строительного блока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брусок",
                                "target_lang_sense_clarification": "форма"
                            },
                            {
                                "target_lang_word": "слиток",
                                "target_lang_sense_clarification": "золота"
                            },
                            {
                                "target_lang_word": "брикет",
                                "target_lang_sense_clarification": "прессованный продукт"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c06f575-9259-3782-8cc1-690bbed05fd7",
                    "sense_definition": "An electronic device, such as a smartphone, that has become permanently non-functional.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "electronics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My expensive smartphone turned into a useless <w>brick</w> after the operating system update failed.",
                            "target_lang_translations": {
                                "ru": "Мой дорогой смартфон превратился в бесполезный <w>кирпич</w> после неудачного обновления операционной системы."
                            }
                        },
                        {
                            "text": "If you drop your laptop in the ocean, it will likely become a <w>brick</w>.",
                            "target_lang_translations": {
                                "ru": "Если вы уроните ноутбук в океан, он, скорее всего, станет <w>кирпичом</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "turn into a brick"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Электронное устройство (например, смартфон), которое перестало работать и не подлежит ремонту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кирпич",
                                "target_lang_sense_clarification": "сломанная электроника"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0d4cd7b8-b096-3036-8889-2e57371c0201",
                    "sense_definition": "A helpful, reliable, and kind person.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You were a real <w>brick</w> for helping me move all those heavy boxes yesterday.",
                            "target_lang_translations": {
                                "ru": "Ты был настоящим <w>молодцом</w>, что помог мне перенести все эти тяжелые коробки вчера."
                            }
                        },
                        {
                            "text": "Thanks for staying late to help us finish the project; you are such a <w>brick</w>.",
                            "target_lang_translations": {
                                "ru": "Спасибо, что остался допоздна, чтобы помочь нам закончить проект; ты просто <w>молоток</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "be a brick"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Отзывчивый, надежный и добрый человек."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "молоток",
                                "target_lang_sense_clarification": "молодец"
                            },
                            {
                                "target_lang_word": "надежный человек",
                                "target_lang_sense_clarification": "о человеке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b2c6dd65-c4b4-3a88-9198-a1591501bfec",
                    "sense_definition": "A basketball shot that misses the basket completely or bounces hard off the rim.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sports",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The player kept throwing up a <w>brick</w> every time he tried to shoot a three-pointer.",
                            "target_lang_translations": {
                                "ru": "Игрок совершал <w>промах</w> каждый раз, когда пытался забить трехочковый."
                            }
                        },
                        {
                            "text": "We lost the game because our best shooter threw a <w>brick</w> in the final seconds.",
                            "target_lang_translations": {
                                "ru": "Мы проиграли игру, потому что наш лучший снайпер сделал <w>неудачный бросок</w> на последних секундах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "throw a brick"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "sports"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бросок в баскетболе, который не попадает в корзину или сильно отскакивает от кольца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "промах",
                                "target_lang_sense_clarification": "в спорте"
                            },
                            {
                                "target_lang_word": "неудачный бросок",
                                "target_lang_sense_clarification": "баскетбол"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "cdc32f0c-467b-3809-a84a-036f0e280d68",
                    "sense_definition": "To close, block, or fill an opening using bricks.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "construction_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They decided to <w>brick</w> up the old window to make the room warmer in winter.",
                            "target_lang_translations": {
                                "ru": "Они решили <w>заложить кирпичом</w> старое окно, чтобы в комнате было теплее зимой."
                            }
                        },
                        {
                            "text": "The workers will <w>brick</w> over the entrance to the abandoned tunnel for safety.",
                            "target_lang_translations": {
                                "ru": "Рабочие <w>замуруют</w> вход в заброшенный туннель ради безопасности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "brick up",
                        "brick over"
                    ],
                    "target_lang_definitions": {
                        "ru": "Закрывать, блокировать или заполнять проем кирпичами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закладывать кирпичом",
                                "target_lang_sense_clarification": "строительство"
                            },
                            {
                                "target_lang_word": "замуровывать",
                                "target_lang_sense_clarification": "закрывать наглухо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c68afbc5-2cea-383c-b15b-0eb9b048cea4",
                    "sense_definition": "To cause an electronic device to stop working permanently, usually due to a software error.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "electronics_verb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Be careful not to <w>brick</w> your phone when you try to install unauthorized software.",
                            "target_lang_translations": {
                                "ru": "Будьте осторожны, чтобы не <w>окирпичить</w> свой телефон при попытке установить неавторизованное программное обеспечение."
                            }
                        },
                        {
                            "text": "The lightning storm caused a power surge that <w>bricked</w> my computer instantly.",
                            "target_lang_translations": {
                                "ru": "Гроза вызвала скачок напряжения, который мгновенно <w>превратил в кирпич</w> мой компьютер."
                            }
                        }
                    ],
                    "antonyms": [
                        "unbrick"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Привести электронное устройство в нерабочее состояние, обычно из-за программной ошибки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "окирпичить",
                                "target_lang_sense_clarification": "сленг"
                            },
                            {
                                "target_lang_word": "превратить в кирпич",
                                "target_lang_sense_clarification": "фраза"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [

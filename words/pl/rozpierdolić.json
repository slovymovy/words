    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ec2e5396-90ec-325a-8374-9c52275e020b",
                    "sense_definition": "Zniszczyć coś całkowicie, rozbić na kawałki lub zepsuć w sposób gwałtowny.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ze złości rzucił telefonem o ścianę i go <w>rozpierdolił</w>.",
                            "target_lang_translations": {
                                "ru": "От злости он швырнул телефон об стену и <w>расхуярил</w> его."
                            }
                        },
                        {
                            "text": "Musisz uważać, żeby nie <w>rozpierdolić</w> tej delikatnej konstrukcji jednym ruchem.",
                            "target_lang_translations": {
                                "ru": "Ты должен быть осторожен, чтобы не <w>разъебать</w> эту хрупкую конструкцию одним движением."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozbić",
                        "tłuc"
                    ],
                    "common_phrases": [
                        "rozpierdolić w drobny mak",
                        "rozpierdolić w pył"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью уничтожить что-то, разбить вдребезги или грубо сломать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разъебать",
                                "target_lang_sense_clarification": "разбить, сломать (вульг.)"
                            },
                            {
                                "target_lang_word": "расхуярить",
                                "target_lang_sense_clarification": "разбить вдребезги (вульг.)"
                            },
                            {
                                "target_lang_word": "раздолбать",
                                "target_lang_sense_clarification": "сильно повредить (груб.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e0deed06-7afb-30d9-994f-c5a5cf7ecb25",
                    "sense_definition": "Ulec poważnemu wypadkowi, rozbić się (o pojeździe) lub zepsuć się nieodwracalnie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kierowca jechał zdecydowanie za szybko i <w>rozpierdolił</w> się na drzewie.",
                            "target_lang_translations": {
                                "ru": "Водитель ехал слишком быстро и <w>разъебался</w> об дерево."
                            }
                        },
                        {
                            "text": "Mój stary laptop ostatecznie <w>rozpierdolił</w> się wczoraj wieczorem i nie działa.",
                            "target_lang_translations": {
                                "ru": "Мой старый ноутбук вчера вечером окончательно <w>наебнулся</w> и не работает."
                            }
                        }
                    ],
                    "synonyms": [
                        "rozbić"
                    ],
                    "common_phrases": [
                        "rozpierdolić się autem",
                        "rozpierdolić się na drzewie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Попасть в серьезную аварию, разбиться (о транспорте) или окончательно сломаться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разъебаться",
                                "target_lang_sense_clarification": "разбиться в аварии (вульг.)"
                            },
                            {
                                "target_lang_word": "наебнуться",
                                "target_lang_sense_clarification": "сломаться, перестать работать (вульг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b2f8e08-f34a-34b7-8444-2edd699df6f9",
                    "sense_definition": "Pobić kogoś bardzo dotkliwie i brutalnie, wyrządzając mu krzywdę fizyczną.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "violence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bandyci napadli go w parku i dotkliwie <w>rozpierdolili</w>.",
                            "target_lang_translations": {
                                "ru": "Бандиты напали на него в парке и жестоко <w>отпиздили</w>."
                            }
                        },
                        {
                            "text": "Groził, że jeśli nie oddam długu, to <w>rozpierdoli</w> mi twarz.",
                            "target_lang_translations": {
                                "ru": "Он угрожал, что если я не верну долг, то он <w>разъебет</w> мне лицо."
                            }
                        }
                    ],
                    "synonyms": [
                        "skatować",
                        "poturbować"
                    ],
                    "common_phrases": [
                        "rozpierdolić komuś łeb",
                        "rozpierdolić gościa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Жестоко избить кого-то, нанеся физические увечья."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отпиздить",
                                "target_lang_sense_clarification": "сильно избить (вульг.)"
                            },
                            {
                                "target_lang_word": "разъебать",
                                "target_lang_sense_clarification": "разбить (лицо), покалечить (вульг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab1b6cbc-463c-3a41-8916-21dddf89ca17",
                    "sense_definition": "Wydać bezmyślnie dużą ilość pieniędzy lub zmarnować cenne zasoby.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "waste",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W jeden weekend <w>rozpierdolił</w> w kasynie całą swoją miesięczną wypłatę.",
                            "target_lang_translations": {
                                "ru": "За одни выходные он <w>проебал</w> в казино всю свою месячную зарплату."
                            }
                        },
                        {
                            "text": "Nie możesz tak po prostu <w>rozpierdolić</w> spadku po babci na głupoty.",
                            "target_lang_translations": {
                                "ru": "Ты не можешь просто так <w>просрать</w> бабушкино наследство на ерунду."
                            }
                        }
                    ],
                    "synonyms": [
                        "trwonić"
                    ],
                    "common_phrases": [
                        "rozpierdolić hajs",
                        "rozpierdolić wypłatę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бездумно потратить большую сумму денег или растратить ценные ресурсы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проебать",
                                "target_lang_sense_clarification": "потерять или растратить (вульг.)"
                            },
                            {
                                "target_lang_word": "просрать",
                                "target_lang_sense_clarification": "потратить впустую (вульг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e605e3a-ec87-3ce2-848d-a1e863cf4b0e",
                    "sense_definition": "Wywrzeć na kimś ogromne wrażenie, zszokować lub zachwycić (często w odniesieniu do sztuki lub osiągnięć).",
                    "learner_level": "C2",
                    "frequency": "Middle",
                    "semantic_group_id": "impress",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten nowy album zespołu totalnie <w>rozpierdolił</w> system swoim brzmieniem.",
                            "target_lang_translations": {
                                "ru": "Этот новый альбом группы тотально <w>разъебал</w> своим звучанием."
                            }
                        },
                        {
                            "text": "Finałowa scena tego filmu po prostu <w>rozpierdoliła</w> mi mózg.",
                            "target_lang_translations": {
                                "ru": "Финальная сцена этого фильма просто <w>вынесла</w> мне мозг."
                            }
                        }
                    ],
                    "synonyms": [
                        "zmiażdżyć"
                    ],
                    "common_phrases": [
                        "rozpierdolić system",
                        "rozpierdolić mózg"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "użycie młodzieżowe, metaforyczne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Произвести огромное впечатление, шокировать или восхитить (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разъебать",
                                "target_lang_sense_clarification": "произвести фурор (сленг, вульг.)"
                            },
                            {
                                "target_lang_word": "вынести",
                                "target_lang_sense_clarification": "шокировать (напр. вынести мозг)"
                            },
                            {
                                "target_lang_word": "порвать",
                                "target_lang_sense_clarification": "восхитить публику"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
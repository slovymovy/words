                {
                    "sense_id": "2ba5868e-19f0-38f2-8673-abaf943fc0b7",
                    "sense_definition": "Массовое добавление фальшивых бюллетеней в урны для голосования с целью подделки результатов выборов.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "election_fraud",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наблюдатели зафиксировали массовый <w>вброс</w> бюллетеней на избирательном участке.",
                            "target_lang_translations": {
                                "en": "Observers recorded mass ballot <w>stuffing</w> at the polling station."
                            }
                        },
                        {
                            "text": "Из-за подтверждённого факта <w>вброса</w> результаты голосования были аннулированы.",
                            "target_lang_translations": {
                                "en": "Due to the confirmed fact of <w>stuffing</w>, the voting results were annulled."
                            }
                        }
                    ],
                    "common_phrases": [
                        "вброс бюллетеней",
                        "массовый вброс",
                        "вброс на выборах"
                    ],
                    "target_lang_definitions": {
                        "en": "The illegal act of adding fake ballots to ballot boxes to manipulate election results."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ballot stuffing",
                                "target_lang_sense_clarification": "elections"
                            },
                            {
                                "target_lang_word": "stuffing",
                                "target_lang_sense_clarification": "short for ballot stuffing"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f918087-7cce-308a-bbb1-f81d3b04b1c7",

{
    "word_family": [
        "dzwonek",
        "dzwon",
        "zadzwonić",
        "oddzwonić",
        "przedzwonić",
        "wydzwaniać",
        "dzwonnica",
        "dzwonnik"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9c515180-e763-342a-95bc-9d571d94bf34",
                    "sense_definition": "Telefonować, próbować nawiązać połączenie telefoniczne z inną osobą lub instytucją.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <dzwonić> do mamy codziennie wieczorem.",
                            "target_lang_translations": {
                                "ru": "Я должен <звонить> маме каждый вечер."
                            }
                        },
                        {
                            "text": "Kiedy będziesz <dzwonić> w sprawie tej pracy?",
                            "target_lang_translations": {
                                "ru": "Когда ты будешь <звонить> по поводу этой работы?"
                            }
                        },
                        {
                            "text": "Mężczyzna <dzwonił> na policję, aby zgłosić wypadek.",
                            "target_lang_translations": {
                                "ru": "Мужчина <звонил> в полицию, чтобы сообщить об аварии."
                            }
                        }
                    ],
                    "synonyms": [
                        "telefonować"
                    ],
                    "common_phrases": [
                        "dzwonić do kogoś",
                        "dzwonić na policję",
                        "dzwonić po taksówkę",
                        "dzwonić w sprawie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Звонить, пытаться установить телефонную связь с другим человеком или учреждением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звонить",
                                "target_lang_sense_clarification": "по телефону"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5538b32f-703e-3190-a421-fd0f24c4d84d",
                    "sense_definition": "Wydawać głośny dźwięk (o telefonie, budziku lub dzwonku szkolnym).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "sound_emission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twój telefon <dzwoni> już od minuty, odbierz go.",
                            "target_lang_translations": {
                                "ru": "Твой телефон <звонит> уже минуту, ответь."
                            }
                        },
                        {
                            "text": "Rano budzik <dzwoni> o siódmej, ale nie chcę wstawać.",
                            "target_lang_translations": {
                                "ru": "Утром будильник <звонит> в семь, но я не хочу вставать."
                            }
                        }
                    ],
                    "common_phrases": [
                        "telefon dzwoni",
                        "budzik dzwoni",
                        "dzwonić na przerwę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать громкий звук (о телефоне, будильнике или школьном звонке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звонить",
                                "target_lang_sense_clarification": "издавать звук"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73c1d437-3d77-3600-b263-6e37570b06ef",
                    "sense_definition": "Używać dzwonka (zazwyczaj przy wejściu do domu), aby zasygnalizować swoje przybycie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "signaling",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Listonosz <dzwonił> do drzwi, ale nikogo nie było w domu.",
                            "target_lang_translations": {
                                "ru": "Почтальон <звонил> в дверь, но никого не было дома."
                            }
                        },
                        {
                            "text": "Nie lubię, gdy ktoś <dzwoni> do bramy późno w nocy.",
                            "target_lang_translations": {
                                "ru": "Я не люблю, когда кто-то <звонит> в ворота поздно ночью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dzwonić do drzwi",
                        "dzwonić dzwonkiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Использовать звонок (обычно у входа в дом), чтобы сообщить о своем приходе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звонить",
                                "target_lang_sense_clarification": "в дверь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6877006-444e-38cb-a8a1-12478bbe9508",
                    "sense_definition": "Wydawać metaliczny odgłos poprzez uderzanie o siebie twardych przedmiotów.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sound_emission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Było tak zimno, że aż zęby <dzwoniły> mi z mrozu.",
                            "target_lang_translations": {
                                "ru": "Было так холодно, что у меня даже зубы <стучали> от мороза."
                            }
                        },
                        {
                            "text": "Strażnik szedł korytarzem i głośno <dzwonił> kluczami.",
                            "target_lang_translations": {
                                "ru": "Охранник шел по коридору и громко <звенел> ключами."
                            }
                        }
                    ],
                    "synonyms": [
                        "szczękać",
                        "dryndać"
                    ],
                    "common_phrases": [
                        "dzwonić kluczami",
                        "zęby dzwonią",
                        "dzwonić monetami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Издавать металлический звук при ударе твердых предметов друг о друга."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звенеть",
                                "target_lang_sense_clarification": "издавать звон"
                            },
                            {
                                "target_lang_word": "бренчать",
                                "target_lang_sense_clarification": "ключами или монетами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c6672282-5619-3244-b836-1294e35098f2",
                    "sense_definition": "Odczuwać nieprzyjemny szum lub pisk wewnątrz uszu.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sensation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po głośnym koncercie przez godzinę <dzwoniło> mi w uszach.",
                            "target_lang_translations": {
                                "ru": "После громкого концерта у меня час <звенело> в ушах."
                            }
                        },
                        {
                            "text": "Jeśli ciągle <dzwoni> ci w głowie, powinieneś iść do lekarza.",
                            "target_lang_translations": {
                                "ru": "Если у тебя постоянно <звенит> в голове, тебе следует пойти к врачу."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dzwonić w uszach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ощущать неприятный шум или писк внутри ушей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "звенеть",
                                "target_lang_sense_clarification": "в ушах"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
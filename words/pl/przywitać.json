{
    "word_family": [
        "witać",
        "powitać",
        "powitanie",
        "zawitać"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6f78d0c0-d99b-3ebd-9e5a-5ebe8f903ce5",
                    "sense_definition": "Zwrócić się do kogoś słowami lub gestem w momencie spotkania, wyrażając radość z czyjegoś przybycia.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "greeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Gospodarz wyszedł przed dom, aby serdecznie <przywitać> gości.",
                            "target_lang_translations": {
                                "ru": "Хозяин вышел из дома, чтобы сердечно <w>встретить</w> гостей."
                            }
                        },
                        {
                            "text": "Musimy odpowiednio <przywitać> naszą nową koleżankę w biurze.",
                            "target_lang_translations": {
                                "ru": "Мы должны должным образом <w>поприветствовать</w> нашу новую коллегу в офисе."
                            }
                        }
                    ],
                    "synonyms": [
                        "powitać"
                    ],
                    "antonyms": [
                        "pożegnać"
                    ],
                    "common_phrases": [
                        "serdecznie przywitać",
                        "przywitać gości",
                        "przywitać chlebem i solą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обратиться к кому-либо с приветствием или жестом при встрече, выражая радость от чьего-либо прибытия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поприветствовать",
                                "target_lang_sense_clarification": "совершить приветствие"
                            },
                            {
                                "target_lang_word": "встретить",
                                "target_lang_sense_clarification": "принять гостей, выйти навстречу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7c4f7d0b-f57e-3017-94f8-e2e083dc9538",
                    "sense_definition": "Wymienić z kimś uścisk dłoni, słowa lub gest powitania (używane z zaimkiem \"się\").",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "greeting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pamiętaj, żeby grzecznie <przywitać> się z nauczycielem.",
                            "target_lang_translations": {
                                "ru": "Помни, что нужно вежливо <w>поздороваться</w> с учителем."
                            }
                        },
                        {
                            "text": "Marek podszedł do stolika, aby <przywitać> się z przyjaciółmi.",
                            "target_lang_translations": {
                                "ru": "Марек подошел к столику, чтобы <w>поздороваться</w> с друзьями."
                            }
                        }
                    ],
                    "synonyms": [
                        "powitać się"
                    ],
                    "antonyms": [
                        "pożegnać się"
                    ],
                    "common_phrases": [
                        "przywitać się z kimś",
                        "przywitać się skinieniem głowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обменяться рукопожатием, словами или жестом приветствия (обычно используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поздороваться",
                                "target_lang_sense_clarification": "обменяться приветствием"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "efae177b-23d0-3135-a26d-023486665b89",
                    "sense_definition": "Pojawić się przed kimś, stać się widocznym lub odczuwalnym (o zjawiskach, widokach, zapachach).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po wyjściu z pociągu <przywitał> nas mroźny, zimowy wiatr.",
                            "target_lang_translations": {
                                "ru": "После выхода из поезда нас <w>встретил</w> морозный зимний ветер."
                            }
                        },
                        {
                            "text": "Piękny wschód słońca <przywitał> turystów na górskim szczycie.",
                            "target_lang_translations": {
                                "ru": "Красивый восход солнца <w>поприветствовал</w> туристов на горной вершине."
                            }
                        }
                    ],
                    "common_phrases": [
                        "przywitać nowy dzień",
                        "przywitać nowy rok"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предстать перед кем-либо, стать видимым или ощутимым (о явлениях, видах, запахах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "встретить",
                                "target_lang_sense_clarification": "попасться навстречу (о явлениях)"
                            },
                            {
                                "target_lang_word": "приветствовать",
                                "target_lang_sense_clarification": "литературное, олицетворение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
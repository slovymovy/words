{
    "word_family": [
        "wel",
        "eens"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "5fa38f63-1f5c-3399-a938-09f7361e43ae",
                    "sense_definition": "Minstens één keer; op enig moment in het verleden of de toekomst.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "frequency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De meeste Nederlanders zijn <weleens> in Amsterdam geweest.",
                            "target_lang_translations": {
                                "en": "Most Dutch people have been to Amsterdam <at some point>.",
                                "ru": "Большинство голландцев <w>хоть раз</w> бывали в Амстердаме."
                            }
                        },
                        {
                            "text": "Heb jij <weleens> een prijs gewonnen bij de loterij?",
                            "target_lang_translations": {
                                "en": "Have you <ever> won a prize in the lottery?",
                                "ru": "Ты <w>когда-нибудь</w> выигрывал приз в лотерею?"
                            }
                        }
                    ],
                    "synonyms": [
                        "ooit",
                        "een keertje"
                    ],
                    "common_phrases": [
                        "heb je weleens",
                        "ben je weleens"
                    ],
                    "target_lang_definitions": {
                        "en": "At least once; at some point in the past or future.",
                        "ru": "По крайней мере один раз; в какой-то момент в прошлом или будущем."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "ever",
                                "target_lang_sense_clarification": "used in questions or negatives regarding experience"
                            },
                            {
                                "target_lang_word": "at some point",
                                "target_lang_sense_clarification": "at an unspecified time"
                            },
                            {
                                "target_lang_word": "once",
                                "target_lang_sense_clarification": "on at least one occasion"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "когда-нибудь",
                                "target_lang_sense_clarification": "используется в вопросах или утверждениях о неопределенном времени"
                            },
                            {
                                "target_lang_word": "хоть раз",
                                "target_lang_sense_clarification": "подчеркивает, что действие произошло минимум однажды"
                            },
                            {
                                "target_lang_word": "когда-либо",
                                "target_lang_sense_clarification": "более формальный вариант для вопросов или отрицаний"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09d0a801-b1ee-30de-b780-53e468f1f57d",
                    "sense_definition": "Af en toe; meer dan eens, maar niet heel vaak.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "frequency",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op zondag ging hij <weleens> wandelen met zijn vrouw.",
                            "target_lang_translations": {
                                "en": "On Sundays he <sometimes> went for a walk with his wife.",
                                "ru": "По воскресеньям он <w>иногда</w> ходил гулять с женой."
                            }
                        },
                        {
                            "text": "Ik moet <weleens> terugdenken aan mijn oude schooltijd.",
                            "target_lang_translations": {
                                "en": "I <occasionally> think back to my old school days.",
                                "ru": "Я <w>время от времени</w> вспоминаю свои школьные годы."
                            }
                        }
                    ],
                    "synonyms": [
                        "soms",
                        "nu en dan"
                    ],
                    "common_phrases": [
                        "komt weleens voor",
                        "ik ga weleens"
                    ],
                    "target_lang_definitions": {
                        "en": "Now and then; more than once, but not very often.",
                        "ru": "Время от времени; более одного раза, но не очень часто."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sometimes",
                                "target_lang_sense_clarification": "occasionally"
                            },
                            {
                                "target_lang_word": "occasionally",
                                "target_lang_sense_clarification": "now and then"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "иногда",
                                "target_lang_sense_clarification": "случается время от времени"
                            },
                            {
                                "target_lang_word": "время от времени",
                                "target_lang_sense_clarification": "периодически, нерегулярно"
                            },
                            {
                                "target_lang_word": "бывало",
                                "target_lang_sense_clarification": "используется при описании повторяющихся действий в прошлом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6068afad-690b-346e-8942-5fa782a9d4fb",
                    "sense_definition": "Gebruikt als versterking van een wens, vermoeden of verwijt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kun jij op je handen lopen? Dat wil ik <weleens> zien.",
                            "target_lang_translations": {
                                "en": "Can you walk on your hands? I would <really> like to see that.",
                                "ru": "Ты умеешь ходить на руках? Я бы <w>очень</w> хотел на это посмотреть."
                            }
                        },
                        {
                            "text": "Hij kan er <weleens> spijt van krijgen dat hij verhuisd is.",
                            "target_lang_translations": {
                                "en": "He might <well> regret having moved.",
                                "ru": "Он <w>вполне</w> может пожалеть о том, что переехал."
                            }
                        }
                    ],
                    "synonyms": [
                        "echt",
                        "heus"
                    ],
                    "common_phrases": [
                        "dat wil ik weleens",
                        "zou weleens kunnen"
                    ],
                    "target_lang_definitions": {
                        "en": "Used to reinforce a wish, suspicion, or reproach.",
                        "ru": "Используется для усиления желания, предположения или упрека."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "well",
                                "target_lang_sense_clarification": "used with modals like 'might' or 'could' to express probability"
                            },
                            {
                                "target_lang_word": "really",
                                "target_lang_sense_clarification": "used to emphasize a desire or skepticism"
                            },
                            {
                                "target_lang_word": "just",
                                "target_lang_sense_clarification": "used to emphasize a wish to see something happen"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "вполне",
                                "target_lang_sense_clarification": "используется для выражения высокой вероятности"
                            },
                            {
                                "target_lang_word": "очень",
                                "target_lang_sense_clarification": "используется для усиления желания что-то сделать"
                            },
                            {
                                "target_lang_word": "пожалуй",
                                "target_lang_sense_clarification": "выражает допущение или вероятность"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cbb71912-f75a-3df2-803c-13d3ead16bce",
                    "sense_definition": "Joined by treaty, agreement, or common cause.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "allied_union",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two countries became <w>allied</w> to defend their borders against the invasion.",
                            "target_lang_translations": {
                                "ru": "Две страны <w>объединились</w>, чтобы защитить свои границы от вторжения."
                            }
                        },
                        {
                            "text": "They are <w>allied</w> in their determination to reduce global pollution levels.",
                            "target_lang_translations": {
                                "ru": "Они <w>едины</w> в своей решимости снизить уровень глобального загрязнения."
                            }
                        }
                    ],
                    "synonyms": [
                        "united",
                        "joined",
                        "confederated",
                        "associated"
                    ],
                    "antonyms": [
                        "hostile",
                        "enemy",
                        "opposed"
                    ],
                    "common_phrases": [
                        "allied forces",
                        "allied nations",
                        "closely allied"
                    ],
                    "target_lang_definitions": {
                        "ru": "Объединенный договором, соглашением или общей целью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сою́зный",
                                "target_lang_sense_clarification": "связанный союзом"
                            },
                            {
                                "target_lang_word": "объединённый",
                                "target_lang_sense_clarification": "сплоченный общей целью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04a74c48-3897-3837-b4e9-a017149e69e1",
                    "sense_definition": "Relating to the group of nations that fought together against the Central Powers in World War I or the Axis Powers in World War II.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "allied_union",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>Allied</w> troops landed on the beaches of Normandy on D-Day.",
                            "target_lang_translations": {
                                "ru": "<w>Союзные</w> войска высадились на пляжах Нормандии в день \"Д\"."
                            }
                        },
                        {
                            "text": "My grandfather served in the <w>Allied</w> army during the Second World War.",
                            "target_lang_translations": {
                                "ru": "Мой дед служил в <w>союзной</w> армии во время Второй мировой войны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Allied Powers",
                        "Allied invasion",
                        "Allied victory"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к группе стран, сражавшихся вместе против Центральных держав в Первой мировой войне или стран Оси во Второй мировой войне."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сою́зный",
                                "target_lang_sense_clarification": "относящийся к войскам союзников (антигитлеровской коалиции или Антанты)"
                            },
                            {
                                "target_lang_word": "сою́знический",
                                "target_lang_sense_clarification": "принадлежащий союзникам"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed5ee12f-b003-3de1-9596-772980c7af15",
                    "sense_definition": "Connected by nature, qualities, or origin; related.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is studying medicine and <w>allied</w> sciences at the university.",
                            "target_lang_translations": {
                                "ru": "Она изучает медицину и <w>смежные</w> науки в университете."
                            }
                        },
                        {
                            "text": "The museum exhibition features pottery and <w>allied</w> arts from the region.",
                            "target_lang_translations": {
                                "ru": "На выставке в музее представлены керамика и <w>родственные</w> виды искусства этого региона."
                            }
                        }
                    ],
                    "synonyms": [
                        "related",
                        "connected",
                        "kindred",
                        "associated"
                    ],
                    "antonyms": [
                        "unrelated",
                        "distinct"
                    ],
                    "common_phrases": [
                        "allied health",
                        "allied subjects",
                        "allied trades"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связанный по природе, качествам или происхождению; родственный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сме́жный",
                                "target_lang_sense_clarification": "находящийся в непосредственной связи, соседний (о науках, профессиях)"
                            },
                            {
                                "target_lang_word": "ро́дственный",
                                "target_lang_sense_clarification": "близкий по происхождению или характеру"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b26a63aa-1634-38e1-9fdd-98560d95d234",

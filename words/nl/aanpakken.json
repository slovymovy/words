{
    "word_family": [
        "aanpak",
        "aanpakker"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "72e883df-1b73-30b5-be5b-5033fabf36cf",
                    "sense_definition": "Iets met de handen aannemen dat iemand je geeft.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wil je deze zware doos even van mij <w>aanpakken</w>?",
                            "target_lang_translations": {
                                "en": "Could you <w>take</w> this heavy box from me for a moment?",
                                "ru": "Ты не мог бы <w>взять</w> у меня эту тяжелую коробку?"
                            }
                        },
                        {
                            "text": "Hij <w>pakte</w> het cadeau met een grote glimlach <w>aan</w>.",
                            "target_lang_translations": {
                                "en": "He <w>accepted</w> the gift with a big smile.",
                                "ru": "Он с широкой улыбкой <w>принял</w> подарок."
                            }
                        }
                    ],
                    "synonyms": [
                        "aannemen",
                        "overnemen"
                    ],
                    "antonyms": [
                        "lanterfanten"
                    ],
                    "common_phrases": [
                        "iets van iemand aanpakken"
                    ],
                    "target_lang_definitions": {
                        "en": "To take something into one's hands that is being offered or passed to you.",
                        "ru": "Взять или принять что-то из рук другого человека."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "take",
                                "target_lang_sense_clarification": "to receive something into one's hands"
                            },
                            {
                                "target_lang_word": "accept",
                                "target_lang_sense_clarification": "to take something that is offered, like a gift"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "брать",
                                "target_lang_sense_clarification": "взять в руки"
                            },
                            {
                                "target_lang_word": "принимать",
                                "target_lang_sense_clarification": "получать от кого-то"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91b792c0-6b67-3e91-b00c-02179bfdf02e",
                    "sense_definition": "Beginnen aan een taak of probleem om het op te lossen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten dit probleem zo snel mogelijk <w>aanpakken</w>.",
                            "target_lang_translations": {
                                "en": "We need to <w>tackle</w> this problem as soon as possible.",
                                "ru": "Мы должны <w>заняться</w> этой проблемой как можно скорее."
                            }
                        },
                        {
                            "text": "De nieuwe directeur <w>pakt</w> de zaken heel anders <w>aan</w>.",
                            "target_lang_translations": {
                                "en": "The new director <w>handles</w> things very differently.",
                                "ru": "Новый директор <w>подходит</w> к делам совершенно иначе."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanvatten",
                        "beginnen"
                    ],
                    "antonyms": [
                        "lanterfanten"
                    ],
                    "common_phrases": [
                        "een probleem aanpakken",
                        "het groots aanpakken",
                        "de zaken aanpakken"
                    ],
                    "target_lang_definitions": {
                        "en": "To start working on a task or problem with determination in order to solve it.",
                        "ru": "Приступить к выполнению задачи или решению проблемы."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tackle",
                                "target_lang_sense_clarification": "to make a determined effort to deal with a problem"
                            },
                            {
                                "target_lang_word": "deal with",
                                "target_lang_sense_clarification": "to take action to solve a problem or handle a situation"
                            },
                            {
                                "target_lang_word": "address",
                                "target_lang_sense_clarification": "to give attention to or deal with a matter"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "браться за",
                                "target_lang_sense_clarification": "начинать дело"
                            },
                            {
                                "target_lang_word": "подходить к",
                                "target_lang_sense_clarification": "иметь метод решения"
                            },
                            {
                                "target_lang_word": "решать",
                                "target_lang_sense_clarification": "проблему или задачу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6fee4a3-0b25-33ea-8a54-99a54693cc69",
                    "sense_definition": "Streng optreden tegen iemand of iets om ongewenst gedrag te stoppen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "punitive_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De politie gaat mensen die te hard rijden strenger <w>aanpakken</w>.",
                            "target_lang_translations": {
                                "en": "The police are going to <w>deal</w> more strictly <w>with</w> people who speed.",
                                "ru": "Полиция будет строже <w>наказывать</w> тех, кто превышает скорость."
                            }
                        },
                        {
                            "text": "De fraudeurs werden hard <w>aangepakt</w> door de rechter.",
                            "target_lang_translations": {
                                "en": "The fraudsters were <w>dealt with</w> harshly by the judge.",
                                "ru": "Судья сурово <w>обошелся</w> с мошенниками."
                            }
                        }
                    ],
                    "synonyms": [
                        "bestraffen",
                        "vervolgen"
                    ],
                    "common_phrases": [
                        "iemand hard aanpakken",
                        "streng aanpakken"
                    ],
                    "target_lang_definitions": {
                        "en": "To take strict action against someone or something to stop unwanted behavior.",
                        "ru": "Принять строгие меры против кого-либо или чего-либо для прекращения нежелательного поведения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "deal with",
                                "target_lang_sense_clarification": "to handle someone, often involving punishment or strict measures"
                            },
                            {
                                "target_lang_word": "crack down on",
                                "target_lang_sense_clarification": "to take severe measures against someone or something"
                            },
                            {
                                "target_lang_word": "handle",
                                "target_lang_sense_clarification": "to manage a situation or person, often firmly"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "разбираться с",
                                "target_lang_sense_clarification": "принимать меры"
                            },
                            {
                                "target_lang_word": "наказывать",
                                "target_lang_sense_clarification": "карать"
                            }
                        ]
                    }
                }
            ]
        },
        {

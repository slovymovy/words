    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f3272461-07ec-3c84-a950-0859573acf9b",
                    "sense_definition": "Часть носовой оконечности судна, которая рассекает воду при движении.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "nautical_term",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Корабль плавно рассекал волны своим острым <w>водорезом</w>.",
                            "target_lang_translations": {
                                "en": "The ship smoothly cut through the waves with its sharp <w>cutwater</w>."
                            }
                        },
                        {
                            "text": "Удар встречной волны пришёлся прямо в <w>водорез</w> яхты.",
                            "target_lang_translations": {
                                "en": "The impact of the oncoming wave struck right against the yacht's <w>cutwater</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "острый водорез",
                        "водорез судна"
                    ],
                    "target_lang_definitions": {
                        "en": "The part of a ship's bow that cuts through the water while in motion."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cutwater",
                                "target_lang_sense_clarification": "nautical term for the forward edge of the stem"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "139cddb6-ffb4-3b8b-aa93-d373e4e79e29",
                    "sense_definition": "Морская птица семейства чайковых с неравным клювом, летающая низко над водой для ловли рыбы.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "bird_name",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Над поверхностью реки пролетел быстрый <w>водорез</w> в поисках добычи.",
                            "target_lang_translations": {
                                "en": "A fast <w>skimmer</w> flew over the river surface in search of prey."
                            }
                        },
                        {
                            "text": "Африканские <w>водорезы</w> гнездятся большими колониями на песчаных отмелях.",
                            "target_lang_translations": {
                                "en": "African <w>skimmers</w> nest in large colonies on sandy shoals."
                            }
                        }
                    ],
                    "synonyms": [
                        "ножеклюв"
                    ],
                    "common_phrases": [
                        "чёрный водорез",
                        "стая водорезов"
                    ],
                    "target_lang_definitions": {
                        "en": "A seabird of the gull family with an uneven beak that flies low over the water to catch fish."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "skimmer",
                                "target_lang_sense_clarification": "bird of the genus Rynchops"
                            },
                            {
                                "target_lang_word": "cutwater",
                                "target_lang_sense_clarification": "synonym for the skimmer bird"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a1185888-807a-3198-a886-0413fb743825",
                    "sense_definition": "Водное растение с жёсткими пилообразными листьями, плавающее на поверхности или погружённое в воду.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "plant_name",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В стоячей воде озера густо разросся колючий <w>водорез</w>.",
                            "target_lang_translations": {
                                "en": "Prickly <w>water soldier</w> grew thickly in the stagnant water of the lake."
                            }
                        },
                        {
                            "text": "Листья <w>водореза</w> могут легко поранить кожу купальщика.",
                            "target_lang_translations": {
                                "en": "The leaves of the <w>water soldier</w> can easily injure a swimmer's skin."
                            }
                        }
                    ],
                    "synonyms": [
                        "телорез"
                    ],
                    "common_phrases": [
                        "заросли водореза",
                        "листья водореза"
                    ],
                    "target_lang_definitions": {
                        "en": "An aquatic plant with stiff saw-like leaves that floats on the surface or is submerged in water."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "water soldier",
                                "target_lang_sense_clarification": "aquatic plant Stratiotes aloides"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
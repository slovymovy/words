                {
                    "sense_id": "9e982188-7e6a-3789-848f-b68e7a7d80c5",
                    "sense_definition": "Iets strak trekken of vastzetten zodat er druk op komt te staan en het niet meer slap hangt.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "tighten_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij <w>spande</w> een paar waslijnen tussen de bomen om de kleren te drogen.",
                            "target_lang_translations": {
                                "en": "He <w>stretched</w> a few clotheslines between the trees to dry the clothes.",
                                "ru": "Он <w>натянул</w> пару бельевых веревок между деревьями, чтобы высушить одежду."
                            }
                        },
                        {
                            "text": "De boogschutter <w>spande</w> zijn boog en mikte zorgvuldig op het doel.",
                            "target_lang_translations": {
                                "en": "The archer <w>drew</w> his bow and aimed carefully at the target.",
                                "ru": "Лучник <w>натянул</w> свой лук и тщательно прицелился в мишень."
                            }
                        }
                    ],
                    "synonyms": [
                        "strakrekken",
                        "aanspannen"
                    ],
                    "antonyms": [
                        "vieren",
                        "ontspannen",
                        "losmaken"
                    ],
                    "common_phrases": [
                        "de kroon spannen",
                        "de boog spannen",
                        "een draad spannen"
                    ],
                    "target_lang_definitions": {
                        "en": "To pull something tight or fasten it so that it is under tension and no longer hangs slack.",
                        "ru": "Натягивать что-либо туго или закреплять так, чтобы оно было под напряжением и не провисало."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "tighten",
                                "target_lang_sense_clarification": "to make tight or taut"
                            },
                            {
                                "target_lang_word": "stretch",
                                "target_lang_sense_clarification": "to pull something (like a rope) to its full length"
                            },
                            {
                                "target_lang_word": "draw",
                                "target_lang_sense_clarification": "to pull back a bowstring"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "натягивать",
                                "target_lang_sense_clarification": "делать тугим (веревку, ткань)"

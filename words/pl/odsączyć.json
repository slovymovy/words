                        "odsączyć na sitku",
                        "odsączyć wodę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отделить жидкость от твердого вещества, например, используя сито, дуршлаг или впитывающий материал."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отцедить",
                                "target_lang_sense_clarification": "удалить жидкость через фильтр или сито"
                            },
                            {
                                "target_lang_word": "обсушить",
                                "target_lang_sense_clarification": "удалить влагу впитывающим материалом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "787b5190-9c02-30f8-b768-4b88f33742fd",
                    "sense_definition": "Pozbyć się nadmiaru cieczy poprzez jej samoistne ściekanie (używane z zaimkiem „się”).",

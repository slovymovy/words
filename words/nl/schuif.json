    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d944a55a-ea67-3d10-95ba-e1f592506896",
                    "sense_definition": "Een uitschuifbare bak in een meubelstuk, zoals een kast of bureau, om spullen in op te bergen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "storage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kleren lagen netjes opgevouwen in de bovenste <w>schuif</w>.",
                            "target_lang_translations": {
                                "en": "The clothes lay neatly folded in the top <w>drawer</w>.",
                                "ru": "Одежда лежала аккуратно сложенная в верхнем <w>ящике</w>."
                            }
                        },
                        {
                            "text": "Leg de pennen en potloden maar in de <w>schuif</w> van het bureau.",
                            "target_lang_translations": {
                                "en": "Just put the pens and pencils in the desk <w>drawer</w>.",
                                "ru": "Просто положи ручки и карандаши в <w>ящик</w> стола."
                            }
                        }
                    ],
                    "synonyms": [
                        "la"
                    ],
                    "common_phrases": [
                        "in de schuif",
                        "de onderste schuif"
                    ],
                    "target_lang_definitions": {
                        "en": "A sliding box in a piece of furniture, such as a cupboard or desk, for storing items.",
                        "ru": "Выдвижной ящик в предмете мебели, например, в шкафу или письменном столе, для хранения вещей."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drawer",
                                "target_lang_sense_clarification": "storage compartment in furniture"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ящик",
                                "target_lang_sense_clarification": "отделение в мебели"
                            },
                            {
                                "target_lang_word": "выдвижной ящик",
                                "target_lang_sense_clarification": "полная форма"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f7709350-0bd3-3204-8a79-7b97a657303d",
                    "sense_definition": "Een beweegbaar metalen stangetje waarmee men een deur of raam kan vergrendelen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "lock",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Om de deur te openen moet je eerst de <w>schuif</w> eraf halen.",
                            "target_lang_translations": {
                                "en": "To open the door, you first have to take the <w>bolt</w> off.",
                                "ru": "Чтобы открыть дверь, нужно сначала отодвинуть <w>засов</w>."
                            }
                        },
                        {
                            "text": "Doe de <w>schuif</w> op de achterdeur voordat je naar bed gaat.",
                            "target_lang_translations": {
                                "en": "Put the <w>bolt</w> on the back door before you go to bed.",
                                "ru": "Закрой <w>задвижку</w> на задней двери, перед тем как лечь спать."
                            }
                        }
                    ],
                    "synonyms": [
                        "grendel"
                    ],
                    "common_phrases": [
                        "de schuif erop doen",
                        "van de schuif halen"
                    ],
                    "target_lang_definitions": {
                        "en": "A movable metal bar used to lock a door or window.",
                        "ru": "Подвижный металлический стержень, используемый для запирания двери или окна."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bolt",
                                "target_lang_sense_clarification": "sliding locking mechanism"
                            },
                            {
                                "target_lang_word": "latch",
                                "target_lang_sense_clarification": "door fastener"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "засов",
                                "target_lang_sense_clarification": "запирающий механизм"
                            },
                            {
                                "target_lang_word": "щеколда",
                                "target_lang_sense_clarification": "дверная задвижка"
                            },
                            {
                                "target_lang_word": "задвижка",
                                "target_lang_sense_clarification": "механизм запирания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89bc785e-43c0-3b29-9bb2-729dd6ff99de",
                    "sense_definition": "Een mechanisme of onderdeel dat heen en weer geschoven kan worden om iets te bedienen of te regelen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mechanism",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Met deze <w>schuif</w> regel je de toevoer van frisse lucht in de kamer.",
                            "target_lang_translations": {
                                "en": "With this <w>slider</w> you regulate the supply of fresh air into the room.",
                                "ru": "С помощью этой <w>заслонки</w> вы регулируете приток свежего воздуха в комнату."
                            }
                        },
                        {
                            "text": "De <w>schuif</w> op het mengpaneel stond helemaal open voor maximaal volume.",
                            "target_lang_translations": {
                                "en": "The <w>fader</w> on the mixing console was fully open for maximum volume.",
                                "ru": "<w>Ползунок</w> на микшерном пульте был поднят до упора для максимальной громкости."
                            }
                        }
                    ],
                    "common_phrases": [
                        "de schuif openzetten",
                        "aan de schuiven zitten"
                    ],
                    "target_lang_definitions": {
                        "en": "A mechanism or part that can be slid back and forth to operate or regulate something.",
                        "ru": "Механизм или деталь, которую можно сдвигать вперед и назад для управления или регулировки чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "slider",
                                "target_lang_sense_clarification": "control mechanism"
                            },
                            {
                                "target_lang_word": "fader",
                                "target_lang_sense_clarification": "audio volume control"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "заслонка",
                                "target_lang_sense_clarification": "для воздуха или жидкости"
                            },
                            {
                                "target_lang_word": "ползунок",
                                "target_lang_sense_clarification": "элемент управления (напр. громкостью)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6630a550-f6bf-3620-83f1-6a4a96507d9d",
                    "sense_definition": "De mond.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "body_part_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Houd je <w>schuif</w> eens even dicht als ik aan het praten ben.",
                            "target_lang_translations": {
                                "en": "Keep your <w>trap</w> shut for a moment while I am talking.",
                                "ru": "А ну-ка прикрой <w>варежку</w>, пока я говорю."
                            }
                        },
                        {
                            "text": "Hij stopte snel een groot stuk brood in zijn <w>schuif</w>.",
                            "target_lang_translations": {
                                "en": "He quickly stuffed a large piece of bread into his <w>gob</w>.",
                                "ru": "Он быстро запихнул большой кусок хлеба в свою <w>пасть</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "houd je schuif"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "informeel"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The mouth (slang).",
                        "ru": "Рот (сленг)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "trap",
                                "target_lang_sense_clarification": "slang for mouth"
                            },
                            {
                                "target_lang_word": "gob",
                                "target_lang_sense_clarification": "slang for mouth"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "пасть",
                                "target_lang_sense_clarification": "грубое название рта"
                            },
                            {
                                "target_lang_word": "варежка",
                                "target_lang_sense_clarification": "в выражении 'закрой варежку'"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2abd028b-5fbe-33f7-be43-e2799afefbf6",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>schuiven</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>schuif</w> mijn stoel wat dichter bij de tafel.",
                            "target_lang_translations": {
                                "en": "I <w>slide</w> my chair a bit closer to the table.",
                                "ru": "Я <w>пододвигаю</w> свой стул немного ближе к столу."
                            }
                        },
                        {
                            "text": "Elke avond <w>schuif</w> ik de gordijnen dicht voor wat privacy.",
                            "target_lang_translations": {
                                "en": "Every evening I <w>draw</w> the curtains for some privacy.",
                                "ru": "Каждый вечер я <w>задергиваю</w> шторы для уединения."
                            }
                        }
                    ],
                    "traits": [

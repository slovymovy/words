            "senses": [
                {
                    "sense_id": "759dd147-5857-38ca-85f9-ee84a5142fb3",
                    "sense_definition": "Stan silnej koncentracji uwagi i myśli na określonym obiekcie, czynności lub problemie.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "mental_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To trudne zadanie wymaga od pracownika pełnego <skupienia>.",
                            "target_lang_translations": {
                                "ru": "Эта трудная задача требует от работника полной <сосредоточенности>."
                            }
                        },
                        {
                            "text": "Wszyscy słuchali wykładu profesora w głębokim <skupieniu>.",
                            "target_lang_translations": {
                                "ru": "Все слушали лекцию профессора в глубоком <сосредоточении>."
                            }
                        }
                    ],
                    "synonyms": [
                        "koncentracja"
                    ],
                    "antonyms": [
                        "dekoncentracja",
                        "nieuwaga"
                    ],
                    "common_phrases": [
                        "w głębokim skupieniu",
                        "wymagać skupienia",
                        "chwila skupienia",
                        "brak skupienia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние сильной концентрации внимания и мыслей на определённом объекте, действии или проблеме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сосредоточенность",
                                "target_lang_sense_clarification": "состояние глубокого внимания"
                            },
                            {
                                "target_lang_word": "концентрация",
                                "target_lang_sense_clarification": "напряжение внимания"
                            },
                            {
                                "target_lang_word": "сосредоточение",
                                "target_lang_sense_clarification": "процесс или состояние фокусировки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ad53044-e29d-310f-8373-747a53c6c1f8",
                    "sense_definition": "Fizyczna postać, w jakiej występuje materia, zależna od układu cząsteczek (np. ciało stałe, ciecz, gaz).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Woda zmienia stan <skupienia> na gazowy w temperaturze stu stopni Celsjusza.",
                            "target_lang_translations": {
                                "ru": "Вода меняет <агрегатное состояние> на газообразное при температуре сто градусов Цельсия."
                            }
                        },
                        {
                            "text": "Lód to woda w stałym stanie <skupienia>.",
                            "target_lang_translations": {
                                "ru": "Лёд — это вода в твёрдом <агрегатном состоянии>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stan skupienia",
                        "zmiana stanu skupienia",
                        "stały stan skupienia",
                        "gazowy stan skupienia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическая форма существования материи, зависящая от расположения молекул (например, твёрдое тело, жидкость, газ)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "агрегатное состояние",
                                "target_lang_sense_clarification": "физический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dec2eda8-ae95-3434-ad31-acacff6f1c4b",
                    "sense_definition": "Duża liczba osób, przedmiotów lub zjawisk zgromadzonych blisko siebie w jednym miejscu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "accumulation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W centrum miasta występuje duże <skupienie> zabytkowych budynków.",
                            "target_lang_translations": {
                                "ru": "В центре города наблюдается большое <скопление> исторических зданий."
                            }
                        },
                        {
                            "text": "To <skupienie> gwiazd jest doskonale widoczne na nocnym niebie.",
                            "target_lang_translations": {
                                "ru": "Это <скопление> звёзд прекрасно видно на ночном небе."
                            }
                        }
                    ],
                    "synonyms": [
                        "skupisko"
                    ],
                    "common_phrases": [
                        "skupienie ludności",
                        "duże skupienie",
                        "skupienie wojsk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большое количество людей, предметов или явлений, собранных близко друг к другу в одном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скопление",
                                "target_lang_sense_clarification": "группа объектов или людей"
                            },
                            {
                                "target_lang_word": "концентрация",
                                "target_lang_sense_clarification": "плотность размещения"
                            }
                        ]
                    }
                }
            ]
        }

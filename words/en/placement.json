    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1b378bec-4945-392a-8b57-c46f486ebf88",
                    "sense_definition": "The act of putting something in a particular place or position.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "positioning_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The careful <w>placement</w> of the speakers improved the sound quality in the room.",
                            "target_lang_translations": {
                                "ru": "Тщательное <w>размещение</w> колонок улучшило качество звука в комнате."
                            }
                        },
                        {
                            "text": "Product <w>placement</w> in movies has become a major advertising strategy.",
                            "target_lang_translations": {
                                "ru": "<w>Размещение</w> продукции в фильмах стало главной рекламной стратегией."
                            }
                        }
                    ],
                    "synonyms": [
                        "positioning",
                        "arrangement",
                        "location"
                    ],
                    "antonyms": [
                        "removal",
                        "displacement"
                    ],
                    "common_phrases": [
                        "product placement",
                        "strategic placement",
                        "careful placement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по размещению чего-либо в определенном месте или положении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "размещение",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "расстановка",
                                "target_lang_sense_clarification": "порядок расположения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63dcc550-33ad-3bc7-a0e4-f087c12d918e",
                    "sense_definition": "The process of finding a suitable job, home, or course of study for someone.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "assignment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The university has a high rate of job <w>placement</w> for its graduates.",
                            "target_lang_translations": {
                                "ru": "Университет имеет высокий показатель <w>трудоустройства</w> своих выпускников."
                            }
                        },
                        {
                            "text": "She took a <w>placement</w> test to determine which class level was right for her.",
                            "target_lang_translations": {
                                "ru": "Она прошла тест на <w>распределение</w>, чтобы определить, какой уровень класса ей подходит."
                            }
                        }
                    ],
                    "synonyms": [
                        "assignment",
                        "appointment"
                    ],
                    "common_phrases": [
                        "job placement",
                        "placement test",
                        "placement agency"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс поиска подходящей работы, жилья или курса обучения для кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трудоустройство",
                                "target_lang_sense_clarification": "поиск работы"
                            },
                            {
                                "target_lang_word": "распределение",
                                "target_lang_sense_clarification": "назначение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83aa09ae-3334-309d-a139-718e92868045",
                    "sense_definition": "A temporary job or role undertaken by a student or trainee to gain work experience.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "assignment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "During her work <w>placement</w>, she learned valuable skills for her future career.",
                            "target_lang_translations": {
                                "ru": "Во время своей трудовой <w>стажировки</w> она освоила ценные навыки для будущей карьеры."
                            }
                        },
                        {
                            "text": "He is currently doing a six-month <w>placement</w> at a law firm in London.",
                            "target_lang_translations": {
                                "ru": "В настоящее время он проходит шестимесячную <w>стажировку</w> в юридической фирме в Лондоне."
                            }
                        }
                    ],
                    "synonyms": [
                        "internship",
                        "work experience"
                    ],
                    "common_phrases": [
                        "work placement",
                        "clinical placement",
                        "on placement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Временная работа или роль, выполняемая студентом или стажером для получения опыта работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стажировка",
                                "target_lang_sense_clarification": "обучение на работе"
                            },
                            {
                                "target_lang_word": "практика",
                                "target_lang_sense_clarification": "получение опыта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa574174-b5a7-3e88-a19e-7f09ab554f95",
                    "sense_definition": "A specific location or position where something is situated.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "positioning_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>placement</w> of the new building blocks the view from my window.",
                            "target_lang_translations": {
                                "ru": "<w>Расположение</w> нового здания закрывает вид из моего окна."
                            }
                        },
                        {
                            "text": "We discussed the <w>placement</w> of the furniture to maximize the available space.",
                            "target_lang_translations": {
                                "ru": "Мы обсудили <w>расстановку</w> мебели, чтобы максимально использовать имеющееся пространство."
                            }
                        }
                    ],
                    "synonyms": [
                        "position",
                        "location",
                        "spot"
                    ],
                    "common_phrases": [
                        "exact placement",
                        "correct placement"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретное местоположение или позиция, где что-то находится."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расположение",
                                "target_lang_sense_clarification": "местонахождение"
                            },
                            {
                                "target_lang_word": "размещение",
                                "target_lang_sense_clarification": "позиция"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a96f4448-c004-3e98-ae8a-55c34cee79c6",
                    "sense_definition": "The position of a celestial body in a specific region of the sky at a particular time.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Astrologers analyze the <w>placement</w> of planets to interpret a birth chart.",
                            "target_lang_translations": {
                                "ru": "Астрологи анализируют <w>положение</w> планет для толкования натальной карты."
                            }
                        },
                        {
                            "text": "The <w>placement</w> of the moon is believed by some to influence daily events.",
                            "target_lang_translations": {
                                "ru": "Некоторые считают, что <w>положение</w> Луны влияет на повседневные события."
                            }
                        }
                    ],
                    "synonyms": [
                        "alignment",
                        "position"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ef6089ea-455a-35f9-890b-2c95d1bd6771",
                    "sense_definition": "To understand or grasp the meaning of something fully.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I simply cannot <w>comprehend</w> why he would make such a decision.",
                            "target_lang_translations": {
                                "ru": "Я просто не могу <w>понять</w>, почему он принял такое решение."
                            }
                        },
                        {
                            "text": "It is difficult to fully <w>comprehend</w> the scale of the universe.",
                            "target_lang_translations": {
                                "ru": "Трудно полностью <w>постичь</w> масштабы Вселенной."
                            }
                        }
                    ],
                    "synonyms": [
                        "see"
                    ],
                    "antonyms": [
                        "misunderstand"
                    ],
                    "common_phrases": [
                        "fail to comprehend",
                        "fully comprehend",
                        "hard to comprehend"
                    ],
                    "target_lang_definitions": {
                        "ru": "Понимать или полностью осознавать смысл чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "понимать",
                                "target_lang_sense_clarification": "воспринимать смысл"
                            },
                            {
                                "target_lang_word": "постигать",
                                "target_lang_sense_clarification": "понимать глубоко или с трудом"
                            },
                            {
                                "target_lang_word": "осознавать",
                                "target_lang_sense_clarification": "полностью отдавать себе отчет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3719d957-de61-34e5-bfa5-b4b0e26104ad",
                    "sense_definition": "To include or contain something as part of a whole.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The final report <w>comprehends</w> all the findings from the recent survey.",
                            "target_lang_translations": {
                                "ru": "Итоговый отчет <w>охватывает</w> все результаты недавнего опроса."
                            }
                        },
                        {
                            "text": "His vast empire <w>comprehended</w> many different nations and cultures.",
                            "target_lang_translations": {
                                "ru": "Его огромная империя <w>включала</w> в себя множество различных народов и культур."
                            }
                        }
                    ],
                    "antonyms": [
                        "exclude"
                    ],
                    "target_lang_definitions": {
                        "ru": "Включать в себя или содержать как часть целого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "охватывать",
                                "target_lang_sense_clarification": "включать в свой состав или пределы"
                            },
                            {
                                "target_lang_word": "включать",
                                "target_lang_sense_clarification": "содержать в себе"
                            }
                        ]
                    }
                }
            ]
        }
    ]

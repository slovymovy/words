                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Increasing the <w>leading</w> makes the paragraph easier to read.",
                            "target_lang_translations": {
                                "ru": "Увеличение <w>интерлиньяжа</w> делает абзац более удобным для чтения."
                            }

            "senses": [
                {
                    "sense_id": "59efa3f6-1f9e-34cd-8e2f-3777bfcbc44f",
                    "sense_definition": "Dotyczący procentów, wyrażony w procentach lub odnoszący się do obliczania części setnych całości.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "math_stat",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wzrost <w>procentowy</w> sprzedaży w tym kwartale był wyższy niż przewidywali analitycy.",
                            "target_lang_translations": {
                                "ru": "<w>Процентный</w> рост продаж в этом квартале был выше, чем прогнозировали аналитики."
                            }
                        },
                        {
                            "text": "Musimy dokładnie obliczyć udział <w>procentowy</w> każdego składnika w gotowym produkcie.",
                            "target_lang_translations": {
                                "ru": "Мы должны точно рассчитать <w>процентное</w> содержание каждого ингредиента в готовом продукте."
                            }
                        }
                    ],
                    "common_phrases": [
                        "punkt procentowy",
                        "udział procentowy",
                        "skład procentowy",
                        "próg procentowy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к процентам, выраженный в процентах или связанный с вычислением сотых долей целого."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "процентный",
                                "target_lang_sense_clarification": "выраженный в процентах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8d74aff3-f169-3243-b15b-d4b1cee7dac3",
                    "sense_definition": "Zawierający alkohol; określenie używane głównie w odniesieniu do napojów.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "alcohol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na przyjęciu urodzinowym serwowano wyłącznie soki i inne napoje bezalkoholowe, nie było nic <w>procentowego</w>.",
                            "target_lang_translations": {
                                "ru": "На дне рождения подавали только соки и другие безалкогольные напитки, не было ничего <w>спиртного</w>."
                            }
                        },
                        {
                            "text": "Ten sklep oferuje szeroki wybór trunków <w>procentowych</w> z różnych stron świata.",
                            "target_lang_translations": {
                                "ru": "Этот магазин предлагает широкий выбор <w>алкогольных</w> напитков из разных уголков мира."
                            }
                        }
                    ],
                    "synonyms": [
                        "alkoholowy",
                        "wyskokowy"
                    ],
                    "antonyms": [
                        "bezalkoholowy",
                        "bezprocentowy"
                    ],
                    "common_phrases": [
                        "napój procentowy",
                        "trunek procentowy"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "często używane jako eufemizm zamiast słowa alkoholowy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Содержащий алкоголь; термин, используемый в основном по отношению к напиткам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "алкогольный",
                                "target_lang_sense_clarification": "содержащий спирт"
                            },
                            {
                                "target_lang_word": "спиртной",
                                "target_lang_sense_clarification": "о напитках"
                            }
                        ]
                    }
                }

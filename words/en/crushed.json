    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2fd3dfbb-9a42-311e-b40e-f97f9ff92c57",
                    "sense_definition": "Simple past and past participle of <w>crush</w>.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He <w>crushed</w> the empty soda can with his foot.",
                            "target_lang_translations": {
                                "ru": "Он <w>сплющил</w> ногой пустую банку из-под газировки."
                            }
                        },
                        {
                            "text": "The rebellion was quickly <w>crushed</w> by the army.",
                            "target_lang_translations": {
                                "ru": "Восстание было быстро <w>подавлено</w> армией."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "past tense and participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прошедшее время и причастие прошедшего времени от глагола crush (давить, сокрушать)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раздавил",
                                "target_lang_sense_clarification": "сплющил или разрушил давлением"
                            },
                            {
                                "target_lang_word": "подавил",
                                "target_lang_sense_clarification": "пресек силой (восстание, сопротивление)"
                            },
                            {
                                "target_lang_word": "смял",
                                "target_lang_sense_clarification": "деформировал"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0344c651-b887-37d6-8db9-436b2e084744",
                    "sense_definition": "Pulverized or squeezed into small, disconnected fragments or out of shape.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The recipe requires a cup of <w>crushed</w> ice.",
                            "target_lang_translations": {
                                "ru": "Для рецепта требуется чашка <w>дроблёного</w> льда."
                            }
                        },
                        {
                            "text": "She sprinkled some <w>crushed</w> nuts on top of the cake.",
                            "target_lang_translations": {
                                "ru": "Она посыпала торт <w>толчёными</w> орехами."
                            }
                        }
                    ],
                    "synonyms": [
                        "pulverized",
                        "ground",
                        "smashed"
                    ],
                    "antonyms": [
                        "whole",
                        "intact"
                    ],
                    "common_phrases": [
                        "crushed ice",
                        "crushed garlic",
                        "crushed stone",
                        "crushed pepper"
                    ],
                    "target_lang_definitions": {
                        "ru": "Измельчённый, раздробленный на мелкие частицы или деформированный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дроблёный",
                                "target_lang_sense_clarification": "разбитый на мелкие куски (о льде, орехах)"
                            },
                            {
                                "target_lang_word": "толчёный",
                                "target_lang_sense_clarification": "измельченный ударами или растиранием"
                            },
                            {
                                "target_lang_word": "измельчённый",
                                "target_lang_sense_clarification": "превращенный в мелкие частицы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afd22121-dd69-346c-96bd-99c43298e9e7",
                    "sense_definition": "Extremely upset, disappointed, or devastated.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He felt completely <w>crushed</w> when he didn't get the job.",
                            "target_lang_translations": {
                                "ru": "Он чувствовал себя совершенно <w>раздавленным</w>, когда не получил эту работу."
                            }
                        },
                        {
                            "text": "We were totally <w>crushed</w> upon hearing the bad news.",
                            "target_lang_translations": {
                                "ru": "Мы были совершенно <w>разбиты</w>, услышав плохие новости."
                            }
                        }
                    ],
                    "synonyms": [
                        "devastated",
                        "heartbroken",
                        "shattered"
                    ],
                    "antonyms": [
                        "elated",
                        "overjoyed"
                    ],
                    "common_phrases": [
                        "feel crushed",
                        "totally crushed",
                        "look crushed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Крайне расстроенный, разочарованный или морально уничтоженный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "раздавленный",
                                "target_lang_sense_clarification": "морально уничтоженный"
                            },
                            {
                                "target_lang_word": "подавленный",
                                "target_lang_sense_clarification": "угнетенный, в депрессивном состоянии"
                            },
                            {
                                "target_lang_word": "разбитый",
                                "target_lang_sense_clarification": "потрясенный горем"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e8e82662-f09a-35fc-a153-ae8720c00ca8",
                    "sense_definition": "Of a fabric, deliberately treated to have a wrinkled or creased appearance.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wore a long dress made of black <w>crushed</w> velvet.",
                            "target_lang_translations": {
                                "ru": "На ней было длинное платье из чёрного <w>жатого</w> бархата."
                            }
                        },
                        {
                            "text": "The curtains in the theater were heavy <w>crushed</w> silk.",
                            "target_lang_translations": {
                                "ru": "Занавес в театре был из тяжёлого <w>жатого</w> шёлка."
                            }
                        }
                    ],
                    "synonyms": [
                        "creased",
                        "wrinkled"
                    ],
                    "antonyms": [
                        "smooth"
                    ],
                    "common_phrases": [
                        "crushed velvet",
                        "crushed silk"
                    ],
                    "target_lang_definitions": {
                        "ru": "О ткани: специально обработанная для создания эффекта помятости или складок."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жатый",
                                "target_lang_sense_clarification": "о фактуре ткани (бархат, шелк)"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "b2666029-7b95-3d7c-b872-58875126d343",
                    "sense_definition": "In most cases or situations; usually.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>generally</w> have a walk in the park during the afternoon.",
                            "target_lang_translations": {
                                "ru": "Я <w>обычно</w> гуляю в парке днём."
                            }
                        },
                        {
                            "text": "Generally speaking, it takes about an hour to get there by train.",
                            "target_lang_translations": {
                                "ru": "<w>Вообще говоря</w>, дорога туда на поезде занимает около часа."
                            }
                        }
                    ],
                    "synonyms": [
                        "usually",
                        "normally",
                        "mostly",
                        "ordinarily",
                        "regularly",
                        "by and large",
                        "as a whole",
                        "overall"
                    ],
                    "antonyms": [
                        "specifically",
                        "specifically"
                    ],
                    "common_phrases": [
                        "generally speaking"
                    ],
                    "target_lang_definitions": {
                        "ru": "В большинстве случаев или ситуаций; обычно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обычно",
                                "target_lang_sense_clarification": "в большинстве случаев"
                            },
                            {
                                "target_lang_word": "как правило",
                                "target_lang_sense_clarification": "почти всегда"
                            },
                            {
                                "target_lang_word": "в основном",
                                "target_lang_sense_clarification": "преимущественно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d020baa0-30a6-3d42-9ef5-9f6b326125f4",
                    "sense_definition": "By a large number of people or in many places.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is <w>generally</w> known that a healthy diet is important.",
                            "target_lang_translations": {
                                "ru": "<w>Широко</w> известно, что здоровое питание важно."
                            }
                        },
                        {
                            "text": "The new safety regulations were <w>generally</w> accepted by the employees.",
                            "target_lang_translations": {
                                "ru": "Новые правила техники безопасности были <w>в целом</w> приняты сотрудниками."
                            }
                        }
                    ],
                    "synonyms": [
                        "commonly",
                        "universally",
                        "broadly",
                        "at large"
                    ],
                    "common_phrases": [
                        "generally known",
                        "generally accepted",
                        "generally recognized"
                    ],
                    "target_lang_definitions": {
                        "ru": "Большим количеством людей или во многих местах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "широко",
                                "target_lang_sense_clarification": "среди многих людей"
                            },
                            {
                                "target_lang_word": "всеобще",
                                "target_lang_sense_clarification": "повсеместно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "99d7283d-bfbc-3619-a875-6dfb5856e80e",
                    "sense_definition": "Without reference to specific details or exceptions.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She described the project <w>generally</w> without going into the technical costs.",
                            "target_lang_translations": {
                                "ru": "Она описала проект <w>в общих чертах</w>, не вдаваясь в технические расходы."
                            }
                        },
                        {
                            "text": "The two proposals are <w>generally</w> similar, though they differ in minor points.",
                            "target_lang_translations": {
                                "ru": "Эти два предложения <w>в целом</w> схожи, хотя и различаются в мелочах."
                            }
                        }
                    ],
                    "synonyms": [
                        "broadly",
                        "basically",
                        "generically",
                        "in general",
                        "mostly"
                    ],
                    "antonyms": [
                        "specifically"
                    ],
                    "common_phrases": [
                        "describe generally"
                    ],
                    "target_lang_definitions": {
                        "ru": "Без упоминания конкретных деталей или исключений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в общих чертах",
                                "target_lang_sense_clarification": "без подробностей"
                            },
                            {
                                "target_lang_word": "в целом",
                                "target_lang_sense_clarification": "обобщая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c06098d4-c2fa-3231-bf11-136d8f37b069",
                    "sense_definition": "Collectively; as a whole group.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The citizens were <w>generally</w> summoned to the town square for the announcement.",
                            "target_lang_translations": {
                                "ru": "Горожане были <w>все вместе</w> созваны на городскую площадь для объявления."
                            }
                        },
                        {
                            "text": "I counsel that all the people be <w>generally</w> gathered together.",
                            "target_lang_translations": {
                                "ru": "Я советую, чтобы все люди были <w>сообща</w> собраны вместе."
                            }
                        }
                    ],
                    "synonyms": [
                        "collaboratively",
                        "jointly",
                        "en masse",
                        "altogether",
                        "as a whole"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "found in older texts like the Bible"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Коллективно; всей группой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сообща",
                                "target_lang_sense_clarification": "устар. совместно"
                            },
                            {
                                "target_lang_word": "все вместе",
                                "target_lang_sense_clarification": "коллективно"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d3628f2b-6252-391d-b84d-d04ccf5dfbb3",
                    "sense_definition": "To have a home in a particular place.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_reside",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I <w>live</w> in a small apartment near the city center.",
                            "target_lang_translations": {
                                "ru": "Я <w>живу</w> в маленькой квартире недалеко от центра города."
                            }
                        },
                        {
                            "text": "She <w>lives</w> with her parents while she finishes university.",
                            "target_lang_translations": {
                                "ru": "Она <w>живёт</w> с родителями, пока заканчивает университет."
                            }
                        }
                    ],
                    "synonyms": [
                        "dwell",
                        "in person",
                        "remain"
                    ],
                    "common_phrases": [
                        "live in",
                        "live at",
                        "live with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь дом в определенном месте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жить",
                                "target_lang_sense_clarification": "обитать, иметь жилье"
                            },
                            {
                                "target_lang_word": "проживать",
                                "target_lang_sense_clarification": "официально или временно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cbd96372-f746-388e-923a-befec58b89a5",
                    "sense_definition": "To be alive; to have life and not be dead.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "verb_exist",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is very sick and is not expected to <w>live</w> much longer.",
                            "target_lang_translations": {
                                "ru": "Он очень болен, и не ожидается, что он будет <w>жить</w> долго."
                            }
                        },
                        {
                            "text": "Spiders can <w>live</w> for several years if they are not disturbed.",
                            "target_lang_translations": {
                                "ru": "Пауки могут <w>жить</w> несколько лет, если их не тревожить."
                            }
                        }
                    ],
                    "synonyms": [
                        "go on",
                        "last",
                        "remain"
                    ],
                    "antonyms": [
                        "dead"
                    ],
                    "common_phrases": [
                        "live long",
                        "live forever"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть живым; иметь жизнь и не быть мертвым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жить",
                                "target_lang_sense_clarification": "существовать, быть живым"
                            },
                            {
                                "target_lang_word": "выживать",
                                "target_lang_sense_clarification": "сохранять жизнь в трудных условиях"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27a3b85b-7b5a-3f52-9a92-f508fb06f8ac",
                    "sense_definition": "To have enough food or money to survive.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_subsist",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is very difficult to <w>live</w> on such a low wage in this city.",
                            "target_lang_translations": {
                                "ru": "В этом городе очень трудно <w>жить</w> на такую низкую зарплату."
                            }
                        },
                        {
                            "text": "They <w>lived</w> on bread and water for three days.",
                            "target_lang_translations": {
                                "ru": "Они три дня <w>жили</w> на хлебе и воде."
                            }
                        }
                    ],
                    "synonyms": [
                        "go on"
                    ],
                    "common_phrases": [
                        "live on",
                        "live off"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иметь достаточно еды или денег для выживания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жить",
                                "target_lang_sense_clarification": "существовать на средства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d56dd3ed-a0b4-36d9-9c6c-29c660a42d4a",
                    "sense_definition": "To spend one's life in a particular way.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_experience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The prince and the princess <w>lived</w> happily ever after.",
                            "target_lang_translations": {
                                "ru": "Принц и принцесса <w>жили</w> долго и счастливо."
                            }
                        },
                        {
                            "text": "He wants to <w>live</w> a quiet life in the countryside.",
                            "target_lang_translations": {
                                "ru": "Он хочет <w>прожить</w> спокойную жизнь в сельской местности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "live a life",
                        "live happily"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проводить жизнь определенным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жить",
                                "target_lang_sense_clarification": "вести образ жизни"
                            },
                            {
                                "target_lang_word": "прожить",
                                "target_lang_sense_clarification": "провести жизнь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af76771e-8bf5-355d-a7f6-35d7976b2422",
                    "sense_definition": "To accept or tolerate an unpleasant situation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_cope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You made a mistake, and now you just have to <w>live</w> with it.",
                            "target_lang_translations": {
                                "ru": "Ты совершил ошибку, и теперь тебе просто придется с этим <w>жить</w>."
                            }
                        },
                        {
                            "text": "I cannot <w>live</w> with the guilt of what I did.",
                            "target_lang_translations": {
                                "ru": "Я не могу <w>жить</w> с чувством вины за то, что я сделал."
                            }
                        }
                    ],
                    "common_phrases": [
                        "live with it"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принимать или терпеть неприятную ситуацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "жить",
                                "target_lang_sense_clarification": "свыкнуться, терпеть (обычно с предлогом 'с')"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "9440c1c2-1590-3869-b421-3e183e4ddbc4",
                    "sense_definition": "Having life; not dead.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "adj_alive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The zoo has a special exhibit for <w>live</w> tropical fish.",
                            "target_lang_translations": {
                                "ru": "В зоопарке есть специальная экспозиция <w>живых</w> тропических рыб."
                            }
                        },
                        {
                            "text": "The post office will not ship <w>live</w> animals in boxes.",
                            "target_lang_translations": {
                                "ru": "Почта не пересылает <w>живых</w> животных в коробках."
                            }
                        }
                    ],
                    "synonyms": [
                        "living",
                        "alive"
                    ],
                    "antonyms": [
                        "dead"
                    ],
                    "common_phrases": [
                        "live animals",
                        "live bait"
                    ],
                    "target_lang_definitions": {
                        "ru": "Живой; не мертвый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "живой",
                                "target_lang_sense_clarification": "обладающий жизнью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8d619c3-912a-31a3-bf87-45fe1ba76ae7",
                    "sense_definition": "Broadcast at the exact time it happens; not recorded.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "adj_media",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are watching a <w>live</w> football match on television.",
                            "target_lang_translations": {
                                "ru": "Мы смотрим футбольный матч в <w>прямом эфире</w> по телевизору."
                            }
                        },
                        {
                            "text": "The news reporter gave a <w>live</w> update from the scene.",
                            "target_lang_translations": {
                                "ru": "Репортер новостей передал репортаж в <w>прямом эфире</w> с места событий."
                            }
                        }
                    ],
                    "antonyms": [
                        "recorded",
                        "prerecorded"
                    ],
                    "common_phrases": [
                        "live broadcast",
                        "live stream",
                        "live coverage"
                    ],
                    "target_lang_definitions": {
                        "ru": "Транслируемый в точное время события; не в записи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прямой",
                                "target_lang_sense_clarification": "о трансляции (прямой эфир)"
                            },
                            {
                                "target_lang_word": "в прямом эфире",
                                "target_lang_sense_clarification": "транслируемый сейчас"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0bda8ee0-8fb0-37d7-a4f5-c0a57823cc72",
                    "sense_definition": "Performed in front of an audience, not recorded in a studio.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "adj_media",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I prefer listening to <w>live</w> music rather than the radio.",
                            "target_lang_translations": {
                                "ru": "Я предпочитаю слушать <w>живую</w> музыку, а не радио."
                            }
                        },
                        {
                            "text": "This nightclub hosts a <w>live</w> band every Friday night.",
                            "target_lang_translations": {
                                "ru": "В этом ночном клубе каждую пятницу выступает <w>живая</w> группа."
                            }
                        }
                    ],
                    "synonyms": [
                        "in person",
                        "in the flesh"
                    ],
                    "common_phrases": [
                        "live music",
                        "live band",
                        "live performance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Исполняемый перед аудиторией, не записанный в студии."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "живой",
                                "target_lang_sense_clarification": "о музыке или исполнении"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a7289d6e-ff61-30e7-8295-45a337a1f6b8",
                    "sense_definition": "Involving real people or animals, not animated.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "adj_media",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The movie is a <w>live</w> action remake of the classic cartoon.",
                            "target_lang_translations": {
                                "ru": "Этот фильм — <w>игровой</w> ремейк классического мультфильма."
                            }
                        },
                        {
                            "text": "They used <w>live</w> actors instead of computer graphics for the crowd scenes.",
                            "target_lang_translations": {
                                "ru": "Для массовых сцен они использовали <w>живых</w> актеров вместо компьютерной графики."
                            }
                        }
                    ],
                    "antonyms": [
                        "animated"
                    ],
                    "common_phrases": [
                        "live action"
                    ],
                    "target_lang_definitions": {
                        "ru": "С участием реальных людей или животных, не анимация."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "игровой",
                                "target_lang_sense_clarification": "о кино (с живыми актерами)"
                            },
                            {
                                "target_lang_word": "живой",
                                "target_lang_sense_clarification": "настоящий, не нарисованный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b33756cc-e2ab-3fda-a197-4110217fd723",
                    "sense_definition": "Carrying an electric current.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "adj_energy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must be very careful when working near <w>live</w> wires.",
                            "target_lang_translations": {
                                "ru": "Нужно быть очень осторожным при работе рядом с проводами <w>под напряжением</w>."
                            }
                        },
                        {
                            "text": "Do not touch that switch because the circuit is still <w>live</w>.",
                            "target_lang_translations": {
                                "ru": "Не трогай этот выключатель, потому что цепь все еще <w>под напряжением</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "hot"
                    ],
                    "antonyms": [
                        "dead",
                        "neutral"
                    ],
                    "common_phrases": [
                        "live wire"
                    ],
                    "target_lang_definitions": {
                        "ru": "Под электрическим напряжением."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "под напряжением",
                                "target_lang_sense_clarification": "об электричестве"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c699bd9c-5b70-3a40-8574-310333990fff",
                    "sense_definition": "In actual use or operation; not a test version.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "adj_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new website will be <w>live</w> starting tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "Новый веб-сайт <w>заработает</w> завтра утром."
                            }
                        },
                        {
                            "text": "We are testing the software in a safe environment before it goes <w>live</w>.",
                            "target_lang_translations": {
                                "ru": "Мы тестируем программное обеспечение в безопасной среде перед тем, как оно будет <w>запущено</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "go live",
                        "live site"
                    ],
                    "target_lang_definitions": {
                        "ru": "В реальном использовании или работе; не тестовая версия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рабочий",
                                "target_lang_sense_clarification": "действующий, активный"
                            },
                            {
                                "target_lang_word": "запущенный",
                                "target_lang_sense_clarification": "введенный в эксплуатацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c44036bd-ce55-3bad-9f27-b892403f581e",
                    "sense_definition": "Capable of exploding or firing; not a dummy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "adj_energy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers were training with <w>live</w> ammunition on the range.",
                            "target_lang_translations": {
                                "ru": "Солдаты тренировались с <w>боевыми</w> патронами на полигоне."
                            }
                        },
                        {
                            "text": "The air force practices dropping <w>live</w> bombs on the uninhabited island.",
                            "target_lang_translations": {
                                "ru": "ВВС отрабатывают сброс <w>боевых</w> бомб на необитаемый остров."
                            }
                        }
                    ],
                    "antonyms": [
                        "blank",
                        "dummy"
                    ],
                    "common_phrases": [
                        "live ammo",
                        "live bomb"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способный взорваться или выстрелить; не муляж."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "боевой",
                                "target_lang_sense_clarification": "настоящий (об оружии)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "50dcab26-32d4-38f9-8b5a-b689709e2b56",
                    "sense_definition": "At the time an event happens; in real time.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "adv_broadcast",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The concert was broadcast <w>live</w> on national radio.",
                            "target_lang_translations": {
                                "ru": "Концерт транслировался <w>в прямом эфире</w> по национальному радио."
                            }
                        },
                        {
                            "text": "Reporters were on the scene to report <w>live</w> as the story developed.",
                            "target_lang_translations": {
                                "ru": "Репортеры были на месте, чтобы вести репортаж <w>в прямом эфире</w> по мере развития событий."
                            }
                        }
                    ],
                    "common_phrases": [
                        "broadcast live",
                        "report live"
                    ],
                    "target_lang_definitions": {
                        "ru": "В момент события; в реальном времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "в прямом эфире",
                                "target_lang_sense_clarification": "о трансляции"
                            },
                            {
                                "target_lang_word": "вживую",
                                "target_lang_sense_clarification": "непосредственно на месте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb566e79-ed00-3e06-b674-16d294aca858",
                    "sense_definition": "In a public performance; in person.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "adv_performance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The famous singer will be appearing <w>live</w> at the auditorium tonight.",
                            "target_lang_translations": {
                                "ru": "Знаменитый певец будет выступать <w>вживую</w> в аудитории сегодня вечером."
                            }
                        },
                        {
                            "text": "I have never seen this band perform <w>live</w> before.",
                            "target_lang_translations": {
                                "ru": "Я никогда раньше не видел, как эта группа выступает <w>вживую</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "perform live",
                        "appear live"
                    ],
                    "target_lang_definitions": {
                        "ru": "В публичном выступлении; лично."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вживую",
                                "target_lang_sense_clarification": "лично, не в записи"
                            },
                            {
                                "target_lang_word": "живьём",
                                "target_lang_sense_clarification": "разговорный вариант"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to",
                                "target_lang_sense_clarification": "speech/direction"
                            },
                            {
                                "target_lang_word": "towards",
                                "target_lang_sense_clarification": "behavior/attitude"

                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бусины",
                                "target_lang_sense_clarification": "отдельные элементы"
                            },
                            {
                                "target_lang_word": "капли",
                                "target_lang_sense_clarification": "жидкости"
                            },
                            {
                                "target_lang_word": "бисер",
                                "target_lang_sense_clarification": "мелкие бусинки для рукоделия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9334927e-f95b-32f5-a3ce-890443c0c3de",
                    "sense_definition": "A necklace made of small objects strung together.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wore a long string of <w>beads</w> around her neck.",
                            "target_lang_translations": {
                                "ru": "Она носила на шее длинную нитку <w>бус</w>."
                            }
                        },
                        {
                            "text": "The vintage <w>beads</w> complemented her summer dress perfectly.",
                            "target_lang_translations": {
                                "ru": "Эти винтажные <w>бусы</w> идеально дополняли её летнее платье."
                            }
                        }
                    ],
                    "synonyms": [
                        "necklace"
                    ],
                    "common_phrases": [
                        "wear beads",
                        "string of beads"
                    ],
                    "target_lang_definitions": {
                        "ru": "Украшение на шею, состоящее из нанизанных на нить маленьких предметов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бусы",
                                "target_lang_sense_clarification": "украшение"
                            }
                        ]
                    }
                }
            ]
        },
        {

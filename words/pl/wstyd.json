    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7b9cd701-7e3f-3e8e-83b4-eacf4e1e74bb",
                    "sense_definition": "Uczucie onieśmielenia, skrępowania i niepewności, często pojawiające się w obecności innych osób.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "emocje",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dziewczynka schowała się za mamę ze <w>wstydu</w>.",
                            "target_lang_translations": {
                                "ru": "Девочка спряталась за маму от <w>стеснения</w>."
                            }
                        },
                        {
                            "text": "Musisz pokonać fałszywy <w>wstyd</w> i zapytać o drogę.",
                            "target_lang_translations": {
                                "ru": "Ты должен побороть ложный <w>стыд</w> и спросить дорогу."
                            }
                        }
                    ],
                    "synonyms": [
                        "onieśmielenie",
                        "skrępowanie",
                        "zażenowanie"
                    ],
                    "antonyms": [
                        "śmiałość",
                        "pewność siebie"
                    ],
                    "common_phrases": [
                        "fałszywy wstyd",
                        "bez wstydu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство робости, стеснения и неуверенности, часто возникающее в присутствии других людей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стеснение",
                                "target_lang_sense_clarification": "чувство неловкости и робости"
                            },
                            {
                                "target_lang_word": "смущение",
                                "target_lang_sense_clarification": "состояние замешательства"
                            },
                            {
                                "target_lang_word": "стыд",
                                "target_lang_sense_clarification": "в значении ложного стыда или робости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "db2c9cbd-d57d-3795-91e8-cab541fe56c9",
                    "sense_definition": "Bardzo przykre uczucie wywołane świadomością zrobienia czegoś złego, niemoralnego lub niestosownego.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emocje",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Agnieszka zaczerwieniła się ze <w>wstydu</w>, gdy kłamstwo wyszło na jaw.",
                            "target_lang_translations": {
                                "ru": "Агнешка покраснела от <w>стыда</w>, когда ложь вышла наружу."
                            }
                        },
                        {
                            "text": "Chłopak spuścił głowę ze <w>wstydu</w> za swoje złe zachowanie.",
                            "target_lang_translations": {
                                "ru": "Мальчик опустил голову от <w>стыда</w> за своё плохое поведение."
                            }
                        }
                    ],
                    "synonyms": [
                        "upokorzenie"
                    ],
                    "antonyms": [
                        "duma",
                        "satysfakcja"
                    ],
                    "common_phrases": [
                        "spalić się ze wstydu",
                        "umrzeć ze wstydu",
                        "płonąć ze wstydu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Очень неприятное чувство, вызванное осознанием совершения чего-то плохого, аморального или неуместного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стыд",
                                "target_lang_sense_clarification": "чувство вины за проступок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4ddc64ac-8a07-3f56-aca6-4d809b5b67ac",
                    "sense_definition": "Sytuacja, czyn lub fakt oceniany negatywnie, który przynosi ujmę i naraża na utratę dobrego imienia.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "sytuacje_spoleczne",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bójka kibiców na stadionie to wielki <w>wstyd</w> dla całego klubu.",
                            "target_lang_translations": {
                                "ru": "Драка болельщиков на стадионе — это большой <w>позор</w> для всего клуба."
                            }
                        },
                        {
                            "text": "To żaden <w>wstyd</w> poprosić o pomoc, gdy jej potrzebujesz.",
                            "target_lang_translations": {
                                "ru": "В том, чтобы попросить о помощи, когда она тебе нужна, нет <w>стыда</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "kompromitacja",
                        "hańba",
                        "blamaż",
                        "poruta",
                        "obciach",
                        "wiocha",
                        "żenada",
                        "gańba"
                    ],
                    "antonyms": [
                        "zaszczyt",
                        "chwała"
                    ],
                    "common_phrases": [
                        "przynosić wstyd",
                        "wstyd i hańba",
                        "to żaden wstyd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация, поступок или факт, оцениваемый негативно, который приносит бесчестье и вредит репутации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "позор",
                                "target_lang_sense_clarification": "бесчестье, унизительное положение"
                            },
                            {
                                "target_lang_word": "стыд",
                                "target_lang_sense_clarification": "постыдная ситуация"
                            }
                        ]
                    }
                }
            ]
        }
    ]

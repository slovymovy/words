                },
                {
                    "sense_id": "c878db15-b792-3560-b9b3-d0f3b0a6279d",
                    "sense_definition": "Het zich verplaatsen van een voertuig (zoals een trein, bus of auto)",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement_vehicle",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De treinen <w>rijden</w> vandaag volgens de normale dienstregeling.",
                            "target_lang_translations": {
                                "en": "The trains are <w>running</w> according to the normal schedule today.",
                                "ru": "Поезда сегодня <w>ходят</w> по обычному расписанию."
                            }
                        },
                        {
                            "text": "Er <w>rijden</w> hier veel vrachtwagens op de snelweg.",
                            "target_lang_translations": {
                                "en": "Many trucks <w>drive</w> on the highway here.",
                                "ru": "Здесь по шоссе <w>ездит</w> много грузовиков."
                            }
                        }
                    ],
                    "antonyms": [
                        "stilstaan"
                    ],
                    "common_phrases": [
                        "op tijd rijden"
                    ],
                    "target_lang_definitions": {

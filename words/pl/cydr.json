    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "29db1bd2-0f0e-34c5-84b4-6b84be71e1d1",
                    "sense_definition": "Niskoalkoholowy napój wytwarzany ze sfermentowanego soku z jabłek.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "alcoholic_beverages",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W upalny dzień najlepiej smakuje mocno schłodzony <w>cydr</w>.",
                            "target_lang_translations": {
                                "ru": "В жаркий день вкуснее всего сильно охлажденный <w>сидр</w>."
                            }
                        },
                        {
                            "text": "Mój dziadek co roku robi domowy <w>cydr</w> z jabłek ze swojego sadu.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка каждый год делает домашний <w>сидр</w> из яблок из своего сада."
                            }
                        }
                    ],
                    "synonyms": [
                        "jabłecznik"
                    ],
                    "common_phrases": [
                        "cydr jabłkowy",
                        "zimny cydr",
                        "kieliszek cydru",
                        "produkcja cydru"
                    ],
                    "target_lang_definitions": {
                        "ru": "Слабоалкогольный напиток из сброженного яблочного сока."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сидр",
                                "target_lang_sense_clarification": "напиток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9aa3a345-f320-3a56-9072-8c7148e0aa16",
                    "sense_definition": "Porcja cydru, na przykład butelka, puszka lub szklanka tego napoju.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "alcoholic_beverages",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kelner przyniósł nam dwa <w>cydry</w> i miseczkę orzeszków.",
                            "target_lang_translations": {
                                "ru": "Официант принес нам два <w>сидра</w> и мисочку орешков."
                            }
                        },
                        {
                            "text": "Kupiliśmy kilka <w>cydrów</w> na wieczorne spotkanie ze znajomymi.",
                            "target_lang_translations": {
                                "ru": "Мы купили несколько <w>сидров</w> для вечерней встречи с друзьями."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dwa cydry",
                        "zamówić cydr"
                    ],
                    "target_lang_definitions": {
                        "ru": "Порция сидра, например, бутылка, банка или стакан этого напитка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сидр",
                                "target_lang_sense_clarification": "порция"
                            }
                        ]
                    }
                }
            ]
        }
    ]

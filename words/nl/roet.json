                            {
                                "target_lang_word": "копти",
                                "target_lang_sense_clarification": "покрывай копотью"
                            },
                            {
                                "target_lang_word": "не пачкай сажей",
                                "target_lang_sense_clarification": "отрицательная форма"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "soot",
                                "target_lang_sense_clarification": "command to cover with soot"

{
    "word_family": [
        "ogarniać",
        "ogarnięcie",
        "ogarnięty",
        "nieogarnięty"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "510e9bd0-4fbc-321d-b12e-8d0780ce2cf2",
                    "sense_definition": "Doprowadzić miejsce lub rzeczy do porządku; posprzątać.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "cleaning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę szybko <ogarnąć> pokój przed przyjściem gości.",
                            "target_lang_translations": {
                                "ru": "Мне нужно быстро <прибрать> комнату перед приходом гостей."
                            }
                        },
                        {
                            "text": "Czy możesz wreszcie <ogarnąć> ten bałagan na swoim biurku?",
                            "target_lang_translations": {
                                "ru": "Ты можешь наконец <разобрать> этот бардак на своем столе?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "ogarnąć mieszkanie",
                        "ogarnąć bałagan",
                        "ogarnąć pokój"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in everyday speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Привести место или вещи в порядок; убраться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привести в порядок",
                                "target_lang_sense_clarification": "о помещении или вещах"
                            },
                            {
                                "target_lang_word": "прибрать",
                                "target_lang_sense_clarification": "навести чистоту"
                            },
                            {
                                "target_lang_word": "убрать",
                                "target_lang_sense_clarification": "устранить беспорядок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b665f6f0-6bb9-3e10-a33f-939d7ba37ab9",
                    "sense_definition": "Zrozumieć coś, pojąć sens czegoś lub zorientować się w sytuacji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "understanding",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie mogę <ogarnąć>, jak działa ten nowy program komputerowy.",
                            "target_lang_translations": {
                                "ru": "Я не могу <понять>, как работает эта новая компьютерная программа."
                            }
                        },
                        {
                            "text": "W końcu <ogarnąłem> te trudne zadania z matematyki.",
                            "target_lang_translations": {
                                "ru": "Я наконец <разобрался> с этими трудными задачами по математике."
                            }
                        }
                    ],
                    "synonyms": [
                        "zrozumieć",
                        "skapować",
                        "skumać",
                        "sczaić",
                        "chwycić",
                        "złapać"
                    ],
                    "common_phrases": [
                        "ogarnąć temat",
                        "nie ogarniam",
                        "ciężko ogarnąć"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "very common in spoken language"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Понять что-то, уловить смысл или сориентироваться в ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "понять",
                                "target_lang_sense_clarification": "уловить смысл"
                            },
                            {
                                "target_lang_word": "разобраться",
                                "target_lang_sense_clarification": "вникнуть, понять устройство"
                            },
                            {
                                "target_lang_word": "постичь",
                                "target_lang_sense_clarification": "глубоко понять"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "19d71fc0-2017-3bea-8d75-70bb63399e48",
                    "sense_definition": "Odzyskać równowagę psychiczną, uspokoić się lub doprowadzić swój wygląd do ładu (używane z zaimkiem „się”).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "reflexive_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz się <ogarnąć> i przestać płakać z byle powodu.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно <взять себя в руки> и перестать плакать по пустякам."
                            }
                        },
                        {
                            "text": "Daj mi chwilę, muszę się <ogarnąć> po długiej podróży.",
                            "target_lang_translations": {
                                "ru": "Дай мне минуту, мне нужно <привести себя в порядок> после долгой поездки."
                            }
                        }
                    ],
                    "common_phrases": [
                        "ogarnij się",
                        "muszę się ogarnąć"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Восстановить душевное равновесие, успокоиться или привести свой внешний вид в порядок (используется с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прийти в себя",
                                "target_lang_sense_clarification": "успокоиться, восстановить равновесие"
                            },
                            {
                                "target_lang_word": "привести себя в порядок",
                                "target_lang_sense_clarification": "о внешнем виде"
                            },
                            {
                                "target_lang_word": "взять себя в руки",
                                "target_lang_sense_clarification": "собраться с духом"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ef107e4b-0936-3de0-b708-e6325ba20165",
                    "sense_definition": "Zorganizować coś, załatwić jakąś sprawę lub poradzić sobie z zadaniem.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "managing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy uda ci się <ogarnąć> bilety na ten piątkowy koncert?",
                            "target_lang_translations": {
                                "ru": "У тебя получится <достать> билеты на этот пятничный концерт?"
                            }
                        },
                        {
                            "text": "Spokojnie, <ogarnę> ten projekt do końca tygodnia.",
                            "target_lang_translations": {
                                "ru": "Спокойно, я <разберусь с> этим проектом до конца недели."
                            }
                        }
                    ],
                    "synonyms": [
                        "uporać się"
                    ],
                    "common_phrases": [
                        "ogarnąć sprawę",
                        "ogarnąć temat",
                        "ogarnąć wszystko"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Организовать что-то, решить вопрос или справиться с задачей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "организовать",
                                "target_lang_sense_clarification": "устроить что-либо"
                            },
                            {
                                "target_lang_word": "уладить",
                                "target_lang_sense_clarification": "решить проблему или дело"
                            },
                            {
                                "target_lang_word": "достать",
                                "target_lang_sense_clarification": "разг. добыть, купить (напр. билеты)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "be3e8492-d7b8-35fb-9911-74dbf015db47",
                    "sense_definition": "O uczuciach, myślach lub zjawiskach: silnie na kogoś wpłynąć, opanować kogoś całkowicie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotions",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nagle <ogarnął> go wielki strach przed nieznanym.",
                            "target_lang_translations": {
                                "ru": "Внезапно его <охватил> сильный страх перед неизвестным."
                            }
                        },
                        {
                            "text": "Głęboki smutek <ogarnął> wszystkich zgromadzonych na sali.",
                            "target_lang_translations": {
                                "ru": "Глубокая печаль <охватила> всех собравшихся в зале."
                            }
                        }
                    ],
                    "synonyms": [
                        "opanować",
                        "objąć",
                        "chwycić"
                    ],
                    "common_phrases": [
                        "ogarnął go strach",
                        "ogarnęła go radość",
                        "ogarnął go smutek"
                    ],
                    "target_lang_definitions": {
                        "ru": "О чувствах, мыслях или явлениях: сильно повлиять на кого-то, полностью завладеть кем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "охватить",
                                "target_lang_sense_clarification": "о сильных чувствах или состояниях"
                            },
                            {
                                "target_lang_word": "овладеть",
                                "target_lang_sense_clarification": "полностью подчинить себе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d8a983bc-0ed5-3b86-8392-c0dd4f6ca519",
                    "sense_definition": "Objąć kogoś ramionami lub otoczyć coś ze wszystkich stron (także wzrokiem).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_embrace",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Matka czule <ogarnęła> płaczące dziecko ramionami.",
                            "target_lang_translations": {
                                "ru": "Мать нежно <обняла> плачущего ребенка."
                            }
                        },
                        {
                            "text": "Dyrektor <ogarnął> wzrokiem całą salę konferencyjną.",
                            "target_lang_translations": {
                                "ru": "Директор <окинул взглядом> весь конференц-зал."
                            }
                        }
                    ],
                    "synonyms": [
                        "objąć",
                        "otoczyć"
                    ],
                    "common_phrases": [
                        "ogarnąć wzrokiem",
                        "ogarnąć spojrzeniem",
                        "ogarnąć ramionami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обнять кого-то или окружить что-то со всех сторон (также взглядом)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обнять",
                                "target_lang_sense_clarification": "заключить в объятия"
                            },
                            {
                                "target_lang_word": "окинуть взглядом",
                                "target_lang_sense_clarification": "посмотреть на всё сразу"
                            },
                            {
                                "target_lang_word": "окружить",
                                "target_lang_sense_clarification": "расположиться вокруг"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
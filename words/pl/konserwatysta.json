            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e9fe90b7-277b-3562-a78b-862c721b1548",
                    "sense_definition": "Osoba przywiązana do tradycji i niechętna wszelkim zmianom.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek to zatwardziały <w>konserwatysta</w>, który nie uznaje płatności kartą.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка — убежденный <w>консерватор</w>, который не признает оплату картой."
                            }
                        },
                        {
                            "text": "Jako <w>konserwatysta</w>, dyrektor firmy wolał sprawdzone metody zarządzania od nowoczesnych eksperymentów.",
                            "target_lang_translations": {
                                "ru": "Как <w>консерватор</w>, директор фирмы предпочитал проверенные методы управления современным экспериментам."
                            }
                        }
                    ],
                    "synonyms": [
                        "tradycjonalista",
                        "człowiek starej daty"
                    ],
                    "antonyms": [
                        "nowator",
                        "postępowiec"
                    ],
                    "common_phrases": [
                        "zatwardziały konserwatysta",
                        "typowy konserwatysta"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, приверженный традициям и противящийся переменам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "консерватор",
                                "target_lang_sense_clarification": "человек старых взглядов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da329930-56c7-3ec1-8dfb-3226b4f57d46",
                    "sense_definition": "Członek partii konserwatywnej lub jej sympatyk.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "group_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W debacie telewizyjnej wystąpił znany <w>konserwatysta</w>, który bronił obecnej polityki rządu.",
                            "target_lang_translations": {
                                "ru": "В теледебатах выступил известный <w>консерватор</w>, который защищал нынешнюю политику правительства."
                            }
                        },
                        {
                            "text": "Wielu wyborców określa się jako <w>konserwatyści</w>, głosując na partie prawicowe.",
                            "target_lang_translations": {
                                "ru": "Многие избиратели называют себя <w>консерваторами</w>, голосуя за правые партии."
                            }
                        }
                    ],
                    "synonyms": [
                        "reakcjonista"
                    ],
                    "antonyms": [
                        "postępowiec"
                    ],
                    "common_phrases": [
                        "umiarkowany konserwatysta",
                        "rząd konserwatystów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Член консервативной партии или её сторонник."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "консерватор",
                                "target_lang_sense_clarification": "политическая принадлежность"
                            }
                        ]
                    }
                }
            ]
        }
    ]

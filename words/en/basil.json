            "senses": [
                {
                    "sense_id": "a456ed96-d2c4-35a4-9a25-aa2370aab480",
                    "sense_definition": "An aromatic culinary herb (Ocimum basilicum) widely used in cooking, especially in Italian and Southeast Asian cuisine, or the plant itself.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I love the smell of fresh <w>basil</w> growing in the garden.",
                            "target_lang_translations": {
                                "ru": "Я обожаю запах свежего <w>базилика</w>, растущего в саду."
                            }
                        },
                        {
                            "text": "Please chop the <w>basil</w> leaves finely before adding them to the sauce.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, мелко нарежь листья <w>базилика</w> перед тем, как добавить их в соус."
                            }
                        }
                    ],
                    "synonyms": [
                        "sweet basil",
                        "common basil"
                    ],
                    "common_phrases": [
                        "fresh basil",
                        "basil leaves",
                        "tomato and basil",
                        "basil pesto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ароматная трава (Ocimum basilicum), широко используемая в кулинарии, особенно в итальянской и юго-восточной азиатской кухне, или само растение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "базили́к",
                                "target_lang_sense_clarification": "растение/пряность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a9ea22a-b5b1-3f25-90ad-e1ac72c62fdc",
                    "sense_definition": "Sheepskin that has been tanned using bark, often used for bookbinding or leather aprons.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old ledger was bound in smooth red <w>basil</w>.",
                            "target_lang_translations": {
                                "ru": "Старая бухгалтерская книга была переплетена гладкой красной <w>дублёной овчиной</w>."
                            }
                        },
                        {
                            "text": "The shoemaker used a piece of <w>basil</w> to repair the lining.",
                            "target_lang_translations": {
                                "ru": "Сапожник использовал кусок <w>базана</w> для починки подкладки."

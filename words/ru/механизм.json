    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9752db12-030e-34f3-bb08-dcc59d2f2033",
                    "sense_definition": "Внутреннее устройство машины, прибора или аппарата, приводящее их в действие.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мастер аккуратно починил сложный часовой <механизм>.",
                            "target_lang_translations": {
                                "en": "The master carefully repaired the complex clockwork <mechanism>."
                            }
                        },
                        {
                            "text": "Внутри этой игрушки спрятан простой пружинный <механизм>.",
                            "target_lang_translations": {
                                "en": "A simple spring <mechanism> is hidden inside this toy."
                            }
                        }
                    ],
                    "common_phrases": [
                        "часовой механизм",
                        "спусковой механизм",
                        "сложный механизм",
                        "запустить механизм"
                    ],
                    "target_lang_definitions": {
                        "en": "The internal working parts of a machine, device, or apparatus that make it function."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mechanism",
                                "target_lang_sense_clarification": "internal parts"
                            },
                            {
                                "target_lang_word": "works",
                                "target_lang_sense_clarification": "of a clock or watch"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d7b83b8-8014-3c48-9cde-0b1a275d8af3",
                    "sense_definition": "Совокупность состояний и процессов, из которых складывается какое-либо явление или действие.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач подробно объяснил пациенту <механизм> действия нового лекарства.",
                            "target_lang_translations": {
                                "en": "The doctor explained the <mechanism> of action of the new drug to the patient in detail."
                            }
                        },
                        {
                            "text": "Рыночный <механизм> помогает регулировать цены на товары и услуги.",
                            "target_lang_translations": {
                                "en": "The market <mechanism> helps regulate prices for goods and services."
                            }
                        }
                    ],
                    "common_phrases": [
                        "рыночный механизм",
                        "механизм действия",
                        "защитный механизм",
                        "механизм памяти"
                    ],
                    "target_lang_definitions": {
                        "en": "A set of processes or states that constitute a phenomenon or action."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mechanism",
                                "target_lang_sense_clarification": "process or system"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7a99f8f-b6c8-36c7-b498-a07f84327d1f",
                    "sense_definition": "Сложное техническое устройство; машина.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На строительную площадку доставили мощный подъемный <механизм>.",
                            "target_lang_translations": {
                                "en": "A powerful hoisting <mechanism> was delivered to the construction site."
                            }
                        },
                        {
                            "text": "Этот <механизм> значительно облегчает тяжелый физический труд рабочих.",
                            "target_lang_translations": {
                                "en": "This <mechanism> significantly eases the heavy physical labor of the workers."
                            }
                        }
                    ],
                    "synonyms": [
                        "машина"
                    ],
                    "common_phrases": [
                        "подъемный механизм",
                        "исполнительный механизм",
                        "поворотный механизм"
                    ],
                    "target_lang_definitions": {
                        "en": "A complex technical device or machine designed for a specific task."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mechanism",
                                "target_lang_sense_clarification": "device"
                            },
                            {
                                "target_lang_word": "machinery",
                                "target_lang_sense_clarification": "equipment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f68c9f2f-a397-347b-a8a8-7be6b53e58d9",
                    "sense_definition": "Внутреннее устройство или система организации деятельности чего-либо (государства, общества, управления).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Государственный <механизм> должен работать четко и без сбоев.",
                            "target_lang_translations": {
                                "en": "The state <machinery> must work precisely and without failures."
                            }
                        },
                        {
                            "text": "Мы пытаемся понять сложный <механизм> управления этой огромной корпорацией.",
                            "target_lang_translations": {
                                "en": "We are trying to understand the complex management <mechanism> of this huge corporation."
                            }
                        }
                    ],
                    "common_phrases": [
                        "государственный механизм",
                        "механизм власти",
                        "механизм управления",
                        "хозяйственный механизм"
                    ],
                    "target_lang_definitions": {
                        "en": "The internal structure or system of organization of something (e.g., state, society, management)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "machinery",
                                "target_lang_sense_clarification": "administrative system"
                            },
                            {
                                "target_lang_word": "mechanism",
                                "target_lang_sense_clarification": "organizational system"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f4b79acc-bc71-3463-ae37-f8d5103fe1c1",
                    "sense_definition": "Философское направление, сводящее всё многообразие бытия к простым законам механики.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "philosophy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В своих лекциях профессор критиковал <механизм> как устаревший подход к биологии.",
                            "target_lang_translations": {
                                "en": "In his lectures, the professor criticized <mechanism> as an outdated approach to biology."
                            }
                        },
                        {
                            "text": "Этот философский <механизм> отрицает качественное своеобразие живой природы.",
                            "target_lang_translations": {
                                "en": "This philosophical <mechanism> denies the qualitative uniqueness of living nature."
                            }
                        }
                    ],
                    "synonyms": [
                        "механицизм"
                    ],
                    "target_lang_definitions": {
                        "en": "A philosophical doctrine that explains all phenomena by simple mechanical laws."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mechanism",
                                "target_lang_sense_clarification": "philosophy"
                            },
                            {
                                "target_lang_word": "mechanicism",
                                "target_lang_sense_clarification": "philosophical theory"
                            }
                        ]
                    }
                }
            ]
        }
    ]

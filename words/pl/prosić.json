                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приглашать",
                                "target_lang_sense_clarification": "звать войти или принять участие"
                            },
                            {
                                "target_lang_word": "звать",
                                "target_lang_sense_clarification": "приглашать куда-либо"
                            },
                            {
                                "target_lang_word": "просить",
                                "target_lang_sense_clarification": "вежливо приглашать (напр. к столу)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "68fbb49b-b0fe-361a-a658-c7ab76708ee9",
                    "sense_definition": "Bardzo usilnie i emocjonalnie domagać się czegoś od kogoś; błagać.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Skazaniec <w>prosił</w> sędziego o łagodniejszy wyrok.",
                            "target_lang_translations": {

{
    "word_family": [],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "bc48a1be-0c54-3293-b4b3-ecfce5269e86",
                    "sense_definition": "Used to indicate the source, origin, or place where someone or something comes.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "origin_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Paul is <w>from</w> New Zealand.",
                            "target_lang_translations": {
                                "ru": "Пол <w>из</w> Новой Зеландии."
                            }
                        },
                        {
                            "text": "I received a letter <w>from</w> my brother yesterday.",
                            "target_lang_translations": {
                                "ru": "Вчера я получил письмо <w>от</w> брата."
                            }
                        }
                    ],
                    "synonyms": [
                        "out of"
                    ],
                    "common_phrases": [
                        "come from",
                        "hear from",
                        "from abroad"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания источника, происхождения или места, откуда кто-то или что-то прибыло."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "из",
                                "target_lang_sense_clarification": "происхождение, место"
                            },
                            {
                                "target_lang_word": "от",
                                "target_lang_sense_clarification": "источник, отправитель"
                            },
                            {
                                "target_lang_word": "с",
                                "target_lang_sense_clarification": "направление, место прибытия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aae5b7fe-a294-30ff-90da-aa9df9eac66c",
                    "sense_definition": "Used to indicate the starting point of a movement, time period, or range.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "origin_source",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The store is open <w>from</w> nine to five.",
                            "target_lang_translations": {
                                "ru": "Магазин открыт <w>с</w> девяти до пяти."
                            }
                        },
                        {
                            "text": "He walked <w>from</w> the station to his house.",
                            "target_lang_translations": {
                                "ru": "Он шел <w>от</w> станции до своего дома."
                            }
                        }
                    ],
                    "antonyms": [
                        "to"
                    ],
                    "common_phrases": [
                        "from time to time",
                        "from now on",
                        "from start to finish"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания начальной точки движения, периода времени или диапазона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с",
                                "target_lang_sense_clarification": "время, начало действия"
                            },
                            {
                                "target_lang_word": "от",
                                "target_lang_sense_clarification": "пространство, расстояние"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "976c64cc-9c71-37cb-9c24-5d20614478f6",
                    "sense_definition": "Used to show the material or substance used to create something.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "material",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This juice is made <w>from</w> fresh oranges.",
                            "target_lang_translations": {
                                "ru": "Этот сок сделан <w>из</w> свежих апельсинов."
                            }
                        },
                        {
                            "text": "Paper is produced <w>from</w> wood.",
                            "target_lang_translations": {
                                "ru": "Бумага производится <w>из</w> древесины."
                            }
                        }
                    ],
                    "common_phrases": [
                        "made from"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания материала или вещества, из которого что-то сделано."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "из",
                                "target_lang_sense_clarification": "материал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "478e64c0-43aa-3a4a-a936-82db334d3965",
                    "sense_definition": "Used to indicate a cause or reason for something happening.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "cause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Too many people die <w>from</w> heart disease.",
                            "target_lang_translations": {
                                "ru": "Слишком много людей умирает <w>от</w> болезней сердца."
                            }
                        },
                        {
                            "text": "He is suffering <w>from</w> a bad headache.",
                            "target_lang_translations": {
                                "ru": "Он страдает <w>от</w> сильной головной боли."
                            }
                        }
                    ],
                    "common_phrases": [
                        "suffer from",
                        "die from",
                        "exhausted from"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания причины или повода происходящего."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "от",
                                "target_lang_sense_clarification": "причина, болезнь, внешнее воздействие"
                            },
                            {
                                "target_lang_word": "из",
                                "target_lang_sense_clarification": "внутренний мотив"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff804556-3e1f-37f2-9e3f-aa2b01cd7d4a",
                    "sense_definition": "Used to indicate removal, separation, or protection.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please remove the cake <w>from</w> the oven.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, достань пирог <w>из</w> духовки."
                            }
                        },
                        {
                            "text": "Sunscreen protects your skin <w>from</w> the sun.",
                            "target_lang_translations": {
                                "ru": "Солнцезащитный крем защищает кожу <w>от</w> солнца."
                            }
                        }
                    ],
                    "common_phrases": [
                        "remove from",
                        "protect from",
                        "stop from"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания на удаление, отделение или защиту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "из",
                                "target_lang_sense_clarification": "извлечение изнутри"
                            },
                            {
                                "target_lang_word": "от",
                                "target_lang_sense_clarification": "защита, избавление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1796415a-42cb-3dab-86d8-3d63578b32e8",
                    "sense_definition": "Used to indicate the position or point of view of an observer.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "vantage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is hard to tell <w>from</w> here.",
                            "target_lang_translations": {
                                "ru": "Трудно сказать <w>с</w> этого места."
                            }
                        },
                        {
                            "text": "Try to see the problem <w>from</w> his point of view.",
                            "target_lang_translations": {
                                "ru": "Постарайся увидеть проблему <w>с</w> его точки зрения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "from my perspective",
                        "from here",
                        "from where I stand"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания позиции или точки зрения наблюдателя."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с",
                                "target_lang_sense_clarification": "точка зрения, место обзора"
                            },
                            {
                                "target_lang_word": "от",
                                "target_lang_sense_clarification": "относительно объекта"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad6bdc83-c681-3b51-b4dc-2b8bae1571b7",
                    "sense_definition": "Used to show a difference or distinction between two things.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Your opinion differs <w>from</w> mine.",
                            "target_lang_translations": {
                                "ru": "Твое мнение отличается <w>от</w> моего."
                            }
                        },
                        {
                            "text": "He knows right <w>from</w> wrong.",
                            "target_lang_translations": {
                                "ru": "Он отличает хорошее <w>от</w> плохого."
                            }
                        }
                    ],
                    "common_phrases": [
                        "different from",
                        "know right from wrong"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания различия или отличия между двумя вещами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "от",
                                "target_lang_sense_clarification": "отличие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df980046-a921-363f-b3d0-85e5a4afbbdb",
                    "sense_definition": "Used to indicate that a number or amount is being subtracted.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "separation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twenty <w>from</w> thirty leaves ten.",
                            "target_lang_translations": {
                                "ru": "Если вычесть двадцать <w>из</w> тридцати, останется десять."
                            }
                        },
                        {
                            "text": "Subtract five <w>from</w> the total.",
                            "target_lang_translations": {
                                "ru": "Вычти пять <w>из</w> общей суммы."
                            }
                        }
                    ],
                    "synonyms": [
                        "take away"
                    ],
                    "common_phrases": [
                        "take from"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания числа или количества, которое вычитается."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "из",
                                "target_lang_sense_clarification": "вычитание"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
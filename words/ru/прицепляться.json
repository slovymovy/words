    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "380e4f25-d578-3d0c-ab05-a66c3c8970e1",
                    "sense_definition": "Прикрепляться, присоединяться к чему-либо (обычно с помощью крючка, застежки или другого механизма).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "physical_attachment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот вагон легко <прицепляется> к любому поезду.",
                            "target_lang_translations": {
                                "en": "This wagon <w>attaches</w> easily to any train."
                            }
                        },
                        {
                            "text": "Брошь плохо <прицепляется> к этой толстой ткани.",
                            "target_lang_translations": {
                                "en": "The brooch <w>fastens</w> poorly to this thick fabric."
                            }
                        }
                    ],
                    "synonyms": [
                        "прикрепляться",
                        "цепляться",
                        "присоединяться"
                    ],
                    "antonyms": [
                        "отцепляться",
                        "отсоединяться"
                    ],
                    "common_phrases": [
                        "прицепляться к одежде",
                        "прицепляться сзади",
                        "прицепляться крючком"
                    ],
                    "target_lang_definitions": {
                        "en": "To become attached or fastened to something, often via a mechanism like a hook or clasp."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to attach",
                                "target_lang_sense_clarification": "general term for connecting one thing to another"
                            },
                            {
                                "target_lang_word": "to fasten",
                                "target_lang_sense_clarification": "to close or join securely"
                            },
                            {
                                "target_lang_word": "to hitch",
                                "target_lang_sense_clarification": "specifically for vehicles or trailers"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d128ebb6-9e39-33d6-9bfb-224cdd7234c8",
                    "sense_definition": "Назойливо присоединяться к кому-либо, увязываться за кем-либо, не желая отставать.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_pestering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Младший брат постоянно <прицепляется> к нам, когда мы идём в кино.",
                            "target_lang_translations": {
                                "en": "My younger brother constantly <w>tags along</w> with us when we go to the movies."
                            }
                        },
                        {
                            "text": "Пьяный человек на улице начал <прицепляться> к прохожим с разговорами.",
                            "target_lang_translations": {
                                "en": "A drunk man on the street started <w>pestering</w> passers-by with conversation."
                            }
                        }
                    ],
                    "synonyms": [
                        "увязываться",
                        "приставать",
                        "навязываться"
                    ],
                    "antonyms": [
                        "отставать"
                    ],
                    "common_phrases": [
                        "прицепляться к компании",
                        "прицепляться к туристам"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in everyday speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To follow someone intrusively or annoyingly; to impose one's company on others."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to tag along",
                                "target_lang_sense_clarification": "to follow someone, often uninvited"
                            },
                            {
                                "target_lang_word": "to latch onto",
                                "target_lang_sense_clarification": "to attach oneself to someone socially"
                            },
                            {
                                "target_lang_word": "to pester",
                                "target_lang_sense_clarification": "to annoy someone by approaching them repeatedly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "515245ad-c929-35b4-a0ac-dd5348e6faca",
                    "sense_definition": "Искать недостатки, придираться к чьим-либо словам, действиям или внешнему виду.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "social_conflict",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Начальник вечно <прицепляется> к мелочам в моих отчётах.",
                            "target_lang_translations": {
                                "en": "The boss is always <w>nitpicking</w> the details in my reports."
                            }
                        },
                        {
                            "text": "Не стоит <прицепляться> к словам, если ты понимаешь общий смысл.",
                            "target_lang_translations": {
                                "en": "You shouldn't <w>pick at</w> words if you understand the general meaning."
                            }
                        }
                    ],
                    "synonyms": [
                        "придираться",
                        "докапываться"
                    ],
                    "common_phrases": [
                        "прицепляться к словам",
                        "прицепляться к мелочам",
                        "прицепляться по пустякам"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To criticize petty details or look for faults in words or actions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to nitpick",
                                "target_lang_sense_clarification": "to look for small, unimportant errors"
                            },
                            {
                                "target_lang_word": "to find fault with",
                                "target_lang_sense_clarification": "to criticize or complain about something"
                            },
                            {
                                "target_lang_word": "to pick on",
                                "target_lang_sense_clarification": "to criticize repeatedly or unfairly"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7813fc37-735e-3400-b2c4-67e5f3e2c66e",
                    "sense_definition": "Становиться постоянным, закрепляться за кем-либо (о прозвище) или передаваться кому-либо (о болезни).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_sticking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Это смешное прозвище <прицеплялось> к нему ещё в школе.",
                            "target_lang_translations": {
                                "en": "This funny nickname <w>stuck</w> to him back in school."
                            }
                        },
                        {
                            "text": "Осенью простуда моментально <прицепляется> к людям с слабым иммунитетом.",
                            "target_lang_translations": {
                                "en": "In autumn, a cold instantly <w>latches onto</w> people with weak immunity."
                            }
                        }
                    ],
                    "synonyms": [
                        "прилипать",
                        "приставать"
                    ],
                    "common_phrases": [
                        "прозвище прицепляется",
                        "болезнь прицепляется",
                        "кличка прицепляется"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To become permanently associated with someone (like a nickname) or to infect someone (like an illness)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to stick",
                                "target_lang_sense_clarification": "to remain attached or associated"
                            },
                            {
                                "target_lang_word": "to cling",
                                "target_lang_sense_clarification": "to hold on tightly or persist"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50e038f2-2cbc-3a95-adca-09af384b9e66",
                    "sense_definition": "Страдательный залог к глаголу <w>прицеплять</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "grammar",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Прицеп <прицепляется> к автомобилю опытным механиком.",
                            "target_lang_translations": {
                                "en": "The trailer <w>is attached</w> to the car by an experienced mechanic."
                            }
                        },
                        {
                            "text": "Этот модуль <прицепляется> к основной программе через специальный интерфейс.",
                            "target_lang_translations": {
                                "en": "This module <w>is attached</w> to the main program via a special interface."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "passive voice construction"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Passive form indicating something is being attached by an agent."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "to be attached",
                                "target_lang_sense_clarification": "passive voice"
                            },
                            {
                                "target_lang_word": "to be hitched",
                                "target_lang_sense_clarification": "passive voice, usually for vehicles"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "73da859c-ade8-312c-8560-7a964a053cd7",
                    "sense_definition": "Iemand die niet gelooft in de God van de Bijbel, de Koran of de Tenach; een ongelovige.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "religion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In sommige religieuze teksten wordt een niet-gelovige aangeduid als een <w>kafir</w>.",
                            "target_lang_translations": {
                                "ru": "В некоторых религиозных текстах неверующего называют <w>кафиром</w>."
                            }
                        },
                        {
                            "text": "Hij beschouwt zichzelf niet als een <w>kafir</w>, maar als een vrijdenker.",
                            "target_lang_translations": {
                                "ru": "Он считает себя не <w>кафиром</w>, а вольнодумцем."
                            }
                        }
                    ],
                    "synonyms": [
                        "ongelovige",
                        "atheïst"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который не верит в Бога Библии, Корана или Танаха; неверующий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кафир",
                                "target_lang_sense_clarification": "исламский термин"
                            },
                            {
                                "target_lang_word": "неверный",
                                "target_lang_sense_clarification": "религиозный контекст"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e9a6ff4-efa4-35e9-a489-dcf17f3f2011",
                    "sense_definition": "Een graansoort (Sorghum bicolor) die gebruikt wordt als voedsel of veevoer.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boeren verbouwen <w>kafir</w> op de droge velden.",
                            "target_lang_translations": {
                                "ru": "Фермеры выращивают <w>сорго</w> на засушливых полях."
                            }
                        },
                        {
                            "text": "Je kunt <w>kafir</w> gebruiken als alternatief voor rijst in dit gerecht.",
                            "target_lang_translations": {
                                "ru": "В этом блюде вы можете использовать <w>сорго</w> вместо риса."
                            }
                        }
                    ],
                    "synonyms": [
                        "sorghum",
                        "kafferkoren",
                        "durra",
                        "sorgo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вид зерновой культуры (Sorghum bicolor), используемый в пищу или на корм скоту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сорго",
                                "target_lang_sense_clarification": "ботаника"
                            },
                            {
                                "target_lang_word": "кафрское просо",
                                "target_lang_sense_clarification": "разновидность сорго"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "age",
        "aged",
        "ageless",
        "ageism"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "67853fac-41a6-3b8b-a410-866613baa79d",
                    "sense_definition": "The process of becoming older or more mature.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "process_of_aging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Regular exercise and a balanced diet can help slow down the effects of <w>aging</w>.",
                            "target_lang_translations": {
                                "ru": "Регулярные упражнения и сбалансированная диета могут помочь замедлить последствия <w>старения</w>."
                            }
                        },
                        {
                            "text": "The <w>aging</w> of the population is creating new challenges for the healthcare system.",
                            "target_lang_translations": {
                                "ru": "<w>Старение</w> населения создает новые проблемы для системы здравоохранения."
                            }
                        }
                    ],
                    "synonyms": [
                        "senescence"
                    ],
                    "common_phrases": [
                        "signs of aging",
                        "premature aging",
                        "anti-aging cream",
                        "population aging"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс старения или взросления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "старение",
                                "target_lang_sense_clarification": "биологический процесс"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fe4c5e74-e5a6-3bac-9ba6-7cb94d0c3c4c",
                    "sense_definition": "The deliberate process of making something appear older than it is.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "artificial_aging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>aging</w> of the wood was achieved by applying a special chemical solution.",
                            "target_lang_translations": {
                                "ru": "<w>Состаривание</w> древесины было достигнуто путем нанесения специального химического раствора."
                            }
                        },
                        {
                            "text": "He specializes in the <w>aging</w> of props to make them look authentic for historical movies.",
                            "target_lang_translations": {
                                "ru": "Он специализируется на <w>состаривании</w> реквизита, чтобы тот выглядел аутентично в исторических фильмах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "artificial aging"
                    ],
                    "target_lang_definitions": {
                        "ru": "Намеренный процесс придания предмету старого вида."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "состаривание",
                                "target_lang_sense_clarification": "искусственное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8658f970-f2ca-32c9-b585-1391b7865034",
                    "sense_definition": "Wear; wear and tear.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "material_wear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>aging</w> of the rubber seals caused the machine to leak oil.",
                            "target_lang_translations": {
                                "ru": "<w>Старение</w> резиновых уплотнителей привело к утечке масла из машины."
                            }
                        },
                        {
                            "text": "Engineers must account for material <w>aging</w> when designing long-lasting structures.",
                            "target_lang_translations": {
                                "ru": "Инженеры должны учитывать <w>старение</w> материалов при проектировании долговечных конструкций."
                            }
                        }
                    ],
                    "common_phrases": [
                        "thermal aging"
                    ],
                    "target_lang_definitions": {
                        "ru": "Износ; ухудшение свойств материала со временем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "старение",
                                "target_lang_sense_clarification": "материалов"
                            },
                            {
                                "target_lang_word": "износ",
                                "target_lang_sense_clarification": "физический"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3f76a77c-f180-360e-82db-ae5cd3ed66cb",
                    "sense_definition": "Becoming elderly.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "becoming_elderly",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She spends her weekends caring for her <w>aging</w> parents.",
                            "target_lang_translations": {
                                "ru": "Она проводит выходные, ухаживая за своими <w>стареющими</w> родителями."
                            }
                        },
                        {
                            "text": "The government needs to provide better support for the <w>aging</w> workforce.",
                            "target_lang_translations": {
                                "ru": "Правительству необходимо обеспечить лучшую поддержку <w>стареющей</w> рабочей силе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aging population",
                        "aging parents",
                        "aging infrastructure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становящийся пожилым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стареющий",
                                "target_lang_sense_clarification": "о людях"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "044ad4c1-32c5-353c-8fd4-6204df3f7154",
                    "sense_definition": "Present participle and gerund of <w>age</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verb_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is <w>aging</w> gracefully and staying active in his retirement.",
                            "target_lang_translations": {
                                "ru": "Он красиво <w>стареет</w> и остается активным на пенсии."
                            }
                        },
                        {
                            "text": "The fine wine is <w>aging</w> in oak barrels in the cellar.",
                            "target_lang_translations": {
                                "ru": "Хорошее вино <w>выдерживается</w> в дубовых бочках в погребе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aging gracefully"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "present participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола age."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стареющий",
                                "target_lang_sense_clarification": "причастие"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
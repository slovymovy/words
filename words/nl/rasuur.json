    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f76bb12a-8bdc-375b-b2ce-8442c0b9bf0e",
                    "sense_definition": "Een gedeelte van een handschrift of document waar de oorspronkelijke tekst is weggekrabd.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "text_correction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De onderzoeker bestudeerde de <w>rasuur</w> om te achterhalen wat er oorspronkelijk stond.",
                            "target_lang_translations": {
                                "en": "The researcher studied the <w>erasure</w> to figure out what was originally written there."
                            }
                        },
                        {
                            "text": "Op de plaats van de <w>rasuur</w> is het perkament dunner en ruwer geworden.",
                            "target_lang_translations": {
                                "en": "At the site of the <w>erasure</w>, the parchment has become thinner and rougher."
                            }
                        }
                    ],
                    "synonyms": [
                        "rasure"
                    ],
                    "common_phrases": [
                        "een zichtbare rasuur",
                        "sporen van rasuur"
                    ],
                    "target_lang_definitions": {
                        "en": "A section of a manuscript or document where the original text has been scraped away."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "erasure",
                                "target_lang_sense_clarification": "spot where text was removed"
                            },
                            {
                                "target_lang_word": "scraped area",
                                "target_lang_sense_clarification": "physical location of removal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "32bc769f-9490-30ff-8a58-199aa56d9908",
                    "sense_definition": "De handeling van het wegkrabben van inkt of tekst uit een handschrift, meestal om een correctie aan te brengen.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "text_correction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In de middeleeuwen was <w>rasuur</w> een gebruikelijke methode om schrijffouten te herstellen.",
                            "target_lang_translations": {
                                "en": "In the Middle Ages, <w>erasure</w> was a common method for correcting writing errors."
                            }
                        },
                        {
                            "text": "Hij verwijderde het woord door middel van <w>rasuur</w> met een scherp mesje.",
                            "target_lang_translations": {
                                "en": "He removed the word by means of <w>scraping</w> with a sharp knife."
                            }
                        }
                    ],
                    "synonyms": [
                        "rasure"
                    ],
                    "common_phrases": [
                        "correctie door rasuur",
                        "toepassing van rasuur"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of scraping away ink or text from a manuscript, usually to make a correction."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "erasure",
                                "target_lang_sense_clarification": "act of removing text"
                            },
                            {
                                "target_lang_word": "scraping",
                                "target_lang_sense_clarification": "physical removal with a blade"
                            }
                        ]
                    }
                }
            ]
        }
    ]

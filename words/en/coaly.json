            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a9f51d33-1ad2-34c4-9088-eeb89492dc51",
                    "sense_definition": "Covered in, or containing, <w>coal</w>.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "composition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The miners returned home with <w>coaly</w> faces after a long shift underground.",
                            "target_lang_translations": {
                                "ru": "Шахтеры вернулись домой с <w>черными от угля</w> лицами после долгой смены под землей."
                            }
                        },
                        {
                            "text": "She tried to scrub the <w>coaly</w> dust off her hands before dinner.",
                            "target_lang_translations": {
                                "ru": "Она пыталась оттереть <w>угольную</w> пыль с рук перед ужином."
                            }
                        }
                    ],
                    "synonyms": [
                        "sooty",
                        "carbonaceous"
                    ],
                    "antonyms": [
                        "clean"
                    ],
                    "common_phrases": [
                        "coaly dust",
                        "coaly residue"
                    ],
                    "target_lang_definitions": {
                        "ru": "Покрытый углем или содержащий уголь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угольный",
                                "target_lang_sense_clarification": "состоящий из угля или покрытый им"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c88d12e2-f711-3403-a845-ea72d36043ea",
                    "sense_definition": "Resembling <w>coal</w>, especially in color or darkness.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The raven's feathers had a deep, <w>coaly</w> shine that glistened in the sun.",
                            "target_lang_translations": {
                                "ru": "Перья ворона отливали глубоким <w>угольным</w> блеском, сверкавшим на солнце."
                            }
                        },
                        {
                            "text": "We peered into the <w>coaly</w> blackness of the abandoned tunnel.",
                            "target_lang_translations": {
                                "ru": "Мы вглядывались в <w>угольную</w> тьму заброшенного туннеля."
                            }
                        }
                    ],
                    "synonyms": [
                        "jet-black",
                        "pitch-black",
                        "dark"
                    ],
                    "common_phrases": [
                        "coaly blackness",
                        "coaly eyes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Напоминающий уголь, особенно цветом или темнотой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "угольно-черный",
                                "target_lang_sense_clarification": "цвета угля"
                            },
                            {
                                "target_lang_word": "угольный",
                                "target_lang_sense_clarification": "похожий на уголь"
                            }
                        ]
                    }
                }
            ]
        }
    ]

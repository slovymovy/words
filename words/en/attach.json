{
    "word_family": [
        "attachment",
        "detached",
        "detachment",
        "attachable",
        "unattached",
        "reattach"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a9a6534f-8c3e-3d74-b609-3716006df2dc",
                    "sense_definition": "To fasten, join, or connect one thing to another.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should <w>attach</w> the label firmly to your suitcase before traveling.",
                            "target_lang_translations": {
                                "ru": "Вам следует прочно <w>прикрепить</w> бирку к чемодану перед путешествием."
                            }
                        },
                        {
                            "text": "The workers will <w>attach</w> the new sign to the wall tomorrow morning.",
                            "target_lang_translations": {
                                "ru": "Рабочие <w>прикрепят</w> новую вывеску к стене завтра утром."
                            }
                        }
                    ],
                    "synonyms": [
                        "fasten",
                        "join",
                        "connect",
                        "affix",
                        "fix"
                    ],
                    "antonyms": [
                        "detach",
                        "unfasten",
                        "separate",
                        "disengage"
                    ],
                    "common_phrases": [
                        "attach securely",
                        "attach to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Скреплять, соединять или присоединять одну вещь к другой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прикреплять",
                                "target_lang_sense_clarification": "физическое соединение"
                            },
                            {
                                "target_lang_word": "присоединять",
                                "target_lang_sense_clarification": "добавлять или соединять части"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6d39e20-9784-34ed-96bc-d9489c76a568",
                    "sense_definition": "To include a file or document with an electronic message, such as an email.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "digital_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will <w>attach</w> the signed contract to this email for your review.",
                            "target_lang_translations": {
                                "ru": "Я <w>прикреплю</w> подписанный договор к этому письму для ознакомления."
                            }
                        },
                        {
                            "text": "She forgot to <w>attach</w> the photos she mentioned in her message.",
                            "target_lang_translations": {
                                "ru": "Она забыла <w>прикрепить</w> фотографии, о которых упоминала в сообщении."
                            }
                        }
                    ],
                    "synonyms": [
                        "include",
                        "add"
                    ],
                    "common_phrases": [
                        "attach a file",
                        "attach a document",
                        "attach a photo"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Прикладывать файл или документ к электронному сообщению."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прикреплять",
                                "target_lang_sense_clarification": "добавлять файл к письму"
                            },
                            {
                                "target_lang_word": "вкладывать",
                                "target_lang_sense_clarification": "помещать внутрь отправления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3da8171f-4cec-3fcf-aebe-d09b0791bb06",
                    "sense_definition": "To believe that something has a specific quality, meaning, or value.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "People often <w>attach</w> sentimental value to objects from their childhood.",
                            "target_lang_translations": {
                                "ru": "Люди часто <w>придают</w> сентиментальное значение предметам из своего детства."
                            }
                        },
                        {
                            "text": "The manager does not <w>attach</w> much importance to these minor errors.",
                            "target_lang_translations": {
                                "ru": "Менеджер не <w>придает</w> большого значения этим мелким ошибкам."
                            }
                        }
                    ],
                    "synonyms": [
                        "ascribe",
                        "attribute",
                        "assign"
                    ],
                    "common_phrases": [
                        "attach importance",
                        "attach significance",
                        "attach value",
                        "attach weight"
                    ],
                    "target_lang_definitions": {
                        "ru": "Считать, что что-то обладает определенным качеством, значением или ценностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "придавать",
                                "target_lang_sense_clarification": "наделять значением или важностью"
                            },
                            {
                                "target_lang_word": "приписывать",
                                "target_lang_sense_clarification": "относить качество к объекту"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ffcaddce-801e-33c9-86fd-3023c1f3ee72",
                    "sense_definition": "To form a close emotional relationship or connection with someone.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "emotional_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The shy puppy quickly <w>attached</w> itself to the kind stranger.",
                            "target_lang_translations": {
                                "ru": "Застенчивый щенок быстро <w>привязался</w> к доброму незнакомцу."
                            }
                        },
                        {
                            "text": "He tries not to <w>attach</w> himself too deeply to places he might have to leave.",
                            "target_lang_translations": {
                                "ru": "Он старается не слишком сильно <w>привязываться</w> к местам, которые, возможно, придется покинуть."
                            }
                        }
                    ],
                    "synonyms": [
                        "bind",
                        "bond"
                    ],
                    "antonyms": [
                        "separate"
                    ],
                    "common_phrases": [
                        "attach oneself to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Формировать близкую эмоциональную связь или привязанность к кому-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привязываться",
                                "target_lang_sense_clarification": "эмоциональная связь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7a136514-3708-398d-905d-7775c23f77cb",
                    "sense_definition": "To be associated with or applicable to a particular situation, rank, or condition.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "No blame <w>attaches</w> to the driver, as the accident was unavoidable.",
                            "target_lang_translations": {
                                "ru": "Никакая вина не <w>ложится</w> на водителя, так как авария была неизбежна."
                            }
                        },
                        {
                            "text": "Certain privileges <w>attach</w> to the position of senior director.",
                            "target_lang_translations": {
                                "ru": "Определенные привилегии <w>прилагаются</w> к должности старшего директора."
                            }
                        }
                    ],
                    "synonyms": [
                        "pertain",
                        "belong"
                    ],
                    "common_phrases": [
                        "blame attaches to",
                        "suspicion attaches to"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть связанным с чем-либо или быть свойственным определенной ситуации или статусу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "прилагаться",
                                "target_lang_sense_clarification": "сопутствовать чему-либо"
                            },
                            {
                                "target_lang_word": "быть присущим",
                                "target_lang_sense_clarification": "относиться к свойству или качеству"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
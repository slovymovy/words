    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "1c3f96b9-ee2f-32bf-8073-f774328e7fdd",
                    "sense_definition": "W sposób, który angażuje lub pochodzi tylko od jednej ze stron, często bez porozumienia z innymi uczestnikami.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "action_legal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Firma postanowiła <w>jednostronnie</w> zerwać kontrakt z dostawcą.",
                            "target_lang_translations": {
                                "ru": "Компания решила <w>в одностороннем порядке</w> расторгнуть контракт с поставщиком."
                            }
                        },
                        {
                            "text": "Nie możesz <w>jednostronnie</w> zmieniać ustalonych wcześniej zasad współpracy.",
                            "target_lang_translations": {
                                "ru": "Ты не можешь <w>в одностороннем порядке</w> менять ранее установленные правила сотрудничества."
                            }
                        }
                    ],
                    "synonyms": [
                        "samodzielnie"
                    ],
                    "antonyms": [
                        "dwustronnie",
                        "wielostronnie",
                        "wspólnie"
                    ],
                    "common_phrases": [

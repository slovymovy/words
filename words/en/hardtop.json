                {
                    "sense_id": "6ae7091d-d7b8-304f-b5b5-7bd3eed779e1",
                    "sense_definition": "The removable rigid roof of a convertible or sports car.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "automotive_roof",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He decided to attach the <w>hardtop</w> to his car for the winter months.",
                            "target_lang_translations": {
                                "ru": "Он решил установить <w>жёсткий верх</w> на свою машину на зимние месяцы."
                            }
                        },
                        {
                            "text": "The removable <w>hardtop</w> offers better protection against the rain than the fabric roof.",
                            "target_lang_translations": {
                                "ru": "Съемная <w>жёсткая крыша</w> обеспечивает лучшую защиту от дождя, чем тканевый верх."
                            }
                        }
                    ],
                    "synonyms": [
                        "rigid roof"
                    ],
                    "antonyms": [
                        "soft top"
                    ],
                    "common_phrases": [
                        "removable hardtop",
                        "convertible hardtop"
                    ],
                    "target_lang_definitions": {

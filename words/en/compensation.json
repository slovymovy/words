                    ],
                    "synonyms": [
                        "remuneration"
                    ],
                    "common_phrases": [
                        "compensation package",
                        "executive compensation",
                        "total compensation"
                    ],
                    "target_lang_definitions": {
                        "ru": "Оплата и льготы, предоставляемые работнику за его труд или услуги."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вознаграждение",
                                "target_lang_sense_clarification": "оплата труда"
                            },
                            {
                                "target_lang_word": "компенсационный пакет",
                                "target_lang_sense_clarification": "зарплата и льготы"

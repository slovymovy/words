{
    "word_family": [
        "betrekking",
        "betrokkene",
        "betrokkenheid"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0a37998e-63a3-3430-a95e-a524cf118148",
                    "sense_definition": "Iemand of iets ergens aan laten deelnemen of erin mengen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "group_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De leraar probeert alle leerlingen actief bij de les te <w>betrekken</w>.",
                            "target_lang_translations": {
                                "ru": "Учитель старается активно <w>вовлекать</w> всех учеников в урок.",
                                "en": "The teacher tries to actively <w>involve</w> all students in the lesson."
                            }
                        },
                        {
                            "text": "Wij willen graag de buurtbewoners bij het nieuwe project <w>betrekken</w>.",
                            "target_lang_translations": {
                                "ru": "Мы хотели бы <w>привлечь</w> местных жителей к новому проекту.",
                                "en": "We would like to <w>involve</w> the local residents in the new project."
                            }
                        }
                    ],
                    "synonyms": [
                        "erbij halen",
                        "inschakelen",
                        "mengen"
                    ],
                    "antonyms": [
                        "buitensluiten",
                        "uitsluiten"
                    ],
                    "common_phrases": [
                        "iemand ergens bij betrekken",
                        "nauw betrokken zijn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вовлекать кого-либо в деятельность или заставлять участвовать в чем-либо.",
                        "en": "To cause someone or something to participate in an activity or situation."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вовлекать",
                                "target_lang_sense_clarification": "побуждать к участию"
                            },
                            {
                                "target_lang_word": "привлекать",
                                "target_lang_sense_clarification": "приглашать к участию в деле"
                            },
                            {
                                "target_lang_word": "приобщать",
                                "target_lang_sense_clarification": "знакомить, включать в круг участников"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "involve",
                                "target_lang_sense_clarification": "to include someone in an activity"
                            },
                            {
                                "target_lang_word": "engage",
                                "target_lang_sense_clarification": "to cause to participate"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a63a5970-bcdc-367d-bfcf-6cfb986c45ba",
                    "sense_definition": "In verband brengen met iets anders; toepassen op.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je moet die algemene kritiek niet direct op jezelf <w>betrekken</w>.",
                            "target_lang_translations": {
                                "ru": "Ты не должен сразу <w>относить</w> эту общую критику к себе.",
                                "en": "You shouldn't immediately <w>apply</w> that general criticism to yourself."
                            }
                        },
                        {
                            "text": "De spreker wist het onderwerp goed op de actualiteit te <w>betrekken</w>.",
                            "target_lang_translations": {
                                "ru": "Оратор сумел удачно <w>увязать</w> тему с текущими событиями.",
                                "en": "The speaker managed to <w>relate</w> the subject well to current events."
                            }
                        }
                    ],
                    "synonyms": [
                        "relateren",
                        "koppelen"
                    ],
                    "common_phrases": [
                        "op zichzelf betrekken",
                        "betrekken op"
                    ],
                    "target_lang_definitions": {
                        "ru": "Устанавливать связь с чем-то другим; применять по отношению к кому-то.",
                        "en": "To connect or link to something else; to apply to a specific situation or person."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "относить",
                                "target_lang_sense_clarification": "касательно кого-то или чего-то"
                            },
                            {
                                "target_lang_word": "связывать",
                                "target_lang_sense_clarification": "устанавливать логическую связь"
                            },
                            {
                                "target_lang_word": "увязывать",
                                "target_lang_sense_clarification": "согласовывать, соединять"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "relate",
                                "target_lang_sense_clarification": "to establish a connection"
                            },
                            {
                                "target_lang_word": "apply",
                                "target_lang_sense_clarification": "to bring into relation with"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d14474ba-c9ba-3a86-a230-af027849e0eb",
                    "sense_definition": "In een huis of gebouw gaan wonen of werken.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_habitation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Volgende week zullen zij hun nieuwe woning in het centrum <w>betrekken</w>.",
                            "target_lang_translations": {
                                "ru": "На следующей неделе они <w>въедут</w> в свое новое жилье в центре.",
                                "en": "Next week they will <w>move into</w> their new home in the city center."
                            }
                        },
                        {
                            "text": "Het bedrijf gaat binnenkort een groter kantoor <w>betrekken</w>.",
                            "target_lang_translations": {
                                "ru": "Компания скоро <w>займет</w> более просторный офис.",
                                "en": "The company will soon <w>occupy</w> a larger office."
                            }
                        }
                    ],
                    "synonyms": [
                        "intrekken",
                        "gaan bewonen"
                    ],
                    "antonyms": [
                        "verlaten",
                        "ontruimen"
                    ],
                    "common_phrases": [
                        "een woning betrekken",
                        "een pand betrekken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Начинать жить в доме или работать в здании; переезжать в новое помещение.",
                        "en": "To move into a house or building to live or work there."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "въезжать",
                                "target_lang_sense_clarification": "перемещаться внутрь помещения"
                            },
                            {
                                "target_lang_word": "вселяться",
                                "target_lang_sense_clarification": "начинать жить в помещении"
                            },
                            {
                                "target_lang_word": "занимать",
                                "target_lang_sense_clarification": "офис или помещение (официально)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "move into",
                                "target_lang_sense_clarification": "to start living in a place"
                            },
                            {
                                "target_lang_word": "occupy",
                                "target_lang_sense_clarification": "to take possession of a space"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd8dd5d7-9a97-35c7-b648-a163814c0413",
                    "sense_definition": "Donker en bewolkt worden (van de lucht) of een sombere uitdrukking krijgen (van een gezicht).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tijdens de wandeling zag ik dat de lucht begon te <w>betrekken</w>.",
                            "target_lang_translations": {
                                "ru": "Во время прогулки я увидел, что небо начало <w>затягиваться</w>.",
                                "en": "During the walk I saw that the sky was starting to <w>cloud over</w>."
                            }
                        },
                        {
                            "text": "Toen hij het slechte nieuws hoorde, <w>betrok</w> zijn gezicht meteen.",
                            "target_lang_translations": {
                                "ru": "Когда он услышал плохие новости, его лицо сразу <w>помрачнело</w>.",
                                "en": "When he heard the bad news, his face immediately <w>darkened</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "dichttrekken",
                        "verduisteren",
                        "versomberen"
                    ],
                    "antonyms": [
                        "opklaren"
                    ],
                    "common_phrases": [
                        "de lucht betrekt",
                        "het gezicht betrok"
                    ],
                    "target_lang_definitions": {
                        "ru": "Становиться пасмурным (о небе) или приобретать мрачное выражение (о лице).",
                        "en": "To become overcast (of the sky) or to take on a gloomy expression (of a face)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "затягиваться",
                                "target_lang_sense_clarification": "облаками (о небе)"
                            },
                            {
                                "target_lang_word": "мрачнеть",
                                "target_lang_sense_clarification": "становиться угрюмым (о лице)"
                            },
                            {
                                "target_lang_word": "хмуриться",
                                "target_lang_sense_clarification": "о лице или погоде"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "cloud over",
                                "target_lang_sense_clarification": "of the sky"
                            },
                            {
                                "target_lang_word": "darken",
                                "target_lang_sense_clarification": "of a face or mood"
                            },
                            {
                                "target_lang_word": "become overcast",
                                "target_lang_sense_clarification": "weather condition"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "22b9461c-fa5a-33b3-8b8e-ca433a41f65f",
                    "sense_definition": "Goederen of producten ergens vandaan halen of kopen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_acquisition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wij <w>betrekken</w> onze verse ingrediënten rechtstreeks van de boer.",
                            "target_lang_translations": {
                                "ru": "Мы <w>закупаем</w> наши свежие ингредиенты напрямую у фермера.",
                                "en": "We <w>source</w> our fresh ingredients directly from the farmer."
                            }
                        },
                        {
                            "text": "Het eiland moet al zijn drinkwater van het vasteland <w>betrekken</w>.",
                            "target_lang_translations": {
                                "ru": "Остров вынужден <w>получать</w> всю питьевую воду с материка.",
                                "en": "The island has to <w>obtain</w> all its drinking water from the mainland."
                            }
                        }
                    ],
                    "synonyms": [
                        "afnemen",
                        "inkopen",
                        "verkrijgen"
                    ],
                    "antonyms": [
                        "leveren"
                    ],
                    "common_phrases": [
                        "goederen betrekken van",
                        "stroom betrekken"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Получать или приобретать товары, сырье из определенного источника.",
                        "en": "To obtain, purchase, or source goods from a specific supplier."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "получать",
                                "target_lang_sense_clarification": "добывать, доставлять"
                            },
                            {
                                "target_lang_word": "закупать",
                                "target_lang_sense_clarification": "приобретать (обычно оптом)"
                            },
                            {
                                "target_lang_word": "приобретать",
                                "target_lang_sense_clarification": "покупать"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "obtain",
                                "target_lang_sense_clarification": "to get or secure"
                            },
                            {
                                "target_lang_word": "source",
                                "target_lang_sense_clarification": "to get from a supplier"
                            },
                            {
                                "target_lang_word": "acquire",
                                "target_lang_sense_clarification": "to buy or gain possession of"
                            }
                        ]
                    }
                }

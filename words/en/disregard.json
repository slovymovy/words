{
    "word_family": [
        "regard",
        "regardless",
        "regardful"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "817c27ef-0c0a-3c79-8f6d-78bc473680ab",
                    "sense_definition": "To pay no attention to something; to ignore.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "group_ignore",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>disregard</w> the previous email as it contained an error.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>не обращайте внимания</w> на предыдущее письмо, так как оно содержало ошибку."
                            }
                        },
                        {
                            "text": "He chose to <w>disregard</w> the advice of his doctor.",
                            "target_lang_translations": {
                                "ru": "Он решил <w>пренебречь</w> советом своего врача."
                            }
                        }
                    ],
                    "synonyms": [
                        "ignore"
                    ],
                    "antonyms": [
                        "heed",
                        "pay attention to",
                        "mind"
                    ],
                    "common_phrases": [
                        "disregard the warning",
                        "please disregard",
                        "disregard instructions"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не обращать внимания на что-либо; игнорировать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "игнорировать",
                                "target_lang_sense_clarification": "умышленно не замечать"
                            },
                            {
                                "target_lang_word": "пренебрегать",
                                "target_lang_sense_clarification": "относиться без должного внимания или уважения"
                            },
                            {
                                "target_lang_word": "не обращать внимания",
                                "target_lang_sense_clarification": "оставлять без реакции"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "19d5bb92-d55c-3d29-a11c-2997630ea639",
                    "sense_definition": "The act of deliberately ignoring or not caring about something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "group_ignore",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The driver showed a reckless <w>disregard</w> for the safety of others.",
                            "target_lang_translations": {
                                "ru": "Водитель проявил безрассудное <w>пренебрежение</w> безопасностью окружающих."
                            }
                        },
                        {
                            "text": "Her complete <w>disregard</w> for the rules led to her dismissal.",
                            "target_lang_translations": {
                                "ru": "Её полное <w>пренебрежение</w> правилами привело к её увольнению."
                            }
                        }
                    ],
                    "antonyms": [
                        "attention",
                        "concern",
                        "regard"
                    ],
                    "common_phrases": [
                        "disregard for",
                        "total disregard",
                        "reckless disregard"
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт намеренного игнорирования или безразличия к чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пренебрежение",
                                "target_lang_sense_clarification": "отсутствие уважения или заботы"
                            },
                            {
                                "target_lang_word": "игнорирование",
                                "target_lang_sense_clarification": "намеренное оставление без внимания"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
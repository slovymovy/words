            "senses": [
                {
                    "sense_id": "6f46e4c0-4ecf-3bee-afe9-63afe0365aa0",
                    "sense_definition": "Spójnik łączący zdania lub części zdania o przeciwstawnym znaczeniu, wprowadzający treść ograniczającą lub korygującą; synonim słowa \"ale\", często używany w języku pisanym.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciałbym ci pomóc, <w>lecz</w> nie mam teraz czasu.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы тебе помочь, <w>но</w> у меня сейчас нет времени."
                            }
                        },
                        {
                            "text": "To zadanie jest trudne, <w>lecz</w> możliwe do wykonania.",
                            "target_lang_translations": {
                                "ru": "Это задание трудное, <w>однако</w> выполнимое."
                            }
                        }
                    ],
                    "synonyms": [
                        "ale",
                        "jednak",
                        "jednakże"
                    ],
                    "common_phrases": [
                        "wolno, lecz pewnie",
                        "mały, lecz ważny",
                        "smutny, lecz prawdziwy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Союз, соединяющий предложения или части предложения с противоположным значением, вводящий ограничивающее или корректирующее содержание; синоним слова \"ale\" (но), часто используется в письменной речи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "но",
                                "target_lang_sense_clarification": "противительный союз"
                            },
                            {
                                "target_lang_word": "однако",
                                "target_lang_sense_clarification": "книжный или более формальный вариант"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [

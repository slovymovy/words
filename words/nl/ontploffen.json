{
    "word_family": [
        "ontploffing",
        "ontplofbaar"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "99cc4a4a-35c1-3bab-a38e-7d1682b18300",
                    "sense_definition": "Plotseling en met veel kracht uit elkaar barsten, vaak met een harde knal.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "explode_literal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude gasfles <w>ontplofte</w> door de hitte van het vuur.",
                            "target_lang_translations": {
                                "ru": "Старый газовый баллон <w>взорвался</w> от жара огня."
                            }
                        },
                        {
                            "text": "Iedereen schrok toen het vuurwerk onverwacht <w>ontplofte</w>.",
                            "target_lang_translations": {
                                "ru": "Все испугались, когда фейерверк неожиданно <w>взорвался</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "exploderen",
                        "knallen",
                        "klappen"
                    ],
                    "antonyms": [
                        "imploderen"
                    ],
                    "common_phrases": [
                        "de bom ontploft",
                        "kunnen ontploffen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно разрываться на части с большой силой, часто с громким звуком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взрываться",
                                "target_lang_sense_clarification": "прямое значение"
                            },
                            {
                                "target_lang_word": "разорваться",
                                "target_lang_sense_clarification": "лопнуть с силой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7f5e93c-66cc-3493-8282-6524297a7472",
                    "sense_definition": "Plotseling heel erg boos worden.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "explode_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn vader <w>ontplofte</w> bijna toen hij de schade aan de auto zag.",
                            "target_lang_translations": {
                                "ru": "Мой отец чуть не <w>взорвался</w>, когда увидел повреждения автомобиля."
                            }
                        },
                        {
                            "text": "Ze stond op het punt te <w>ontploffen</w> van woede.",
                            "target_lang_translations": {
                                "ru": "Она была готова <w>взорваться</w> от ярости."
                            }
                        }
                    ],
                    "synonyms": [
                        "knallen",
                        "exploderen",
                        "knallen"
                    ],
                    "antonyms": [
                        "kalmeren"
                    ],
                    "common_phrases": [
                        "ontploffen van woede",
                        "bijna ontploffen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внезапно приходить в сильную ярость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взрываться",
                                "target_lang_sense_clarification": "переносное значение: от злости"
                            },
                            {
                                "target_lang_word": "вспылить",
                                "target_lang_sense_clarification": "внезапно рассердиться"
                            }
                        ]
                    }
                }
            ]
        }
    ]

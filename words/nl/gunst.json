            "pos": "noun",
            "senses": [
                {
                    "sense_id": "efa34bfe-e22e-39a8-acd5-56d0c5b77f58",
                    "sense_definition": "Een vrijwillige daad of dienst waarmee men iemand anders helpt of een plezier doet, zonder daartoe verplicht te zijn.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wil je mij een grote <w>gunst</w> doen en dit pakketje even naar het postkantoor brengen?",
                            "target_lang_translations": {
                                "ru": "Не мог бы ты сделать мне большое <w>одолжение</w> и отнести этот пакет на почту?"
                            }
                        },
                        {
                            "text": "De politicus probeerde bij de kiezers in de <w>gunst</w> te komen door belastingverlagingen te beloven.",
                            "target_lang_translations": {
                                "ru": "Политик пытался добиться <w>расположения</w> избирателей, обещая снижение налогов."
                            }
                        }
                    ],
                    "synonyms": [
                        "dienst",
                        "vriendendienst",
                        "plezier"
                    ],
                    "antonyms": [
                        "ongunst",
                        "ongenade"
                    ],
                    "common_phrases": [
                        "iemand een gunst verlenen",
                        "om een gunst vragen",
                        "in de gunst staan",
                        "bij iemand in de gunst komen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Добровольное действие или услуга, совершаемые для помощи кому-либо или чтобы сделать приятное, без каких-либо обязательств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "одолжение",
                                "target_lang_sense_clarification": "любезность, услуга"
                            },
                            {
                                "target_lang_word": "услуга",
                                "target_lang_sense_clarification": "помощь, доброе дело"
                            },
                            {
                                "target_lang_word": "благосклонность",
                                "target_lang_sense_clarification": "расположение, симпатия (в контексте отношений)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "4ae43f0b-165b-34ac-9965-15ec582f1948",
                    "sense_definition": "Een uitroep die wordt gebruikt om verbazing, verrassing of lichte schrik uit te drukken.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Gunst</w>, ik had niet verwacht dat het vandaag zo hard zou gaan regenen.",
                            "target_lang_translations": {
                                "ru": "<w>Боже</w>, я не ожидал, что сегодня пойдет такой сильный дождь."
                            }
                        },
                        {
                            "text": "<w>Gunst</w>, wat zie jij er prachtig uit in die nieuwe jurk!",
                            "target_lang_translations": {
                                "ru": "<w>Надо же</w>, как ты прекрасно выглядишь в этом новом платье!"
                            }
                        }
                    ],
                    "synonyms": [
                        "tjonge",
                        "jeetje"
                    ],
                    "target_lang_definitions": {

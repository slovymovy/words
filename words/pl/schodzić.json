    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "341f31c4-bb48-3c1a-bb93-2db9e5a1e677",
                    "sense_definition": "Przemieszczać się z góry na dół, na przykład po schodach lub zboczu.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Codziennie rano <schodzę> do kuchni na śniadanie.",
                            "target_lang_translations": {
                                "ru": "Каждое утро я <спускаюсь> на кухню к завтраку."
                            }
                        },
                        {
                            "text": "Musimy ostrożnie <schodzić> ze szczytu, bo robi się ciemno.",
                            "target_lang_translations": {
                                "ru": "Мы должны осторожно <спускаться> с вершины, потому что темнеет."
                            }
                        }
                    ],
                    "synonyms": [
                        "złazić"
                    ],
                    "antonyms": [
                        "wchodzić",
                        "wchodzić"
                    ],
                    "common_phrases": [
                        "schodzić po schodach",
                        "schodzić na dół",
                        "schodzić z drogi"
                    ],
                    "target_lang_definitions": {
                        "ru": "Перемещаться сверху вниз, например, по лестнице или склону."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "спускаться",
                                "target_lang_sense_clarification": "двигаться вниз"
                            },
                            {
                                "target_lang_word": "сходить",
                                "target_lang_sense_clarification": "со ступеньки или возвышенности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "250c4a7d-7a12-30aa-a04a-b62d7a6f4c86",
                    "sense_definition": "O plamach, farbie, skórze lub śniegu: znikać, oddzielać się od powierzchni lub ustępować.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "disappearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po opalaniu skóra zaczęła mi <schodzić> z pleców.",
                            "target_lang_translations": {
                                "ru": "После загара у меня начала <облезать> кожа со спины."
                            }
                        },
                        {
                            "text": "Ta tłusta plama nie chce <schodzić> z obrusu mimo prania.",
                            "target_lang_translations": {
                                "ru": "Это жирное пятно не хочет <сходить> со скатерти, несмотря на стирку."
                            }
                        }
                    ],
                    "synonyms": [
                        "ustępować"
                    ],
                    "antonyms": [
                        "zalegać"
                    ],
                    "common_phrases": [
                        "śnieg schodzi",
                        "skóra schodzi",
                        "plama nie schodzi"
                    ],
                    "target_lang_definitions": {
                        "ru": "О пятнах, краске, коже или снеге: исчезать, отслаиваться или таять."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сходить",
                                "target_lang_sense_clarification": "исчезать (о пятнах, снеге)"
                            },
                            {
                                "target_lang_word": "облезать",
                                "target_lang_sense_clarification": "отслаиваться (о коже)"
                            },
                            {
                                "target_lang_word": "отходить",
                                "target_lang_sense_clarification": "отделяться от поверхности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45122ea1-ef24-363c-91e4-cd7a7e9fcfcf",
                    "sense_definition": "Gromadzić się w jednym miejscu, przybywając z różnych stron (używane z „się”).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "gathering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszyscy goście powoli <schodzą> się do salonu na uroczystość.",
                            "target_lang_translations": {
                                "ru": "Все гости медленно <собираются> в гостиной на торжество."
                            }
                        },
                        {
                            "text": "Wieczorami sąsiedzi <schodzili> się na pogawędki przed domem.",
                            "target_lang_translations": {
                                "ru": "По вечерам соседи <сходились> поболтать перед домом."
                            }
                        }
                    ],
                    "synonyms": [
                        "zleźć się"
                    ],
                    "common_phrases": [
                        "schodzić się do domu",
                        "ludzie się schodzą"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собираться в одном месте, приходя с разных сторон."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сходиться",
                                "target_lang_sense_clarification": "встречаться в одном месте"
                            },
                            {
                                "target_lang_word": "собираться",
                                "target_lang_sense_clarification": "группироваться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c702130-cf1d-3401-b344-90a4d8a7cecb",
                    "sense_definition": "O towarach: być szybko kupowanym i znikać ze sklepu.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "commerce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Świeże bułki <schodzą> w naszej piekarni błyskawicznie.",
                            "target_lang_translations": {
                                "ru": "Свежие булочки <расходятся> в нашей пекарне молниеносно."
                            }
                        },
                        {
                            "text": "Bilety na ten koncert <schodziły> jak świeże bułeczki.",
                            "target_lang_translations": {
                                "ru": "Билеты на этот концерт <расходились> как горячие пирожки."
                            }
                        }
                    ],
                    "antonyms": [
                        "zalegać"
                    ],
                    "common_phrases": [
                        "schodzić na pniu",
                        "towar dobrze schodzi"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "О товарах: быстро раскупаться и исчезать с прилавков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расходиться",
                                "target_lang_sense_clarification": "быстро продаваться"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1705cd77-0c27-3f23-9fbc-ad72fa031f4b",
                    "sense_definition": "Zgadzać się na niższą cenę lub mniejsze wymagania podczas negocjacji.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "commerce",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sprzedawca nie chciał <schodzić> z ustalonej ceny ani o grosz.",
                            "target_lang_translations": {
                                "ru": "Продавец не хотел <уступать> в установленной цене ни на грош."
                            }
                        },
                        {
                            "text": "Podczas negocjacji rzadko <schodzi> ze swoich wymagań.",
                            "target_lang_translations": {
                                "ru": "Во время переговоров он редко <отступает> от своих требований."
                            }
                        }
                    ],
                    "synonyms": [
                        "maleć"
                    ],
                    "antonyms": [
                        "rosnąć"
                    ],
                    "common_phrases": [
                        "schodzić z ceny",
                        "schodzić z kosztów"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соглашаться на более низкую цену или снижать требования во время переговоров."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уступать",
                                "target_lang_sense_clarification": "соглашаться на меньшее"
                            },
                            {
                                "target_lang_word": "снижать",
                                "target_lang_sense_clarification": "уменьшать цену или требования"
                            },
                            {
                                "target_lang_word": "сбрасывать",
                                "target_lang_sense_clarification": "разг. уменьшать цену"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d81295f6-ed6e-3355-9cc3-af7dbe658a9e",
                    "sense_definition": "O drogach, liniach lub krawędziach: łączyć się lub stykać w jednym punkcie (używane z „się”).",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "geometry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym miejscu <schodzą> się trzy główne szlaki turystyczne.",
                            "target_lang_translations": {
                                "ru": "В этом месте <сходятся> три главных туристических маршрута."
                            }
                        },
                        {
                            "text": "Tutaj <schodzą> się dwa rurociągi doprowadzające wodę.",
                            "target_lang_translations": {
                                "ru": "Здесь <сходятся> два трубопровода, подающих воду."
                            }
                        }
                    ],
                    "common_phrases": [
                        "drogi się schodzą",
                        "linie się schodzą"
                    ],
                    "target_lang_definitions": {
                        "ru": "О дорогах, линиях или краях: соединяться или пересекаться в одной точке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сходиться",
                                "target_lang_sense_clarification": "соединяться в точке"
                            },
                            {
                                "target_lang_word": "пересекаться",
                                "target_lang_sense_clarification": "встречаться (о путях)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
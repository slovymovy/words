    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7364ebf5-4c47-3b33-ba72-cb9c5cebc49f",
                    "sense_definition": "Большой кусок плотной ткани, закрепляемый на мачте судна, который надувается ветром и приводит судно в движение.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Команда быстро подняла <w>паруса</w>, чтобы поймать попутный ветер.",
                            "target_lang_translations": {
                                "en": "The crew quickly raised the <w>sails</w> to catch the tailwind."
                            }
                        },
                        {
                            "text": "Вдали виднелся белый <w>парус</w> одинокой яхты.",
                            "target_lang_translations": {
                                "en": "A white <w>sail</w> of a lonely yacht was visible in the distance."
                            }
                        }
                    ],
                    "common_phrases": [
                        "поднять паруса",
                        "спустить паруса",
                        "идти под парусом",
                        "алые паруса"
                    ],
                    "target_lang_definitions": {
                        "en": "A large piece of strong fabric attached to a ship's mast to catch the wind and propel the vessel."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sail",
                                "target_lang_sense_clarification": "piece of fabric for propulsion"
                            },
                            {
                                "target_lang_word": "canvas",
                                "target_lang_sense_clarification": "sails collectively or fabric material"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e40b050d-44e5-3bd9-beca-4aae1776b975",
                    "sense_definition": "Элемент архитектурной конструкции в виде сферического треугольника, обеспечивающий переход от квадратного основания здания к круглому куполу.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Древние зодчие использовали <w>паруса</w> для поддержки массивного купола храма.",
                            "target_lang_translations": {
                                "en": "Ancient architects used <w>pendentives</w> to support the massive dome of the temple."
                            }
                        },
                        {
                            "text": "На каждом <w>парусе</w> собора были изображены лики евангелистов.",
                            "target_lang_translations": {
                                "en": "The faces of the Evangelists were depicted on each <w>pendentive</w> of the cathedral."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An architectural element in the form of a spherical triangle that connects a square base to a circular dome."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "pendentive",
                                "target_lang_sense_clarification": "architectural support for a dome"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "a1438f2c-96c4-3e4c-ae2d-8ea2ef68bc53",
                    "sense_definition": "Действуя крайне дерзко, бесстыдно и без должного уважения к окружающим.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он <w>нагло</w> врал мне прямо в глаза, даже не краснея.",
                            "target_lang_translations": {
                                "en": "He <w>brazenly</w> lied right to my face without even blushing."
                            }
                        },
                        {
                            "text": "Водитель <w>нагло</w> подрезал автобус и уехал на красный свет.",
                            "target_lang_translations": {
                                "en": "The driver <w>impudently</w> cut off the bus and drove through a red light."
                            }
                        }
                    ],
                    "synonyms": [
                        "дерзко",
                        "бесстыдно",
                        "нахально",
                        "вызывающе",
                        "бесцеремонно"
                    ],
                    "antonyms": [
                        "скромно",
                        "вежливо",
                        "робко",
                        "учтиво"
                    ],
                    "common_phrases": [
                        "нагло врать",
                        "вести себя нагло",
                        "нагло смотреть",
                        "нагло заявить"
                    ],
                    "target_lang_definitions": {
                        "en": "Acting in an extremely bold, shameless manner without due respect for others."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "brazenly",
                                "target_lang_sense_clarification": "acting without shame or embarrassment"
                            },
                            {
                                "target_lang_word": "impudently",
                                "target_lang_sense_clarification": "failing to show due respect"
                            },
                            {
                                "target_lang_word": "insolently",
                                "target_lang_sense_clarification": "showing a rude and arrogant lack of respect"
                            }
                        ]
                    }
                }
            ]
        },
        {

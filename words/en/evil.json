                        "nasty",
                        "vile"
                    ],
                    "common_phrases": [
                        "evil smell",
                        "evil temper"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mostly used in literary contexts describing smells or tempers"
                        }
                    ],
                    "target_lang_definitions": {

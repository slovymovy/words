                            {
                                "target_lang_word": "кругозор",
                                "target_lang_sense_clarification": "объем знаний и интересов"
                            },
                            {
                                "target_lang_word": "горизонт",
                                "target_lang_sense_clarification": "перен. сфера интересов (обычно мн.ч.)"
                            }

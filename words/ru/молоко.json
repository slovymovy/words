    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "001bc4b6-9709-340a-a021-8e7564bdffd8",
                    "sense_definition": "Питательная жидкость белого цвета, которую дают коровы, козы и другие млекопитающие; основной продукт питания для младенцев.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Каждое утро бабушка давала нам тёплое парное <w>молоко</w>.",
                            "target_lang_translations": {
                                "en": "Every morning grandmother gave us warm fresh <w>milk</w>."
                            }
                        },
                        {
                            "text": "В магазине нужно купить хлеб и пакет <w>молока</w>.",
                            "target_lang_translations": {
                                "en": "We need to buy bread and a carton of <w>milk</w> at the store."
                            }
                        }
                    ],
                    "common_phrases": [
                        "свежее молоко",
                        "коровье молоко",
                        "стакан молока",
                        "кислое молоко"
                    ],
                    "target_lang_definitions": {
                        "en": "A nutrient-rich white liquid produced by cows, goats, and other mammals; a staple food for infants."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "milk",
                                "target_lang_sense_clarification": "dairy product"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e6f8c018-d180-3ea6-a56b-75c8fbcded1d",
                    "sense_definition": "Белый сок, выделяемый некоторыми растениями или получаемый из орехов и семян.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "food",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Для приготовления этого тайского супа обязательно нужно кокосовое <w>молоко</w>.",
                            "target_lang_translations": {
                                "en": "To make this Thai soup, you definitely need coconut <w>milk</w>."
                            }
                        },
                        {
                            "text": "Если надломить стебель одуванчика, на пальцах останется горькое белое <w>молоко</w>.",
                            "target_lang_translations": {
                                "en": "If you break a dandelion stem, bitter white <w>milk</w> will remain on your fingers."
                            }
                        }
                    ],
                    "common_phrases": [
                        "кокосовое молоко",
                        "миндальное молоко"
                    ],
                    "target_lang_definitions": {
                        "en": "White juice secreted by certain plants or obtained from nuts and seeds."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "milk",
                                "target_lang_sense_clarification": "plant-based liquid"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73b7cb13-8b2c-33ce-8cd2-706398912787",
                    "sense_definition": "Густой, непрозрачный туман или плотные облака, через которые ничего не видно.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "weather",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Самолёт летел в сплошном <w>молоке</w>, и земли совсем не было видно.",
                            "target_lang_translations": {
                                "en": "The plane was flying in a solid <w>whiteout</w>, and the ground was not visible at all."
                            }
                        },
                        {
                            "text": "Из-за густого тумана водитель ехал очень медленно, словно в белом <w>молоке</w>.",
                            "target_lang_translations": {
                                "en": "Due to the thick fog, the driver drove very slowly, as if in white <w>milk</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "туман"
                    ],
                    "antonyms": [
                        "чистый воздух"
                    ],
                    "common_phrases": [
                        "как в молоке"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "метафора"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Thick, opaque fog or dense clouds through which nothing is visible."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "whiteout",
                                "target_lang_sense_clarification": "weather condition"
                            },
                            {
                                "target_lang_word": "thick fog",
                                "target_lang_sense_clarification": "weather condition"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "427dfe84-435b-391f-9381-13a6a5633b3f",
                    "sense_definition": "Белое поле вокруг центра мишени; промах при стрельбе, попадание в эту зону.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Первый выстрел попал в цель, а второй ушёл в <w>молоко</w>.",
                            "target_lang_translations": {
                                "en": "The first shot hit the target, but the second went into the <w>white</w>."
                            }
                        },
                        {
                            "text": "Стрелок сильно нервничал, поэтому все его пули летели в <w>молоко</w>.",
                            "target_lang_translations": {
                                "en": "The shooter was very nervous, so all his bullets flew into the <w>white</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "промах"
                    ],
                    "antonyms": [
                        "яблочко"
                    ],
                    "common_phrases": [
                        "попасть в молоко",
                        "уйти в молоко"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "спортивный жаргон"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The white area around the center of a target; a miss during shooting."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "white",
                                "target_lang_sense_clarification": "part of a target"
                            },
                            {
                                "target_lang_word": "miss",
                                "target_lang_sense_clarification": "shooting result"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "403d56c1-38ed-35da-8f77-434578f67cab",
                    "sense_definition": "Техническая жидкость белого цвета, представляющая собой смесь воды с каким-либо веществом.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "technical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рабочие использовали известковое <w>молоко</w> для побелки стен в здании.",
                            "target_lang_translations": {
                                "en": "The workers used lime <w>milk</w> to whitewash the walls in the building."
                            }
                        },
                        {
                            "text": "В химической лаборатории мы приготовили раствор, похожий на <w>молоко</w>.",
                            "target_lang_translations": {
                                "en": "In the chemical laboratory, we prepared a solution resembling <w>milk</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "известковое молоко"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "технический термин"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A white technical liquid consisting of a mixture of water with a substance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "milk",
                                "target_lang_sense_clarification": "chemical suspension"
                            }
                        ]
                    }
                }
            ]
        }
    ]

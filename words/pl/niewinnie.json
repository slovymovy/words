    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "c7af360a-c180-39b0-baec-6469abb5f51e",
                    "sense_definition": "W sposób szczery, naiwny, pełen prostoty lub świadczący o braku złych intencji.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chłopiec uśmiechnął się <w>niewinnie</w>, chowając ręce za plecami.",
                            "target_lang_translations": {
                                "ru": "Мальчик <w>невинно</w> улыбнулся, пряча руки за спиной."
                            }
                        },
                        {
                            "text": "Zapytała <w>niewinnie</w>, o czym tak szeptaliście w kuchni.",
                            "target_lang_translations": {
                                "ru": "Она <w>невинно</w> спросила, о чём вы так шептались на кухне."
                            }
                        }
                    ],
                    "synonyms": [
                        "szczerze",
                        "skromnie"
                    ],
                    "common_phrases": [
                        "uśmiechać się niewinnie",
                        "wyglądać niewinnie",
                        "zapytać niewinnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Искренне, наивно, простодушно или не имея дурных намерений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невинно",
                                "target_lang_sense_clarification": "выражая наивность или отсутствие злого умысла"
                            },
                            {
                                "target_lang_word": "наивно",
                                "target_lang_sense_clarification": "простодушно, по-детски"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ea8b3227-47d2-30ee-95d3-21da729d3915",
                    "sense_definition": "W sposób niezasłużony, bez popełnienia winy (często w kontekście kary lub oskarżenia).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna spędził w więzieniu dziesięć lat, choć został skazany <w>niewinnie</w>.",
                            "target_lang_translations": {
                                "ru": "Мужчина провёл в тюрьме десять лет, хотя был осуждён <w>невинно</w>."
                            }
                        },
                        {
                            "text": "Wiele osób cierpiało <w>niewinnie</w> podczas tej okrutnej wojny.",
                            "target_lang_translations": {
                                "ru": "Многие люди пострадали <w>невинно</w> во время этой жестокой войны."
                            }
                        }
                    ],
                    "synonyms": [
                        "niesłusznie",
                        "niesprawiedliwie",
                        "niezasłużenie"
                    ],
                    "common_phrases": [
                        "skazany niewinnie",
                        "cierpieć niewinnie",
                        "siedzieć niewinnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Незаслуженно, без совершения проступка (часто в контексте наказания или обвинения)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "невинно",
                                "target_lang_sense_clarification": "незаслуженно, без вины"
                            },
                            {
                                "target_lang_word": "без вины",
                                "target_lang_sense_clarification": "отсутствие вины"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b38b51ef-8de3-3d2c-b86b-77451a452008",
                    "sense_definition": "W sposób, który nie powoduje złych skutków ani zagrożenia, mimo że może na taki wyglądać.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta cała historia zaczęła się całkiem <w>niewinnie</w>, ale skończyła wielką awanturą.",
                            "target_lang_translations": {
                                "ru": "Вся эта история началась совсем <w>невинно</w>, но закончилась крупным скандалом."
                            }
                        },
                        {
                            "text": "Choroba wyglądała początkowo <w>niewinnie</w>, lecz szybko się rozwinęła.",
                            "target_lang_translations": {
                                "ru": "Поначалу болезнь выглядела <w>безобидно</w>, но быстро прогрессировала."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zacząć się niewinnie",
                        "brzmieć niewinnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Безобидно, не вызывая плохих последствий или угрозы, даже если кажется иначе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "безобидно",
                                "target_lang_sense_clarification": "не представляя угрозы"
                            },
                            {
                                "target_lang_word": "невинно",
                                "target_lang_sense_clarification": "казалось бы, без последствий"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
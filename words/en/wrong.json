    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "cac6049e-f9fa-3ba0-9448-dad8ec9919c8",
                    "sense_definition": "Incorrect or untrue; not agreeing with facts or reality.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "incorrect_fact",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Your answer to the math question is <w>wrong</w>.",
                            "target_lang_translations": {
                                "ru": "Твой ответ на задачу по математике <w>неправильный</w>."
                            }
                        },
                        {
                            "text": "I was <w>wrong</w> about the time the movie starts.",
                            "target_lang_translations": {
                                "ru": "Я <w>ошибся</w> насчет времени начала фильма."
                            }
                        }
                    ],
                    "synonyms": [
                        "false"
                    ],
                    "antonyms": [
                        "right"
                    ],
                    "common_phrases": [
                        "wrong answer",
                        "prove wrong",
                        "get it wrong"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неверный или ложный; не соответствующий фактам или реальности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неправильный",
                                "target_lang_sense_clarification": "содержащий ошибки"
                            },
                            {
                                "target_lang_word": "неверный",
                                "target_lang_sense_clarification": "ложный, ошибочный"
                            },
                            {
                                "target_lang_word": "ошибочный",
                                "target_lang_sense_clarification": "являющийся ошибкой"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "833eb285-24d5-3df1-9960-be7c0c30ae68",
                    "sense_definition": "Not working properly; having a problem or defect.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "problematic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is something <w>wrong</w> with my computer.",
                            "target_lang_translations": {
                                "ru": "С моим компьютером что-то <w>не так</w>."
                            }
                        },
                        {
                            "text": "Please tell me what is <w>wrong</w>.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, скажи мне, что <w>случилось</w>."
                            }
                        }
                    ],
                    "synonyms": [

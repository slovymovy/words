    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "2196c833-ef91-3a53-b67e-8934fb930989",
                    "sense_definition": "Geeft een eindpunt in tijd, ruimte of hoeveelheid aan.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "limit_endpoint",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De winkel is open van negen <tot> vijf uur.",
                            "target_lang_translations": {
                                "en": "The shop is open from nine <w>to</w> five o'clock.",
                                "ru": "Магазин открыт с девяти <до> пяти часов."
                            }
                        },
                        {
                            "text": "We liepen <tot> het einde van de straat.",
                            "target_lang_translations": {
                                "en": "We walked <w>to</w> the end of the street.",
                                "ru": "Мы шли <до> конца улицы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "tot ziens",
                        "tot morgen",
                        "tot nu toe",
                        "van ... tot ...",
                        "tot en met"
                    ],
                    "target_lang_definitions": {
                        "en": "Indicates an endpoint in time, space, or quantity.",
                        "ru": "Указывает на конечную точку во времени, пространстве или количестве."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "until",
                                "target_lang_sense_clarification": "indicating a time limit"
                            },
                            {
                                "target_lang_word": "to",
                                "target_lang_sense_clarification": "indicating a spatial endpoint or range"
                            },
                            {
                                "target_lang_word": "up to",
                                "target_lang_sense_clarification": "indicating a maximum quantity or degree"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "до",
                                "target_lang_sense_clarification": "указывает на предел"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a912d869-4da3-39c1-8480-92266ab04e05",
                    "sense_definition": "Geeft aan waartoe iets leidt, wat het resultaat is of waarin iets verandert.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "result_change",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij werd veroordeeld <tot> een gevangenisstraf.",
                            "target_lang_translations": {

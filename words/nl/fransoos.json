    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8ac45966-73e5-3a19-954c-d5184d643ddf",
                    "sense_definition": "Een persoon met de Franse nationaliteit of van Franse afkomst.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De vrolijke <w>fransoos</w> vertelde honderduit over zijn land.",
                            "target_lang_translations": {
                                "en": "The cheerful <w>Frenchman</w> spoke at length about his country.",
                                "ru": "Веселый <w>француз</w> без умолку рассказывал о своей стране."
                            }
                        },
                        {
                            "text": "Tijdens de vakantie hebben we een aardige <w>fransoos</w> ontmoet.",
                            "target_lang_translations": {
                                "en": "During the holiday we met a nice <w>Frenchman</w>.",
                                "ru": "Во время отпуска мы встретили приятного <w>француза</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "Fransman"
                    ],
                    "target_lang_definitions": {
                        "en": "A person of French nationality or of French descent.",
                        "ru": "Человек французской национальности или французского происхождения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Frenchman",
                                "target_lang_sense_clarification": "standard term"
                            },
                            {
                                "target_lang_word": "French fellow",
                                "target_lang_sense_clarification": "slightly informal"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "француз",
                                "target_lang_sense_clarification": "национальность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7d8bb417-3fcd-3d83-99e7-526c9f59d603",
                    "sense_definition": "Een minachtende benaming voor iemand met de Franse nationaliteit.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wat denken die <w>fransozen</w> wel!",
                            "target_lang_translations": {
                                "en": "Who do those <w>Frenchies</w> think they are!",
                                "ru": "Что эти <w>французишки</w> о себе возомнили!"
                            }
                        },
                        {
                            "text": "Ik ergerde me groen en geel aan de houding van die <w>fransoos</w>.",
                            "target_lang_translations": {
                                "en": "I was incredibly annoyed by the attitude of that <w>Frenchy</w>.",
                                "ru": "Меня страшно раздражало поведение этого <w>французишки</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A derogatory term for someone of French nationality.",
                        "ru": "Презрительное название человека французской национальности."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Frenchy",
                                "target_lang_sense_clarification": "informal, often derogatory"
                            },
                            {
                                "target_lang_word": "Frog",
                                "target_lang_sense_clarification": "offensive slang"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "французишка",
                                "target_lang_sense_clarification": "пренебрежительно"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "874798d0-2d86-32fc-98ad-da3180a2b4ac",
                    "sense_definition": "Сохранять равновесие или положение, ухватившись за что-либо руками.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_hold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В автобусе нужно обязательно <держаться> за поручень.",
                            "target_lang_translations": {
                                "en": "In the bus, you must <hold on> to the handrail."
                            }
                        },
                        {
                            "text": "Ребёнок крепко <держался> за мамину руку.",
                            "target_lang_translations": {
                                "en": "The child <held> tightly onto mom's hand."
                            }
                        }
                    ],
                    "synonyms": [
                        "сохраняться"
                    ],
                    "antonyms": [
                        "падать",
                        "отпускать"
                    ],
                    "common_phrases": [
                        "держаться за руки",
                        "держаться за перила",
                        "крепко держаться"
                    ],
                    "target_lang_definitions": {
                        "en": "To maintain balance or position by grabbing something with one's hands."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hold on",
                                "target_lang_sense_clarification": "to grasp or grip firmly"
                            },
                            {
                                "target_lang_word": "hold",
                                "target_lang_sense_clarification": "to grasp or grip"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a50cb323-cffe-3417-8143-5997e4481356",
                    "sense_definition": "Вести себя определённым образом, иметь определённый вид или осанку.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На собеседовании он <держался> очень уверенно и спокойно.",
                            "target_lang_translations": {
                                "en": "At the interview, he <behaved> very confidently and calmly."
                            }
                        },
                        {
                            "text": "Несмотря на усталость, солдат <держался> прямо.",
                            "target_lang_translations": {
                                "en": "Despite fatigue, the soldier <held himself> upright."
                            }
                        }
                    ],
                    "synonyms": [
                        "вести себя"
                    ],
                    "common_phrases": [
                        "держаться уверенно",
                        "держаться молодцом",
                        "держаться скромно",
                        "держаться прямо"
                    ],
                    "target_lang_definitions": {
                        "en": "To behave in a certain way, to have a certain appearance or posture."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "behave",
                                "target_lang_sense_clarification": "to conduct oneself"
                            },
                            {
                                "target_lang_word": "hold",
                                "target_lang_sense_clarification": "to bear or carry oneself (reflexive)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e2a865ed-18ac-357d-ba26-cf18b207f28c",
                    "sense_definition": "Находиться в определённом месте, не отходить далеко или следовать определённой стороне при движении.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "location_direction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Во время экскурсии прошу всех <держаться> вместе.",
                            "target_lang_translations": {
                                "en": "During the excursion, I ask everyone to <stick> together."
                            }
                        },
                        {
                            "text": "Водитель должен <держаться> правой стороны дороги.",
                            "target_lang_translations": {
                                "en": "The driver must <keep> to the right side of the road."
                            }
                        }
                    ],
                    "synonyms": [
                        "оставаться",
                        "придерживаться"
                    ],
                    "common_phrases": [
                        "держаться вместе",
                        "держаться в стороне",
                        "держаться правой стороны",
                        "держаться берега"
                    ],
                    "target_lang_definitions": {
                        "en": "To be in a certain place, not to move far away, or to follow a certain side when moving."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stick",
                                "target_lang_sense_clarification": "to remain together or in a place"
                            },
                            {
                                "target_lang_word": "keep",
                                "target_lang_sense_clarification": "to remain in a specific position or direction"
                            },
                            {
                                "target_lang_word": "stay",
                                "target_lang_sense_clarification": "to remain in a particular place"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1af116b-ca78-3221-a1d6-b57eb39d8d4c",
                    "sense_definition": "Сохранять силу духа, не сдаваться в трудной ситуации; оказывать сопротивление.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "resilience",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Враг наступает, но наши войска продолжают <держаться>.",
                            "target_lang_translations": {
                                "en": "The enemy is advancing, but our troops continue to <hold out>."
                            }
                        },
                        {
                            "text": "Ты должен <держаться> и не падать духом ради семьи.",
                            "target_lang_translations": {
                                "en": "You must <hold on> and not lose heart for the sake of your family."
                            }
                        }
                    ],
                    "synonyms": [
                        "крепиться",
                        "сопротивляться",
                        "сохраняться"
                    ],
                    "antonyms": [
                        "сдаваться"
                    ],
                    "common_phrases": [
                        "держись",
                        "держаться до последнего",
                        "держаться из последних сил"
                    ],
                    "target_lang_definitions": {
                        "en": "To maintain spirit, not to give up in a difficult situation; to resist."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hold out",
                                "target_lang_sense_clarification": "to survive, endure, or resist"
                            },
                            {
                                "target_lang_word": "hold on",
                                "target_lang_sense_clarification": "to persist or endure"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6d4097c1-32e9-3bb6-bd60-2bc0aa886904",
                    "sense_definition": "Продолжать существовать, не исчезать и не меняться (о погоде, состоянии, запахе и т.п.).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хорошая погода <держится> уже целую неделю.",
                            "target_lang_translations": {
                                "en": "Good weather has been <holding> for a whole week already."
                            }
                        },
                        {
                            "text": "Высокая температура у больного <держалась> три дня.",
                            "target_lang_translations": {
                                "en": "The patient's high fever <lasted> three days."
                            }
                        }
                    ],
                    "synonyms": [
                        "сохраняться",
                        "оставаться"
                    ],
                    "antonyms": [
                        "исчезать",
                        "проходить"
                    ],
                    "common_phrases": [
                        "погода держится",
                        "мороз держится",
                        "запах держится"
                    ],
                    "target_lang_definitions": {
                        "en": "To continue to exist, not to disappear or change (about weather, state, smell, etc.)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "last",
                                "target_lang_sense_clarification": "to endure or continue over time"
                            },
                            {
                                "target_lang_word": "hold",
                                "target_lang_sense_clarification": "to remain valid or in a state"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2e070417-fa18-355f-b872-fbd5a88ab107",
                    "sense_definition": "Следовать чему-либо (правилам, советам), не отступать от своих принципов или мнений.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "adherence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я предпочитаю <держаться> своего мнения в этом вопросе.",
                            "target_lang_translations": {
                                "en": "I prefer to <stick> to my opinion on this matter."
                            }
                        },
                        {
                            "text": "Чтобы похудеть, нужно строго <держаться> диеты.",
                            "target_lang_translations": {
                                "en": "To lose weight, one must strictly <stick> to a diet."
                            }
                        }
                    ],
                    "synonyms": [
                        "придерживаться",
                        "поддерживаться"
                    ],
                    "antonyms": [
                        "нарушать"
                    ],
                    "common_phrases": [
                        "держаться курса",
                        "держаться правил",
                        "держаться мнения"
                    ],
                    "target_lang_definitions": {
                        "en": "To follow something (rules, advice), not to deviate from one's principles or opinions."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stick",
                                "target_lang_sense_clarification": "to remain loyal or firm to something"
                            },
                            {
                                "target_lang_word": "adhere",
                                "target_lang_sense_clarification": "to follow or observe (rules, plans)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "77235aed-1385-3476-ac89-8fdeb035c17f",
                    "sense_definition": "Быть прикреплённым к чему-либо, не падать благодаря опоре или креплению.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "physical_hold",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Старая картина <держится> на стене на одном гвозде.",
                            "target_lang_translations": {
                                "en": "The old painting <stays> on the wall on a single nail."
                            }
                        },
                        {
                            "text": "Пуговица на пальто еле <держится>.",
                            "target_lang_translations": {
                                "en": "The button on the coat is barely <holding on>."
                            }
                        }
                    ],
                    "synonyms": [
                        "крепиться",
                        "сохраняться"
                    ],
                    "antonyms": [
                        "отваливаться"
                    ],
                    "common_phrases": [
                        "держаться на клею",
                        "держаться на честном слове",
                        "еле держаться"
                    ],
                    "target_lang_definitions": {
                        "en": "To be attached to something, not to fall thanks to support or fastening."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hold on",
                                "target_lang_sense_clarification": "to remain attached or supported"
                            },
                            {
                                "target_lang_word": "stay",
                                "target_lang_sense_clarification": "to remain in place"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
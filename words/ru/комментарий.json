    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ef61806f-c935-3402-8f0c-161216796147",
                    "sense_definition": "Мнение, рассуждение или замечание, высказанное по поводу чего-либо (часто в интернете или в беседе).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я решил написать короткий <комментарий> под этой интересной статьёй.",
                            "target_lang_translations": {
                                "en": "I decided to write a short <comment> under this interesting article."
                            }
                        },
                        {
                            "text": "Политик ответил на вопросы журналистов фразой «без <комментариев>».",
                            "target_lang_translations": {
                                "en": "The politician answered the journalists' questions with the phrase \"no <comment>\"."
                            }
                        },
                        {
                            "text": "Твой <комментарий> помог мне лучше понять ситуацию.",
                            "target_lang_translations": {
                                "en": "Your <comment> helped me understand the situation better."
                            }
                        }
                    ],
                    "synonyms": [
                        "рассуждения",
                        "коммент"
                    ],
                    "common_phrases": [
                        "оставить комментарий",
                        "без комментариев",
                        "написать комментарий",
                        "лишние комментарии"
                    ],
                    "target_lang_definitions": {
                        "en": "An opinion, reasoning, or remark expressed about something (often on the internet or in conversation)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "comment",
                                "target_lang_sense_clarification": "a spoken or written remark"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2959ced3-62b5-3708-9052-ec2e5a11ab47",
                    "sense_definition": "Пояснения или примечания к тексту, книге или документу, помогающие понять его смысл.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В конце учебника мы нашли полезные <комментарии> к сложным правилам.",
                            "target_lang_translations": {
                                "en": "At the end of the textbook, we found useful <commentaries> on the complex rules."
                            }
                        },
                        {
                            "text": "Юристы подготовили подробные <комментарии> к новому закону.",
                            "target_lang_translations": {
                                "en": "The lawyers prepared detailed <commentaries> on the new law."
                            }
                        }
                    ],
                    "synonyms": [
                        "пояснения",
                        "примечания",
                        "толкование"
                    ],
                    "common_phrases": [
                        "комментарии к тексту",
                        "дать комментарий",
                        "развёрнутый комментарий"
                    ],
                    "target_lang_definitions": {
                        "en": "Explanations or notes to a text, book, or document helping to understand its meaning."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "commentary",
                                "target_lang_sense_clarification": "series of explanations or notes"
                            }
                        ]
                    }
                }
            ]
        }
    ]

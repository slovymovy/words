    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "b04ec04c-a0be-360b-89c5-fd868d8c092c",
                    "sense_definition": "Each of the two; one and the other; referring to two individuals or items.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_two_items",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I held the heavy box with <w>both</w> hands.",
                            "target_lang_translations": {
                                "ru": "Я держал тяжелую коробку <w>обеими</w> руками."
                            }
                        },
                        {
                            "text": "<w>Both</w> children are playing in the garden.",
                            "target_lang_translations": {
                                "ru": "<w>Оба</w> ребенка играют в саду."
                            }
                        }
                    ],
                    "synonyms": [
                        "the two"
                    ],
                    "antonyms": [
                        "neither"
                    ],
                    "common_phrases": [
                        "both sides",
                        "both ways",
                        "both eyes",
                        "both hands"
                    ],
                    "target_lang_definitions": {
                        "ru": "Каждый из двух; и тот, и другой (употребляется с существительными)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оба",
                                "target_lang_sense_clarification": "мужской и средний род"
                            },
                            {
                                "target_lang_word": "обе",
                                "target_lang_sense_clarification": "женский род"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "3795bc40-33e6-370e-bed9-5f36b9a132b6",
                    "sense_definition": "Each of the two people or things previously mentioned.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "group_two_items",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I have two sisters and <w>both</w> live in London.",
                            "target_lang_translations": {
                                "ru": "У меня две сестры, и <w>обе</w> живут в Лондоне."
                            }
                        },
                        {
                            "text": "Do you want tea or coffee? — I want <w>both</w>.",
                            "target_lang_translations": {
                                "ru": "Ты хочешь чай или кофе? — Я хочу <w>и то, и другое</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "the two"
                    ],
                    "antonyms": [
                        "neither"
                    ],
                    "common_phrases": [
                        "both of them",
                        "both of us",
                        "both of you"
                    ],
                    "target_lang_definitions": {
                        "ru": "Каждый из двух людей или предметов, упомянутых ранее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оба",
                                "target_lang_sense_clarification": "мужской и средний род"
                            },
                            {
                                "target_lang_word": "обе",
                                "target_lang_sense_clarification": "женский род"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "fbfc8f66-b822-3b7a-8212-fad1b489a554",
                    "sense_definition": "Used before the first of two alternatives to emphasize that the statement applies to each of them (used with 'and').",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She can <w>both</w> sing and dance very well.",
                            "target_lang_translations": {
                                "ru": "Она умеет <w>и</w> петь, и танцевать очень хорошо."
                            }
                        },
                        {
                            "text": "<w>Both</w> you and I are students at this university.",
                            "target_lang_translations": {
                                "ru": "<w>И</w> ты, и я — студенты этого университета."
                            }
                        }
                    ],
                    "common_phrases": [
                        "both ... and"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется перед первым из двух однородных членов предложения для усиления (в сочетании с 'and')."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "и... и...",
                                "target_lang_sense_clarification": "союз"
                            },
                            {
                                "target_lang_word": "как..., так и...",
                                "target_lang_sense_clarification": "союз"
                            }
                        ]
                    }
                }
            ]
        },

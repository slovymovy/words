    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0fd71c4e-b734-3087-9d32-1bd50391f60c",
                    "sense_definition": "To clean something using water and usually soap or detergent.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "cleaning_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need to <w>wash</w> the car because it is covered in mud.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>помыть</w> машину, потому что она вся в грязи."
                            }
                        },
                        {
                            "text": "Please <w>wash</w> the vegetables thoroughly before you cook them.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, тщательно <w>помой</w> овощи перед тем, как их готовить."
                            }
                        }
                    ],
                    "synonyms": [
                        "lavatory"
                    ],
                    "common_phrases": [
                        "wash the dishes",
                        "wash your hands",
                        "wash clothes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чистить что-либо, используя воду и обычно мыло или моющее средство."
                    },
                    "translations": {

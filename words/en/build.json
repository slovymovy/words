{
    "word_family": [
        "builder",
        "building",
        "rebuild",
        "overbuild",
        "underbuild",
        "upbuild"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "622861d5-7c87-3a17-900e-50c2d3cb7211",
                    "sense_definition": "To form something by combining materials or parts; to construct.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "construction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They plan to <w>build</w> a new school in the village next year.",
                            "target_lang_translations": {
                                "ru": "Они планируют <w>построить</w> новую школу в деревне в следующем году."
                            }
                        },
                        {
                            "text": "Birds <w>build</w> their nests using twigs and leaves.",
                            "target_lang_translations": {
                                "ru": "Птицы <w>строят</w> свои гнезда, используя ветки и листья."
                            }
                        }
                    ],
                    "synonyms": [
                        "construct",
                        "erect",
                        "assemble",
                        "put together",
                        "make"
                    ],
                    "antonyms": [
                        "demolish",
                        "destroy",
                        "tear down",
                        "dismantle",
                        "knock down",
                        "raze"
                    ],
                    "common_phrases": [
                        "build a house",
                        "build a bridge",
                        "build from scratch"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать что-либо, соединяя материалы или части; конструировать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стро́ить",
                                "target_lang_sense_clarification": "создавать здания или конструкции"
                            },
                            {
                                "target_lang_word": "сооружа́ть",
                                "target_lang_sense_clarification": "возводить (обычно о крупных объектах)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "816689c0-b910-3df4-948b-638b7c45530d",
                    "sense_definition": "To develop or create something abstract, such as a business, career, or relationship, over a period of time.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abstract_development",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It takes a lot of hard work to <w>build</w> a successful company.",
                            "target_lang_translations": {
                                "ru": "Требуется много упорного труда, чтобы <w>построить</w> успешную компанию."
                            }
                        },
                        {
                            "text": "We are trying to <w>build</w> a better future for our children.",
                            "target_lang_translations": {
                                "ru": "Мы пытаемся <w>построить</w> лучшее будущее для наших детей."
                            }
                        }
                    ],
                    "synonyms": [
                        "create",
                        "develop",
                        "establish",
                        "form"
                    ],
                    "antonyms": [
                        "destroy",
                        "ruin"
                    ],
                    "common_phrases": [
                        "build a career",
                        "build a relationship",
                        "build a life",
                        "build a business"
                    ],
                    "target_lang_definitions": {
                        "ru": "Развивать или создавать что-либо абстрактное (бизнес, карьеру, отношения) в течение определенного времени."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стро́ить",
                                "target_lang_sense_clarification": "созидать (карьеру, отношения, планы)"
                            },
                            {
                                "target_lang_word": "создава́ть",
                                "target_lang_sense_clarification": "делать существующим"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc917a8c-742a-36e5-8f2f-b5044596f525",
                    "sense_definition": "To increase or strengthen something gradually.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "strengthen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team needs to <w>build</w> confidence before the next match.",
                            "target_lang_translations": {
                                "ru": "Команде нужно <w>укрепить</w> уверенность перед следующим матчем."
                            }
                        },
                        {
                            "text": "Regular exercise helps to <w>build</w> muscle and stamina.",
                            "target_lang_translations": {
                                "ru": "Регулярные упражнения помогают <w>нарастить</w> мышцы и выносливость."
                            }
                        }
                    ],
                    "synonyms": [
                        "strengthen",
                        "increase",
                        "boost",
                        "enhance"
                    ],
                    "antonyms": [
                        "weaken",
                        "decrease",
                        "dissipate"
                    ],
                    "common_phrases": [
                        "build confidence",
                        "build trust",
                        "build strength",
                        "build momentum"
                    ],
                    "target_lang_definitions": {
                        "ru": "Постепенно увеличивать или укреплять что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укрепля́ть",
                                "target_lang_sense_clarification": "делать более сильным или устойчивым"
                            },
                            {
                                "target_lang_word": "нара́щивать",
                                "target_lang_sense_clarification": "увеличивать количество или объем (мышцы, темп)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8a0e4a13-8260-3103-81b4-a5e824179ed0",
                    "sense_definition": "To develop in magnitude or extent; to become more intense.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "intransitive_growth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The excitement began to <w>build</w> as the concert started.",
                            "target_lang_translations": {
                                "ru": "Волнение начало <w>нарастать</w>, когда начался концерт."
                            }
                        },
                        {
                            "text": "Pressure is <w>building</w> on the government to change the law.",
                            "target_lang_translations": {
                                "ru": "Давление на правительство <w>растет</w> с требованием изменить закон."
                            }
                        }
                    ],
                    "synonyms": [
                        "increase",
                        "intensify",
                        "grow",
                        "mount",
                        "rise"
                    ],
                    "antonyms": [
                        "decrease",
                        "dissipate",
                        "weaken"
                    ],
                    "common_phrases": [
                        "tension builds",
                        "pressure builds",
                        "excitement builds"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрастаться или усиливаться; становиться более интенсивным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нараста́ть",
                                "target_lang_sense_clarification": "увеличиваться (о напряжении, звуке, чувствах)"
                            },
                            {
                                "target_lang_word": "уси́ливаться",
                                "target_lang_sense_clarification": "становиться сильнее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "70a76ead-3723-3dee-a84a-05b24c9fcd29",
                    "sense_definition": "To establish a basis for something; to use as a foundation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "basis",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We must <w>build</w> on our previous success to achieve our goals.",
                            "target_lang_translations": {
                                "ru": "Мы должны <w>опираться</w> на наш предыдущий успех, чтобы достичь наших целей."
                            }
                        },
                        {
                            "text": "Her theory is <w>built</w> on years of scientific research.",
                            "target_lang_translations": {
                                "ru": "Ее теория <w>построена</w> на годах научных исследований."
                            }
                        }
                    ],
                    "synonyms": [
                        "base",
                        "found",
                        "ground"
                    ],
                    "common_phrases": [
                        "build on",
                        "build upon"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать основу для чего-либо; использовать в качестве фундамента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "осно́вываться",
                                "target_lang_sense_clarification": "базироваться на чем-либо"
                            },
                            {
                                "target_lang_word": "опира́ться",
                                "target_lang_sense_clarification": "использовать как опору для развития"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "91b43179-1842-381b-bc6f-e8466a4956f2",
                    "sense_definition": "To construct software by compiling its source code.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "computing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The developer needs to <w>build</w> the application before testing it.",
                            "target_lang_translations": {
                                "ru": "Разработчику нужно <w>собрать</w> приложение перед его тестированием."
                            }
                        },
                        {
                            "text": "It took several minutes to <w>build</w> the entire project.",
                            "target_lang_translations": {
                                "ru": "Заняло несколько минут, чтобы <w>собрать</w> весь проект."
                            }
                        }
                    ],
                    "synonyms": [
                        "compile"
                    ],
                    "common_phrases": [
                        "build the code",
                        "build the project"
                    ],
                    "target_lang_definitions": {
                        "ru": "Создавать программное обеспечение путем компиляции исходного кода."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "собира́ть",
                                "target_lang_sense_clarification": "компилировать код программы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fdc740b8-9646-30c6-bdcf-a314f3a19e52",
                    "sense_definition": "The physical shape, size, or structure of a human or animal body.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "physique",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rugby player has a very strong and muscular <w>build</w>.",
                            "target_lang_translations": {
                                "ru": "У регбиста очень сильное и мускулистое <w>телосложение</w>."
                            }
                        },
                        {
                            "text": "She is of a slight <w>build</w> but is surprisingly strong.",
                            "target_lang_translations": {
                                "ru": "Она хрупкого <w>телосложения</w>, но удивительно сильная."
                            }
                        }
                    ],
                    "synonyms": [
                        "physique",
                        "figure",
                        "frame",
                        "body type"
                    ],
                    "common_phrases": [
                        "athletic build",
                        "medium build",
                        "stocky build",
                        "slight build"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическая форма, размер или строение тела человека или животного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "телосложе́ние",
                                "target_lang_sense_clarification": "строение тела, фигура"
                            },
                            {
                                "target_lang_word": "компле́кция",
                                "target_lang_sense_clarification": "общая характеристика строения тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "50a390c8-aaff-31df-81e2-aedb3f9a863b",
                    "sense_definition": "A specific version of a software product created during development.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "software_version",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The latest <w>build</w> of the operating system fixes several bugs.",
                            "target_lang_translations": {
                                "ru": "Последняя <w>сборка</w> операционной системы исправляет несколько ошибок."
                            }
                        },
                        {
                            "text": "Testers are checking the new beta <w>build</w> for errors.",
                            "target_lang_translations": {
                                "ru": "Тестировщики проверяют новую бета-<w>сборку</w> на наличие ошибок."
                            }
                        }
                    ],
                    "synonyms": [
                        "version",
                        "release"
                    ],
                    "common_phrases": [
                        "latest build",
                        "stable build",
                        "nightly build",
                        "beta build"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретная версия программного продукта, созданная в процессе разработки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбо́рка",
                                "target_lang_sense_clarification": "версия программы после компиляции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "becfaa4e-89ce-31e8-8522-a410a6bb33e9",
                    "sense_definition": "A specific configuration of a character's skills, items, or attributes in a game.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "gaming_config",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am trying a new defensive <w>build</w> for my warrior character.",
                            "target_lang_translations": {
                                "ru": "Я пробую новую защитную <w>сборку</w> для моего персонажа-воина."
                            }
                        },
                        {
                            "text": "Players often share their best character <w>builds</w> online.",
                            "target_lang_translations": {
                                "ru": "Игроки часто делятся своими лучшими <w>сборками</w> персонажей в интернете."
                            }
                        }
                    ],
                    "common_phrases": [
                        "character build",
                        "tank build",
                        "optimal build"
                    ],
                    "target_lang_definitions": {
                        "ru": "Набор характеристик, навыков и предметов персонажа в игре."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сбо́рка",
                                "target_lang_sense_clarification": "конфигурация персонажа"
                            },
                            {
                                "target_lang_word": "билд",
                                "target_lang_sense_clarification": "игровой сленг"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
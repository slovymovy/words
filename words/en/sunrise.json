    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9fba89fc-f02a-30b5-a7aa-f634b537e93b",
                    "sense_definition": "The time of day when the sun appears above the eastern horizon.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "time_of_day",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I'll meet you at the docks at <w>sunrise</w>.",
                            "target_lang_translations": {
                                "ru": "Я встречу тебя в доках на <w>рассвете</w>."
                            }
                        },
                        {
                            "text": "Farmers often wake up before <w>sunrise</w> to start their work.",
                            "target_lang_translations": {
                                "ru": "Фермеры часто просыпаются до <w>восхода</w>, чтобы начать работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "sunup",
                        "dawn",
                        "daybreak",
                        "first light",
                        "crack of dawn"
                    ],
                    "antonyms": [
                        "sunset",
                        "sundown"
                    ],
                    "common_phrases": [
                        "at sunrise",
                        "before sunrise",
                        "sunrise to sunset"
                    ],
                    "target_lang_definitions": {
                        "ru": "Время суток, когда солнце появляется над восточным горизонтом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "восход",
                                "target_lang_sense_clarification": "появление солнца"
                            },
                            {
                                "target_lang_word": "рассвет",
                                "target_lang_sense_clarification": "время перед восходом"
                            },
                            {
                                "target_lang_word": "восход солнца",
                                "target_lang_sense_clarification": "время суток"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af268e28-ee20-32e8-8589-68b435336c69",
                    "sense_definition": "The colorful display in the sky when the sun rises.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time_of_day",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Did you see the beautiful <w>sunrise</w> this morning?",
                            "target_lang_translations": {
                                "ru": "Ты видел красивый <w>рассвет</w> сегодня утром?"
                            }
                        },
                        {
                            "text": "We sat on the beach to watch the <w>sunrise</w> over the ocean.",
                            "target_lang_translations": {
                                "ru": "Мы сидели на пляже, чтобы наблюдать <w>восход</w> над океаном."
                            }
                        }
                    ],
                    "antonyms": [
                        "sunset"
                    ],
                    "common_phrases": [
                        "beautiful sunrise",
                        "watch the sunrise",
                        "golden sunrise"
                    ],
                    "target_lang_definitions": {
                        "ru": "Красочное зрелище на небе во время восхода солнца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заря",
                                "target_lang_sense_clarification": "свечение неба"
                            },
                            {
                                "target_lang_word": "рассвет",
                                "target_lang_sense_clarification": "картина неба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53607edc-cc31-3cf8-929a-1d95228e7c33",
                    "sense_definition": "A fresh beginning or a time of awakening.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was the <w>sunrise</w> of her spirit.",
                            "target_lang_translations": {
                                "ru": "Это был <w>рассвет</w> её духа."
                            }
                        },
                        {
                            "text": "The invention marked the <w>sunrise</w> of the digital age.",
                            "target_lang_translations": {
                                "ru": "Это изобретение ознаменовало <w>зарю</w> цифровой эры."
                            }
                        }
                    ],
                    "synonyms": [
                        "dawn"
                    ],
                    "common_phrases": [
                        "sunrise of a new era"
                    ],
                    "target_lang_definitions": {
                        "ru": "Новое начало или время пробуждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заря",
                                "target_lang_sense_clarification": "зарождение чего-либо"
                            },
                            {
                                "target_lang_word": "рассвет",
                                "target_lang_sense_clarification": "начало расцвета"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {

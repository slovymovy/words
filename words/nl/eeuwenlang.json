{
    "word_family": [
        "eeuw",
        "lang",
        "eeuwig",
        "eeuwigheid"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6d6edef7-065b-3515-a531-cc2a697f2ac6",
                    "sense_definition": "Die of dat honderden jaren duurt of geduurd heeft; een periode van meerdere eeuwen beslaand.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "literal_centuries",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er woedde een <w>eeuwenlange</w> strijd tussen die twee volken om het grondgebied.",
                            "target_lang_translations": {
                                "ru": "Между этими двумя народами шла <w>многовековая</w> борьба за территорию."
                            }
                        },
                        {
                            "text": "Het land kent een <w>eeuwenlange</w> traditie van handel en scheepvaart.",
                            "target_lang_translations": {
                                "ru": "У страны есть <w>многовековая</w> традиция торговли и судоходства."
                            }
                        }
                    ],
                    "synonyms": [
                        "langdurig"
                    ],
                    "common_phrases": [
                        "een eeuwenlange traditie",
                        "een eeuwenlange geschiedenis",
                        "een eeuwenlange strijd"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длившийся или длящийся сотни лет; охватывающий период в несколько веков."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "многовековой",
                                "target_lang_sense_clarification": "охватывающий много веков"
                            },
                            {
                                "target_lang_word": "вековой",
                                "target_lang_sense_clarification": "существующий очень долго"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "ad4391b0-fcd3-396c-a0e7-55cbc10d153d",
                    "sense_definition": "Gedurende een periode van honderden jaren.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "literal_centuries",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De stad Amsterdam bestaat al <w>eeuwenlang</w> en heeft een rijke historie.",
                            "target_lang_translations": {
                                "ru": "Город Амстердам существует уже <w>столетиями</w> и имеет богатую историю."
                            }
                        },
                        {
                            "text": "Deze bijzondere techniek wordt al <w>eeuwenlang</w> door ambachtslieden toegepast.",
                            "target_lang_translations": {
                                "ru": "Эта особая техника применяется ремесленниками уже <w>веками</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "al eeuwenlang"
                    ],
                    "target_lang_definitions": {
                        "ru": "В течение периода, длящегося сотни лет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "веками",
                                "target_lang_sense_clarification": "на протяжении веков"
                            },
                            {
                                "target_lang_word": "столетиями",
                                "target_lang_sense_clarification": "на протяжении столетий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "019254f7-5c12-33ca-9b26-299447b4bf26",
                    "sense_definition": "Gedurende een zeer lange tijd, zonder dat het letterlijk om honderden jaren hoeft te gaan.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_long_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Waarom zouden we het nu veranderen als het al <w>eeuwenlang</w> zo is?",
                            "target_lang_translations": {
                                "ru": "Зачем нам менять это сейчас, если так было <w>веками</w>?"
                            }
                        },
                        {
                            "text": "Voor mijn gevoel sta ik hier al <w>eeuwenlang</w> op de bus te wachten.",
                            "target_lang_translations": {
                                "ru": "По моим ощущениям, я жду здесь автобус уже <w>целую вечность</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "al eeuwenlang"
                    ],
                    "target_lang_definitions": {
                        "ru": "В течение очень долгого времени, не обязательно буквально сотни лет."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "целую вечность",
                                "target_lang_sense_clarification": "очень долго (фигурально)"
                            },
                            {
                                "target_lang_word": "сто лет",
                                "target_lang_sense_clarification": "разговорное преувеличение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "opmaak"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "85135313-48f5-3492-927f-7fe675b85bc3",
                    "sense_definition": "Zichzelf of iemand anders mooi maken met cosmetica.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "verzorging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij <maakt> zich elke ochtend <op> voor de spiegel.",
                            "target_lang_translations": {
                                "ru": "Она <красится> каждое утро перед зеркалом.",
                                "en": "She <puts on makeup> every morning in front of the mirror."
                            }
                        },
                        {
                            "text": "Voor het feest heeft ze zich prachtig <opgemaakt>.",
                            "target_lang_translations": {
                                "ru": "Перед вечеринкой она прекрасно <накрасилась>.",
                                "en": "She <made herself up> beautifully for the party."
                            }
                        }
                    ],
                    "synonyms": [
                        "schminken"
                    ],
                    "antonyms": [
                        "afschminken"
                    ],
                    "common_phrases": [
                        "zich mooi opmaken",
                        "je ogen opmaken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Наносить косметику на себя или кого-то другого, чтобы выглядеть красивее.",
                        "en": "To apply cosmetics to oneself or someone else to look attractive."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "краситься",
                                "target_lang_sense_clarification": "наносить макияж на себя"
                            },
                            {
                                "target_lang_word": "накладывать макияж",
                                "target_lang_sense_clarification": "процесс нанесения косметики"
                            },
                            {
                                "target_lang_word": "красить",
                                "target_lang_sense_clarification": "наносить макияж кому-то другому"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "put on makeup",
                                "target_lang_sense_clarification": "general action of applying cosmetics"
                            },
                            {
                                "target_lang_word": "apply makeup",
                                "target_lang_sense_clarification": "more formal term for using cosmetics"
                            },
                            {
                                "target_lang_word": "make up",
                                "target_lang_sense_clarification": "to beautify with cosmetics"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42f93c36-03e1-37be-a50b-a633d2444a81",
                    "sense_definition": "Het beddengoed weer netjes leggen na het slapen of verschonen.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "huishouden",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wil jij vandaag de bedden <opmaken>?",
                            "target_lang_translations": {
                                "ru": "Ты хочешь сегодня <заправить> кровати?",
                                "en": "Do you want to <make> the beds today?"
                            }
                        },
                        {
                            "text": "In het hotel wordt je bed elke dag voor je <opgemaakt>.",
                            "target_lang_translations": {
                                "ru": "В отеле твою кровать <заправляют> для тебя каждый день.",
                                "en": "In the hotel, your bed is <made> for you every day."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het bed opmaken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить постель в порядок после сна или смены белья.",
                        "en": "To arrange the bedclothes neatly after sleeping."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заправлять",
                                "target_lang_sense_clarification": "приводить кровать в порядок"
                            },
                            {
                                "target_lang_word": "застилать",
                                "target_lang_sense_clarification": "покрывать кровать покрывалом"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "make",
                                "target_lang_sense_clarification": "specifically used with 'the bed'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4225c74d-404b-33bf-985d-b67ca6d1dc96",
                    "sense_definition": "Iets volledig verbruiken totdat er niets meer van over is.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "consumptie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten de restjes uit de koelkast <opmaken> voordat we op vakantie gaan.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <доесть> остатки из холодильника перед отъездом в отпуск.",
                                "en": "We need to <finish> the leftovers in the fridge before we go on vacation."
                            }
                        },
                        {
                            "text": "Hij heeft al zijn zakgeld in één dag <opgemaakt>.",
                            "target_lang_translations": {
                                "ru": "Он <потратил> все свои карманные деньги за один день.",
                                "en": "He <spent> all his pocket money in one day."
                            }
                        }
                    ],
                    "synonyms": [
                        "opgebruiken",
                        "consumeren"
                    ],
                    "antonyms": [
                        "bewaren",
                        "sparen"
                    ],
                    "common_phrases": [
                        "alles opmaken",
                        "geld opmaken",
                        "de voorraad opmaken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью использовать или потратить что-либо, пока ничего не останется.",
                        "en": "To use something completely until nothing is left."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тратить",
                                "target_lang_sense_clarification": "расходовать деньги"
                            },
                            {
                                "target_lang_word": "доедать",
                                "target_lang_sense_clarification": "съедать остатки еды"
                            },
                            {
                                "target_lang_word": "израсходовать",
                                "target_lang_sense_clarification": "полностью использовать запасы"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "use up",
                                "target_lang_sense_clarification": "to consume the entire supply"
                            },
                            {
                                "target_lang_word": "spend",
                                "target_lang_sense_clarification": "to use up money"
                            },
                            {
                                "target_lang_word": "finish",
                                "target_lang_sense_clarification": "to eat or drink the last of something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6696337c-a9a9-378d-b314-267de94c2719",
                    "sense_definition": "Een conclusie trekken uit wat men ziet, hoort of leest.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cognitie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uit zijn boze reactie kon ik <opmaken> dat hij het er niet mee eens was.",
                            "target_lang_translations": {
                                "ru": "Из его гневной реакции я мог <сделать вывод>, что он не согласен.",
                                "en": "From his angry reaction, I could <gather> that he did not agree."
                            }
                        },
                        {
                            "text": "Wat kunnen we uit deze teleurstellende cijfers <opmaken>?",
                            "target_lang_translations": {
                                "ru": "Какой <вывод> мы можем <сделать> из этих неутешительных цифр?",
                                "en": "What can we <conclude> from these disappointing figures?"
                            }
                        }
                    ],
                    "synonyms": [
                        "afleiden",
                        "concluderen"
                    ],
                    "common_phrases": [
                        "uit iets opmaken",
                        "hieruit valt op te maken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Делать вывод или заключение на основе увиденного, услышанного или прочитанного.",
                        "en": "To draw a conclusion based on observation or information."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делать вывод",
                                "target_lang_sense_clarification": "приходить к умозаключению"
                            },
                            {
                                "target_lang_word": "заключать",
                                "target_lang_sense_clarification": "выводить следствие"
                            },
                            {
                                "target_lang_word": "понимать",
                                "target_lang_sense_clarification": "осознавать смысл ситуации"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "infer",
                                "target_lang_sense_clarification": "to deduce from evidence"
                            },
                            {
                                "target_lang_word": "conclude",
                                "target_lang_sense_clarification": "to arrive at a judgment"
                            },
                            {
                                "target_lang_word": "gather",
                                "target_lang_sense_clarification": "to understand from context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b96e2bf5-fd19-3ca3-bc19-33d947ccf27c",
                    "sense_definition": "Een tekst of officieel document samenstellen, schrijven of vormgeven.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "administratie",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De notaris zal de akte voor de aankoop van het huis <opmaken>.",
                            "target_lang_translations": {
                                "ru": "Нотариус <составит> акт купли-продажи дома.",
                                "en": "The notary will <draw up> the deed for the purchase of the house."
                            }
                        },
                        {
                            "text": "De grafisch ontwerper <maakt> de pagina's van het tijdschrift <op>.",
                            "target_lang_translations": {
                                "ru": "Графический дизайнер <верстает> страницы журнала.",
                                "en": "The graphic designer <lays out> the pages of the magazine."
                            }
                        }
                    ],
                    "synonyms": [
                        "opstellen",
                        "vormgeven"
                    ],
                    "common_phrases": [
                        "de balans opmaken",
                        "een rekening opmaken",
                        "een testament opmaken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Составлять официальный документ или оформлять текст и дизайн.",
                        "en": "To prepare an official document or design the layout of a text."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "составлять",
                                "target_lang_sense_clarification": "писать официальный документ"
                            },
                            {
                                "target_lang_word": "оформлять",
                                "target_lang_sense_clarification": "придавать внешний вид"
                            },
                            {
                                "target_lang_word": "верстать",
                                "target_lang_sense_clarification": "компоновать страницы издания"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "draw up",
                                "target_lang_sense_clarification": "to compose a legal or official document"
                            },
                            {
                                "target_lang_word": "draft",
                                "target_lang_sense_clarification": "to write a preliminary version"
                            },
                            {
                                "target_lang_word": "lay out",
                                "target_lang_sense_clarification": "to arrange the visual design of a page"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
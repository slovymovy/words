    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c4240517-0f72-3b94-9f9e-cd018e459ee0",
                    "sense_definition": "zdrobnienie od <w>czoło</w>; mała przednia część głowy nad oczami, używane często w odniesieniu do dzieci",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mama czule pocałowała swoje dziecko w gorące <czółko>.",
                            "target_lang_translations": {
                                "ru": "Мама нежно поцеловала своего ребенка в горячий <w>лобик</w>."
                            }
                        },
                        {
                            "text": "Dziewczynka ma wysokie <czółko> i jasne włosy splecione w warkocze.",
                            "target_lang_translations": {
                                "ru": "У девочки высокий <w>лобик</w> и светлые волосы, заплетенные в косы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "pocałować w czółko",
                        "wysokie czółko"
                    ],
                    "target_lang_definitions": {
                        "ru": "уменьшительная форма от слова \"лоб\"; передняя часть головы над глазами, часто используется по отношению к детям"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лобик",
                                "target_lang_sense_clarification": "часть лица (уменьшительно)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f1f937b-b3b4-3a3e-8ff5-d69450ec2fa8",
                    "sense_definition": "ozdobna opaska noszona na głowie przez dziewczęta, będąca elementem dawnego stroju ludowego",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do tradycyjnego stroju ludowego tancerka założyła aksamitne <czółko>.",
                            "target_lang_translations": {
                                "ru": "К традиционному народному костюму танцовщица надела бархатное <w>очелье</w>."
                            }
                        },
                        {
                            "text": "W muzeum etnograficznym podziwialiśmy bogato zdobione <czółko> panny młodej.",
                            "target_lang_translations": {
                                "ru": "В этнографическом музее мы любовались богато украшенным <w>очельем</w> невесты."
                            }
                        }
                    ],
                    "synonyms": [
                        "stroik",
                        "podwstążnik"
                    ],
                    "common_phrases": [
                        "aksamitne czółko",
                        "czółko panny młodej"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "element stroju ludowego"
                        },
                        {
                            "trait_type": "dated",
                            "comment": "dawna ozdoba"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "декоративная повязка на голову, которую носили девушки, элемент старинного народного костюма"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "очелье",
                                "target_lang_sense_clarification": "элемент народного костюма"
                            },
                            {
                                "target_lang_word": "налобник",
                                "target_lang_sense_clarification": "украшение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5a9ee8d1-1fc5-3697-97f0-07d8b95372e2",
                    "sense_definition": "przednia część naboju kulowego",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Eksperci balistyczni zbadali zdeformowane <czółko> pocisku znalezionego w ścianie.",
                            "target_lang_translations": {
                                "ru": "Баллистические эксперты исследовали деформированную <w>головную часть</w> пули, найденной в стене."
                            }
                        },
                        {
                            "text": "To <czółko> zostało wykonane ze specjalnie utwardzanego stopu metalu.",
                            "target_lang_translations": {
                                "ru": "Эта <w>головная часть</w> была изготовлена из специально закаленного металлического сплава."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "передняя часть пули"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "головная часть",
                                "target_lang_sense_clarification": "баллистика"
                            },
                            {
                                "target_lang_word": "вершинка",
                                "target_lang_sense_clarification": "часть пули"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                {
                    "sense_id": "c4daa3bf-108e-3e95-a7a5-d175f4f03495",
                    "sense_definition": "Notować informacje na papierze lub w urządzeniu, aby je utrwalić i o nich nie zapomnieć.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "writing_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę <w>zapisywać</w> wszystkie ważne spotkania w moim kalendarzu.",
                            "target_lang_translations": {
                                "ru": "Я должен <w>записывать</w> все важные встречи в свой календарь."
                            }
                        },
                        {
                            "text": "Student pilnie <w>zapisywał</w> każde słowo wypowiedziane przez profesora.",
                            "target_lang_translations": {
                                "ru": "Студент усердно <w>записывал</w> каждое слово, сказанное профессором."
                            }
                        }
                    ],
                    "synonyms": [
                        "notować",
                        "pisać"
                    ],
                    "common_phrases": [
                        "zapisywać adres",
                        "zapisywać numer telefonu",
                        "zapisywać w notesie",
                        "zapisywać datę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Записывать информацию на бумаге или устройстве, чтобы сохранить её и не забыть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "записывать",
                                "target_lang_sense_clarification": "делать заметки"
                            },
                            {
                                "target_lang_word": "отмечать",
                                "target_lang_sense_clarification": "фиксировать информацию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5f860a0-dcce-3242-afb3-e53d16f28c98",
                    "sense_definition": "Włączać kogoś lub siebie na listę uczestników jakiegoś wydarzenia, organizacji lub do lekarza.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "registration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Właśnie idę <w>zapisywać</w> się na nowy kurs języka angielskiego.",
                            "target_lang_translations": {
                                "ru": "Я как раз иду <w>записываться</w> на новый курс английского языка."
                            }
                        },
                        {
                            "text": "Rodzice zaczęli masowo <w>zapisywać</w> dzieci do przedszkola już w styczniu.",
                            "target_lang_translations": {
                                "ru": "Родители начали массово <w>записывать</w> детей в детский сад уже в январе."
                            }

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "43ed9cdc-d60e-3990-a4d7-acbd6ccf1015",
                    "sense_definition": "Easy to understand, do, or use; not difficult.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "ease",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The instructions for the new game are very <w>simple</w> and easy to follow.",
                            "target_lang_translations": {
                                "ru": "Инструкции к новой игре очень <w>простые</w>, и им легко следовать."
                            }
                        },
                        {
                            "text": "It is a <w>simple</w> task that will only take you a few minutes to finish.",
                            "target_lang_translations": {
                                "ru": "Это <w>простая</w> задача, выполнение которой займет у вас всего несколько минут."
                            }
                        }
                    ],
                    "synonyms": [
                        "easy",
                        "straightforward",
                        "uncomplicated",
                        "effortless",
                        "painless"
                    ],
                    "antonyms": [
                        "difficult",
                        "hard",
                        "challenging"
                    ],
                    "common_phrases": [
                        "simple task",
                        "simple instructions",
                        "simple to use"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкий для понимания, выполнения или использования; не вызывающий затруднений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "простой",
                                "target_lang_sense_clarification": "несложный"
                            },
                            {
                                "target_lang_word": "лёгкий",
                                "target_lang_sense_clarification": "не требующий усилий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ceea18e-caf2-36d5-ad00-ab7cd0f7235b",
                    "sense_definition": "Uncomplicated in form or nature; lacking complexity or extra parts.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "complexity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She prefers a <w>simple</w> life in the countryside away from the busy city.",
                            "target_lang_translations": {
                                "ru": "Она предпочитает <w>простую</w> жизнь в сельской местности, вдали от шумного города."
                            }
                        },
                        {
                            "text": "The scientist explained the <w>simple</w> structure of the molecule to the students.",
                            "target_lang_translations": {
                                "ru": "Ученый объяснил студентам <w>простую</w> структуру молекулы."
                            }
                        }
                    ],
                    "synonyms": [
                        "basic",
                        "elementary",
                        "fundamental",
                        "uncomplicated"
                    ],
                    "antonyms": [
                        "complex",
                        "complicated",
                        "involved"
                    ],
                    "common_phrases": [
                        "simple life",
                        "simple structure",
                        "keep it simple"
                    ],
                    "target_lang_definitions": {
                        "ru": "Несложный по форме или природе; лишенный сложности или лишних деталей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "простой",
                                "target_lang_sense_clarification": "базовый, элементарный"
                            },
                            {
                                "target_lang_word": "несложный",
                                "target_lang_sense_clarification": "по структуре"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "69ea6f95-4db0-333b-94da-e5f473c7ea7a",
                    "sense_definition": "Without decoration or ornamentation; plain and modest.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She wore a <w>simple</w> black dress to the party last night.",
                            "target_lang_translations": {
                                "ru": "Вчера вечером на вечеринку она надела <w>простое</w> черное платье."
                            }
                        },
                        {
                            "text": "The room was decorated in a <w>simple</w> style with very little furniture.",
                            "target_lang_translations": {
                                "ru": "Комната была оформлена в <w>простом</w> стиле с минимумом мебели."
                            }
                        }
                    ],
                    "synonyms": [
                        "plain",
                        "unadorned",
                        "modest",
                        "austere",
                        "unembellished"
                    ],
                    "antonyms": [
                        "flashy",
                        "ornate",
                        "fancy"
                    ],
                    "common_phrases": [
                        "simple dress",
                        "simple design"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c0e9e30c-1c3b-3407-bfa9-b32f4a4ce9e9",
                    "sense_definition": "A specific task or duty assigned to a person or group, often with a special purpose or objective.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "task_goal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers were sent on a dangerous <w>mission</w> behind enemy lines.",
                            "target_lang_translations": {
                                "ru": "Солдаты были отправлены на опасное <w>задание</w> в тыл врага."
                            }
                        },
                        {
                            "text": "Our <w>mission</w> is to provide clean drinking water to every village in the region.",
                            "target_lang_translations": {
                                "ru": "Наша <w>миссия</w> — обеспечить чистой питьевой водой каждую деревню в регионе."
                            }
                        }
                    ],
                    "synonyms": [
                        "assignment",
                        "task",
                        "objective",
                        "undertaking",
                        "operation"
                    ],
                    "common_phrases": [
                        "mission accomplished",
                        "rescue mission",
                        "on a mission",
                        "secret mission",
                        "suicide mission"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретное задание или обязанность, возложенные на человека или группу, часто с особой целью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миссия",
                                "target_lang_sense_clarification": "ответственное задание, призвание"
                            },
                            {
                                "target_lang_word": "задание",
                                "target_lang_sense_clarification": "порученная работа, задача"
                            },
                            {
                                "target_lang_word": "задача",
                                "target_lang_sense_clarification": "цель, то, что нужно выполнить"
                            },
                            {
                                "target_lang_word": "поручение",
                                "target_lang_sense_clarification": "возложенная обязанность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "45f108b4-3ca6-3bbd-90f6-4519bb76fa8b",
                    "sense_definition": "A group of people sent to a foreign country to conduct negotiations, establish relations, or perform a specific service.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "delegation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The government sent a diplomatic <w>mission</w> to negotiate the peace treaty.",
                            "target_lang_translations": {
                                "ru": "Правительство направило дипломатическую <w>миссию</w> для переговоров о мирном договоре."
                            }
                        },
                        {
                            "text": "She was part of a trade <w>mission</w> aimed at boosting exports to Asia.",
                            "target_lang_translations": {
                                "ru": "Она была частью торговой <w>миссии</w>, нацеленной на увеличение экспорта в Азию."
                            }
                        }
                    ],
                    "synonyms": [
                        "delegation",
                        "deputation",
                        "embassy",
                        "commission"
                    ],
                    "common_phrases": [
                        "diplomatic mission",
                        "trade mission",
                        "fact-finding mission"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа людей, отправленная в иностранное государство для ведения переговоров или установления отношений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миссия",
                                "target_lang_sense_clarification": "дипломатическая делегация"
                            },
                            {
                                "target_lang_word": "делегация",
                                "target_lang_sense_clarification": "группа представителей"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff21d4a6-978b-3f72-8fd0-fcbb53349725",
                    "sense_definition": "An assignment to spread a religion or provide aid, often in a foreign country; the work performed during such an assignment.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "religious_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He spent two years on a <w>mission</w> in South America teaching English and preaching.",
                            "target_lang_translations": {
                                "ru": "Он провел два года в <w>миссии</w> в Южной Америке, обучая английскому и проповедуя."
                            }
                        },
                        {
                            "text": "The church supports a medical <w>mission</w> that helps sick children in rural areas.",
                            "target_lang_translations": {
                                "ru": "Церковь поддерживает медицинскую <w>миссию</w>, которая помогает больным детям в сельских районах."
                            }
                        }
                    ],
                    "synonyms": [
                        "evangelism",
                        "ministry"
                    ],
                    "common_phrases": [
                        "foreign mission",
                        "medical mission"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поручение по распространению религии или оказанию помощи, часто в чужой стране; работа, выполняемая во время такого поручения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миссия",
                                "target_lang_sense_clarification": "миссионерская деятельность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42cda46b-8ac7-3f45-880a-b547e40051a0",
                    "sense_definition": "A building or settlement used as a base for religious or charitable work.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "religious_place",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Tourists often visit the old Spanish <w>mission</w> to see its historic architecture.",
                            "target_lang_translations": {
                                "ru": "Туристы часто посещают старую испанскую <w>миссию</w>, чтобы увидеть её историческую архитектуру."
                            }
                        },
                        {
                            "text": "The monks live and work at the <w>mission</w> located in the valley.",
                            "target_lang_translations": {
                                "ru": "Монахи живут и работают в <w>миссии</w>, расположенной в долине."
                            }
                        }
                    ],
                    "synonyms": [
                        "station"
                    ],
                    "common_phrases": [
                        "Spanish mission"
                    ],
                    "target_lang_definitions": {
                        "ru": "Здание или поселение, используемое как база для религиозной или благотворительной деятельности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миссия",
                                "target_lang_sense_clarification": "здание или представительство"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f121a5ec-0b82-3a4e-99a1-7cf3e6b45db3",
                    "sense_definition": "An organization or facility that provides aid to people in need, such as food and shelter for the homeless.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "charity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He volunteers at the local <w>mission</w> serving hot meals to the homeless.",
                            "target_lang_translations": {
                                "ru": "Он работает волонтером в местной <w>миссии</w>, раздавая горячую еду бездомным."
                            }
                        },
                        {
                            "text": "The rescue <w>mission</w> offers a bed and breakfast to anyone in need.",
                            "target_lang_translations": {
                                "ru": "Спасательная <w>миссия</w> предлагает ночлег и завтрак всем нуждающимся."
                            }
                        }
                    ],
                    "synonyms": [
                        "shelter",
                        "charity"
                    ],
                    "common_phrases": [
                        "homeless mission",
                        "rescue mission"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организация или учреждение, оказывающее помощь нуждающимся, например, еду и ночлег бездомным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миссия",
                                "target_lang_sense_clarification": "благотворительная организация"
                            },
                            {
                                "target_lang_word": "приют",
                                "target_lang_sense_clarification": "место помощи бездомным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fcbde90-a43a-3349-b377-07a4391cb96b",
                    "sense_definition": "A settlement where Indigenous Australians live, often historically managed by religious organizations or the government.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "indigenous_settlement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Her grandmother grew up on a <w>mission</w> in the outback before moving to the city.",
                            "target_lang_translations": {
                                "ru": "Её бабушка выросла в <w>миссии</w> в глубинке, прежде чем переехать в город."
                            }
                        },
                        {
                            "text": "Life on the <w>mission</w> was difficult, but the community remained strong.",
                            "target_lang_translations": {
                                "ru": "Жизнь в <w>миссии</w> была трудной, но община оставалась сплоченной."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Australia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поселение коренных австралийцев, исторически управляемое религиозными организациями или государством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миссия",
                                "target_lang_sense_clarification": "поселение (ист. Австралия)"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "5c1b49dd-c2a8-3150-bfd1-a046a765f9ef",
                    "sense_definition": "A specific district or neighborhood, notably a famous one in San Francisco.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "place_name",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "We went to the <w>Mission</w> to eat some authentic burritos.",
                            "target_lang_translations": {
                                "ru": "Мы пошли в <w>Мишен</w>, чтобы поесть настоящих буррито."
                            }
                        },
                        {
                            "text": "The <w>Mission</w> district is known for its vibrant street art and culture.",
                            "target_lang_translations": {
                                "ru": "Район <w>Мишен</w> известен своим ярким уличным искусством и культурой."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Mission District"
                    ],
                    "target_lang_definitions": {
                        "ru": "Определенный район или квартал, в частности известный район в Сан-Франциско."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Мишен",
                                "target_lang_sense_clarification": "название района"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9b0578e4-a2d5-37dc-a6ad-9c5fde81e69f",
                    "sense_definition": "To send someone to perform a specific task or duty.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_send",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The general <w>missioned</w> his best troops to secure the border.",
                            "target_lang_translations": {
                                "ru": "Генерал <w>отправил</w> свои лучшие войска для охраны границы."
                            }
                        },
                        {
                            "text": "She was <w>missioned</w> to gather intelligence on the opposing faction.",
                            "target_lang_translations": {
                                "ru": "Ей было <w>поручено</w> собрать разведданные о противоборствующей фракции."
                            }
                        }
                    ],
                    "synonyms": [
                        "send",
                        "dispatch",
                        "commission"
                    ],
                    "target_lang_definitions": {
                        "ru": "Посылать кого-либо для выполнения определенной задачи или обязанности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посылать",
                                "target_lang_sense_clarification": "отправлять с заданием"
                            },
                            {
                                "target_lang_word": "командировать",
                                "target_lang_sense_clarification": "отправлять в командировку"
                            },
                            {
                                "target_lang_word": "поручать",
                                "target_lang_sense_clarification": "давать задание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "615e9bff-b7ee-33af-92b3-0a3b3d1a33fa",
                    "sense_definition": "To perform the work of a missionary; to preach or provide aid.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "verb_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They <w>missioned</w> in the remote villages for over a decade.",
                            "target_lang_translations": {
                                "ru": "Они <w>миссионерствовали</w> в отдаленных деревнях более десяти лет."
                            }
                        },
                        {
                            "text": "The group <w>missioned</w> throughout the region, helping to build schools.",
                            "target_lang_translations": {
                                "ru": "Группа <w>вела миссионерскую деятельность</w> по всему региону, помогая строить школы."
                            }
                        }
                    ],
                    "synonyms": [
                        "evangelize",
                        "proselytize"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вести миссионерскую деятельность; проповедовать или оказывать помощь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "миссионерствовать",
                                "target_lang_sense_clarification": "вести миссионерскую работу"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
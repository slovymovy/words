    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "667ccfd7-1d10-3d48-9674-7c83ba1f6e20",
                    "sense_definition": "Устройство или система механизмов для передачи движения от двигателя к рабочим частям машины (автомобиля, станка и т.д.)",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У этого внедорожника полный <w>привод</w>, поэтому он легко проехал по грязи.",
                            "target_lang_translations": {
                                "en": "This SUV has all-wheel <w>drive</w>, so it drove easily through the mud."
                            }
                        },
                        {
                            "text": "Ворота гаража оснащены автоматическим электрическим <w>приводом</w>.",
                            "target_lang_translations": {
                                "en": "The garage door is equipped with an automatic electric <w>actuator</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "передача",
                        "механизм"
                    ],
                    "common_phrases": [
                        "передний привод",
                        "задний привод",
                        "полный привод",
                        "электрический привод"
                    ],
                    "target_lang_definitions": {
                        "en": "A device or system for transmitting motion from an engine to the working parts of a machine"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drive",
                                "target_lang_sense_clarification": "mechanism for transmitting power (e.g., 4-wheel drive)"
                            },
                            {
                                "target_lang_word": "actuator",
                                "target_lang_sense_clarification": "component that moves or controls a mechanism"
                            },
                            {
                                "target_lang_word": "transmission",
                                "target_lang_sense_clarification": "system of gears"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78f4d7e9-154e-3f2d-9765-68e4c7f3e868",
                    "sense_definition": "Устройство в компьютере для чтения и записи информации на диски (CD, DVD и др.)",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В моем новом ноутбуке отсутствует оптический <w>привод</w>.",
                            "target_lang_translations": {
                                "en": "My new laptop lacks an optical <w>drive</w>."
                            }
                        },
                        {
                            "text": "Система не может распознать диск, возможно, сломался <w>привод</w>.",
                            "target_lang_translations": {
                                "en": "The system cannot recognize the disc; perhaps the <w>drive</w> is broken."
                            }
                        }
                    ],
                    "synonyms": [
                        "дисковод"
                    ],
                    "common_phrases": [
                        "оптический привод",
                        "DVD-привод",
                        "виртуальный привод"
                    ],
                    "target_lang_definitions": {
                        "en": "A computer device for reading and writing data to discs (CD, DVD, etc.)"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drive",
                                "target_lang_sense_clarification": "computer hardware (e.g., optical drive)"
                            },
                            {
                                "target_lang_word": "disk drive",
                                "target_lang_sense_clarification": "specific hardware component"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f257210-339c-343b-8e73-73ff119bf1c6",
                    "sense_definition": "Задержание и принудительная доставка человека в отделение полиции; также запись о таком задержании в личном деле",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "legal_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У подозреваемого уже был <w>привод</w> в полицию за хулиганство в прошлом году.",
                            "target_lang_translations": {
                                "en": "The suspect already had a <w>record</w> of being brought to the police for hooliganism last year."
                            }
                        },
                        {
                            "text": "Патруль оформил <w>привод</w> нарушителей в дежурную часть.",
                            "target_lang_translations": {
                                "en": "The patrol processed the <w>detention</w> of the violators at the duty station."
                            }
                        }
                    ],
                    "synonyms": [
                        "задержание"
                    ],
                    "common_phrases": [
                        "привод в полицию",
                        "иметь приводы"
                    ],
                    "target_lang_definitions": {
                        "en": "Detention and forced delivery of a person to a police station; also a record of such detention"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "detention",
                                "target_lang_sense_clarification": "act of holding someone in custody"
                            },
                            {
                                "target_lang_word": "police record",
                                "target_lang_sense_clarification": "record of being brought to police"
                            },
                            {
                                "target_lang_word": "bringing in",
                                "target_lang_sense_clarification": "act of taking to the station"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fd9776e3-add5-3049-a9ba-622dee2df109",
                    "sense_definition": "Принудительная доставка лица (свидетеля, обвиняемого) к следователю или в суд в случае неявки по вызову",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Так как свидетель не явился добровольно, судья вынес постановление о его <w>приводе</w>.",
                            "target_lang_translations": {
                                "en": "Since the witness did not appear voluntarily, the judge issued an order for his <w>forced appearance</w>."
                            }
                        },
                        {
                            "text": "Судебные приставы осуществили <w>привод</w> ответчика на заседание.",
                            "target_lang_translations": {
                                "en": "Court bailiffs carried out the <w>forced bringing</w> of the defendant to the hearing."
                            }
                        }
                    ],
                    "synonyms": [

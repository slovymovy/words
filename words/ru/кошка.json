    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "97470ffb-07b2-33d3-92a7-d0c9d77ab27c",
                    "sense_definition": "Домашнее животное, небольшое хищное млекопитающее, которое ловит мышей и крыс; популярный питомец.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У моей бабушки в деревне живёт пушистая <w>кошка</w>.",
                            "target_lang_translations": {
                                "en": "A fluffy <w>cat</w> lives at my grandmother's in the village."
                            }
                        },
                        {
                            "text": "Моя <w>кошка</w> любит спать на тёплом подоконнике.",
                            "target_lang_translations": {
                                "en": "My <w>cat</w> loves sleeping on the warm windowsill."
                            }
                        },
                        {
                            "text": "Они постоянно ссорятся и живут как <w>кошка</w> с собакой.",
                            "target_lang_translations": {
                                "en": "They constantly quarrel and live like <w>cat</w> and dog."
                            }
                        }
                    ],
                    "synonyms": [
                        "киса́",
                        "киска",
                        "мурка"
                    ],
                    "antonyms": [
                        "мышь",
                        "крыса",
                        "кот"
                    ],
                    "common_phrases": [
                        "чёрная кошка",
                        "как кошка с собакой",
                        "играть в кошки-мышки",
                        "кошки скребут на душе"
                    ],
                    "target_lang_definitions": {
                        "en": "A domestic animal, a small carnivorous mammal that catches mice and rats; a popular pet."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cat",
                                "target_lang_sense_clarification": "domestic animal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a11632dc-ebdc-3b1c-93a7-0975c5c626fb",
                    "sense_definition": "Самка кота.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Наша <w>кошка</w> скоро родит маленьких котят.",
                            "target_lang_translations": {
                                "en": "Our <w>cat</w> will soon give birth to little kittens."
                            }
                        },
                        {
                            "text": "Я не могу понять, это кот или <w>кошка</w>.",
                            "target_lang_translations": {
                                "en": "I can't tell if it is a male cat or a <w>female cat</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "кот"
                    ],
                    "common_phrases": [
                        "беременная кошка",
                        "трёхцветная кошка"
                    ],
                    "target_lang_definitions": {
                        "en": "A female cat."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "female cat",
                                "target_lang_sense_clarification": "gender specific"
                            },
                            {
                                "target_lang_word": "she-cat",
                                "target_lang_sense_clarification": "gender specific"
                            },
                            {
                                "target_lang_word": "cat",
                                "target_lang_sense_clarification": "general term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1625702a-a8e8-372e-b998-165a3b0c2487",
                    "sense_definition": "Металлическое приспособление с шипами, которое надевают на обувь для передвижения по льду, горам или для лазания на столбы.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Альпинист надел <w>кошки</w>, чтобы безопасно подняться по леднику.",
                            "target_lang_translations": {
                                "en": "The mountaineer put on <w>crampons</w> to safely climb the glacier."
                            }
                        },
                        {
                            "text": "Электрик использовал специальные <w>кошки</w>, чтобы залезть на деревянный столб.",
                            "target_lang_translations": {
                                "en": "The electrician used special <w>climbing irons</w> to climb the wooden pole."
                            }
                        }
                    ],
                    "common_phrases": [
                        "альпинистские кошки",
                        "надеть кошки"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "обычно используется во множественном числе"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A metal device with spikes worn on shoes for moving on ice, mountains, or climbing poles."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crampons",
                                "target_lang_sense_clarification": "for ice/mountaineering"
                            },
                            {
                                "target_lang_word": "climbing irons",
                                "target_lang_sense_clarification": "for poles/trees"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "01323d00-f6ab-3024-976e-c8aa30a4b32e",
                    "sense_definition": "Небольшой якорь или крюк с несколькими лапами (обычно тремя или четырьмя) для поиска и подъёма предметов из воды.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "tool",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Рыбаки забросили железную <w>кошку</w>, чтобы достать утонувшую сеть.",
                            "target_lang_translations": {
                                "en": "The fishermen threw an iron <w>grappling hook</w> to retrieve the sunken net."
                            }
                        },
                        {
                            "text": "Матрос ловко зацепил борт лодки абордажной <w>кошкой</w>.",
                            "target_lang_translations": {
                                "en": "The sailor deftly hooked the boat's side with a boarding <w>grapnel</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "забросить кошку",
                        "абордажная кошка"
                    ],
                    "target_lang_definitions": {
                        "en": "A small anchor or hook with multiple claws (usually three or four) for searching and lifting objects from water."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "grappling hook",
                                "target_lang_sense_clarification": "tool with claws"
                            },
                            {
                                "target_lang_word": "grapnel",
                                "target_lang_sense_clarification": "small anchor"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c23fd2da-55fc-3a6c-a83a-451dcfb0e3d8",
                    "sense_definition": "Плеть с несколькими хвостами, часто с твёрдыми наконечниками, использовавшаяся в прошлом для телесных наказаний.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "weapon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В старину за серьёзные проступки матросов наказывали <w>кошкой</w>.",
                            "target_lang_translations": {
                                "en": "In the old days, sailors were punished with a <w>cat</w> for serious offenses."
                            }
                        },
                        {
                            "text": "Удар <w>кошкой</w> оставлял на спине глубокие следы.",
                            "target_lang_translations": {
                                "en": "A blow with the <w>cat</w> left deep marks on the back."
                            }
                        }
                    ],
                    "synonyms": [
                        "девятихвостка"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "исторический термин"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A whip with multiple tails, often with hard tips, used in the past for corporal punishment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cat-o'-nine-tails",
                                "target_lang_sense_clarification": "whip"
                            },
                            {
                                "target_lang_word": "cat",
                                "target_lang_sense_clarification": "short for cat-o'-nine-tails"
                            }
                        ]
                    }
                }
            ]
        }
    ]

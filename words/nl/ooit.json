                },
                {
                    "sense_id": "f60891a4-0e76-3155-8c78-d34a7fe22c74",
                    "sense_definition": "Op een onbepaald moment in de toekomst.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "future_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik hoop dat we elkaar <w>ooit</w> weer zullen ontmoeten.",

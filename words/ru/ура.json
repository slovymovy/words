{
    "word_family": [],
    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "d68a9b0f-ab6e-38ed-8092-c0966dc49499",
                    "sense_definition": "Возглас, выражающий сильную радость, восторг, торжество или одобрение.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "joy_exclamation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<Ура>, завтра у нас начинаются каникулы!",
                            "target_lang_translations": {
                                "en": "<Hooray>, our holidays start tomorrow!"
                            }
                        },
                        {
                            "text": "Все гости дружно закричали: «Гип-гип <ура>!»",
                            "target_lang_translations": {
                                "en": "All the guests shouted in unison: \"Hip-hip <hooray>!\""
                            }
                        }
                    ],
                    "synonyms": [
                        "виват"
                    ],
                    "antonyms": [
                        "увы"
                    ],
                    "common_phrases": [
                        "гип-гип ура",
                        "кричать ура"
                    ],
                    "target_lang_definitions": {
                        "en": "An exclamation expressing strong joy, delight, triumph, or approval."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hooray",
                                "target_lang_sense_clarification": "exclamation of joy or victory"
                            },
                            {
                                "target_lang_word": "hurray",
                                "target_lang_sense_clarification": "alternative spelling"
                            },
                            {
                                "target_lang_word": "yay",
                                "target_lang_sense_clarification": "informal exclamation of excitement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "997703aa-1d48-3391-8029-1dbdb89a2284",
                    "sense_definition": "Боевой клич войск при атаке или торжественное приветствие командующего.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "battle_cry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солдаты с громким <ура> бросились в атаку на противника.",
                            "target_lang_translations": {
                                "en": "The soldiers rushed to attack the enemy with a loud <hurrah>."
                            }
                        },
                        {
                            "text": "Войска ответили генералу протяжным и раскатистым <ура>.",
                            "target_lang_translations": {
                                "en": "The troops answered the general with a drawn-out and rolling <hurrah>."
                            }
                        }
                    ],
                    "antonyms": [
                        "долой"
                    ],
                    "common_phrases": [
                        "громкое ура",
                        "раскатистое ура"
                    ],
                    "target_lang_definitions": {
                        "en": "A battle cry used by troops during an attack or a solemn greeting to a commander."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hurrah",
                                "target_lang_sense_clarification": "a loud shout or cheer, often used as a noun"
                            },
                            {
                                "target_lang_word": "cheer",
                                "target_lang_sense_clarification": "a shout of encouragement or praise"
                            }
                        ]
                    }
                }
            ]
        }
    ]

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "a2fc240d-6573-3fc2-b839-783d883aeb7d",
                    "sense_definition": "A common, silvery-grey metal that is magnetic, rusts easily, and is used to make steel; a chemical element.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "material_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The heavy gates were made of solid <w>iron</w>.",
                            "target_lang_translations": {
                                "ru": "Тяжёлые ворота были сделаны из цельного <w>железа</w>."
                            }
                        },
                        {
                            "text": "Spinach is a vegetable that is very rich in <w>iron</w>.",
                            "target_lang_translations": {
                                "ru": "Шпинат — это овощ, очень богатый <w>железом</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "ferrum"
                    ],
                    "common_phrases": [
                        "cast iron",
                        "wrought iron",
                        "scrap iron",
                        "iron ore"
                    ],
                    "target_lang_definitions": {
                        "ru": "Химический элемент, серебристо-серый металл, обладающий магнитными свойствами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "железо",
                                "target_lang_sense_clarification": "металл"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a0da70ad-7262-32d9-b435-9f17574a4e8b",
                    "sense_definition": "A handheld electrical device with a flat, heated base used to remove wrinkles from clothing.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "appliance_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She forgot to unplug the <w>iron</w> after pressing her dress.",
                            "target_lang_translations": {
                                "ru": "Она забыла выключить <w>утюг</w> из розетки после глажки платья."
                            }
                        },
                        {
                            "text": "Be careful not to burn your hand on the hot <w>iron</w>.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен, не обожги руку о горячий <w>утюг</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "clothes iron",
                        "flatiron"
                    ],
                    "common_phrases": [
                        "steam iron",
                        "hot iron",
                        "branding iron"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бытовой электроприбор для разглаживания складок на одежде."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "утюг",
                                "target_lang_sense_clarification": "бытовой прибор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4729bdfd-5216-3750-b2da-2f0a9f48ed17",
                    "sense_definition": "A symbol or metaphor for great strength, hardness, or lack of flexibility.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "strength_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Although he looked gentle, his character was pure <w>iron</w>.",
                            "target_lang_translations": {
                                "ru": "Хотя он выглядел мягким, его характер был — чистое <w>железо</w>."
                            }
                        },
                        {
                            "text": "The general ruled the country with a fist of <w>iron</w>.",
                            "target_lang_translations": {
                                "ru": "Генерал правил страной <w>железным</w> кулаком."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Символ или метафора огромной силы, твёрдости или непреклонности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "железо",
                                "target_lang_sense_clarification": "твёрдость характера"

{
    "word_family": [
        "bachorek",
        "bachorski"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4ba8fc86-b0b8-3574-a37a-2057687d291a",
                    "sense_definition": "Potoczne, często lekceważące lub negatywne określenie dziecka, zwłaszcza niegrzecznego, hałaśliwego lub sprawiającego kłopoty wychowawcze.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "child_senses",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten mały <w>bachor</w> znowu kopnął mnie w kostkę i uciekł.",
                            "target_lang_translations": {
                                "ru": "Этот маленький <w>паршивец</w> снова пнул меня в лодыжку и убежал."
                            }
                        },
                        {
                            "text": "Sąsiedzi mają dwa rozwydrzone <w>bachory</w>, które krzyczą przez cały dzień.",
                            "target_lang_translations": {
                                "ru": "У соседей двое избалованных <w>спиногрызов</w>, которые орут целыми днями."
                            }
                        }
                    ],
                    "synonyms": [
                        "gówniarz",
                        "łobuz",
                        "utrapieniec",
                        "urwipołeć",
                        "szkrab"
                    ],
                    "antonyms": [
                        "aniołek"
                    ],
                    "common_phrases": [
                        "rozwydrzony bachor",
                        "wstrętny bachor",
                        "cudze bachory"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разговорное, часто пренебрежительное или негативное название ребенка, особенно непослушного, шумного или доставляющего проблемы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "паршивец",
                                "target_lang_sense_clarification": "о непослушном ребенке"
                            },
                            {
                                "target_lang_word": "сопляк",
                                "target_lang_sense_clarification": "пренебрежительно"
                            },
                            {
                                "target_lang_word": "спиногрыз",
                                "target_lang_sense_clarification": "сленг, о детях как обузе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b82c0bcb-90ff-3e79-a3e1-52c9d2547de5",
                    "sense_definition": "Przestarzałe lub obraźliwe określenie dziecka z nieprawego łoża (nieślubnego).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "child_senses",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych wiekach <w>bachor</w> nie miał prawa do dziedziczenia majątku po ojcu.",
                            "target_lang_translations": {
                                "ru": "В прежние века <w>бастард</w> не имел права наследовать имущество отца."
                            }
                        },
                        {
                            "text": "Ludzie we wsi szeptali, że to <w>bachor</w>, bo nikt nie znał jego ojca.",
                            "target_lang_translations": {
                                "ru": "Люди в деревне шептались, что это <w>байстрюк</w>, так как никто не знал его отца."
                            }
                        }
                    ],
                    "synonyms": [
                        "bękart"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Устаревшее или оскорбительное название внебрачного ребенка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "байстрюк",
                                "target_lang_sense_clarification": "просторечное, грубое"
                            },
                            {
                                "target_lang_word": "бастард",
                                "target_lang_sense_clarification": "историческое"
                            },
                            {
                                "target_lang_word": "ублюдок",
                                "target_lang_sense_clarification": "крайне грубое, устаревшее значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ccfbea14-9cf6-3d52-8548-790c9037e480",
                    "sense_definition": "Brzuch, żołądek lub jelita zwierzęcia; słowo używane głównie w gwarach lub języku potocznym.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "anatomy_senses",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wilk najadł się do syta i teraz ciąży mu pełny <w>bachor</w>.",
                            "target_lang_translations": {
                                "ru": "Волк наелся досыта, и теперь ему тяжелит полное <w>брюхо</w>."
                            }
                        },
                        {
                            "text": "Gospodarz czyścił świński <w>bachor</w>, aby przygotować tradycyjne potrawy.",
                            "target_lang_translations": {
                                "ru": "Хозяин чистил свиной <w>желудок</w>, чтобы приготовить традиционные блюда."
                            }
                        }
                    ],
                    "synonyms": [
                        "bebech",
                        "kałdun",
                        "żwacz"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": ""
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Живот, желудок или кишки животного; слово используется в основном в диалектах или разговорной речи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брюхо",
                                "target_lang_sense_clarification": "живот животного"
                            },
                            {
                                "target_lang_word": "желудок",
                                "target_lang_sense_clarification": "внутренний орган"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "fc0885d3-1351-3c2f-b7f6-8f79caafd77d",
                    "sense_definition": "Polskie nazwisko (żeńskie lub męskie).",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Pan <w>Bachor</w> został wybrany na przewodniczącego rady osiedla.",
                            "target_lang_translations": {
                                "ru": "Господин <w>Бахор</w> был избран председателем совета жилого комплекса."
                            }
                        },
                        {
                            "text": "Nie znam nikogo o nazwisku <w>Bachor</w> w tej okolicy.",
                            "target_lang_translations": {
                                "ru": "Я не знаю никого по фамилии <w>Бахор</w> в этой округе."
                            }
                        }
                    ],

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "6d4f2344-1c0f-3cb8-aa42-f1efc5ba9aaa",
                    "sense_definition": "Regularne, obowiązkowe świadczenia pieniężne na rzecz osób fizycznych (zazwyczaj krewnych, np. dzieci lub byłego małżonka), do których zobowiązuje prawo.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Sąd nakazał ojcu płacić wysokie <w>alimenty</w> na dwoje dzieci.",
                            "target_lang_translations": {
                                "ru": "Суд обязал отца платить высокие <w>алименты</w> на двоих детей."
                            }
                        },
                        {
                            "text": "Mężczyzna unikał płacenia, więc komornik ściągnął zaległe <w>alimenty</w>.",
                            "target_lang_translations": {
                                "ru": "Мужчина уклонялся от уплаты, поэтому судебный пристав взыскал задолженные <w>алименты</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "alimenta"
                    ],
                    "common_phrases": [
                        "płacić alimenty",
                        "zasądzić alimenty",
                        "alimenty na dziecko",
                        "ściągać alimenty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Регулярные обязательные денежные выплаты на содержание членов семьи (обычно детей или бывшего супруга), установленные законом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "алименты",
                                "target_lang_sense_clarification": "выплаты на содержание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9193f5e3-22e3-3ad2-b810-2bbe95b1ed19",
                    "sense_definition": "Środki niezbędne do utrzymania się, pożywienie (znaczenie przestarzałe).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnych tekstach słowo to oznaczało po prostu <w>alimenty</w> dla ciała.",
                            "target_lang_translations": {
                                "ru": "В старых текстах это слово означало просто <w>пропитание</w> для тела."
                            }
                        },
                        {
                            "text": "Zapewniono im skromne <w>alimenty</w> w postaci chleba i wody.",
                            "target_lang_translations": {
                                "ru": "Им обеспечили скромное <w>пропитание</w> в виде хлеба и воды."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "znaczenie historyczne"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Средства, необходимые для поддержания жизни, пища (устаревшее значение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пропитание",
                                "target_lang_sense_clarification": "пища, еда"
                            },
                            {
                                "target_lang_word": "содержание",
                                "target_lang_sense_clarification": "средства к существованию"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "1dc9e92b-2bdd-3dab-9370-c99fb427b722",
                    "sense_definition": "Имущество, принадлежащее кому-либо или чему-либо.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "assets",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта просторная квартира — моя личная <w>собственность</w>.",
                            "target_lang_translations": {
                                "en": "This spacious apartment is my personal <w>property</w>."
                            }
                        },
                        {
                            "text": "Крупный завод является <w>собственностью</w> государства.",
                            "target_lang_translations": {
                                "en": "The large factory is the <w>property</w> of the state."
                            }
                        }
                    ],
                    "synonyms": [
                        "имущество",
                        "владение"
                    ],
                    "common_phrases": [

    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f11d4927-298b-39cf-b6e3-ce284f42ebd1",
                    "sense_definition": "Made unclean, impure, or harmful to health, usually by chemicals or waste.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The river is too <w>polluted</w> for anyone to swim in safely.",
                            "target_lang_translations": {
                                "ru": "Река слишком <w>загрязнена</w>, чтобы в ней можно было безопасно купаться."
                            }
                        },
                        {
                            "text": "Many people in the city suffer from health issues caused by <w>polluted</w> air.",
                            "target_lang_translations": {
                                "ru": "Многие жители города страдают от проблем со здоровьем, вызванных <w>загрязнённым</w> воздухом."
                            }
                        }
                    ],
                    "synonyms": [
                        "foul"
                    ],
                    "antonyms": [
                        "uncontaminated",
                        "unpolluted"
                    ],
                    "common_phrases": [
                        "polluted air",
                        "polluted water",
                        "heavily polluted"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделанный грязным, нечистым или вредным для здоровья, обычно из-за химикатов или отходов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "загрязнённый",
                                "target_lang_sense_clarification": "содержащий вредные примеси"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eb9d693c-59e5-3a8d-8366-c13bb3ba3697",
                    "sense_definition": "Extremely intoxicated with alcohol.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He got absolutely <w>polluted</w> at the party and had to take a taxi home.",
                            "target_lang_translations": {
                                "ru": "Он <w>напился в хлам</w> на вечеринке, и ему пришлось ехать домой на такси."
                            }
                        },
                        {
                            "text": "They were so <w>polluted</w> by midnight that they could barely stand up.",
                            "target_lang_translations": {
                                "ru": "К полуночи они были настолько <w>пьяны</w>, что едва могли стоять на ногах."
                            }

            "pos": "verb",
            "senses": [
                {
                    "sense_id": "06cc3afd-8b66-345b-9940-6650998921b9",
                    "sense_definition": "Wyrwać roślinę z ziemi wraz z korzeniami.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ogrodnik musiał <wykorzenić> stare drzewo, które zagrażało bezpieczeństwu domu.",
                            "target_lang_translations": {
                                "ru": "Садовнику пришлось <w>выкорчевать</w> старое дерево, которое угрожало безопасности дома."
                            }
                        },
                        {
                            "text": "Przez cały dzień próbowaliśmy <wykorzenić> chwasty z naszego ogrodu warzywnego.",
                            "target_lang_translations": {
                                "ru": "Весь день мы пытались <w>вырвать с корнем</w> сорняки из нашего огорода."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyrwać"
                    ],
                    "antonyms": [
                        "zasadzić"
                    ],
                    "common_phrases": [
                        "wykorzenić chwasty",
                        "wykorzenić drzewo",
                        "wykorzenić pień"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вырвать растение из земли вместе с корнями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выкорчевать",
                                "target_lang_sense_clarification": "о деревьях, пнях"
                            },
                            {
                                "target_lang_word": "вырвать с корнем",
                                "target_lang_sense_clarification": "о растениях вообще"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "262d910a-7ed7-3778-8db6-22fe0e501ff6",
                    "sense_definition": "Całkowicie usunąć lub zniszczyć coś negatywnego, na przykład złe nawyki, przestępczość lub choroby.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy rząd obiecał skutecznie <wykorzenić> korupcję w administracji publicznej.",
                            "target_lang_translations": {
                                "ru": "Новое правительство обещало эффективно <w>искоренить</w> коррупцию в государственном управлении."
                            }
                        },
                        {
                            "text": "Bardzo trudno jest <wykorzenić> stare przyzwyczajenia, które towarzyszą nam od lat.",
                            "target_lang_translations": {
                                "ru": "Очень трудно <w>искоренить</w> старые привычки, которые сопровождают нас годами."
                            }
                        }
                    ],
                    "synonyms": [
                        "wytępić",
                        "zlikwidować",
                        "wyeliminować",
                        "zwalczyć"
                    ],
                    "antonyms": [
                        "zaszczepić",
                        "utrwalić"
                    ],
                    "common_phrases": [
                        "wykorzenić zło",
                        "wykorzenić korupcję",
                        "wykorzenić nawyki",
                        "wykorzenić przestępczość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью устранить или уничтожить что-то негативное (например, вредные привычки, преступность)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "искоренить",
                                "target_lang_sense_clarification": "уничтожить полностью (зло, коррупцию)"
                            },
                            {
                                "target_lang_word": "изжить",
                                "target_lang_sense_clarification": "устранить, вытеснить из жизни (привычки)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2eedd025-5a11-3ebb-8b4f-0c0778ab9179",
                    "sense_definition": "Pozbawić kogoś więzi z rodzimym środowiskiem, kulturą lub tradycją.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wojna i przymusowa migracja mogą trwale <wykorzenić> całe społeczności.",
                            "target_lang_translations": {
                                "ru": "Война и принудительная миграция могут навсегда <w>лишить корней</w> целые сообщества."
                            }
                        },
                        {
                            "text": "System edukacji w tamtym okresie próbował <wykorzenić> lokalne tradycje wśród młodzieży.",
                            "target_lang_translations": {
                                "ru": "Система образования в тот период пыталась <w>искоренить</w> местные традиции среди молодежи."
                            }
                        }
                    ],
                    "synonyms": [
                        "wynarodowić"
                    ],
                    "antonyms": [
                        "zakorzenić"
                    ],
                    "common_phrases": [
                        "wykorzenić z kultury",
                        "wykorzenić z tradycji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Лишить кого-либо связей с родной средой, культурой или традициями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лишить корней",
                                "target_lang_sense_clarification": "метафорически о людях"
                            },
                            {
                                "target_lang_word": "оторвать от почвы",
                                "target_lang_sense_clarification": "лишить связи с родиной"
                            }
                        ]
                    }
                }
            ]
        }
    ]

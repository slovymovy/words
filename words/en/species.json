    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3aedf0eb-eba2-3d8f-ba69-3e64b6440d33",
                    "sense_definition": "A group of plants or animals that share similar characteristics and can breed with each other to produce fertile offspring.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The giant panda is an endangered <w>species</w> found in China.",
                            "target_lang_translations": {
                                "ru": "Гигантская панда — это исчезающий <w>вид</w>, обитающий в Китае."
                            }
                        },
                        {
                            "text": "Scientists have identified a new <w>species</w> of beetle in the rainforest.",
                            "target_lang_translations": {
                                "ru": "Ученые обнаружили новый <w>вид</w> жуков в тропическом лесу."
                            }
                        }
                    ],
                    "synonyms": [
                        "breed",
                        "variety"
                    ],
                    "common_phrases": [
                        "endangered species",
                        "invasive species",
                        "native species",
                        "human species",
                        "rare species"
                    ],
                    "target_lang_definitions": {
                        "ru": "Группа растений или животных, обладающих схожими характеристиками и способных скрещиваться между собой, давая плодовитое потомство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вид",
                                "target_lang_sense_clarification": "биологический"
                            },
                            {
                                "target_lang_word": "разновидность",
                                "target_lang_sense_clarification": "общая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9bb20129-c65e-38fb-b826-6fef54bc3ec0",
                    "sense_definition": "A particular type, kind, or sort of something, often used in a formal or literary context.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He felt a <w>species</w> of relief when he finally heard the news.",
                            "target_lang_translations": {
                                "ru": "Он почувствовал <w>своего рода</w> облегчение, когда наконец услышал новости."
                            }
                        },
                        {
                            "text": "The book describes a strange <w>species</w> of courage that few people possess.",
                            "target_lang_translations": {
                                "ru": "В книге описывается странный <w>вид</w> мужества, которым обладают немногие."
                            }
                        }
                    ],
                    "synonyms": [
                        "kind",
                        "sort",
                        "type",
                        "variety"
                    ],
                    "common_phrases": [
                        "species of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретный тип, род или сорт чего-либо; часто используется в формальном или литературном контексте."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разновидность",
                                "target_lang_sense_clarification": "тип"
                            },
                            {
                                "target_lang_word": "род",
                                "target_lang_sense_clarification": "категория"
                            },
                            {
                                "target_lang_word": "вид",
                                "target_lang_sense_clarification": "тип"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b6c8d59-a664-37ab-8700-a225874ac453",
                    "sense_definition": "A specific type of atom, molecule, ion, or other particle.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The chemical reaction produces a highly reactive <w>species</w>.",
                            "target_lang_translations": {
                                "ru": "В результате химической реакции образуются высокореактивные <w>частицы</w>."
                            }
                        },
                        {
                            "text": "We need to identify the ionic <w>species</w> present in the solution.",
                            "target_lang_translations": {
                                "ru": "Нам нужно определить ионные <w>формы</w>, присутствующие в растворе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "chemical species",
                        "reactive species"
                    ],
                    "target_lang_definitions": {

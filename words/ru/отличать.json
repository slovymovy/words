    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ce9083ed-d453-31ce-834e-0d1b7b7cbc19",
                    "sense_definition": "Видеть или понимать разницу между кем-либо или чем-либо; распознавать один объект среди других.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "distinguish",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я с трудом могу <отличать> близнецов друг от друга."
                        },
                        {
                            "text": "Важно уметь <отличать> правду от лжи в новостях."
                        }
                    ],
                    "synonyms": [
                        "различа́ть"
                    ],
                    "antonyms": [
                        "путать"
                    ],
                    "common_phrases": [
                        "отличать правду от лжи",
                        "отличать добро от зла",
                        "отличать главное от второстепенного"
                    ]
                },
                {
                    "sense_id": "8ffc4fc1-a011-37b7-88f8-2f8c4db3b8c3",
                    "sense_definition": "Быть характерной особенностью кого-либо или чего-либо; делать непохожим на других.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "characterize",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Высокое качество всегда <отличало> продукцию этой фирмы."
                        },
                        {
                            "text": "Скромность и доброта <отличают> этого человека."
                        }
                    ],
                    "synonyms": [
                        "различа́ть"
                    ],
                    "common_phrases": [
                        "выгодно отличать",
                        "эта черта отличает",
                        "качество отличает"
                    ]
                },
                {
                    "sense_id": "27cca942-d3f8-3db6-9e5e-f09464fd26d5",
                    "sense_definition": "Награждать или поощрять кого-либо за заслуги; выделять особым вниманием или наградой.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "award",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Командир решил лично <отличать> храбрых солдат медалями."
                        },
                        {
                            "text": "Руководство часто <отличает> лучших сотрудников премиями."
                        }
                    ],
                    "common_phrases": [
                        "отличать наградой",
                        "отличать за службу",
                        "отличать повышением"
                    ]
                },
                {
                    "sense_id": "dc380a23-8630-3573-bf83-269126365926",
                    "sense_definition": "Проявлять особое личное внимание, симпатию или предпочтение к кому-либо.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "favor",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Хозяйка дома старалась <отличать> каждого гостя своим вниманием."
                        },
                        {
                            "text": "Учитель не должен <отличать> любимчиков в классе."

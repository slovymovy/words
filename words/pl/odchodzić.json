    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "768c79e6-62fb-3ff3-bee0-e3e2da0292e5",
                    "sense_definition": "Oddalać się od jakiegoś miejsca lub osoby; iść w innym kierunku",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dlaczego <w>odchodzisz</w> bez słowa pożegnania?",
                            "target_lang_translations": {
                                "ru": "Почему ты <w>уходишь</w> не попрощавшись?"
                            }
                        },
                        {
                            "text": "Pociąg do Warszawy <w>odchodzi</w> z peronu drugiego.",
                            "target_lang_translations": {
                                "ru": "Поезд в Варшаву <w>отходит</w> со второй платформы."
                            }
                        }
                    ],
                    "synonyms": [
                        "oddalać się"
                    ],
                    "antonyms": [
                        "przychodzić",
                        "zbliżać się"
                    ],
                    "common_phrases": [
                        "odchodzić od zmysłów",
                        "odchodzić w dal",
                        "odchodzić od stołu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удаляться от какого-либо места или человека; идти в другом направлении"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уходить",
                                "target_lang_sense_clarification": "покидать место"
                            },
                            {
                                "target_lang_word": "отходить",
                                "target_lang_sense_clarification": "отправляться (о транспорте) или отступать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "559d34ce-8c28-3525-9236-f9a90edd47f6",
                    "sense_definition": "Kończyć z kimś związek, opuszczać kogoś lub rezygnować z pracy, funkcji",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "abandonment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Marek <w>odchodzi</w> z naszej firmy w przyszłym tygodniu.",
                            "target_lang_translations": {
                                "ru": "Марек <w>уходит</w> из нашей фирмы на следующей неделе."
                            }
                        },
                        {
                            "text": "Ona <w>odchodzi</w> od męża, ponieważ nie jest z nim szczęśliwa.",
                            "target_lang_translations": {
                                "ru": "Она <w>уходит</w> от мужа, так как несчастлива с ним."
                            }
                        }
                    ],
                    "synonyms": [
                        "porzucać",
                        "zostawiać",
                        "rezygnować"
                    ],
                    "common_phrases": [
                        "odchodzić z pracy",
                        "odchodzić na emeryturę",
                        "odchodzić od żony"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заканчивать отношения с кем-либо, покидать кого-либо или увольняться с работы"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уходить",
                                "target_lang_sense_clarification": "покидать семью или работу"
                            },
                            {
                                "target_lang_word": "бросать",
                                "target_lang_sense_clarification": "оставлять кого-то (разг.)"
                            },
                            {
                                "target_lang_word": "увольняться",
                                "target_lang_sense_clarification": "с работы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b6da8c01-3454-3714-a17d-9da5d1218141",
                    "sense_definition": "Umierać (często używane jako delikatne, eufemistyczne określenie)",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wielcy artyści <w>odchodzą</w>, ale ich muzyka pozostaje z nami na zawsze.",
                            "target_lang_translations": {
                                "ru": "Великие артисты <w>уходят</w>, но их музыка остаётся с нами навсегда."
                            }
                        },
                        {
                            "text": "Mój dziadek <w>odchodził</w> w spokoju, otoczony całą rodziną.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка <w>уходил</w> спокойно, в окружении всей семьи."
                            }
                        }
                    ],
                    "synonyms": [
                        "umierać",
                        "konać",
                        "gasnąć"
                    ],
                    "common_phrases": [
                        "odchodzić z tego świata",
                        "odchodzić na wieczną wartę",
                        "odchodzić w zapomnienie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Умирать (часто используется как мягкое, эвфемистичное выражение)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уходить",
                                "target_lang_sense_clarification": "умирать (эвфемизм)"
                            },
                            {
                                "target_lang_word": "умирать",
                                "target_lang_sense_clarification": "прямое значение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0867e2d6-eca9-3d55-ba10-b3b051ada19c",
                    "sense_definition": "Dziać się, wydarzać się (zwykle o czymś zaskakującym, dziwnym lub gwałtownym)",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "happening",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym klubie w każdy piątek <w>odchodzą</w> niesamowite imprezy.",
                            "target_lang_translations": {
                                "ru": "В этом клубе каждую пятницу <w>проходят</w> невероятные вечеринки."
                            }
                        },
                        {
                            "text": "Nie mam pojęcia, co tu <w>odchodzi</w>, ale wygląda to podejrzanie.",
                            "target_lang_translations": {
                                "ru": "Понятия не имею, что тут <w>творится</w>, но выглядит это подозрительно."
                            }
                        }
                    ],
                    "common_phrases": [
                        "odchodzą cyrki",
                        "odchodzą numery"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "używane w mowie potocznej"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Происходить, случаться (обычно о чём-то удивительном, странном или бурном)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "твориться",
                                "target_lang_sense_clarification": "происходить (разг., часто о беспорядке)"
                            },
                            {
                                "target_lang_word": "происходить",
                                "target_lang_sense_clarification": "случаться"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
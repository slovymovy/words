{
    "word_family": [
        "hunne"
    ],
    "entries": [
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "aeeceb82-6eed-3ed0-93f9-ce2ef2686769",
                    "sense_definition": "Van hen; geeft aan dat iets het eigendom is van of hoort bij meerdere personen of zaken (bezittelijk voornaamwoord).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "possessive_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen spelen buiten met <w>hun</w> nieuwe bal.",
                            "target_lang_translations": {
                                "en": "The children are playing outside with <w>their</w> new ball.",
                                "ru": "Дети играют на улице со <w>своим</w> новым мячом."
                            }
                        },
                        {
                            "text": "Zij hebben <w>hun</w> huis vorig jaar verkocht.",
                            "target_lang_translations": {
                                "en": "They sold <w>their</w> house last year.",
                                "ru": "Они продали <w>свой</w> дом в прошлом году."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hun eigen",
                        "op hun gemak",
                        "met hun tweeën"
                    ],
                    "target_lang_definitions": {
                        "en": "Of them; indicates that something belongs to or is associated with multiple people or things (possessive determiner).",
                        "ru": "Их; указывает на то, что что-то является собственностью или относится к нескольким людям или предметам (притяжательное местоимение)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "their",
                                "target_lang_sense_clarification": "possessive determiner"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "их",
                                "target_lang_sense_clarification": "притяжательное местоимение"
                            },
                            {
                                "target_lang_word": "свой",
                                "target_lang_sense_clarification": "принадлежащий подлежащему"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "d4f823a3-9bb1-338c-a527-9bbbc5714303",
                    "sense_definition": "Aan hen; de vorm van het persoonlijk voornaamwoord derde persoon meervoud als meewerkend voorwerp (zonder voorzetsel).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "pronoun_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb <w>hun</w> gisteren het goede nieuws verteld.",
                            "target_lang_translations": {
                                "en": "I told <w>them</w> the good news yesterday.",
                                "ru": "Я рассказал <w>им</w> вчера хорошие новости."
                            }
                        },
                        {
                            "text": "De leraar geeft <w>hun</w> elke week huiswerk op.",
                            "target_lang_translations": {
                                "en": "The teacher gives <w>them</w> homework every week.",
                                "ru": "Учитель задает <w>им</w> домашнее задание каждую неделю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "het spijt hun",
                        "ik gun het hun"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "datief / meewerkend voorwerp"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To them; the form of the third person plural personal pronoun used as an indirect object (without a preposition).",
                        "ru": "Им; форма личного местоимения третьего лица множественного числа в качестве косвенного дополнения (дательный падеж)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "them",
                                "target_lang_sense_clarification": "indirect object pronoun"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "им",
                                "target_lang_sense_clarification": "дательный падеж"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e199888c-9e46-3223-81d8-e924c45c571d",
                    "sense_definition": "Zij; gebruikt als onderwerp van de zin (voornamelijk in spreektaal, vaak beschouwd als taalkundig onjuist in formele context).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "pronoun_subject",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Hun</w> hebben de wedstrijd gisteren gewonnen.",
                            "target_lang_translations": {
                                "en": "<w>They</w> won the match yesterday.",
                                "ru": "<w>Они</w> выиграли матч вчера."
                            }
                        },
                        {
                            "text": "Waarom zijn <w>hun</w> niet op het feest gekomen?",
                            "target_lang_translations": {
                                "en": "Why didn't <w>they</w> come to the party?",
                                "ru": "Почему <w>они</w> не пришли на вечеринку?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zijlui",
                        "zijlieden"
                    ],
                    "common_phrases": [
                        "hun hebben",
                        "hun zijn"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "spreektaal"
                        },
                        {
                            "trait_type": "form",
                            "comment": "onstandaard als onderwerp"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "They; used as the subject of the sentence (mainly in spoken language, often considered linguistically incorrect in formal contexts).",
                        "ru": "Они; используется как подлежащее предложения (преимущественно в разговорной речи, часто считается грамматически неправильным в формальном контексте)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "they",
                                "target_lang_sense_clarification": "subject pronoun"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "они",
                                "target_lang_sense_clarification": "личное местоимение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3cde62f6-c895-3b41-af86-f42240b7ed66",
                    "sense_definition": "Lid van een nomadisch ruitervolk uit Centraal-Azië dat in de 4e en 5e eeuw Europa binnendrong.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_historical",
                    "name_type": "ethnic_group_name",
                    "examples": [
                        {
                            "text": "Attila was de beroemdste koning van de <w>Hunnen</w>.",
                            "target_lang_translations": {
                                "en": "Attila was the most famous king of the <w>Huns</w>.",
                                "ru": "Аттила был самым известным королем <w>гуннов</w>."
                            }
                        },
                        {
                            "text": "De <w>Hunnen</w> stonden bekend als zeer bekwame ruiters.",
                            "target_lang_translations": {
                                "en": "The <w>Huns</w> were known as very skilled horsemen.",
                                "ru": "<w>Гунны</w> славились как очень искусные всадники."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "historische context"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Member of a nomadic equestrian people from Central Asia who invaded Europe in the 4th and 5th centuries.",
                        "ru": "Гунн; представитель кочевого народа из Центральной Азии, который вторгся в Европу в IV и V веках."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Hun",
                                "target_lang_sense_clarification": "historical ethnic group"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "гунн",
                                "target_lang_sense_clarification": "исторический народ"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6c31a911-7668-3780-a8d3-6d404782fd60",
                    "sense_definition": "Een barbaar of bruut persoon; iemand die zich onbeschaafd en ruw gedraagt.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "noun_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij gedroeg zich als een <w>Hun</w> tijdens het chique diner.",
                            "target_lang_translations": {
                                "en": "He behaved like a <w>barbarian</w> during the fancy dinner.",
                                "ru": "Он вел себя как <w>варвар</w> во время шикарного ужина."
                            }
                        },
                        {
                            "text": "Wat een <w>Hun</w> is die man, hij heeft totaal geen manieren.",
                            "target_lang_translations": {
                                "en": "What a <w>brute</w> that man is, he has absolutely no manners.",
                                "ru": "Какой же <w>дикарь</w> этот мужчина, у него совершенно нет манер."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A barbarian or brute person; someone who behaves in an uncivilized and rough manner.",
                        "ru": "Варвар или грубый человек; тот, кто ведет себя нецивилизованно и грубо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "barbarian",
                                "target_lang_sense_clarification": "uncivilized person"
                            },
                            {
                                "target_lang_word": "brute",
                                "target_lang_sense_clarification": "rough person"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "варвар",
                                "target_lang_sense_clarification": "грубый человек"
                            },
                            {
                                "target_lang_word": "дикарь",
                                "target_lang_sense_clarification": "нецивилизованный человек"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "90a9cbb8-84e2-38d9-af3e-49037f4b4e18",
                    "sense_definition": "Uczucie żalu i smutku wywołane rozłąką z kimś bliskim, brakiem czegoś lub utratą kogoś.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "emotion_sadness",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas długiej podróży czuł silną <tęsknotę> za domem.",
                            "target_lang_translations": {
                                "ru": "Во время долгого путешествия он чувствовал сильную <тоску> по дому."
                            }
                        },
                        {
                            "text": "Jej oczy były pełne smutku i głębokiej <tęsknoty>.",
                            "target_lang_translations": {
                                "ru": "Её глаза были полны печали и глубокой <тоски>."
                            }
                        }
                    ],
                    "synonyms": [
                        "nostalgia",
                        "utęsknienie",
                        "smutek"
                    ],
                    "antonyms": [
                        "wesołość"
                    ],
                    "common_phrases": [
                        "tęsknota za domem",
                        "tęsknota za krajem",
                        "umierać z tęsknoty",
                        "czuć tęsknotę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство сожаления и грусти, вызванное разлукой с близким человеком, отсутствием чего-либо или потерей кого-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тоска",
                                "target_lang_sense_clarification": "сильное чувство грусти и желания видеть кого-то или что-то"
                            },
                            {
                                "target_lang_word": "грусть",
                                "target_lang_sense_clarification": "чувство печали, вызванное отсутствием кого-либо"
                            },
                            {
                                "target_lang_word": "ностальгия",
                                "target_lang_sense_clarification": "тоска по родине или прошлому"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d480e182-8555-3ddc-81d0-749ab6e2e10c",
                    "sense_definition": "Silna chęć osiągnięcia jakiegoś celu lub pozyskania jakiejś wartości.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "desire",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W sercach ludzi od zawsze istniała <tęsknota> za wolnością.",
                            "target_lang_translations": {
                                "ru": "В сердцах людей всегда жила <жажда> свободы."
                            }
                        },
                        {
                            "text": "Jego nieustanna <tęsknota> do wiedzy budziła podziw nauczycieli.",
                            "target_lang_translations": {
                                "ru": "Его постоянная <тяга> к знаниям вызывала восхищение учителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "pociąg"
                    ],
                    "antonyms": [
                        "przesyt",
                        "odraza",
                        "awersja",
                        "wstręt",
                        "nadmiar"
                    ],
                    "common_phrases": [
                        "tęsknota za wolnością",
                        "tęsknota do wiedzy",
                        "tęsknota do świata"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сильное желание достичь какой-либо цели или обрести какую-либо ценность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "тяга",
                                "target_lang_sense_clarification": "сильное влечение или стремление к чему-либо"
                            },
                            {
                                "target_lang_word": "стремление",
                                "target_lang_sense_clarification": "настойчивое желание чего-нибудь достигнуть"
                            },
                            {
                                "target_lang_word": "жажда",
                                "target_lang_sense_clarification": "страстное желание обладать чем-либо"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "418d01b6-ad10-3355-a1d2-2d71432a4910",
                    "sense_definition": "Część kończyny górnej człowieka od nadgarstka do końców palców",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mężczyzna uśmiechnął się i podał mi <w>dłoń</w> na powitanie.",
                            "target_lang_translations": {
                                "ru": "Мужчина улыбнулся и протянул мне <w>руку</w> для приветствия."
                            }
                        },
                        {
                            "text": "Dziewczynka ogrzała zmarznięte <w>dłonie</w> nad ciepłym ogniem.",
                            "target_lang_translations": {
                                "ru": "Девочка погрела замёрзшие <w>руки</w> над тёплым огнём."
                            }
                        }
                    ],
                    "synonyms": [
                        "ręka"
                    ],
                    "common_phrases": [
                        "podać dłoń",
                        "uścisnąć dłoń",
                        "klaskać w dłonie",
                        "uścisk dłoni"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть верхней конечности человека от запястья до кончиков пальцев"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кисть",
                                "target_lang_sense_clarification": "анатомический термин"
                            },
                            {
                                "target_lang_word": "рука",
                                "target_lang_sense_clarification": "общее название"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "03d24663-d11f-3077-be23-478d93b7e167",
                    "sense_definition": "Wewnętrzna powierzchnia ręki, pozbawiona owłosienia i pokryta liniami papilarnymi",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "anatomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z tej odległości sytuacja była jasna i widoczna jak na <w>dłoni</w>.",
                            "target_lang_translations": {
                                "ru": "С этого расстояния ситуация была ясна и видна как на <w>ладони</w>."
                            }
                        },
                        {
                            "text": "Wróżka próbowała czytać moją przyszłość z linii <w>dłoni</w>.",
                            "target_lang_translations": {
                                "ru": "Гадалка пыталась прочитать моё будущее по линиям <w>ладони</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jak na dłoni",
                        "czytać z dłoni",
                        "wnętrze dłoni",
                        "schować twarz w dłoniach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внутренняя поверхность кисти руки, лишенная волос и покрытая папиллярными линиями"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ладонь",
                                "target_lang_sense_clarification": "внутренняя сторона кисти"
                            },
                            {
                                "target_lang_word": "длань",
                                "target_lang_sense_clarification": "устаревшее, поэтическое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6ad5ff32-fac9-35c8-ad9c-4651f07391c5",
                    "sense_definition": "Dawna miara długości odpowiadająca szerokości ludzkiej ręki",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "measurement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten stary mur był gruby na jedną <w>dłoń</w>.",
                            "target_lang_translations": {
                                "ru": "Эта старая стена была толщиной в одну <w>ладонь</w>."
                            }
                        },
                        {
                            "text": "Krawiec odmierzył dwie <w>dłonie</w> materiału na zapas.",
                            "target_lang_translations": {
                                "ru": "Портной отмерил две <w>ладони</w> ткани про запас."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "dawna miara"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинная мера длины, соответствующая ширине человеческой ладони"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ладонь",
                                "target_lang_sense_clarification": "мера длины"
                            }
                        ]
                    }
                }
            ]
        }
    ]

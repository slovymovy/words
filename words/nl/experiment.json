            "pos": "noun",
            "senses": [
                {
                    "sense_id": "70ddfa83-ae9e-3afd-80de-26e091c6b7f1",
                    "sense_definition": "Een wetenschappelijke proef die wordt uitgevoerd om een hypothese te testen, een theorie te bewijzen of nieuwe kennis op te doen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "science",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De onderzoekers voerden in het laboratorium een complex <w>experiment</w> uit.",
                            "target_lang_translations": {
                                "ru": "Исследователи провели в лаборатории сложный <w>эксперимент</w>."
                            }
                        },
                        {
                            "text": "Het resultaat van het <w>experiment</w> bevestigde dat de nieuwe medicijnen effectief zijn.",
                            "target_lang_translations": {
                                "ru": "Результат <w>эксперимента</w> подтвердил, что новые лекарства эффективны."
                            }
                        }
                    ],
                    "synonyms": [
                        "proef",
                        "test"
                    ],
                    "common_phrases": [
                        "een experiment uitvoeren",
                        "wetenschappelijk experiment",
                        "mislukt experiment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Научное испытание, проводимое для проверки гипотезы, доказательства теории или получения новых знаний."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эксперимент",
                                "target_lang_sense_clarification": "научный"
                            },
                            {
                                "target_lang_word": "опыт",
                                "target_lang_sense_clarification": "испытание, проба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fc12003d-6c36-3eac-b43a-4b195a318a17",
                    "sense_definition": "Een poging om iets nieuws uit te proberen om te zien wat het resultaat is, vaak zonder zekerheid over de uitkomst.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "general_trial",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het nieuwe recept was een leuk <w>experiment</w>, maar het smaakte niet zo goed.",
                            "target_lang_translations": {
                                "ru": "Новый рецепт был интересным <w>экспериментом</w>, но на вкус получилось не очень."
                            }
                        },
                        {
                            "text": "We zien deze nieuwe werkwijze als een <w>experiment</w> om te leren hoe we efficiënter kunnen zijn.",
                            "target_lang_translations": {
                                "ru": "Мы рассматриваем этот новый метод работы как <w>эксперимент</w>, чтобы научиться работать эффективнее."
                            }
                        }
                    ],
                    "synonyms": [
                        "proef",
                        "test"
                    ],
                    "common_phrases": [
                        "bij wijze van experiment",
                        "een geslaagd experiment",
                        "sociaal experiment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Попытка попробовать что-то новое, чтобы увидеть результат, часто без уверенности в исходе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "эксперимент",
                                "target_lang_sense_clarification": "попытка, проба"
                            }
                        ]
                    }
                }
            ]
        }

{
    "word_family": [
        "except",
        "exceptional",
        "exceptionally",
        "exceptionable"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c23d9cc6-b05d-3eab-8e29-c9ad1094cb3a",
                    "sense_definition": "Something or someone that does not follow a rule or is not included in a general statement.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "exclusion_entity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Most birds can fly, but the penguin is a well-known <w>exception</w>."
                        },
                        {
                            "text": "We usually do not allow pets, but we will make an <w>exception</w> in this case."
                        }
                    ],
                    "synonyms": [
                        "outlier"
                    ],
                    "antonyms": [
                        "generality",
                        "commonness"
                    ],
                    "common_phrases": [
                        "exception to the rule",
                        "make an exception",
                        "without exception",
                        "no exception"
                    ]
                },
                {
                    "sense_id": "41d40f72-0825-3f01-bc0d-5a79b1371c40",
                    "sense_definition": "The act of leaving someone or something out of a group or list.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "exclusion_act",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Everyone was invited to the party with the <w>exception</w> of the new neighbors."
                        },
                        {
                            "text": "The law applies to all citizens with the <w>exception</w> of those under eighteen."
                        }
                    ],
                    "common_phrases": [
                        "with the exception of"
                    ]
                },
                {
                    "sense_id": "89221f28-736f-3f4b-9b5e-a122ed0e90cc",
                    "sense_definition": "A feeling of disagreement, anger, or offense, usually used in the phrase 'take exception'.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "objection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She took great <w>exception</w> to the rude comments made about her work."
                        },
                        {
                            "text": "I take <w>exception</w> to the suggestion that I was not honest."
                        }
                    ],
                    "common_phrases": [
                        "take exception to"
                    ]
                },
                {
                    "sense_id": "ca55fe69-af33-3c7f-84a5-cbf74198031e",
                    "sense_definition": "An error or unexpected event that occurs during the execution of a computer program.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "computing_error",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The application crashed because of an unhandled null pointer <w>exception</w>."
                        },
                        {
                            "text": "Developers must write code to catch every possible <w>exception</w> to prevent errors."
                        }
                    ],
                    "common_phrases": [
                        "throw an exception",
                        "catch an exception",
                        "unhandled exception"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ]
                },
                {
                    "sense_id": "1829d40b-77b1-3f5c-8b62-ddfcd6c5aab8",
                    "sense_definition": "A formal objection or a specific clause in a legal document that excludes something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "legal_clause",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The defense attorney filed an <w>exception</w> to the judge's ruling."
                        },
                        {
                            "text": "The property deed contains an <w>exception</w> regarding the mineral rights."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "legal"
                        }
                    ]
                }
            ]
        }
    ]
}
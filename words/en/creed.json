                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Верить или доверять чему-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "верить",
                                "target_lang_sense_clarification": "устаревшее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ee730dc0-296b-3fea-b78e-a082ec05b9bc",
                    "sense_definition": "To provide or establish a creed for someone or something.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new organization was <w>creeded</w> to ensure everyone followed the same principles.",
                            "target_lang_translations": {
                                "ru": "Новая организация была <w>наделена кредо</w>, чтобы гарантировать, что все следуют одним и тем же принципам."
                            }
                        },
                        {
                            "text": "Scholars study religions that are less formally <w>creeded</w> than Christianity.",
                            "target_lang_translations": {
                                "ru": "Ученые изучают религии, которые менее формально <w>кодифицированы</w>, чем христианство."
                            }
                        }
                    ],
                    "traits": [
                        {

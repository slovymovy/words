            "senses": [
                {
                    "sense_id": "b4953252-0263-3e8f-9760-314752b36ae3",
                    "sense_definition": "De personificatie van het kwaad in religieuze context, vaak beschouwd als de tegenstander van God.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "religious_entity",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "In veel oude verhalen probeert de <w>duivel</w> de zielen van mensen te kopen.",
                            "target_lang_translations": {
                                "ru": "Во многих старых историях <w>дьявол</w> пытается купить души людей.",
                                "en": "In many old stories, the <w>devil</w> tries to buy people's souls."
                            }
                        },
                        {
                            "text": "Hij was des <w>duivels</w> toen hij merkte dat zijn fiets gestolen was.",
                            "target_lang_translations": {
                                "ru": "Он был <w>в бешенстве</w>, когда заметил, что его велосипед украли.",
                                "en": "He was <w>furious</w> when he noticed that his bike had been stolen."
                            }
                        }
                    ],
                    "synonyms": [
                        "Satan",
                        "Lucifer",
                        "Beëlzebub",
                        "Mefisto"
                    ],
                    "common_phrases": [
                        "advocaat van de duivel",
                        "des duivels",
                        "voor de duivel niet bang zijn"
                    ],
                    "target_lang_definitions": {
                        "ru": "Олицетворение зла в религиозном контексте, часто рассматриваемое как противник Бога.",
                        "en": "The personification of evil in a religious context, often considered the adversary of God."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дьявол",
                                "target_lang_sense_clarification": "религиозный концепт"
                            },
                            {
                                "target_lang_word": "сатана",
                                "target_lang_sense_clarification": "имя собственное"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "devil",
                                "target_lang_sense_clarification": "religious entity"
                            },
                            {
                                "target_lang_word": "Satan",
                                "target_lang_sense_clarification": "proper name"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8e0b3073-f373-3453-8c2d-d9dba58802f4",
                    "sense_definition": "Een zeer kwaadaardig, wreed of onuitstaanbaar persoon.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De dictator werd door zijn volk gezien als een ware <w>duivel</w>.",
                            "target_lang_translations": {
                                "ru": "Диктатор воспринимался своим народом как сущий <w>дьявол</w>.",
                                "en": "The dictator was seen by his people as a true <w>devil</w>."
                            }
                        },
                        {
                            "text": "Ze vocht als een <w>duivel</w> om haar kinderen te beschermen.",
                            "target_lang_translations": {
                                "ru": "Она сражалась как <w>дьявол</w>, чтобы защитить своих детей.",
                                "en": "She fought like a <w>devil</w> to protect her children."
                            }
                        }
                    ],
                    "common_phrases": [

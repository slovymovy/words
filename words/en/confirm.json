{
    "word_family": [
        "confirmation",
        "confirmatory",
        "confirmable",
        "confirmee",
        "confirmer"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4a4e6888-248c-37a1-9473-9fed105f217a",
                    "sense_definition": "To state or prove that something is true or correct.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "verify",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>confirm</w> your email address by clicking the link.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>подтвердите</w> ваш адрес электронной почты, нажав на ссылку."
                            }
                        },
                        {
                            "text": "The test results <w>confirmed</w> that the patient had a virus.",
                            "target_lang_translations": {
                                "ru": "Результаты анализов <w>подтвердили</w>, что у пациента вирус."
                            }
                        }
                    ],
                    "antonyms": [
                        "deny",
                        "contradict",
                        "disconfirm",
                        "dispute",
                        "question"
                    ],
                    "common_phrases": [
                        "confirm a booking",
                        "confirm the details",
                        "confirm receipt",
                        "confirm a suspicion"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заявить или доказать, что что-то является истинным или правильным."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подтверждать",
                                "target_lang_sense_clarification": "удостоверять истинность"
                            },
                            {
                                "target_lang_word": "подтвердить",
                                "target_lang_sense_clarification": "засвидетельствовать достоверность (сов. вид)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62f0542f-7a7a-37bc-ab1a-c8053c0f78e2",
                    "sense_definition": "To give official approval to a person, agreement, or position.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "approve",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The Senate voted to <w>confirm</w> the president's nominee for the court.",
                            "target_lang_translations": {
                                "ru": "Сенат проголосовал за то, чтобы <w>утвердить</w> кандидата президента в суд."
                            }
                        },
                        {
                            "text": "The board of directors met to <w>confirm</w> the new CEO.",
                            "target_lang_translations": {
                                "ru": "Совет директоров собрался, чтобы <w>утвердить</w> нового генерального директора."
                            }
                        }
                    ],
                    "common_phrases": [
                        "confirm an appointment",
                        "confirm a nomination",
                        "Senate confirmed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дать официальное одобрение лицу, соглашению или должности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "утверждать",
                                "target_lang_sense_clarification": "официально одобрять назначение или решение"
                            },
                            {
                                "target_lang_word": "подтверждать",
                                "target_lang_sense_clarification": "признавать законным или действительным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f2422e92-562e-35fb-b71a-d0eb2ae5c692",
                    "sense_definition": "To make a feeling, belief, or resolve stronger.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "strengthen",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The recent victory <w>confirmed</w> his determination to win the championship.",
                            "target_lang_translations": {
                                "ru": "Недавняя победа <w>укрепила</w> его решимость выиграть чемпионат."
                            }
                        },
                        {
                            "text": "Her success in the project <w>confirmed</w> her in the belief that she was on the right path.",
                            "target_lang_translations": {
                                "ru": "Успех в проекте <w>укрепил</w> её в убеждении, что она на правильном пути."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделать чувство, убеждение или решимость более сильными."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "укреплять",
                                "target_lang_sense_clarification": "делать более сильным или твердым"
                            },
                            {
                                "target_lang_word": "подкреплять",
                                "target_lang_sense_clarification": "обосновывать или усиливать чем-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bdf84845-f13c-3337-b589-e226c78ec715",
                    "sense_definition": "To administer the religious rite of confirmation to someone.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "religious",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was baptized as a baby and <w>confirmed</w> when he was a teenager.",
                            "target_lang_translations": {
                                "ru": "Он был крещён младенцем, а <w>конфирмован</w> в подростковом возрасте."
                            }
                        },
                        {
                            "text": "The bishop visited the parish to <w>confirm</w> the group of young people.",
                            "target_lang_translations": {
                                "ru": "Епископ посетил приход, чтобы <w>совершить конфирмацию</w> группы молодых людей."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Совершить религиозный обряд конфирмации (миропомазания)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совершать конфирмацию",
                                "target_lang_sense_clarification": "проводить религиозный обряд"
                            },
                            {
                                "target_lang_word": "конфирмовать",
                                "target_lang_sense_clarification": "термин в западном христианстве"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "cd693b70-329d-3e8d-b3ab-6ae3d5a2406f",
                    "sense_definition": "Definitely or for sure.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "colloquial_adv",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is <w>confirm</w> going to be late for the meeting.",
                            "target_lang_translations": {
                                "ru": "Он <w>точно</w> опоздает на встречу."
                            }
                        },
                        {
                            "text": "That answer is <w>confirm</w> not the right one.",
                            "target_lang_translations": {
                                "ru": "Этот ответ <w>точно</w> неправильный."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "Singlish / Malaysian English"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Определённо, точно или наверняка (разговорное)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "точно",
                                "target_lang_sense_clarification": "несомненно"
                            },
                            {
                                "target_lang_word": "наверняка",
                                "target_lang_sense_clarification": "без сомнения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

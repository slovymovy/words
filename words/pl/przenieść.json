            "senses": [
                {
                    "sense_id": "84a34576-585c-341f-b8dd-4bda8c06dbdb",
                    "sense_definition": "Zabrać kogoś lub coś z jednego miejsca i umieścić w innym",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <przenieść> to ciężkie pudełko do garażu.",
                            "target_lang_translations": {
                                "ru": "Ты должен <перенести> эту тяжелую коробку в гараж."
                            }
                        },
                        {
                            "text": "Ojciec pomógł mi <przenieść> szafę do drugiego pokoju.",
                            "target_lang_translations": {
                                "ru": "Отец помог мне <перенести> шкаф в другую комнату."
                            }
                        }
                    ],
                    "synonyms": [
                        "przestawić",
                        "przemieścić",
                        "przetransportować"
                    ],
                    "antonyms": [
                        "zostawić"
                    ],
                    "common_phrases": [
                        "przenieść na rękach",
                        "przenieść przez próg",
                        "przenieść meble"
                    ],
                    "target_lang_definitions": {
                        "ru": "Взять кого-то или что-то с одного места и переместить в другое"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перенести",
                                "target_lang_sense_clarification": "физически переместить"
                            },
                            {
                                "target_lang_word": "переместить",
                                "target_lang_sense_clarification": "сдвинуть с места"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e43e1666-7289-33cc-8b09-da04679b98b9",
                    "sense_definition": "Zmienić miejsce własnego zamieszkania lub pobytu (używane z zaimkiem \"się\")",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "relocation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moi sąsiedzi postanowili <przenieść> się na wieś.",
                            "target_lang_translations": {
                                "ru": "Мои соседи решили <переехать> в деревню."
                            }
                        },
                        {
                            "text": "Chciałbym <przenieść> się do większego miasta, żeby znaleźć lepszą pracę.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы <переехать> в город побольше, чтобы найти работу получше."
                            }
                        }
                    ],
                    "synonyms": [
                        "przeprowadzić się"
                    ],
                    "common_phrases": [
                        "przenieść się na wieś",
                        "przenieść się do miasta",
                        "przenieść się na stałe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сменить место жительства или пребывания (используется с возвратным местоимением \"się\")"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переехать",
                                "target_lang_sense_clarification": "сменить жилье"
                            },
                            {
                                "target_lang_word": "переселиться",
                                "target_lang_sense_clarification": "поселиться в другом месте"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b3a045c-747c-3754-91e7-6500840daed1",
                    "sense_definition": "Zmienić ustalony termin wydarzenia na inny, zazwyczaj późniejszy",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "time_management",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z powodu choroby musieliśmy <przenieść> nasze spotkanie na przyszły tydzień.",
                            "target_lang_translations": {
                                "ru": "Из-за болезни нам пришлось <перенести> нашу встречу на следующую неделю."
                            }
                        },
                        {
                            "text": "Organizatorzy zdecydowali <przenieść> koncert na inny dzień z powodu deszczu.",
                            "target_lang_translations": {
                                "ru": "Организаторы решили <перенести> концерт на другой день из-за дождя."
                            }
                        }
                    ],
                    "synonyms": [
                        "przełożyć",
                        "przesunąć"
                    ],
                    "antonyms": [
                        "przyspieszyć"
                    ],
                    "common_phrases": [
                        "przenieść spotkanie",
                        "przenieść wizytę",
                        "przenieść na później",
                        "przenieść termin"
                    ],
                    "target_lang_definitions": {
                        "ru": "Изменить назначенную дату события на другую, обычно более позднюю"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "перенести",
                                "target_lang_sense_clarification": "назначить на другое время"
                            },
                            {
                                "target_lang_word": "отложить",
                                "target_lang_sense_clarification": "отодвинуть по времени"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "92968179-7340-3acb-975f-73d8dd9412a5",
                    "sense_definition": "Zmienić miejsce pracy, nauki lub siedzibę instytucji na inne",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "relocation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Firma planuje <przenieść> swoją główną siedzibę do Warszawy.",
                            "target_lang_translations": {
                                "ru": "Фирма планирует <перенести> свою штаб-квартиру в Варшаву."
                            }
                        },
                        {
                            "text": "Dyrektor chciał go <przenieść> do innego działu, ale on odmówił.",
                            "target_lang_translations": {
                                "ru": "Директор хотел <перевести> его в другой отдел, но он отказался."
                            }
                        }
                    ],
                    "synonyms": [
                        "oddelegować",
                        "przetransferować"

{
    "word_family": [
        "быт",
        "бытие",
        "событие",
        "небытие",
        "прибытие",
        "убытие",
        "пребывание",
        "суть"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "02572db4-52be-36c6-96c7-e1056d044c37",
                    "sense_definition": "Являться кем-либо, чем-либо или обладать каким-либо качеством (используется как связка между подлежащим и сказуемым).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "copula",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мой дедушка <был> очень добрым человеком.",
                            "target_lang_translations": {
                                "en": "My grandfather <was> a very kind man."
                            }
                        },
                        {
                            "text": "Завтра погода <будет> солнечной и тёплой.",
                            "target_lang_translations": {
                                "en": "Tomorrow the weather <will be> sunny and warm."
                            }
                        }
                    ],
                    "synonyms": [
                        "являться"
                    ],
                    "common_phrases": [
                        "быть готовым",
                        "быть уверенным",
                        "быть правым",
                        "быть счастливым"
                    ],
                    "target_lang_definitions": {
                        "en": "To be someone, something, or to possess a certain quality (used as a link between subject and predicate)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be",
                                "target_lang_sense_clarification": "copula / linking verb"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1b9cb1f-3cc6-33f7-9ffc-9426115a7253",
                    "sense_definition": "Иметься в наличии или в распоряжении (обычно используется в конструкции «у кого-то есть»).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "possession",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "У меня <есть> отличная идея для подарка.",
                            "target_lang_translations": {
                                "en": "I <have> a great idea for a gift."
                            }
                        },
                        {
                            "text": "У них совсем не <было> времени на отдых.",
                            "target_lang_translations": {
                                "en": "They <had> absolutely no time for rest."
                            }
                        }
                    ],
                    "synonyms": [
                        "иметься"
                    ],
                    "antonyms": [
                        "отсутствовать"
                    ],
                    "common_phrases": [
                        "у меня есть",
                        "у тебя есть",
                        "у нас было"
                    ],
                    "target_lang_definitions": {
                        "en": "To be available or at one's disposal (usually used in the construction 'someone has')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "have",
                                "target_lang_sense_clarification": "possession (equivalent to 'u menya est')"
                            },
                            {
                                "target_lang_word": "be",
                                "target_lang_sense_clarification": "existence / availability (there is/are)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e38b3dcb-9e3b-3145-9761-7ec0c1aa37df",
                    "sense_definition": "Находиться или присутствовать в определённом месте.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Вчера вечером мы <были> в театре.",
                            "target_lang_translations": {
                                "en": "Last night we <were> at the theater."
                            }
                        },
                        {
                            "text": "Твои ключи <будут> лежать на столе.",
                            "target_lang_translations": {
                                "en": "Your keys <will be> lying on the table."
                            }
                        }
                    ],
                    "synonyms": [
                        "находиться",
                        "присутствовать"
                    ],
                    "antonyms": [
                        "отсутствовать"
                    ],
                    "common_phrases": [
                        "быть дома",
                        "быть на работе",
                        "быть в гостях",
                        "быть на месте"
                    ],
                    "target_lang_definitions": {
                        "en": "To be located or present in a specific place."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be",
                                "target_lang_sense_clarification": "location"
                            },
                            {
                                "target_lang_word": "be located",
                                "target_lang_sense_clarification": "formal location"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "335f804c-4540-3f42-87f4-ef86c3c8b931",
                    "sense_definition": "Служит вспомогательным глаголом для образования будущего времени глаголов несовершенного вида.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "auxiliary_future",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я <буду> читать эту книгу весь вечер.",
                            "target_lang_translations": {
                                "en": "I <will> read this book all evening."
                            }
                        },
                        {
                            "text": "Мы <будем> ждать вас около входа в парк.",
                            "target_lang_translations": {
                                "en": "We <will> wait for you near the park entrance."
                            }
                        }
                    ],
                    "common_phrases": [
                        "буду делать",
                        "будем знать",
                        "будет работать"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "auxiliary verb"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Serves as an auxiliary verb to form the future tense of imperfective verbs."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "will",
                                "target_lang_sense_clarification": "future auxiliary"
                            },
                            {
                                "target_lang_word": "be",
                                "target_lang_sense_clarification": "in 'going to' constructions"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "30bb3039-5422-3c14-a849-426d8df67c14",
                    "sense_definition": "Происходить, случаться или намечаться (о событиях, мероприятиях).",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "occurrence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Важное собрание <будет> завтра в десять часов.",
                            "target_lang_translations": {
                                "en": "The important meeting <will be> tomorrow at ten o'clock."
                            }
                        },
                        {
                            "text": "Никто не знал, что <было> на прошлом уроке.",
                            "target_lang_translations": {
                                "en": "No one knew what <happened> in the last lesson."
                            }
                        }
                    ],
                    "synonyms": [
                        "поступать",
                        "являться"
                    ],
                    "common_phrases": [
                        "что будет то будет",
                        "быть беде",
                        "урок будет"
                    ],
                    "target_lang_definitions": {
                        "en": "To occur, happen, or be scheduled (about events, activities)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be",
                                "target_lang_sense_clarification": "occurrence"
                            },
                            {
                                "target_lang_word": "take place",
                                "target_lang_sense_clarification": "event occurrence"
                            },
                            {
                                "target_lang_word": "happen",
                                "target_lang_sense_clarification": "event occurrence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4d6bc604-2e5a-329a-824f-b2c838d72c6f",
                    "sense_definition": "Хотеть или собираться съесть или выпить (заменяет глаголы «хотеть», «есть», «пить» в разговорной речи).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "consumption",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ты <будешь> чай или кофе с молоком?",
                            "target_lang_translations": {
                                "en": "<Will> you <have> tea or coffee with milk?"
                            }
                        },
                        {
                            "text": "Спасибо, но я не <буду> суп, я не голоден.",
                            "target_lang_translations": {
                                "en": "Thank you, but I <will> not <have> soup, I am not hungry."
                            }
                        }
                    ],
                    "common_phrases": [
                        "будешь чай",
                        "не буду есть"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To want or intend to eat or drink (replaces 'want', 'eat', 'drink' in colloquial speech)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "have",
                                "target_lang_sense_clarification": "consume food/drink"
                            },
                            {
                                "target_lang_word": "want",
                                "target_lang_sense_clarification": "desire"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "62fa6b18-4480-34c4-b6fa-ed228eec7610",
                    "sense_definition": "Существовать, жить в действительности.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "existence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Таких странных животных в природе не <бывает>.",
                            "target_lang_translations": {
                                "en": "Such strange animals do not <exist> in nature."
                            }
                        },
                        {
                            "text": "Этому не <бывать>, пока я здесь главный.",
                            "target_lang_translations": {
                                "en": "This <will> not <happen> as long as I am in charge here."
                            }
                        }
                    ],
                    "synonyms": [
                        "существовать"
                    ],
                    "common_phrases": [
                        "быть или не быть",
                        "так и быть",
                        "может быть"
                    ],
                    "target_lang_definitions": {
                        "en": "To exist, to live in reality."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "exist",
                                "target_lang_sense_clarification": "have reality"
                            },
                            {
                                "target_lang_word": "be",
                                "target_lang_sense_clarification": "existence"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "36353e3e-a284-33fd-aa1d-8ac6747fc8ed",
                    "sense_definition": "Используется для образования форм страдательного залога.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "auxiliary_passive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот старинный дом <был> построен в прошлом веке.",
                            "target_lang_translations": {
                                "en": "This ancient house <was> built in the last century."
                            }
                        },
                        {
                            "text": "Окончательное решение <будет> принято на следующей неделе.",
                            "target_lang_translations": {
                                "en": "The final decision <will be> made next week."
                            }
                        }
                    ],
                    "common_phrases": [
                        "был сделан",
                        "будет решено"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "auxiliary verb"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Used to form passive voice forms."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "be",
                                "target_lang_sense_clarification": "passive auxiliary"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
{
    "word_family": [
        "rijk",
        "verrijken",
        "rijkaard",
        "rijkelijk"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "78818dd3-4655-3e23-a20c-47e3bc917832",
                    "sense_definition": "De toestand van het bezitten van veel geld, goederen en andere waardevolle zaken.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "material_wealth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij leefde jarenlang in grote <w>rijkdom</w> in zijn villa.",
                            "target_lang_translations": {
                                "en": "He lived in great <w>wealth</w> in his villa for years.",
                                "ru": "Он много лет жил в огромном <w>богатстве</w> на своей вилле."
                            }
                        },
                        {
                            "text": "Veel mensen dromen ervan om van armoede naar <w>rijkdom</w> te groeien.",
                            "target_lang_translations": {
                                "en": "Many people dream of rising from poverty to <w>wealth</w>.",
                                "ru": "Многие люди мечтают вырваться из нищеты к <w>богатству</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "welvaart",
                        "vermogen",
                        "weelde",
                        "financiën"
                    ],
                    "antonyms": [
                        "armoede"
                    ],
                    "common_phrases": [
                        "in rijkdom leven",
                        "rijkdom vergaren",
                        "macht en rijkdom"
                    ],
                    "target_lang_definitions": {
                        "en": "The state of possessing a lot of money, goods, and other valuable items.",
                        "ru": "Состояние обладания большими деньгами, имуществом и другими ценностями."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wealth",
                                "target_lang_sense_clarification": "general state of being rich"
                            },
                            {
                                "target_lang_word": "riches",
                                "target_lang_sense_clarification": "emphasizing the valuable possessions themselves"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "богатство",
                                "target_lang_sense_clarification": "материальное благополучие"
                            },
                            {
                                "target_lang_word": "состояние",
                                "target_lang_sense_clarification": "крупный капитал"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "afa078be-a923-326c-88c7-fafd52e22bd7",
                    "sense_definition": "Een grote hoeveelheid of verscheidenheid van iets.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abundance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In het oerwoud is een enorme <w>rijkdom</w> aan bomen en planten te vinden.",
                            "target_lang_translations": {
                                "en": "An enormous <w>wealth</w> of trees and plants can be found in the jungle.",
                                "ru": "В джунглях можно найти огромное <w>богатство</w> деревьев и растений."
                            }
                        },
                        {
                            "text": "Dit boek biedt een <w>rijkdom</w> aan informatie over de geschiedenis.",
                            "target_lang_translations": {
                                "en": "This book offers a <w>wealth</w> of information about history.",
                                "ru": "Эта книга предлагает <w>изобилие</w> информации об истории."
                            }
                        }
                    ],
                    "synonyms": [
                        "abundantie"
                    ],
                    "common_phrases": [
                        "een rijkdom aan"
                    ],
                    "target_lang_definitions": {
                        "en": "A large quantity or variety of something.",
                        "ru": "Большое количество или разнообразие чего-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wealth",
                                "target_lang_sense_clarification": "often used in the phrase 'a wealth of'"
                            },
                            {
                                "target_lang_word": "abundance",
                                "target_lang_sense_clarification": "a very large quantity"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "изобилие",
                                "target_lang_sense_clarification": "большое количество"
                            },
                            {
                                "target_lang_word": "богатство",
                                "target_lang_sense_clarification": "разнообразие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d86d585c-121d-3784-98a0-e5f0da24bdb1",
                    "sense_definition": "Het bezitten van immateriële waarden zoals geluk, gezondheid of liefde.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_wealth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij had niet veel geld, maar beschouwde zijn gezin en zijn gezondheid als echte <w>rijkdom</w>.",
                            "target_lang_translations": {
                                "en": "He didn't have much money, but considered his family and his health as true <w>wealth</w>.",
                                "ru": "У него было немного денег, но он считал свою семью и здоровье настоящим <w>богатством</w>."
                            }
                        },
                        {
                            "text": "Zij vond haar <w>rijkdom</w> in de kleine dingen van het leven.",
                            "target_lang_translations": {
                                "en": "She found her <w>wealth</w> in the small things of life.",
                                "ru": "Она находила свое <w>богатство</w> в мелочах жизни."
                            }
                        }
                    ],
                    "synonyms": [
                        "welzijn"
                    ],
                    "common_phrases": [
                        "innerlijke rijkdom",
                        "geestelijke rijkdom"
                    ],
                    "target_lang_definitions": {
                        "en": "Possessing intangible values such as happiness, health, or love.",
                        "ru": "Обладание нематериальными ценностями, такими как счастье, здоровье или любовь."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wealth",
                                "target_lang_sense_clarification": "metaphorical value"
                            },
                            {
                                "target_lang_word": "richness",
                                "target_lang_sense_clarification": "quality or depth of experience"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "богатство",
                                "target_lang_sense_clarification": "духовные или внутренние ценности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b8e1a285-d2d7-39d0-87ec-249e807ac93a",
                    "sense_definition": "De concrete bezittingen of schatten die iemand rijk maken (vaak in het meervoud gebruikt).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "material_wealth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>rijkdommen</w> die hij in het paleis te zien kreeg, verbijsterden hem.",
                            "target_lang_translations": {
                                "en": "The <w>riches</w> he got to see in the palace astonished him.",
                                "ru": "<w>Богатства</w>, которые он увидел во дворце, ошеломили его."
                            }
                        },
                        {
                            "text": "Het land is bekend om zijn vele natuurlijke <w>rijkdommen</w> in de bodem.",
                            "target_lang_translations": {
                                "en": "The country is known for its many natural <w>resources</w> in the soil.",
                                "ru": "Страна известна своими многочисленными природными <w>богатствами</w> в недрах."
                            }
                        }
                    ],
                    "synonyms": [
                        "middelen"
                    ],
                    "common_phrases": [
                        "natuurlijke rijkdommen",
                        "verborgen rijkdommen"
                    ],
                    "target_lang_definitions": {
                        "en": "The concrete possessions or treasures that make someone rich (often used in plural).",
                        "ru": "Конкретные владения или сокровища, делающие кого-то богатым (часто во множественном числе)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "riches",
                                "target_lang_sense_clarification": "valuable material possessions"
                            },
                            {
                                "target_lang_word": "resources",
                                "target_lang_sense_clarification": "specifically for 'natuurlijke rijkdommen' (natural resources)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "богатства",
                                "target_lang_sense_clarification": "ценности, сокровища"
                            },
                            {
                                "target_lang_word": "ресурсы",
                                "target_lang_sense_clarification": "природные запасы"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
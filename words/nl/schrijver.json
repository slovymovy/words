                            },
                            {
                                "target_lang_word": "clerk",
                                "target_lang_sense_clarification": "historical administrative worker"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "писец",
                                "target_lang_sense_clarification": "переписчик рукописей"
                            },
                            {
                                "target_lang_word": "писарь",

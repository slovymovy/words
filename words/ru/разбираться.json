    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "76502e8c-e6c7-333a-94f0-7ff46d047c78",
                    "sense_definition": "Быть сведущим, иметь хорошие знания, навыки или понимание в какой-либо области.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "knowledge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Он финансист и хорошо во всём этом <w>разбирается</w>.",
                            "target_lang_translations": {
                                "en": "He is a financier and <w>understands</w> all this well."
                            }
                        },
                        {
                            "text": "<w>Разбираться</w> в ракетах нелегко — надо физику понимать.",
                            "target_lang_translations": {
                                "en": "<w>Understanding</w> rockets is not easy — you have to understand physics."
                            }
                        }
                    ],
                    "synonyms": [
                        "понимать"
                    ],
                    "common_phrases": [
                        "разбираться в людях",
                        "разбираться в технике",
                        "плохо разбираться"
                    ],
                    "target_lang_definitions": {
                        "en": "To possess knowledge, skills, or understanding in a specific area."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "understand",
                                "target_lang_sense_clarification": "comprehend a subject or topic"
                            },
                            {
                                "target_lang_word": "know about",
                                "target_lang_sense_clarification": "have knowledge regarding something"
                            },
                            {
                                "target_lang_word": "be good at",
                                "target_lang_sense_clarification": "have skill or competence in"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2687f559-21b5-3b3f-bcea-6ea2ff37d10e",
                    "sense_definition": "Вникать в подробности чего-либо, чтобы понять, выяснить или решить вопрос; улаживать сложную ситуацию.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "action_resolution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне лень <w>разбираться</w> со сломанным окном.",
                            "target_lang_translations": {
                                "en": "I'm too lazy to <w>deal with</w> the broken window."
                            }
                        },
                        {
                            "text": "Начальник обещал лично <w>разбираться</w> с каждой жалобой клиента.",
                            "target_lang_translations": {
                                "en": "The boss promised to personally <w>look into</w> every customer complaint."
                            }
                        }
                    ],
                    "synonyms": [
                        "выяснять"
                    ],
                    "common_phrases": [
                        "разбираться с делами",
                        "разбираться с проблемой",
                        "разбираться в ситуации"
                    ],
                    "target_lang_definitions": {
                        "en": "To examine details to solve a problem, figure something out, or handle a difficult situation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sort out",
                                "target_lang_sense_clarification": "resolve a problem or situation"
                            },
                            {
                                "target_lang_word": "deal with",
                                "target_lang_sense_clarification": "handle or manage a situation"
                            },
                            {
                                "target_lang_word": "look into",
                                "target_lang_sense_clarification": "investigate or examine details"
                            },
                            {
                                "target_lang_word": "figure out",
                                "target_lang_sense_clarification": "understand or solve after thinking"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "18698b89-e282-347f-aa53-0a47091cade3",
                    "sense_definition": "Раскладывать свои вещи по местам, наводить порядок (обычно после приезда, переезда или покупки).",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "tidy_up",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы только вернулись из отпуска и будем весь вечер <w>разбираться</w>.",
                            "target_lang_translations": {
                                "en": "We just returned from vacation and will be <w>unpacking</w> all evening."
                            }
                        },
                        {
                            "text": "Иди в свою комнату и начинай <w>разбираться</w> в шкафу.",
                            "target_lang_translations": {
                                "en": "Go to your room and start <w>tidying up</w> the closet."
                            }
                        }
                    ],
                    "antonyms": [
                        "собираться"
                    ],
                    "common_phrases": [
                        "разбираться в вещах",
                        "разбираться в комнате"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To put things in their places, organize belongings, or unpack (usually after a trip or moving)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "unpack",
                                "target_lang_sense_clarification": "remove contents from luggage or boxes"
                            },
                            {
                                "target_lang_word": "tidy up",
                                "target_lang_sense_clarification": "put things in order"
                            },
                            {
                                "target_lang_word": "sort out",
                                "target_lang_sense_clarification": "organize a collection of items"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5fb93cd3-9ae1-3f8d-a577-d9d6ddaf2434",
                    "sense_definition": "Быть устроенным так, что можно разделить на части; страд. к <w>разбирать</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "passive_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Этот большой стол легко <w>разбирается</w> для перевозки.",
                            "target_lang_translations": {
                                "en": "This large table easily <w>comes apart</w> for transport."
                            }
                        },
                        {
                            "text": "Конструкция <w>разбирается</w> на три основные части.",
                            "target_lang_translations": {
                                "en": "The structure <w>disassembles</w> into three main parts."
                            }
                        }
                    ],
                    "common_phrases": [
                        "легко разбираться",
                        "полностью разбираться"
                    ],
                    "target_lang_definitions": {
                        "en": "To be constructed in a way that allows separation into parts; to be capable of being dismantled."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "come apart",
                                "target_lang_sense_clarification": "separate into pieces"
                            },
                            {
                                "target_lang_word": "be taken apart",
                                "target_lang_sense_clarification": "passive capability to be disassembled"
                            },
                            {
                                "target_lang_word": "dismantle",
                                "target_lang_sense_clarification": "intransitive: to disassemble into parts"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
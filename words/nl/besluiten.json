            "senses": [
                {
                    "sense_id": "30470c36-36f5-3b0f-bf37-b04f05652a29",
                    "sense_definition": "Een keuze maken na erover nagedacht te hebben; vaststellen wat er moet gebeuren.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "decision_making",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik heb gisteren <w>besloten</w> om te stoppen met roken.",
                            "target_lang_translations": {
                                "en": "I <w>decided</w> yesterday to quit smoking.",
                                "ru": "Вчера я <w>решил</w> бросить курить."
                            }
                        },
                        {
                            "text": "De directie <w>besloot</w> om het nieuwe plan goed te keuren.",
                            "target_lang_translations": {
                                "en": "The board <w>decided</w> to approve the new plan.",
                                "ru": "Руководство <w>решило</w> утвердить новый план."
                            }
                        }
                    ],
                    "synonyms": [
                        "beslissen"
                    ],
                    "common_phrases": [
                        "besluiten te doen",
                        "gezamenlijk besluiten",
                        "anders besluiten"
                    ],
                    "target_lang_definitions": {
                        "en": "To make a choice after thinking about it; to determine what needs to happen.",
                        "ru": "Сделать выбор после обдумывания; определить, что должно произойти."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "decide",
                                "target_lang_sense_clarification": "to make a choice"
                            },
                            {
                                "target_lang_word": "determine",
                                "target_lang_sense_clarification": "to settle or establish"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "решить",
                                "target_lang_sense_clarification": "сделать выбор"
                            },
                            {
                                "target_lang_word": "принимать решение",
                                "target_lang_sense_clarification": "официально или формально"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b5bee171-a896-315e-8cea-0e37f2bf8566",
                    "sense_definition": "Een einde maken aan een activiteit of bijeenkomst.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "ending",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We <w>besloten</w> de gezellige avond met een kopje koffie.",
                            "target_lang_translations": {

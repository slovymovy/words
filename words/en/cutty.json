    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "ecbd6339-f4cb-3b23-bb7e-3f34fa662398",
                    "sense_definition": "Short or shortened in length; curtailed.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "size_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The witch in the poem wore a <w>cutty</w> shirt that was far too small for her.",
                            "target_lang_translations": {
                                "ru": "Ведьма в стихотворении была одета в <w>короткую</w> рубашку, которая была ей слишком мала."
                            }
                        },
                        {
                            "text": "He sat uncomfortably on the <w>cutty</w> stool during the long sermon.",
                            "target_lang_translations": {
                                "ru": "Он неудобно сидел на <w>низкой</w> скамейке во время долгой проповеди."
                            }
                        }
                    ],
                    "synonyms": [
                        "short",
                        "brief",
                        "curtailed"
                    ],
                    "antonyms": [
                        "long",
                        "lengthy"
                    ],
                    "common_phrases": [
                        "cutty sark",
                        "cutty stool"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Scottish"
                        },
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Короткий или укороченный по длине; обрезанный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "короткий",
                                "target_lang_sense_clarification": "о длине предмета"
                            },
                            {
                                "target_lang_word": "куцый",
                                "target_lang_sense_clarification": "слишком короткий (разг.)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "939c7faf-666c-33a3-9f9e-5e29b6d03747",
                    "sense_definition": "Sharp or capable of cutting easily.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sharp_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Be careful with that <w>cutty</w> knife while you prepare the vegetables.",
                            "target_lang_translations": {
                                "ru": "Будь осторожен с этим <w>острым</w> ножом, когда готовишь овощи."
                            }
                        },
                        {
                            "text": "The wind had a <w>cutty</w> edge to it as it blew across the moor.",
                            "target_lang_translations": {
                                "ru": "Ветер был <w>пронизывающим</w>, когда дул над вересковой пустошью."
                            }
                        }
                    ],
                    "synonyms": [
                        "sharp",
                        "keen"
                    ],
                    "antonyms": [
                        "blunt",
                        "dull"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Scottish"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Острый или способный легко резать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "острый",
                                "target_lang_sense_clarification": "о лезвии или ветре"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "3160dae7-16ac-369f-bb1d-22e645c80fd6",
                    "sense_definition": "A tobacco pipe with a short stem.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "pipe_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old sailor sat by the fire smoking his clay <w>cutty</w>.",
                            "target_lang_translations": {
                                "ru": "Старый моряк сидел у огня, куря свою глиняную <w>короткую трубку</w>."
                            }
                        },
                        {
                            "text": "He tapped the ash out of his <w>cutty</w> before refilling it with tobacco.",
                            "target_lang_translations": {
                                "ru": "Он выбил пепел из своей <w>трубки</w> перед тем, как снова набить её табаком."
                            }
                        }
                    ],
                    "synonyms": [
                        "pipe",
                        "clay pipe"
                    ],
                    "common_phrases": [
                        "clay cutty"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Scottish/Irish"
                        },
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Курительная трубка с коротким мундштуком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "короткая трубка",
                                "target_lang_sense_clarification": "курительная"
                            },
                            {
                                "target_lang_word": "носогрейка",
                                "target_lang_sense_clarification": "разг. название короткой трубки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cbee1db4-ad0f-362e-8842-616f2e153131",
                    "sense_definition": "A girl or young woman, often used affectionately or to describe someone mischievous.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "person_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The little <w>cutty</w> ran through the garden laughing at her brother.",
                            "target_lang_translations": {
                                "ru": "Маленькая <w>озорница</w> бегала по саду, смеясь над своим братом."
                            }
                        },
                        {
                            "text": "He told stories to the young <w>cutties</w> gathered around his chair.",
                            "target_lang_translations": {
                                "ru": "Он рассказывал истории юным <w>девчонкам</w>, собравшимся вокруг его кресла."
                            }
                        }
                    ],
                    "synonyms": [
                        "lass",
                        "girl"
                    ],
                    "antonyms": [
                        "lad",
                        "boy"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Scottish"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Девушка или молодая женщина (часто ласково или озорно)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "девчонка",
                                "target_lang_sense_clarification": "молодая девушка"
                            },
                            {
                                "target_lang_word": "озорница",
                                "target_lang_sense_clarification": "шаловливая девушка"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f09d4c92-0f1d-3158-ad88-a90be75321aa",
                    "sense_definition": "A spoon with a short handle.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "utensil_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She ate her porridge using a wooden <w>cutty</w>.",
                            "target_lang_translations": {
                                "ru": "Она ела овсянку деревянной <w>короткой ложкой</w>."
                            }
                        },
                        {
                            "text": "The child held the <w>cutty</w> tightly in his small hand.",
                            "target_lang_translations": {
                                "ru": "Ребенок крепко сжимал <w>ложку</w> в своей маленькой руке."
                            }
                        }
                    ],
                    "synonyms": [
                        "spoon"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Scottish"
                        },
                        {
                            "trait_type": "dated",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ложка с короткой ручкой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "короткая ложка",
                                "target_lang_sense_clarification": "утварь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "acf7c54c-b363-3323-b66f-e85c3ded2add",
                    "sense_definition": "A derogatory term for a woman considered immoral or unchaste.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "person_noun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The villagers gossiped about the <w>cutty</w> who lived at the edge of town.",
                            "target_lang_translations": {
                                "ru": "Жители деревни сплетничали о <w>распутнице</w>, жившей на окраине города."
                            }
                        },
                        {
                            "text": "He angrily called her an ungrateful <w>cutty</w> for ignoring his advice.",
                            "target_lang_translations": {
                                "ru": "Он сердито назвал её неблагодарной <w>девкой</w> за то, что она проигнорировала его совет."
                            }
                        }
                    ],
                    "synonyms": [
                        "hussy",
                        "wanton"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Scottish"
                        },
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уничижительный термин для женщины, считающейся аморальной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "распутница",
                                "target_lang_sense_clarification": "осуждающе"
                            },
                            {
                                "target_lang_word": "девка",
                                "target_lang_sense_clarification": "грубое, просторечное"
                            }
                        ]
                    }
                }

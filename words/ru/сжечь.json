    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ceddbb52-7928-3db5-a8e8-4467d2adf7e3",
                    "sense_definition": "Полностью уничтожить что-либо с помощью огня, превратив в пепел.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Враги угрожали <сжечь> город дотла, если жители не сдадутся.",
                            "target_lang_translations": {
                                "en": "The enemies threatened to <w>burn</w> the city to the ground if the residents did not surrender."
                            }
                        },
                        {
                            "text": "Он решил <сжечь> все старые письма, чтобы забыть о прошлом.",
                            "target_lang_translations": {
                                "en": "He decided to <w>burn</w> all the old letters to forget about the past."
                            }
                        }
                    ],
                    "synonyms": [
                        "спалить",
                        "испепелить"
                    ],
                    "common_phrases": [
                        "сжечь дотла",
                        "сжечь мосты",
                        "сжечь мусор"
                    ],
                    "target_lang_definitions": {
                        "en": "To completely destroy something using fire, turning it into ash."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "burn",
                                "target_lang_sense_clarification": "to destroy something by fire"
                            },
                            {
                                "target_lang_word": "burn down",
                                "target_lang_sense_clarification": "to destroy a building or structure completely by fire"
                            },
                            {
                                "target_lang_word": "burn up",
                                "target_lang_sense_clarification": "to destroy something completely by fire"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5db946de-7745-334e-8d2f-5feaad0e11a7",
                    "sense_definition": "Испортить еду в процессе приготовления из-за слишком высокой температуры или долгого нахождения на огне.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cooking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я отвлёкся на телефонный звонок и умудрился <сжечь> ужин.",
                            "target_lang_translations": {
                                "en": "I got distracted by a phone call and managed to <w>burn</w> dinner."
                            }
                        },
                        {
                            "text": "Повар боялся <сжечь> пирог, поэтому постоянно проверял духовку.",
                            "target_lang_translations": {
                                "en": "The cook was afraid to <w>burn</w> the pie, so he constantly checked the oven."
                            }
                        }
                    ],
                    "synonyms": [
                        "пережарить",
                        "спалить"
                    ],
                    "common_phrases": [
                        "сжечь мясо",
                        "сжечь пирог",
                        "сжечь кашу"
                    ],
                    "target_lang_definitions": {
                        "en": "To spoil food during cooking due to excessively high temperature or cooking for too long."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "burn",
                                "target_lang_sense_clarification": "to spoil food by overcooking"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "38b136d6-d0ca-3cd6-88e5-6c2266bf1954",
                    "sense_definition": "Сильно повредить кожу, волосы или ткани тела воздействием огня, химических веществ или излучения.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "injury",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Девушка боялась <сжечь> волосы дешёвой краской.",
                            "target_lang_translations": {
                                "en": "The girl was afraid to <w>burn</w> her hair with cheap dye."
                            }
                        },
                        {
                            "text": "На пляже можно легко <сжечь> кожу, если не пользоваться кремом.",
                            "target_lang_translations": {
                                "en": "You can easily <w>burn</w> your skin on the beach if you don't use cream."
                            }
                        }
                    ],
                    "synonyms": [
                        "обжечь"
                    ],
                    "common_phrases": [
                        "сжечь кожу",
                        "сжечь волосы",
                        "сжечь слизистую"
                    ],
                    "target_lang_definitions": {
                        "en": "To severely damage skin, hair, or body tissue through exposure to fire, chemicals, or radiation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "burn",
                                "target_lang_sense_clarification": "to injure skin or tissue with heat or chemicals"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b78b089a-5492-304a-a0a2-fa365c22177e",
                    "sense_definition": "Погубить или иссушить растительность сильным зноем (обычно о солнце).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "nature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Беспощадное летнее солнце успело <сжечь> траву на полях.",
                            "target_lang_translations": {
                                "en": "The merciless summer sun managed to <w>burn</w> the grass in the fields."
                            }
                        },
                        {
                            "text": "Засуха грозила <сжечь> весь будущий урожай фермеров.",
                            "target_lang_translations": {
                                "en": "The drought threatened to <w>burn</w> all the farmers' future crops."
                            }
                        }
                    ],
                    "synonyms": [
                        "выжечь",
                        "иссушить"
                    ],
                    "common_phrases": [
                        "солнце сожгло траву",
                        "сжечь посевы"
                    ],
                    "target_lang_definitions": {
                        "en": "To destroy or dry out vegetation through intense heat, usually referring to the sun."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "burn",
                                "target_lang_sense_clarification": "to destroy vegetation by heat"
                            },
                            {

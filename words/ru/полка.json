    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "43bd0ae5-e6c0-31e3-862b-8079de07baea",
                    "sense_definition": "Прикреплённая к стене или находящаяся в шкафу горизонтальная доска для размещения книг, посуды и других предметов.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "furniture_shelf",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Моя любимая книга стоит на верхней <полке>.",
                            "target_lang_translations": {
                                "en": "My favorite book is on the top <shelf>."
                            }
                        },
                        {
                            "text": "Отец повесил на кухне новую деревянную <полку> для специй.",
                            "target_lang_translations": {
                                "en": "Father hung a new wooden spice <shelf> in the kitchen."
                            }
                        }
                    ],
                    "common_phrases": [
                        "книжная полка",
                        "положить на полку",
                        "взять с полки"
                    ],
                    "target_lang_definitions": {
                        "en": "A flat horizontal board attached to a wall or inside a cupboard for storing items."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shelf",
                                "target_lang_sense_clarification": "furniture"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7ddf6acf-714a-3272-a19e-1d4b879c7405",
                    "sense_definition": "Место для лежания в железнодорожном вагоне.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "train_berth",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я всегда покупаю билет на нижнюю <полку>, потому что там удобнее спать.",
                            "target_lang_translations": {
                                "en": "I always buy a ticket for the lower <berth> because it is more comfortable to sleep there."
                            }
                        },
                        {
                            "text": "Пассажир застелил свою <полку> чистым бельём и лёг отдыхать.",
                            "target_lang_translations": {
                                "en": "The passenger made up his <berth> with clean linens and lay down to rest."
                            }
                        }
                    ],
                    "synonyms": [
                        "койка"
                    ],
                    "common_phrases": [
                        "верхняя полка",
                        "нижняя полка",
                        "боковая полка"
                    ],
                    "target_lang_definitions": {
                        "en": "A sleeping bunk in a train compartment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "berth",
                                "target_lang_sense_clarification": "train bed"
                            },
                            {
                                "target_lang_word": "bunk",
                                "target_lang_sense_clarification": "sleeping place"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dc733652-9fdb-3bf5-9220-8f9e8dec6de1",
                    "sense_definition": "Форма родительного падежа единственного числа существительного <w>полк</w> (воинская часть).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "form_regiment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Командир <полка> отдал приказ о начале наступления.",
                            "target_lang_translations": {
                                "en": "The commander of the <regiment> gave the order to start the offensive."
                            }
                        },
                        {
                            "text": "В музее хранится старинное знамя нашего <полка>.",
                            "target_lang_translations": {
                                "en": "The museum keeps the ancient banner of our <regiment>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "командир полка",
                        "знамя полка",
                        "сын полка"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "stress is on the last syllable: полка́"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Genitive singular form of the word for a military regiment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "regiment",
                                "target_lang_sense_clarification": "genitive case form"
                            }
                        ]
                    }

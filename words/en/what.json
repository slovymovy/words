{
    "word_family": [
        "whatever",
        "whatsoever",
        "somewhat",
        "whatnot"
    ],
    "entries": [
        {
            "pos": "pronoun",
            "senses": [
                {
                    "sense_id": "55079fe2-e9fd-34e3-aab8-25450fd509cc",
                    "sense_definition": "Used to ask questions about the identity, nature, or value of an object, action, or situation.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "interrogative_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>What</w> is your name?",
                            "target_lang_translations": {
                                "ru": "<w>Как</w> тебя зовут?"
                            }
                        },
                        {
                            "text": "I wonder <w>what</w> happened to him.",
                            "target_lang_translations": {
                                "ru": "Интересно, <w>что</w> с ним случилось."
                            }
                        }
                    ],
                    "common_phrases": [
                        "what if",
                        "what about",
                        "what for"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для задания вопросов о предмете, действии или ситуации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "вопросительное местоимение"
                            },
                            {
                                "target_lang_word": "какой",
                                "target_lang_sense_clarification": "в значении 'какое у тебя имя', 'какой твой номер'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "230a94ef-647a-3a54-b3af-37515a9e6dfd",
                    "sense_definition": "The thing or things that; that which.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "relative_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will do <w>what</w> I can to help you.",
                            "target_lang_translations": {
                                "ru": "Я сделаю <w>то, что</w> смогу, чтобы помочь тебе."
                            }
                        },
                        {
                            "text": "He knows <w>what</w> he wants.",
                            "target_lang_translations": {
                                "ru": "Он знает, <w>что</w> он хочет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "come what may",
                        "guess what",
                        "you know what"
                    ],
                    "target_lang_definitions": {
                        "ru": "То, что; вещь или вещи, которые."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "то, что",
                                "target_lang_sense_clarification": "относительное местоимение"
                            },
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "разговорное сокращение от 'то, что'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d251b600-4cb4-337f-aace-c6ac6bd6a482",
                    "sense_definition": "Used as a relative pronoun referring to people or things, equivalent to 'who' or 'that'.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is the man <w>what</w> fixed my car.",
                            "target_lang_translations": {
                                "ru": "Это тот мужчина, <w>который</w> починил мою машину."
                            }
                        },
                        {
                            "text": "It is the only thing <w>what</w> makes sense.",
                            "target_lang_translations": {
                                "ru": "Это единственная вещь, <w>которая</w> имеет смысл."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dialectal",
                            "comment": "non-standard"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется как относительное местоимение, эквивалент 'который' (в просторечии или диалектах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "который",
                                "target_lang_sense_clarification": "стандартный литературный эквивалент"
                            },
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "просторечное использование"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "determiner",
            "senses": [
                {
                    "sense_id": "69836b28-5946-3248-bc41-4f33f2ceafff",
                    "sense_definition": "Used before a noun to ask which specific person, thing, or type is meant.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "interrogative_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>What</w> time is it?",
                            "target_lang_translations": {
                                "ru": "<w>Который</w> час?"
                            }
                        },
                        {
                            "text": "<w>What</w> colour do you prefer?",
                            "target_lang_translations": {
                                "ru": "<w>Какой</w> цвет вы предпочитаете?"
                            }
                        }
                    ],
                    "common_phrases": [
                        "what kind of",
                        "what time"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется перед существительным, чтобы спросить о конкретном предмете, типе или свойстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "какой",
                                "target_lang_sense_clarification": "вопросительное прилагательное"
                            },
                            {
                                "target_lang_word": "который",
                                "target_lang_sense_clarification": "при вопросе о времени или порядке"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04cd970f-59f9-37b3-9174-51e1a433757a",
                    "sense_definition": "Used to express strong feelings, surprise, or to emphasize a remarkable quality.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "exclamative_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>What</w> a beautiful day it is!",
                            "target_lang_translations": {
                                "ru": "<w>Какой</w> чудесный день!"
                            }
                        },
                        {
                            "text": "<w>What</w> nonsense he is talking.",
                            "target_lang_translations": {
                                "ru": "<w>Что за</w> чушь он несёт."
                            }
                        }
                    ],
                    "common_phrases": [
                        "what a pity",
                        "what a shame"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для выражения сильных чувств, удивления или подчеркивания качества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "какой",
                                "target_lang_sense_clarification": "в восклицаниях"
                            },
                            {
                                "target_lang_word": "что за",
                                "target_lang_sense_clarification": "эмоциональное усиление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5b461392-10e7-36f8-a7f6-7e4d7fd2bf4d",
                    "sense_definition": "Whatever; any ... that.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "relative_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I will give you <w>what</w> help I can.",
                            "target_lang_translations": {
                                "ru": "Я окажу тебе <w>любую</w> помощь, какую смогу."
                            }
                        },
                        {
                            "text": "He spent <w>what</w> little money he had left.",
                            "target_lang_translations": {
                                "ru": "Он потратил <w>те</w> немногие деньги, которые у него оставались."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Любой... который; тот (немногий)... который."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "любой",
                                "target_lang_sense_clarification": "всякий, какой есть"
                            },
                            {
                                "target_lang_word": "тот... который",
                                "target_lang_sense_clarification": "соотносительная конструкция"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "70a7f310-cd65-3878-bd4d-378f4c70cd88",
                    "sense_definition": "Used to ask someone to repeat something that was not heard or understood.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>What</w>? I didn't hear you.",
                            "target_lang_translations": {
                                "ru": "<w>Что</w>? Я не расслышал."
                            }
                        },
                        {
                            "text": "You did <w>what</w>?",
                            "target_lang_translations": {
                                "ru": "Ты сделал <w>что</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "pardon",
                        "come again",
                        "say again"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется, чтобы переспросить, если что-то не было услышано или понято."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "вопрос при переспросе"
                            },
                            {
                                "target_lang_word": "а",
                                "target_lang_sense_clarification": "разговорный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "810d169e-75cd-3392-9fdc-1d4dd947ab96",
                    "sense_definition": "An expression of surprise, disbelief, or anger.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "exclamative_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>What</w>! That is impossible.",
                            "target_lang_translations": {
                                "ru": "<w>Что</w>! Это невозможно."
                            }
                        },
                        {
                            "text": "<w>What</w>? You can't be serious!",
                            "target_lang_translations": {
                                "ru": "<w>Что</w>? Ты не можешь говорить серьёзно!"
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражение удивления, недоверия или гнева."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "восклицание"
                            },
                            {
                                "target_lang_word": "как",
                                "target_lang_sense_clarification": "выражение возмущения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf915d0f-d484-3c57-9c35-12f3c7cafa75",
                    "sense_definition": "Used to indicate an approximation or a pause to recall information.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It was, <w>what</w>, five years ago?",
                            "target_lang_translations": {
                                "ru": "Это было, <w>ну</w>, пять лет назад?"
                            }
                        },
                        {
                            "text": "We have, <w>what</w>, three days to finish this?",
                            "target_lang_translations": {
                                "ru": "У нас есть, <w>скажем</w>, три дня, чтобы закончить это?"
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется для указания на приблизительность или паузу для припоминания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ну",
                                "target_lang_sense_clarification": "частица-заполнитель паузы"
                            },
                            {
                                "target_lang_word": "скажем",
                                "target_lang_sense_clarification": "вводное слово"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "042ceff7-0550-3d95-97e9-e0f719f5b5e1",
                    "sense_definition": "Used as a tag question to emphasize a statement and invite agreement.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is a lovely day, <w>what</w>?",
                            "target_lang_translations": {
                                "ru": "Чудесный день, <w>не так ли</w>?"
                            }
                        },
                        {
                            "text": "Good work, <w>what</w>?",
                            "target_lang_translations": {
                                "ru": "Хорошая работа, <w>да</w>?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "British upper-class stereotype"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в конце предложения для усиления утверждения и приглашения к согласию (устаревшее)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "не так ли",
                                "target_lang_sense_clarification": "вопрос-тег"
                            },
                            {
                                "target_lang_word": "да",
                                "target_lang_sense_clarification": "запрос подтверждения"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "e15ce964-b445-3a44-90b1-5894976e8a09",
                    "sense_definition": "To what extent or in what way; often used in rhetorical questions.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>What</w> does it matter?",
                            "target_lang_translations": {
                                "ru": "<w>Какое</w> это имеет значение?"
                            }
                        },
                        {
                            "text": "<w>What</w> do you care?",
                            "target_lang_translations": {
                                "ru": "<w>Какое</w> тебе дело?"
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "В какой степени или каким образом; часто в риторических вопросах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "в значении 'зачем' или 'почему'"
                            },
                            {
                                "target_lang_word": "какое",
                                "target_lang_sense_clarification": "в фразах типа 'какое это имеет значение'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "939cc633-8fb1-350b-bc14-5a25b5314f79",
                    "sense_definition": "Used in the phrase 'what with' to introduce a contributing cause or factor.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>What</w> with the rain and the wind, we stayed inside.",
                            "target_lang_translations": {
                                "ru": "<w>Из-за</w> дождя и ветра мы остались дома."
                            }
                        },
                        {
                            "text": "She was tired, <w>what</w> with working two jobs.",
                            "target_lang_translations": {
                                "ru": "Она устала, <w>так как</w> работала на двух работах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "what with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в фразе 'what with' для введения причины или фактора."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "из-за",
                                "target_lang_sense_clarification": "предлог причины"
                            },
                            {
                                "target_lang_word": "ввиду",
                                "target_lang_sense_clarification": "официальный синоним"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8a6fb304-5fc6-3735-a4a1-cf3da6bf410c",
                    "sense_definition": "The true nature, identity, or specific details of something.",
                    "learner_level": "C1",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We need to understand the <w>whats</w> and hows of the process.",
                            "target_lang_translations": {
                                "ru": "Нам нужно понять <w>суть</w> и способы этого процесса."
                            }
                        },
                        {
                            "text": "The <w>what</w> is less important than the why.",
                            "target_lang_translations": {
                                "ru": "<w>Суть</w> менее важна, чем причина."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the whats and hows"
                    ],
                    "target_lang_definitions": {
                        "ru": "Истинная природа, сущность или конкретные детали чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "суть",
                                "target_lang_sense_clarification": "сущность"
                            },
                            {
                                "target_lang_word": "что",
                                "target_lang_sense_clarification": "субстантивированное местоимение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                },
                {
                    "sense_id": "22f98853-14cf-3e46-9f95-98c6cceac610",
                    "sense_definition": "A state of being shy, withdrawn, or avoiding social interaction.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "psychological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It took a long time for him to come out of his <w>shell</w> and make friends.",
                            "target_lang_translations": {
                                "ru": "Ему потребовалось много времени, чтобы выбраться из своей <w>скорлупы</w> и завести друзей."
                            }
                        },
                        {
                            "text": "She retreated into her <w>shell</w> after being criticized by the teacher.",
                            "target_lang_translations": {
                                "ru": "Она замкнулась в своей <w>скорлупе</w> после критики учителя."
                            }
                        }
                    ],
                    "common_phrases": [
                        "come out of one's shell",
                        "retreat into one's shell"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in idioms describing personality"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние застенчивости, замкнутости или избегания социального взаимодействия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "скорлупа",
                                "target_lang_sense_clarification": "перен. замкнутость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb830636-8fe0-361e-a618-88f2b0314398",

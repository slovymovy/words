            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "4f761ddb-ed2b-302f-9a1e-7673d0fedec9",
                    "sense_definition": "Самостоятельно, без чужого влияния, помощи или контроля.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Эта компьютерная программа работает <w>независимо</w> и не требует подключения к интернету.",
                            "target_lang_translations": {
                                "en": "This computer program works <w>independently</w> and does not require an internet connection."
                            }
                        },
                        {
                            "text": "Он всегда старался держаться <w>независимо</w> и не примыкал ни к каким группам.",
                            "target_lang_translations": {
                                "en": "He always tried to carry himself <w>independently</w> and did not join any groups."
                            }
                        }
                    ],
                    "synonyms": [
                        "самостоятельно",
                        "автономно",
                        "свободно",
                        "обособленно"
                    ],
                    "antonyms": [
                        "зависимо"
                    ],
                    "common_phrases": [
                        "жить независимо",
                        "действовать независимо",
                        "мыслить независимо",
                        "существовать независимо"
                    ],
                    "target_lang_definitions": {
                        "en": "Independently, without outside influence, help, or control."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "independently",
                                "target_lang_sense_clarification": "general adverbial use"
                            },
                            {
                                "target_lang_word": "autonomously",
                                "target_lang_sense_clarification": "functioning without external control"
                            },
                            {
                                "target_lang_word": "on one's own",
                                "target_lang_sense_clarification": "without help"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "821041a7-2577-3f04-ad89-a36377ef9b71",
                    "sense_definition": "Не принимая во внимание, не считаясь с кем-либо или чем-либо (обычно используется с предлогом «от»).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы обязательно пойдем гулять в парк, <w>независимо</w> от погоды.",
                            "target_lang_translations": {
                                "en": "We will definitely go for a walk in the park, <w>regardless</w> of the weather."
                            }
                        },
                        {
                            "text": "Закон должен быть одинаковым для всех, <w>независимо</w> от социального статуса.",
                            "target_lang_translations": {
                                "en": "The law must be the same for everyone, <w>regardless</w> of social status."
                            }
                        }
                    ],
                    "synonyms": [
                        "безотносительно"
                    ],
                    "common_phrases": [
                        "независимо от того",
                        "независимо от обстоятельств",
                        "независимо от желания",
                        "независимо от возраста"
                    ],
                    "target_lang_definitions": {
                        "en": "Without taking into account, disregarding someone or something (usually used with the preposition 'of')."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "regardless",
                                "target_lang_sense_clarification": "followed by 'of'"
                            },
                            {
                                "target_lang_word": "irrespective",
                                "target_lang_sense_clarification": "formal, followed by 'of'"
                            },
                            {
                                "target_lang_word": "independently",
                                "target_lang_sense_clarification": "literal translation, followed by 'of'"
                            }
                        ]
                    }
                }
            ]
        },
        {

        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "5a5b9180-98f0-39a0-88d7-61098b028392",
                    "sense_definition": "In perfect condition, appearing brand new; often used to describe collectibles.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I found a first edition book in <w>mint</w> condition at the garage sale.",

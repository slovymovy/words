            "senses": [
                {
                    "sense_id": "a7b1a427-3c25-3f26-b5aa-37167193e606",
                    "sense_definition": "W mitologii greckiej: bóg piękna, światła, życia, muzyki, wróżb, prawa i porządku; syn Zeusa i Leto, bliźniaczy brat Artemidy.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "mythology",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "W muzeum podziwialiśmy antyczny, marmurowy posąg <w>Apolla</w>."
                        },
                        {
                            "text": "Starożytni Grecy wierzyli, że <w>Apollo</w> zsyła na ludzi natchnienie poetyckie."
                        }
                    ],
                    "synonyms": [
                        "Apollon",
                        "Febus",
                        "Fojbos",
                        "Apollin"
                    ],
                    "common_phrases": [
                        "świątynia Apolla",
                        "strzała Apolla",
                        "wieniec laurowy"
                    ]
                },
                {
                    "sense_id": "04bfeb54-0687-3771-ae61-80d338989367",
                    "sense_definition": "Imię męskie pochodzenia greckiego, nawiązujące do mitologicznego boga.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "names",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mój wujek <w>Apollo</w> jest bardzo dumny ze swojego nietypowego imienia."
                        },
                        {
                            "text": "W kalendarzu imieniny <w>Apolla</w> przypadają kilka razy w roku."
                        }
                    ]
                },
                {
                    "sense_id": "9cb34872-3368-3abb-ba36-ea550f15f3e4",
                    "sense_definition": "Planetoida z grupy Apolla, której orbita przecina orbitę Ziemi.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "astronomy",
                    "name_type": "astronomical_name",
                    "examples": [
                        {
                            "text": "Astronomowie dokładnie śledzą orbitę <w>Apolla</w>, aby wykluczyć ryzyko kolizji."
                        },
                        {
                            "text": "Planetoida <w>Apollo</w> została odkryta w pierwszej połowie dwudziestego wieku."
                        }
                    ],
                    "common_phrases": [
                        "grupa Apolla"
                    ]
                }
            ]
        },

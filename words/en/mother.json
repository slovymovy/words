    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ac18dd78-6ef2-301f-9b25-8b316433cedc",
                    "sense_definition": "A female parent of a human or an animal.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am going to visit my <w>mother</w> this weekend.",
                            "target_lang_translations": {
                                "ru": "Я собираюсь навестить свою <w>маму</w> в эти выходные."
                            }
                        },
                        {
                            "text": "The lioness was a protective <w>mother</w> to her four cubs.",
                            "target_lang_translations": {
                                "ru": "Львица была заботливой <w>матерью</w> для своих четырех львят."
                            }
                        }
                    ],
                    "synonyms": [
                        "mom",
                        "mum",
                        "mommy",
                        "mama",
                        "parent"
                    ],
                    "antonyms": [
                        "father"
                    ],
                    "common_phrases": [
                        "single mother",
                        "biological mother",
                        "expectant mother",
                        "working mother"
                    ],
                    "target_lang_definitions": {
                        "ru": "Женщина-родитель человека или животного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мать",
                                "target_lang_sense_clarification": "биологическая или официальная"
                            },
                            {
                                "target_lang_word": "мама",
                                "target_lang_sense_clarification": "разговорное, семейное"
                            },
                            {
                                "target_lang_word": "матушка",
                                "target_lang_sense_clarification": "устаревшее или ласковое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0adf93cc-9131-3d7f-8b16-5771c24e30b3",
                    "sense_definition": "A title given to the personification of a force of nature or an abstract concept.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We must do everything we can to protect <w>Mother</w> Earth from pollution.",
                            "target_lang_translations": {
                                "ru": "Мы должны сделать все возможное, чтобы защитить <w>Мать</w>-Землю от загрязнения."
                            }
                        },
                        {
                            "text": "It seems that <w>Mother</w> Nature has decided to send us a storm today.",
                            "target_lang_translations": {
                                "ru": "Кажется, <w>Матушка</w>-Природа решила послать нам сегодня бурю."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Mother Nature",
                        "Mother Earth",
                        "Mother Russia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Титул, даваемый персонификации сил природы или абстрактного понятия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мать",
                                "target_lang_sense_clarification": "в сочетаниях: Мать-Природа, Родина-мать"
                            },
                            {
                                "target_lang_word": "матушка",
                                "target_lang_sense_clarification": "фольклорное: Матушка-Земля"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04d80937-ef79-373d-bae9-da2118ce3984",
                    "sense_definition": "A source or origin from which something derives.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Necessity is often called the <w>mother</w> of invention.",
                            "target_lang_translations": {
                                "ru": "Необходимость часто называют <w>матерью</w> изобретательности."
                            }
                        },
                        {
                            "text": "The Mediterranean region was the <w>mother</w> of many ancient cultures.",
                            "target_lang_translations": {
                                "ru": "Средиземноморский регион был <w>прародительницей</w> многих древних культур."
                            }
                        }
                    ],
                    "synonyms": [
                        "source",
                        "origin"
                    ],
                    "common_phrases": [
                        "mother of invention",
                        "mother of parliaments"
                    ],
                    "target_lang_definitions": {
                        "ru": "Источник или происхождение, от которого что-то происходит."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мать",
                                "target_lang_sense_clarification": "метафорическое: мать учения"
                            },
                            {
                                "target_lang_word": "прародительница",
                                "target_lang_sense_clarification": "источник происхождения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "caff70d0-04f8-3085-97ea-9bca41bd66af",
                    "sense_definition": "The female superior or head of a religious house, such as an abbess.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The nuns gathered in the chapel to listen to <w>Mother</w> Superior speak.",
                            "target_lang_translations": {
                                "ru": "Монахини собрались в часовне, чтобы послушать <w>мать</w>-настоятельницу."
                            }
                        },
                        {
                            "text": "<w>Mother</w> Teresa was world-famous for her charitable work in India.",
                            "target_lang_translations": {
                                "ru": "<w>Мать</w> Тереза была всемирно известна своей благотворительной деятельностью в Индии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Mother Superior"
                    ],
                    "target_lang_definitions": {
                        "ru": "Настоятельница женского монастыря или обращение к монахине."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мать",
                                "target_lang_sense_clarification": "титул: Мать Тереза"
                            },
                            {
                                "target_lang_word": "настоятельница",
                                "target_lang_sense_clarification": "должность"
                            },
                            {
                                "target_lang_word": "матушка",
                                "target_lang_sense_clarification": "обращение к монахине"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0bc53139-34b2-3294-a10d-58c1fe5f64c7",
                    "sense_definition": "A shortened form of a vulgar term used to express anger, surprise, or emphasis.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "4",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He was so angry that he wanted to burn the <w>mother</w> down.",
                            "target_lang_translations": {
                                "ru": "Он был так зол, что хотел сжечь эту <w>заразу</w> дотла."
                            }
                        },
                        {
                            "text": "Start the engine and fire that <w>mother</w> up right now.",
                            "target_lang_translations": {
                                "ru": "Заводи двигатель и запускай эту <w>штуковину</w> прямо сейчас."
                            }
                        }
                    ],
                    "synonyms": [
                        "motherfucker"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "vulgar"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращение от вульгарного выражения, используемое для выражения гнева, удивления или для усиления."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зараза",
                                "target_lang_sense_clarification": "ругательство по отношению к предмету"
                            },
                            {
                                "target_lang_word": "тварь",
                                "target_lang_sense_clarification": "грубое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4a00a360-7138-3118-b2b3-d6f595bd946e",
                    "sense_definition": "A stringy substance that forms in fermenting alcohol and turns it into vinegar.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "5",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can make your own vinegar by adding <w>mother</w> to the wine.",
                            "target_lang_translations": {
                                "ru": "Вы можете сделать свой собственный уксус, добавив <w>уксусную матку</w> в вино."
                            }
                        },
                        {
                            "text": "The cider developed a thick layer of <w>mother</w> during the fermentation process.",
                            "target_lang_translations": {
                                "ru": "В процессе брожения на сидре образовался толстый слой <w>уксусной матки</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "vinegar mother"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Волокнистая субстанция, образующаяся при брожении спирта и превращающая его в уксус."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "уксусная матка",
                                "target_lang_sense_clarification": "технический термин"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "9c1b3f43-94bd-39ef-8c22-933f232dd111",
                    "sense_definition": "To treat someone with great care and affection, often excessively, like a mother.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She tends to <w>mother</w> her younger brother even though he is an adult.",
                            "target_lang_translations": {
                                "ru": "Она склонна <w>опекать</w> своего младшего брата, хотя он уже взрослый."
                            }
                        },
                        {
                            "text": "He wanted independence and did not like it when people tried to <w>mother</w> him.",
                            "target_lang_translations": {
                                "ru": "Он хотел независимости, и ему не нравилось, когда его пытались <w>нянчить</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "nurture",
                        "pamper",
                        "care for"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относиться к кому-либо с большой заботой и любовью, часто чрезмерной."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "опекать",
                                "target_lang_sense_clarification": "заботиться"
                            },
                            {
                                "target_lang_word": "нянчить",
                                "target_lang_sense_clarification": "обращаться как с ребенком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5d856b93-3fa6-3d97-9084-9ced9bc995b9",
                    "sense_definition": "To give birth to or be the female parent of a child.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She had <w>mothered</w> three healthy children by the time she was thirty.",
                            "target_lang_translations": {
                                "ru": "К тридцати годам она <w>родила</w> троих здоровых детей."
                            }
                        },
                        {
                            "text": "The stray cat <w>mothered</w> the abandoned kittens as if they were her own.",
                            "target_lang_translations": {
                                "ru": "Бродячая кошка <w>стала матерью</w> для брошенных котят, как будто они были ее собственными."
                            }
                        }
                    ],
                    "synonyms": [
                        "give birth to"
                    ],
                    "antonyms": [
                        "father"
                    ],
                    "target_lang_definitions": {
                        "ru": "Родить или быть матерью ребенка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "родить",
                                "target_lang_sense_clarification": "дать жизнь"
                            },
                            {
                                "target_lang_word": "быть матерью",
                                "target_lang_sense_clarification": "выполнять роль матери"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "5aff2665-cfc0-32fc-abde-1b3c5500e331",
                    "sense_definition": "One's own mother, used as a proper name or form of address.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "title_or_honorific_name",
                    "examples": [
                        {
                            "text": "I asked <w>Mother</w> if we could go to the park today.",
                            "target_lang_translations": {
                                "ru": "Я спросил <w>маму</w>, можем ли мы пойти сегодня в парк."
                            }
                        },
                        {
                            "text": "Please tell <w>Mother</w> that I will be home late for dinner.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, скажи <w>маме</w>, что я приду к ужину поздно."
                            }
                        }
                    ],
                    "synonyms": [
                        "Mom",
                        "Mum",
                        "Mummy"
                    ],
                    "antonyms": [
                        "Father"
                    ],
                    "target_lang_definitions": {
                        "ru": "Собственная мать (используется как имя собственное или обращение)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мама",
                                "target_lang_sense_clarification": "общеупотребительное"
                            },
                            {
                                "target_lang_word": "мать",
                                "target_lang_sense_clarification": "более строгое"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
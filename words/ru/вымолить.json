    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "582d69ee-0ad5-3f53-aa80-c2ba8b3c75cb",
                    "sense_definition": "Добиться получения чего-либо очень настойчивыми, слёзными просьбами или мольбами.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "request",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Виновник происшествия пытался <вымолить> себе прощение у пострадавшей стороны.",
                            "target_lang_translations": {
                                "en": "The culprit of the accident tried to <beg for> forgiveness from the injured party."
                            }
                        },
                        {
                            "text": "Пленник надеялся <вымолить> жизнь, обещая рассказать всё, что ему известно.",
                            "target_lang_translations": {
                                "en": "The captive hoped to <plead for> his life by promising to tell everything he knew."
                            }
                        }
                    ],
                    "synonyms": [
                        "выпросить"
                    ],
                    "common_phrases": [
                        "вымолить прощение",
                        "вымолить пощаду",
                        "вымолить жизнь",
                        "вымолить разрешение"
                    ],
                    "target_lang_definitions": {
                        "en": "To obtain something through persistent, tearful requests or pleas."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "beg for",
                                "target_lang_sense_clarification": "to obtain by asking earnestly"
                            },
                            {
                                "target_lang_word": "plead for",
                                "target_lang_sense_clarification": "to obtain by making an emotional appeal"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eccf17f8-d45e-3abc-904a-23df21494c30",
                    "sense_definition": "Получить желаемое, обращаясь с усердной молитвой к Богу.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "request",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Монахи верили, что могут <вымолить> спасение для души любого грешника.",
                            "target_lang_translations": {
                                "en": "The monks believed they could <obtain through prayer> salvation for the soul of any sinner."
                            }
                        },
                        {
                            "text": "Женщина каждый день ходила в церковь, чтобы <вымолить> здоровье для своего больного ребёнка.",
                            "target_lang_translations": {
                                "en": "The woman went to church every day to <pray for> health for her sick child."
                            }
                        }
                    ],
                    "common_phrases": [
                        "вымолить душу",
                        "вымолить грешника",
                        "вымолить исцеление"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "указано в исходных данных как устаревшее значение"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To obtain what is desired by addressing God with earnest prayer."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "obtain through prayer",
                                "target_lang_sense_clarification": "to succeed in getting something by praying"
                            },
                            {
                                "target_lang_word": "pray for",
                                "target_lang_sense_clarification": "to ask God for something"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                            {
                                "target_lang_word": "у",
                                "target_lang_sense_clarification": "при нахождении рядом с опорой"
                            },
                            {
                                "target_lang_word": "к",
                                "target_lang_sense_clarification": "прислонившись к чему-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8c0c52a6-277c-3a48-9622-e9eff8ed1a24",
                    "sense_definition": "Gericht tot een persoon, gebruikt om aan te geven tegen wie men spreekt of hoe men zich gedraagt.",

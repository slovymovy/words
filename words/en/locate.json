    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d209d1c0-5896-3bf3-b557-11104739c172",
                    "sense_definition": "To put or establish something in a particular place.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "establish_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new shopping mall is <w>located</w> on the edge of town.",
                            "target_lang_translations": {
                                "ru": "Новый торговый центр <w>расположен</w> на окраине города."
                            }
                        },
                        {
                            "text": "They plan to <w>locate</w> the headquarters in a larger city.",
                            "target_lang_translations": {
                                "ru": "Они планируют <w>разместить</w> штаб-квартиру в более крупном городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "site",
                        "site",
                        "site",
                        "site"
                    ],
                    "common_phrases": [
                        "be located in",
                        "centrally located",
                        "conveniently located"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "often passive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Разместить или установить что-либо в определенном месте"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расположить",
                                "target_lang_sense_clarification": "поместить в определенном месте (часто о зданиях)"
                            },
                            {
                                "target_lang_word": "разместить",
                                "target_lang_sense_clarification": "предоставить место, устроить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e7abaacf-655f-3367-84f5-fe7b6eeff339",
                    "sense_definition": "To find the exact position of someone or something.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "find_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I cannot <w>locate</w> my keys anywhere in the house.",
                            "target_lang_translations": {
                                "ru": "Я нигде в доме не могу <w>найти</w> свои ключи."
                            }
                        },
                        {
                            "text": "The police were able to <w>locate</w> the suspect using his phone signal.",
                            "target_lang_translations": {
                                "ru": "Полиции удалось <w>обнаружить</w> подозреваемого по сигналу его телефона."
                            }
                        }
                    ],
                    "synonyms": [
                        "site"
                    ],
                    "common_phrases": [
                        "locate the source",
                        "unable to locate",
                        "trying to locate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Найти точное местоположение кого-либо или чего-либо"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обнаружить",
                                "target_lang_sense_clarification": "раскрыть местонахождение, разыскать"
                            },
                            {
                                "target_lang_word": "найти",
                                "target_lang_sense_clarification": "отыскать потерянное или скрытое"
                            },
                            {
                                "target_lang_word": "определить местонахождение",
                                "target_lang_sense_clarification": "формальное; установить координаты или место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "60a51111-5069-3b5a-9c30-9b5fb985ea7a",
                    "sense_definition": "To set up a home or business in a specific place.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "establish_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Many tech companies choose to <w>locate</w> in Silicon Valley.",
                            "target_lang_translations": {
                                "ru": "Многие технологические компании предпочитают <w>размещаться</w> в Кремниевой долине."
                            }
                        },
                        {
                            "text": "After retirement, they decided to <w>locate</w> to the coast.",
                            "target_lang_translations": {
                                "ru": "После выхода на пенсию они решили <w>обосноваться</w> на побережье."
                            }
                        }
                    ],
                    "common_phrases": [
                        "locate to",
                        "decide to locate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обосноваться или открыть бизнес в определенном месте"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обосноваться",
                                "target_lang_sense_clarification": "устроиться на жительство или завести дело"
                            },
                            {
                                "target_lang_word": "разместиться",
                                "target_lang_sense_clarification": "занять место (о компании или организации)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "958ddb45-96c5-37a9-89f7-56abedda7164",
                    "sense_definition": "To determine or specify the boundaries or limits of a place.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "find_position",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The surveyor was hired to <w>locate</w> the property lines.",
                            "target_lang_translations": {
                                "ru": "Землемера наняли, чтобы <w>определить</w> границы участка."
                            }
                        },
                        {
                            "text": "They need to <w>locate</w> the claim before mining can begin.",
                            "target_lang_translations": {
                                "ru": "Им нужно <w>определить местоположение</w> участка заявки до начала добычи."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Определить или установить границы участка или места"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "определить",
                                "target_lang_sense_clarification": "установить точные границы или положение"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
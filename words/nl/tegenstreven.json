{
    "word_family": [
        "tegenstrever",
        "tegenstreefster"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "dca49309-ad42-361b-b1de-fc0c725f4d76",
                    "sense_definition": "Zich verzetten tegen iemand of iets; proberen te voorkomen dat een plan of handeling slaagt.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oppositiepartijen <w>streefden</w> het nieuwe beleid van de regering fel <w>tegen</w>.",
                            "target_lang_translations": {
                                "en": "The opposition parties fiercely <w>opposed</w> the government's new policy.",
                                "ru": "Оппозиционные партии яростно <w>противодействовали</w> новой политике правительства."
                            }
                        },
                        {
                            "text": "Het heeft weinig zin om de onvermijdelijke vooruitgang te <w>tegenstreven</w>.",
                            "target_lang_translations": {
                                "en": "It makes little sense to <w>resist</w> inevitable progress.",
                                "ru": "Нет особого смысла <w>противиться</w> неизбежному прогрессу."
                            }
                        }
                    ],
                    "synonyms": [
                        "tegenwerken",
                        "dwarsbomen",
                        "bestrijden",
                        "verhinderen",
                        "dwarszitten"
                    ],
                    "antonyms": [
                        "steunen",
                        "helpen",
                        "meewerken"
                    ],
                    "common_phrases": [
                        "plannen tegenstreven",
                        "elkaar tegenstreven"
                    ],
                    "target_lang_definitions": {
                        "en": "To actively oppose or resist someone or something; to try to prevent a plan or action from succeeding.",
                        "ru": "Оказывать сопротивление кому-либо или чему-либо; пытаться помешать осуществлению плана или действия."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "oppose",
                                "target_lang_sense_clarification": "to actively disagree with or fight against a plan or person"
                            },
                            {
                                "target_lang_word": "resist",
                                "target_lang_sense_clarification": "to withstand the action or effect of something"
                            },
                            {
                                "target_lang_word": "thwart",
                                "target_lang_sense_clarification": "to prevent someone from accomplishing something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "противодействовать",
                                "target_lang_sense_clarification": "активно мешать, оказывать противодействие"
                            },
                            {
                                "target_lang_word": "противиться",
                                "target_lang_sense_clarification": "выражать несогласие, сопротивляться"
                            },
                            {
                                "target_lang_word": "препятствовать",
                                "target_lang_sense_clarification": "создавать преграды, мешать"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ad78b470-2e4b-3f08-ac52-48f727d081c0",
                    "sense_definition": "De handeling van het weerstand bieden of het actief dwarsbomen van iemand.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ondanks zijn felle <w>tegenstreven</w> werd het controversiële besluit toch aangenomen.",
                            "target_lang_translations": {
                                "en": "Despite his fierce <w>opposition</w>, the controversial decision was adopted anyway.",
                                "ru": "Несмотря на его яростное <w>противодействие</w>, спорное решение всё же было принято."
                            }
                        },
                        {
                            "text": "Het project liep vertraging op door het voortdurende <w>tegenstreven</w> van de buurtbewoners.",
                            "target_lang_translations": {
                                "en": "The project was delayed due to the constant <w>resistance</w> from the local residents.",
                                "ru": "Реализация проекта затянулась из-за постоянного <w>противодействия</w> со стороны местных жителей."
                            }
                        }
                    ],
                    "synonyms": [
                        "tegenwerking",
                        "verzet",
                        "oppositie"
                    ],
                    "antonyms": [
                        "steun",
                        "medewerking"
                    ],
                    "common_phrases": [
                        "fel tegenstreven",
                        "ondanks het tegenstreven"
                    ],
                    "target_lang_definitions": {
                        "en": "The act of offering resistance or actively opposing someone.",
                        "ru": "Действие по оказанию сопротивления или активному созданию помех кому-либо."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "opposition",
                                "target_lang_sense_clarification": "hostile or contrary action or stance"
                            },
                            {
                                "target_lang_word": "resistance",
                                "target_lang_sense_clarification": "the refusal to accept or comply with something"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "противодействие",
                                "target_lang_sense_clarification": "активное сопротивление действиям или планам"
                            },
                            {
                                "target_lang_word": "сопротивление",
                                "target_lang_sense_clarification": "отказ подчиниться или согласиться"
                            }
                        ]
                    }
                }
            ]
        }
    ]

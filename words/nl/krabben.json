{
    "word_family": [
        "krab",
        "krabber",
        "krabbel",
        "krabbelen"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "786f2468-6256-3208-b40f-874c2a5066ab",
                    "sense_definition": "Met de nagels of een scherp voorwerp over de huid of een oppervlak wrijven, vaak om jeuk te verlichten.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "scratch_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Als je muggenbulten hebt, moet je proberen niet te <w>krabben</w>.",
                            "target_lang_translations": {
                                "en": "If you have mosquito bites, you should try not to <w>scratch</w>.",
                                "ru": "Если у тебя комариные укусы, нужно стараться их не <w>чесать</w>."
                            }
                        },
                        {
                            "text": "De kat <w>krabt</w> elke dag aan de poot van de tafel.",
                            "target_lang_translations": {
                                "en": "The cat <w>scratches</w> the table leg every day.",
                                "ru": "Кот каждый день <w>царапает</w> ножку стола."
                            }
                        }
                    ],
                    "synonyms": [
                        "krassen",
                        "schrapen"
                    ],
                    "common_phrases": [
                        "achter de oren krabben",
                        "op het hoofd krabben"
                    ],
                    "target_lang_definitions": {
                        "en": "To rub the skin with nails, often to relieve itching, or to rub a surface with a sharp object.",
                        "ru": "Тереть кожу или поверхность ногтями или острым предметом, часто чтобы облегчить зуд."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scratch",
                                "target_lang_sense_clarification": "rub with nails or claws"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "чесать",
                                "target_lang_sense_clarification": "облегчать зуд"
                            },
                            {
                                "target_lang_word": "царапать",
                                "target_lang_sense_clarification": "оставлять следы или повреждать поверхность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a8f86ca-26c2-37d7-92a3-b48424b794ac",
                    "sense_definition": "Het over de bodem slepen van een anker dat geen grip heeft.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "nautical_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ondanks de lange ankerketting <w>krabt</w> het anker nog steeds.",
                            "target_lang_translations": {
                                "en": "Despite the long anchor chain, the anchor is still <w>dragging</w>.",
                                "ru": "Несмотря на длинную якорную цепь, якорь всё ещё <w>ползет</w>."
                            }
                        },
                        {
                            "text": "Door de harde wind begon het anker te <w>krabben</w> en dreef het schip weg.",
                            "target_lang_translations": {
                                "en": "Due to the strong wind, the anchor began to <w>drag</w> and the ship drifted away.",
                                "ru": "Из-за сильного ветра якорь начал <w>ползти</w>, и судно отнесло."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To move along the seabed without holding fast (of an anchor).",
                        "ru": "Волочение якоря по дну, когда он не зацепился за грунт."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "drag",
                                "target_lang_sense_clarification": "nautical"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ползти",
                                "target_lang_sense_clarification": "о якоре"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b35744a9-65cf-3207-8bae-c5819fdf0534",
                    "sense_definition": "Meervoud van <w>krab</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animal_plural",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Op het strand zochten de kinderen naar kleine <w>krabben</w>.",
                            "target_lang_translations": {
                                "en": "On the beach, the children looked for small <w>crabs</w>.",
                                "ru": "На пляже дети искали маленьких <w>крабов</w>."
                            }
                        },
                        {
                            "text": "Veel <w>krabben</w> lopen zijwaarts over het zand.",
                            "target_lang_translations": {
                                "en": "Many <w>crabs</w> walk sideways across the sand.",
                                "ru": "Многие <w>крабы</w> ходят по песку боком."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Plural form of 'crab' (a sea creature with pincers).",
                        "ru": "Множественное число от слова «краб»."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "crabs",
                                "target_lang_sense_clarification": "sea creatures"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "крабы",
                                "target_lang_sense_clarification": "животные"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
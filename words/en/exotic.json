    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "2fe04c07-0184-385b-92f0-9bb152bdab7f",
                    "sense_definition": "Foreign or originating from a distant country, especially in a way that is exciting, colorful, or unusual.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "foreign_general",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "They decided to travel to an <w>exotic</w> island for their honeymoon.",
                            "target_lang_translations": {
                                "ru": "Они решили отправиться на <w>экзотический</w> остров в свой медовый месяц."
                            }
                        },
                        {
                            "text": "The market was filled with the smell of spices and <w>exotic</w> fruit.",
                            "target_lang_translations": {
                                "ru": "Рынок был наполнен запахом специй и <w>экзотических</w> фруктов."
                            }
                        }
                    ],
                    "synonyms": [
                        "foreign",
                        "unfamiliar",
                        "striking",
                        "unusual"
                    ],
                    "antonyms": [
                        "native",
                        "familiar",
                        "ordinary",
                        "indigenous"
                    ],
                    "common_phrases": [
                        "exotic fruit",
                        "exotic location",
                        "exotic animals",
                        "exotic beauty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Иностранный или происходящий из далекой страны, особенно если он кажется необычным, красочным или причудливым."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экзотический",
                                "target_lang_sense_clarification": "общий термин: связанный с далекими странами"
                            },
                            {
                                "target_lang_word": "экзотичный",
                                "target_lang_sense_clarification": "подчеркивает необычность и причудливость"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3b7424d-a726-30b8-853d-416c615c6c44",
                    "sense_definition": "Not native to a specific ecosystem or environment; introduced from elsewhere.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "foreign_biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The park rangers are working hard to remove <w>exotic</w> plants that threaten the local flora.",
                            "target_lang_translations": {
                                "ru": "Смотрители парка усердно работают над удалением <w>экзотических</w> растений, угрожающих местной флоре."
                            }
                        },
                        {
                            "text": "Many <w>exotic</w> species have no natural predators in their new environment.",
                            "target_lang_translations": {
                                "ru": "У многих <w>экзотических</w> видов нет естественных врагов в новой среде обитания."
                            }
                        }
                    ],
                    "synonyms": [
                        "non-native",
                        "introduced",
                        "alien",
                        "invasive"
                    ],
                    "antonyms": [
                        "native",
                        "indigenous",
                        "endemic"
                    ],
                    "common_phrases": [
                        "exotic species",
                        "exotic plants"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "biology/ecology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Не являющийся коренным для данной экосистемы; завезенный из других мест."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экзотический",
                                "target_lang_sense_clarification": "о видах растений или животных"
                            },
                            {
                                "target_lang_word": "чужеродный",
                                "target_lang_sense_clarification": "в контексте инвазивных видов"
                            }
                        ]
                    }
                },
                {

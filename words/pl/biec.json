                {
                    "sense_id": "b13a5ef6-e19b-3796-8fe6-190f84d01435",
                    "sense_definition": "Szybko przemieszczać się używając nóg, w taki sposób, że w pewnych momentach obie stopy są oderwane od ziemi.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "motion_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój mały pies wesoło <w>biegnie</w> do swojego właściciela.",
                            "target_lang_translations": {
                                "ru": "Мой маленький пёс весело <w>бежит</w> к своему хозяину."
                            }
                        },
                        {
                            "text": "Musimy szybko <w>biec</w> na przystanek, żeby zdążyć na autobus do szkoły.",
                            "target_lang_translations": {
                                "ru": "Мы должны быстро <w>бежать</w> на остановку, чтобы успеть на автобус в школу."
                            }
                        }
                    ],
                    "synonyms": [
                        "biegnąć",
                        "pędzić",
                        "gnać"
                    ],
                    "antonyms": [
                        "stać"
                    ],
                    "common_phrases": [
                        "biec do celu",
                        "biec szybko",
                        "biec przed siebie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро перемещаться с помощью ног, так, что в определенные моменты обе ноги оторваны от земли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бежать",
                                "target_lang_sense_clarification": "о людях и животных"
                            },
                            {
                                "target_lang_word": "нестись",
                                "target_lang_sense_clarification": "очень быстро бежать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "63d88204-ed52-3d1d-a54b-1bedb3e7b1c5",
                    "sense_definition": "Szybko mijać lub upływać (używane w kontekście czasu).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "motion_abstract_time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Niestety czas <w>biegnie</w> bardzo szybko, gdy dobrze się bawimy.",
                            "target_lang_translations": {
                                "ru": "К сожалению, время <w>бежит</w> очень быстро, когда мы хорошо проводим время."
                            }
                        },
                        {
                            "text": "Kolejne lata <w>biegną</w>, a my wciąż jesteśmy najlepszymi przyjaciółmi.",
                            "target_lang_translations": {
                                "ru": "Годы <w>бегут</w>, а мы всё ещё лучшие друзья."
                            }
                        }
                    ],
                    "synonyms": [
                        "mknąć"
                    ],
                    "common_phrases": [
                        "czas biegnie",
                        "lata biegną",
                        "życie biegnie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быстро проходить или утекать (используется в контексте времени)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бежать",
                                "target_lang_sense_clarification": "о времени"
                            },
                            {
                                "target_lang_word": "лететь",
                                "target_lang_sense_clarification": "очень быстро проходить"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c79740d2-36fe-3c57-b35d-2b5961c78ec8",
                    "sense_definition": "Rozciągać się lub prowadzić w określonym kierunku (o drodze, rzece, granicy lub instalacji).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "spatial_extension",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta wąska ścieżka <w>biegnie</w> prosto przez gęsty las aż do rzeki.",
                            "target_lang_translations": {
                                "ru": "Эта узкая тропинка <w>бежит</w> прямо через густой лес к реке."
                            }
                        },
                        {
                            "text": "Główna ulica miasta <w>biegnie</w> od rynku aż do starego dworca kolejowego.",
                            "target_lang_translations": {
                                "ru": "Главная улица города <w>проходит</w> от рыночной площади до старого вокзала."
                            }
                        }
                    ],
                    "synonyms": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "582e4a4d-afc4-36b1-b7fe-aa9876828a4d",
                    "sense_definition": "Teren, z którego usunięto drzewa i krzewy wraz z korzeniami, często w celu przygotowania ziemi pod uprawę.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "land_clearing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Rolnicy zamienili dawne <w>karczowisko</w> w żyzne pole uprawne.",
                            "target_lang_translations": {
                                "ru": "Фермеры превратили старый <w>расчищенный участок</w> в плодородное пахотное поле."
                            }
                        },
                        {
                            "text": "Siekierę zgubił gdzieś na rozległym <w>karczowisku</w> w środku lasu.",
                            "target_lang_translations": {
                                "ru": "Он потерял топор где-то на обширной <w>корчёвке</w> посреди леса."
                            }
                        }
                    ],
                    "synonyms": [
                        "karczunek"
                    ],
                    "common_phrases": [
                        "leśne karczowisko",
                        "rozległe karczowisko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Территория, с которой были удалены деревья и кустарники вместе с корнями, часто для подготовки земли к возделыванию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "расчищенный участок",
                                "target_lang_sense_clarification": "земля, освобожденная от леса"
                            },
                            {
                                "target_lang_word": "корчёвка",
                                "target_lang_sense_clarification": "место, где выкорчевали лес"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d093787b-10f1-3e58-b7ac-3eb291d38c63",
                    "sense_definition": "Miejsce w lesie pokryte wystającymi z ziemi pniami i korzeniami po ściętych drzewach.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "land_clearing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zaraz za nierównym <w>karczowiskiem</w> zaczynała się gęsta puszcza.",
                            "target_lang_translations": {
                                "ru": "Сразу за неровной <w>вырубкой</w> начиналась густая пуща."
                            }
                        },
                        {
                            "text": "Trudno było przejść przez <w>karczowisko</w> pełne wystających korzeni.",
                            "target_lang_translations": {
                                "ru": "Было трудно пройти через <w>вырубку</w>, полную торчащих корней."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stare karczowisko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место в лесу, покрытое торчащими из земли пнями и корнями после вырубки деревьев."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вырубка",
                                "target_lang_sense_clarification": "место, где вырублен лес"
                            },
                            {
                                "target_lang_word": "лесосека",
                                "target_lang_sense_clarification": "участок леса, отведенный под рубку"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "senses": [
                {
                    "sense_id": "15b7b2a5-7ce2-3b87-80a0-6dfd157a0d71",
                    "sense_definition": "Precies andersom; in de tegenovergestelde verhouding.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_reverse",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft hem niets aangedaan, maar <w>omgekeerd</w> is dat wel het geval.",
                            "target_lang_translations": {
                                "en": "He didn't hurt him, but <w>conversely</w>, that is the case.",
                                "ru": "Он ему ничего не сделал, но <w>наоборот</w> — это так."
                            }
                        },
                        {
                            "text": "Zij helpt hem vaak met huiswerk, en <w>omgekeerd</w> kookt hij voor haar.",
                            "target_lang_translations": {
                                "en": "She often helps him with homework, and <w>vice versa</w>, he cooks for her.",
                                "ru": "Она часто помогает ему с домашним заданием, и <w>наоборот</w>, он готовит для неё."
                            }
                        }
                    ],

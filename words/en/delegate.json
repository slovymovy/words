{
    "word_family": [
        "delegation",
        "delegator"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "5bb0583a-e44d-389e-9ac7-81754d8720f2",
                    "sense_definition": "To give part of your work, power, or authority to someone in a lower position.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "verb_assign",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A good manager knows when to <w>delegate</w> tasks to the team.",
                            "target_lang_translations": {
                                "ru": "Хороший менеджер знает, когда нужно <w>делегировать</w> задачи команде."
                            }
                        },
                        {
                            "text": "She decided to <w>delegate</w> responsibility for the project to her assistant.",
                            "target_lang_translations": {
                                "ru": "Она решила <w>передать</w> ответственность за проект своему помощнику."
                            }
                        }
                    ],
                    "synonyms": [
                        "assign",
                        "entrust",
                        "hand over",
                        "transfer"
                    ],
                    "antonyms": [
                        "keep",
                        "retain"
                    ],
                    "common_phrases": [
                        "delegate authority",
                        "delegate responsibility",
                        "delegate tasks",
                        "delegate effectively"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать часть своей работы, полномочий или власти кому-то, занимающему более низкую должность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делегировать",
                                "target_lang_sense_clarification": "передавать полномочия"
                            },
                            {
                                "target_lang_word": "поручать",
                                "target_lang_sense_clarification": "давать задание"
                            },
                            {
                                "target_lang_word": "передавать",
                                "target_lang_sense_clarification": "вручать обязанности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "af5769d7-8cf5-3edd-9d8f-16b7bd44e2a6",
                    "sense_definition": "To give away authority over a specific section of a network domain to another entity.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_assign",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The administrator chose to <w>delegate</w> the subdomain to a separate server.",
                            "target_lang_translations": {
                                "ru": "Администратор решил <w>делегировать</w> поддомен отдельному серверу."
                            }
                        },
                        {
                            "text": "We need to <w>delegate</w> control of this zone to the regional office.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>делегировать</w> управление этой зоной региональному офису."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Передавать управление определенной частью сетевого домена другому объекту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делегировать",
                                "target_lang_sense_clarification": "термин в информатике"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "02eaa0fa-560e-319a-8f35-77ab5ceec46f",
                    "sense_definition": "A person chosen or elected to represent a group of people, especially at a conference or meeting.",
                    "learner_level": "C1",
                    "frequency": "High",
                    "semantic_group_id": "noun_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The conference was attended by <w>delegates</w> from over fifty countries.",
                            "target_lang_translations": {
                                "ru": "На конференции присутствовали <w>делегаты</w> из более чем пятидесяти стран."
                            }
                        },
                        {
                            "text": "Each trade union sent a <w>delegate</w> to the annual congress.",
                            "target_lang_translations": {
                                "ru": "Каждый профсоюз направил <w>делегата</w> на ежегодный съезд."
                            }
                        }
                    ],
                    "synonyms": [
                        "representative",
                        "envoy",
                        "deputy",
                        "agent"
                    ],
                    "common_phrases": [
                        "conference delegate",
                        "chief delegate",
                        "trade delegate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, избранный или назначенный представлять группу людей, особенно на конференции или собрании."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делегат",
                                "target_lang_sense_clarification": "участник съезда или конференции"
                            },
                            {
                                "target_lang_word": "представитель",
                                "target_lang_sense_clarification": "выразитель интересов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1adf0a69-b90c-3c54-990d-6e51ebf26e11",
                    "sense_definition": "A member of a specific legislative body, sometimes without full voting power.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "noun_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>delegate</w> from the territory raised an important point during the session.",
                            "target_lang_translations": {
                                "ru": "<w>Делегат</w> от этой территории поднял важный вопрос во время сессии."
                            }
                        },
                        {
                            "text": "Voters elected a new <w>delegate</w> to the state assembly last November.",
                            "target_lang_translations": {
                                "ru": "В прошлом ноябре избиратели выбрали нового <w>делегата</w> в законодательное собрание штата."
                            }
                        }
                    ],
                    "synonyms": [
                        "legislator"
                    ],
                    "common_phrases": [
                        "house of delegates"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "politics"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Член определенного законодательного органа, иногда без права решающего голоса."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делегат",
                                "target_lang_sense_clarification": "член законодательного собрания"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e160978-0c1c-3fcb-8ff1-282122888b1f",
                    "sense_definition": "A type of variable in programming that holds a reference to a method or function.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You can use a <w>delegate</w> to pass a method as a parameter to another method.",
                            "target_lang_translations": {
                                "ru": "Вы можете использовать <w>делегат</w>, чтобы передать метод в качестве параметра другому методу."
                            }
                        },
                        {
                            "text": "The event handler is implemented using a multicast <w>delegate</w>.",
                            "target_lang_translations": {
                                "ru": "Обработчик событий реализован с использованием многоадресного <w>делегата</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "computing"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Тип переменной в программировании, которая хранит ссылку на метод или функцию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делегат",
                                "target_lang_sense_clarification": "тип данных"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "6f4b6d3a-e7b9-3bc1-bf30-e68c899b06b5",
                    "sense_definition": "Acting as a representative or having authority transferred to oneself.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>delegate</w> judge presided over the case in the absence of the magistrate.",
                            "target_lang_translations": {
                                "ru": "<w>Уполномоченный</w> судья председательствовал на процессе в отсутствие магистрата."
                            }
                        },
                        {
                            "text": "He exercised his <w>delegate</w> powers to sign the treaty.",
                            "target_lang_translations": {
                                "ru": "Он воспользовался своими <w>делегированными</w> полномочиями для подписания договора."
                            }
                        }
                    ],
                    "synonyms": [
                        "delegated",
                        "deputized"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mostly replaced by 'delegated'"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Действующий в качестве представителя или обладающий переданными полномочиями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "делегированный",
                                "target_lang_sense_clarification": "наделенный полномочиями"
                            },
                            {
                                "target_lang_word": "уполномоченный",
                                "target_lang_sense_clarification": "имеющий право"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
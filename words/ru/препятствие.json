    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5fc3f937-db07-3bcf-aa26-0edda8282da5",
                    "sense_definition": "То, что преграждает путь, задерживает или останавливает движение.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "obstacle_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Спортсмены должны были быстро преодолеть каждое <w>препятствие</w> на дистанции.",
                            "target_lang_translations": {
                                "en": "The athletes had to quickly overcome every <w>obstacle</w> on the course."
                            }
                        },
                        {
                            "text": "Упавшее дерево создало неожиданное <w>препятствие</w> на лесной дороге.",
                            "target_lang_translations": {
                                "en": "The fallen tree created an unexpected <w>obstacle</w> on the forest road."
                            }
                        }
                    ],
                    "synonyms": [
                        "прегра́да"
                    ],
                    "common_phrases": [
                        "полоса препятствий",
                        "водное препятствие",
                        "преодолеть препятствие"
                    ],
                    "target_lang_definitions": {
                        "en": "A physical object or feature that blocks a path, delays, or stops movement."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "obstacle",
                                "target_lang_sense_clarification": "general physical barrier"
                            },
                            {
                                "target_lang_word": "obstruction",
                                "target_lang_sense_clarification": "something blocking a passage or view"
                            },
                            {
                                "target_lang_word": "hurdle",
                                "target_lang_sense_clarification": "barrier to be jumped over in sports"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b77e0c75-4740-3375-b00d-499ab29db7e8",
                    "sense_definition": "То, что мешает осуществлению какого-либо действия или развитию чего-либо; помеха, затруднение.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "obstacle_group",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Отсутствие опыта не стало для него <w>препятствием</w> в поиске работы.",
                            "target_lang_translations": {
                                "en": "Lack of experience did not become an <w>obstacle</w> for him in finding a job."
                            }
                        },
                        {
                            "text": "Мы постараемся устранить все <w>препятствия</w>, мешающие нашему сотрудничеству.",
                            "target_lang_translations": {
                                "en": "We will try to remove all <w>obstacles</w> hindering our cooperation."
                            }
                        }
                    ],
                    "synonyms": [
                        "поме́ха",
                        "прегра́да",
                        "препо́на"
                    ],
                    "common_phrases": [
                        "чинить препятствия",
                        "серьёзное препятствие",
                        "устранить препятствия"
                    ],
                    "target_lang_definitions": {
                        "en": "A circumstance, fact, or difficulty that prevents progress or makes success difficult."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "obstacle",
                                "target_lang_sense_clarification": "figurative difficulty or barrier"
                            },
                            {
                                "target_lang_word": "hindrance",
                                "target_lang_sense_clarification": "something that delays or resists progress"
                            },
                            {
                                "target_lang_word": "impediment",
                                "target_lang_sense_clarification": "formal; a defect or restriction"
                            }
                        ]
                    }
                }
            ]
        }
    ]

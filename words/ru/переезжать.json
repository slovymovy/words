            "pos": "verb",
            "senses": [
                {
                    "sense_id": "c364456b-efa9-3982-9d73-8a2753bf9c35",
                    "sense_definition": "Переселяться из одного места жительства в другое.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "move_residence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На следующей неделе мы <переезжаем> в новую квартиру в центре города.",
                            "target_lang_translations": {
                                "en": "Next week we are <moving> to a new apartment in the city center."
                            }
                        },
                        {
                            "text": "Мой друг решил <переезжать> в другую страну ради новой работы.",
                            "target_lang_translations": {
                                "en": "My friend decided to <move> to another country for a new job."
                            }
                        }
                    ],
                    "synonyms": [
                        "переселяться"
                    ],
                    "common_phrases": [
                        "переезжать на новую квартиру",
                        "переезжать в другой город",
                        "собираться переезжать"
                    ],
                    "target_lang_definitions": {
                        "en": "To move from one place of residence to another."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "move",
                                "target_lang_sense_clarification": "change residence"
                            },
                            {
                                "target_lang_word": "relocate",
                                "target_lang_sense_clarification": "formal, often for work"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fef04719-3a9b-3414-88ec-1cc7da18d4b8",
                    "sense_definition": "Пересекать какое-либо пространство или препятствие, передвигаясь на транспортном средстве.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "cross_space",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Троллейбус медленно <переезжал> через трамвайные пути на перекрёстке.",
                            "target_lang_translations": {
                                "en": "The trolleybus was slowly <driving across> the tram tracks at the intersection."
                            }
                        },
                        {
                            "text": "Мы каждый день <переезжаем> этот мост по дороге на работу.",
                            "target_lang_translations": {
                                "en": "We <cross> this bridge every day on the way to work."
                            }
                        }
                    ],
                    "synonyms": [
                        "пересекать",
                        "переправляться"
                    ],
                    "common_phrases": [
                        "переезжать через мост",
                        "переезжать дорогу",
                        "переезжать границу"
                    ],
                    "target_lang_definitions": {
                        "en": "To cross a space or obstacle while moving in a vehicle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cross",
                                "target_lang_sense_clarification": "go across something"
                            },
                            {
                                "target_lang_word": "drive across",
                                "target_lang_sense_clarification": "cross by driving"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "25c109ac-66f0-37b1-bf1e-fbed3ef1bb2f",
                    "sense_definition": "Менять что-либо привычное или устоявшееся на новое (обычно о технологиях, платформах или сервисах).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_switch",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Многие пользователи сейчас <переезжают> на более современные операционные системы.",
                            "target_lang_translations": {
                                "en": "Many users are now <migrating> to more modern operating systems."
                            }
                        },
                        {
                            "text": "Наша компания полностью <переезжает> на удалённый режим работы.",
                            "target_lang_translations": {
                                "en": "Our company is fully <switching> to remote work."
                            }
                        }
                    ],
                    "synonyms": [
                        "мигрировать"
                    ],
                    "common_phrases": [
                        "переезжать на новую платформу",
                        "переезжать на другой софт"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "разговорное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To switch from an old system, platform, or service to a new one."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "migrate",
                                "target_lang_sense_clarification": "tech/software context"
                            },
                            {
                                "target_lang_word": "switch",
                                "target_lang_sense_clarification": "change to something else"
                            },
                            {
                                "target_lang_word": "move",
                                "target_lang_sense_clarification": "general transfer"
                            }
                        ]
                    }
                }
            ]

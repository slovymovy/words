                {
                    "sense_id": "38869469-14e9-30e7-96ae-41f2542a55aa",
                    "sense_definition": "De mogelijkheid om van buitenaf bij iemand naar binnen te kijken, vooral in een woning.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit huis aan de straatkant heeft helaas veel <w>inkijk</w>.",
                            "target_lang_translations": {
                                "ru": "Этот дом со стороны улицы, к сожалению, сильно <w>просматривается</w>."
                            }
                        },
                        {
                            "text": "We hebben vitrage opgehangen om de <w>inkijk</w> te beperken.",
                            "target_lang_translations": {
                                "ru": "Мы повесили тюль, чтобы ограничить <w>обзор с улицы</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "veel inkijk hebben",
                        "tegen de inkijk",
                        "last van inkijk"
                    ],
                    "target_lang_definitions": {
                        "ru": "Возможность видеть снаружи то, что происходит внутри, особенно в жилом помещении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "просматриваемость",
                                "target_lang_sense_clarification": "свойство помещения быть видимым с улицы"

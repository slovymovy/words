                    "sense_id": "b41f8930-dc27-3e39-ac82-c22347ec1bd8",
                    "sense_definition": "Przedmiot przeznaczony do gromadzenia, przechowywania lub transportu materiałów, zwłaszcza cieczy i gazów.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój samochód ma bardzo pojemny <w>zbiornik</w> paliwa, co pozwala na długą jazdę bez tankowania.",
                            "target_lang_translations": {
                                "ru": "У моей машины очень вместительный топливный <w>бак</w>, что позволяет долго ехать без заправки."
                            }
                        },
                        {
                            "text": "Na działce zainstalowaliśmy duży <w>zbiornik</w> na deszczówkę, aby podlewać nią kwiaty.",
                            "target_lang_translations": {
                                "ru": "На даче мы установили большой <w>бак</w> для дождевой воды, чтобы поливать ею цветы."
                            }
                        }
                    ],
                    "synonyms": [
                        "pojemnik",
                        "bak",
                        "butla",
                        "beczka",
                        "rezerwuar"
                    ],
                    "common_phrases": [
                        "zbiornik paliwa",
                        "zbiornik na wodę",
                        "pusty zbiornik",
                        "napełnić zbiornik"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет, предназначенный для сбора, хранения или транспортировки материалов, особенно жидкостей и газов."

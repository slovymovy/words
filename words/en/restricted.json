            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0df9f65e-f5a1-34dc-a725-40569963849a",
                    "sense_definition": "Available only to specific people or groups; not open to the public.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "access_control",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You cannot enter the <w>restricted</w> area without a security pass.",
                            "target_lang_translations": {
                                "ru": "Вы не можете войти в <w>закрытую</w> зону без пропуска."
                            }
                        },
                        {
                            "text": "The documents contained <w>restricted</w> information about the project.",
                            "target_lang_translations": {
                                "ru": "В документах содержалась <w>секретная</w> информация о проекте."
                            }
                        }
                    ],
                    "synonyms": [
                        "private",
                        "closed",
                        "exclusive",
                        "secure"
                    ],
                    "antonyms": [
                        "public",
                        "open",
                        "unrestricted"
                    ],
                    "common_phrases": [
                        "restricted area",
                        "restricted access",
                        "restricted zone",
                        "restricted information"
                    ],
                    "target_lang_definitions": {
                        "ru": "Доступный только определенным людям или группам; закрытый для широкой публики."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "закрытый",
                                "target_lang_sense_clarification": "недоступный для общего пользования"
                            },
                            {
                                "target_lang_word": "секретный",
                                "target_lang_sense_clarification": "об информации или документах"
                            },
                            {
                                "target_lang_word": "запретный",
                                "target_lang_sense_clarification": "о территории или зоне"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3b362b8e-3406-360b-94db-719296ef1043",
                    "sense_definition": "Kept within certain limits or bounds; not free or unlimited.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "limitation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor placed the patient on a <w>restricted</w> diet for health reasons.",
                            "target_lang_translations": {
                                "ru": "Врач назначил пациенту <w>ограниченную</w> диету по состоянию здоровья."
                            }
                        },
                        {
                            "text": "Due to the small budget, our options are quite <w>restricted</w>.",
                            "target_lang_translations": {
                                "ru": "Из-за небольшого бюджета наши возможности весьма <w>ограничены</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "limited",
                        "bounded",
                        "confined",
                        "constrained"
                    ],
                    "antonyms": [
                        "unlimited",
                        "free",
                        "broad"
                    ],
                    "common_phrases": [
                        "restricted diet",
                        "restricted growth",
                        "restricted movement",
                        "restricted view"
                    ],
                    "target_lang_definitions": {
                        "ru": "Удерживаемый в определенных пределах или границах; лимитированный."
                    },
                    "translations": {
                        "ru": [
                            {

                },
                {
                    "sense_id": "303f599e-5100-3cd3-a4d1-2a130071bce0",
                    "sense_definition": "To move up to a higher level of difficulty, complexity, or responsibility; to change gradually.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "progression",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The small local business eventually <w>graduated</w> to a large international corporation.",
                            "target_lang_translations": {
                                "ru": "Малый местный бизнес со временем <w>вырос</w> до крупной международной корпорации."
                            }
                        },
                        {
                            "text": "He started riding a tricycle and soon <w>graduated</w> to a bicycle.",
                            "target_lang_translations": {
                                "ru": "Он начал кататься на трехколесном велосипеде и вскоре <w>перешел</w> на двухколесный."
                            }
                        }
                    ],
                    "common_phrases": [
                        "graduate to something",
                        "graduate from something"
                    ],
                    "target_lang_definitions": {
                        "ru": "Переходить на более высокий уровень сложности или ответственности; постепенно меняться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "переходить",
                                "target_lang_sense_clarification": "на новый уровень или этап"
                            },
                            {
                                "target_lang_word": "расти",
                                "target_lang_sense_clarification": "развиваться до более высокого уровня"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3688932d-124b-3c4e-a436-bf36c65fd75f",
                    "sense_definition": "To mark a container or instrument with degrees or units of measurement.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "measurement_tools",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The manufacturer must <w>graduate</w> the cylinder precisely for accurate lab work.",
                            "target_lang_translations": {
                                "ru": "Производитель должен точно <w>градуировать</w> цилиндр для точной лабораторной работы."
                            }
                        },
                        {
                            "text": "They <w>graduate</w> the thermometer scale in both Celsius and Fahrenheit.",
                            "target_lang_translations": {
                                "ru": "Они <w>градуируют</w> шкалу термометра как в Цельсиях, так и в Фаренгейтах."
                            }
                        }
                    ],
                    "common_phrases": [
                        "graduate a scale",
                        "graduate a vessel"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Наносить на емкость или инструмент градусы или единицы измерения."

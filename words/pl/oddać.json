    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "76212e00-dc1c-3a95-a311-ed3bd94934ad",
                    "sense_definition": "Zwrócić coś właścicielowi; dać z powrotem to, co się wcześniej wzięło lub pożyczyło.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "return_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę jutro <oddać> książkę do biblioteki, bo kończy się termin.",
                            "target_lang_translations": {
                                "ru": "Я должен завтра <вернуть> книгу в библиотеку, так как срок заканчивается."
                            }
                        },
                        {
                            "text": "Kiedy wreszcie <oddasz> mi pieniądze, które pożyczyłeś miesiąc temu?",
                            "target_lang_translations": {
                                "ru": "Когда ты наконец <вернешь> мне деньги, которые одолжил месяц назад?"
                            }
                        }
                    ],
                    "synonyms": [
                        "zwrócić"
                    ],
                    "antonyms": [
                        "pożyczyć",
                        "zabrać",
                        "wziąć"
                    ],
                    "common_phrases": [
                        "oddać dług",
                        "oddać pieniądze",
                        "oddać książkę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вернуть что-либо владельцу; отдать обратно то, что было взято или одолжено ранее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вернуть",
                                "target_lang_sense_clarification": "отдать обратно владельцу"
                            },
                            {
                                "target_lang_word": "отдать",
                                "target_lang_sense_clarification": "возвратить долг или вещь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "09e1aef6-4910-369e-8bc7-257740ce6e7e",
                    "sense_definition": "Przekazać coś komuś, dostarczyć w określone miejsce lub powierzyć czyjejś opiece.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "hand_over",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Uczeń <oddał> nauczycielowi pustą kartkę, ponieważ nic nie napisał.",
                            "target_lang_translations": {
                                "ru": "Ученик <сдал> учителю пустой лист, так как ничего не написал."
                            }
                        },
                        {
                            "text": "Musimy <oddać> ten projekt do sprawdzenia przed końcem tygodnia.",
                            "target_lang_translations": {
                                "ru": "Мы должны <сдать> этот проект на проверку до конца недели."
                            }
                        }
                    ],
                    "synonyms": [
                        "dać",
                        "zwrócić",
                        "sprzedać"
                    ],
                    "antonyms": [
                        "odebrać",
                        "przyjąć"
                    ],
                    "common_phrases": [
                        "oddać głos",
                        "oddać do naprawy",
                        "oddać do druku"
                    ],
                    "target_lang_definitions": {
                        "ru": "Передать что-либо кому-либо, вручить или сдать (например, работу на проверку или вещь в ремонт)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдать",
                                "target_lang_sense_clarification": "работу, проект, экзамен"
                            },
                            {
                                "target_lang_word": "отдать",
                                "target_lang_sense_clarification": "вручить, передать в руки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "26218609-1a59-327a-85bb-25840f8ad14c",
                    "sense_definition": "Przedstawić coś wiernie, odzwierciedlić charakter, wygląd, nastrój lub treść czegoś.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "reflect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "To zdjęcie doskonale <oddaje> piękno górskiego krajobrazu.",
                            "target_lang_translations": {
                                "ru": "Эта фотография прекрасно <передает> красоту горного пейзажа."
                            }
                        },
                        {
                            "text": "Tłumaczenie nie zawsze potrafi w pełni <oddać> sens oryginału.",
                            "target_lang_translations": {
                                "ru": "Перевод не всегда может полностью <передать> смысл оригинала."
                            }
                        }
                    ],
                    "synonyms": [
                        "odzwierciedlić",
                        "odpowiedzieć"
                    ],
                    "common_phrases": [
                        "oddać atmosferę",
                        "oddać sens",
                        "oddać rzeczywistość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верно изобразить, передать характер, вид, настроение или содержание чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "передать",
                                "target_lang_sense_clarification": "смысл, атмосферу, красоту"
                            },
                            {
                                "target_lang_word": "отразить",
                                "target_lang_sense_clarification": "показать, воспроизвести"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96b6b8d9-8caa-346d-ac84-ab75c59ea1ca",
                    "sense_definition": "Zrezygnować z czegoś na czyjąś korzyść, podarować coś komuś lub poświęcić coś cennego.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "donate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Postanowiła <oddać> wszystkie swoje stare ubrania biednym ludziom.",
                            "target_lang_translations": {
                                "ru": "Она решила <отдать> все свои старые вещи бедным людям."
                            }
                        },
                        {
                            "text": "Żołnierze byli gotowi <oddać> życie za wolność swojego kraju.",
                            "target_lang_translations": {
                                "ru": "Солдаты были готовы <отдать> жизнь за свободу своей страны."
                            }
                        }
                    ],
                    "synonyms": [
                        "ofiarować",
                        "podarować",
                        "dać"
                    ],
                    "antonyms": [
                        "zachować",
                        "zatrzymać"
                    ],
                    "common_phrases": [
                        "oddać krew",
                        "oddać życie",
                        "oddać za darmo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отказаться от чего-либо в чью-либо пользу, пожертвовать или подарить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отдать",
                                "target_lang_sense_clarification": "пожертвовать, подарить"
                            },
                            {
                                "target_lang_word": "пожертвовать",
                                "target_lang_sense_clarification": "принести в жертву"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da356dde-a16e-3d93-99eb-334e2343bfc6",
                    "sense_definition": "Udostępnić coś do użytku po zakończeniu budowy, remontu lub prac przygotowawczych.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "commission",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy odcinek autostrady zostanie <oddany> do użytku w przyszłym miesiącu.",
                            "target_lang_translations": {
                                "ru": "Новый участок автострады будет <сдан> в эксплуатацию в следующем месяце."
                            }
                        },
                        {
                            "text": "Deweloper <oddał> klucze do mieszkań miesiąc przed terminem.",
                            "target_lang_translations": {
                                "ru": "Застройщик <выдал> ключи от квартир за месяц до срока."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oddać do użytku",
                        "oddać budynek",
                        "oddać inwestycję"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предоставить в пользование после завершения строительства или ремонта; сдать в эксплуатацию."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдать",
                                "target_lang_sense_clarification": "в эксплуатацию (здание, объект)"
                            },
                            {
                                "target_lang_word": "открыть",
                                "target_lang_sense_clarification": "для использования"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a654f98a-c303-32aa-b79b-22cfd190a947",
                    "sense_definition": "Poświęcić się całkowicie jakiejś sprawie, pasji lub osobie (używane z zaimkiem 'się').",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "devote",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Po przejściu na emeryturę dziadek całkowicie <oddał> się pracy w ogrodzie.",
                            "target_lang_translations": {
                                "ru": "После выхода на пенсию дедушка полностью <отдался> работе в саду."
                            }
                        },
                        {
                            "text": "Ona bez reszty <oddała> się wychowaniu swoich dzieci.",
                            "target_lang_translations": {
                                "ru": "Она без остатка <посвятила> себя воспитанию своих детей."
                            }
                        }
                    ],
                    "synonyms": [
                        "ofiarować"
                    ],
                    "common_phrases": [
                        "oddać się pracy",
                        "oddać się marzeniom",
                        "oddać się lekturze"
                    ],
                    "target_lang_definitions": {
                        "ru": "Полностью посвятить себя какому-либо делу, увлечению или человеку."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отдаться",
                                "target_lang_sense_clarification": "посвятить себя целиком"
                            },
                            {
                                "target_lang_word": "посвятить себя",
                                "target_lang_sense_clarification": "занятию или человеку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dca80b7c-e971-32fc-924f-de1c28c1bf7d",
                    "sense_definition": "Odwzajemnić się; zareagować na czyjeś działanie tym samym (np. uderzeniem, przysługą).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reciprocate",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciał go uderzyć, ale przeciwnik był szybszy i zdołał <oddać>.",
                            "target_lang_translations": {
                                "ru": "Он хотел его ударить, но противник был быстрее и сумел <дать сдачи>."
                            }
                        },
                        {
                            "text": "Kiedyś ci to <oddam> z nawiązką, obiecuję.",
                            "target_lang_translations": {
                                "ru": "Когда-нибудь я тебе <отплачу> за это с лихвой, обещаю."
                            }
                        }
                    ],
                    "synonyms": [
                        "zwrócić",
                        "odpowiedzieć"
                    ],
                    "common_phrases": [
                        "oddać cios",
                        "oddać przysługę",
                        "oddać wet za wet"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отплатить тем же; ответить на чье-либо действие (ударом, услугой)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дать сдачи",
                                "target_lang_sense_clarification": "ответить ударом на удар"
                            },
                            {
                                "target_lang_word": "отплатить",
                                "target_lang_sense_clarification": "ответить услугой или местью"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1cdc6b03-c193-374e-8a9e-75b47231afa9",
                    "sense_definition": "Wydalić z organizmu mocz lub kał.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physiological",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pacjent musi <oddać> mocz do specjalnego pojemnika w celu badania.",
                            "target_lang_translations": {
                                "ru": "Пациент должен <сдать> мочу в специальный контейнер для анализа."
                            }
                        },
                        {
                            "text": "Lekarz zapytał, czy pacjent może bez bólu <oddać> stolec.",
                            "target_lang_translations": {
                                "ru": "Врач спросил, может ли пациент без боли <опорожнить> кишечник."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oddać mocz",
                        "oddać stolec"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "medyczny lub formalny"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выделить из организма мочу или кал (медицинский или формальный контекст)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сдать",
                                "target_lang_sense_clarification": "анализ"
                            },
                            {
                                "target_lang_word": "опорожнить",
                                "target_lang_sense_clarification": "кишечник или мочевой пузырь"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "c07b722e-8c72-3ee2-a756-b5bba95dda87",
                    "sense_definition": "Zoals het op het eerste gezicht lijkt, maar wat niet noodzakelijk waar is.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft <w>schijnbaar</w> geen interesse in het aanbod dat we hem deden.",
                            "target_lang_translations": {
                                "en": "He <w>seemingly</w> has no interest in the offer we made him.",
                                "ru": "У него, <w>по-видимому</w>, нет интереса к предложению, которое мы ему сделали."
                            }
                        },
                        {
                            "text": "Het is een <w>schijnbaar</w> onmogelijke taak om dit project op tijd af te ronden.",
                            "target_lang_translations": {
                                "en": "It is a <w>seemingly</w> impossible task to complete this project on time.",
                                "ru": "Завершить этот проект вовремя — это <w>с виду</w> невыполнимая задача."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogenschijnlijk",
                        "vermoedelijk"
                    ],
                    "antonyms": [
                        "feitelijk",
                        "werkelijk",
                        "blijkbaar"
                    ],
                    "common_phrases": [
                        "schijnbaar onmogelijk",
                        "schijnbaar toevallig",
                        "schijnbaar eindeloos"
                    ],
                    "target_lang_definitions": {
                        "en": "As it appears at first glance, but is not necessarily true.",
                        "ru": "Как кажется на первый взгляд, но что не обязательно является правдой."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "seemingly",
                                "target_lang_sense_clarification": "appearing to be something, especially when this might not be true"
                            },
                            {
                                "target_lang_word": "apparently",
                                "target_lang_sense_clarification": "used to describe how a situation appears, though it may not be the reality"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "по-видимому",
                                "target_lang_sense_clarification": "судя по всему, вероятно"
                            },
                            {
                                "target_lang_word": "с виду",
                                "target_lang_sense_clarification": "внешне, на первый взгляд"
                            },
                            {
                                "target_lang_word": "казалось бы",
                                "target_lang_sense_clarification": "как может показаться"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "66546553-9fe2-30c5-84cc-80a9ab1f79ae",
                    "sense_definition": "Wat zo lijkt, maar in werkelijkheid anders is.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Er heerste een <w>schijnbare</w> rust in de stad vlak voor de storm losbarstte.",
                            "target_lang_translations": {
                                "en": "An <w>apparent</w> calm prevailed in the city just before the storm broke.",
                                "ru": "В городе царило <w>кажущееся</w> спокойствие прямо перед тем, как разразилась буря."
                            }
                        },
                        {
                            "text": "Ondanks zijn <w>schijnbare</w> onverschilligheid, luisterde hij heel aandachtig naar het gesprek.",
                            "target_lang_translations": {
                                "en": "Despite his <w>apparent</w> indifference, he listened very attentively to the conversation.",
                                "ru": "Несмотря на свое <w>видимое</w> безразличие, он очень внимательно слушал разговор."
                            }
                        }
                    ],
                    "synonyms": [
                        "ogenschijnlijk",
                        "vermeend"
                    ],
                    "antonyms": [
                        "echt",
                        "werkelijk",
                        "reëel"
                    ],
                    "common_phrases": [
                        "schijnbare tegenstelling",
                        "schijnbare rust",
                        "schijnbare beweging"
                    ],
                    "target_lang_definitions": {
                        "en": "Something that appears a certain way, but is different in reality.",
                        "ru": "То, что кажется таковым, но на самом деле иное."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "apparent",
                                "target_lang_sense_clarification": "seeming to be real or true, but distinct from reality"
                            },
                            {
                                "target_lang_word": "seeming",
                                "target_lang_sense_clarification": "appearing to be real or true, but not necessarily being so"

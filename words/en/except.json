{
    "word_family": [
        "exception",
        "exceptional",
        "exceptionally",
        "exceptionable"
    ],
    "entries": [
        {
            "pos": "preposition",
            "senses": [
                {
                    "sense_id": "5ede5be1-4e37-3e59-aa2e-d11a435169a1",
                    "sense_definition": "Used to specify a person, thing, or fact that is not included in the general statement made.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "exclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There was nothing in the cupboard <w>except</w> a tin of beans."
                        },
                        {
                            "text": "Everyone went to the party <w>except</w> for John, who was sick."
                        }
                    ],
                    "synonyms": [
                        "apart from",
                        "aside from",
                        "bar",
                        "barring",
                        "besides",
                        "but",
                        "excluding",
                        "other than",
                        "save",
                        "with the exception of"
                    ],
                    "antonyms": [
                        "in addition to"
                    ],
                    "common_phrases": [
                        "except for",
                        "except that"
                    ]
                }
            ]
        },
        {
            "pos": "conjunction",
            "senses": [
                {
                    "sense_id": "4d37103d-03bf-3bf3-8e8b-8cbf3a9b732d",
                    "sense_definition": "Used to introduce a statement that explains why the previous statement is not completely true or possible.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "exclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You look a bit like my sister, <w>except</w> she has longer hair."
                        },
                        {
                            "text": "I would go with you, <w>except</w> I have too much work to do right now."
                        }
                    ],
                    "synonyms": [
                        "but"
                    ],
                    "common_phrases": [
                        "except that"
                    ]
                },
                {
                    "sense_id": "e4a379e3-d16a-3718-a9f4-1718aa3b3d89",
                    "sense_definition": "Used to introduce a condition or hypothetical case; unless.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "condition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Offensive wars, <w>except</w> the cause be very just, I will not allow of."
                        },
                        {
                            "text": "I cannot grant this request <w>except</w> you provide proof of your identity."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ]
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0391582c-e7c0-343e-8415-5ed655839413",
                    "sense_definition": "To leave out or exclude someone or something from a group or list.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "exclusion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I find most people annoying, present company <w>excepted</w> of course."
                        },
                        {
                            "text": "The new law applies to all citizens, <w>excepting</w> those with diplomatic immunity."
                        }
                    ],
                    "common_phrases": [
                        "present company excepted"
                    ]
                },
                {
                    "sense_id": "da5381dd-a5b2-3967-b9cb-aea43b2aafe8",
                    "sense_definition": "To object formally to a statement or witness.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "objection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The defense attorney decided to <w>except</w> to the witness's testimony."
                        },
                        {
                            "text": "I must <w>except</w> against that conclusion based on the evidence provided."
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal or legal"
                        }
                    ]
                }
            ]
        }
    ]
}
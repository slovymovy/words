            "senses": [
                {
                    "sense_id": "b064c472-dd95-34cd-9f16-23d1f87c4142",
                    "sense_definition": "Martwić się czymś, odczuwać niepokój lub traktować coś bardzo poważnie i emocjonalnie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "worry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nie warto <w>przejmować się</w> rzeczami, na które nie mamy żadnego wpływu.",
                            "target_lang_translations": {
                                "ru": "Не стоит <w>переживать</w> из-за вещей, на которые мы не имеем никакого влияния."
                            }
                        },
                        {
                            "text": "Ona zawsze bardzo <w>przejmuje się</w> swoimi ocenami w szkole.",
                            "target_lang_translations": {
                                "ru": "Она всегда очень <w>волнуется</w> из-за своих оценок в школе."
                            }
                        }
                    ],
                    "synonyms": [
                        "martwić się",
                        "niepokoić się",
                        "brać do serca",
                        "wzruszać się"
                    ],
                    "antonyms": [
                        "lekceważyć",
                        "ignorować"
                    ],
                    "common_phrases": [
                        "nie przejmować się",
                        "przejmować się opinią",
                        "przejmować się drobiazgami"
                    ],
                    "target_lang_definitions": {
                        "ru": "Беспокоиться о чем-то, испытывать тревогу или воспринимать что-то очень серьезно и эмоционально."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волноваться",
                                "target_lang_sense_clarification": "испытывать беспокойство"
                            },
                            {
                                "target_lang_word": "переживать",
                                "target_lang_sense_clarification": "эмоционально реагировать"
                            },
                            {
                                "target_lang_word": "принимать близко к сердцу",
                                "target_lang_sense_clarification": "сильно расстраиваться"
                            }
                        ]
                    }
                }
            ]
        }

            "senses": [
                {
                    "sense_id": "58b0bee9-bf19-3d5a-938c-b3a9446e3c16",
                    "sense_definition": "Wyrazić wdzięczność za coś, co się otrzymało, lub za czyjąś pomoc.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chciałbym ci bardzo <podziękować> za twoją pomoc przy przeprowadzce.",
                            "target_lang_translations": {
                                "ru": "Я хотел бы очень <поблагодарить> тебя за твою помощь с переездом."
                            }
                        },
                        {
                            "text": "Anna <podziękowała> rodzicom za wspaniały prezent urodzinowy.",
                            "target_lang_translations": {
                                "ru": "Анна <поблагодарила> родителей за замечательный подарок на день рождения."
                            }
                        }
                    ],
                    "synonyms": [
                        "wyrazić wdzięczność"
                    ],
                    "common_phrases": [
                        "podziękować za pomoc",
                        "podziękować za zaproszenie",
                        "z góry podziękować",
                        "serdecznie podziękować"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выразить благодарность за что-то полученное или за чью-то помощь."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поблагодарить",
                                "target_lang_sense_clarification": "выразить признательность"
                            },
                            {
                                "target_lang_word": "сказать спасибо",
                                "target_lang_sense_clarification": "разговорный вариант"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6daee259-d0ba-3aa4-aee0-3ac4a81ec3d0",
                    "sense_definition": "Uprzejmie odmówić przyjęcia czegoś, zrezygnować z propozycji lub zakończyć współpracę.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jestem już bardzo najedzony, więc muszę <podziękować> za deser.",
                            "target_lang_translations": {
                                "ru": "Я уже очень сыт, поэтому вынужден <отказаться> от десерта."
                            }
                        },
                        {
                            "text": "Firma postanowiła <podziękować> mu za współpracę po trzech miesiącach.",
                            "target_lang_translations": {
                                "ru": "Фирма решила <распрощаться> с ним после трех месяцев сотрудничества."
                            }
                        }
                    ],
                    "synonyms": [
                        "odmówić",
                        "zrezygnować"
                    ],
                    "antonyms": [
                        "przyjąć",
                        "zaakceptować"
                    ],
                    "common_phrases": [
                        "podziękować za współpracę",
                        "grzecznie podziękować",
                        "podziękować za udział"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often used as a polite euphemism for refusal or dismissal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Вежливо отказаться от чего-либо, отклонить предложение или прекратить сотрудничество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отказаться",

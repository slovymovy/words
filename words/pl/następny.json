            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "4df098cb-8144-3703-9dda-47cbf15a6dc9",
                    "sense_definition": "Taki, który występuje bezpośrednio po kimś lub po czymś w czasie, przestrzeni lub ustalonej kolejności.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "sequence",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Proszę przyjść na wizytę kontrolną w <w>następny</w> poniedziałek rano.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, приходите на контрольный осмотр в <w>следующий</w> понедельник утром."
                            }
                        },
                        {
                            "text": "To jest już <w>następny</w> raz, kiedy spóźniasz się do pracy bez ważnego powodu.",
                            "target_lang_translations": {
                                "ru": "Это уже <w>очередной</w> раз, когда ты опаздываешь на работу без уважительной причины."
                            }
                        },
                        {
                            "text": "Ja jestem pierwszy w kolejce, a <w>następna</w> jest ta pani w płaszczu.",
                            "target_lang_translations": {
                                "ru": "Я первый в очереди, а <w>следующая</w> — та дама в пальто."
                            }
                        }
                    ],
                    "synonyms": [
                        "kolejny",
                        "następujący"
                    ],
                    "antonyms": [
                        "poprzedni"
                    ],
                    "common_phrases": [
                        "następny dzień",
                        "w następnym tygodniu",
                        "następny raz",
                        "następny przystanek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, который идет непосредственно после кого-то или чего-то во времени, пространстве или установленном порядке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "следующий",
                                "target_lang_sense_clarification": "идущий следом"
                            },
                            {
                                "target_lang_word": "очередной",
                                "target_lang_sense_clarification": "следующий по порядку"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "050c4c0b-ef65-3b7c-a1f5-df324efd2fe2",
                    "sense_definition": "Odnoszący się do dziedziczenia majątku, praw lub tytułów; związany z sukcesją.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "legal_archaic",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dawnym prawie istniał specyficzny porządek <w>następny</w> dotyczący wielkich majątków ziemskich.",
                            "target_lang_translations": {
                                "ru": "В древнем праве существовал особый <w>наследственный</w> порядок, касающийся крупных земельных владений."
                            }
                        },
                        {
                            "text": "Był to tytuł <w>następny</w>, który przechodził z ojca na najstarszego syna.",
                            "target_lang_translations": {
                                "ru": "Это был <w>наследственный</w> титул, который переходил от отца к старшему сыну."
                            }
                        }
                    ],
                    "synonyms": [
                        "dziedziczny",
                        "spadkowy"

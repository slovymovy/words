                    "sense_id": "c32c093e-486f-3424-b8b3-bdb475eeff94",
                    "sense_definition": "Een moeilijke situatie of omstandigheid die om een oplossing vraagt",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",

                    "frequency": "Middle",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Фермер поставил тяжелый <w>бидон</w> с молоком в кузов машины."
                        },
                        {
                            "text": "На даче мы используем старый алюминиевый <w>бидон</w> для хранения питьевой воды."
                        }
                    ],
                    "common_phrases": [
                        "алюминиевый бидон",
                        "бидон с молоком",

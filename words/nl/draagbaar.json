{
    "word_family": [
        "dragen",
        "drager",
        "draaglijk",
        "draagkracht",
        "draagvlak"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "db48b0eb-8134-31bf-b611-800a84c0ea2c",
                    "sense_definition": "Geschikt om gemakkelijk meegenomen of verplaatst te worden.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "portable_objects",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die stoel kan je wel vast buiten zetten, die is goed <w>draagbaar</w>.",
                            "target_lang_translations": {
                                "en": "You can go ahead and put that chair outside; it is easily <w>portable</w>.",
                                "ru": "Ты можешь выставить этот стул на улицу, он вполне <w>переносной</w>."
                            }
                        },
                        {
                            "text": "Heb je een <w>draagbare</w> radio meegenomen naar het strand?",
                            "target_lang_translations": {
                                "en": "Did you bring a <w>portable</w> radio to the beach?",
                                "ru": "Ты взял с собой на пляж <w>портативное</w> радио?"
                            }
                        }
                    ],
                    "antonyms": [
                        "ondraagbaar"
                    ],
                    "common_phrases": [
                        "draagbare computer",
                        "gemakkelijk draagbaar"
                    ],
                    "target_lang_definitions": {
                        "en": "Suitable to be easily taken along or moved.",
                        "ru": "Пригодный для легкой переноски или перемещения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "portable",
                                "target_lang_sense_clarification": "able to be easily carried or moved"
                            },
                            {
                                "target_lang_word": "movable",
                                "target_lang_sense_clarification": "capable of being moved"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "переносной",
                                "target_lang_sense_clarification": "который можно переносить с места на место"
                            },
                            {
                                "target_lang_word": "портативный",
                                "target_lang_sense_clarification": "компактный, удобный для ношения с собой (обычно о технике)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "90922cdc-adac-3f47-a0ce-22c6be289378",
                    "sense_definition": "Mogelijk om emotioneel of fysiek te verdragen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "bearable_emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na een half jaar werd het verlies toch <w>draagbaar</w>.",
                            "target_lang_translations": {
                                "en": "After six months, the loss finally became <w>bearable</w>.",
                                "ru": "Спустя полгода утрата всё же стала <w>выносимой</w>."
                            }
                        },
                        {
                            "text": "De pijn was door de zware medicatie weer enigszins <w>draagbaar</w>.",
                            "target_lang_translations": {
                                "en": "Thanks to the heavy medication, the pain was somewhat <w>bearable</w> again.",
                                "ru": "Благодаря сильным лекарствам боль снова стала более-менее <w>терпимой</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "ondraagbaar"
                    ],
                    "common_phrases": [
                        "nauwelijks draagbaar"
                    ],
                    "target_lang_definitions": {
                        "en": "Possible to endure emotionally or physically.",
                        "ru": "Такой, который можно вынести или вытерпеть (физически или эмоционально)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bearable",
                                "target_lang_sense_clarification": "able to be endured"
                            },
                            {
                                "target_lang_word": "tolerable",
                                "target_lang_sense_clarification": "able to be tolerated"
                            },
                            {
                                "target_lang_word": "endurable",
                                "target_lang_sense_clarification": "capable of being withstood"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "терпимый",
                                "target_lang_sense_clarification": "такой, который можно стерпеть"
                            },
                            {
                                "target_lang_word": "выносимый",
                                "target_lang_sense_clarification": "возможный для перенесения, выдерживания"
                            },
                            {
                                "target_lang_word": "сносный",
                                "target_lang_sense_clarification": "удовлетворительный, такой, с которым можно мириться"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "205929c2-05dd-3028-97e1-88f302d99fa1",
                    "sense_definition": "Een hulpmiddel bestaande uit een frame met doek om zieken of gewonden liggend te verplaatsen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "medical_equipment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Omdat hij bewusteloos was geraakt werd hij op de <w>draagbaar</w> gelegd.",
                            "target_lang_translations": {
                                "en": "Because he had lost consciousness, he was placed on the <w>stretcher</w>.",
                                "ru": "Так как он потерял сознание, его положили на <w>носилки</w>."
                            }
                        },
                        {
                            "text": "De ziekenbroeders tilden de <w>draagbaar</w> voorzichtig de ambulance in.",
                            "target_lang_translations": {
                                "en": "The paramedics carefully lifted the <w>stretcher</w> into the ambulance.",
                                "ru": "Санитары осторожно подняли <w>носилки</w> в машину скорой помощи."
                            }
                        }
                    ],
                    "synonyms": [
                        "brancard",
                        "baar"
                    ],
                    "common_phrases": [
                        "op een draagbaar",
                        "per draagbaar"
                    ],
                    "target_lang_definitions": {
                        "en": "A tool consisting of a frame with cloth to move sick or injured people lying down.",
                        "ru": "Приспособление для переноски больных или раненых в лежачем положении."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stretcher",
                                "target_lang_sense_clarification": "medical device for carrying patients"
                            },
                            {
                                "target_lang_word": "litter",
                                "target_lang_sense_clarification": "framework for carrying sick or injured people"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "носилки",
                                "target_lang_sense_clarification": "медицинское приспособление для переноски людей"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c7071a55-6093-3fc1-a409-e366b268cb2e",
                    "sense_definition": "A brief extra performance done after the main performance is complete.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "performance_content",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The band returned to the stage to play an <w>encore</w> for the screaming fans."
                        },
                        {
                            "text": "We stayed for the <w>encore</w> because it was their most famous song."
                        }
                    ],
                    "common_phrases": [
                        "play an encore",
                        "do an encore",
                        "come back for an encore"
                    ]
                },
                {
                    "sense_id": "e9b5ed65-e2e6-3475-aa72-ce307cd3c8b7",
                    "sense_definition": "A call or demand from the audience for a repeat performance.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "performance_request",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The loud <w>encores</w> from the crowd forced the singer to return."
                        },
                        {
                            "text": "Despite the enthusiastic <w>encores</w>, the lights went up and the show ended."
                        }
                    ],
                    "common_phrases": [
                        "deafening encores"
                    ]
                }
            ]
        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "bab0ae48-d7b2-359b-9eb4-387de283bcd8",
                    "sense_definition": "An exclamation used to ask a performer to perform again.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "performance_request",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The audience stood up and shouted '<w>Encore</w>!' at the end of the concert."
                        },
                        {
                            "text": "She smiled as she heard cries of '<w>Encore</w>!' echoing through the hall."
                        }
                    ],
                    "common_phrases": [
                        "shout encore"
                    ]
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2765a091-8cb9-3993-9870-f6ae1517f032",
                    "sense_definition": "To call for an extra performance from an artist or group.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "performance_request",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The crowd <w>encored</w> the pianist until he sat down to play again."
                        },
                        {
                            "text": "They <w>encored</w> the band enthusiastically for several minutes."
                        }
                    ]
                },
                {
                    "sense_id": "1efcf4c4-f37a-340b-a1c7-97f283eea659",
                    "sense_definition": "To perform an extra piece after the main show.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "performance_content",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The group <w>encored</w> with a cover of a classic rock song."
                        },
                        {
                            "text": "They <w>encored</w> twice because the audience refused to leave."
                        }
                    ]
                }
            ]
        }
    ]

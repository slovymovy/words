    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "d9ed4ad8-b2a8-32b9-b3d2-0bada6f36b5b",
                    "sense_definition": "Naar achteren gericht; in de richting van de rug of de achterkant.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij zette voorzichtig een stap <w>achteruit</w> toen de hond begon te blaffen.",
                            "target_lang_translations": {
                                "ru": "Он осторожно сделал шаг <w>назад</w>, когда собака начала лаять.",
                                "en": "He carefully took a step <w>back</w> when the dog started barking."
                            }
                        },
                        {
                            "text": "De auto reed langzaam <w>achteruit</w> om uit het parkeervak te komen.",
                            "target_lang_translations": {
                                "ru": "Машина медленно ехала <w>назад</w>, чтобы выехать с парковочного места.",
                                "en": "The car slowly drove <w>backwards</w> to get out of the parking space."
                            }
                        }
                    ],
                    "synonyms": [
                        "achterwaarts",
                        "terug"
                    ],
                    "antonyms": [
                        "vooruit"
                    ],
                    "common_phrases": [
                        "stap achteruit",
                        "achteruit rijden",
                        "achteruit kijken",
                        "achteruit lopen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Направленный назад; в сторону спины или задней части.",
                        "en": "Directed backwards; towards the back or rear."
                    },
                    "translations": {

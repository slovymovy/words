            "senses": [
                {
                    "sense_id": "c9641bd4-142e-370e-9f9a-54998bf1c5d0",
                    "sense_definition": "Ssak drapieżny z rodziny niedźwiedziowatych, o charakterystycznym czarno-białym ubarwieniu, żywiący się głównie pędami bambusa.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W zoo urodziła się mała <w>panda</w>, która od razu stała się ulubieńcem zwiedzających.",
                            "target_lang_translations": {
                                "ru": "В зоопарке родилась маленькая <w>панда</w>, которая сразу стала любимицей посетителей."
                            }
                        },
                        {
                            "text": "Wielka <w>panda</w> spędza większość dnia na jedzeniu bambusa.",
                            "target_lang_translations": {
                                "ru": "Большая <w>панда</w> проводит большую часть дня за поеданием бамбука."
                            }
                        }
                    ],
                    "synonyms": [
                        "panda wielka",
                        "niedźwiedź bambusowy"
                    ],
                    "common_phrases": [
                        "panda wielka",
                        "ochrona pandy",
                        "miś panda"
                    ],
                    "target_lang_definitions": {
                        "ru": "Хищное млекопитающее семейства медвежьих с характерной черно-белой окраской, питающееся в основном бамбуком."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "панда",
                                "target_lang_sense_clarification": "большая панда"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "303d69a2-c46b-3543-8270-243b04bdf9f5",
                    "sense_definition": "Model małego samochodu osobowego produkowany przez włoską markę Fiat.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "cars",
                    "name_type": "brand_or_product_name",
                    "examples": [
                        {
                            "text": "Mój sąsiad kupił używaną, czerwoną <w>pandę</w> do jazdy po mieście.",
                            "target_lang_translations": {
                                "ru": "Мой сосед купил подержанную красную <w>панду</w> для езды по городу."
                            }
                        },
                        {
                            "text": "Ta stara <w>panda</w> jest bardzo ekonomiczna i łatwo nią zaparkować.",
                            "target_lang_translations": {
                                "ru": "Эта старая <w>панда</w> очень экономична, и на ней легко парковаться."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fiat panda",
                        "nowa panda"
                    ],
                    "target_lang_definitions": {
                        "ru": "Модель малолитражного легкового автомобиля, выпускаемая итальянской маркой Fiat."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "панда",
                                "target_lang_sense_clarification": "автомобиль"
                            }

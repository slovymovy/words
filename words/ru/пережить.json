                            },
                            {
                                "target_lang_word": "to outlast",
                                "target_lang_sense_clarification": "to continue to exist or function longer than something else"
                            },
                            {
                                "target_lang_word": "to survive",
                                "target_lang_sense_clarification": "to live longer than a spouse or family member"
                            }
                        ]
                    }
                }

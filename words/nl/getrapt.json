                    "sense_definition": "Voltooid deelwoord van <w>trappen</w>.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij heeft de bal ver over het hek <w>getrapt</w>.",
                            "target_lang_translations": {
                                "en": "He <w>kicked</w> the ball far over the fence.",
                                "ru": "Он <w>пнул</w> мяч далеко за забор."
                            }
                        },
                        {
                            "text": "Ik ben per ongeluk in een stuk glas <w>getrapt</w>.",
                            "target_lang_translations": {
                                "en": "I accidentally <w>stepped</w> on a piece of glass.",
                                "ru": "Я случайно <w>наступил</w> на осколок стекла."
                            }
                        }
                    ],
                    "common_phrases": [

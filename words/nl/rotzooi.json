{
    "word_family": [
        "rotzooien"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "abea1745-99e5-3dd4-b585-82726c92bc87",
                    "sense_definition": "Een wanordelijke toestand of situatie waarin spullen niet op hun plek liggen.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het wordt tijd dat je je <w>rotzooi</w> op je kamer eens opruimt.",
                            "target_lang_translations": {
                                "en": "It's about time you cleaned up the <w>mess</w> in your room."
                            }
                        },
                        {
                            "text": "Ziet nou niemand van de gemeente dat het op straat een grote <w>rotzooi</w> is?",
                            "target_lang_translations": {
                                "en": "Does no one from the municipality see that the street is a big <w>mess</w>?"
                            }
                        }
                    ],
                    "synonyms": [
                        "rommel",
                        "troep",
                        "puinhoop",
                        "zooi"
                    ],
                    "antonyms": [
                        "orde",
                        "netheid"
                    ],
                    "common_phrases": [
                        "wat een rotzooi",
                        "er een rotzooi van maken"
                    ],
                    "target_lang_definitions": {
                        "en": "A disorderly state or situation where things are not in their place."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mess",
                                "target_lang_sense_clarification": "a dirty or untidy state"
                            },
                            {
                                "target_lang_word": "clutter",
                                "target_lang_sense_clarification": "a collection of things lying about in an untidy mass"
                            },
                            {
                                "target_lang_word": "disorder",
                                "target_lang_sense_clarification": "a state of confusion or lack of organization"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f07a8112-6fc9-3c16-8389-e82c5859cce5",
                    "sense_definition": "Waardeloze spullen, afval of vuilnis.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Die hele handel kan wel weggegooid worden nu het stinkende <w>rotzooi</w> geworden is.",
                            "target_lang_translations": {
                                "en": "That whole lot can be thrown away now that it has become smelly <w>trash</w>."
                            }
                        },
                        {
                            "text": "Gooi die oude <w>rotzooi</w> toch eens in de prullenbak, want je gebruikt het nooit meer.",
                            "target_lang_translations": {
                                "en": "Just throw that old <w>junk</w> in the trash can, because you never use it anymore."
                            }
                        }
                    ],
                    "synonyms": [
                        "rommel",
                        "troep",
                        "brol"
                    ],
                    "common_phrases": [
                        "oude rotzooi"
                    ],
                    "target_lang_definitions": {
                        "en": "Worthless items, waste, or garbage."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "junk",
                                "target_lang_sense_clarification": "old or discarded articles that are considered useless"
                            },
                            {
                                "target_lang_word": "trash",
                                "target_lang_sense_clarification": "waste material"
                            },
                            {
                                "target_lang_word": "rubbish",
                                "target_lang_sense_clarification": "waste material; refuse"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a67dc614-4ba5-3118-9c14-fe3b179ebae6",
                    "sense_definition": "Eerste persoon enkelvoud tegenwoordige tijd van <w>rotzooien</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>rotzooi</w> in het weekend graag wat aan met mijn oude brommer.",
                            "target_lang_translations": {
                                "en": "I like to <w>tinker</w> around a bit with my old moped on the weekend."
                            }
                        },
                        {
                            "text": "Als ik me verveel, <w>rotzooi</w> ik soms wat met computeronderdelen.",
                            "target_lang_translations": {
                                "en": "When I'm bored, I sometimes <w>mess around</w> with computer parts."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "verb form"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "First person singular present tense of <w>rotzooien</w>."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "mess around",
                                "target_lang_sense_clarification": "to spend time doing something without a clear purpose"
                            },
                            {
                                "target_lang_word": "tinker",
                                "target_lang_sense_clarification": "to attempt to repair or improve something in a casual way"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                    "sense_id": "b1fafec5-2c82-30d6-87a3-03f7807ad5bf",
                    "sense_definition": "Het uitoefenen van macht of druk om iemand tegen zijn of haar wil iets te laten doen.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verdachte beweerde dat hij handelde onder <w>dwang</w> en bedreiging.",
                            "target_lang_translations": {
                                "en": "The suspect claimed that he acted under <w>coercion</w> and threat.",
                                "ru": "Подозреваемый утверждал, что действовал под <w>принуждением</w> и угрозами."
                            }
                        },
                        {
                            "text": "Zonder enige vorm van <w>dwang</w> besloot hij mee te werken aan het onderzoek.",
                            "target_lang_translations": {
                                "en": "Without any form of <w>coercion</w>, he decided to cooperate with the investigation.",
                                "ru": "Без какой-либо формы <w>принуждения</w> он решил сотрудничать со следствием."
                            }
                        }
                    ],
                    "synonyms": [
                        "druk",
                        "coërcitie"
                    ],
                    "antonyms": [
                        "vrijwilligheid",
                        "vrije wil"
                    ],
                    "common_phrases": [
                        "onder dwang",
                        "met zachte dwang"
                    ],
                    "target_lang_definitions": {
                        "en": "The exercise of power or pressure to make someone do something against their will.",
                        "ru": "Применение силы или давления, чтобы заставить кого-то сделать что-то против его воли."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "coercion",
                                "target_lang_sense_clarification": "use of force or threats"
                            },
                            {
                                "target_lang_word": "compulsion",
                                "target_lang_sense_clarification": "act of compelling"
                            },
                            {
                                "target_lang_word": "force",
                                "target_lang_sense_clarification": "power exerted against will"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "принуждение",
                                "target_lang_sense_clarification": "действие против воли"
                            },
                            {
                                "target_lang_word": "давление",
                                "target_lang_sense_clarification": "сильное влияние"

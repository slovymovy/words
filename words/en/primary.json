                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "chiefly US"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": "political jargon"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Бросить вызов действующему политику за выдвижение от партии на предварительных выборах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бросить вызов на праймериз",

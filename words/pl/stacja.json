{
    "word_family": [
        "stacyjny",
        "stacyjka",
        "podstacja",
        "nadstacja"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0940a903-22b2-3112-b104-78ead8ed7c49",
                    "sense_definition": "Miejsce zatrzymywania się pociągów, gdzie pasażerowie mogą wsiadać i wysiadać, często wyposażone w budynek dworca i perony.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "transport_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pociąg do Warszawy odjeżdża z głównej <stacji> w mieście.",
                            "target_lang_translations": {
                                "ru": "Поезд в Варшаву отправляется с главной <w>станции</w> города."
                            }
                        },
                        {
                            "text": "Musimy wysiąść na następnej <stacji>, aby przesiąść się w autobus.",
                            "target_lang_translations": {
                                "ru": "Мы должны выйти на следующей <w>станции</w>, чтобы пересесть на автобус."
                            }
                        }
                    ],
                    "synonyms": [
                        "dworzec",
                        "przystanek",
                        "terminal"
                    ],
                    "common_phrases": [
                        "stacja kolejowa",
                        "stacja metra",
                        "na stacji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место остановки поездов, где пассажиры могут садиться и выходить, часто оборудованное зданием вокзала и платформами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "станция",
                                "target_lang_sense_clarification": "железнодорожная"
                            },
                            {
                                "target_lang_word": "вокзал",
                                "target_lang_sense_clarification": "здание станции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0c4a6f7b-d4ba-38d6-af1d-19af8803bdd3",
                    "sense_definition": "Punkt usługowy, w którym sprzedawane jest paliwo do samochodów oraz inne artykuły dla podróżnych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "transport_location",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Muszę zjechać na najbliższą <stację>, ponieważ kończy mi się benzyna.",
                            "target_lang_translations": {
                                "ru": "Мне нужно заехать на ближайшую <w>заправку</w>, так как у меня заканчивается бензин."
                            }
                        },
                        {
                            "text": "Na tej <stacji> benzynowej można kupić kawę i hot dogi.",
                            "target_lang_translations": {
                                "ru": "На этой автозаправочной <w>станции</w> можно купить кофе и хот-доги."
                            }
                        }
                    ],
                    "synonyms": [
                        "stacja benzynowa"
                    ],
                    "common_phrases": [
                        "stacja benzynowa",
                        "stacja paliw",
                        "tankować na stacji"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пункт обслуживания, где продается топливо для автомобилей и другие товары для путешественников."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заправка",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "автозаправочная станция",
                                "target_lang_sense_clarification": "официальное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ef08cd1-5145-31b0-8262-6c294df89c8b",
                    "sense_definition": "Instytucja lub przedsiębiorstwo zajmujące się nadawaniem programów radiowych lub telewizyjnych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "media_institution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja ulubiona <stacja> radiowa gra tylko muzykę rockową.",
                            "target_lang_translations": {
                                "ru": "Моя любимая <w>радиостанция</w> играет только рок-музыку."
                            }
                        },
                        {
                            "text": "Ta <stacja> telewizyjna wyemituje wieczorem ciekawy film dokumentalny.",
                            "target_lang_translations": {
                                "ru": "Этот теле<w>канал</w> покажет вечером интересный документальный фильм."
                            }
                        }
                    ],
                    "synonyms": [
                        "kanał",
                        "program"
                    ],
                    "common_phrases": [
                        "stacja telewizyjna",
                        "stacja radiowa",
                        "zmienić stację"
                    ],
                    "target_lang_definitions": {
                        "ru": "Учреждение или предприятие, занимающееся вещанием радио- или телепрограмм."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "станция",
                                "target_lang_sense_clarification": "радиостанция"
                            },
                            {
                                "target_lang_word": "канал",
                                "target_lang_sense_clarification": "телевизионный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ca3fbec-5fd8-3ae3-87c5-901ac83186bc",
                    "sense_definition": "Wyspecjalizowana placówka lub zakład zajmujący się określoną działalnością badawczą, techniczną lub ratunkową.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "specialized_institution",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Naukowcy pracujący na <stacji> polarnej badają zmiany klimatu.",
                            "target_lang_translations": {
                                "ru": "Ученые, работающие на полярной <w>станции</w>, изучают изменения климата."
                            }
                        },
                        {
                            "text": "Międzynarodowa <stacja> kosmiczna okrąża Ziemię wiele razy na dobę.",
                            "target_lang_translations": {
                                "ru": "Международная космическая <w>станция</w> облетает Землю много раз в сутки."
                            }
                        }
                    ],
                    "synonyms": [
                        "placówka"
                    ],
                    "common_phrases": [
                        "stacja kosmiczna",
                        "stacja badawcza",
                        "stacja pogotowia",
                        "stacja krwiodawstwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Специализированное учреждение или предприятие, занимающееся определенной исследовательской, технической или спасательной деятельностью."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "станция",
                                "target_lang_sense_clarification": "научная или техническая"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ac612701-4341-39e9-9d2e-1d88d2a53ac3",
                    "sense_definition": "Urządzenie komputerowe służące do odczytu i zapisu danych na nośnikach wymiennych lub wydzielone stanowisko komputerowe.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "technology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Stare komputery były wyposażone w <stację> dyskietek, której dziś się już nie używa.",
                            "target_lang_translations": {
                                "ru": "Старые компьютеры были оснащены <w>дисководом</w> для дискет, который сегодня уже не используется."
                            }
                        },
                        {
                            "text": "Grafik komputerowy potrzebuje bardzo wydajnej <stacji> roboczej do renderowania filmów.",
                            "target_lang_translations": {
                                "ru": "Компьютерному графику нужна очень мощная рабочая <w>станция</w> для рендеринга фильмов."
                            }
                        }
                    ],
                    "synonyms": [
                        "napęd"
                    ],
                    "common_phrases": [
                        "stacja dysków",
                        "stacja robocza"
                    ],
                    "target_lang_definitions": {
                        "ru": "Компьютерное устройство для чтения и записи данных на сменные носители или выделенное компьютерное рабочее место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дисковод",
                                "target_lang_sense_clarification": "устройство чтения"
                            },
                            {
                                "target_lang_word": "рабочая станция",
                                "target_lang_sense_clarification": "компьютер"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9a05bfd0-d6c2-3a69-bfea-d5dfa1c7a129",
                    "sense_definition": "Każdy z czternastu etapów nabożeństwa Drogi Krzyżowej, upamiętniających mękę Chrystusa, a także wizerunek lub krzyż oznaczający to miejsce.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "religion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Procesja zatrzymała się przy trzeciej <stacji>, aby wierni mogli odmówić modlitwę.",
                            "target_lang_translations": {
                                "ru": "Процессия остановилась у третьего <w>стояния</w>, чтобы верующие могли помолиться."
                            }
                        },
                        {
                            "text": "W kościele wiszą piękne obrazy przedstawiające kolejne <stacje> męki Pańskiej.",
                            "target_lang_translations": {
                                "ru": "В церкви висят красивые картины, изображающие последовательные <w>стояния</w> Страстей Господних."
                            }
                        }
                    ],
                    "common_phrases": [
                        "stacja drogi krzyżowej"
                    ],
                    "target_lang_definitions": {
                        "ru": "Каждый из четырнадцати этапов богослужения Крестного пути, напоминающих о страстях Христовых, а также изображение или крест, обозначающие это место."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стояние",
                                "target_lang_sense_clarification": "часть Крестного пути"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7aec058e-dd5e-39c4-a6dd-f6324a9b8d85",
                    "sense_definition": "Dawny obowiązek zapewnienia noclegu i wyżywienia władcy lub urzędnikom w czasie ich podróży przez kraj.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "history",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Szlachta często narzekała na uciążliwą <stację>, która wiązała się z dużymi kosztami goszczenia dworu.",
                            "target_lang_translations": {
                                "ru": "Шляхта часто жаловалась на обременительный <w>постой</w>, который был связан с большими расходами на прием двора."
                            }
                        },
                        {
                            "text": "Prawo <stacji> było jednym z obciążeń ludności w średniowiecznej Polsce.",
                            "target_lang_translations": {
                                "ru": "Право <w>постоя</w> было одной из повинностей населения в средневековой Польше."
                            }
                        }
                    ],
                    "synonyms": [
                        "stan",
                        "gościtwa"
                    ],
                    "common_phrases": [
                        "prawo stacji"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "dotyczy historycznego prawa i zwyczajów"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Старинная обязанность предоставлять ночлег и питание правителю или чиновникам во время их путешествия по стране."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "постой",
                                "target_lang_sense_clarification": "историческая повинность"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
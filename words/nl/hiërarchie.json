    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "dc14bc82-9a9b-3776-a73a-3751096b4952",
                    "sense_definition": "Een systeem of indeling waarbij mensen of dingen in volgorde van belangrijkheid, macht of niveau zijn gerangschikt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In dit moderne bedrijf is er een vlakke <w>hiërarchie</w> met weinig managers.",
                            "target_lang_translations": {
                                "en": "In this modern company, there is a flat <w>hierarchy</w> with few managers."
                            }
                        },
                        {
                            "text": "Een strikte <w>hiërarchie</w> zorgt ervoor dat iedereen precies weet wie de leiding heeft.",
                            "target_lang_translations": {
                                "en": "A strict <w>hierarchy</w> ensures that everyone knows exactly who is in charge."
                            }
                        }
                    ],
                    "synonyms": [
                        "rangorde",
                        "pikorde"
                    ],
                    "antonyms": [
                        "gelijkheid"
                    ],
                    "common_phrases": [
                        "strikte hiërarchie",
                        "sociale hiërarchie",
                        "in de hiërarchie"
                    ],
                    "target_lang_definitions": {
                        "en": "A system or arrangement in which people or things are ranked according to importance, power, or level."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hierarchy",
                                "target_lang_sense_clarification": "system of ranking"
                            },
                            {
                                "target_lang_word": "pecking order",
                                "target_lang_sense_clarification": "informal ranking"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "24a96662-cb83-3a91-84c3-4d36e5c9ecdd",
                    "sense_definition": "De officiële rangorde van gezagsdragers, met name binnen een kerkelijke of ambtelijke organisatie.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De paus staat helemaal bovenaan in de katholieke <w>hiërarchie</w>.",
                            "target_lang_translations": {
                                "en": "The pope is at the very top of the Catholic <w>hierarchy</w>."
                            }
                        },
                        {
                            "text": "Hij probeerde jarenlang op te klimmen in de ambtelijke <w>hiërarchie</w> van het ministerie.",
                            "target_lang_translations": {
                                "en": "He tried for years to climb the administrative <w>hierarchy</w> of the ministry."
                            }
                        }
                    ],
                    "synonyms": [
                        "rangstelsel"
                    ],
                    "common_phrases": [
                        "kerkelijke hiërarchie",
                        "ambtelijke hiërarchie"
                    ],
                    "target_lang_definitions": {
                        "en": "The official ranking of authority figures, particularly within a church or administrative organization."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "hierarchy",
                                "target_lang_sense_clarification": "body of officials"
                            }
                        ]
                    }
                }
            ]
        }
    ]

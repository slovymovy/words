                {
                    "sense_id": "4898df84-a22d-3180-ac23-09fde0bd656a",
                    "sense_definition": "A green, leafy plant used to add flavor or seasoning to food.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "plants_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I grow fresh <w>herbs</w> like basil and mint in my kitchen garden.",
                            "target_lang_translations": {
                                "ru": "Я выращиваю на огороде свежую <w>зелень</w>, такую как базилик и мята."
                            }
                        },
                        {
                            "text": "You should add the dried <w>herbs</w> to the sauce early so the flavors can blend.",
                            "target_lang_translations": {
                                "ru": "Сушеные <w>травы</w> следует добавлять в соус пораньше, чтобы ароматы смешались."
                            }
                        }
                    ],
                    "common_phrases": [
                        "fresh herbs",
                        "dried herbs",
                        "herb garden",
                        "mixed herbs"
                    ],
                    "target_lang_definitions": {

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "70d7cbc4-b1b9-38c2-8d3e-2a22e99b4fbe",
                    "sense_definition": "Устройство или машина, которая выполняет работу, продает товары или предоставляет услуги без участия человека.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "machine_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я подошёл к <автомату>, чтобы купить стаканчик горячего кофе.",
                            "target_lang_translations": {
                                "en": "I went up to the <vending machine> to buy a cup of hot coffee."
                            }
                        },
                        {
                            "text": "В торговом центре установили новый игровой <автомат> для детей.",
                            "target_lang_translations": {
                                "en": "A new <arcade machine> for children was installed in the shopping mall."
                            }
                        }
                    ],
                    "common_phrases": [
                        "торговый автомат",
                        "игровой автомат",
                        "автомат с газировкой",
                        "банковский автомат"
                    ],
                    "target_lang_definitions": {
                        "en": "A machine that dispenses goods or provides services without human intervention."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "vending machine",
                                "target_lang_sense_clarification": "dispenses food or drink"
                            },
                            {
                                "target_lang_word": "slot machine",
                                "target_lang_sense_clarification": "gambling"
                            },
                            {
                                "target_lang_word": "arcade machine",
                                "target_lang_sense_clarification": "gaming"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e1540ad3-b413-30b9-8c3e-ded0d6ce00d9",
                    "sense_definition": "Ручное огнестрельное оружие, способное стрелять очередями (обычно имеется в виду автомат Калашникова).",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "weapon",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Солдат быстро почистил свой <автомат> и приготовился к выходу на пост.",
                            "target_lang_translations": {
                                "en": "The soldier quickly cleaned his <assault rifle> and prepared to go on duty."
                            }
                        },
                        {
                            "text": "На столе лежал разобранный <автомат> Калашникова, который нужно было собрать на время.",
                            "target_lang_translations": {
                                "en": "A disassembled Kalashnikov <assault rifle> lay on the table, which had to be reassembled against the clock."
                            }
                        }
                    ],
                    "synonyms": [
                        "штурмовая винтовка"
                    ],
                    "common_phrases": [
                        "автомат Калашникова",
                        "стрельба из автомата",
                        "магазин автомата",
                        "очередь из автомата"
                    ],
                    "target_lang_definitions": {
                        "en": "A handheld firearm capable of automatic fire, such as a Kalashnikov."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "assault rifle",
                                "target_lang_sense_clarification": "military weapon"
                            },
                            {
                                "target_lang_word": "automatic rifle",
                                "target_lang_sense_clarification": "general term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "55c3aea0-3bfb-337a-b21a-d622a6081c6b",
                    "sense_definition": "Автоматическая коробка переключения передач в автомобиле.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "car_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Учиться водить машину на <автомате> гораздо проще, чем на механике.",
                            "target_lang_translations": {
                                "en": "Learning to drive a car on an <automatic> is much easier than on a manual."
                            }
                        },
                        {
                            "text": "При покупке нового автомобиля мы выбирали только модели, где есть <автомат>.",
                            "target_lang_translations": {
                                "en": "When buying a new car, we only chose models that have an <automatic transmission>."
                            }
                        }
                    ],
                    "antonyms": [
                        "механика"
                    ],
                    "common_phrases": [
                        "коробка автомат",
                        "ездить на автомате",
                        "машина с автоматом"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "An automatic transmission in a vehicle."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "automatic transmission",
                                "target_lang_sense_clarification": "technical term"
                            },
                            {
                                "target_lang_word": "automatic",
                                "target_lang_sense_clarification": "colloquial short form"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e3cd78a-f0f5-3112-bbb4-5b221dc6353b",
                    "sense_definition": "Уличный телефон общего пользования, работающий от монет или специальных карт.",
                    "learner_level": "B1",
                    "frequency": "Low",
                    "semantic_group_id": "machine_device",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Раньше на каждом углу висел телефонный <автомат>, а теперь у всех есть мобильные.",
                            "target_lang_translations": {
                                "en": "There used to be a <payphone> on every corner, but now everyone has a mobile."
                            }
                        },
                        {
                            "text": "У меня не было мелочи, чтобы позвонить родителям из уличного <автомата>.",
                            "target_lang_translations": {
                                "en": "I didn't have any change to call my parents from the street <payphone>."
                            }
                        }
                    ],
                    "synonyms": [
                        "таксофон"
                    ],
                    "common_phrases": [
                        "телефонный автомат",
                        "звонить из автомата",
                        "будка автомата"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "устройства практически вышли из употребления"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A public telephone operated by coins or cards."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "payphone",
                                "target_lang_sense_clarification": "common term"
                            },
                            {
                                "target_lang_word": "public phone",
                                "target_lang_sense_clarification": "general term"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "43737fda-4365-30b3-b3c1-ed424475cd6c",
                    "sense_definition": "Оценка за экзамен или зачёт в вузе, полученная без сдачи самого экзамена, по результатам работы в семестре.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "education",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Студент так хорошо работал весь семестр, что преподаватель поставил ему <автомат>.",
                            "target_lang_translations": {
                                "en": "The student worked so well all semester that the teacher gave him an <automatic pass>."
                            }
                        },
                        {
                            "text": "Если не пропускать лекции, есть шанс получить <автомат> по этому сложному предмету.",
                            "target_lang_translations": {
                                "en": "If you don't skip lectures, there is a chance to get an <automatic pass> in this difficult subject."
                            }
                        }
                    ],
                    "common_phrases": [
                        "получить автомат",
                        "сдать автоматом",
                        "поставить автомат"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "студенческий жаргон"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A passing grade received without taking the final exam based on term performance."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "automatic pass",
                                "target_lang_sense_clarification": "academic context"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "553a24c9-1371-31af-bbe6-61c004926980",
                    "sense_definition": "Бессознательное, механическое выполнение действий по привычке (обычно в выражении «на автомате»).",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "state_of_mind",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я так устал на работе, что доехал до дома просто на <автомате>.",
                            "target_lang_translations": {
                                "en": "I was so tired at work that I drove home purely on <autopilot>."
                            }
                        },
                        {
                            "text": "Утром я на <автомате> насыпал кофе в чашку, забыв вскипятить воду.",
                            "target_lang_translations": {
                                "en": "In the morning, I poured coffee into the cup on <autopilot>, forgetting to boil the water."
                            }
                        }
                    ],
                    "common_phrases": [
                        "делать на автомате",
                        "ответить на автомате",
                        "действовать на автомате"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Unconscious or mechanical behavior, usually done out of habit."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "autopilot",
                                "target_lang_sense_clarification": "state of mind"
                            },
                            {
                                "target_lang_word": "automaton",
                                "target_lang_sense_clarification": "person acting mechanically"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b041c5a-970c-30d6-a8c4-ddfe3028de7e",
                    "sense_definition": "Прибор для автоматического размыкания электрической цепи при перегрузке или коротком замыкании.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "electronics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за включения мощного обогревателя в квартире внезапно выбило <автомат>.",
                            "target_lang_translations": {
                                "en": "Due to turning on a powerful heater, the <circuit breaker> in the apartment suddenly tripped."
                            }
                        },
                        {
                            "text": "Электрик посоветовал заменить старый <автомат> в щитке на более надежный.",
                            "target_lang_translations": {
                                "en": "The electrician advised replacing the old <circuit breaker> in the panel with a more reliable one."
                            }
                        }
                    ],
                    "common_phrases": [
                        "выбило автомат",
                        "электрический автомат",
                        "включить автомат"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "профессиональный жаргон электриков"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A device that automatically interrupts an electric circuit to prevent overload."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "circuit breaker",
                                "target_lang_sense_clarification": "electrical device"
                            },
                            {
                                "target_lang_word": "breaker",
                                "target_lang_sense_clarification": "short form"
                            }
                        ]
                    }
                }
            ]
        }
    ]

{
    "word_family": [
        "bespieden",
        "spieder"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4ed52795-c795-3278-a707-7dcd18c27948",
                    "sense_definition": "Scherp, aandachtig en onderzoekend kijken, vaak in de verte, om iets te ontdekken of waar te nemen.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_observing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kapitein <w>spiedde</w> over de woeste zee op zoek naar land.",
                            "target_lang_translations": {
                                "ru": "Капитан <w>всматривался</w> в бушующее море в поисках земли."
                            }
                        },
                        {
                            "text": "Hij <w>spiedde</w> in het rond om te zien of zijn vrienden al aankwamen.",
                            "target_lang_translations": {
                                "ru": "Он <w>осматривался</w> по сторонам, чтобы увидеть, не идут ли его друзья."
                            }
                        }
                    ],
                    "synonyms": [
                        "turen",
                        "uitkijken",
                        "observeren"
                    ],
                    "common_phrases": [
                        "in het rond spieden",
                        "in de verte spieden",
                        "naar gevaar spieden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пристально, внимательно и пытливо смотреть, часто вдаль, чтобы что-то обнаружить или заметить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "всматриваться",
                                "target_lang_sense_clarification": "смотреть пристально, стараясь увидеть что-то"
                            },
                            {
                                "target_lang_word": "вглядываться",
                                "target_lang_sense_clarification": "напрягать зрение, чтобы рассмотреть"
                            },
                            {
                                "target_lang_word": "высматривать",
                                "target_lang_sense_clarification": "искать кого-либо или что-либо глазами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb24353f-0dc3-3743-a496-d9d167c09e9c",
                    "sense_definition": "Heimelijk of stiekem kijken om onopgemerkt informatie te verkrijgen of iemand in de gaten te houden.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "group_spying",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De inlichtingendienst mag onder strikte voorwaarden in computers <w>spieden</w>.",
                            "target_lang_translations": {
                                "ru": "Разведслужбе разрешено при строгих условиях <w>заглядывать</w> в компьютеры."
                            }
                        },
                        {
                            "text": "Nieuwsgierige buren <w>spieden</w> soms van achter hun gordijnen naar de straat.",
                            "target_lang_translations": {
                                "ru": "Любопытные соседи иногда <w>подглядывают</w> из-за штор на улицу."
                            }
                        }
                    ],
                    "synonyms": [
                        "gluren",
                        "bespioneren",
                        "loeren",
                        "spioneren"
                    ],
                    "common_phrases": [
                        "stiekem spieden",
                        "naar binnen spieden"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тайно или украдкой смотреть, чтобы незаметно получить информацию или проследить за кем-то."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подглядывать",
                                "target_lang_sense_clarification": "смотреть тайком за кем-то или чем-то"
                            },
                            {
                                "target_lang_word": "подсматривать",
                                "target_lang_sense_clarification": "наблюдать скрытно"
                            },
                            {
                                "target_lang_word": "шпионить",
                                "target_lang_sense_clarification": "следить с целью сбора сведений"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
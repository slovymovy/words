            "pos": "noun",
            "senses": [
                {
                    "sense_id": "eb913524-4e52-371d-8305-057df62a9e98",
                    "sense_definition": "Mały przedmiot, taki jak zawieszka, żeton lub karteczka z wydrukowanym numerem, służący do identyfikacji w szatni, kolejce lub loterii.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "identification_tag",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz pobrać <w>numerek</w> z automatu i poczekać na swoją kolej do okienka.",
                            "target_lang_translations": {
                                "ru": "Нужно взять <w>номерок</w> в автомате и подождать своей очереди к окошку."
                            }
                        },
                        {
                            "text": "Zgubiłem mój <w>numerek</w> do szatni, więc nie mogę odebrać płaszcza.",
                            "target_lang_translations": {
                                "ru": "Я потерял свой <w>номерок</w> от гардероба, поэтому не могу забрать пальто."
                            }
                        }
                    ],
                    "synonyms": [
                        "zawieszka",
                        "żeton"
                    ],
                    "common_phrases": [
                        "numerek do szatni",
                        "pobrać numerek",
                        "zgubić numerek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой предмет (бирка, жетон или талон) с номером, используемый для идентификации в гардеробе, очереди или лотерее."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "номерок",
                                "target_lang_sense_clarification": "в гардеробе или очереди"
                            },
                            {
                                "target_lang_word": "жетон",
                                "target_lang_sense_clarification": "пластиковый или металлический"
                            },
                            {
                                "target_lang_word": "талон",
                                "target_lang_sense_clarification": "бумажный, в очереди"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7969ec01-fa94-3ca7-8fb9-945abef773e0",
                    "sense_definition": "Zdrobnienie od słowa <w>numer</w>; często oznacza małą liczbę lub numer w kontekście gier i zabaw.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "identification_tag",
                    "name_type": "no",
                    "examples": [

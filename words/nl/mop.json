                },
                {
                    "sense_id": "0c7d5f18-b080-3e15-9f68-da8df1841811",
                    "sense_definition": "Een liefkozende benaming voor een meisje of vrouw.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "endearment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kom eens hier, <w>mop</w>, dan krijg je een dikke knuffel.",

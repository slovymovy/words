{
    "word_family": [
        "suppose",
        "supposition",
        "supposable",
        "supposed"
    ],
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "527c1d97-4f7b-3d43-abb7-f6ed315055d6",
                    "sense_definition": "According to what is generally assumed or believed, although not certain or proven.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The ancient castle is <w>supposedly</w> haunted by the spirits of former kings.",
                            "target_lang_translations": {
                                "ru": "В этом древнем замке <w>якобы</w> обитают духи бывших королей."
                            }
                        },
                        {
                            "text": "He is <w>supposedly</w> working on a new book, but nobody has seen any drafts yet.",
                            "target_lang_translations": {
                                "ru": "Он <w>предположительно</w> работает над новой книгой, но черновиков ещё никто не видел."
                            }
                        }
                    ],
                    "synonyms": [
                        "allegedly",
                        "reportedly",
                        "purportedly",
                        "ostensibly"
                    ],
                    "antonyms": [
                        "actually",
                        "certainly",
                        "definitely",
                        "really"
                    ],
                    "common_phrases": [
                        "supposedly true",
                        "supposedly innocent",
                        "supposedly impossible"
                    ],
                    "target_lang_definitions": {
                        "ru": "Как принято считать или как утверждают, хотя это не известно наверняка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "якобы",
                                "target_lang_sense_clarification": "указывает на то, что информация может быть недостоверной"
                            },
                            {
                                "target_lang_word": "предположительно",
                                "target_lang_sense_clarification": "основываясь на догадках или вероятности"
                            },
                            {
                                "target_lang_word": "по общему мнению",
                                "target_lang_sense_clarification": "как считают многие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "092900df-2b26-306a-89ab-48377e0aea89",
                    "sense_definition": "Used informally to mean \"supposed to be\".",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I am <w>supposedly</w> cleaning my room right now, but I am reading instead.",
                            "target_lang_translations": {
                                "ru": "Я сейчас <w>по идее</w> убираю свою комнату, но вместо этого читаю."
                            }
                        },
                        {
                            "text": "They are <w>supposedly</w> quiet in the library, but they keep talking.",
                            "target_lang_translations": {
                                "ru": "Они <w>вроде бы</w> сидят тихо в библиотеке, но продолжают разговаривать."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "often considered incorrect or a mispronunciation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Используется в разговорной речи для обозначения того, что должно происходить или ожидается, но часто не соответствует действительности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "по идее",
                                "target_lang_sense_clarification": "как должно быть в теории"
                            },
                            {
                                "target_lang_word": "вроде бы",
                                "target_lang_sense_clarification": "выражает неуверенность или видимость действия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
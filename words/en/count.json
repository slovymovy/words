{
    "word_family": [
        "counter",
        "countable",
        "uncountable",
        "recount",
        "discount",
        "account",
        "accountant",
        "countdown"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "0e29d9a4-7885-38a6-87a4-986128df679b",
                    "sense_definition": "To say numbers one after another in the correct order.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "calculation_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My young daughter can <w>count</w> to ten.",
                            "target_lang_translations": {
                                "ru": "Моя маленькая дочь умеет <w>считать</w> до десяти."
                            }
                        },
                        {
                            "text": "The teacher asked the students to <w>count</w> out loud together.",
                            "target_lang_translations": {
                                "ru": "Учитель попросил учеников <w>считать</w> вслух вместе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "count to ten",
                        "count out loud",
                        "count down"
                    ],
                    "target_lang_definitions": {
                        "ru": "Называть числа по порядку одно за другим."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "считать",
                                "target_lang_sense_clarification": "называть числа"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "6e8a5890-a292-362f-8522-3b687a766447",
                    "sense_definition": "To calculate the total number of things or people in a group.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "calculation_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please <w>count</w> the money to make sure it is all there.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, <w>посчитайте</w> деньги, чтобы убедиться, что всё на месте."
                            }
                        },
                        {
                            "text": "I need to <w>count</w> how many chairs we have for the party.",
                            "target_lang_translations": {
                                "ru": "Мне нужно <w>посчитать</w>, сколько у нас стульев для вечеринки."
                            }
                        }
                    ],
                    "synonyms": [
                        "tally",
                        "enumerate"
                    ],
                    "antonyms": [
                        "miscount"
                    ],
                    "common_phrases": [
                        "count the cost",
                        "count them up"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вычислять общее количество вещей или людей в группе."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "считать",
                                "target_lang_sense_clarification": "определять количество"
                            },
                            {
                                "target_lang_word": "посчитать",
                                "target_lang_sense_clarification": "совершенный вид"
                            },
                            {
                                "target_lang_word": "сосчитать",
                                "target_lang_sense_clarification": "подвести итог"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f012e78-3a17-3cc6-b72f-271040f6a20a",
                    "sense_definition": "To be important or to have influence.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "importance_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In an election, every single vote <w>counts</w>.",
                            "target_lang_translations": {
                                "ru": "На выборах каждый голос <w>считается</w>."
                            }
                        },
                        {
                            "text": "It does not <w>count</w> if you break the rules.",
                            "target_lang_translations": {
                                "ru": "Это не <w>считается</w>, если ты нарушаешь правила."
                            }
                        }
                    ],
                    "common_phrases": [
                        "that counts",
                        "what counts is",
                        "every vote counts"
                    ],
                    "target_lang_definitions": {
                        "ru": "Быть важным или иметь влияние."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "считаться",
                                "target_lang_sense_clarification": "иметь значение"
                            },
                            {
                                "target_lang_word": "иметь значение",
                                "target_lang_sense_clarification": "быть важным"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b43fd4f-10c5-329f-817b-e98657ade022",
                    "sense_definition": "To include someone or something in a calculation or group.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "calculation_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There are five people in my family, not <w>counting</w> the dog.",
                            "target_lang_translations": {
                                "ru": "В моей семье пять человек, не <w>считая</w> собаки."
                            }
                        },
                        {
                            "text": "If you are going to the cinema, please <w>count</w> me in.",
                            "target_lang_translations": {
                                "ru": "Если вы идете в кино, пожалуйста, <w>считайте</w> и меня."
                            }
                        }
                    ],
                    "common_phrases": [
                        "count me in",
                        "not counting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Включать кого-то или что-то в расчёт или группу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "считать",
                                "target_lang_sense_clarification": "учитывать"
                            },
                            {
                                "target_lang_word": "включать",
                                "target_lang_sense_clarification": "присоединять к группе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1950f5a9-0ffa-3f52-a85a-2056f5e0e301",
                    "sense_definition": "To consider or regard someone or something in a specific way.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "importance_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should <w>count</w> yourself lucky that you were not hurt.",
                            "target_lang_translations": {
                                "ru": "Тебе стоит <w>считать</w>, что тебе повезло, что ты не пострадал."
                            }
                        },
                        {
                            "text": "I <w>count</w> him among my very best friends.",
                            "target_lang_translations": {
                                "ru": "Я <w>считаю</w> его одним из своих лучших друзей."
                            }
                        }
                    ],
                    "common_phrases": [
                        "count yourself lucky",
                        "count as one"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рассматривать или воспринимать кого-то или что-то определенным образом."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "считать",
                                "target_lang_sense_clarification": "иметь мнение"
                            },
                            {
                                "target_lang_word": "счесть",
                                "target_lang_sense_clarification": "расценить"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2a581e8e-cbb3-34bc-b0bc-ccf4121a46b4",
                    "sense_definition": "The act of calculating a total number, or the result obtained from this act.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "calculation_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I lost <w>count</w> of how many times he apologized.",
                            "target_lang_translations": {
                                "ru": "Я потерял <w>счёт</w> тому, сколько раз он извинялся."
                            }
                        },
                        {
                            "text": "The final <w>count</w> showed that our team won the election.",
                            "target_lang_translations": {
                                "ru": "Окончательный <w>подсчёт</w> показал, что наша команда выиграла выборы."
                            }
                        }
                    ],
                    "synonyms": [
                        "tally"
                    ],
                    "common_phrases": [
                        "lose count",
                        "keep count",
                        "final count"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по вычислению общего числа или результат этого действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "счёт",
                                "target_lang_sense_clarification": "действие"
                            },
                            {
                                "target_lang_word": "подсчёт",
                                "target_lang_sense_clarification": "результат вычисления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48dd0ffe-945a-35cb-b515-2a0db26288ad",
                    "sense_definition": "A specific charge or accusation in a legal case.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "legal_1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The jury found the defendant guilty on two <w>counts</w> of theft.",
                            "target_lang_translations": {
                                "ru": "Присяжные признали подсудимого виновным по двум <w>пунктам обвинения</w> в краже."
                            }
                        },
                        {
                            "text": "He faces multiple <w>counts</w> of fraud in the upcoming trial.",
                            "target_lang_translations": {
                                "ru": "Ему грозит несколько <w>пунктов обвинения</w> в мошенничестве на предстоящем суде."
                            }
                        }
                    ],
                    "common_phrases": [
                        "counts of murder",
                        "guilty on all counts"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "legal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретное обвинение в судебном деле."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пункт обвинения",
                                "target_lang_sense_clarification": "юридический термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21ad6da5-0b83-3de4-8d4d-b695499779b4",
                    "sense_definition": "A European nobleman with a rank similar to a British earl.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "title_1",
                    "name_type": "title_or_honorific_name",
                    "examples": [
                        {
                            "text": "The <w>Count</w> lived in a large castle on top of the hill.",
                            "target_lang_translations": {
                                "ru": "<w>Граф</w> жил в большом замке на вершине холма."
                            }
                        },
                        {
                            "text": "She was introduced to a wealthy Italian <w>count</w> at the ball.",
                            "target_lang_translations": {
                                "ru": "На балу её представили богатому итальянскому <w>графу</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "the Count"
                    ],
                    "target_lang_definitions": {
                        "ru": "Европейский дворянин с титулом, соответствующим английскому графу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "граф",
                                "target_lang_sense_clarification": "титул"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
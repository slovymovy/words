    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "bdf6b70c-efe6-327b-9791-0228aa3ab842",
                    "sense_definition": "Able to be put into something else.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The memory card is easily <w>insertable</w> into the slot on the side of the camera.",
                            "target_lang_translations": {
                                "ru": "Карта памяти легко <w>вставляется</w> в слот на боковой стороне камеры."
                            }
                        },
                        {
                            "text": "Doctors recommended an <w>insertable</w> cardiac monitor to track the patient's heart rhythm.",
                            "target_lang_translations": {
                                "ru": "Врачи рекомендовали <w>имплантируемый</w> кардиомонитор для отслеживания сердечного ритма пациента."
                            }
                        }
                    ],
                    "antonyms": [
                        "uninsertable"
                    ],
                    "common_phrases": [
                        "easily insertable",
                        "insertable device",
                        "insertable lens"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способный быть вставленным или помещенным внутрь чего-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вставляемый",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "имплантируемый",
                                "target_lang_sense_clarification": "медицина"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "d9d7c661-4dc7-3708-bfcb-dd0d6bd5e109",
                    "sense_definition": "Something that is designed to be put inside something else, often a medical device.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hospital stocks various medical <w>insertables</w> for patient care.",
                            "target_lang_translations": {
                                "ru": "В больнице хранятся различные медицинские <w>имплантаты</w> для ухода за пациентами."
                            }
                        },
                        {
                            "text": "These <w>insertables</w> are made from safe materials to prevent allergic reactions.",
                            "target_lang_translations": {
                                "ru": "Эти <w>вставки</w> изготовлены из безопасных материалов, чтобы предотвратить аллергические реакции."
                            }
                        }
                    ],
                    "common_phrases": [
                        "medical insertables"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет, предназначенный для размещения внутри чего-либо, часто медицинское устройство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вставка",
                                "target_lang_sense_clarification": "общий смысл"
                            },
                            {
                                "target_lang_word": "имплантат",
                                "target_lang_sense_clarification": "медицина"
                            }
                        ]
                    }
                }
            ]

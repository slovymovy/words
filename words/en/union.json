{
    "word_family": [
        "unionize",
        "unionization",
        "reunion",
        "disunion",
        "unionist",
        "unite",
        "unity",
        "unification"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "5eac5065-32cc-3bd8-b631-3a4730fc136a",
                    "sense_definition": "An organization of workers formed to protect their collective rights and interests, such as wages and working conditions.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The teachers voted to form a <w>union</w> to negotiate better salaries.",
                            "target_lang_translations": {
                                "ru": "Учителя проголосовали за создание <w>профсоюза</w> для переговоров о повышении зарплаты."
                            }
                        },
                        {
                            "text": "She has been an active member of the trade <w>union</w> for ten years.",
                            "target_lang_translations": {
                                "ru": "Она была активным членом <w>профсоюза</w> в течение десяти лет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "trade union",
                        "labor union",
                        "join the union",
                        "union member"
                    ],
                    "target_lang_definitions": {
                        "ru": "Организация работников, созданная для защиты их коллективных прав и интересов, таких как заработная плата и условия труда."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "профсоюз",
                                "target_lang_sense_clarification": "labor union"
                            },
                            {
                                "target_lang_word": "союз",
                                "target_lang_sense_clarification": "organization"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b765a8db-437e-3228-93f2-606b003877fb",
                    "sense_definition": "A political unit consisting of a number of states or organizations with the same central government.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The European <w>Union</w> allows for free movement of goods between member countries.",
                            "target_lang_translations": {
                                "ru": "Европейский <w>союз</w> разрешает свободное перемещение товаров между странами-участницами."
                            }
                        },
                        {
                            "text": "Several nations decided to join the <w>union</w> to strengthen their economy.",
                            "target_lang_translations": {
                                "ru": "Несколько стран решили вступить в <w>союз</w>, чтобы укрепить свою экономику."
                            }
                        }
                    ],
                    "synonyms": [
                        "coalition"
                    ],
                    "common_phrases": [
                        "European Union",
                        "Soviet Union",
                        "state of the union"
                    ],
                    "target_lang_definitions": {
                        "ru": "Политическое образование, состоящее из ряда государств или организаций с единым центральным правительством."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "союз",
                                "target_lang_sense_clarification": "political bloc"
                            },
                            {
                                "target_lang_word": "объединение",
                                "target_lang_sense_clarification": "unification of states"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d3a5adc-a562-3af9-9d2a-8223b1832617",
                    "sense_definition": "The act of joining two or more things into one, or the state of being joined together.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>union</w> of the two companies created a powerful new leader in the market.",
                            "target_lang_translations": {
                                "ru": "<w>Слияние</w> двух компаний создало нового мощного лидера на рынке."
                            }
                        },
                        {
                            "text": "They worked in perfect <w>union</w> to complete the difficult task.",
                            "target_lang_translations": {
                                "ru": "Они работали в полном <w>единении</w>, чтобы выполнить сложную задачу."
                            }
                        }
                    ],
                    "synonyms": [
                        "combination",
                        "junction"
                    ],
                    "antonyms": [
                        "disunion"
                    ],
                    "common_phrases": [
                        "in union",
                        "perfect union",
                        "strength in union"
                    ],
                    "target_lang_definitions": {
                        "ru": "Действие по соединению двух или более вещей в одно целое или состояние соединенности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объединение",
                                "target_lang_sense_clarification": "act of joining"
                            },
                            {
                                "target_lang_word": "слияние",
                                "target_lang_sense_clarification": "merger"
                            },
                            {
                                "target_lang_word": "союз",
                                "target_lang_sense_clarification": "state of unity"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e76184c5-cc7a-37ab-801a-ef94c7067033",
                    "sense_definition": "The legal or spiritual joining of two people in marriage.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Their <w>union</w> was celebrated by friends and family at a beautiful ceremony.",
                            "target_lang_translations": {
                                "ru": "Их <w>союз</w> отпраздновали друзья и семья на красивой церемонии."
                            }
                        },
                        {
                            "text": "A civil <w>union</w> provides many of the same legal protections as a traditional marriage.",
                            "target_lang_translations": {
                                "ru": "Гражданский <w>союз</w> предоставляет многие из тех же правовых гарантий, что и традиционный брак."
                            }
                        }
                    ],
                    "common_phrases": [
                        "civil union",
                        "blessed union"
                    ],
                    "target_lang_definitions": {
                        "ru": "Юридическое или духовное соединение двух людей в браке."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "брак",
                                "target_lang_sense_clarification": "marriage"
                            },
                            {
                                "target_lang_word": "союз",
                                "target_lang_sense_clarification": "union of people"
                            },
                            {
                                "target_lang_word": "брачный союз",
                                "target_lang_sense_clarification": "matrimonial union"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8b7921c4-d31d-37b8-abe8-8ebb5c0d98d8",
                    "sense_definition": "A set containing all of the elements that are in any of the sets being combined.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>union</w> of set A and set B includes every number found in either group.",
                            "target_lang_translations": {
                                "ru": "<w>Объединение</w> множества A и множества B включает каждое число, найденное в любой из групп."
                            }
                        },
                        {
                            "text": "In this diagram, the shaded area represents the <w>union</w> of the two circles.",
                            "target_lang_translations": {
                                "ru": "На этой диаграмме заштрихованная область представляет собой <w>объединение</w> двух кругов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "union and intersection"
                    ],
                    "target_lang_definitions": {
                        "ru": "Множество, содержащее все элементы, которые находятся в любом из объединяемых множеств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объединение",
                                "target_lang_sense_clarification": "set theory"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "57277671-178c-322a-8ed0-737d987d4473",
                    "sense_definition": "Belonging to, represented by, or relating to a labor union.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The construction site hires only <w>union</w> workers for the project.",
                            "target_lang_translations": {
                                "ru": "На строительную площадку для этого проекта нанимают только <w>профсоюзных</w> рабочих."
                            }
                        },
                        {
                            "text": "She works as a <w>union</w> representative for the factory employees.",
                            "target_lang_translations": {
                                "ru": "Она работает <w>профсоюзным</w> представителем работников фабрики."
                            }
                        }
                    ],
                    "synonyms": [
                        "unionized"
                    ],
                    "antonyms": [
                        "nonunion"
                    ],
                    "common_phrases": [
                        "union member",
                        "union job",
                        "union official"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принадлежащий, представленный или относящийся к профсоюзу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "профсоюзный",
                                "target_lang_sense_clarification": "adjective"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "bd79fb65-5a7b-3133-bed5-d15cf9e77ef3",
                    "sense_definition": "The United States of America, specifically the northern states during the American Civil War.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "4",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "President Lincoln was determined to preserve the <w>Union</w> at all costs.",
                            "target_lang_translations": {
                                "ru": "Президент Линкольн был полон решимости сохранить <w>Союз</w> любой ценой."
                            }
                        },
                        {
                            "text": "The <w>Union</w> army marched south to engage the Confederate forces.",
                            "target_lang_translations": {
                                "ru": "Армия <w>Союза</w> двинулась на юг, чтобы вступить в бой с силами Конфедерации."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединенные Штаты Америки, особенно северные штаты во время Гражданской войны в США."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Союз",
                                "target_lang_sense_clarification": "USA historical"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ebb3f9cd-0c0c-312e-87fb-02f7bca47d9e",
                    "sense_definition": "Short for the Soviet Union.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "5",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "He traveled extensively throughout the <w>Union</w> before the political changes occurred.",
                            "target_lang_translations": {
                                "ru": "Он много путешествовал по <w>Союзу</w> до того, как произошли политические изменения."
                            }
                        },
                        {
                            "text": "The policies of the <w>Union</w> were controlled centrally from Moscow.",
                            "target_lang_translations": {
                                "ru": "Политика <w>Союза</w> контролировалась централизованно из Москвы."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сокращенное название Советского Союза."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Союз",
                                "target_lang_sense_clarification": "USSR"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "361e6ad4-6265-3371-bc47-ec5c621f0f93",
                    "sense_definition": "To combine sets using the union operation in mathematics or computing.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "3",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The algorithm <w>unions</w> the two data lists into a single collection.",
                            "target_lang_translations": {
                                "ru": "Алгоритм <w>объединяет</w> два списка данных в единую коллекцию."
                            }
                        },
                        {
                            "text": "We need to <w>union</w> these geometries to create the final shape.",
                            "target_lang_translations": {
                                "ru": "Нам нужно <w>объединить</w> эти геометрические фигуры, чтобы создать окончательную форму."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Объединять множества, используя операцию объединения в математике или информатике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объединять",
                                "target_lang_sense_clarification": "verb"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
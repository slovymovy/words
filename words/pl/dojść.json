        },
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "18f27e59-1cdd-3ee4-92ca-c4830cf7622b",
                    "sense_definition": "Alternatywna, potoczna forma słowa <w>dość</w>.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "dialectal_variant",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mam już <dojść> tego czekania!",
                            "target_lang_translations": {
                                "ru": "С меня уже <хватит> этого ожидания!"
                            }
                        },
                        {
                            "text": "Było tego <dojść> dużo.",
                            "target_lang_translations": {
                                "ru": "Этого было <довольно> много."
                            }
                        }
                    ],
                    "synonyms": [
                        "dość",
                        "wystarczająco"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        },
                        {
                            "trait_type": "dialectal",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Альтернативная, разговорная форма слова dość."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хватит",
                                "target_lang_sense_clarification": "достаточно"
                            },
                            {
                                "target_lang_word": "довольно",
                                "target_lang_sense_clarification": "достаточно"
                            }
                        ]
                    }
                }
            ]
        }
    ]

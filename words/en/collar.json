{
    "word_family": [
        "collarless"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b89be5e6-480f-3ab9-919d-6e7c9ffcefe0",
                    "sense_definition": "The part of a shirt, jacket, or coat that fits around the neck.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "neckwear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He buttoned his <w>collar</w> and straightened his tie.",
                            "target_lang_translations": {
                                "ru": "Он застегнул <w>воротник</w> и поправил галстук."
                            }
                        },
                        {
                            "text": "It was a hot day, so I loosened my <w>collar</w>.",
                            "target_lang_translations": {
                                "ru": "Был жаркий день, поэтому я ослабил <w>воротник</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "shirt collar",
                        "stiff collar",
                        "open collar",
                        "pop the collar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Часть рубашки, куртки или пальто, которая облегает шею."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "воротник",
                                "target_lang_sense_clarification": "деталь одежды"
                            },
                            {
                                "target_lang_word": "ворот",
                                "target_lang_sense_clarification": "вырез или часть одежды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "259c5549-c906-30b5-a3b2-e94a47f64e3f",
                    "sense_definition": "A band, often made of leather or nylon, worn around an animal's neck to restrain or identify it.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "neckwear",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Make sure your dog has a <w>collar</w> with an ID tag.",
                            "target_lang_translations": {
                                "ru": "Убедитесь, что на вашей собаке есть <w>ошейник</w> с биркой."
                            }
                        },
                        {
                            "text": "She bought a red <w>collar</w> for her new kitten.",
                            "target_lang_translations": {
                                "ru": "Она купила красный <w>ошейник</w> для своего нового котенка."
                            }
                        }
                    ],
                    "common_phrases": [
                        "dog collar",
                        "flea collar",
                        "shock collar"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ремешок, часто из кожи или нейлона, надеваемый на шею животного."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ошейник",
                                "target_lang_sense_clarification": "для животных"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "67227ead-b5ab-33ad-864d-d1762b10e238",
                    "sense_definition": "A ring or band used to connect, secure, or restrict parts in machinery or structures.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "mechanical_ring",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The plumber tightened the metal <w>collar</w> around the leaking pipe.",
                            "target_lang_translations": {
                                "ru": "Сантехник затянул металлический <w>хомут</w> вокруг протекающей трубы."
                            }
                        },
                        {
                            "text": "A nylon <w>collar</w> prevented the bolt from scratching the surface.",
                            "target_lang_translations": {
                                "ru": "Нейлоновая <w>втулка</w> предотвращала царапание поверхности болтом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "shaft collar",
                        "locking collar"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "technical"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Кольцо или обод, используемые для соединения, фиксации или ограничения подвижности деталей в механизмах."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хомут",
                                "target_lang_sense_clarification": "крепежная скоба"
                            },
                            {
                                "target_lang_word": "втулка",
                                "target_lang_sense_clarification": "деталь машины"
                            },
                            {
                                "target_lang_word": "кольцо",
                                "target_lang_sense_clarification": "техническое"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "93eef31f-3f14-301d-b430-ea49bf881f4e",
                    "sense_definition": "An arrest of a criminal suspect.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "arrest_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The detective made a <w>collar</w> shortly after the robbery.",
                            "target_lang_translations": {
                                "ru": "Детектив произвел <w>арест</w> вскоре после ограбления."
                            }
                        },
                        {
                            "text": "It was an easy <w>collar</w> for the experienced officer.",
                            "target_lang_translations": {
                                "ru": "Это было легкое <w>задержание</w> для опытного офицера."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Арест или задержание подозреваемого в преступлении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "арест",
                                "target_lang_sense_clarification": "взятие под стражу"
                            },
                            {
                                "target_lang_word": "задержание",
                                "target_lang_sense_clarification": "полицейская процедура"
                            },
                            {
                                "target_lang_word": "поимка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "da76c4fd-7427-3a62-8bd8-d19ed1726e8e",
                    "sense_definition": "To put a collar on a person or animal.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "neckwear_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You must <w>collar</w> and leash your dog in the park.",
                            "target_lang_translations": {
                                "ru": "Вы должны <w>надеть ошейник</w> и поводок на свою собаку в парке."
                            }
                        },
                        {
                            "text": "The farmer <w>collared</w> the horse before leading it to the stable.",
                            "target_lang_translations": {
                                "ru": "Фермер <w>надел хомут</w> на лошадь, прежде чем вести её в конюшню."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Надевать ошейник или хомут на животное или человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надевать ошейник",
                                "target_lang_sense_clarification": "на животное"
                            },
                            {
                                "target_lang_word": "надевать хомут",
                                "target_lang_sense_clarification": "на лошадь"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c0a3f1ca-58a2-3a38-b768-7ecd91409834",
                    "sense_definition": "To catch, seize, or arrest someone.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "arrest_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police <w>collared</w> the thief as he tried to escape.",
                            "target_lang_translations": {
                                "ru": "Полиция <w>схватила</w> вора, когда он пытался сбежать."
                            }
                        },
                        {
                            "text": "He was <w>collared</w> for shoplifting at the mall.",
                            "target_lang_translations": {
                                "ru": "Его <w>задержали</w> за кражу в торговом центре."
                            }
                        }
                    ],
                    "synonyms": [
                        "halter"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поймать, схватить или арестовать кого-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "схватить",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "арестовать",
                                "target_lang_sense_clarification": "официальное"
                            },
                            {
                                "target_lang_word": "задержать",
                                "target_lang_sense_clarification": "официальное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "49e49d0e-2d5a-33ad-8e22-088d31579f1a",
                    "sense_definition": "To stop someone and force them to listen or talk, often when they are trying to leave.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "conversation_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "My boss <w>collared</w> me in the hallway to discuss the project.",
                            "target_lang_translations": {
                                "ru": "Мой начальник <w>поймал</w> меня в коридоре, чтобы обсудить проект."
                            }
                        },
                        {
                            "text": "I tried to leave the party, but he <w>collared</w> me for an hour.",
                            "target_lang_translations": {
                                "ru": "Я пытался уйти с вечеринки, но он <w>задержал</w> меня на целый час."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Остановить кого-то и заставить слушать или разговаривать, часто против воли."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "поймать",
                                "target_lang_sense_clarification": "переносное значение"
                            },
                            {
                                "target_lang_word": "задержать",
                                "target_lang_sense_clarification": "остановить для разговора"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "4416f7f8-8c29-3a49-a1bb-28eaa8e50534",
                    "sense_definition": "A surname.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "person_name",
                    "examples": [
                        {
                            "text": "Mr. <w>Collars</w> is listed in the directory.",
                            "target_lang_translations": {
                                "ru": "Мистер <w>Колларс</w> указан в справочнике."
                            }
                        },
                        {
                            "text": "I met a family named <w>Collars</w> on my trip.",
                            "target_lang_translations": {
                                "ru": "Я встретил семью по фамилии <w>Колларс</w> во время поездки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "surname",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Фамилия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Коллар",
                                "target_lang_sense_clarification": "фамилия"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
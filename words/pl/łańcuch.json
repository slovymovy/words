                    "sense_id": "95d416e0-dde9-3b70-8b0c-9ef3e9565436",
                    "sense_definition": "Szereg połączonych ze sobą, zwykle metalowych ogniw, służący do wiązania, ciągnięcia, napędu lub ozdoby",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój rower ma stary i zardzewiały <w>łańcuch</w>, który muszę wymienić.",
                            "target_lang_translations": {
                                "ru": "У моего велосипеда старая и ржавая <w>цепь</w>, которую я должен заменить."
                            }
                        },
                        {
                            "text": "Pies zerwał się z <w>łańcucha</w> i uciekł na ulicę.",
                            "target_lang_translations": {
                                "ru": "Собака сорвалась с <w>цепи</w> и убежала на улицу."
                            }
                        }
                    ],
                    "synonyms": [
                        "reg. śl. lańcuch"
                    ],
                    "common_phrases": [
                        "łańcuch rowerowy",
                        "pies na łańcuchu",
                        "zerwać łańcuch",
                        "złoty łańcuch"
                    ],
                    "traits": [

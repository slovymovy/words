                    "frequency": "Low",
                    "semantic_group_id": "beer_type",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kelner, poproszę jednego zimnego <w>fulla</w> do stolika.",
                            "target_lang_translations": {
                                "ru": "Официант, пожалуйста, одно холодное <w>светлое</w> за столик."
                            }
                        },
                        {
                            "text": "Po pracy lubię wypić <w>fulla</w> w lokalnym barze.",
                            "target_lang_translations": {
                                "ru": "После работы я люблю выпить <w>светлого</w> в местном баре."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zimny full",
                        "jeden full"

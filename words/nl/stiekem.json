    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "689f778d-26c1-37db-adbf-7af9808b6594",
                    "sense_definition": "Zonder dat iemand het ziet of merkt; in het geheim.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "secretly_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De kinderen aten <w>stiekem</w> snoepjes vlak voor het avondeten.",
                            "target_lang_translations": {
                                "en": "The children were <w>secretly</w> eating sweets just before dinner.",
                                "ru": "Дети <w>тайком</w> ели конфеты прямо перед ужином."
                            }
                        },
                        {
                            "text": "Hij is gisteravond <w>stiekem</w> het huis uit geslopen om naar het feest te gaan.",
                            "target_lang_translations": {
                                "en": "He <w>secretly</w> snuck out of the house last night to go to the party.",
                                "ru": "Вчера вечером он <w>украдкой</w> выскользнул из дома, чтобы пойти на вечеринку."
                            }
                        }
                    ],
                    "synonyms": [
                        "heimelijk"
                    ],
                    "common_phrases": [
                        "stiekem weglopen",
                        "stiekem roken",
                        "iets stiekem doen"
                    ],
                    "target_lang_definitions": {
                        "en": "Without anyone seeing or noticing; done in secret.",
                        "ru": "Без ведома других; тайно, скрытно."
                    },
                    "translations": {

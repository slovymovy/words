    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "00115eff-7a83-31a2-809f-7e16765dfa8e",
                    "sense_definition": "Taki, który nie słyszy lub słyszy bardzo słabo z powodu uszkodzenia narządu słuchu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_disability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mój dziadek jest już prawie całkowicie <w>głuchy</w> i musi nosić aparat słuchowy.",
                            "target_lang_translations": {
                                "ru": "Мой дедушка уже почти полностью <w>глухой</w> и должен носить слуховой аппарат."
                            }
                        },
                        {
                            "text": "Ten pies jest <w>głuchy</w> od urodzenia, więc reaguje tylko na gesty ręką.",
                            "target_lang_translations": {
                                "ru": "Эта собака <w>глухая</w> с рождения, поэтому реагирует только на жесты рукой."
                            }
                        }
                    ],
                    "synonyms": [
                        "niesłyszący"
                    ],
                    "antonyms": [
                        "słyszący"
                    ],
                    "common_phrases": [
                        "głuchy jak pień",
                        "głuchy na jedno ucho",
                        "aparat dla głuchych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто не слышит или слышит очень плохо из-за повреждения органа слуха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глухой",
                                "target_lang_sense_clarification": "о слухе"
                            },
                            {
                                "target_lang_word": "неслышащий",
                                "target_lang_sense_clarification": "формальный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3ff8b454-555e-3ccf-b30f-05ec397025d7",
                    "sense_definition": "O dźwięku: cichy, stłumiony, niewyraźny i pozbawiony dźwięczności.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "sound_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z piwnicy dobiegał nas jakiś dziwny, <w>głuchy</w> odgłos uderzania o ścianę.",
                            "target_lang_translations": {
                                "ru": "Из подвала до нас доносился какой-то странный, <w>глухой</w> звук ударов о стену."
                            }
                        },
                        {
                            "text": "Usłyszeliśmy <w>głuchy</w> łoskot, gdy ciężkie pudło spadło na dywan.",
                            "target_lang_translations": {
                                "ru": "Мы услышали <w>глухой</w> грохот, когда тяжёлая коробка упала на ковёр."
                            }
                        }
                    ],
                    "synonyms": [
                        "bezdźwięczny",
                        "ogłuchły"
                    ],
                    "antonyms": [
                        "donośny",
                        "dźwięczny"
                    ],
                    "common_phrases": [
                        "głuchy odgłos",
                        "głuchy łoskot",
                        "głuchy stukot"
                    ],
                    "target_lang_definitions": {
                        "ru": "О звуке: тихий, приглушённый, невнятный и лишённый звонкости."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глухой",
                                "target_lang_sense_clarification": "о звуке"
                            },
                            {
                                "target_lang_word": "приглушённый",
                                "target_lang_sense_clarification": "сдавленный, негромкий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8f191ae6-3256-3cb6-867d-24aca9c2e419",
                    "sense_definition": "Taki, który celowo nie reaguje na czyjeś słowa, prośby, argumenty lub skargi.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dyrektor pozostał <w>głuchy</w> na prośby pracowników o poprawę warunków pracy.",
                            "target_lang_translations": {
                                "ru": "Директор остался <w>глух</w> к просьбам сотрудников об улучшении условий труда."
                            }
                        },
                        {
                            "text": "Nie bądź <w>głuchy</w> na argumenty innych ludzi i spróbuj zrozumieć ich punkt widzenia.",
                            "target_lang_translations": {
                                "ru": "Не будь <w>глух</w> к аргументам других людей и попытайся понять их точку зрения."
                            }
                        }
                    ],
                    "common_phrases": [
                        "głuchy na prośby",
                        "głuchy na argumenty",
                        "pozostać głuchym"
                    ],
                    "target_lang_definitions": {
                        "ru": "Тот, кто намеренно не реагирует на чьи-либо слова, просьбы, аргументы или жалобы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глухой",
                                "target_lang_sense_clarification": "перен. равнодушный"
                            },
                            {
                                "target_lang_word": "неотзывчивый",
                                "target_lang_sense_clarification": "не реагирующий на просьбы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79508425-3a9f-386d-8818-df55315e500f",
                    "sense_definition": "O miejscu, czasie lub ciszy: pusty, bezludny, całkowity lub bardzo odległy.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "emptiness_isolation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W całym domu panowała <w>głucha</w> cisza, która zaczynała mnie powoli przerażać.",
                            "target_lang_translations": {
                                "ru": "Во всём доме царила <w>глухая</w> тишина, которая начинала меня понемногу пугать."
                            }
                        },
                        {
                            "text": "Wyjechali na wakacje w jakieś <w>głuche</w> miejsce, gdzie nie było nawet zasięgu telefonu.",
                            "target_lang_translations": {
                                "ru": "Они уехали в отпуск в какое-то <w>глухое</w> место, где не было даже телефонной связи."
                            }
                        }
                    ],
                    "common_phrases": [
                        "głucha cisza",
                        "głucha noc",
                        "głucha prowincja",
                        "głuchy telefon"
                    ],
                    "target_lang_definitions": {
                        "ru": "О месте, времени или тишине: пустой, безлюдный, полный или очень отдалённый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глухой",
                                "target_lang_sense_clarification": "о месте или тишине"
                            },
                            {
                                "target_lang_word": "захолустный",
                                "target_lang_sense_clarification": "об отдаленном месте"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "4d59a0b8-5a39-33a9-ad3c-9ef427fb4496",
                    "sense_definition": "Osoba, która nie posiada zmysłu słuchu.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tym urzędzie każdy <w>głuchy</w> może skorzystać z pomocy tłumacza języka migowego.",
                            "target_lang_translations": {
                                "ru": "В этом учреждении каждый <w>глухой</w> может воспользоваться помощью переводчика жестового языка."
                            }
                        },
                        {
                            "text": "Szkoła dla <w>głuchych</w> organizuje w przyszłym tygodniu dzień otwarty dla nowych uczniów.",
                            "target_lang_translations": {
                                "ru": "Школа для <w>глухих</w> организует на следующей неделе день открытых дверей для новых учеников."
                            }
                        }
                    ],
                    "synonyms": [
                        "niesłyszący"
                    ],
                    "common_phrases": [
                        "język głuchych",
                        "szkoła dla głuchych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, который не обладает чувством слуха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глухой",
                                "target_lang_sense_clarification": "существительное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85615519-1b13-3fdd-bf4c-8cdfc9b15cd1",
                    "sense_definition": "Osoba niesłysząca lub słabosłysząca, która utożsamia się z kulturą i społecznością Głuchych.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person_cultural",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Jako dumny <w>Głuchy</w>, aktywnie walczy o prawa mniejszości językowych w swoim kraju.",
                            "target_lang_translations": {
                                "ru": "Как гордый <w>Глухой</w>, он активно борется за права языковых меньшинств в своей стране."
                            }
                        },
                        {
                            "text": "Kultura <w>Głuchych</w> posiada własne tradycje, historię oraz unikalny język wizualno-przestrzenny.",
                            "target_lang_translations": {
                                "ru": "Культура <w>Глухих</w> имеет собственные традиции, историю и уникальный визуально-пространственный язык."
                            }
                        }
                    ],
                    "common_phrases": [
                        "kultura Głuchych",
                        "społeczność Głuchych"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неслышащий или слабослышащий человек, который отождествляет себя с культурой и сообществом Глухих."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Глухой",
                                "target_lang_sense_clarification": "член сообщества"
                            }
                        ]
                    }
                }
            ]
        },

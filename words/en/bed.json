    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ca43decb-719a-3a8e-8d21-82e38dfeaf48",
                    "sense_definition": "A piece of furniture, usually flat and soft, used for sleeping or resting.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "furniture_sleep",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I was so tired that I fell asleep as soon as I touched the <w>bed</w>.",
                            "target_lang_translations": {
                                "ru": "Я так устал, что заснул, как только коснулся <w>кровати</w>."
                            }
                        },
                        {
                            "text": "She sat on the edge of the <w>bed</w> to put on her shoes.",
                            "target_lang_translations": {
                                "ru": "Она села на край <w>кровати</w>, чтобы надеть туфли."
                            }
                        }
                    ],
                    "synonyms": [
                        "berth",
                        "cot",
                        "bunk"
                    ],
                    "common_phrases": [
                        "single bed",
                        "double bed",
                        "king-size bed",
                        "make the bed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет мебели, обычно плоский и мягкий, используемый для сна или отдыха."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крова́ть",
                                "target_lang_sense_clarification": "предмет мебели"
                            },
                            {
                                "target_lang_word": "посте́ль",
                                "target_lang_sense_clarification": "место для сна с принадлежностями"
                            },
                            {
                                "target_lang_word": "ко́йка",
                                "target_lang_sense_clarification": "казенная или больничная кровать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "13983609-0dab-330f-bc0d-36758570ee29",
                    "sense_definition": "The place or time for sleeping; the act of going to sleep.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "furniture_sleep",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It is late and time for you to go to <w>bed</w>.",
                            "target_lang_translations": {
                                "ru": "Уже поздно, и тебе пора идти в <w>постель</w>."
                            }
                        },
                        {
                            "text": "He stayed in <w>bed</w> all day because he had a fever.",
                            "target_lang_translations": {
                                "ru": "Он пролежал в <w>постели</w> весь день, потому что у него была температура."
                            }
                        }
                    ],
                    "synonyms": [
                        "sleep",
                        "rest"
                    ],
                    "common_phrases": [
                        "go to bed",
                        "get out of bed",
                        "time for bed",
                        "breakfast in bed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место или время для сна; процесс отхода ко сну."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посте́ль",
                                "target_lang_sense_clarification": "место для сна"
                            },
                            {
                                "target_lang_word": "спать",
                                "target_lang_sense_clarification": "в значении 'ложиться спать'"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15a109c6-6db2-30e5-873f-48fa626b8eb2",
                    "sense_definition": "A plot of ground in a garden where plants or flowers are grown.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "ground_layer",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We planted bright red tulips in the flower <w>bed</w>.",
                            "target_lang_translations": {
                                "ru": "Мы посадили ярко-красные тюльпаны на цветочной <w>клумбе</w>."
                            }
                        },
                        {
                            "text": "The gardener removed all the weeds from the rose <w>bed</w>.",
                            "target_lang_translations": {
                                "ru": "Садовник удалил все сорняки с <w>клумбы</w> роз."
                            }
                        }
                    ],
                    "synonyms": [
                        "plot",
                        "patch"
                    ],
                    "common_phrases": [
                        "flower bed",
                        "rose bed",
                        "raised bed"
                    ],
                    "target_lang_definitions": {
                        "ru": "Участок земли в саду, где выращиваются растения или цветы."

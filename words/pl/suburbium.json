        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c6748c63-599c-33a3-85bc-b5d0ee0c608c",
                    "sense_definition": "Obszar położony na peryferiach miasta lub dużego ośrodka miejskiego, pełniący funkcje mieszkalne lub usługowe.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "suburb_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Historycy odkryli, że to rzymskie <w>suburbium</w> tętniło życiem handlowym.",
                            "target_lang_translations": {
                                "ru": "Историки обнаружили, что это римское <w>предместье</w> кипело торговой жизнью."
                            }
                        },
                        {
                            "text": "Nowoczesne <w>suburbium</w> zostało zaprojektowane z myślą o ekologii i dostępie do zieleni.",
                            "target_lang_translations": {
                                "ru": "Современный <w>пригород</w> был спроектирован с учетом экологии и доступа к зелени."
                            }
                        }
                    ],
                    "synonyms": [
                        "przedmieście",
                        "peryferie"
                    ],
                    "common_phrases": [
                        "rozwój suburbium",
                        "starożytne suburbium"
                    ],
                    "target_lang_definitions": {
                        "ru": "Территория, расположенная на окраине города или крупного городского центра, выполняющая жилые или служебные функции."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "предместье",
                                "target_lang_sense_clarification": "исторический или литературный контекст"
                            },
                            {
                                "target_lang_word": "пригород",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2d454144-7732-3eaf-91f4-2b56df1307f6",
                    "sense_definition": "Rozległy obszar na obrzeżach wielkiego miasta, zwłaszcza w stylu amerykańskim, zabudowany podobnymi do siebie domami jednorodzinnymi.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "suburb_concept",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Typowe amerykańskie <w>suburbia</w> ciągną się kilometrami i wymagają posiadania samochodu.",
                            "target_lang_translations": {
                                "ru": "Типичная американская <w>субурбия</w> тянется на километры и требует наличия автомобиля."
                            }
                        },
                        {
                            "text": "Wielu ludzi marzy o przeprowadzce na spokojne <w>suburbia</w> z dala od hałasu centrum.",
                            "target_lang_translations": {
                                "ru": "Многие люди мечтают о переезде в тихий <w>пригород</w> вдали от шума центра."
                            }
                        }
                    ],
                    "synonyms": [
                        "przedmieście"
                    ],
                    "common_phrases": [
                        "amerykańskie suburbia",
                        "bogate suburbia",
                        "życie na suburbiach"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обширная территория на окраине большого города, застроенная однотипными частными домами (особенно в американском стиле)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "субурбия",
                                "target_lang_sense_clarification": "социологический термин, американские пригороды"
                            },
                            {
                                "target_lang_word": "пригород",
                                "target_lang_sense_clarification": "общий термин"
                            }
                        ]
                    }
                }
            ]
        }

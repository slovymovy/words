    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "64027b22-8251-3a0d-982c-2ee0db6217eb",
                    "sense_definition": "An organ through which animals see (perceive surroundings via light).",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "vision_organ",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She closed her <w>eyes</w> and tried to fall asleep.",
                            "target_lang_translations": {
                                "ru": "Она закрыла <w>глаза</w> и попыталась уснуть."
                            }
                        },
                        {
                            "text": "Bright lights can really hurt your <w>eyes</w> if you stare at them.",
                            "target_lang_translations": {
                                "ru": "Яркий свет может вызвать боль в <w>глазах</w>, если пристально смотреть на него."
                            }
                        }
                    ],
                    "synonyms": [
                        "eyeball",
                        "optic",
                        "peeper"
                    ],
                    "common_phrases": [
                        "keep an eye on",
                        "turn a blind eye",
                        "naked eye",
                        "bird's-eye view"
                    ],
                    "target_lang_definitions": {
                        "ru": "Орган зрения, с помощью которого животные видят (воспринимают окружающее через свет)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глаз",
                                "target_lang_sense_clarification": "орган зрения"
                            },
                            {
                                "target_lang_word": "око",
                                "target_lang_sense_clarification": "поэтическое или устаревшее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96bc11be-d077-3f96-89e9-57645226de47",
                    "sense_definition": "The visual sense; the faculty of sight.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "vision_sense",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The garden was very pleasing to the <w>eye</w> with its colorful flowers.",
                            "target_lang_translations": {
                                "ru": "Сад был очень приятен <w>глазу</w> своими красочными цветами."
                            }
                        },
                        {
                            "text": "He lost the use of one <w>eye</w> after the accident.",
                            "target_lang_translations": {
                                "ru": "Он ослеп на один <w>глаз</w> после аварии."
                            }
                        }
                    ],
                    "common_phrases": [
                        "feast one's eyes",
                        "believe one's eyes"
                    ],
                    "target_lang_definitions": {
                        "ru": "Зрительное чувство; способность видеть."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зрение",
                                "target_lang_sense_clarification": "способность видеть"
                            },
                            {
                                "target_lang_word": "глаз",
                                "target_lang_sense_clarification": "метафорически о способности видеть"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5508248-1b74-3eeb-be46-81fbf0771aa2",
                    "sense_definition": "Attention or notice.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "attention",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bright red poster caught my <w>eye</w> as I walked past.",
                            "target_lang_translations": {
                                "ru": "Ярко-красный плакат бросился мне в <w>глаза</w>, когда я проходил мимо."
                            }
                        },
                        {
                            "text": "She tried to catch the waiter's <w>eye</w> to ask for the bill.",
                            "target_lang_translations": {
                                "ru": "Она пыталась поймать <w>взгляд</w> официанта, чтобы попросить счет."
                            }
                        }
                    ],
                    "common_phrases": [
                        "catch someone's eye"
                    ],
                    "target_lang_definitions": {
                        "ru": "Внимание или интерес."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взгляд",
                                "target_lang_sense_clarification": "направленность внимания"
                            },
                            {
                                "target_lang_word": "внимание",
                                "target_lang_sense_clarification": "интерес"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "751c8861-4f7b-327c-80fd-05d7bc9274ae",
                    "sense_definition": "Opinion or view.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "opinion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In the <w>eyes</w> of the law, everyone is considered innocent until proven guilty.",
                            "target_lang_translations": {
                                "ru": "В <w>глазах</w> закона каждый считается невиновным, пока его вина не доказана."
                            }
                        },
                        {
                            "text": "This achievement made him a hero in the <w>eyes</w> of the public.",
                            "target_lang_translations": {
                                "ru": "Это достижение сделало его героем в <w>глазах</w> общественности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "in the eyes of"
                    ],
                    "target_lang_definitions": {
                        "ru": "Мнение или точка зрения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глаза",
                                "target_lang_sense_clarification": "мнение, обычно во мн. числе"
                            },
                            {
                                "target_lang_word": "мнение",
                                "target_lang_sense_clarification": "суждение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "85a057ed-729f-3c4e-af5b-4958f3069b9a",
                    "sense_definition": "The ability to notice what others might miss; judgment or appreciation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "judgment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The manager has an <w>eye</w> for talent and hires excellent staff.",
                            "target_lang_translations": {
                                "ru": "У менеджера <w>чутьё</w> на таланты, и он нанимает отличных сотрудников."
                            }
                        },
                        {
                            "text": "As an artist, she has a good <w>eye</w> for color and composition.",
                            "target_lang_translations": {
                                "ru": "Как художник, она имеет верный <w>глаз</w> на цвет и композицию."
                            }
                        }
                    ],
                    "synonyms": [
                        "perceptiveness"
                    ],
                    "common_phrases": [
                        "have an eye for",
                        "with an eye to"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность замечать то, что могут упустить другие; проницательность или вкус."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чутьё",
                                "target_lang_sense_clarification": "интуиция, способность разбираться"
                            },
                            {
                                "target_lang_word": "глаз",
                                "target_lang_sense_clarification": "умение видеть, наметанный глаз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5c7e584a-bafe-3076-b22f-c64797045eda",
                    "sense_definition": "A meaningful look or stare.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "look",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She gave him the <w>eye</w> across the crowded room.",
                            "target_lang_translations": {
                                "ru": "Она строила ему <w>глазки</w> через переполненную комнату."
                            }
                        },
                        {
                            "text": "The teacher cast a strict <w>eye</w> over the noisy classroom.",
                            "target_lang_translations": {
                                "ru": "Учитель обвел строгим <w>взглядом</w> шумный класс."
                            }
                        }
                    ],
                    "common_phrases": [
                        "give someone the eye"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выразительный или пристальный взгляд."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "взгляд",
                                "target_lang_sense_clarification": "выражение глаз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5946aa11-ed93-3155-8f28-077220c7a062",
                    "sense_definition": "A hole at the blunt end of a needle through which thread is passed.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hole_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "It can be very difficult to thread the <w>eye</w> of a small needle.",
                            "target_lang_translations": {
                                "ru": "Бывает очень трудно продеть нитку в <w>ушко</w> маленькой иголки."
                            }
                        },
                        {
                            "text": "She squinted as she tried to push the cotton through the <w>eye</w>.",
                            "target_lang_translations": {
                                "ru": "Она щурилась, пытаясь протолкнуть хлопок через <w>ушко</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "eye of a needle"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отверстие на тупом конце иглы, через которое продевается нить."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ушко",
                                "target_lang_sense_clarification": "отверстие в игле"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "96c2ad58-0103-3293-ab08-b6dabd2ce7ba",
                    "sense_definition": "The relatively calm and clear centre of a hurricane or other cyclonic storm.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "storm_center",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The plane flew directly into the <w>eye</w> of the hurricane to gather data.",
                            "target_lang_translations": {
                                "ru": "Самолет влетел прямо в <w>глаз</w> урагана для сбора данных."
                            }
                        },
                        {
                            "text": "Winds dropped suddenly as the <w>eye</w> of the storm passed over the island.",
                            "target_lang_translations": {
                                "ru": "Ветер внезапно стих, когда <w>глаз</w> бури прошел над островом."
                            }
                        }
                    ],
                    "common_phrases": [
                        "eye of the storm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относительно спокойный и ясный центр урагана или циклона."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глаз",
                                "target_lang_sense_clarification": "центр бури"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c9afb06d-78b3-3918-bf1d-53f26af29f65",
                    "sense_definition": "The iris of the eye, being of a specified colour.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "vision_organ",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has beautiful green <w>eyes</w> that match her dress.",
                            "target_lang_translations": {
                                "ru": "У нее красивые зеленые <w>глаза</w> под цвет платья."
                            }
                        },
                        {
                            "text": "The baby was born with bright blue <w>eyes</w>.",
                            "target_lang_translations": {
                                "ru": "Ребенок родился с ярко-голубыми <w>глазами</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Радужная оболочка глаза определенного цвета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глаза",
                                "target_lang_sense_clarification": "цвет глаз"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c7b4f69c-2813-3314-ad74-dfbccdc25757",
                    "sense_definition": "A private detective.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The family hired a private <w>eye</w> to investigate the disappearance.",
                            "target_lang_translations": {
                                "ru": "Семья наняла частного <w>детектива</w>, чтобы расследовать исчезновение."
                            }
                        },
                        {
                            "text": "In the movie, the <w>eye</w> followed the suspect through the dark streets.",
                            "target_lang_translations": {
                                "ru": "В фильме <w>сыщик</w> преследовал подозреваемого по темным улицам."
                            }
                        }
                    ],
                    "common_phrases": [
                        "private eye"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "short for private eye"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Частный детектив."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сыщик",
                                "target_lang_sense_clarification": "разговорное"
                            },
                            {
                                "target_lang_word": "частный детектив",
                                "target_lang_sense_clarification": "профессия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "310a2234-d3f3-3b44-8ccd-666ca40a5a0b",
                    "sense_definition": "A reproductive bud in a potato.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "botany",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should cut out the <w>eyes</w> of the potato before cooking it.",
                            "target_lang_translations": {
                                "ru": "Перед готовкой следует вырезать <w>глазки</w> у картофеля."
                            }
                        },
                        {
                            "text": "If you leave potatoes too long, each <w>eye</w> will start to sprout.",
                            "target_lang_translations": {
                                "ru": "Если оставить картофель слишком надолго, каждый <w>глазок</w> начнет прорастать."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Почка на клубне картофеля."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глазок",
                                "target_lang_sense_clarification": "почка растения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27f7a90d-6b60-3159-9489-f56abcc54ce1",
                    "sense_definition": "A burner on a kitchen stove.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "appliance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Put the kettle on the front <w>eye</w> of the stove.",
                            "target_lang_translations": {
                                "ru": "Поставь чайник на переднюю <w>конфорку</w> плиты."
                            }
                        },
                        {
                            "text": "Make sure the <w>eye</w> is turned off after you finish cooking.",
                            "target_lang_translations": {
                                "ru": "Убедись, что <w>конфорка</w> выключена, когда закончишь готовить."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "common in US"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Конфорка на кухонной плите."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конфорка",
                                "target_lang_sense_clarification": "нагревательный элемент"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "abb7434a-6a71-3ea8-86d4-3eb482ee292c",
                    "sense_definition": "A loop forming part of anything, or a hole through anything, to receive a hook, pin, rope, or shaft.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "hole_object",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pass the rope through the <w>eye</w> of the anchor.",
                            "target_lang_translations": {
                                "ru": "Продень веревку через <w>ушко</w> якоря."
                            }
                        },
                        {
                            "text": "The dress fastens with a small hook and <w>eye</w> at the back.",
                            "target_lang_translations": {
                                "ru": "Платье застегивается на маленький крючок и <w>петельку</w> сзади."
                            }
                        }
                    ],
                    "common_phrases": [
                        "hook and eye"
                    ],
                    "target_lang_definitions": {
                        "ru": "Петля или отверстие в предмете для крючка, штифта или веревки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "петелька",
                                "target_lang_sense_clarification": "для застежки"
                            },
                            {
                                "target_lang_word": "ушко",
                                "target_lang_sense_clarification": "отверстие для крепления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f5dfd588-64f5-38f1-a4de-afa27a420274",
                    "sense_definition": "A mark on an animal, such as a butterfly or peacock, resembling a human eye.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "pattern",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The peacock spread its tail to reveal the beautiful <w>eyes</w> on its feathers.",
                            "target_lang_translations": {
                                "ru": "Павлин распустил хвост, чтобы показать красивые <w>глазки</w> на перьях."
                            }
                        },
                        {
                            "text": "Some butterflies have large <w>eyes</w> on their wings to scare away predators.",
                            "target_lang_translations": {
                                "ru": "У некоторых бабочек на крыльях есть большие <w>глазки</w>, чтобы отпугивать хищников."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Пятно на животном (например, на бабочке или павлине), напоминающее человеческий глаз."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глазок",
                                "target_lang_sense_clarification": "узор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a5a458a-e6ab-3fb2-b586-83b4d1edc2d1",
                    "sense_definition": "One of the holes in certain kinds of cheese.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "food_feature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Swiss cheese is famous for having large <w>eyes</w>.",
                            "target_lang_translations": {
                                "ru": "Швейцарский сыр славится большими <w>дырками</w>."
                            }
                        },
                        {
                            "text": "The size of the <w>eye</w> indicates how the cheese was fermented.",
                            "target_lang_translations": {
                                "ru": "Размер <w>глазка</w> указывает на то, как ферментировался сыр."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Одно из отверстий в некоторых сортах сыра."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "глазок",
                                "target_lang_sense_clarification": "термин сыроделия"
                            },
                            {
                                "target_lang_word": "дырка",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3c62135-7051-38f6-b57e-f45f96dd6d7f",
                    "sense_definition": "A brood (specifically of pheasants).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "group_animals",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The hunter spotted an <w>eye</w> of pheasants in the tall grass.",
                            "target_lang_translations": {
                                "ru": "Охотник заметил <w>выводок</w> фазанов в высокой траве."
                            }
                        },
                        {
                            "text": "We saw an <w>eye</w> of young birds crossing the road.",
                            "target_lang_translations": {
                                "ru": "Мы увидели <w>выводок</w> птенцов, переходящих дорогу."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "or specialized"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выводок (особенно фазанов)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выводок",
                                "target_lang_sense_clarification": "группа птенцов"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "efdbf278-0751-359b-8341-bdbbebdbf043",
                    "sense_definition": "To carefully or appraisingly observe someone or something.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "action_observe",
                    "name_type": "no",
                    "examples": [
                        {

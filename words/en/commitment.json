{
    "word_family": [
        "commit",
        "committed",
        "committal",
        "recommitment"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "52a240c4-2f85-3dd1-8445-3c65983fadd8",
                    "sense_definition": "A promise or firm decision to do something in the future.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "obligation_promise",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She made a <w>commitment</w> to finish the project by the end of the week.",
                            "target_lang_translations": {
                                "ru": "Она взяла на себя <w>обязательство</w> закончить проект к концу недели."
                            }
                        },
                        {
                            "text": "We need a firm <w>commitment</w> from the buyers before we can proceed with the sale.",
                            "target_lang_translations": {
                                "ru": "Нам нужно твердое <w>обязательство</w> от покупателей, прежде чем мы сможем продолжить продажу."
                            }
                        }
                    ],
                    "synonyms": [
                        "promise",
                        "pledge",
                        "vow",
                        "undertaking",
                        "guarantee",
                        "word",
                        "oath",
                        "engagement"
                    ],
                    "common_phrases": [
                        "make a commitment",
                        "honor a commitment",
                        "firm commitment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обещание или твердое решение сделать что-либо в будущем."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обязательство",
                                "target_lang_sense_clarification": "обещание или договоренность"
                            },
                            {
                                "target_lang_word": "обещание",
                                "target_lang_sense_clarification": "устное или письменное заверение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21838bb0-59f7-3f1e-ab75-885bbfd74d84",
                    "sense_definition": "The willingness to give your time and energy to a job, activity, or person; the quality of being dedicated.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The players showed great <w>commitment</w> and energy throughout the entire match.",
                            "target_lang_translations": {
                                "ru": "Игроки демонстрировали огромную <w>самоотдачу</w> и энергию на протяжении всего матча."
                            }
                        },
                        {
                            "text": "His deep <w>commitment</w> to public service has inspired many young people.",
                            "target_lang_translations": {
                                "ru": "Его глубокая <w>приверженность</w> государственной службе вдохновила многих молодых людей."
                            }
                        }
                    ],
                    "synonyms": [
                        "dedication",
                        "loyalty",
                        "allegiance"
                    ],
                    "common_phrases": [
                        "show commitment",
                        "lack of commitment",
                        "personal commitment",
                        "deep commitment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Готовность отдавать свое время и энергию работе, занятию или человеку; преданность делу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "приверженность",
                                "target_lang_sense_clarification": "следование убеждениям или целям"
                            },
                            {
                                "target_lang_word": "преданность",
                                "target_lang_sense_clarification": "верность и лояльность"
                            },
                            {
                                "target_lang_word": "самоотдача",
                                "target_lang_sense_clarification": "готовность жертвовать силами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "79023e30-ab04-3c62-820c-d40216b7999a",
                    "sense_definition": "Something that you must do or deal with that takes your time, such as a responsibility or financial obligation.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "obligation_burden",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I cannot join the club right now because I have too many work <w>commitments</w>.",
                            "target_lang_translations": {
                                "ru": "Я не могу вступить в клуб сейчас, потому что у меня слишком много рабочих <w>обязанностей</w>."
                            }
                        },
                        {
                            "text": "Buying a house is a major financial <w>commitment</w> that requires careful planning.",
                            "target_lang_translations": {
                                "ru": "Покупка дома — это серьезное финансовое <w>обязательство</w>, требующее тщательного планирования."
                            }
                        }
                    ],
                    "synonyms": [
                        "obligation",
                        "responsibility",
                        "liability",
                        "duty",
                        "charge"
                    ],
                    "common_phrases": [
                        "financial commitment",
                        "family commitments",
                        "meet commitments",
                        "work commitments"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дело или обязанность, требующие времени и внимания; часто финансовое обязательство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обязательство",
                                "target_lang_sense_clarification": "финансовое или деловое бремя"
                            },
                            {
                                "target_lang_word": "обязанность",
                                "target_lang_sense_clarification": "рабочая или личная задача"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a62a4ef2-8bc7-3dae-84aa-a7c20943ba72",
                    "sense_definition": "The official act of sending a person to prison or a mental health institution.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "legal_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The judge signed the order for his involuntary <w>commitment</w> to the psychiatric hospital.",
                            "target_lang_translations": {
                                "ru": "Судья подписал ордер на его принудительную <w>госпитализацию</w> в психиатрическую больницу."
                            }
                        },
                        {
                            "text": "Civil <w>commitment</w> laws allow authorities to detain individuals who pose a danger to themselves.",
                            "target_lang_translations": {
                                "ru": "Законы о гражданской <w>госпитализации</w> позволяют властям задерживать лиц, представляющих опасность для самих себя."
                            }
                        }
                    ],
                    "synonyms": [
                        "committal",
                        "consignment"
                    ],
                    "common_phrases": [
                        "involuntary commitment",
                        "civil commitment"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "legal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Официальное решение о направлении человека в тюрьму или психиатрическое учреждение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заключение",
                                "target_lang_sense_clarification": "под стражу"
                            },
                            {
                                "target_lang_word": "помещение",
                                "target_lang_sense_clarification": "в закрытое учреждение"
                            },
                            {
                                "target_lang_word": "госпитализация",
                                "target_lang_sense_clarification": "принудительная, в больницу"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9935eaf1-4277-32ab-b111-396e63c21d06",
                    "sense_definition": "The act of performing a crime, mistake, or bad action.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "action_perpetration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The police found evidence linking him to the <w>commitment</w> of the robbery.",
                            "target_lang_translations": {
                                "ru": "Полиция нашла улики, связывающие его с <w>совершением</w> ограбления."
                            }
                        },
                        {
                            "text": "He felt a deep sense of guilt over the <w>commitment</w> of such a foolish error.",
                            "target_lang_translations": {
                                "ru": "Он испытывал глубокое чувство вины из-за <w>совершения</w> такой глупой ошибки."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "formal"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Акт совершения преступления, ошибки или проступка."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совершение",
                                "target_lang_sense_clarification": "действия, обычно негативного"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "26cf4130-d812-3aa6-adeb-8045f2bdadfd",
                    "sense_definition": "W czasie, który minął bardzo niedawno; przed krótką chwilą.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "time",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na stole stał <świeżo> upieczony chleb, który pięknie pachniał.",
                            "target_lang_translations": {
                                "ru": "На столе лежал <только что> испечённый хлеб, который чудесно пах."
                            }
                        },
                        {
                            "text": "Uważaj, ta ławka w parku jest <świeżo> malowana.",
                            "target_lang_translations": {
                                "ru": "Осторожно, эта скамейка в парке <только что> покрашена."
                            }
                        }
                    ],
                    "synonyms": [
                        "niedawno",
                        "dopiero co"
                    ],
                    "antonyms": [
                        "dawno"
                    ],
                    "common_phrases": [
                        "świeżo malowane",
                        "świeżo upieczony",
                        "świeżo wyciśnięty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совсем недавно; только что (о времени совершения действия)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "только что",
                                "target_lang_sense_clarification": "о действии, совершенном в недавнем прошлом"
                            },
                            {
                                "target_lang_word": "недавно",
                                "target_lang_sense_clarification": "в последнее время"
                            },
                            {
                                "target_lang_word": "свеже-",
                                "target_lang_sense_clarification": "первая часть сложных прилагательных (напр. свежеиспечённый)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cf45449b-6b7c-3a9a-8353-11fe35686456",
                    "sense_definition": "W sposób świadczący o zdrowiu, energii, młodości lub braku zmęczenia.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "appearance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mimo długiej podróży, Anna wyglądała bardzo <świeżo> i radośnie.",
                            "target_lang_translations": {
                                "ru": "Несмотря на долгую поездку, Анна выглядела очень <свежо> и радостно."
                            }
                        },
                        {
                            "text": "Po dobrym śnie czuję się <świeżo> i mam dużo siły do pracy.",
                            "target_lang_translations": {
                                "ru": "После хорошего сна я чувствую себя <свежо> и у меня много сил для работы."
                            }
                        }
                    ],
                    "synonyms": [
                        "rześko",
                        "zdrowo",
                        "młodo"
                    ],
                    "antonyms": [
                        "mizernie",
                        "staro"
                    ],
                    "common_phrases": [
                        "wyglądać świeżo",
                        "czuć się świeżo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Бодро, энергично; имея здоровый, отдохнувший вид."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свежо",
                                "target_lang_sense_clarification": "о внешнем виде или самочувствии"
                            },
                            {
                                "target_lang_word": "бодро",
                                "target_lang_sense_clarification": "энергично, без усталости"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cada280f-d1e9-3ec0-a242-29c0450021a0",
                    "sense_definition": "W sposób czysty, schludny i przyjemny dla zmysłów.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W całym mieszkaniu pachniało <świeżo> po porannym sprzątaniu.",
                            "target_lang_translations": {
                                "ru": "Во всей квартире пахло <свежо> после утренней уборки."
                            }
                        },
                        {
                            "text": "Zawsze stara się ubierać <świeżo> i elegancko do pracy.",
                            "target_lang_translations": {
                                "ru": "Она всегда старается одеваться <свежо> и элегантно на работу."
                            }
                        }
                    ],
                    "synonyms": [
                        "czysto",
                        "schludnie"
                    ],
                    "antonyms": [
                        "nieświeżo",
                        "brudno"
                    ],
                    "common_phrases": [
                        "pachnieć świeżo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чисто, опрятно; приятно для органов чувств (часто о запахе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "свежо",
                                "target_lang_sense_clarification": "о запахе или ощущении чистоты"
                            },
                            {
                                "target_lang_word": "чисто",
                                "target_lang_sense_clarification": "без грязи, опрятно"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "75ac4afe-69ce-3d8c-81be-2b12c1b6d2e1",
                    "sense_definition": "Een officiële aanspraak op een vergoeding voor geleden schade of verlies.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De verzekering wees zijn <w>claim</w> voor de waterschade direct af.",
                            "target_lang_translations": {
                                "en": "The insurance company rejected his <w>claim</w> for the water damage immediately.",
                                "ru": "Страховая компания сразу отклонила его <w>требование</w> о возмещении ущерба от воды."
                            }
                        },
                        {
                            "text": "Na het ongeluk besloot het slachtoffer een <w>claim</w> in te dienen bij de gemeente.",
                            "target_lang_translations": {
                                "en": "After the accident, the victim decided to file a <w>claim</w> with the municipality.",
                                "ru": "После аварии пострадавший решил подать <w>иск</w> в муниципалитет."
                            }
                        }
                    ],
                    "synonyms": [
                        "schadeclaim",
                        "eis"
                    ],
                    "common_phrases": [
                        "een claim indienen",
                        "een claim afwijzen",
                        "een claim toewijzen"
                    ],
                    "target_lang_definitions": {
                        "en": "An official request for money, usually from an insurance company or government, to cover damage or loss.",
                        "ru": "Официальное требование о возмещении ущерба или убытков."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "claim",
                                "target_lang_sense_clarification": "request for compensation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "претензия",
                                "target_lang_sense_clarification": "требование возмещения"
                            },
                            {
                                "target_lang_word": "иск",
                                "target_lang_sense_clarification": "судебное требование"
                            },
                            {
                                "target_lang_word": "требование",
                                "target_lang_sense_clarification": "настойчивая просьба"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d1b8b866-c018-30d0-8b7a-1f271c4dd4d4",
                    "sense_definition": "Een bewering of stelling waarvan men zegt dat deze waar is, vaak zonder direct bewijs.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zijn <w>claim</w> dat hij de snelste loper van de school is, werd door niemand geloofd.",
                            "target_lang_translations": {
                                "en": "His <w>claim</w> that he is the fastest runner in the school was believed by no one.",
                                "ru": "Его <w>утверждению</w>, что он самый быстрый бегун в школе, никто не поверил."
                            }
                        },
                        {
                            "text": "De wetenschapper onderbouwde zijn <w>claim</w> met nieuwe onderzoeksresultaten.",
                            "target_lang_translations": {
                                "en": "The scientist supported his <w>claim</w> with new research results.",
                                "ru": "Ученый обосновал свое <w>утверждение</w> новыми результатами исследований."
                            }
                        }
                    ],
                    "synonyms": [
                        "bewering",
                        "stelling"
                    ],
                    "common_phrases": [
                        "een harde claim",
                        "een claim onderbouwen"
                    ],
                    "target_lang_definitions": {
                        "en": "A statement that something is true or is a fact, although it has not been proved and other people might not believe it.",
                        "ru": "Утверждение или заявление, которое считается истинным, часто без прямых доказательств."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "claim",
                                "target_lang_sense_clarification": "statement without proof"
                            },
                            {
                                "target_lang_word": "assertion",
                                "target_lang_sense_clarification": "confident statement"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "утверждение",
                                "target_lang_sense_clarification": "высказывание"
                            },
                            {
                                "target_lang_word": "заявление",
                                "target_lang_sense_clarification": "декларация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9b290957-a326-36ac-a223-18cdbf84f014",
                    "sense_definition": "Het recht dat men meent te hebben op het bezit van een bepaald gebied of object.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het buurland legde een <w>claim</w> op het eiland vanwege de historische banden.",
                            "target_lang_translations": {
                                "en": "The neighboring country laid a <w>claim</w> to the island because of historical ties.",
                                "ru": "Соседняя страна предъявила <w>притязания</w> на остров из-за исторических связей."
                            }
                        },
                        {
                            "text": "De goudzoeker verdedigde zijn <w>claim</w> op het stuk grond met hand en tand.",
                            "target_lang_translations": {
                                "en": "The gold prospector defended his <w>claim</w> to the piece of land tooth and nail.",
                                "ru": "Золотоискатель отчаянно защищал свои <w>права</w> на участок земли."
                            }
                        }
                    ],
                    "synonyms": [
                        "aanspraak"
                    ],
                    "common_phrases": [
                        "een claim leggen op"
                    ],
                    "target_lang_definitions": {
                        "en": "A right to have something or get something, often regarding land or property ownership.",
                        "ru": "Право, которое кто-либо заявляет на владение определенной территорией или объектом."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "claim",
                                "target_lang_sense_clarification": "right to ownership"
                            },
                            {
                                "target_lang_word": "title",
                                "target_lang_sense_clarification": "legal right"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "притязание",
                                "target_lang_sense_clarification": "претензия на права"
                            },
                            {
                                "target_lang_word": "права",
                                "target_lang_sense_clarification": "юридическое право на владение"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "d815e8d8-53ec-3ccb-bd96-257f0697877b",
                    "sense_definition": "eerste persoon enkelvoud tegenwoordige tijd van <w>claimen</w>",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>claim</w> deze stoel omdat ik hier als eerste was.",
                            "target_lang_translations": {
                                "en": "I <w>claim</w> this chair because I was here first.",
                                "ru": "Я <w>занимаю</w> этот стул, потому что был здесь первым."
                            }
                        },
                        {
                            "text": "Als ik mijn bagage niet vind, <w>claim</w> ik de kosten bij de luchtvaartmaatschappij.",
                            "target_lang_translations": {
                                "en": "If I don't find my luggage, I will <w>claim</w> the costs from the airline.",
                                "ru": "Если я не найду свой багаж, я <w>потребую возмещения</w> расходов у авиакомпании."
                            }
                        }

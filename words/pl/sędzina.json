                    "sense_id": "da32a279-649a-3f64-9ced-f6e1fcdb6400",
                    "sense_definition": "Kobieta sprawująca urząd sędziego, uprawniona do wydawania wyroków w sądzie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "judge_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młoda <w>sędzina</w> dokładnie przeanalizowała wszystkie dowody zgromadzone w sprawie.",
                            "target_lang_translations": {
                                "ru": "Молодая <w>судья</w> тщательно проанализировала все доказательства, собранные по делу."
                            }
                        },
                        {
                            "text": "Wystudiowany szloch powódki ani przez chwilę nie zwiódł doświadczonej <w>sędziny</w>.",
                            "target_lang_translations": {
                                "ru": "Наигранные рыдания истицы ни на минуту не обманули опытную <w>судью</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sędzia"
                    ],
                    "common_phrases": [
                        "doświadczona sędzina",
                        "sędzina sądu rejonowego",
                        "niezawisła sędzina"
                    ],
                    "traits": [

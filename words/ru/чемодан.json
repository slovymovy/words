    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8669d9ea-08a7-38d9-af30-5d75004600a8",
                    "sense_definition": "Раскладная сумка в форме коробки с ручкой и жёсткими стенками, используемая для перевозки вещей и багажа.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Я уже собрал свой <w>чемодан</w> и готов ехать в аэропорт.",
                            "target_lang_translations": {
                                "en": "I have already packed my <w>suitcase</w> and am ready to go to the airport."
                            }
                        },
                        {
                            "text": "Этот <w>чемодан</w> слишком тяжёлый, я не могу его поднять.",
                            "target_lang_translations": {
                                "en": "This <w>suitcase</w> is too heavy, I can't lift it."
                            }
                        }
                    ],
                    "synonyms": [
                        "сумка"
                    ],
                    "common_phrases": [
                        "собрать чемодан",
                        "паковать чемоданы",
                        "сидеть на чемоданах",
                        "чемодан без ручки"
                    ],
                    "target_lang_definitions": {
                        "en": "A rectangular bag with a handle and rigid walls, used for carrying clothes and luggage when travelling."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "suitcase",
                                "target_lang_sense_clarification": "standard term"
                            },
                            {
                                "target_lang_word": "trunk",
                                "target_lang_sense_clarification": "large, rigid box for luggage"
                            },
                            {
                                "target_lang_word": "valise",
                                "target_lang_sense_clarification": "small travelling bag (dated)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3352ae7c-b9f2-385f-a8f9-2412ce7b9764",
                    "sense_definition": "Любая большая, громоздкая сумка или портфель.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "container",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зачем ты таскаешь с собой этот огромный <w>чемодан</w> каждый день?",
                            "target_lang_translations": {
                                "en": "Why do you drag this huge <w>bag</w> around with you every day?"
                            }
                        },
                        {
                            "text": "В твоём <w>чемодане</w> совсем нет места для новых книг.",
                            "target_lang_translations": {
                                "en": "There is absolutely no room for new books in your <w>suitcase</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "сумка"
                    ],
                    "traits": [

{
    "word_family": [
        "extraordinary",
        "extraordinarily"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3adc8d0a-b7f2-3910-b031-3ccbfa2f7693",
                    "sense_definition": "More than is usual, expected, or necessary.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "additional_quantity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I need some <w>extra</w> time to finish the exam.",
                            "target_lang_translations": {
                                "ru": "Мне нужно немного <w>дополнительного</w> времени, чтобы закончить экзамен."
                            }
                        },
                        {
                            "text": "She earned <w>extra</w> money by working on weekends.",
                            "target_lang_translations": {
                                "ru": "Она заработала <w>дополнительные</w> деньги, работая по выходным."
                            }
                        }
                    ],
                    "synonyms": [
                        "addition",
                        "supplement"
                    ],
                    "antonyms": [
                        "basic",
                        "standard"
                    ],
                    "common_phrases": [
                        "extra time",
                        "extra cost",
                        "extra effort",
                        "extra help"
                    ],
                    "target_lang_definitions": {
                        "ru": "Больше, чем обычно, ожидалось или необходимо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дополнительный",
                                "target_lang_sense_clarification": "добавочный, сверх нормы"
                            },
                            {
                                "target_lang_word": "лишний",
                                "target_lang_sense_clarification": "свободный или запасной"
                            },
                            {
                                "target_lang_word": "добавочный",
                                "target_lang_sense_clarification": "прибавленный к основному"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "aa222635-2b4b-3620-9266-e375053b7a92",
                    "sense_definition": "Behaving in an overly dramatic, excessive, or inappropriate way.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "behavior_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Don't be so <w>extra</w> about a simple mistake.",
                            "target_lang_translations": {
                                "ru": "Не будь таким <w>драматичным</w> из-за простой ошибки."
                            }
                        },
                        {
                            "text": "His reaction to the news was totally <w>extra</w>.",
                            "target_lang_translations": {
                                "ru": "Его реакция на новости была просто <w>чересчур</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "sundry"
                    ],
                    "common_phrases": [
                        "so extra",
                        "being extra",
                        "totally extra"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "popular in youth culture and social media"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Ведущий себя чрезмерно драматично, вызывающе или неуместно (сленг)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "чересчур",
                                "target_lang_sense_clarification": "слишком, перебор"
                            },
                            {
                                "target_lang_word": "драматичный",
                                "target_lang_sense_clarification": "наигранный, театральный"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "2fec435b-6742-3d74-b35b-6bbcdd391722",
                    "sense_definition": "To an exceptionally high degree; particularly.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "degree_adverb",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Please be <w>extra</w> careful when driving on the ice.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, будь <w>особенно</w> осторожен при вождении на льду."
                            }
                        },
                        {
                            "text": "The teacher was <w>extra</w> nice to us today.",
                            "target_lang_translations": {
                                "ru": "Учитель был <w>особенно</w> мил с нами сегодня."
                            }
                        }
                    ],
                    "synonyms": [
                        "supplement"
                    ],
                    "common_phrases": [
                        "extra careful",
                        "extra special",
                        "extra large",
                        "extra hard"
                    ],
                    "target_lang_definitions": {
                        "ru": "В исключительно высокой степени; особенно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "особенно",
                                "target_lang_sense_clarification": "в высокой степени"
                            },
                            {
                                "target_lang_word": "особо",
                                "target_lang_sense_clarification": "специально, отдельно"
                            },
                            {
                                "target_lang_word": "сверх",
                                "target_lang_sense_clarification": "чрезвычайно"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c80b42a2-11e8-324f-8623-42f25d97da9b",
                    "sense_definition": "An item that is added to something else, often for an additional charge.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "added_item",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The car comes with many optional <w>extras</w> included in the price.",
                            "target_lang_translations": {
                                "ru": "В цену автомобиля включено много дополнительных <w>опций</w>."
                            }
                        },
                        {
                            "text": "We were surprised by the hidden <w>extras</w> on our hotel bill.",
                            "target_lang_translations": {
                                "ru": "Мы были удивлены скрытыми <w>доплатами</w> в нашем счете за отель."
                            }
                        }
                    ],
                    "synonyms": [
                        "addition",
                        "supplement"
                    ],
                    "common_phrases": [
                        "optional extras",
                        "hidden extras",
                        "no extras"
                    ],
                    "target_lang_definitions": {
                        "ru": "Предмет или услуга, добавляемые к чему-либо, часто за дополнительную плату."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дополнение",
                                "target_lang_sense_clarification": "то, что добавлено"
                            },
                            {
                                "target_lang_word": "доплата",
                                "target_lang_sense_clarification": "дополнительная плата"
                            },
                            {
                                "target_lang_word": "опция",
                                "target_lang_sense_clarification": "дополнительная возможность"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cc49cd66-1ac8-3b62-9125-72041107b788",
                    "sense_definition": "An actor in a film or play who appears in the background and has no speaking lines.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "cinema_role",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She started her acting career as an <w>extra</w> in a commercial.",
                            "target_lang_translations": {
                                "ru": "Она начала свою актерскую карьеру как <w>статист</w> в рекламе."
                            }
                        },
                        {
                            "text": "They need hundreds of <w>extras</w> for the battle scene.",
                            "target_lang_translations": {
                                "ru": "Им нужны сотни <w>актёров массовки</w> для сцены битвы."
                            }
                        }
                    ],
                    "common_phrases": [
                        "movie extra",
                        "cast as an extra"
                    ],
                    "target_lang_definitions": {
                        "ru": "Актёр в фильме или пьесе, который появляется на заднем плане и не имеет слов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "статист",
                                "target_lang_sense_clarification": "актёр без слов"
                            },
                            {
                                "target_lang_word": "актёр массовки",
                                "target_lang_sense_clarification": "участник массовых сцен"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bd5cca49-351d-3810-8bf7-09ce8852ce1c",
                    "sense_definition": "A special edition of a newspaper printed to report important late-breaking news.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "newspaper_edition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The paperboy shouted on the corner to sell the <w>extra</w>.",
                            "target_lang_translations": {
                                "ru": "Разносчик газет кричал на углу, продавая <w>экстренный выпуск</w>."
                            }
                        },
                        {
                            "text": "They printed an <w>extra</w> to announce the end of the war.",
                            "target_lang_translations": {
                                "ru": "Они напечатали <w>экстренный выпуск</w>, чтобы объявить об окончании войны."
                            }
                        }
                    ],
                    "common_phrases": [
                        "read all about it"
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "mostly historical as digital news replaced printed extras"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Специальный выпуск газеты, напечатанный для сообщения важных последних новостей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "экстренный выпуск",
                                "target_lang_sense_clarification": "специальный номер газеты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a844ba6d-6277-30b5-913c-5e60dc4e65a8",
                    "sense_definition": "A run in cricket scored without the ball hitting the striker's bat.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "cricket_score",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The bowler gave away too many <w>extras</w> in the final over.",
                            "target_lang_translations": {
                                "ru": "Боулер отдал слишком много <w>дополнительных очков</w> в последнем овере."
                            }
                        },
                        {
                            "text": "Their total score included ten <w>extras</w>.",
                            "target_lang_translations": {
                                "ru": "Их общий счет включал десять <w>дополнительных очков</w>."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Очко в крикете, набранное без удара битой по мячу."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "дополнительное очко",
                                "target_lang_sense_clarification": "в спорте"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
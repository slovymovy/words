                {
                    "sense_id": "5015a159-b650-3ddb-8fa7-c1de389c6ec4",
                    "sense_definition": "Bewegend met kleine, snelle schokjes, vaak door kou, emotie, ouderdom of zwakte.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "shaking",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude man probeerde op <w>bevende</w> benen overeind te komen.",
                            "target_lang_translations": {
                                "en": "The old man tried to stand up on <w>trembling</w> legs.",
                                "ru": "Старик пытался подняться на <w>дрожащих</w> ногах."
                            }
                        },
                        {
                            "text": "Zij vertelde met <w>bevende</w> stem wat er die avond was gebeurd.",
                            "target_lang_translations": {
                                "en": "She told what had happened that evening with a <w>quivering</w> voice.",
                                "ru": "Она <w>дрожащим</w> голосом рассказала, что случилось в тот вечер."
                            }
                        },
                        {
                            "text": "Hij hield het briefje met <w>bevende</w> handen vast.",
                            "target_lang_translations": {
                                "en": "He held the note with <w>shaking</w> hands.",
                                "ru": "Он держал записку <w>трясущимися</w> руками."
                            }
                        }
                    ],
                    "synonyms": [
                        "trillend",
                        "bibberend",
                        "rillend"
                    ],
                    "common_phrases": [
                        "met bevende stem",
                        "op bevende benen",
                        "met bevende handen"
                    ],
                    "target_lang_definitions": {

    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "ec69ebe3-c719-3528-b124-750ca5aeafe5",
                    "sense_definition": "Резко или кратковременно замедлить движение, нажать на тормоз.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "stopping_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Водитель успел вовремя <w>тормознуть</w> перед выбежавшей на дорогу собакой.",
                            "target_lang_translations": {
                                "en": "The driver managed to <w>brake</w> in time before the dog that ran out onto the road."
                            }
                        },
                        {
                            "text": "Тебе нужно <w>тормознуть</w> перед поворотом, чтобы машина не вылетела с трассы.",
                            "target_lang_translations": {
                                "en": "You need to <w>slow down</w> before the turn so the car doesn't fly off the track."
                            }
                        }
                    ],
                    "synonyms": [
                        "затормозить",
                        "притормозить"
                    ],
                    "antonyms": [
                        "газануть",
                        "ускориться"
                    ],
                    "common_phrases": [
                        "резко тормознуть",
                        "успеть тормознуть"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To slow down sharply or briefly, to apply the brakes."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "brake",
                                "target_lang_sense_clarification": "vehicle context"
                            },
                            {
                                "target_lang_word": "hit the brakes",
                                "target_lang_sense_clarification": "idiomatic"
                            },
                            {
                                "target_lang_word": "slow down",
                                "target_lang_sense_clarification": "reduce speed"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15a24253-4448-3485-83c7-01d7ab4415df",
                    "sense_definition": "Сделать короткую остановку в пути, прервать движение.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "stopping_motion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы решили <w>тормознуть</w> возле кафе, чтобы немного передохнуть и выпить кофе.",
                            "target_lang_translations": {
                                "en": "We decided to <w>stop</w> near a cafe to rest a bit and drink coffee."
                            }
                        },
                        {
                            "text": "Давай <w>тормознём</w> здесь на пару минут и посмотрим карту города.",
                            "target_lang_translations": {
                                "en": "Let's <w>stop</w> here for a couple of minutes and look at the city map."
                            }
                        }
                    ],
                    "synonyms": [
                        "остановиться"
                    ],
                    "antonyms": [
                        "пойти",
                        "продолжить"
                    ],
                    "common_phrases": [
                        "тормознуть у обочины",
                        "тормознуть перекусить"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To make a short stop during a journey."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "general"
                            },
                            {
                                "target_lang_word": "pull over",
                                "target_lang_sense_clarification": "move vehicle to the side of the road"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fb273146-0f3a-3ec3-88cd-cf49c61fff8f",
                    "sense_definition": "Остановить проезжающий автомобиль жестом руки.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "hailing_transport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мне пришлось <w>тормознуть</w> попутку, так как автобусы уже не ходили.",
                            "target_lang_translations": {
                                "en": "I had to <w>flag down</w> a passing car since the buses were no longer running."
                            }
                        },
                        {
                            "text": "Он вышел на проспект и попытался <w>тормознуть</w> такси, чтобы успеть на вокзал.",
                            "target_lang_translations": {
                                "en": "He went out onto the avenue and tried to <w>hail</w> a taxi to make it to the station on time."
                            }
                        }
                    ],
                    "synonyms": [
                        "остановить"
                    ],
                    "antonyms": [
                        "пропустить"
                    ],
                    "common_phrases": [
                        "тормознуть такси",
                        "тормознуть машину"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To signal a passing vehicle to stop."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "flag down",
                                "target_lang_sense_clarification": "a passing car"
                            },
                            {
                                "target_lang_word": "hail",
                                "target_lang_sense_clarification": "a taxi"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dd02a535-d206-37b7-af41-3ccebbf46373",
                    "sense_definition": "Задержать кого-либо для проверки или выяснения обстоятельств (обычно о полиции или охране).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "impeding_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Охрана может <w>тормознуть</w> посетителей без пропуска прямо на входе в здание.",
                            "target_lang_translations": {
                                "en": "Security can <w>stop</w> visitors without a pass right at the building entrance."
                            }
                        },
                        {
                            "text": "Нас могут <w>тормознуть</w> на границе, если документы на машину не в полном порядке.",
                            "target_lang_translations": {
                                "en": "We might get <w>stopped</w> at the border if the car papers aren't fully in order."
                            }
                        }
                    ],
                    "synonyms": [
                        "задержать",
                        "остановить"
                    ],
                    "antonyms": [
                        "пропустить",
                        "освободить"
                    ],
                    "common_phrases": [
                        "тормознуть на посту",
                        "тормознуть для проверки"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To detain or stop someone for inspection (slang)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stop",
                                "target_lang_sense_clarification": "by authority"
                            },
                            {
                                "target_lang_word": "pull over",
                                "target_lang_sense_clarification": "traffic stop by police"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "058f641b-0e07-3fa2-abe4-24af0e5aebd3",
                    "sense_definition": "Приостановить или прекратить развитие, ход какого-либо дела или процесса.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "impeding_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Директор приказал <w>тормознуть</w> подписание договора до выяснения всех финансовых деталей.",
                            "target_lang_translations": {
                                "en": "The director ordered to <w>put</w> the signing of the contract <w>on hold</w> until all financial details are clarified."
                            }
                        },
                        {
                            "text": "Строительство нового корпуса пришлось <w>тормознуть</w> из-за внезапной нехватки бюджета.",
                            "target_lang_translations": {
                                "en": "The construction of the new building had to be <w>frozen</w> due to a sudden lack of budget."
                            }
                        }
                    ],
                    "synonyms": [
                        "приостановить",
                        "прекратить"
                    ],
                    "antonyms": [
                        "возобновить",
                        "продолжить"
                    ],
                    "common_phrases": [
                        "тормознуть проект",
                        "тормознуть дело"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To suspend or pause the progress of an activity (slang)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "put on hold",
                                "target_lang_sense_clarification": "suspend temporarily"
                            },
                            {
                                "target_lang_word": "freeze",
                                "target_lang_sense_clarification": "stop completely, often regarding budget or projects"
                            },
                            {
                                "target_lang_word": "pause",
                                "target_lang_sense_clarification": "interrupt briefly"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
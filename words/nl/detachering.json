{
    "word_family": [
        "detacheren",
        "gedetacheerde"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "38391616-aa0a-335b-a2a1-055b776fd5fa",
                    "sense_definition": "Het tijdelijk uitlenen van personeel aan een derde partij om daar werkzaamheden te verrichten, terwijl ze in dienst blijven bij het uitlenende bedrijf.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "employment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij werkt via <w>detachering</w> bij verschillende banken om ervaring op te doen.",
                            "target_lang_translations": {
                                "en": "He works on <w>secondment</w> at various banks to gain experience."
                            }
                        },
                        {
                            "text": "Het bedrijf zoekt tijdelijk personeel en maakt daarom gebruik van <w>detachering</w>.",
                            "target_lang_translations": {
                                "en": "The company is looking for temporary staff and is therefore using <w>secondment</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "uitlening",
                        "tewerkstelling"
                    ],
                    "common_phrases": [
                        "werken via detachering",
                        "bureau voor detachering",
                        "op basis van detachering"
                    ],
                    "target_lang_definitions": {
                        "en": "The temporary lending of personnel to a third party to perform work there, while they remain employed by the lending company."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "secondment",
                                "target_lang_sense_clarification": "temporary transfer of an employee"
                            },
                            {
                                "target_lang_word": "contracting",
                                "target_lang_sense_clarification": "working on a contract basis"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "87bcdaf2-5aa4-3161-9e82-75206e2ad8ef",
                    "sense_definition": "Het tijdelijk stationeren of onderbrengen van militairen of ambtenaren op een andere standplaats.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "placement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De officier bereidde zich voor op zijn <w>detachering</w> naar een basis in het buitenland.",
                            "target_lang_translations": {
                                "en": "The officer prepared for his <w>posting</w> to a base abroad."
                            }
                        },
                        {
                            "text": "Tijdens de <w>detachering</w> verbleven de soldaten in tijdelijke kampementen.",
                            "target_lang_translations": {
                                "en": "During the <w>deployment</w>, the soldiers stayed in temporary camps."
                            }
                        }
                    ],
                    "synonyms": [
                        "stationering",
                        "uitzending"
                    ],
                    "common_phrases": [
                        "detachering naar het buitenland"
                    ],
                    "target_lang_definitions": {
                        "en": "The temporary stationing or accommodation of soldiers or officials at another location."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "posting",
                                "target_lang_sense_clarification": "assignment to a specific post or location"
                            },
                            {
                                "target_lang_word": "deployment",
                                "target_lang_sense_clarification": "movement of troops for action"
                            },
                            {
                                "target_lang_word": "detachment",
                                "target_lang_sense_clarification": "military assignment"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
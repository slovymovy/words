    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "fcdd10a0-38d6-3066-9b03-3519d3c469be",
                    "sense_definition": "Kunstvorm die gebruikmaakt van klanken, ritme en melodie om gevoelens uit te drukken.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "art_sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij luistert elke avond naar rustige <w>muziek</w>.",
                            "target_lang_translations": {
                                "en": "She listens to quiet <w>music</w> every evening.",
                                "ru": "Она каждый вечер слушает спокойную <w>музыку</w>."
                            }
                        },
                        {
                            "text": "Wij maken samen <w>muziek</w> in een klein orkest.",
                            "target_lang_translations": {
                                "en": "We make <w>music</w> together in a small orchestra.",
                                "ru": "Мы вместе играем <w>музыку</w> в небольшом оркестре."
                            }
                        }
                    ],
                    "common_phrases": [
                        "muziek maken",
                        "naar muziek luisteren",
                        "klassieke muziek",
                        "live muziek"
                    ],
                    "target_lang_definitions": {
                        "en": "Art form that uses sounds, rhythm, and melody to express feelings.",
                        "ru": "Вид искусства, использующий звуки, ритм и мелодию для выражения чувств."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "music",
                                "target_lang_sense_clarification": "art form/sound"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "музыка",
                                "target_lang_sense_clarification": "вид искусства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c5e1e508-747b-35f6-a681-161b1bc839ef",
                    "sense_definition": "Op papier vastgelegde noten van een muziekstuk.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "written_music",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De pianist sloeg de <w>muziek</w> open en begon te spelen.",
                            "target_lang_translations": {
                                "en": "The pianist opened the <w>sheet music</w> and started playing.",
                                "ru": "Пианист открыл <w>ноты</w> и начал играть."
                            }
                        },
                        {
                            "text": "Ben je je <w>muziek</w> vergeten mee te nemen naar de les?",
                            "target_lang_translations": {
                                "en": "Did you forget to bring your <w>music</w> to the lesson?",
                                "ru": "Ты забыл взять свои <w>ноты</w> на урок?"
                            }
                        }
                    ],
                    "synonyms": [
                        "bladmuziek"
                    ],
                    "common_phrases": [
                        "muziek lezen"
                    ],
                    "target_lang_definitions": {
                        "en": "Notes of a musical piece recorded on paper.",
                        "ru": "Записанные на бумаге ноты музыкального произведения."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sheet music",
                                "target_lang_sense_clarification": "written notation"
                            },
                            {
                                "target_lang_word": "music",
                                "target_lang_sense_clarification": "written notation"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "ноты",
                                "target_lang_sense_clarification": "нотная запись"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "48474a21-d4f2-310e-ac7b-6158cc08c056",
                    "sense_definition": "Iets wat veelbelovend is of waar goede toekomstmogelijkheden in zitten.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De investeerder dacht dat er wel <w>muziek</w> in het nieuwe plan zat.",
                            "target_lang_translations": {
                                "en": "The investor thought there was <w>potential</w> in the new plan.",
                                "ru": "Инвестор подумал, что в новом плане есть <w>перспектива</w>."
                            }
                        },
                        {
                            "text": "Er zit veel <w>muziek</w> in deze samenwerking tussen de twee bedrijven.",
                            "target_lang_translations": {
                                "en": "There is a lot of <w>promise</w> in this collaboration between the two companies.",
                                "ru": "В этом сотрудничестве между двумя компаниями есть большой <w>потенциал</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "daar zit muziek in"
                    ],
                    "target_lang_definitions": {
                        "en": "Something that is promising or has good future possibilities.",
                        "ru": "Что-то многообещающее или имеющее хорошие перспективы на будущее."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "potential",
                                "target_lang_sense_clarification": "figurative: future possibilities"
                            },
                            {
                                "target_lang_word": "promise",
                                "target_lang_sense_clarification": "figurative: good prospects"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "перспектива",
                                "target_lang_sense_clarification": "переносное значение: будущее"
                            },
                            {
                                "target_lang_word": "потенциал",
                                "target_lang_sense_clarification": "скрытые возможности"
                            }
                        ]
                    }
                }
            ]
        }
    ]

            "pos": "noun",
            "senses": [
                {
                    "sense_id": "95c90351-f9dc-3883-8fec-b56e9b3ec344",
                    "sense_definition": "Zdolność fizyczna lub umysłowa organizmu do szybkiego, zręcznego i skutecznego działania.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "fitness_skill",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Regularne ćwiczenia na siłowni pomagają zachować wysoką <w>sprawność</w> fizyczną przez wiele lat.",
                            "target_lang_translations": {
                                "ru": "Регулярные упражнения в тренажерном зале помогают сохранять хорошую физическую <w>форму</w> на долгие годы."
                            }
                        },
                        {
                            "text": "Po poważnym wypadku pacjent musiał przejść długą rehabilitację, aby odzyskać pełną <w>sprawność</w> w nodze.",
                            "target_lang_translations": {
                                "ru": "После серьезной аварии пациенту пришлось пройти долгую реабилитацию, чтобы полностью восстановить <w>подвижность</w> ноги."
                            }
                        }
                    ],
                    "synonyms": [
                        "kondycja",
                        "zręczność",
                        "forma"
                    ],
                    "antonyms": [
                        "niesprawność",
                        "niepełnosprawność",
                        "niezdarność"
                    ],
                    "common_phrases": [
                        "sprawność fizyczna",
                        "sprawność ruchowa",
                        "sprawność umysłowa",
                        "utrzymać sprawność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическая или умственная способность организма действовать быстро, ловко и эффективно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "физическая форма",
                                "target_lang_sense_clarification": "состояние здоровья и тренированности тела"
                            },
                            {
                                "target_lang_word": "ловкость",
                                "target_lang_sense_clarification": "способность быстро и точно двигаться"
                            },
                            {
                                "target_lang_word": "работоспособность",
                                "target_lang_sense_clarification": "способность организма функционировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "27267061-5e3a-3031-b435-27797024e1df",
                    "sense_definition": "Skuteczność, szybkość i dobra organizacja w wykonywaniu zadań lub funkcjonowaniu instytucji.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "efficiency_abstract",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy system komputerowy znacznie poprawił <w>sprawność</w> obsługi klientów w naszym banku.",
                            "target_lang_translations": {
                                "ru": "Новая компьютерная система значительно улучшила <w>оперативность</w> обслуживания клиентов в нашем банке."
                            }
                        },
                        {
                            "text": "Dyrektor pochwalił cały zespół za dużą <w>sprawność</w> w realizacji tego skomplikowanego projektu.",
                            "target_lang_translations": {
                                "ru": "Директор похвалил всю команду за высокую <w>эффективность</w> в реализации этого сложного проекта."
                            }
                        }
                    ],
                    "synonyms": [
                        "efektywność",
                        "skuteczność",
                        "wydajność"
                    ],
                    "antonyms": [
                        "nieskuteczność",
                        "nieefektywność",
                        "opieszałość"
                    ],
                    "common_phrases": [
                        "sprawność działania",
                        "sprawność organizacyjna",
                        "sprawność urzędu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Эффективность, быстрота и хорошая организация при выполнении задач или функционировании учреждения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оперативность",
                                "target_lang_sense_clarification": "быстрота и четкость действий"
                            },
                            {
                                "target_lang_word": "эффективность",
                                "target_lang_sense_clarification": "результативность действий"
                            },
                            {
                                "target_lang_word": "слаженность",
                                "target_lang_sense_clarification": "организованность в работе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1fb2f137-1574-3393-aec4-d41caef9a741",
                    "sense_definition": "Stan techniczny urządzenia lub maszyny, który pozwala na jego prawidłowe i bezawaryjne użytkowanie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "technical_state",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Przed wyruszeniem w długą trasę kierowca powinien sprawdzić <w>sprawność</w> hamulców i świateł.",
                            "target_lang_translations": {
                                "ru": "Перед отправлением в долгий путь водитель должен проверить <w>исправность</w> тормозов и фар."
                            }
                        },
                        {
                            "text": "Mechanik wystawił certyfikat potwierdzający pełną <w>sprawność</w> techniczną pojazdu po naprawie.",
                            "target_lang_translations": {
                                "ru": "Механик выдал сертификат, подтверждающий полную техническую <w>исправность</w> автомобиля после ремонта."
                            }
                        }
                    ],
                    "synonyms": [
                        "funkcjonalność"
                    ],
                    "antonyms": [
                        "awaria",
                        "usterka",
                        "niesprawność"
                    ],
                    "common_phrases": [
                        "sprawność techniczna",
                        "kontrola sprawności",
                        "certyfikat sprawności"
                    ],
                    "target_lang_definitions": {
                        "ru": "Техническое состояние устройства или машины, позволяющее его правильное и безаварийное использование."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "исправность",
                                "target_lang_sense_clarification": "рабочее состояние техники"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0e02220-9f7a-33fe-b4ff-5d6d049b2bd8",
                    "sense_definition": "Wielkość fizyczna określająca stosunek energii użytecznej do energii dostarczonej do układu lub urządzenia.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "physics_ratio",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowoczesne żarówki LED charakteryzują się znacznie wyższą <w>sprawnością</w> energetyczną niż tradycyjne oświetlenie.",
                            "target_lang_translations": {
                                "ru": "Современные светодиодные лампы характеризуются значительно более высокой энергетической <w>эффективностью</w> по сравнению с традиционным освещением."
                            }
                        },
                        {
                            "text": "Inżynierowie wciąż pracują nad tym, aby zwiększyć <w>sprawność</w> paneli słonecznych w pochmurne dni.",
                            "target_lang_translations": {
                                "ru": "Инженеры продолжают работать над тем, чтобы повысить <w>КПД</w> солнечных панелей в пасмурные дни."
                            }
                        }
                    ],
                    "synonyms": [
                        "wydajność"
                    ],
                    "common_phrases": [
                        "sprawność silnika",
                        "sprawność energetyczna",
                        "wysoka sprawność"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физическая величина, определяющая отношение полезной энергии к энергии, подведенной к системе или устройству."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "КПД",
                                "target_lang_sense_clarification": "коэффициент полезного действия"
                            },
                            {
                                "target_lang_word": "эффективность",
                                "target_lang_sense_clarification": "энергетическая отдача"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3699289a-5a0c-3fd0-8314-f856372be9c4",
                    "sense_definition": "W harcerstwie: zdobyta umiejętność w określonej dziedzinie, potwierdzona przyznaniem symbolicznej odznaki na mundur.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "scouting",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Podczas letniego obozu każdy młody harcerz starał się zdobyć przynajmniej jedną nową <w>sprawność</w>.",
                            "target_lang_translations": {
                                "ru": "Во время летнего лагеря каждый юный скаут старался получить хотя бы одну новую <w>нашивку</w>."
                            }
                        },
                        {
                            "text": "Na rękawie jego munduru widniały liczne kolorowe <w>sprawności</w>, świadczące o jego wiedzy i doświadczeniu.",
                            "target_lang_translations": {
                                "ru": "На рукаве его формы виднелись многочисленные цветные <w>нашивки</w>, свидетельствующие о его знаниях и опыте."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zdobywać sprawności",
                        "sprawność kuchcika",
                        "sprawność sanitariusza"
                    ],
                    "target_lang_definitions": {
                        "ru": "В скаутском движении: приобретенный навык в определенной области, подтвержденный символическим значком на форме."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нашивка",
                                "target_lang_sense_clarification": "знак отличия за навык"
                            },
                            {
                                "target_lang_word": "знак отличия",
                                "target_lang_sense_clarification": "символ достижения"
                            }
                        ]
                    }
                }
            ]
        }
    ]

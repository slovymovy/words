    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "b904b5df-5883-37fa-8555-8851ce8387d0",
                    "sense_definition": "Het vaste land of de kade, vooral in contrast met het water of een schip",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "land_water_border",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De matrozen gingen in Rotterdam aan <w>wal</w>.",
                            "target_lang_translations": {
                                "ru": "Матросы сошли на <w>берег</w> в Роттердаме."
                            }
                        },
                        {
                            "text": "Het schip ligt stevig vast aan de <w>wal</w>.",
                            "target_lang_translations": {
                                "ru": "Корабль надежно пришвартован к <w>причалу</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "aan wal",
                        "van wal steken",
                        "aan lager wal raken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Суша или пристань, особенно в противопоставлении воде или кораблю"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "берег",
                                "target_lang_sense_clarification": "суша в отличие от моря"
                            },
                            {
                                "target_lang_word": "суша",
                                "target_lang_sense_clarification": "твердая земля"
                            },
                            {
                                "target_lang_word": "причал",
                                "target_lang_sense_clarification": "место швартовки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "53dca098-b3e3-3a95-a89c-d5f0f0348716",
                    "sense_definition": "Een zichtbare verdikking of donkere kring onder het oog, meestal door vermoeidheid",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "body_feature",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Door het slaapgebrek heeft zij diepe <w>wallen</w> onder haar ogen.",
                            "target_lang_translations": {
                                "ru": "Из-за недосыпа у нее глубокие <w>мешки</w> под глазами."
                            }
                        },
                        {
                            "text": "Hij probeerde zijn <w>wallen</w> te verbergen met make-up.",
                            "target_lang_translations": {
                                "ru": "Он пытался скрыть свои <w>мешки</w> под глазами с помощью макияжа."
                            }
                        }
                    ],
                    "synonyms": [
                        "oogwal"
                    ],
                    "common_phrases": [
                        "wallen onder de ogen"
                    ],
                    "target_lang_definitions": {
                        "ru": "Заметная припухлость или темный круг под глазом, обычно от усталости"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мешки",
                                "target_lang_sense_clarification": "под глазами"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "15f3cc38-f846-31e2-a41e-78841162e4e7",
                    "sense_definition": "Een aarden verhoging of muur die dient als bescherming tegen water of vijanden",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "defense_structure",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De oude stad wordt nog steeds omringd door een <w>wal</w>.",
                            "target_lang_translations": {
                                "ru": "Старый город до сих пор окружен <w>валом</w>."
                            }
                        },
                        {
                            "text": "Wandelaars lopen graag over de groene <w>wal</w> rond het dorp.",
                            "target_lang_translations": {
                                "ru": "Гуляющие любят ходить по зеленому <w>валу</w> вокруг деревни."
                            }
                        }
                    ],
                    "synonyms": [
                        "dijk",
                        "omwalling",
                        "stadsmuur"
                    ],
                    "common_phrases": [
                        "de wal keert het schip"
                    ],
                    "target_lang_definitions": {
                        "ru": "Земляная насыпь или стена, служащая защитой от воды или врагов"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вал",
                                "target_lang_sense_clarification": "оборонительное сооружение"
                            },
                            {
                                "target_lang_word": "насыпь",
                                "target_lang_sense_clarification": "земляное возвышение"
                            },
                            {
                                "target_lang_word": "дамба",
                                "target_lang_sense_clarification": "защита от воды"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f06aaefd-c74a-3a6a-89df-5a932c580caa",
                    "sense_definition": "Een groot zeezoogdier",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "animal",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De jagers zagen een <w>wal</w> in de verte spuiten.",
                            "target_lang_translations": {
                                "ru": "Охотники увидели, как вдалеке пускает фонтан <w>кит</w>."
                            }
                        },
                        {
                            "text": "In oude verhalen wordt vaak over de <w>wal</w> gesproken.",
                            "target_lang_translations": {
                                "ru": "В старых историях часто говорится о <w>ките</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "walvis",
                        "waldier"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "shortening of walvis"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Крупное морское млекопитающее"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кит",
                                "target_lang_sense_clarification": "морское животное"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [

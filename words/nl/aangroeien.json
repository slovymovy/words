    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "4900d87a-60ec-3085-bcee-b053f975175e",
                    "sense_definition": "Groter worden in aantal, omvang of hoeveelheid.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "vermeerdering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De stapel onbetaalde rekeningen bleef maar <w>aangroeien</w>.",
                            "target_lang_translations": {
                                "ru": "Стопка неоплаченных счетов продолжала <w>расти</w>.",
                                "en": "The pile of unpaid bills kept <w>growing</w>."
                            }
                        },
                        {
                            "text": "Door de rente is mijn spaargeld flink <w>aangegroeid</w>.",
                            "target_lang_translations": {
                                "ru": "Благодаря процентам мои сбережения значительно <w>выросли</w>.",
                                "en": "Due to the interest, my savings have <w>grown</w> considerably."
                            }
                        }
                    ],
                    "synonyms": [
                        "toenemen",
                        "vermeerderen",
                        "aanwassen",
                        "groeien"
                    ],
                    "antonyms": [
                        "afnemen",
                        "verminderen",
                        "slinken"
                    ],
                    "common_phrases": [
                        "snel aangroeien",
                        "gestaag aangroeien",
                        "laten aangroeien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увеличиваться в числе, объеме или количестве.",
                        "en": "To become larger in number, size, or quantity."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "увеличиваться",
                                "target_lang_sense_clarification": "становиться больше по размеру или количеству"
                            },
                            {
                                "target_lang_word": "возрастать",
                                "target_lang_sense_clarification": "повышаться, усиливаться"
                            },
                            {
                                "target_lang_word": "прирастать",
                                "target_lang_sense_clarification": "увеличиваться путем добавления (о капитале, территории)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "grow",
                                "target_lang_sense_clarification": "to become larger or greater over time"
                            },
                            {
                                "target_lang_word": "increase",
                                "target_lang_sense_clarification": "to become greater in amount or number"
                            },
                            {
                                "target_lang_word": "accumulate",
                                "target_lang_sense_clarification": "to gather or pile up gradually"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0b90be8-89ce-30e0-8ff3-3e490f23fe00",
                    "sense_definition": "Opnieuw groeien of weer aan elkaar vastgroeien na een breuk of beschadiging.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "herstel",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na de operatie moet het bot weer goed <w>aangroeien</w>.",
                            "target_lang_translations": {
                                "ru": "После операции кость должна снова хорошо <w>срастись</w>.",
                                "en": "After the operation, the bone must <w>knit</w> together well again."
                            }
                        },
                        {
                            "text": "Het afgeknipte haar zal vanzelf weer <w>aangroeien</w>.",
                            "target_lang_translations": {
                                "ru": "Остриженные волосы сами снова <w>отрастут</w>.",
                                "en": "The cut hair will <w>grow back</w> by itself."
                            }
                        }
                    ],
                    "synonyms": [
                        "vermeerderen",
                        "groeien"
                    ],
                    "antonyms": [
                        "afsterven"
                    ],
                    "common_phrases": [
                        "weer aangroeien",
                        "mooi aangroeien"
                    ],
                    "target_lang_definitions": {
                        "ru": "Снова вырастать или срастаться после перелома или повреждения.",
                        "en": "To grow again or grow back together after a break or damage."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "срастаться",
                                "target_lang_sense_clarification": "соединяться в процессе роста (о костях, тканях)"
                            },
                            {
                                "target_lang_word": "отрастать",
                                "target_lang_sense_clarification": "вырастать заново (о волосах, ногтях)"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "grow back",
                                "target_lang_sense_clarification": "to grow again after being cut or lost (e.g. hair)"
                            },
                            {
                                "target_lang_word": "knit",
                                "target_lang_sense_clarification": "to join together or heal (especially of broken bones)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
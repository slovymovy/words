{
    "word_family": [
        "association",
        "associative",
        "disassociate",
        "reassociate"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "996933aa-19ed-32a2-8688-d8f14db83052",
                    "sense_definition": "To connect evidentially, or in the mind or imagination.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "verb_connect",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "People often <w>associate</w> the color red with passion and love.",
                            "target_lang_translations": {
                                "ru": "Люди часто <w>ассоциируют</w> красный цвет со страстью и любовью."
                            }
                        },
                        {
                            "text": "I always <w>associate</w> the smell of freshly cut grass with summer holidays.",
                            "target_lang_translations": {
                                "ru": "Я всегда <w>ассоциирую</w> запах свежескошенной травы с летними каникулами."
                            }
                        }
                    ],
                    "synonyms": [
                        "connect",
                        "link",
                        "relate"
                    ],
                    "antonyms": [
                        "disassociate"
                    ],
                    "common_phrases": [
                        "associate with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Связывать факты, идеи или ощущения в уме или воображении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ассоциировать",
                                "target_lang_sense_clarification": "проводить ментальную связь"
                            },
                            {
                                "target_lang_word": "связывать",
                                "target_lang_sense_clarification": "соединять понятия или события"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d4071d83-a753-3795-a345-b04b6fd60383",
                    "sense_definition": "To spend time socially; to keep company with someone.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "verb_social",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She prefers not to <w>associate</w> with people who are constantly negative.",
                            "target_lang_translations": {
                                "ru": "Она предпочитает не <w>общаться</w> с людьми, которые постоянно настроены негативно."
                            }
                        },
                        {
                            "text": "He <w>associates</w> with a group of artists and musicians in the city.",
                            "target_lang_translations": {
                                "ru": "Он <w>общается</w> с группой художников и музыкантов в городе."
                            }
                        }
                    ],
                    "synonyms": [
                        "socialize",
                        "mingle",
                        "mix"
                    ],
                    "common_phrases": [
                        "associate with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Проводить время в общении; поддерживать знакомство или водиться с кем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "общаться",
                                "target_lang_sense_clarification": "поддерживать отношения"
                            },
                            {
                                "target_lang_word": "водиться",
                                "target_lang_sense_clarification": "разг. проводить время, часто с негативным оттенком"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "561c0edc-f5c0-3f82-b984-54e572b4ff7f",
                    "sense_definition": "To join as a partner, ally, or friend; to combine or unite.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "verb_join",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The politician tried to <w>associate</w> himself with the popular movement to gain votes.",
                            "target_lang_translations": {
                                "ru": "Политик пытался <w>ассоциировать</w> себя с популярным движением, чтобы получить голоса."
                            }
                        },
                        {
                            "text": "Our organization is proud to <w>associate</w> with such a prestigious charity.",
                            "target_lang_translations": {
                                "ru": "Наша организация гордится тем, что <w>сотрудничает</w> с такой престижной благотворительной организацией."
                            }
                        }
                    ],
                    "synonyms": [
                        "join",
                        "unite",
                        "ally",
                        "combine"
                    ],
                    "antonyms": [
                        "disassociate"
                    ],
                    "common_phrases": [
                        "associate oneself with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Объединяться в качестве партнера, союзника или друга; сотрудничать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "присоединяться",
                                "target_lang_sense_clarification": "вступать в союз или группу"
                            },
                            {
                                "target_lang_word": "сотрудничать",
                                "target_lang_sense_clarification": "работать вместе"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c5c29dba-4c1e-3ab2-ba7c-7858dce1f871",
                    "sense_definition": "A person united with another or others in an act, enterprise, or business; a partner or employee.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "noun_person",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She introduced me to her business <w>associate</w> during the conference.",
                            "target_lang_translations": {
                                "ru": "Она представила меня своему деловому <w>партнёру</w> во время конференции."
                            }
                        },
                        {
                            "text": "He works as a sales <w>associate</w> at a large department store.",
                            "target_lang_translations": {
                                "ru": "Он работает торговым <w>сотрудником</w> в большом универмаге."
                            }
                        }
                    ],
                    "synonyms": [
                        "colleague",
                        "coworker",
                        "partner",
                        "fellow"
                    ],
                    "common_phrases": [
                        "business associate",
                        "sales associate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, связанный с другими общим делом или бизнесом; партнер, коллега или сотрудник."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "партнёр",
                                "target_lang_sense_clarification": "участник совместной деятельности или бизнеса"
                            },
                            {
                                "target_lang_word": "коллега",
                                "target_lang_sense_clarification": "товарищ по работе или профессии"
                            },
                            {
                                "target_lang_word": "компаньон",
                                "target_lang_sense_clarification": "соучастник в деле или бизнесе"
                            },
                            {
                                "target_lang_word": "напарник",
                                "target_lang_sense_clarification": "партнер по паре в работе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4682c60a-a805-37fe-bff0-fc36bf64bada",
                    "sense_definition": "A member of an institution or society who is granted only partial status or privileges.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "noun_member",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "As an <w>associate</w>, he can attend meetings but does not have the right to vote.",
                            "target_lang_translations": {
                                "ru": "Как <w>ассоциированный член</w>, он может посещать собрания, но не имеет права голоса."
                            }
                        },
                        {
                            "text": "The law firm hired three new <w>associates</w> straight out of university.",
                            "target_lang_translations": {
                                "ru": "Юридическая фирма наняла трех новых <w>сотрудников</w> сразу после университета."
                            }
                        }
                    ],
                    "synonyms": [
                        "member"
                    ],
                    "common_phrases": [
                        "junior associate"
                    ],
                    "target_lang_definitions": {
                        "ru": "Член учреждения или общества, имеющий неполный статус; младший сотрудник (обычно в юридических фирмах)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сотрудник",
                                "target_lang_sense_clarification": "младший юрист или работник фирмы"
                            },
                            {
                                "target_lang_word": "член-корреспондент",
                                "target_lang_sense_clarification": "член академии с правом совещательного голоса"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "3384926e-3cf8-36e6-a76b-deadf71eaa51",
                    "sense_definition": "Joined with another or others and having lower status or rank.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "adj_rank",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She was promoted to <w>associate</w> professor after publishing her second book.",
                            "target_lang_translations": {
                                "ru": "Она получила должность <w>доцента</w> после публикации своей второй книги."
                            }
                        },
                        {
                            "text": "The <w>associate</w> director handles the daily operations of the company.",
                            "target_lang_translations": {
                                "ru": "<w>Заместитель</w> директора занимается повседневной деятельностью компании."
                            }
                        }
                    ],
                    "synonyms": [
                        "assistant",
                        "deputy",
                        "subordinate"
                    ],
                    "common_phrases": [
                        "associate professor",
                        "associate director",
                        "associate editor"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединенный с другими и имеющий более низкий статус или ранг (в названиях должностей)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "заместитель",
                                "target_lang_sense_clarification": "в должностях типа associate director"
                            },
                            {
                                "target_lang_word": "доцент",
                                "target_lang_sense_clarification": "эквивалент associate professor"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3c766209-8935-3173-bffb-8d63bbe60664",
                    "sense_definition": "Having partial status or privileges within a group or organization.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "adj_status",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He is an <w>associate</w> member of the club and pays a reduced fee.",
                            "target_lang_translations": {
                                "ru": "Он является <w>ассоциированным</w> членом клуба и платит сниженный взнос."
                            }
                        },
                        {
                            "text": "The <w>associate</w> partners share in the profits but have less control over decisions.",
                            "target_lang_translations": {
                                "ru": "<w>Младшие</w> партнёры участвуют в прибыли, но имеют меньше контроля над решениями."
                            }
                        }
                    ],
                    "synonyms": [
                        "adjunct"
                    ],
                    "common_phrases": [
                        "associate member"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий частичный статус или ограниченные привилегии в группе или организации."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ассоциированный",
                                "target_lang_sense_clarification": "имеющий неполное членство"
                            },
                            {
                                "target_lang_word": "младший",
                                "target_lang_sense_clarification": "о статусе партнера"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
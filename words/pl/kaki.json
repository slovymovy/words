            "senses": [
                {
                    "sense_id": "88d676ab-08d3-30f8-8fdc-0fe5e17967a9",
                    "sense_definition": "Jadalny, słodki owoc o pomarańczowej, gładkiej skórce i miąższu, przypominający kształtem pomidora; owoc hebanowca.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "fruit_tree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do sałatki owocowej dodamy też pokrojone <w>kaki</w>.",
                            "target_lang_translations": {
                                "ru": "В фруктовый салат мы также добавим нарезанную <w>хурму</w>."
                            }
                        },
                        {
                            "text": "Kupiłem w sklepie bardzo dojrzałe i miękkie <w>kaki</w>.",
                            "target_lang_translations": {
                                "ru": "Я купил в магазине очень спелую и мягкую <w>хурму</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "persymona",
                        "hurma",
                        "szaron",
                        "jabłko orientu",
                        "sharon"
                    ],
                    "common_phrases": [
                        "dojrzałe kaki",
                        "słodkie kaki",
                        "owoc kaki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Съедобный сладкий фрукт с гладкой оранжевой кожицей и мякотью, по форме напоминающий помидор; плод эбенового дерева."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хурма",
                                "target_lang_sense_clarification": "фрукт"
                            },
                            {
                                "target_lang_word": "королёк",
                                "target_lang_sense_clarification": "сорт хурмы"
                            },
                            {
                                "target_lang_word": "шарон",
                                "target_lang_sense_clarification": "сорт хурмы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d29f6619-0fe1-33aa-8fd0-53ddefc3e42a",
                    "sense_definition": "Drzewo z rodziny hebanowatych, uprawiane w ciepłym klimacie, rodzące jadalne owoce o tej samej nazwie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fruit_tree",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ogrodzie botanicznym rośnie stare drzewo <w>kaki</w>.",
                            "target_lang_translations": {
                                "ru": "В ботаническом саду растет старое дерево <w>хурмы</w>."
                            }
                        },
                        {
                            "text": "Liście <w>kaki</w> przebarwiają się jesienią na piękny czerwony kolor.",
                            "target_lang_translations": {
                                "ru": "Листья <w>хурмы</w> осенью окрашиваются в красивый красный цвет."
                            }
                        }
                    ],
                    "synonyms": [
                        "hebanowiec",
                        "persymona",
                        "hurma"
                    ],
                    "common_phrases": [
                        "drzewo kaki",
                        "uprawa kaki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Дерево семейства эбеновых, выращиваемое в теплом климате и дающее одноименные съедобные плоды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "хурма",
                                "target_lang_sense_clarification": "дерево"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c2b3e99f-0781-30a9-ba85-582771b853ae",
                    "sense_definition": "Forma fleksyjna (dopełniacz liczby pojedynczej lub mianownik liczby mnogiej) rzeczownika <w>kaka</w> (gatunek papugi).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Upierzenie tej <w>kaki</w> jest ciemnobrązowe z czerwonymi elementami.",
                            "target_lang_translations": {
                                "ru": "Оперение этого <w>кака</w> темно-коричневое с красными элементами."
                            }
                        },
                        {
                            "text": "W nowozelandzkim lesie obserwowaliśmy dwie dzikie <w>kaki</w>.",
                            "target_lang_translations": {
                                "ru": "В новозеландском лесу мы наблюдали двух диких <w>кака</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грамматическая форма (родительный падеж единственного числа или именительный падеж множественного числа) существительного «kaka» (вид попугая)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кака",
                                "target_lang_sense_clarification": "попугай"
                            }
                        ]
                    }
                }
            ]
        }

                            },
                            {
                                "target_lang_word": "основоположник",
                                "target_lang_sense_clarification": "создатель учения или направления"
                            },
                            {
                                "target_lang_word": "родоначальник",
                                "target_lang_sense_clarification": "инициатор"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "df5311f4-e8dd-3c80-86dd-7f26908c2214",
                    "sense_definition": "Przodkowie, poprzednie pokolenia (zwykle używane w liczbie mnogiej).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "ancestry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musimy szanować tradycje, które przekazali nam nasi <w>ojcowie</w>.",
                            "target_lang_translations": {
                                "ru": "Мы должны уважать традиции, которые передали нам наши <w>отцы</w>."
                            }
                        },
                        {
                            "text": "Oni walczyli o wolność tej ziemi, tak jak ich <w>ojcowie</w> i dziadkowie.",
                            "target_lang_translations": {
                                "ru": "Они сражались за свободу этой земли, так же как их <w>отцы</w> и деды."
                            }
                        }
                    ],
                    "synonyms": [
                        "przodkowie"
                    ],
                    "antonyms": [
                        "potomkowie"
                    ],
                    "common_phrases": [
                        "wiara ojców",
                        "ziemia ojców",
                        "groby ojców"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "literary"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Предки, предыдущие поколения (обычно используется во множественном числе)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "отцы",
                                "target_lang_sense_clarification": "предки"
                            }
                        ]
                    }
                }

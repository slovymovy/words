    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "27a9c0d7-d235-34b0-9a97-fe7aa8053eac",
                    "sense_definition": "Coinciding exactly when superimposed; having the same size and shape.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "math_geometry",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The two triangles are <w>congruent</w> because their sides are equal in length.",
                            "target_lang_translations": {
                                "ru": "Эти два треугольника <w>конгруэнтны</w>, так как их стороны равны по длине."
                            }
                        },
                        {
                            "text": "If you fold the paper perfectly, the two halves will be <w>congruent</w>.",
                            "target_lang_translations": {
                                "ru": "Если идеально сложить бумагу, две половинки будут <w>конгруэнтны</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "congruent triangles",
                        "congruent shapes",
                        "congruent figures"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совпадающий при наложении; имеющий одинаковые размеры и форму."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конгруэнтный",
                                "target_lang_sense_clarification": "геометрический термин"
                            },
                            {
                                "target_lang_word": "равный",
                                "target_lang_sense_clarification": "о геометрических фигурах"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f4f3b52-6034-3993-8da9-3bc04ee2acf9",
                    "sense_definition": "Corresponding in character or qualities; agreeing or harmonious.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "general_agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His behavior is not <w>congruent</w> with his stated principles.",
                            "target_lang_translations": {
                                "ru": "Его поведение не <w>согласуется</w> с его заявленными принципами."
                            }
                        },
                        {
                            "text": "We need to ensure that our actions are <w>congruent</w> with our values.",
                            "target_lang_translations": {
                                "ru": "Нам нужно убедиться, что наши действия <w>согласуются</w> с нашими ценностями."
                            }
                        }
                    ],
                    "synonyms": [
                        "congruous"
                    ],
                    "antonyms": [
                        "incongruous"
                    ],
                    "common_phrases": [
                        "congruent with"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соответствующий по характеру или качествам; согласующийся или гармоничный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соответствующий",
                                "target_lang_sense_clarification": "подходящий"
                            },
                            {
                                "target_lang_word": "согласующийся",
                                "target_lang_sense_clarification": "гармоничный"
                            },
                            {
                                "target_lang_word": "совпадающий",
                                "target_lang_sense_clarification": "идентичный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1e9e20e8-b795-3c15-96b9-158eabb80efe",
                    "sense_definition": "Having a difference divisible by a specific modulus.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "math_number_theory",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "In this equation, twelve is <w>congruent</w> to two modulo five.",
                            "target_lang_translations": {
                                "ru": "В этом уравнении двенадцать <w>сравнимо</w> с двумя по модулю пять."
                            }
                        },
                        {
                            "text": "The professor explained how two integers can be <w>congruent</w>.",
                            "target_lang_translations": {
                                "ru": "Профессор объяснил, как два целых числа могут быть <w>сравнимы</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "congruent modulo"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий разность, делящуюся на определенный модуль."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сравнимый",
                                "target_lang_sense_clarification": "в теории чисел"
                            },
                            {
                                "target_lang_word": "конгруэнтный",
                                "target_lang_sense_clarification": "математический термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]

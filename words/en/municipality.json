    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "9ba69966-3396-379f-a3c2-3d72bf73b559",
                    "sense_definition": "A city, town, or district that has corporate status and its own local government.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "administrative_division",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>municipality</w> is responsible for maintaining the roads and parks.",
                            "target_lang_translations": {
                                "ru": "<w>Муниципалитет</w> отвечает за содержание дорог и парков."
                            }
                        },
                        {
                            "text": "Residents of the <w>municipality</w> voted to increase funding for local schools.",
                            "target_lang_translations": {
                                "ru": "Жители <w>муниципалитета</w> проголосовали за увеличение финансирования местных школ."
                            }
                        }
                    ],
                    "synonyms": [
                        "city",
                        "town",
                        "district",
                        "borough"
                    ],
                    "common_phrases": [
                        "local municipality",
                        "within the municipality",
                        "municipality limits"
                    ],
                    "target_lang_definitions": {
                        "ru": "Город, поселок или район, имеющий статус юридического лица и собственное местное самоуправление"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "муниципалитет",
                                "target_lang_sense_clarification": "административная единица"
                            },
                            {
                                "target_lang_word": "муниципальное образование",
                                "target_lang_sense_clarification": "официальный термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "887b0faf-2084-3298-9e98-f5c2c2e331c7",
                    "sense_definition": "The governing body of a city or district.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "government_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>municipality</w> has announced new regulations for building safety.",
                            "target_lang_translations": {
                                "ru": "<w>Муниципалитет</w> объявил о новых правилах безопасности строительства."
                            }
                        },
                        {
                            "text": "She filed a formal complaint with the <w>municipality</w> about the noise.",
                            "target_lang_translations": {
                                "ru": "Она подала официальную жалобу в <w>муниципалитет</w> по поводу шума."
                            }
                        }
                    ],
                    "synonyms": [
                        "council",
                        "local government",
                        "city hall"
                    ],
                    "common_phrases": [
                        "contact the municipality",
                        "municipality officials"
                    ],
                    "target_lang_definitions": {
                        "ru": "Орган управления городом или районом"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "муниципалитет",
                                "target_lang_sense_clarification": "орган власти"
                            },
                            {
                                "target_lang_word": "городская администрация",
                                "target_lang_sense_clarification": "исполнительный орган"
                            },
                            {
                                "target_lang_word": "местные власти",
                                "target_lang_sense_clarification": "органы самоуправления"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d3cdd856-09e7-3221-88ce-18784d893064",
                    "sense_definition": "A second-level administrative division in the Philippines and certain Spanish- or Portuguese-speaking countries.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "administrative_division",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The governor visited every <w>municipality</w> in the province during his tour.",
                            "target_lang_translations": {
                                "ru": "Губернатор посетил каждый <w>муниципалитет</w> провинции во время своей поездки."
                            }
                        },
                        {
                            "text": "Each <w>municipality</w> is governed by a mayor and a municipal council.",
                            "target_lang_translations": {
                                "ru": "Каждый <w>муниципалитет</w> управляется мэром и муниципальным советом."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",

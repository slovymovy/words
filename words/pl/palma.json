{
    "word_family": [
        "palemka",
        "palmowy",
        "palmiarnia"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "c60649f4-d1c0-3ebc-b7b8-5dc92d437edf",
                    "sense_definition": "Egzotyczne drzewo o prostym pniu, zakończonym na szczycie pióropuszem dużych liści, rosnące w ciepłym klimacie.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Na piaszczystej plaży rosną wysokie <w>palmy</w> kokosowe.",
                            "target_lang_translations": {
                                "ru": "На песчаном пляже растут высокие кокосовые <w>пальмы</w>."
                            }
                        },
                        {
                            "text": "W ogrodzie botanicznym widzieliśmy ogromną <w>palmę</w> z daktylami.",
                            "target_lang_translations": {
                                "ru": "В ботаническом саду мы видели огромную финиковую <w>пальму</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "palma kokosowa",
                        "liść palmy",
                        "olej z palmy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Экзотическое дерево с прямым стволом, увенчанным пучком больших листьев, растущее в теплом климате."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пальма",
                                "target_lang_sense_clarification": "дерево"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2b88f0e7-2e98-3669-8b48-8a1809e8891f",
                    "sense_definition": "Ozdobna wiązanka z gałązek, suszonych kwiatów i traw, święcona w kościele na tydzień przed Wielkanocą.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W Niedzielę Palmową wierni niosą do kościoła kolorowe <w>palmy</w>.",
                            "target_lang_translations": {
                                "ru": "В Вербное воскресенье верующие несут в церковь разноцветные <w>вербы</w>."
                            }
                        },
                        {
                            "text": "Dzieci przygotowały w szkole własną <w>palmę</w> z bibuły.",
                            "target_lang_translations": {
                                "ru": "Дети сделали в школе свою <w>пасхальную пальму</w> из гофрированной бумаги."
                            }
                        }
                    ],
                    "synonyms": [
                        "marszałka"
                    ],
                    "common_phrases": [
                        "Niedziela Palmowa",
                        "święcenie palmy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Декоративный букет из веток, сухих цветов и трав, освящаемый в церкви за неделю до Пасхи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "верба",
                                "target_lang_sense_clarification": "церковный праздник"
                            },
                            {
                                "target_lang_word": "пасхальная пальма",
                                "target_lang_sense_clarification": "изделие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "07b4f8b5-1512-354b-a23c-ec7a85184a8d",
                    "sense_definition": "Stan utraty rozsądku, dziwnego zachowania lub obsesji na jakimś punkcie.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "2",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chyba odbiła mu <w>palma</w>, skoro rzucił dobrą pracę bez powodu.",
                            "target_lang_translations": {
                                "ru": "Кажется, у него поехала <w>крыша</w>, раз он бросил хорошую работу без причины."
                            }
                        },
                        {
                            "text": "Nie gadaj głupot, bo widzę, że <w>palma</w> ci odbija.",
                            "target_lang_translations": {
                                "ru": "Не болтай ерунды, я же вижу, что у тебя <w>крыша</w> едет."
                            }
                        }
                    ],
                    "synonyms": [
                        "marszałka"
                    ],
                    "common_phrases": [
                        "palma odbija",
                        "mieć palmę"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "used in informal speech"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Состояние потери рассудка, странного поведения или одержимости чем-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "крыша",
                                "target_lang_sense_clarification": "сленг: поехала крыша"
                            },
                            {
                                "target_lang_word": "бзик",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0bf02d1f-f6b6-3576-98f8-b24d8515f59e",
                    "sense_definition": "Symbol oznaczający zwycięstwo, sukces lub pierwszeństwo w jakiejś dziedzinie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "1",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ten zawodnik od lat dzierży <w>palmę</w> pierwszeństwa w skokach narciarskich.",
                            "target_lang_translations": {
                                "ru": "Этот спортсмен уже много лет удерживает <w>пальму</w> первенства в прыжках на лыжах с трамплина."
                            }
                        },
                        {
                            "text": "Nikt nie był w stanie odebrać jej <w>palmy</w> zwycięstwa w tym konkursie.",
                            "target_lang_translations": {
                                "ru": "Никто не мог отобрать у неё <w>пальму</w> первенства в этом конкурсе."
                            }
                        }
                    ],
                    "common_phrases": [
                        "palma pierwszeństwa",
                        "palma zwycięstwa"
                    ],
                    "target_lang_definitions": {
                        "ru": "Символ, означающий победу, успех или первенство в какой-либо области."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "пальма",
                                "target_lang_sense_clarification": "символ победы"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "4a079a2e-2d76-3b99-9df4-fe18cae5b3b2",
                    "sense_definition": "Miasto będące stolicą wyspy Majorka (Palma de Mallorca) lub inne nazwy geograficzne.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "W te wakacje lecimy samolotem do <w>Palmy</w>.",
                            "target_lang_translations": {
                                "ru": "В эти каникулы мы летим на самолете в <w>Пальму</w>."
                            }
                        },
                        {
                            "text": "Zwiedzanie <w>Palmy</w> rozpoczęliśmy od pięknej katedry.",
                            "target_lang_translations": {
                                "ru": "Осмотр <w>Пальмы</w> мы начали с красивого собора."
                            }
                        }
                    ],
                    "common_phrases": [
                        "Palma de Mallorca"
                    ],
                    "target_lang_definitions": {
                        "ru": "Город, являющийся столицей острова Майорка (Пальма-де-Майорка), или другие географические названия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Пальма",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
            "senses": [
                {
                    "sense_id": "225d0243-d380-354e-8144-b98991658d18",
                    "sense_definition": "Город в России, административный центр Пермского края, крупный промышленный, научный и культурный центр на реке Каме.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "city_perm",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "Мой брат учится в университете в <w>Перми</w>.",
                            "target_lang_translations": {
                                "en": "My brother studies at a university in <w>Perm</w>."
                            }
                        },
                        {
                            "text": "Поезд Москва — <w>Пермь</w> прибывает по расписанию.",
                            "target_lang_translations": {
                                "en": "The Moscow — <w>Perm</w> train arrives on schedule."
                            }
                        },
                        {
                            "text": "И <w>Пермь</w> заслуженно гордилась своим хореографическим училищем и театром.",
                            "target_lang_translations": {
                                "en": "And <w>Perm</w> was deservedly proud of its choreographic school and theater."
                            }
                        }
                    ],
                    "synonyms": [
                        "Молотов"
                    ],
                    "common_phrases": [
                        "город Пермь",
                        "в Перми",
                        "из Перми"
                    ],
                    "target_lang_definitions": {
                        "en": "A city in Russia, the administrative center of Perm Krai, and a major industrial, scientific, and cultural center on the Kama River."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "Perm",
                                "target_lang_sense_clarification": "city"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3093ed77-871e-3dd9-85a7-c2fbaeaef709",

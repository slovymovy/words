                },
                {
                    "sense_id": "a7474e8b-a396-32a1-b844-7b44e935dc13",
                    "sense_definition": "Calculated by adding several amounts together and then dividing the total by the number of amounts.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "statistical_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>average</w> age of the students in the class is twenty.",
                            "target_lang_translations": {
                                "ru": "<w>Средний</w> возраст студентов в классе — двадцать лет."
                            }
                        },
                        {
                            "text": "We calculated the <w>average</w> speed of the train for the entire journey.",
                            "target_lang_translations": {
                                "ru": "Мы рассчитали <w>среднюю</w> скорость поезда за всю поездку."
                            }
                        }
                    ],
                    "synonyms": [
                        "mean"
                    ],
                    "common_phrases": [
                        "average age",
                        "average price",
                        "average speed",
                        "average temperature"
                    ],
                    "target_lang_definitions": {
                        "ru": "Рассчитанный путем сложения нескольких величин и деления суммы на их количество."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "средний",
                                "target_lang_sense_clarification": "полученный путем вычисления среднего значения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "541defe2-ab6b-311f-a0d1-1498a5c53340",
                    "sense_definition": "Neither very good nor very bad; of medium or ordinary quality.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "mediocre_quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I soon realized I was only an <w>average</w> player compared to the others.",
                            "target_lang_translations": {
                                "ru": "Вскоре я понял, что я лишь <w>средний</w> игрок по сравнению с остальными."
                            }
                        },
                        {
                            "text": "The food at the restaurant was very <w>average</w> and not worth the high price.",
                            "target_lang_translations": {
                                "ru": "Еда в ресторане была весьма <w>посредственной</w> и не стоила такой высокой цены."
                            }
                        }
                    ],
                    "synonyms": [
                        "mediocre",
                        "ordinary",
                        "so-so",
                        "middling",
                        "unremarkable"
                    ],
                    "antonyms": [
                        "extraordinary"
                    ],
                    "common_phrases": [
                        "very average",
                        "average performance",
                        "average quality"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не очень хороший, но и не очень плохой; среднего или обычного качества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "посредственный",
                                "target_lang_sense_clarification": "невысокого качества"
                            },
                            {
                                "target_lang_word": "средний",
                                "target_lang_sense_clarification": "ни хороший, ни плохой"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e26208a3-bf43-383c-a73e-f02934b2e9bb",
                    "sense_definition": "A number calculated by adding quantities together and then dividing the total by the number of quantities.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "statistical_value",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>average</w> of these three numbers is eighteen.",
                            "target_lang_translations": {

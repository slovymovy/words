                {
                    "sense_id": "f412b2a1-da40-3a3c-9384-b4cb6f8cc0ac",
                    "sense_definition": "An abbreviation for \"post scriptum\" (postscript), a short message added at the end of a letter or email after the signature.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "writing_abbrev",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She added a <w>PS</w> to the letter to remind him about the meeting time.",
                            "target_lang_translations": {
                                "ru": "Она добавила <w>P.S.</w> к письму, чтобы напомнить ему о времени встречи."
                            }
                        },
                        {
                            "text": "The email ended with a quick <w>PS</w> asking for a reply by tomorrow.",
                            "target_lang_translations": {
                                "ru": "Электронное письмо заканчивалось коротким <w>P.S.</w> с просьбой ответить к завтрашнему дню."
                            }
                        }
                    ],
                    "synonyms": [
                        "postscript"
                    ],
                    "antonyms": [
                        "body"
                    ],
                    "common_phrases": [
                        "PS I love you",
                        "add a PS"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "abbreviation"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура от \"post scriptum\" (постскриптум), короткое сообщение, добавляемое в конце письма или электронного сообщения после подписи."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "P.S.",
                                "target_lang_sense_clarification": "аббревиатура"
                            },
                            {
                                "target_lang_word": "постскриптум",
                                "target_lang_sense_clarification": "полная форма"
                            }
                        ]
                    }
                },

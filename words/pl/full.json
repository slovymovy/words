            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8a973266-ec95-33fa-91ad-edb8359d8a28",
                    "sense_definition": "Układ kart w pokerze lub oczek w grze w kości, składający się z trójki i pary.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "poker_hand",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W ostatnim rozdaniu wygrał, mając <w>fulla</w> asów na króle.",
                            "target_lang_translations": {
                                "ru": "В последней раздаче он выиграл, имея <w>фулл-хаус</w> из тузов и королей."
                            }
                        },
                        {
                            "text": "Aby skompletować <w>fulla</w>, potrzebujesz jeszcze jednej karty do pary.",
                            "target_lang_translations": {
                                "ru": "Чтобы собрать <w>фулл-хаус</w>, тебе нужна еще одна карта для пары."
                            }
                        }
                    ],
                    "synonyms": [
                        "ful"
                    ],
                    "common_phrases": [
                        "mieć fulla",
                        "kareta i full"
                    ],
                    "target_lang_definitions": {
                        "ru": "Комбинация карт в покере или очков в игре в кости, состоящая из тройки и пары."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "фулл-хаус",
                                "target_lang_sense_clarification": "покерная комбинация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bf0c5530-3848-3c30-9f11-e4eb98ddd0c1",

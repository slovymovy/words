                    "sense_id": "f9478219-b13f-3d6b-941a-112d01bca731",
                    "sense_definition": "zdrobnienie od: <w>siatka</w>; mała torba na zakupy lub niewielki przedmiot wykonany z plecionki",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "object_diminutive",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Włożyłam wszystkie owoce do małej, plastikowej siateczki.",
                            "target_lang_translations": {
                                "ru": "Я сложила все фрукты в маленькую пластиковую <w>сеточку</w>."
                            }
                        },
                        {
                            "text": "Ta siateczka na zakupy jest bardzo lekka i zmieści się w kieszeni.",
                            "target_lang_translations": {
                                "ru": "Эта <w>сеточка</w> для покупок очень легкая и поместится в кармане."
                            }
                        }
                    ],
                    "synonyms": [
                        "woreczek",
                        "torebka"
                    ],
                    "common_phrases": [

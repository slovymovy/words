                {
                    "sense_id": "cca8b4bc-9b72-3212-a8e5-a9f17ee05067",
                    "sense_definition": "A punishment of imprisonment that lasts until the prisoner dies.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The judge sentenced the criminal to <w>life</w> for his terrible crimes.",

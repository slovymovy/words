                        "plooiloos"
                    ],
                    "antonyms": [
                        "ruw",
                        "oneffen",
                        "ribbelig",
                        "bultig",
                        "stroef"
                    ],
                    "common_phrases": [
                        "glad oppervlak",
                        "gladde huid",
                        "glad scheren"
                    ],
                    "target_lang_definitions": {
                        "en": "Having a flat, even surface without roughness or irregularities."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "smooth",
                                "target_lang_sense_clarification": "texture"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "40253649-0e2a-3786-95b9-666a5e553ea4",
                    "sense_definition": "Waarop men makkelijk uitglijdt door een gebrek aan wrijving.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "texture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pas op, de vloer is erg <w>glad</w>.",
                            "target_lang_translations": {

        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0d336a9f-2abd-3dec-9dbb-9ab16b39bdc8",
                    "sense_definition": "Met veel kennis, inzicht en levenservaring; verstandig.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "wisdom",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Een <w>wijs</w> persoon denkt goed na voordat hij een beslissing neemt.",
                            "target_lang_translations": {
                                "en": "A <w>wise</w> person thinks carefully before making a decision."
                            }
                        },
                        {
                            "text": "Met de jaren werd mijn grootvader steeds <w>wijzer</w> en milder.",
                            "target_lang_translations": {
                                "en": "Over the years, my grandfather became <w>wiser</w> and milder."
                            }
                        }
                    ],
                    "antonyms": [
                        "onwijs"
                    ],
                    "common_phrases": [
                        "niet goed wijs",
                        "wijs worden uit",
                        "een wijs man"
                    ],
                    "target_lang_definitions": {
                        "en": "Having or showing experience, knowledge, and good judgment."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "wise",
                                "target_lang_sense_clarification": "possessing wisdom"
                            },
                            {
                                "target_lang_word": "sensible",
                                "target_lang_sense_clarification": "showing good judgment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "601929ca-7d9b-381f-8106-2e1ad2508299",
                    "sense_definition": "Erg leuk, aangenaam of tof.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "fun",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dat feestje gisteren was echt <w>wijs</w>, we hebben enorm gelachen.",
                            "target_lang_translations": {
                                "en": "That party yesterday was really <w>cool</w>, we laughed a lot."
                            }
                        },
                        {
                            "text": "Hij heeft een <w>wijze</w> nieuwe jas gekocht in de stad.",
                            "target_lang_translations": {
                                "en": "He bought a <w>cool</w> new coat in the city."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "vooral in Vlaanderen"
                        },
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Very fun, pleasant, or impressive (slang)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "cool",
                                "target_lang_sense_clarification": "slang for good or fun"
                            },
                            {
                                "target_lang_word": "awesome",
                                "target_lang_sense_clarification": "slang for impressive"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6a586e52-d928-3c72-b350-8adc48b3effe",
                    "sense_definition": "Vervoeging van <w>wijzen</w>: eerste persoon enkelvoud tegenwoordige tijd, gebiedende wijs of inversie tweede persoon enkelvoud.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ik <w>wijs</w> de toerist de weg naar het station.",
                            "target_lang_translations": {
                                "en": "I <w>show</w> the tourist the way to the station."
                            }
                        },
                        {
                            "text": "<w>Wijs</w> niet met je vinger naar andere mensen, dat is onbeleefd.",
                            "target_lang_translations": {
                                "en": "Do not <w>point</w> your finger at other people, that is rude."
                            }
                        },
                        {
                            "text": "<w>Wijs</w> jij even aan waar we nu precies zijn op de kaart?",
                            "target_lang_translations": {
                                "en": "Can you just <w>point out</w> exactly where we are on the map?"
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To indicate a direction or object, or to show the way."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "point",
                                "target_lang_sense_clarification": "indicate with a finger"
                            },
                            {
                                "target_lang_word": "show",
                                "target_lang_sense_clarification": "indicate direction or location"
                            }
                        ]
                    }
                }
            ]
        },
        {

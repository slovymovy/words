    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "52bd2980-9bff-3f72-9175-e6cda19d1259",
                    "sense_definition": "The present participle and gerund of <w>know</w>.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is no way of <knowing> what will happen tomorrow.",
                            "target_lang_translations": {
                                "ru": "Нет способа <узнать>, что случится завтра."
                            }
                        },
                        {
                            "text": "<Knowing> the answer, she raised her hand immediately.",
                            "target_lang_translations": {
                                "ru": "<Зная> ответ, она немедленно подняла руку."
                            }
                        }
                    ],
                    "common_phrases": [
                        "worth knowing",
                        "not knowing"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "participle"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Причастие настоящего времени и герундий от глагола know."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "зная",
                                "target_lang_sense_clarification": "деепричастие"
                            },
                            {
                                "target_lang_word": "знание",
                                "target_lang_sense_clarification": "процесс или факт"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c84fc7f4-8958-31a6-946e-1921cf0601e1",
                    "sense_definition": "Suggestive of private knowledge or understanding.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "implied_knowledge",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She gave him a <knowing> smile across the crowded room.",
                            "target_lang_translations": {
                                "ru": "Она одарила его <многозначительной> улыбкой через переполненную комнату."
                            }
                        },
                        {
                            "text": "The two friends exchanged a <knowing> look when the topic was mentioned.",
                            "target_lang_translations": {
                                "ru": "Два друга обменялись <понимающими> взглядами, когда была упомянута эта тема."
                            }
                        }
                    ],
                    "synonyms": [
                        "knowful"
                    ],
                    "common_phrases": [
                        "knowing smile",
                        "knowing look",
                        "knowing glance"
                    ],
                    "target_lang_definitions": {
                        "ru": "Намекающий на наличие скрытого знания или понимания."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "многозначительный",
                                "target_lang_sense_clarification": "содержащий намёк"
                            },
                            {
                                "target_lang_word": "понимающий",
                                "target_lang_sense_clarification": "выражающий понимание"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "bae1a90d-9d3d-397c-9ab0-805a0d93864c",
                    "sense_definition": "Done with full awareness and intent; deliberate or wilful.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The jury decided that his action was a <knowing> violation of the law.",
                            "target_lang_translations": {
                                "ru": "Присяжные решили, что его действия были <сознательным> нарушением закона."
                            }
                        },
                        {
                            "text": "She made a <knowing> choice to ignore the warnings.",
                            "target_lang_translations": {
                                "ru": "Она сделала <осознанный> выбор проигнорировать предупреждения."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Сделанный с полным осознанием и намерением; преднамеренный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "сознательный",
                                "target_lang_sense_clarification": "осмысленный"
                            },
                            {
                                "target_lang_word": "умышленный",
                                "target_lang_sense_clarification": "преднамеренный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "d2828628-48d5-3b46-a4e6-802196aae3e3",
                    "sense_definition": "Shrewd or showing clever awareness; discerning.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The antique dealer cast a <knowing> eye over the collection.",
                            "target_lang_translations": {
                                "ru": "Антиквар окинул коллекцию <опытным> взглядом."
                            }
                        },
                        {
                            "text": "He is a <knowing> observer of the political landscape.",
                            "target_lang_translations": {
                                "ru": "Он — <проницательный> наблюдатель политического ландшафта."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Проницательный или демонстрирующий умную осведомленность."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проницательный",
                                "target_lang_sense_clarification": "наблюдательный"
                            },
                            {
                                "target_lang_word": "опытный",
                                "target_lang_sense_clarification": "знающий дело"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "776de61b-6d61-3b31-8a31-7a8cfd8ea6e4",
                    "sense_definition": "Possessing knowledge or understanding; intelligent.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She is a <knowing> guide who can explain the history of the city.",
                            "target_lang_translations": {
                                "ru": "Она — <знающий> гид, который может объяснить историю города."
                            }
                        },
                        {
                            "text": "The professor gave a <knowing> explanation of the complex theory.",
                            "target_lang_translations": {
                                "ru": "Профессор дал <компетентное> объяснение сложной теории."
                            }
                        }
                    ],
                    "synonyms": [
                        "knowful"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обладающий знаниями или пониманием; умный."
                    },
                    "translations": {

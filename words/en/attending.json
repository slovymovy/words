                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "лечащий врач",
                                "target_lang_sense_clarification": "ответственный специалист"
                            },
                            {
                                "target_lang_word": "куратор",
                                "target_lang_sense_clarification": "наставник для младших врачей"
                            },
                            {

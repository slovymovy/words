        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "e972bdc2-9350-34fe-aee9-935845a939d2",
                    "sense_definition": "A stream of water, typically smaller than a river.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "water_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The children jumped across the shallow <w>creek</w> in the woods.",
                            "target_lang_translations": {
                                "ru": "Дети перепрыгнули через мелкий <w>ручей</w> в лесу."
                            }
                        },
                        {
                            "text": "We walked along the <w>creek</w> bed looking for interesting stones.",
                            "target_lang_translations": {
                                "ru": "Мы шли по руслу <w>ручья</w> в поисках интересных камней."
                            }
                        }
                    ],
                    "synonyms": [
                        "stream",
                        "brook"
                    ],
                    "common_phrases": [
                        "up the creek",
                        "creek bed",
                        "cross the creek"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "North America, Australia"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Поток воды, обычно меньше реки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ручей",
                                "target_lang_sense_clarification": "небольшой поток воды"
                            },
                            {
                                "target_lang_word": "речушка",
                                "target_lang_sense_clarification": "маленькая река"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c13bea19-090d-38a3-b6dd-7f1c1d76ebca",
                    "sense_definition": "A small inlet, often saltwater, leading to the sea or to the main channel of a river.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "water_body",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The boat was anchored in a sheltered <w>creek</w> away from the rough sea.",
                            "target_lang_translations": {
                                "ru": "Лодка стояла на якоре в защищённой <w>бухте</w>, вдали от бурного моря."
                            }
                        },
                        {
                            "text": "At low tide, the muddy banks of the <w>creek</w> are visible.",
                            "target_lang_translations": {
                                "ru": "Во время отлива видны илистые берега <w>залива</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "run",
                        "bayou",
                        "stream"
                    ],
                    "common_phrases": [
                        "tidal creek",
                        "muddy creek"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "British"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Небольшой залив, часто с соленой водой, ведущий к морю или главному руслу реки."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бухта",
                                "target_lang_sense_clarification": "небольшой залив"
                            },
                            {
                                "target_lang_word": "залив",
                                "target_lang_sense_clarification": "часть водоема, вдающаяся в сушу"
                            },
                            {
                                "target_lang_word": "бухточка",
                                "target_lang_sense_clarification": "маленькая бухта"
                            }
                        ]
                    }
                },
                {

                },
                {
                    "sense_id": "c50a3dad-bce4-3c10-b378-5ef7ff08db2c",
                    "sense_definition": "Extremely tired or physically exhausted.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "adj_exhausted",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "After running the marathon, I was completely <w>done</w>.",
                            "target_lang_translations": {
                                "ru": "После марафона я был совершенно <w>без сил</w>."
                            }
                        },
                        {
                            "text": "He looks <w>done</w> in; he should really get some sleep.",
                            "target_lang_translations": {
                                "ru": "Он выглядит <w>вымотанным</w>; ему действительно стоит поспать."
                            }
                        }
                    ],
                    "synonyms": [
                        "completed",
                        "finished"
                    ],
                    "common_phrases": [
                        "done in"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чрезвычайно уставший или физически истощённый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "вымотанный",
                                "target_lang_sense_clarification": "сильно уставший"
                            },
                            {
                                "target_lang_word": "без сил",
                                "target_lang_sense_clarification": "истощённый"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ab5ca58e-87b9-3a53-90dd-141bae277f8c",
                    "sense_definition": "In a situation without hope of success or recovery; doomed.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "adj_doomed",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "If the police find the evidence, he is <w>done</w>.",
                            "target_lang_translations": {
                                "ru": "Если полиция найдет улики, ему <w>конец</w>."
                            }
                        },
                        {
                            "text": "The team is <w>done</w> for if they lose this match.",
                            "target_lang_translations": {
                                "ru": "Команде <w>конец</w>, если они проиграют этот матч."
                            }
                        }
                    ],
                    "synonyms": [
                        "finished"
                    ],
                    "common_phrases": [
                        "done for"
                    ],
                    "target_lang_definitions": {
                        "ru": "В ситуации без надежды на успех или спасение; обречённый."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "конец",
                                "target_lang_sense_clarification": "в значении гибели или провала"
                            },
                            {
                                "target_lang_word": "обречённый",
                                "target_lang_sense_clarification": "безнадёжный"

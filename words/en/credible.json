    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c0feff32-cce5-3144-8ba1-3f9eb455c190",
                    "sense_definition": "Believable or plausible.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He failed to provide a <w>credible</w> excuse for missing the meeting.",
                            "target_lang_translations": {
                                "ru": "Он не смог придумать <w>правдоподобное</w> оправдание своему отсутствию на собрании."
                            }
                        },
                        {
                            "text": "The police are investigating a <w>credible</w> threat against the bank.",
                            "target_lang_translations": {
                                "ru": "Полиция расследует <w>вполне реальную</w> угрозу в адрес банка."
                            }
                        }
                    ],
                    "synonyms": [
                        "believable",
                        "plausible",
                        "reasonable"
                    ],
                    "antonyms": [
                        "incredible",
                        "uncredible"
                    ],
                    "common_phrases": [
                        "credible excuse",
                        "credible explanation",
                        "credible threat",
                        "credible alternative"
                    ],
                    "target_lang_definitions": {
                        "ru": "Правдоподобный, вероятный или вызывающий доверие."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "правдоподобный",
                                "target_lang_sense_clarification": "похожий на правду"
                            },
                            {
                                "target_lang_word": "достоверный",
                                "target_lang_sense_clarification": "не вызывающий сомнений"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a24f45bd-2152-3275-9258-b514bc651eb3",
                    "sense_definition": "Dependable, trustworthy, or reliable.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The journalist refused to publish the story without a <w>credible</w> source.",
                            "target_lang_translations": {
                                "ru": "Журналист отказался публиковать статью без <w>надежного</w> источника."
                            }
                        },
                        {
                            "text": "She is considered a <w>credible</w> witness by the prosecution.",
                            "target_lang_translations": {
                                "ru": "Обвинение считает её свидетелем, <w>заслуживающим доверия</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "trustworthy",
                        "reliable",
                        "dependable"
                    ],
                    "antonyms": [
                        "incredible"
                    ],
                    "common_phrases": [
                        "credible source",
                        "credible witness",
                        "credible evidence"
                    ],
                    "target_lang_definitions": {
                        "ru": "Надежный, заслуживающий доверия (о людях или источниках)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "надежный",
                                "target_lang_sense_clarification": "на которого можно положиться"
                            },
                            {
                                "target_lang_word": "заслуживающий доверия",
                                "target_lang_sense_clarification": "честный, проверенный"
                            },
                            {
                                "target_lang_word": "авторитетный",
                                "target_lang_sense_clarification": "уважаемый и компетентный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1a65c17c-1f0d-33d3-87a4-4f3719dec2b0",
                    "sense_definition": "Authentic or convincing.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The actor gave a very <w>credible</w> performance as a historical figure.",
                            "target_lang_translations": {
                                "ru": "Актер продемонстрировал очень <w>убедительную</w> игру в роли исторической личности."
                            }
                        },
                        {
                            "text": "The special effects were not <w>credible</w> enough to look real.",
                            "target_lang_translations": {
                                "ru": "Спецэффекты были недостаточно <w>реалистичными</w>, чтобы выглядеть настоящими."
                            }
                        }
                    ],
                    "synonyms": [
                        "convincing",
                        "authentic",
                        "realistic"
                    ],
                    "antonyms": [
                        "incredible"
                    ],
                    "common_phrases": [
                        "credible performance",
                        "credible acting"
                    ],
                    "target_lang_definitions": {
                        "ru": "Убедительный или реалистичный (об игре актеров, искусстве)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "убедительный",
                                "target_lang_sense_clarification": "заставляющий поверить"
                            },
                            {
                                "target_lang_word": "реалистичный",
                                "target_lang_sense_clarification": "близкий к реальности"
                            }
                        ]
                    }
                }
            ]
        }

                        {
                            "text": "The horn of <w>plenty</w> is a traditional symbol of abundance.",
                            "target_lang_translations": {
                                "ru": "Рог <w>изобилия</w> — это традиционный символ богатства."
                            }
                        }
                    ],
                    "synonyms": [
                        "abundance",
                        "profusion"
                    ],
                    "antonyms": [
                        "scarcity",
                        "poverty"
                    ],
                    "common_phrases": [
                        "peace and plenty",
                        "land of plenty",
                        "horn of plenty"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ситуация, при которой имеется большой запас еды, денег или товаров; состояние изобилия."
                    },

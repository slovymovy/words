            "pos": "verb",
            "senses": [
                {
                    "sense_id": "f518ed19-0804-39f6-85e0-bac0e3419718",
                    "sense_definition": "Powodować zniszczenie i rozpad budynku, muru lub innej konstrukcji, zrównując ją z ziemią.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "destruction",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Władze miasta zdecydowały, aby <w>burzyć</w> stare magazyny i budować tam park.",
                            "target_lang_translations": {
                                "ru": "Городские власти решили <w>сносить</w> старые склады и строить там парк."
                            }
                        },
                        {
                            "text": "Nie wolno nam <w>burzyć</w> tego, co nasi przodkowie budowali przez wieki.",
                            "target_lang_translations": {
                                "ru": "Нам нельзя <w>разрушать</w> то, что наши предки строили веками."
                            }
                        }
                    ],
                    "synonyms": [
                        "niszczyć",
                        "wyburzać",
                        "rujnować",
                        "demolować",
                        "rozwalać"
                    ],
                    "antonyms": [
                        "budować",
                        "wznosić",
                        "stawiać"
                    ],
                    "common_phrases": [
                        "burzyć mury",
                        "burzyć mosty",
                        "burzyć domy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Разрушать или сносить здания, стены или другие конструкции, сравнивая их с землей."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "разрушать",
                                "target_lang_sense_clarification": "общая деструкция"
                            },
                            {
                                "target_lang_word": "сносить",
                                "target_lang_sense_clarification": "здания, постройки"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7bf89afa-3bd7-38ce-8840-1855947b7afb",
                    "sense_definition": "Okazywać gwałtowny sprzeciw, niezadowolenie lub oburzenie (zwykle w połączeniu z zaimkiem „się”).",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "emotion",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pracownicy zaczęli się <w>burzyć</w>, gdy usłyszeli o planowanych zwolnieniach.",
                            "target_lang_translations": {
                                "ru": "Работники начали <w>возмущаться</w>, когда услышали о планируемых увольнениях."
                            }
                        },
                        {
                            "text": "Nie ma powodu, żeby się tak <w>burzyć</w> o jedną małą pomyłkę.",
                            "target_lang_translations": {
                                "ru": "Нет причины так <w>возмущаться</w> из-за одной маленькой ошибки."
                            }
                        }
                    ],
                    "synonyms": [
                        "buntować się",
                        "demolować",
                        "gotować się",
                        "kipieć",
                        "wrzeć"
                    ],
                    "antonyms": [
                        "zgadzać się",
                        "akceptować"
                    ],
                    "common_phrases": [
                        "burzyć się o coś",
                        "burzyć się przeciwko"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выражать резкий протест, недовольство или возмущение (обычно с возвратным местоимением)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "возмущаться",
                                "target_lang_sense_clarification": "выражать недовольство"
                            },
                            {
                                "target_lang_word": "бунтовать",
                                "target_lang_sense_clarification": "протестовать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8272c6c0-7d4b-3429-a9b0-73e14ab0d667",
                    "sense_definition": "Naruszać istniejący spokój, porządek, ciszę lub harmonię; wprowadzać zamęt.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "disturbance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Głośna muzyka sąsiadów zaczęła <w>burzyć</w> nocny spokój całego osiedla.",
                            "target_lang_translations": {
                                "ru": "Громкая музыка соседей начала <w>нарушать</w> ночной покой всего района."
                            }
                        },
                        {
                            "text": "Złe wiadomości potrafią skutecznie <w>burzyć</w> nasz wewnętrzny ład i równowagę.",
                            "target_lang_translations": {
                                "ru": "Плохие новости могут эффективно <w>разрушать</w> наш внутренний строй и равновесие."
                            }
                        }
                    ],
                    "synonyms": [
                        "naruszać",
                        "musować",
                        "targać"
                    ],
                    "antonyms": [
                        "uspokajać",
                        "łagodzić"
                    ],
                    "common_phrases": [
                        "burzyć spokój",
                        "burzyć porządek",
                        "burzyć krew"
                    ],
                    "target_lang_definitions": {
                        "ru": "Нарушать существующий покой, порядок, тишину или гармонию; вносить смуту."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "нарушать",
                                "target_lang_sense_clarification": "покой, тишину"
                            },
                            {
                                "target_lang_word": "расстраивать",
                                "target_lang_sense_clarification": "порядок, планы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "897bcb46-f694-39c4-a236-6afa92e17992",
                    "sense_definition": "Wprawiać wodę, inną ciecz lub chmury w gwałtowny ruch, powodując ich kłębienie się lub pienienie.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "agitation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Silny wiatr zaczął <w>burzyć</w> powierzchnię jeziora, tworząc niebezpieczne fale.",
                            "target_lang_translations": {
                                "ru": "Сильный ветер начал <w>волновать</w> поверхность озера, создавая опасные волны."
                            }
                        },
                        {
                            "text": "Nadchodząca nawałnica zaczęła <w>burzyć</w> ciemne chmury na horyzoncie.",
                            "target_lang_translations": {
                                "ru": "Надвигающаяся буря начала <w>клубить</w> темные тучи на горизонте."
                            }
                        }
                    ],
                    "synonyms": [
                        "wzburzać",
                        "kotłować się",
                        "pienić się"
                    ],
                    "antonyms": [
                        "wygładzać",
                        "uspokajać"
                    ],
                    "common_phrases": [
                        "wiatr burzy wodę"
                    ],
                    "target_lang_definitions": {
                        "ru": "Приводить воду, другую жидкость или облака в бурное движение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "волновать",
                                "target_lang_sense_clarification": "воду, поверхность"
                            },
                            {
                                "target_lang_word": "взбаламучивать",
                                "target_lang_sense_clarification": "жидкость, осадок"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "42744002-1cad-3bde-a441-9661d5d82af9",
                    "sense_definition": "O winie lub innej cieczy: ulegać procesowi intensywnej fermentacji, wydzielając pianę i gaz.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "fermentation",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Młode wino w beczce zaczyna się <w>burzyć</w> już po kilku dniach od nastawienia.",
                            "target_lang_translations": {
                                "ru": "Молодое вино в бочке начинает <w>бродить</w> уже через несколько дней после закладки."
                            }
                        },
                        {
                            "text": "Musisz uważać, bo sok w butelce może się <w>burzyć</w> i wykipieć.",
                            "target_lang_translations": {
                                "ru": "Ты должен быть осторожен, потому что сок в бутылке может <w>забродить</w> и выкипеть."
                            }
                        }
                    ],
                    "synonyms": [
                        "fermentować",
                        "musować"
                    ],
                    "common_phrases": [
                        "wino się burzy"
                    ],
                    "target_lang_definitions": {
                        "ru": "О вине или другой жидкости: подвергаться процессу интенсивного брожения."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бродить",
                                "target_lang_sense_clarification": "о вине, ферментация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1206c54d-a15a-3b0b-acbd-f7dfaa4911c9",
                    "sense_definition": "Głośno stukać, uderzać lub dobijać się do czegoś.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "sound",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ktoś zaczął gwałtownie <w>burzyć</w> w drzwi wejściowe w środku nocy.",
                            "target_lang_translations": {
                                "ru": "Кто-то начал яростно <w>колотить</w> во входную дверь посреди ночи."
                            }
                        },
                        {
                            "text": "Słychać było, jak sąsiad <w>burzy</w> czymś ciężkim w ścianę.",
                            "target_lang_translations": {
                                "ru": "Было слышно, как сосед <w>стучит</w> чем-то тяжелым в стену."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "Górny Śląsk"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Громко стучать, ударять или ломиться во что-либо."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "стучать",

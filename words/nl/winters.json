                },
                {
                    "sense_id": "3e3e6fd3-171d-3d34-98ec-649c14d4ad25",
                    "sense_definition": "Genitief enkelvoud van <w>winter</w>.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {

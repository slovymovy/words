    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "8fa4b8bf-2489-3b80-95da-d2dceef4cbd0",
                    "sense_definition": "A planned endeavor, usually with a specific goal and accomplished in several steps or stages.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We are working on a science <w>project</w> for school.",
                            "target_lang_translations": {
                                "ru": "Мы работаем над школьным научным <w>проектом</w>."
                            }
                        },
                        {
                            "text": "The construction <w>project</w> will take two years to complete.",
                            "target_lang_translations": {
                                "ru": "На завершение строительного <w>проекта</w> уйдет два года."
                            }
                        }
                    ],
                    "synonyms": [
                        "plan",
                        "scheme",
                        "design",
                        "proposal"
                    ],
                    "common_phrases": [
                        "school project",
                        "research project",
                        "construction project",
                        "manage a project"
                    ],
                    "target_lang_definitions": {
                        "ru": "Запланированное мероприятие, обычно с конкретной целью и выполняемое в несколько этапов."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "проект",
                                "target_lang_sense_clarification": "замысел, план создания чего-либо"
                            },
                            {
                                "target_lang_word": "план",
                                "target_lang_sense_clarification": "намеченная последовательность действий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "14aa48d1-e831-3873-ab14-fea424fbbe2b",
                    "sense_definition": "An urban, low-income housing building or development.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "59316294-687b-3549-b8c2-2b895aefa06b",
                    "sense_definition": "Obszar występowania i eksploatacji złóż kopalin użytecznych, np. węgla lub miedzi.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "region_abundance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Górny Śląsk to największe przemysłowe <w>zagłębie</w> w Polsce.",
                            "target_lang_translations": {
                                "ru": "Верхняя Силезия — это крупнейший промышленный <w>бассейн</w> в Польше."
                            }
                        },
                        {
                            "text": "W tym rejonie odkryto nowe <w>zagłębie</w> ropy naftowej.",
                            "target_lang_translations": {
                                "ru": "В этом районе открыли новый нефтяной <w>бассейн</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zagłębie węglowe",
                        "zagłębie miedziowe",
                        "zagłębie naftowe",
                        "zagłębie przemysłowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Район залегания и добычи полезных ископаемых, например, угля или меди."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бассейн",
                                "target_lang_sense_clarification": "геология, горное дело"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a88daa2f-1ad1-3091-a770-c21609e45a0c",
                    "sense_definition": "Region znany z intensywnej produkcji rolniczej lub hodowli określonego typu.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "region_abundance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Okolice Grójca są znane jako największe <w>zagłębie</w> sadownicze w kraju.",
                            "target_lang_translations": {
                                "ru": "Окрестности Груйца известны как крупнейший садоводческий <w>район</w> в стране."
                            }
                        },
                        {
                            "text": "To województwo stanowi prawdziwe <w>zagłębie</w> uprawy chmielu.",
                            "target_lang_translations": {
                                "ru": "Это воеводство представляет собой настоящий <w>центр</w> выращивания хмеля."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zagłębie truskawkowe",
                        "zagłębie sadownicze",
                        "zagłębie paprykowe"
                    ],
                    "target_lang_definitions": {
                        "ru": "Регион, известный интенсивным сельскохозяйственным производством или разведением определенных культур."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "район",
                                "target_lang_sense_clarification": "область производства"
                            },
                            {
                                "target_lang_word": "центр",
                                "target_lang_sense_clarification": "сосредоточение деятельности"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97fe809e-33c5-3815-b21f-5a23cb1b1ed3",
                    "sense_definition": "Miejsce lub środowisko, w którym występuje duże nagromadzenie określonych osób, talentów lub zjawisk.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "region_abundance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Ta szkoła sportowa to prawdziwe <w>zagłębie</w> przyszłych mistrzów.",
                            "target_lang_translations": {
                                "ru": "Эта спортивная школа — настоящая <w>кузница</w> будущих чемпионов."
                            }
                        },
                        {
                            "text": "Dzielnica stała się niestety lokalnym <w>zagłębiem</w> przestępczości.",
                            "target_lang_translations": {
                                "ru": "Район, к сожалению, стал местным <w>рассадником</w> преступности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "zagłębie talentów",
                        "zagłębie bezrobocia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Место или среда с большим скоплением определенных людей, талантов или явлений."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "кузница",
                                "target_lang_sense_clarification": "талантов, кадров"
                            },
                            {
                                "target_lang_word": "рассадник",
                                "target_lang_sense_clarification": "негативных явлений"
                            },
                            {
                                "target_lang_word": "средоточие",
                                "target_lang_sense_clarification": "скопление"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c229a289-33ff-3f8d-b2b2-2a87aed07a6c",
                    "sense_definition": "Wklęsła forma terenu, dół lub miejsce położone niżej niż otoczenie.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Deszczówka spływała powoli do małego <w>zagłębia</w> w ziemi.",
                            "target_lang_translations": {
                                "ru": "Дождевая вода медленно стекала в небольшое <w>углубление</w> в земле."
                            }
                        },
                        {
                            "text": "Zwierzę ukryło się w naturalnym <w>zagłębiu</w> między skałami.",
                            "target_lang_translations": {
                                "ru": "Животное спряталось в естественном <w>углублении</w> между скалами."
                            }
                        }
                    ],
                    "synonyms": [
                        "Zagłębie Dąbrowskie"
                    ],
                    "target_lang_definitions": {
                        "ru": "Вогнутая форма рельефа, яма или место, расположенное ниже уровня окружающей местности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "углубление",
                                "target_lang_sense_clarification": "яма"
                            },
                            {
                                "target_lang_word": "впадина",
                                "target_lang_sense_clarification": "низина"
                            }
                        ]
                    }
                }
            ]
        },
        {

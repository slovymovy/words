{
    "word_family": [
        "align",
        "realign",
        "realignment",
        "misalign",
        "misalignment",
        "nonalignment"
    ],
    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "2bdcf977-fbaf-3a68-94a7-76d5c7c4e024",
                    "sense_definition": "An arrangement of items in a straight line or in correct relative positions.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "positioning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The soldiers stood in perfect <w>alignment</w> during the parade.",
                            "target_lang_translations": {
                                "ru": "Во время парада солдаты стояли в идеальном <w>равнении</w>."
                            }
                        },
                        {
                            "text": "Please check the <w>alignment</w> of the text on the page before printing.",
                            "target_lang_translations": {
                                "ru": "Пожалуйста, проверьте <w>выравнивание</w> текста на странице перед печатью."
                            }
                        }
                    ],
                    "synonyms": [
                        "arrangement",
                        "positioning",
                        "lineup"
                    ],
                    "antonyms": [
                        "misalignment",
                        "disorder"
                    ],
                    "common_phrases": [
                        "in alignment",
                        "out of alignment",
                        "perfect alignment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Расположение предметов на одной линии или в правильном относительном положении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выравнивание",
                                "target_lang_sense_clarification": "действие по расположению в линию"
                            },
                            {
                                "target_lang_word": "равнение",
                                "target_lang_sense_clarification": "военный термин, строй"
                            },
                            {
                                "target_lang_word": "строение",
                                "target_lang_sense_clarification": "порядок расположения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "645abe4d-647b-3c8c-92d6-3c59fd3373fc",
                    "sense_definition": "The process of adjusting the parts of a mechanism so that they work together correctly, or the state of being so adjusted.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "positioning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The mechanic fixed the <w>alignment</w> of the car's front wheels.",
                            "target_lang_translations": {
                                "ru": "Механик отрегулировал <w>развал-схождение</w> передних колес автомобиля."
                            }
                        },
                        {
                            "text": "Improper <w>alignment</w> can cause the machine to vibrate excessively.",
                            "target_lang_translations": {
                                "ru": "Неправильная <w>центровка</w> может вызвать чрезмерную вибрацию станка."
                            }
                        }
                    ],
                    "synonyms": [
                        "adjustment",
                        "calibration"
                    ],
                    "antonyms": [
                        "misalignment"
                    ],
                    "common_phrases": [
                        "wheel alignment",
                        "check the alignment",
                        "correct alignment"
                    ],
                    "target_lang_definitions": {
                        "ru": "Процесс регулировки частей механизма для их правильной совместной работы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "регулировка",
                                "target_lang_sense_clarification": "настройка механизма"
                            },
                            {
                                "target_lang_word": "центровка",
                                "target_lang_sense_clarification": "выставление по центру"
                            },
                            {
                                "target_lang_word": "наладка",
                                "target_lang_sense_clarification": "подготовка к работе"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "04ee6ffa-ffc6-3771-b7bb-4765f22fe0b6",
                    "sense_definition": "An agreement, union, or cooperation between different groups, nations, or factions sharing similar views.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The new treaty ensures closer <w>alignment</w> between the two countries on trade policies.",
                            "target_lang_translations": {
                                "ru": "Новый договор обеспечивает более тесное <w>сближение</w> двух стран в торговой политике."
                            }
                        },
                        {
                            "text": "The CEO wants better <w>alignment</w> between the sales and marketing departments.",
                            "target_lang_translations": {
                                "ru": "Генеральный директор хочет лучшей <w>согласованности</w> между отделами продаж и маркетинга."
                            }
                        }
                    ],
                    "synonyms": [
                        "alliance",
                        "affiliation",
                        "association",
                        "partnership"
                    ],
                    "common_phrases": [
                        "political alignment",
                        "strategic alignment",
                        "alignment of interests"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соглашение, союз или сотрудничество между группами, разделяющими схожие взгляды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "согласованность",
                                "target_lang_sense_clarification": "в действиях или мнениях"
                            },
                            {
                                "target_lang_word": "альянс",
                                "target_lang_sense_clarification": "политический или деловой союз"
                            },
                            {
                                "target_lang_word": "сближение",
                                "target_lang_sense_clarification": "позиций или интересов"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1b182d14-64bc-37fc-9b2e-4fd6267cffa4",
                    "sense_definition": "The goals and values of an artificial intelligence considered relative to human ethical standards.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "technical_ai",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Researchers are working hard to solve the AI <w>alignment</w> problem before systems become too powerful.",
                            "target_lang_translations": {
                                "ru": "Исследователи усердно работают над решением проблемы <w>согласования</w> ИИ, прежде чем системы станут слишком мощными."
                            }
                        },
                        {
                            "text": "Safety depends on the <w>alignment</w> of the intelligent system with human interests.",
                            "target_lang_translations": {
                                "ru": "Безопасность зависит от <w>согласованности</w> интеллектуальной системы с человеческими интересами."
                            }
                        }
                    ],
                    "common_phrases": [
                        "AI alignment",
                        "value alignment",
                        "alignment problem"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "used in Artificial Intelligence"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Соответствие целей и ценностей искусственного интеллекта человеческим этическим стандартам."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "согласование",
                                "target_lang_sense_clarification": "термин в области ИИ"
                            },
                            {
                                "target_lang_word": "алайнмент",
                                "target_lang_sense_clarification": "профессиональный жаргон"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "da822918-8c77-3fd6-a801-430107867d72",
                    "sense_definition": "The conjunction or straight-line arrangement of celestial objects in space.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "astronomy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rare <w>alignment</w> of the planets was visible just before dawn.",
                            "target_lang_translations": {
                                "ru": "Редкий <w>парад</w> планет был виден прямо перед рассветом."
                            }
                        },
                        {
                            "text": "Astronomers predicted the <w>alignment</w> would cause a total solar eclipse.",
                            "target_lang_translations": {
                                "ru": "Астрономы предсказали, что это <w>выстраивание</w> небесных тел вызовет полное солнечное затмение."
                            }
                        }
                    ],
                    "synonyms": [
                        "conjunction"
                    ],
                    "common_phrases": [
                        "planetary alignment"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "used in Astronomy"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Выстраивание небесных тел на одной прямой линии в пространстве."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "парад",
                                "target_lang_sense_clarification": "планет"
                            },
                            {
                                "target_lang_word": "выстраивание",
                                "target_lang_sense_clarification": "в одну линию"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f211b6f6-28bd-30d6-88ea-61ea3ab4db8c",
                    "sense_definition": "One of a set of moral positions or philosophies that a character can take in a role-playing game.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "gaming",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The player chose a chaotic good <w>alignment</w> for her new character.",
                            "target_lang_translations": {
                                "ru": "Игрок выбрала хаотично-доброе <w>мировоззрение</w> для своего нового персонажа."
                            }
                        },
                        {
                            "text": "His actions in the game did not match his chosen <w>alignment</w>.",
                            "target_lang_translations": {
                                "ru": "Его действия в игре не соответствовали выбранному им <w>мировоззрению</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "alignment chart",
                        "character alignment"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "used in Role-playing games"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Система моральных ценностей персонажа в ролевых играх."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "мировоззрение",
                                "target_lang_sense_clarification": "термин ролевых игр (D&D)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "1193411e-31ce-3a2d-8a5a-9605e6b025be",
                    "sense_definition": "The precise route or course taken by a road, railway, or path.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "engineering",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Engineers surveyed the land to determine the best <w>alignment</w> for the new highway.",
                            "target_lang_translations": {
                                "ru": "Инженеры обследовали местность, чтобы определить лучшую <w>трассировку</w> для нового шоссе."
                            }
                        },
                        {
                            "text": "The vertical <w>alignment</w> of the track had to be adjusted for safety.",
                            "target_lang_translations": {
                                "ru": "Вертикальный <w>профиль</w> пути пришлось скорректировать ради безопасности."
                            }
                        }
                    ],
                    "common_phrases": [
                        "road alignment",
                        "vertical alignment"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "used in Civil Engineering"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Точное направление, маршрут или траектория дороги или пути."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "трассировка",
                                "target_lang_sense_clarification": "прокладывание линии пути"
                            },
                            {
                                "target_lang_word": "профиль",
                                "target_lang_sense_clarification": "вертикальная проекция пути"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f314e568-7c59-3668-8c8a-3b29ed673ad4",
                    "sense_definition": "A way of arranging DNA, RNA, or protein sequences to identify regions of similarity.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "biology",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The scientist performed a sequence <w>alignment</w> to compare the two genes.",
                            "target_lang_translations": {
                                "ru": "Ученый выполнил <w>выравнивание</w> последовательностей, чтобы сравнить два гена."
                            }
                        },
                        {
                            "text": "Computer software is often used to speed up the <w>alignment</w> of complex protein structures.",
                            "target_lang_translations": {
                                "ru": "Компьютерные программы часто используются для ускорения <w>выравнивания</w> сложных белковых структур."
                            }
                        }
                    ],
                    "common_phrases": [
                        "sequence alignment"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "used in Genetics and Biology"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Метод сопоставления последовательностей ДНК, РНК или белков для выявления сходства."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выравнивание",
                                "target_lang_sense_clarification": "последовательностей"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "f50195d5-6db0-3cf2-9114-1ae4b92cb6e3",
                    "sense_definition": "Not firmly fixed, secure, or steady; likely to fall over or give way.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "physical_instability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The old wooden chair is very <w>unstable</w> and might break if you sit on it.",
                            "target_lang_translations": {
                                "ru": "Старый деревянный стул очень <w>шаткий</w> и может сломаться, если на него сесть."
                            }
                        },
                        {
                            "text": "Hikers should be careful because the ground near the cliff edge is <w>unstable</w>.",
                            "target_lang_translations": {
                                "ru": "Туристам следует быть осторожными, так как грунт у края обрыва <w>неустойчивый</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "wobbly",
                        "rickety",
                        "unsteady",
                        "tottering"
                    ],
                    "antonyms": [
                        "stable"
                    ],
                    "common_phrases": [
                        "unstable ground",
                        "unstable foundation",
                        "unstable structure"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не прочно закрепленный, шаткий или ненадежный; который может упасть или обрушиться."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неустойчивый",
                                "target_lang_sense_clarification": "о положении, опоре или равновесии"
                            },
                            {
                                "target_lang_word": "шаткий",
                                "target_lang_sense_clarification": "о мебели или конструкции"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4b389ded-5d6b-3ac4-aacf-d115e8f32293",
                    "sense_definition": "Likely to change suddenly and unpredictably; not constant or secure.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "abstract_instability",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The political situation in the region remains <w>unstable</w> after the recent election.",

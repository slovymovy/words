    "entries": [
        {
            "pos": "name",
            "senses": [
                {
                    "sense_id": "4414a264-84ef-34f9-86e1-88cec7abb23a",
                    "sense_definition": "miasto powiatowe i nadmorski kurort w Polsce, w województwie pomorskim",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "city_poland",
                    "name_type": "place_name",
                    "examples": [
                        {
                            "text": "W każde wakacje jeździmy całą rodziną do <w>Sopotu</w> na plażę.",
                            "target_lang_translations": {
                                "ru": "Каждые каникулы мы всей семьей ездим в <w>Сопот</w> на пляж."
                            }
                        },
                        {
                            "text": "Niedaleko od <w>Sopotu</w> przebiegała granica między Polską a Wolnym Miastem Gdańsk.",
                            "target_lang_translations": {
                                "ru": "Недалеко от <w>Сопота</w> проходила граница между Польшей и Вольным городом Гданьск."
                            }
                        }
                    ],
                    "synonyms": [
                        "letnia stolica Polski"
                    ],
                    "common_phrases": [
                        "molo w Sopocie",
                        "wycieczka do Sopotu",
                        "festiwal w Sopocie"
                    ],
                    "target_lang_definitions": {
                        "ru": "город и морской курорт в Польше, в Поморском воеводстве"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сопот",
                                "target_lang_sense_clarification": "город"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "10c9bd41-a2fa-3311-8ad5-57dd7f4f0e77",
                    "sense_definition": "festiwal muzyczny odbywający się w Sopocie",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "festival",
                    "name_type": "event_name",
                    "examples": [
                        {
                            "text": "W tym roku <w>Sopot</w> transmitowała inna stacja telewizyjna niż zwykle.",
                            "target_lang_translations": {
                                "ru": "В этом году <w>Сопот</w> транслировал другой телеканал, не тот, что обычно."
                            }
                        },
                        {
                            "text": "Wielu młodych artystów marzy o tym, by wygrać <w>Sopot</w>.",
                            "target_lang_translations": {
                                "ru": "Многие молодые артисты мечтают выиграть <w>Сопот</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "oglądać Sopot",
                        "wystąpić na Sopocie"
                    ],
                    "target_lang_definitions": {
                        "ru": "музыкальный фестиваль, проходящий в Сопоте"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сопот",
                                "target_lang_sense_clarification": "фестиваль"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8263d4b-833f-3b8d-bf8d-45e391d46cde",
                    "sense_definition": "rzeka w Polsce, dopływ Tanwi, na Roztoczu",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "river",
                    "name_type": "geographical_feature",
                    "examples": [
                        {
                            "text": "Rzeka <w>Sopot</w> płynie przez malownicze tereny leśne.",
                            "target_lang_translations": {
                                "ru": "Река <w>Сопот</w> течет через живописные лесные территории."
                            }
                        },
                        {
                            "text": "Nad rzeką <w>Sopot</w> można spotkać wiele rzadkich gatunków ptaków.",
                            "target_lang_translations": {
                                "ru": "На реке <w>Сопот</w> можно встретить много редких видов птиц."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "река в Польше, приток Танви, на Расточье"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Сопот",
                                "target_lang_sense_clarification": "река"
                            }
                        ]
                    }
                },
                {

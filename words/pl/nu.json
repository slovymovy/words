    "entries": [
        {
            "pos": "interjection",
            "senses": [
                {
                    "sense_id": "4a45bb14-05a0-3a12-bd95-59d32286a813",
                    "sense_definition": "Wykrzyknik wyrażający groźbę, naganę lub zakaz, zazwyczaj powtarzany podwójnie.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "warning",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mama pogroziła Jasiowi palcem i powiedziała: <w>nu</w>, <w>nu</w>, nie wolno!",
                            "target_lang_translations": {
                                "ru": "Мама погрозила Ясю пальцем и сказала: <w>ну</w>, <w>ну</w>, нельзя!"
                            }
                        },
                        {
                            "text": "<w>Nu</w>, <w>nu</w>, zostaw tego kota w spokoju, bo cię podrapie.",
                            "target_lang_translations": {
                                "ru": "<w>Ну</w>, <w>ну</w>, оставь этого кота в покое, а то он тебя поцарапает."
                            }
                        }
                    ],
                    "common_phrases": [
                        "nu, nu"
                    ],
                    "target_lang_definitions": {
                        "ru": "Междометие, выражающее угрозу, порицание или запрет, обычно повторяется дважды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "ну-ну",
                                "target_lang_sense_clarification": "выражение угрозы или предостережения"
                            },
                            {
                                "target_lang_word": "но-но",
                                "target_lang_sense_clarification": "строгий запрет"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3d0845df-deec-3d44-abf3-b39bcf8aa49b",
                    "sense_definition": "Okrzyk ponaglający do działania, oznaczający „szybko”, „dalej”.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "urging",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "<w>Nu</w>, chłopcy, bierzmy się szybko do ciężkiej pracy!",
                            "target_lang_translations": {
                                "ru": "<w>Ну</w>, ребята, давайте быстро браться за тяжелую работу!"
                            }
                        },
                        {
                            "text": "Woźnica krzyknął głośno na zmęczone konie: <w>nu</w>, biegiem!",
                            "target_lang_translations": {
                                "ru": "Кучер громко крикнул уставшим лошадям: <w>ну</w>, бегом!"
                            }

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7df74f91-2fac-3f5d-a925-4a08326df1f7",
                    "sense_definition": "Ручное сельскохозяйственное орудие в виде изогнутого полукругом ножа для срезания злаков и травы.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Крестьяне вышли в поле, чтобы жать спелую пшеницу <w>серпами</w>.",
                            "target_lang_translations": {
                                "en": "The peasants went out into the field to reap the ripe wheat with <w>sickles</w>."
                            }
                        },
                        {
                            "text": "На государственном гербе были изображены скрещённые <w>серп</w> и молот.",
                            "target_lang_translations": {
                                "en": "The crossed <w>sickle</w> and hammer were depicted on the state coat of arms."
                            }
                        }
                    ],
                    "common_phrases": [
                        "серп и молот",
                        "острый серп",
                        "работать серпом"
                    ],
                    "target_lang_definitions": {
                        "en": "A hand-held agricultural tool with a curved blade used for cutting grain and grass."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sickle",
                                "target_lang_sense_clarification": "agricultural tool"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3fc1b22b-339c-3acb-8e70-f6229f480029",
                    "sense_definition": "Форма, напоминающая узкую дугу или полукруг (обычно о Луне).",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "В ночном небе ярко светил тонкий <w>серп</w> молодого месяца.",
                            "target_lang_translations": {
                                "en": "A thin <w>crescent</w> of the new moon shone brightly in the night sky."
                            }
                        },
                        {
                            "text": "Из-за тёмных туч выглянул бледный <w>серп</w> луны.",
                            "target_lang_translations": {
                                "en": "The pale <w>crescent</w> of the moon peeked out from behind the dark clouds."
                            }
                        }
                    ],
                    "synonyms": [
                        "полумесяц"
                    ],
                    "common_phrases": [
                        "лунный серп",
                        "серп луны"
                    ],
                    "target_lang_definitions": {
                        "en": "A shape resembling a narrow arc or semicircle, usually referring to the moon."
                    },

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "6f201fca-6253-3272-8d60-9caeb7416706",
                    "sense_definition": "Быть похожим, иметь внешнее или внутреннее сходство с кем-либо или чем-либо.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "resemblance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Сын очень <w>походит</w> на своего отца характером.",
                            "target_lang_translations": {
                                "en": "The son closely <w>resembles</w> his father in character."
                            }
                        },
                        {
                            "text": "Этот старый дом <w>походит</w> на средневековый замок.",
                            "target_lang_translations": {
                                "en": "This old house <w>looks like</w> a medieval castle."
                            }
                        },
                        {
                            "text": "Ситуация <w>походит</w> на ту, что случилась в прошлом году.",
                            "target_lang_translations": {
                                "en": "The situation <w>resembles</w> the one that happened last year."
                            }
                        }
                    ],
                    "synonyms": [
                        "быть похо́жим",
                        "напоминать",
                        "сма́хивать"
                    ],
                    "antonyms": [
                        "отличаться"
                    ],
                    "common_phrases": [
                        "походить на отца",
                        "походить на правду",
                        "сильно походить",
                        "походить друг на друга"
                    ],
                    "target_lang_definitions": {
                        "en": "To be similar to someone or something in appearance or character."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "resemble",
                                "target_lang_sense_clarification": "to have a similar nature or appearance"
                            },
                            {
                                "target_lang_word": "look like",
                                "target_lang_sense_clarification": "to have the visual appearance of"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a3b580ef-52e4-36d3-944e-20492589618d",
                    "sense_definition": "Провести некоторое время в ходьбе; ходить недолго.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "movement_duration",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Врач посоветовал мне больше <w>походить</w> пешком для здоровья.",
                            "target_lang_translations": {
                                "en": "The doctor advised me to <w>walk</w> more for my health."
                            }
                        },
                        {
                            "text": "Он встал и <w>походил</w> по комнате, чтобы успокоиться.",
                            "target_lang_translations": {
                                "en": "He stood up and <w>walked</w> around the room to calm down."
                            }
                        },
                        {
                            "text": "Мы решили немного <w>походить</w> по магазинам и выбрать подарки.",
                            "target_lang_translations": {
                                "en": "We decided to <w>walk around</w> the shops a bit and choose gifts."
                            }
                        }
                    ],
                    "common_phrases": [
                        "походить по комнате",
                        "походить по парку",
                        "немного походить"
                    ],
                    "target_lang_definitions": {
                        "en": "To spend some time walking; to walk for a short while."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "walk",
                                "target_lang_sense_clarification": "to move on foot for a period of time"
                            },
                            {
                                "target_lang_word": "walk around",
                                "target_lang_sense_clarification": "to move about in a place"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
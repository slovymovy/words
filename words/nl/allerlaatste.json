{
    "word_family": [
        "laat",
        "laatste",
        "allerlaatst",
        "verlaat"
    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "a0f17172-76f6-31f3-bf7a-23a5204553d8",
                    "sense_definition": "Helemaal aan het einde komend; na alle anderen.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "literal_last",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dit is de <w>allerlaatste</w> keer dat ik je waarschuw.",
                            "target_lang_translations": {
                                "en": "This is the <w>very last</w> time I warn you.",
                                "ru": "Это <w>самый последний</w> раз, когда я тебя предупреждаю."
                            }
                        },
                        {
                            "text": "We bleven tot het <w>allerlaatste</w> moment op het feest.",
                            "target_lang_translations": {
                                "en": "We stayed at the party until the <w>very last</w> moment.",
                                "ru": "Мы оставались на вечеринке до <w>самого последнего</w> момента."
                            }
                        }
                    ],
                    "synonyms": [
                        "laatste",
                        "ultieme"
                    ],
                    "antonyms": [
                        "allereerste"
                    ],
                    "common_phrases": [
                        "het allerlaatste moment",
                        "de allerlaatste keer"
                    ],
                    "target_lang_definitions": {
                        "en": "Coming at the very end; after everyone else.",
                        "ru": "Самый последний, идущий в самом конце; после всех остальных."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "very last",
                                "target_lang_sense_clarification": "emphasizing the absolute end"
                            },
                            {
                                "target_lang_word": "final",
                                "target_lang_sense_clarification": "conclusive, nothing comes after"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "самый последний",
                                "target_lang_sense_clarification": "подчеркивает окончательность"
                            },
                            {
                                "target_lang_word": "последний",
                                "target_lang_sense_clarification": "общий смысл"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "abe5bae0-e84b-3d91-8349-566dac56981c",
                    "sense_definition": "Iemand of iets dat de absolute laatste plaats inneemt in een reeks.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "literal_last",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij kwam als <w>allerlaatste</w> over de finish bij de hardloopwedstrijd.",
                            "target_lang_translations": {
                                "en": "He crossed the finish line as the <w>very last one</w> in the running race.",
                                "ru": "Он пришел к финишу <w>самым последним</w> в забеге."
                            }
                        },
                        {
                            "text": "Het is niet leuk om altijd de <w>allerlaatste</w> te zijn die wordt gekozen.",
                            "target_lang_translations": {
                                "en": "It is not fun to always be the <w>very last one</w> chosen.",
                                "ru": "Неприятно всегда быть <w>самым последним</w>, кого выбирают."
                            }
                        }
                    ],
                    "synonyms": [
                        "hekkensluiter"
                    ],
                    "antonyms": [
                        "allereerste",
                        "koploper"
                    ],
                    "common_phrases": [
                        "als allerlaatste eindigen"
                    ],
                    "target_lang_definitions": {
                        "en": "Someone or something taking the absolute last place in a sequence.",
                        "ru": "Кто-то или что-то, занимающее абсолютно последнее место в ряду."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "very last one",
                                "target_lang_sense_clarification": "person or thing at the end of a series"
                            },
                            {
                                "target_lang_word": "dead last",
                                "target_lang_sense_clarification": "finishing in the absolute last position (informal)"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "самый последний",
                                "target_lang_sense_clarification": "замыкающий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "b622b06f-df94-3464-8819-c447a5e388b4",
                    "sense_definition": "Het meest recente nieuws of de nieuwste informatie.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "news",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Heb je het <w>allerlaatste</w> al gehoord over de verkiezingen?",
                            "target_lang_translations": {
                                "en": "Have you heard the <w>very latest</w> about the elections yet?",
                                "ru": "Ты уже слышал <w>самое последнее</w> о выборах?"
                            }
                        },
                        {
                            "text": "Ik weet ook het <w>allerlaatste</w> nog niet over die situatie.",
                            "target_lang_translations": {
                                "en": "I don't know the <w>very latest</w> about that situation yet either.",
                                "ru": "Я тоже еще не знаю <w>самого последнего</w> об этой ситуации."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The most recent news or newest information.",
                        "ru": "Самые свежие новости или новейшая информация."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "very latest",
                                "target_lang_sense_clarification": "referring to news or information"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "самое последнее",
                                "target_lang_sense_clarification": "о новостях"
                            },
                            {
                                "target_lang_word": "свежайшее",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3881c086-e06d-364c-80e3-eb5b0af6a376",
                    "sense_definition": "De persoon van wie men een bepaalde handeling het minst verwacht.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_unlikely",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Zij is de <w>allerlaatste</w> die zoiets zou doen.",
                            "target_lang_translations": {
                                "en": "She is the <w>very last person</w> who would do something like that.",
                                "ru": "Она <w>последняя</w>, кто сделал бы такое."
                            }
                        },
                        {
                            "text": "Ik ben de <w>allerlaatste</w> om anderen een verwijt te maken.",
                            "target_lang_translations": {
                                "en": "I am the <w>last person</w> to blame others.",
                                "ru": "Я <w>последний</w>, кто стал бы упрекать других."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The person from whom one expects a certain action the least.",
                        "ru": "Человек, от которого меньше всего ожидают определенного действия."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "last person",
                                "target_lang_sense_clarification": "the least likely person"
                            },
                            {
                                "target_lang_word": "very last person",
                                "target_lang_sense_clarification": "emphatic form of least likely person"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "последний",
                                "target_lang_sense_clarification": "в значении 'наименее вероятный'"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
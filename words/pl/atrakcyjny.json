                    ],
                    "common_phrases": [
                        "atrakcyjna kobieta",
                        "atrakcyjny wygląd",
                        "bardzo atrakcyjny"
                    ],
                    "target_lang_definitions": {
                        "ru": "Физически привлекательный, красивый (обычно о человеке)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "привлекательный",
                                "target_lang_sense_clarification": "о внешности"
                            },
                            {
                                "target_lang_word": "симпатичный",
                                "target_lang_sense_clarification": "милый, приятный"

            "senses": [
                {
                    "sense_id": "13cbd03b-1aa9-3d4f-a73a-dc9771a6ee9f",
                    "sense_definition": "Небольшое торговое заведение, магазинчик (часто торгующий определенным видом товаров).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "shop",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Утром бабушка всегда ходит в мясную <w>лавку</w>.",
                            "target_lang_translations": {
                                "en": "In the morning, grandmother always goes to the butcher <w>shop</w>."
                            }
                        },
                        {
                            "text": "Туристы долго выбирали подарки в сувенирной <w>лавке</w>.",
                            "target_lang_translations": {
                                "en": "The tourists spent a long time choosing gifts in the souvenir <w>shop</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "магазинчик",
                        "магазин"
                    ],
                    "common_phrases": [
                        "сувенирная лавка",
                        "мясная лавка",
                        "продуктовая лавка",
                        "частная лавка"
                    ],
                    "target_lang_definitions": {
                        "en": "A small shop or store, often selling a specific type of goods."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "shop",
                                "target_lang_sense_clarification": "small retail establishment"
                            },
                            {
                                "target_lang_word": "store",
                                "target_lang_sense_clarification": "retail establishment"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "379fe9f3-5605-36a0-84b8-07c7c1ab2c84",
                    "sense_definition": "Длинная, обычно деревянная скамья для сидения (часто без спинки, расположенная на улице или в деревенском доме).",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "furniture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Мы устали гулять и сели на <w>лавку</w> в парке.",
                            "target_lang_translations": {
                                "en": "We got tired of walking and sat on a <w>bench</w> in the park."
                            }
                        },
                        {
                            "text": "Возле каждого деревенского дома стояла деревянная <w>лавка</w>.",
                            "target_lang_translations": {
                                "en": "Near every village house stood a wooden <w>bench</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "скамейка"
                    ],
                    "common_phrases": [
                        "сидеть на лавке",
                        "деревянная лавка",
                        "садовая лавка"
                    ],
                    "target_lang_definitions": {
                        "en": "A long seat for several people, often made of wood and found outdoors."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bench",
                                "target_lang_sense_clarification": "long seat"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "97d9dfd9-9855-38fd-9616-4e6366954d5b",
                    "sense_definition": "Место для запасных игроков спортивной команды.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "sport",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Из-за травмы спортсмен провёл весь матч на <w>лавке</w>.",
                            "target_lang_translations": {
                                "en": "Due to an injury, the athlete spent the whole match on the <w>bench</w>."
                            }
                        },
                        {
                            "text": "У этой футбольной команды очень длинная <w>лавка</w>.",
                            "target_lang_translations": {
                                "en": "This football team has a very deep <w>bench</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "скамейка"
                    ],
                    "common_phrases": [
                        "сидеть на лавке",
                        "длинная лавка",
                        "короткая лавка",
                        "полировать лавку"
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": "sport"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "The place where substitute players sit during a game, or the reserve players themselves."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "bench",
                                "target_lang_sense_clarification": "sports reserves"

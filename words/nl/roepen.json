                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "calls",
                                "target_lang_sense_clarification": "plural of call"
                            },
                            {
                                "target_lang_word": "cries",
                                "target_lang_sense_clarification": "plural of cry"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "крики",
                                "target_lang_sense_clarification": "громкие звуки"
                            },
                            {
                                "target_lang_word": "призывы",
                                "target_lang_sense_clarification": "требования действий или изменений"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                        {
                            "trait_type": "form",
                            "comment": "initialism"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Аббревиатура от modus operandi: особый образ действий или метод, особенно характерный или устоявшийся (часто используется в криминалистике)."
                    },
                    "translations": {
                        "ru": [

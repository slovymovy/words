                {
                    "sense_id": "b37d242a-b3d3-3318-96b2-90a2c2254cc9",
                    "sense_definition": "Zich op grote afstand bevindend; niet dichtbij.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "distance_adj",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Hij reist graag naar <w>verre</w> oorden voor zijn vakantie.",
                            "target_lang_translations": {
                                "en": "He likes to travel to <w>distant</w> places for his vacation.",
                                "ru": "Он любит ездить в <w>далёкие</w> края в отпуск."
                            }
                        },
                        {
                            "text": "Het station is niet <w>ver</w> van ons huis vandaan.",
                            "target_lang_translations": {
                                "en": "The station is not <w>far</w> from our house.",
                                "ru": "Станция находится не <w>далеко</w> от нашего дома."
                            }
                        }
                    ],
                    "antonyms": [
                        "dichtbij",
                        "nabij"
                    ],
                    "common_phrases": [
                        "in de verre toekomst",
                        "een ver land",
                        "van heinde en verre"
                    ],
                    "target_lang_definitions": {
                        "en": "Located at a great distance; not close.",
                        "ru": "Находящийся на большом расстоянии; не близкий."
                    },
                    "translations": {
                        "en": [
                            {

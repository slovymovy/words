    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "0bb02973-351f-3710-8270-6f643de44c7f",
                    "sense_definition": "Having a large physical distance from one side to the other.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "physical_width",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The river is too <w>wide</w> for us to swim across safely.",
                            "target_lang_translations": {
                                "ru": "Река слишком <w>широкая</w>, чтобы мы могли безопасно переплыть её."
                            }
                        },
                        {
                            "text": "She greeted us with a <w>wide</w> smile on her face.",
                            "target_lang_translations": {
                                "ru": "Она встретила нас с <w>широкой</w> улыбкой на лице."
                            }
                        }
                    ],
                    "synonyms": [
                        "broad"
                    ],
                    "antonyms": [
                        "narrow"
                    ],
                    "common_phrases": [
                        "wide road",
                        "wide river",
                        "wide smile",
                        "wide variety"
                    ],
                    "target_lang_definitions": {
                        "ru": "Имеющий большое расстояние от одной стороны до другой; большой в поперечнике."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "широкий",
                                "target_lang_sense_clarification": "о физическом размере"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "dedd9942-efac-3e29-9f17-7d93e8a36d77",
                    "sense_definition": "Including a large number or variety of different people or things; large in scope.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "scope",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The company offers a <w>wide</w> range of products for different needs.",
                            "target_lang_translations": {
                                "ru": "Компания предлагает <w>широкий</w> ассортимент товаров для различных нужд."
                            }
                        },
                        {
                            "text": "Candidates need to have a <w>wide</w> knowledge of current affairs.",
                            "target_lang_translations": {
                                "ru": "Кандидаты должны обладать <w>обширными</w> знаниями о текущих событиях."
                            }
                        }
                    ],
                    "synonyms": [
                        "broad"
                    ],
                    "antonyms": [
                        "narrow"
                    ],
                    "common_phrases": [
                        "wide range",
                        "wide selection",
                        "wide experience"
                    ],
                    "target_lang_definitions": {
                        "ru": "Охватывающий большое количество или разнообразие людей или вещей; обширный по охвату."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "широкий",
                                "target_lang_sense_clarification": "об ассортименте или знаниях"
                            },
                            {
                                "target_lang_word": "обширный",
                                "target_lang_sense_clarification": "большой по объему"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "fa967277-9438-3d00-b9fc-ab36273e084c",
                    "sense_definition": "At a considerable distance from the intended point or target; missing the mark.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "off_target",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "His shot went <w>wide</w> of the goal and hit the fence.",
                            "target_lang_translations": {
                                "ru": "Его удар прошел <w>мимо</w> ворот и попал в забор."
                            }
                        },
                        {
                            "text": "The initial estimates proved to be <w>wide</w> of the mark.",
                            "target_lang_translations": {
                                "ru": "Первоначальные оценки оказались <w>далеки</w> от истины."
                            }
                        }
                    ],
                    "common_phrases": [
                        "wide of the mark"
                    ],
                    "target_lang_definitions": {
                        "ru": "Находящийся на значительном расстоянии от намеченной цели; неточный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "далёкий",
                                "target_lang_sense_clarification": "от истины или цели"
                            },
                            {
                                "target_lang_word": "мимо",
                                "target_lang_sense_clarification": "в значении промаха"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "adverb",
            "senses": [
                {
                    "sense_id": "3f6f95ab-84a9-35a5-aa10-100bd4d6c702",

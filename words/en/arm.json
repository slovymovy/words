    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7c61a998-e651-3f1a-a7b9-a8af35657990",
                    "sense_definition": "The upper limb of the human body, extending from the shoulder to the hand.",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She held the sleeping baby gently in her <w>arms</w>.",
                            "target_lang_translations": {
                                "ru": "Она нежно держала спящего ребенка на <w>руках</w>."
                            }
                        },
                        {
                            "text": "He broke his <w>arm</w> while playing football last weekend.",
                            "target_lang_translations": {
                                "ru": "Он сломал <w>руку</w>, играя в футбол в прошлые выходные."
                            }
                        }
                    ],
                    "synonyms": [
                        "sleeve"
                    ],
                    "common_phrases": [
                        "arm in arm",
                        "with open arms",
                        "twist someone's arm",
                        "cost an arm and a leg"
                    ],
                    "target_lang_definitions": {
                        "ru": "Верхняя конечность человеческого тела, простирающаяся от плеча до кисти."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "рука",
                                "target_lang_sense_clarification": "часть тела"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ed195618-9b8f-3cb4-8fc7-7d15efdf379c",
                    "sense_definition": "Weapons and ammunition; armaments.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "military",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The rebels agreed to lay down their <w>arms</w> and negotiate for peace.",
                            "target_lang_translations": {
                                "ru": "Повстанцы согласились сложить <w>оружие</w> и начать мирные переговоры."
                            }
                        },
                        {
                            "text": "The country exports <w>arms</w> to several of its allies.",
                            "target_lang_translations": {
                                "ru": "Страна экспортирует <w>оружие</w> нескольким своим союзникам."
                            }
                        }
                    ],
                    "synonyms": [
                        "beweapon",
                        "arm"
                    ],
                    "common_phrases": [
                        "bear arms",
                        "take up arms",
                        "up in arms",
                        "small arms"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "plural"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Оружие и боеприпасы; вооружение."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "оружие",
                                "target_lang_sense_clarification": "средство нападения или защиты"
                            },
                            {
                                "target_lang_word": "вооружение",
                                "target_lang_sense_clarification": "совокупность оружия"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "75507e08-1c0e-38ac-be12-0a9588938e10",
                    "sense_definition": "A long, narrow part of an object extending from the center, such as on a chair or machine.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "body_part",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He rested his elbows comfortably on the <w>arms</w> of the chair.",
                            "target_lang_translations": {
                                "ru": "Он удобно оперся локтями на <w>подлокотники</w> кресла."
                            }
                        },
                        {
                            "text": "The robot <w>arm</w> moved precisely to assemble the parts.",
                            "target_lang_translations": {
                                "ru": "<w>Рука</w> робота двигалась точно, собирая детали."
                            }
                        }
                    ],
                    "common_phrases": [
                        "arm of a chair",
                        "robot arm"
                    ],
                    "target_lang_definitions": {
                        "ru": "Длинная узкая часть предмета, отходящая от центра, например, у кресла или машины."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подлокотник",
                                "target_lang_sense_clarification": "часть кресла"
                            },
                            {
                                "target_lang_word": "манипулятор",
                                "target_lang_sense_clarification": "часть механизма"
                            },
                            {
                                "target_lang_word": "рука",
                                "target_lang_sense_clarification": "робота"
                            }
                        ]

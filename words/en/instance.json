                },
                {
                    "sense_id": "90c3f015-2596-3a03-8aac-13b13df98ed8",
                    "sense_definition": "A step or stage in a process, often used to denote the initial step in a formal procedure.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "You should report the problem to your supervisor in the first <w>instance</w>.",
                            "target_lang_translations": {

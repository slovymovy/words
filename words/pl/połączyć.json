        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "41cf3c0a-b58f-3ca6-8f54-92ff6b87ccc9",
                    "sense_definition": "Zespolić ze sobą oddzielne elementy, tworząc jedną całość fizyczną lub chemiczną.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musisz <w>połączyć</w> te dwa przewody, aby lampa wreszcie zadziałała.",
                            "target_lang_translations": {
                                "ru": "Тебе нужно <w>соединить</w> эти два провода, чтобы лампа наконец заработала."
                            }
                        },
                        {
                            "text": "Kucharz postanowił <w>połączyć</w> wszystkie składniki w dużej misce.",
                            "target_lang_translations": {
                                "ru": "Повар решил <w>соединить</w> все ингредиенты в большой миске."
                            }
                        }
                    ],
                    "antonyms": [
                        "oddzielić",
                        "rozdzielić"
                    ],
                    "common_phrases": [
                        "połączyć kable",
                        "połączyć siły",
                        "połączyć w całość"
                    ],
                    "target_lang_definitions": {
                        "ru": "Соединить отдельные элементы, создавая единое физическое или химическое целое."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соединить",
                                "target_lang_sense_clarification": "физически"
                            },
                            {
                                "target_lang_word": "скрепить",
                                "target_lang_sense_clarification": "прочно зафиксировать"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8ad36cb8-ff22-3db1-aede-26bea80f09fc",
                    "sense_definition": "Umożliwić rozmowę telefoniczną lub nawiązać kontakt za pomocą urządzeń technicznych.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "communication",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Czy może mnie pani <w>połączyć</w> z działem obsługi klienta?",
                            "target_lang_translations": {
                                "ru": "Можете ли вы <w>соединить</w> меня с отделом обслуживания клиентов?"
                            }
                        },
                        {
                            "text": "Operator nie mógł nas <w>połączyć</w> z powodu awarii sieci.",
                            "target_lang_translations": {
                                "ru": "Оператор не мог нас <w>соединить</w> из-за сбоя в сети."
                            }
                        }
                    ],
                    "common_phrases": [
                        "połączyć z numerem",
                        "połączyć rozmowę",
                        "połączyć z internetem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Обеспечить телефонный разговор или установить контакт с помощью технических устройств."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соединить",
                                "target_lang_sense_clarification": "по телефону"
                            },
                            {
                                "target_lang_word": "подключить",
                                "target_lang_sense_clarification": "к сети или устройству"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "096e07f4-ce43-3f38-b12a-7066a33edb80",
                    "sense_definition": "Stanowić drogę lub przejście umożliwiające przemieszczanie się między dwoma miejscami.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_connection",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Nowy most ma <w>połączyć</w> wyspę ze stałym lądem w przyszłym roku.",
                            "target_lang_translations": {
                                "ru": "Новый мост должен <w>соединить</w> остров с материком в следующем году."
                            }
                        },
                        {
                            "text": "Ta linia kolejowa <w>połączy</w> stolicę z nadmorskimi kurortami.",
                            "target_lang_translations": {
                                "ru": "Эта железнодорожная линия <w>свяжет</w> столицу с приморскими курортами."
                            }
                        }
                    ],
                    "antonyms": [
                        "rozdzielić"
                    ],
                    "common_phrases": [
                        "most połączył",
                        "droga połączyła"
                    ],
                    "target_lang_definitions": {
                        "ru": "Служить путем или переходом, позволяющим перемещаться между двумя местами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "соединить",
                                "target_lang_sense_clarification": "географически"
                            },
                            {
                                "target_lang_word": "связать",
                                "target_lang_sense_clarification": "транспортным сообщением"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9e425350-1fdf-3841-b3b5-6c3ef941ccc3",
                    "sense_definition": "Dostrzec związek między różnymi faktami, zdarzeniami lub osobami; skojarzyć.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_association",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Policja zdołała szybko <w>połączyć</w> te dwa z pozoru różne przestępstwa.",
                            "target_lang_translations": {
                                "ru": "Полиции удалось быстро <w>связать</w> эти два, казалось бы, разных преступления."
                            }
                        },
                        {
                            "text": "Trudno jest logicznie <w>połączyć</w> jego zachowanie z tym, co mówił wcześniej.",
                            "target_lang_translations": {
                                "ru": "Трудно логически <w>связать</w> его поведение с тем, что он говорил ранее."
                            }
                        }
                    ],
                    "common_phrases": [
                        "połączyć fakty",
                        "połączyć wątki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Увидеть связь между различными фактами, событиями или людьми; ассоциировать."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "связать",
                                "target_lang_sense_clarification": "найти логическую связь"
                            },
                            {
                                "target_lang_word": "сопоставить",
                                "target_lang_sense_clarification": "сравнить факты"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f0fda1c3-e903-3b57-ab29-83fd043e57a4",
                    "sense_definition": "Sprawić, że ludzie stają się sobie bliscy, mają wspólne cele lub tworzą wspólnotę.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "social_union",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wspólna pasja do muzyki potrafi <w>połączyć</w> ludzi z różnych kultur.",
                            "target_lang_translations": {
                                "ru": "Общая страсть к музыке способна <w>объединить</w> людей разных культур."
                            }
                        },
                        {
                            "text": "To tragiczne wydarzenie <w>połączyło</w> wszystkich mieszkańców miasta w żałobie.",
                            "target_lang_translations": {
                                "ru": "Это трагическое событие <w>объединило</w> всех жителей города в скорби."
                            }
                        }
                    ],
                    "synonyms": [
                        "zjednoczyć"
                    ],
                    "antonyms": [
                        "rozdzielić"
                    ],
                    "common_phrases": [
                        "połączyć węzłem małżeńskim",
                        "nic nas nie połączy",
                        "połączyć pokolenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Сблизить людей, объединить их общими целями или чувствами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "объединить",
                                "target_lang_sense_clarification": "людей или группы"
                            },
                            {
                                "target_lang_word": "сплотить",
                                "target_lang_sense_clarification": "сделать дружнее"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "a8fc0304-4dd1-377b-8419-2c63146d23a0",
                    "sense_definition": "Wykonywać dwie czynności jednocześnie lub zestawiać ze sobą różne cechy.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "abstract_association",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Bardzo trudno jest skutecznie <w>połączyć</w> pracę na pełen etat z wychowywaniem dzieci.",
                            "target_lang_translations": {
                                "ru": "Очень трудно эффективно <w>совместить</w> работу на полную ставку с воспитанием детей."
                            }
                        },
                        {
                            "text": "W tym projekcie architekt chciał <w>połączyć</w> tradycję z nowoczesnością.",
                            "target_lang_translations": {
                                "ru": "В этом проекте архитектор хотел <w>сочетать</w> традиции с современностью."
                            }
                        }
                    ],
                    "common_phrases": [
                        "połączyć przyjemne z pożytecznym",
                        "połączyć pracę z nauką"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выполнять два действия одновременно или сочетать разные качества."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "совместить",
                                "target_lang_sense_clarification": "разные занятия"
                            },
                            {
                                "target_lang_word": "сочетать",
                                "target_lang_sense_clarification": "разные качества или стили"
                            }
                        ]
                    }
                }
            ]
        }
    ]

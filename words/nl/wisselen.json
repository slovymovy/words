                {
                    "sense_id": "6a622311-1aa7-354d-acc5-0ec09075839b",
                    "sense_definition": "Het ene geven en daarvoor iets anders in de plaats krijgen; ruilen van positie of object.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "exchange_action",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De spelers <w>wisselden</w> na de wedstrijd van shirt.",
                            "target_lang_translations": {
                                "en": "The players <w>swapped</w> shirts after the match.",
                                "ru": "Игроки <w>обменялись</w> футболками после матча."
                            }
                        },
                        {
                            "text": "Zullen wij van plaats <w>wisselen</w> zodat jij bij het raam kunt zitten?",
                            "target_lang_translations": {
                                "en": "Shall we <w>swap</w> places so you can sit by the window?",
                                "ru": "Давай <w>поменяемся</w> местами, чтобы ты мог сесть у окна?"
                            }
                        }
                    ],
                    "synonyms": [
                        "ruilen",
                        "omwisselen",
                        "verruilen"
                    ],
                    "antonyms": [
                        "houden",
                        "behouden"
                    ],
                    "common_phrases": [
                        "van plaats wisselen",
                        "van eigenaar wisselen",
                        "van kleren wisselen"
                    ],
                    "target_lang_definitions": {
                        "en": "To give one thing and receive something else in return; to trade positions or objects.",
                        "ru": "Давать одно и получать взамен другое; менять положение или объект."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "swap",
                                "target_lang_sense_clarification": "to trade objects or places"
                            },

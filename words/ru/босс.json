                    ],
                    "common_phrases": [
                        "большой босс",
                        "строгий босс",
                        "мой босс"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A person in charge of a worker, group, or organization."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "boss",
                                "target_lang_sense_clarification": "person in charge"
                            },
                            {

    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "3e8182f7-95bd-34dc-90f7-0f26ca4088c0",
                    "sense_definition": "Чрезмерно расшалиться, прийти в возбуждённое состояние, активно двигаясь, бегая и прыгая (обычно о детях или животных).",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "behavior",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "К вечеру дети так <w>расскакались</w>, что родители долго не могли их успокоить.",
                            "target_lang_translations": {
                                "en": "By evening, the children <w>got so carried away jumping around</w> that their parents couldn't calm them down for a long time."
                            }
                        },
                        {
                            "text": "Щенок почувствовал свободу и <w>расскакался</w> по всей квартире, сбивая коврики.",
                            "target_lang_translations": {
                                "en": "The puppy felt freedom and <w>started bounding wildly</w> all over the apartment, knocking over rugs."
                            }
                        }
                    ],
                    "synonyms": [
                        "расшалиться",
                        "разыграться",
                        "разойтись",
                        "разбеситься"
                    ],
                    "antonyms": [
                        "успокоиться",
                        "угомониться",
                        "притихнуть"
                    ],
                    "common_phrases": [
                        "дети расскакались",
                        "совсем расскакался"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To become overly energetic, running and jumping around wildly; to get carried away playing (usually said of children or animals)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "start jumping around wildly",
                                "target_lang_sense_clarification": "to become boisterous and active"
                            },
                            {
                                "target_lang_word": "get carried away jumping",
                                "target_lang_sense_clarification": "to lose composure while playing"
                            },
                            {
                                "target_lang_word": "romp around",
                                "target_lang_sense_clarification": "to play roughly and energetically"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4f63b8bf-44fc-3d04-82ee-73b4d9b2d8b8",
                    "sense_definition": "Сильно разогнаться во время скачки верхом или прыжков; начать скакать слишком быстро или увлечённо.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Лошадь почувствовала простор и <w>расскакалась</w> так, что всадник с трудом её сдерживал.",
                            "target_lang_translations": {
                                "en": "The horse felt the open space and <w>broke into such a wild gallop</w> that the rider could barely hold it back."
                            }
                        },
                        {
                            "text": "На старой и уставшей лошади в поле особо не <w>расскачешься</w>.",
                            "target_lang_translations": {
                                "en": "You won't really <w>get a good gallop going</w> in the field on an old and tired horse."
                            }
                        }
                    ],
                    "synonyms": [
                        "разогнаться",
                        "разбежаться"
                    ],
                    "antonyms": [
                        "остановиться"
                    ],
                    "common_phrases": [
                        "конь расскакался",
                        "не расскачешься"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To pick up significant speed while galloping or jumping; to start galloping too fast or enthusiastically."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "break into a wild gallop",
                                "target_lang_sense_clarification": "to start moving very fast on a horse"
                            },
                            {
                                "target_lang_word": "get up to speed",
                                "target_lang_sense_clarification": "to reach a high speed while galloping"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "89110c59-f5fb-3e38-9795-9e4643ef59b1",
                    "sense_definition": "Разлететься или разбежаться в разные стороны, подпрыгивая (о множестве предметов или существ).",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "От сильного удара мелкие камни <w>расскакались</w> по всей дороге.",
                            "target_lang_translations": {
                                "en": "From the strong impact, small stones <w>bounced and scattered</w> all over the road."
                            }
                        },
                        {
                            "text": "Испуганные зайцы мгновенно <w>расскакались</w> кто куда в поисках укрытия.",
                            "target_lang_translations": {
                                "en": "The frightened hares instantly <w>hopped off</w> in all directions looking for cover."
                            }
                        }
                    ],
                    "synonyms": [
                        "разлететься",
                        "рассыпаться"
                    ],
                    "antonyms": [
                        "собраться"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "To scatter or disperse in different directions while hopping or bouncing (of many objects or creatures)."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "scatter bouncing",
                                "target_lang_sense_clarification": "to disperse while bouncing (of objects)"
                            },
                            {
                                "target_lang_word": "hop off in all directions",
                                "target_lang_sense_clarification": "to disperse while hopping (of animals)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
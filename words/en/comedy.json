    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "33a6cf26-99ac-320e-95b1-8d7889803fca",
                    "sense_definition": "Entertainment composed of jokes, satire, or humorous performance.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "genre_entertainment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We decided to go to a club and watch some stand-up <w>comedy</w>.",
                            "target_lang_translations": {
                                "ru": "Мы решили пойти в клуб и посмотреть стендап-<w>комедию</w>."
                            }
                        },
                        {
                            "text": "He prefers watching <w>comedy</w> because it helps him relax after work.",
                            "target_lang_translations": {
                                "ru": "Он предпочитает смотреть <w>комедии</w>, потому что это помогает ему расслабиться после работы."
                            }
                        }
                    ],
                    "antonyms": [
                        "drama",
                        "tragedy"
                    ],
                    "common_phrases": [
                        "stand-up comedy",
                        "comedy show",
                        "sketch comedy",
                        "comedy series"
                    ],
                    "target_lang_definitions": {
                        "ru": "Развлекательный жанр, состоящий из шуток, сатиры или юмористических выступлений"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комедия",
                                "target_lang_sense_clarification": "жанр"
                            },
                            {
                                "target_lang_word": "юмор",
                                "target_lang_sense_clarification": "развлечение"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5eb07b9a-d241-31e1-880e-d2bac0b0b33c",
                    "sense_definition": "A light, amusing play, film, or broadcast program with a happy ending.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "genre_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "A Midsummer Night's Dream is one of Shakespeare's most famous <w>comedies</w>.",
                            "target_lang_translations": {
                                "ru": "«Сон в летнюю ночь» — одна из самых известных <w>комедий</w> Шекспира."
                            }
                        },
                        {
                            "text": "They went to the cinema to see a new romantic <w>comedy</w>.",
                            "target_lang_translations": {
                                "ru": "Они пошли в кино на новую романтическую <w>комедию</w>."
                            }
                        }
                    ],
                    "antonyms": [
                        "tragedy"
                    ],
                    "common_phrases": [
                        "romantic comedy",
                        "situation comedy",
                        "musical comedy",
                        "black comedy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Легкая, забавная пьеса, фильм или передача со счастливым концом"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комедия",
                                "target_lang_sense_clarification": "фильм или пьеса"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "81eb0b17-a403-3062-a612-743deca248f3",
                    "sense_definition": "A humorous event or the funny aspect of a situation.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She failed to see the <w>comedy</w> in the situation when she dropped the cake.",
                            "target_lang_translations": {
                                "ru": "Она не увидела <w>комизма</w> ситуации, когда уронила торт."
                            }
                        },
                        {
                            "text": "The whole trip turned into a <w>comedy</w> of errors due to poor planning.",
                            "target_lang_translations": {
                                "ru": "Вся поездка превратилась в <w>комедию</w> ошибок из-за плохого планирования."
                            }
                        }
                    ],
                    "synonyms": [
                        "humor"
                    ],
                    "common_phrases": [
                        "comedy of errors",
                        "pure comedy"
                    ],
                    "target_lang_definitions": {
                        "ru": "Смешное событие или забавная сторона ситуации"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комизм",
                                "target_lang_sense_clarification": "смешная сторона"
                            },
                            {
                                "target_lang_word": "комичность",
                                "target_lang_sense_clarification": "свойство"
                            },
                            {
                                "target_lang_word": "комедия",
                                "target_lang_sense_clarification": "нелепая ситуация"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7f606126-2743-3a2a-8286-c4dd237e4dab",
                    "sense_definition": "A narrative poem with an agreeable ending.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "literary_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dante's Divine <w>Comedy</w> is a masterpiece of medieval literature.",
                            "target_lang_translations": {
                                "ru": "«Божественная <w>комедия</w>» Данте — шедевр средневековой литературы."
                            }
                        },
                        {
                            "text": "In the classical sense, a <w>comedy</w> does not have to be funny, but it must end happily.",
                            "target_lang_translations": {
                                "ru": "В классическом смысле <w>комедия</w> не обязательно должна быть смешной, но она должна иметь счастливый конец."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "dated",
                            "comment": "literary context"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Повествовательная поэма с благополучным концом (в средневековой литературе)"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "комедия",
                                "target_lang_sense_clarification": "литературная форма"
                            }
                        ]
                    }
                }
            ]
        }
    ]

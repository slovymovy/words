    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "e739d280-730c-3c40-8e52-9d5e8a4be8b7",
                    "sense_definition": "geen partij kiezend in een conflict of discussie",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "impartiality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De <w>neutrale</w> landen boden aan te bemiddelen in het geschil.",
                            "target_lang_translations": {
                                "en": "The <w>neutral</w> countries offered to mediate in the dispute."
                            }
                        },
                        {
                            "text": "Als scheidsrechter moet je altijd <w>neutraal</w> blijven en niemand voortrekken.",
                            "target_lang_translations": {
                                "en": "As a referee, you must always remain <w>neutral</w> and favor no one."
                            }
                        }
                    ],
                    "synonyms": [
                        "onpartijdig"
                    ],
                    "antonyms": [
                        "partijdig"
                    ],
                    "common_phrases": [
                        "neutraal terrein",
                        "neutraal blijven",
                        "zich neutraal opstellen"
                    ],
                    "target_lang_definitions": {
                        "en": "not taking sides in a conflict or discussion"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "neutral",
                                "target_lang_sense_clarification": "not supporting any side"
                            },
                            {
                                "target_lang_word": "impartial",
                                "target_lang_sense_clarification": "treating all parties equally"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "679e2c1a-e6c7-3787-a527-fc30295cb5fa",
                    "sense_definition": "zonder opvallende kenmerken of uitgesproken mening; niet opdringerig",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "general_characteristics",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We hebben de muren in een <w>neutrale</w> kleur geschilderd zodat de meubels beter uitkomen.",
                            "target_lang_translations": {
                                "en": "We painted the walls a <w>neutral</w> color so the furniture stands out better."
                            }
                        },
                        {
                            "text": "Hij gaf een <w>neutraal</w> antwoord waaruit niet bleek wat hij echt dacht.",
                            "target_lang_translations": {
                                "en": "He gave a <w>neutral</w> answer that didn't reveal what he really thought."
                            }
                        }
                    ],
                    "common_phrases": [
                        "neutrale kleuren",
                        "neutrale blik",
                        "neutrale smaak"
                    ],
                    "target_lang_definitions": {

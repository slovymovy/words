    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "79ab0f20-a77b-399a-9ca2-38604745961f",
                    "sense_definition": "Not wearing a hat, specifically a beaver hat.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The gentleman stood <w>unbeavered</w> in the rain after the wind carried his hat away.",
                            "target_lang_translations": {
                                "ru": "Джентльмен стоял <w>с непокрытой головой</w> под дождем после того, как ветер унес его шляпу."
                            }
                        },
                        {
                            "text": "He felt quite cold and exposed while walking <w>unbeavered</w> through the winter streets.",
                            "target_lang_translations": {
                                "ru": "Ему было довольно холодно и неуютно идти <w>без шляпы</w> по зимним улицам."
                            }
                        }
                    ],
                    "synonyms": [
                        "hatless",
                        "bareheaded"
                    ],
                    "antonyms": [
                        "hatted"
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": "refers to a style of hat no longer in common use"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Не носящий шляпу (в частности, высокую бобровую шляпу); с непокрытой головой."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с непокрытой головой",
                                "target_lang_sense_clarification": "без головного убора"
                            },
                            {
                                "target_lang_word": "без шляпы",
                                "target_lang_sense_clarification": "отсутствие шляпы"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0fa0c93a-62a5-3b0f-a010-bf5d920be706",
                    "sense_definition": "Having the face-guard of a helmet raised or open.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The knight sat <w>unbeavered</w> on his horse to breathe fresh air after the joust.",
                            "target_lang_translations": {
                                "ru": "Рыцарь сидел на лошади <w>с поднятым забралом</w>, чтобы вдохнуть свежего воздуха после поединка."
                            }
                        },
                        {
                            "text": "Soldiers could see their commander's face because he rode <w>unbeavered</w> into the camp.",
                            "target_lang_translations": {
                                "ru": "Солдаты могли видеть лицо своего командира, так как он въехал в лагерь <w>с поднятым забралом</w>."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "archaic",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "С поднятым или открытым забралом шлема."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "с поднятым забралом",
                                "target_lang_sense_clarification": "о рыцарском шлеме"
                            },
                            {
                                "target_lang_word": "с открытым лицом",
                                "target_lang_sense_clarification": "в контексте доспехов"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
                        "en": [
                            {
                                "target_lang_word": "right",
                                "target_lang_sense_clarification": "entitlement"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "f6027305-b948-349b-8705-53944801e0d0",
                    "sense_definition": "Het geheel van wetten en regels in een land; de rechtswetenschap.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "legal_system",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mijn zus studeert <w>rechten</w> aan de universiteit in Leiden.",
                            "target_lang_translations": {
                                "ru": "Моя сестра изучает <w>право</w> в университете в Лейдене.",
                                "en": "My sister studies <w>law</w> at the university in Leiden."
                            }
                        },
                        {
                            "text": "Volgens het Nederlands <w>recht</w> is dit contract niet geldig.",
                            "target_lang_translations": {
                                "ru": "Согласно нидерландскому <w>праву</w>, этот контракт недействителен.",
                                "en": "According to Dutch <w>law</w>, this contract is not valid."
                            }
                        }
                    ],
                    "common_phrases": [
                        "rechten studeren",
                        "volgens het recht"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность законов и правил в стране; правовая наука.",
                        "en": "The body of laws and rules in a country; legal science."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "право",
                                "target_lang_sense_clarification": "система законов"
                            },
                            {
                                "target_lang_word": "юриспруденция",
                                "target_lang_sense_clarification": "наука"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "law",
                                "target_lang_sense_clarification": "system of rules"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "8fd0172d-a37e-32f6-9778-b1cc1c185b86",
                    "sense_definition": "Het principe van eerlijkheid en rechtvaardigheid.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "justice",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De rechter moet zorgen dat het <w>recht</w> zegeviert.",
                            "target_lang_translations": {
                                "ru": "Судья должен обеспечить, чтобы <w>справедливость</w> восторжествовала.",
                                "en": "The judge must ensure that <w>justice</w> prevails."
                            }
                        },
                        {
                            "text": "Deze beslissing doet geen <w>recht</w> aan de werkelijke situatie.",
                            "target_lang_translations": {
                                "ru": "Это решение не отдаёт <w>должного</w> реальной ситуации.",
                                "en": "This decision does not do <w>justice</w> to the actual situation."
                            }
                        }
                    ],
                    "antonyms": [
                        "onrecht"
                    ],
                    "common_phrases": [
                        "recht doen aan",
                        "recht spreken"
                    ],
                    "target_lang_definitions": {
                        "ru": "Принцип честности и справедливости.",
                        "en": "The principle of honesty and justice."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "справедливость",
                                "target_lang_sense_clarification": "моральный принцип"
                            },
                            {
                                "target_lang_word": "правосудие",
                                "target_lang_sense_clarification": "судебная деятельность"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "justice",
                                "target_lang_sense_clarification": "moral principle"
                            },
                            {
                                "target_lang_word": "right",
                                "target_lang_sense_clarification": "abstract concept of goodness"
                            }
                        ]
                    }
                }
            ]
        },

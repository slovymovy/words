    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "1a060ec6-9fd4-3a3c-be8c-80791fef63e5",
                    "sense_definition": "The ability to grasp the meaning or nature of something; knowledge about a specific subject.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "cognition",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She has a solid <w>understanding</w> of particle physics.",
                            "target_lang_translations": {
                                "ru": "У нее глубокое <w>понимание</w> физики элементарных частиц."
                            }
                        },
                        {
                            "text": "He lacks a basic <w>understanding</w> of how the computer system works.",
                            "target_lang_translations": {
                                "ru": "У него отсутствует элементарное <w>понимание</w> того, как работает компьютерная система."
                            }
                        }
                    ],
                    "synonyms": [
                        "comprehension",
                        "grasp"
                    ],
                    "common_phrases": [
                        "basic understanding",
                        "clear understanding",
                        "deep understanding",
                        "better understanding"
                    ],
                    "target_lang_definitions": {
                        "ru": "Способность постигать смысл или природу чего-либо; знание определенного предмета."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "понимание",
                                "target_lang_sense_clarification": "способность осмыслять"
                            },
                            {
                                "target_lang_word": "представление",
                                "target_lang_sense_clarification": "знание предмета"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "9ff19143-57a7-3be3-8e36-d414cd357dca",
                    "sense_definition": "An informal agreement or mutual arrangement between two or more parties.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "agreement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I thought we had an <w>understanding</w> regarding who would do the dishes.",
                            "target_lang_translations": {
                                "ru": "Я думал, у нас была <w>договорённость</w> насчет того, кто будет мыть посуду."
                            }
                        },
                        {
                            "text": "The two companies reached an <w>understanding</w> to share their resources.",
                            "target_lang_translations": {
                                "ru": "Две компании достигли <w>договорённости</w> о совместном использовании ресурсов."
                            }
                        }
                    ],
                    "common_phrases": [
                        "come to an understanding",
                        "reach an understanding",
                        "have an understanding"
                    ],
                    "target_lang_definitions": {
                        "ru": "Неформальное соглашение или взаимная договоренность между двумя или более сторонами."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "договорённость",
                                "target_lang_sense_clarification": "устное соглашение"
                            },
                            {
                                "target_lang_word": "взаимопонимание",
                                "target_lang_sense_clarification": "обоюдное согласие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "21507efb-5dd7-37b9-84b3-97ec1e4b1ba3",
                    "sense_definition": "The ability to understand people's feelings and problems and to forgive them; sympathy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "empathy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He showed much <w>understanding</w> for my problems when he heard about my past.",
                            "target_lang_translations": {
                                "ru": "Он проявил большое <w>понимание</w> к моим проблемам, когда узнал о моем прошлом."
                            }
                        },
                        {
                            "text": "We need to treat these customer complaints with patience and <w>understanding</w>.",
                            "target_lang_translations": {
                                "ru": "Мы должны относиться к этим жалобам клиентов с терпением и <w>пониманием</w>."
                            }

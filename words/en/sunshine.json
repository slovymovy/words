    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "145ded1b-31d7-3501-9ce7-92ab096c937f",
                    "sense_definition": "The direct rays, light, or warmth coming from the sun.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "physical_light",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We sat in the garden and enjoyed the warm <w>sunshine</w>.",
                            "target_lang_translations": {
                                "ru": "Мы сидели в саду и наслаждались теплым <w>солнцем</w>."
                            }
                        },
                        {
                            "text": "The bright <w>sunshine</w> woke me up early this morning.",
                            "target_lang_translations": {
                                "ru": "Яркий <w>солнечный свет</w> разбудил меня сегодня рано утром."
                            }
                        }
                    ],
                    "synonyms": [
                        "sunlight",
                        "sun"
                    ],
                    "antonyms": [
                        "darkness",
                        "shade"
                    ],
                    "common_phrases": [
                        "bright sunshine",
                        "warm sunshine",
                        "in the sunshine"
                    ],
                    "target_lang_definitions": {
                        "ru": "Прямые лучи, свет или тепло, исходящие от солнца."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "со́лнечный свет",
                                "target_lang_sense_clarification": "излучение"
                            },
                            {
                                "target_lang_word": "со́лнце",
                                "target_lang_sense_clarification": "свет и тепло"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "2c0ab1cc-1ffe-31e9-95d1-0fc4e823e2d9",
                    "sense_definition": "A specific area or place where the sun's light is falling.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "physical_light",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Let's move our chairs out of the shade and into the <w>sunshine</w>.",
                            "target_lang_translations": {
                                "ru": "Давай переставим стулья из тени на <w>солнце</w>."
                            }
                        },
                        {
                            "text": "The cat found a patch of <w>sunshine</w> on the floor and fell asleep.",
                            "target_lang_translations": {
                                "ru": "Кошка нашла пятно <w>солнечного света</w> на полу и уснула."
                            }
                        }
                    ],
                    "antonyms": [
                        "shade",
                        "shadow"
                    ],
                    "common_phrases": [
                        "into the sunshine",
                        "out of the sunshine"
                    ],
                    "target_lang_definitions": {
                        "ru": "Освещенное солнцем место или пространство."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "со́лнце",
                                "target_lang_sense_clarification": "освещенное место"
                            },
                            {
                                "target_lang_word": "со́лнечная сторона",
                                "target_lang_sense_clarification": "место"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "3982fb17-9d92-3229-b18c-d072862336c5",
                    "sense_definition": "A feeling of cheerfulness, happiness, or a person who brings joy.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_joy",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "She brings a little bit of <w>sunshine</w> into the office every day.",
                            "target_lang_translations": {
                                "ru": "Она каждый день приносит немного <w>радости</w> в офис."
                            }
                        },
                        {
                            "text": "You are the <w>sunshine</w> of my life.",
                            "target_lang_translations": {
                                "ru": "Ты — <w>солнце</w> моей жизни."
                            }
                        }
                    ],
                    "antonyms": [
                        "gloom",
                        "misery"
                    ],
                    "common_phrases": [
                        "ray of sunshine",
                        "bring sunshine"
                    ],
                    "target_lang_definitions": {
                        "ru": "Чувство жизнерадостности, счастья или человек, приносящий радость."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "со́лнышко",
                                "target_lang_sense_clarification": "о человеке"

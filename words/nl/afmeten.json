    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "a08dc956-a196-38a0-bcdf-d0e517cf5f4c",
                    "sense_definition": "Een specifieke hoeveelheid of lengte bepalen en afzonderen door te meten",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "measure_physical",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De apotheker moest de vloeistof zeer nauwkeurig <w>afmeten</w>.",
                            "target_lang_translations": {
                                "en": "The pharmacist had to <w>measure out</w> the liquid very precisely.",
                                "ru": "Аптекарь должен был очень точно <w>отмерить</w> жидкость."
                            }
                        },
                        {
                            "text": "Voor dit recept moet je precies honderd gram suiker <w>afmeten</w>.",
                            "target_lang_translations": {
                                "en": "For this recipe, you must <w>measure out</w> exactly one hundred grams of sugar.",
                                "ru": "Для этого рецепта нужно точно <w>отмерить</w> сто грамм сахара."
                            }
                        }
                    ],
                    "synonyms": [
                        "afpassen",
                        "uitmeten"
                    ],
                    "common_phrases": [
                        "nauwkeurig afmeten",
                        "een hoeveelheid afmeten"
                    ],
                    "target_lang_definitions": {
                        "en": "To determine and separate a specific quantity or length by measuring",
                        "ru": "Определить конкретное количество или длину и отделить их путем измерения"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "measure out",
                                "target_lang_sense_clarification": "a specific amount"
                            },
                            {
                                "target_lang_word": "measure",
                                "target_lang_sense_clarification": "general"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "отмерять",
                                "target_lang_sense_clarification": "отделять часть чего-либо путем измерения"
                            },
                            {
                                "target_lang_word": "отмерить",
                                "target_lang_sense_clarification": "совершенный вид, законченное действие"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "73b9780d-4c2d-3c7d-b2e3-6003db141ed4",
                    "sense_definition": "Iets beoordelen of de waarde ervan bepalen aan de hand van een vergelijking of criterium",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "measure_figurative",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Je kunt iemands succes niet alleen aan zijn salaris <w>afmeten</w>.",
                            "target_lang_translations": {
                                "en": "You cannot <w>measure</w> someone's success solely by their salary.",
                                "ru": "Нельзя <w>оценивать</w> чей-то успех только по его зарплате."
                            }
                        },
                        {
                            "text": "We zullen de vooruitgang <w>afmeten</w> aan de behaalde resultaten van het afgelopen jaar.",
                            "target_lang_translations": {
                                "en": "We will <w>measure</w> the progress against the results achieved last year.",
                                "ru": "Мы будем <w>оценивать</w> прогресс по результатам, достигнутым за прошлый год."
                            }
                        }
                    ],
                    "synonyms": [
                        "beoordelen",
                        "toetsen"
                    ],
                    "common_phrases": [
                        "afmeten aan",
                        "succes afmeten"
                    ],
                    "target_lang_definitions": {
                        "en": "To judge something or determine its value based on a comparison or criterion",
                        "ru": "Оценивать что-либо или определять значимость на основе сравнения или критерия"
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "measure (by/against)",
                                "target_lang_sense_clarification": "judge based on criteria"
                            },
                            {
                                "target_lang_word": "judge (by)",
                                "target_lang_sense_clarification": "assess value"
                            }
                        ],
                        "ru": [
                            {
                                "target_lang_word": "оценивать",
                                "target_lang_sense_clarification": "судить о качестве или значении на основе чего-либо"
                            },
                            {
                                "target_lang_word": "судить",
                                "target_lang_sense_clarification": "составлять мнение, основываясь на критериях"
                            },
                            {
                                "target_lang_word": "измерять",
                                "target_lang_sense_clarification": "в переносном смысле: определять степень или величину"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
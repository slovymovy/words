    ],
    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "454fbb1e-712c-3570-bad5-3a96127597e3",
                    "sense_definition": "Not confirmed or authorized by an official source.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "not_authorized",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>unofficial</w> results suggest that the current mayor has won the election.",
                            "target_lang_translations": {
                                "ru": "<w>Неофициальные</w> результаты указывают на то, что действующий мэр победил на выборах."
                            }
                        },
                        {
                            "text": "We received an <w>unofficial</w> report about the incident before the police arrived.",
                            "target_lang_translations": {
                                "ru": "Мы получили <w>неофициальное</w> сообщение об инциденте до прибытия полиции."
                            }
                        }
                    ],
                    "synonyms": [
                        "inofficial"
                    ],
                    "antonyms": [
                        "official"
                    ],
                    "common_phrases": [
                        "unofficial results",
                        "unofficial sources",
                        "unofficial visit"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не подтвержденный или не санкционированный официальным источником."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неофициальный",
                                "target_lang_sense_clarification": "предварительный, не подтвержденный властями"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "945ca48c-1479-3d5a-b105-967fd6cc059a",
                    "sense_definition": "Not holding a formal position or acting with formal authority.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "not_authorized",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He served as an <w>unofficial</w> advisor to the president during the crisis.",
                            "target_lang_translations": {
                                "ru": "Во время кризиса он был <w>неофициальным</w> советником президента."
                            }
                        },
                        {
                            "text": "The workers took <w>unofficial</w> action by walking off the job without union approval.",
                            "target_lang_translations": {
                                "ru": "Рабочие предприняли <w>неофициальную</w> акцию, покинув рабочие места без одобрения профсоюза."
                            }
                        }
                    ],
                    "antonyms": [
                        "official"
                    ],
                    "common_phrases": [
                        "unofficial advisor",
                        "unofficial capacity",
                        "unofficial action"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не занимающий официальной должности или действующий без официальных полномочий."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неофициальный",
                                "target_lang_sense_clarification": "частный, без формальных полномочий"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "83098c30-c60b-3c52-8417-f4891241e7e0",
                    "sense_definition": "True or existing in reality but not acknowledged by those in authority.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "not_authorized",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "There is an <w>unofficial</w> policy that employees should not discuss their salaries.",
                            "target_lang_translations": {
                                "ru": "Существует <w>негласная</w> политика, согласно которой сотрудники не должны обсуждать свои зарплаты."
                            }
                        },
                        {
                            "text": "The <w>unofficial</w> reason for his resignation was a conflict with the board of directors.",
                            "target_lang_translations": {
                                "ru": "<w>Неофициальной</w> причиной его отставки был конфликт с советом директоров."
                            }
                        }
                    ],
                    "antonyms": [
                        "official"
                    ],
                    "common_phrases": [
                        "unofficial policy",
                        "unofficial rule"
                    ],
                    "target_lang_definitions": {
                        "ru": "Существующий в действительности, но не признаваемый руководством или властями."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "негласный",
                                "target_lang_sense_clarification": "тайный, не афишируемый"
                            },
                            {
                                "target_lang_word": "неофициальный",
                                "target_lang_sense_clarification": "существующий, но не признанный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "cd3ecc76-6387-32a8-9c8e-6456fc920645",
                    "sense_definition": "Not listed in a national pharmacopeia or standard reference.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The pharmacist prepared an <w>unofficial</w> remedy for the patient's skin condition.",
                            "target_lang_translations": {
                                "ru": "Фармацевт приготовил <w>неофициальное</w> средство для лечения кожного заболевания пациента."
                            }
                        },
                        {
                            "text": "This substance is considered an <w>unofficial</w> drug because it is not in the registry.",
                            "target_lang_translations": {
                                "ru": "Это вещество считается <w>неофициальным</w> препаратом, так как его нет в реестре."
                            }
                        }
                    ],
                    "antonyms": [
                        "official"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не включенный в государственную фармакопею или стандартный справочник."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "неофициальный",
                                "target_lang_sense_clarification": "не включенный в фармакопею"
                            },
                            {
                                "target_lang_word": "нефармакопейный",
                                "target_lang_sense_clarification": "специальный медицинский термин"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
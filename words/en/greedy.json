                {
                    "sense_id": "3d8c7001-3325-39fa-bc7f-908ac351ea65",
                    "sense_definition": "Wanting to eat or drink more than is necessary or polite.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The <w>greedy</w> child ate all the cookies before the guests arrived.",
                            "target_lang_translations": {
                                "ru": "<w>Жадный</w> ребёнок съел всё печенье до прихода гостей."
                            }
                        },
                        {
                            "text": "Don't be a <w>greedy</w> pig and leave some pizza for your brother.",
                            "target_lang_translations": {
                                "ru": "Не будь <w>жадной</w> свиньёй и оставь немного пиццы брату."
                            }
                        }
                    ],
                    "synonyms": [
                        "gluttonous",
                        "piggish",
                        "voracious"
                    ],
                    "common_phrases": [
                        "greedy eater",
                        "greedy pig"
                    ],
                    "target_lang_definitions": {

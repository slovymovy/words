{
    "word_family": [
        "obchodzić",
        "obejście",
        "obchód",
        "obchodowy"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "2da60e7e-b282-3eb1-987d-6b6feb37106c",
                    "sense_definition": "Przejść dookoła jakiegoś miejsca, obiektu lub osoby.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Musieliśmy <obejść> całe jezioro, aby znaleźć dogodne miejsce na biwak.",
                            "target_lang_translations": {
                                "ru": "Нам пришлось <обойти> всё озеро, чтобы найти удобное место для лагеря."
                            }
                        },
                        {
                            "text": "Postanowił <obejść> dom dookoła, sprawdzając wszystkie okna.",
                            "target_lang_translations": {
                                "ru": "Он решил <обойти> дом кругом, проверяя все окна."
                            }
                        }
                    ],
                    "synonyms": [
                        "okrążyć"
                    ],
                    "common_phrases": [
                        "obejść jezioro",
                        "obejść dom",
                        "obejść przeszkodę",
                        "obejść miasto"
                    ],
                    "target_lang_definitions": {
                        "ru": "Пройти вокруг какого-либо места, объекта или человека."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обойти",
                                "target_lang_sense_clarification": "пройти вокруг чего-либо"
                            },
                            {
                                "target_lang_word": "окружить",
                                "target_lang_sense_clarification": "стать или пройти вокруг"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0a3a6821-5568-3c54-ac1e-73e82c8145d2",
                    "sense_definition": "Poradzić sobie bez posiadania czegoś lub bez czyjejś pomocy; używane z zaimkiem \"się\".",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "reflexive_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W dzisiejszych czasach trudno <obejść> się bez telefonu komórkowego.",
                            "target_lang_translations": {
                                "ru": "В наше время трудно <обойтись> без мобильного телефона."
                            }
                        },
                        {
                            "text": "Niestety musimy <obejść> się smakiem, bo ciasto już się skończyło.",
                            "target_lang_translations": {
                                "ru": "К сожалению, нам придётся <обойтись> без этого (облизнуться), так как пирог уже закончился."
                            }
                        }
                    ],
                    "synonyms": [
                        "poradzić sobie"
                    ],
                    "common_phrases": [
                        "obejść się bez czegoś",
                        "obejść się smakiem"
                    ],
                    "target_lang_definitions": {
                        "ru": "Справиться без наличия чего-либо или без чьей-либо помощи; используется с возвратным местоимением \"się\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обойтись",
                                "target_lang_sense_clarification": "справиться без чего-либо"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "7e0cfb8b-a4a3-3d87-a336-98ba7e033ed9",
                    "sense_definition": "Uniknąć przeszkód, zakazów lub trudności poprzez znalezienie innego, często sprytnego sposobu działania.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "abstract_bypass",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Doświadczony prawnik pomógł im legalnie <obejść> nowe przepisy podatkowe.",
                            "target_lang_translations": {
                                "ru": "Опытный юрист помог им легально <обойти> новые налоговые правила."
                            }
                        },
                        {
                            "text": "Hakerom udało się <obejść> zabezpieczenia systemowe banku.",
                            "target_lang_translations": {
                                "ru": "Хакерам удалось <обойти> систему безопасности банка."
                            }
                        }
                    ],
                    "synonyms": [
                        "ominąć",
                        "wymanewrować"
                    ],
                    "common_phrases": [
                        "obejść prawo",
                        "obejść przepisy",
                        "obejść system",
                        "obejść zabezpieczenia"
                    ],
                    "target_lang_definitions": {
                        "ru": "Избежать препятствий, запретов или трудностей, найдя другой, часто хитрый способ действия."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обойти",
                                "target_lang_sense_clarification": "миновать, перехитрить (закон, систему)"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ad3fe632-e318-3905-b77c-97bdbd61c93b",
                    "sense_definition": "Postąpić z kimś lub czymś w określony sposób, potraktować kogoś; używane z zaimkiem \"się\".",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "reflexive_usage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Z tym delikatnym urządzeniem należy <obejść> się bardzo ostrożnie.",
                            "target_lang_translations": {
                                "ru": "С этим хрупким устройством следует <обращаться> очень осторожно."
                            }
                        },
                        {
                            "text": "Nie powinieneś tak surowo <obejść> się ze swoim nowym pracownikiem.",
                            "target_lang_translations": {
                                "ru": "Тебе не следует так сурово <обходиться> со своим новым сотрудником."
                            }
                        }
                    ],
                    "synonyms": [
                        "potraktować"
                    ],
                    "common_phrases": [
                        "obejść się łagodnie",
                        "obejść się brutalnie",
                        "obejść się z czymś"
                    ],
                    "target_lang_definitions": {
                        "ru": "Поступить с кем-то или чем-то определенным образом; используется с возвратным местоимением \"się\"."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "обойтись",
                                "target_lang_sense_clarification": "поступить с кем-либо"
                            },
                            {
                                "target_lang_word": "обращаться",
                                "target_lang_sense_clarification": "вести себя по отношению к кому-либо/чему-либо"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "656230c1-2dcf-32a9-91c1-6d313418703a",
                    "sense_definition": "Forma dopełniacza liczby mnogiej rzeczownika <w>obejście</w>.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "grammar_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "W tej wsi jest tylko kilka starych <obejść> gospodarskich.",
                            "target_lang_translations": {
                                "ru": "В этой деревне есть только несколько старых хозяйских <подворий>."
                            }
                        },
                        {
                            "text": "Większość tych wiejskich <obejść> została już dawno opuszczona.",
                            "target_lang_translations": {
                                "ru": "Большинство этих деревенских <усадеб> было уже давно заброшено."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "genitive plural of obejście"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Форма родительного падежа множественного числа существительного \"obejście\" (двор, усадьба)."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "подворий",
                                "target_lang_sense_clarification": "родительный падеж мн.ч. от \"подворье\""
                            },
                            {
                                "target_lang_word": "усадеб",
                                "target_lang_sense_clarification": "родительный падеж мн.ч. от \"усадьба\""
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
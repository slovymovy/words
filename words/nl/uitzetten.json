                {
                    "sense_id": "9987cc61-1ce5-3c0a-9444-f094e272b7a8",
                    "sense_definition": "Dieren vrijlaten in de natuur om daar te leven",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "release",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De boswachters gaan morgen nieuwe herten <w>uitzetten</w> in het natuurgebied.",
                            "target_lang_translations": {
                                "ru": "Лесники собираются завтра <w>выпустить</w> новых оленей в заповеднике.",
                                "en": "The rangers are going to <w>release</w> new deer in the nature reserve tomorrow."
                            }
                        },
                        {
                            "text": "Er is veel jonge vis <w>uitgezet</w> in de rivier om de populatie te herstellen.",
                            "target_lang_translations": {
                                "ru": "В реку было <w>выпущено</w> много молодняка рыбы для восстановления популяции.",
                                "en": "A lot of young fish have been <w>released</w> into the river to restore the population."
                            }
                        }
                    ],
                    "synonyms": [
                        "vrijlaten"
                    ],
                    "common_phrases": [
                        "vis uitzetten",
                        "dieren uitzetten"
                    ],
                    "target_lang_definitions": {
                        "ru": "Выпускать животных в дикую природу для обитания",
                        "en": "To release animals into the wild to live there"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выпускать",
                                "target_lang_sense_clarification": "животных"
                            }
                        ],
                        "en": [
                            {
                                "target_lang_word": "release",
                                "target_lang_sense_clarification": "animals into nature"
                            }
                        ]
                    }

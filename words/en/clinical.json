    "entries": [
        {
            "pos": "adjective",
            "senses": [
                {
                    "sense_id": "c7bde463-0481-39b8-8534-2e241063d19f",
                    "sense_definition": "Relating to the examination and treatment of patients rather than theoretical studies or research.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "medical_context",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The doctor has extensive <w>clinical</w> experience in treating this disease.",
                            "target_lang_translations": {
                                "ru": "У врача есть обширный <w>клинический</w> опыт лечения этого заболевания."
                            }
                        },
                        {
                            "text": "She is a <w>clinical</w> psychologist who helps people manage their anxiety.",
                            "target_lang_translations": {
                                "ru": "Она <w>клинический</w> психолог, который помогает людям справляться с тревожностью."
                            }
                        }
                    ],
                    "antonyms": [
                        "nonclinical"
                    ],
                    "common_phrases": [
                        "clinical trial",
                        "clinical practice",
                        "clinical research",
                        "clinical psychology",
                        "clinical depression"
                    ],
                    "target_lang_definitions": {
                        "ru": "Относящийся к обследованию и лечению пациентов, а не к теоретическим исследованиям."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "клинический",
                                "target_lang_sense_clarification": "медицинский термин"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "035c434d-f7c6-374f-be45-04040dd57dcc",
                    "sense_definition": "Showing no emotion or sympathy; cold, objective, and detached.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "attitude",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "He described the accident with <w>clinical</w> detachment, showing no sign of sadness.",
                            "target_lang_translations": {
                                "ru": "Он описал аварию с <w>холодной</w> отстраненностью, не выказывая признаков печали."
                            }
                        },
                        {
                            "text": "Her assessment of the situation was cold and <w>clinical</w>.",
                            "target_lang_translations": {
                                "ru": "Её оценка ситуации была холодной и <w>бесстрастной</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "unemotional",
                        "detached",
                        "dispassionate",
                        "emotionless",
                        "cold-blooded",
                        "unsentimental",
                        "unfeeling"
                    ],
                    "common_phrases": [
                        "clinical detachment",
                        "clinical coolness"
                    ],
                    "target_lang_definitions": {
                        "ru": "Не проявляющий эмоций или сочувствия; холодный, объективный и отстраненный."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бесстрастный",
                                "target_lang_sense_clarification": "лишенный эмоций"
                            },
                            {
                                "target_lang_word": "холодный",
                                "target_lang_sense_clarification": "об отношении или взгляде"
                            },
                            {
                                "target_lang_word": "сухой",
                                "target_lang_sense_clarification": "сдержанный, безэмоциональный"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "80072345-21a7-376a-87ca-3ef3af1cace4",
                    "sense_definition": "Very efficient, accurate, and effective, often without making mistakes.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "quality",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The team produced a <w>clinical</w> performance to win the championship match.",

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "7a36d431-d0a0-31cc-b015-e54cad7f57df",
                    "sense_definition": "But wyposażony w cztery kółka (zazwyczaj ułożone w dwóch rzędach), służący do rekreacyjnej lub sportowej jazdy po twardej nawierzchni.",
                    "learner_level": "A2",
                    "frequency": "Middle",
                    "semantic_group_id": "sport_equipment",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Moja młodsza siostra dostała na urodziny nowe, kolorowe <w>wrotki</w>.",
                            "target_lang_translations": {
                                "ru": "Моя младшая сестра получила на день рождения новые разноцветные <w>роликовые коньки</w>."
                            }
                        },
                        {
                            "text": "W każdy słoneczny weekend lubimy jeździć na <w>wrotkach</w> w pobliskim parku.",
                            "target_lang_translations": {
                                "ru": "В каждые солнечные выходные мы любим кататься на <w>роликах</w> в соседнем парке."
                            }
                        }
                    ],
                    "common_phrases": [
                        "jeździć na wrotkach",
                        "para wrotek"
                    ],
                    "target_lang_definitions": {
                        "ru": "Ботинок с четырьмя колесами (обычно расположенными в два ряда), предназначенный для любительского или спортивного катания по твердой поверхности."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "роликовый конёк",
                                "target_lang_sense_clarification": "спортивный инвентарь"
                            },
                            {
                                "target_lang_word": "ролики",
                                "target_lang_sense_clarification": "разговорное"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "00e5ed05-9aac-39ce-abe5-83fb9cdcdc00",
                    "sense_definition": "Zdrobnienie od słowa <w>wrota</w>; niewielka brama lub drzwiczki w ogrodzeniu.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "architecture",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Drewniana <w>wrotka</w> prowadząca do ogrodu cicho skrzypnęła przy otwieraniu.",
                            "target_lang_translations": {
                                "ru": "Деревянная <w>калитка</w>, ведущая в сад, тихо скрипнула, когда её открывали."
                            }
                        },
                        {
                            "text": "Gospodarz zamknął <w>wrotkę</w>, aby zwierzęta nie wybiegły na drogę.",
                            "target_lang_translations": {
                                "ru": "Хозяин закрыл <w>калитку</w>, чтобы животные не выбежали на дорогу."
                            }
                        }
                    ],
                    "synonyms": [
                        "roller"
                    ],
                    "traits": [
                        {
                            "trait_type": "regional",
                            "comment": "często używane w kontekście wiejskim"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от слова «wrota» (ворота); небольшие ворота или дверца в ограждении."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "калитка",
                                "target_lang_sense_clarification": "дверца в заборе"
                            },
                            {
                                "target_lang_word": "воротца",
                                "target_lang_sense_clarification": "уменьшительное от ворота"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "eeea5fcb-373e-3e35-bdce-c53f1b3793b1",
                    "sense_definition": "Linijka tekstu w nagłówku wiadomości e-mail lub posta, informująca o autorze cytowanego fragmentu.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "internet_slang",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Twój program pocztowy niepoprawnie wyświetla <w>wrotkę</w> przy odpowiedzi.",
                            "target_lang_translations": {
                                "ru": "Твоя почтовая программа неправильно отображает <w>строку цитирования</w> при ответе."
                            }
                        },
                        {
                            "text": "Zwróć uwagę na <w>wrotkę</w>, żeby wiedzieć, kto napisał te słowa.",
                            "target_lang_translations": {
                                "ru": "Обрати внимание на <w>строку цитирования</w>, чтобы знать, кто написал эти слова."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "slang",
                            "comment": ""
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Строка текста в заголовке электронного письма или поста, указывающая на автора цитируемого фрагмента."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "строка цитирования",
                                "target_lang_sense_clarification": "в электронной переписке"
                            },
                            {
                                "target_lang_word": "атрибуция",
                                "target_lang_sense_clarification": "указание авторства"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "c553c0cd-af60-38fe-ac67-96bc73cad6bf",
                    "sense_definition": "Forma gramatyczna (dopełniacz lub biernik) słowa <w>wrotek</w>, oznaczającego drobny organizm wodny.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "biology_form",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Pod mikroskopem uczniowie obserwowali budowę małego <w>wrotka</w>.",
                            "target_lang_translations": {
                                "ru": "Под микроскопом ученики рассматривали строение маленькой <w>коловратки</w>."
                            }
                        },
                        {
                            "text": "Nigdy wcześniej nie widziałem tak wyraźnie żywego <w>wrotka</w> w kropli wody.",
                            "target_lang_translations": {
                                "ru": "Я никогда раньше не видел так отчетливо живую <w>коловратку</w> в капле воды."
                            }
                        }
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "od: wrotek"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Грамматическая форма (родительный или винительный падеж) слова «wrotek», обозначающего мелкий водный организм."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "коловратка",
                                "target_lang_sense_clarification": "микроскопический организм"
                            }
                        ]
                    }
                }
            ]
        }
    ]

                            {
                                "target_lang_word": "скамья-ларь",
                                "target_lang_sense_clarification": "мебель"
                            },
                            {
                                "target_lang_word": "скамья",
                                "target_lang_sense_clarification": "деревянная"
                            }
                        ]
                    }
                }
            ]
        },

    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "ed364f9b-446e-3279-9d0f-d7351d49b0ef",
                    "sense_definition": "Апоплексический удар, острое нарушение мозгового кровообращения.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "stroke_death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Года через два-три вы умрёте от <w>кондрашки</w>, или я вспылю и всажу вам пулю в лоб.",
                            "target_lang_translations": {
                                "en": "In two or three years you will die of a <w>stroke</w>, or I will lose my temper and put a bullet in your forehead."
                            }
                        },
                        {
                            "text": "Была со мной, как говорится, лёгкая <w>кондрашка</w> вроде кровоизлияния в мозг.",
                            "target_lang_translations": {
                                "en": "I had, as they say, a mild <w>stroke</w>, something like a cerebral hemorrhage."
                            }
                        }
                    ],
                    "synonyms": [
                        "инсульт",
                        "апоплексический удар",
                        "кондратий"
                    ],
                    "common_phrases": [
                        "кондрашка хватил",
                        "кондрашка пришиб"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "просторечное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Apoplectic stroke, acute violation of cerebral circulation."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "stroke",
                                "target_lang_sense_clarification": "medical condition"
                            },
                            {
                                "target_lang_word": "apoplexy",
                                "target_lang_sense_clarification": "dated or colloquial term for a stroke"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78a5df60-7c9b-39e1-94b0-8f5d7e4c1e2c",
                    "sense_definition": "Внезапная смерть.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "stroke_death",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Толпы пожилых гражданок прогуливались по кладбищу в ожидании <w>кондрашки</w>.",
                            "target_lang_translations": {
                                "en": "Crowds of elderly female citizens strolled through the cemetery waiting for <w>sudden death</w>."
                            }
                        },
                        {
                            "text": "Она жила в надежде на то, что майора <w>кондрашка</w> хватит.",
                            "target_lang_translations": {
                                "en": "She lived in the hope that <w>sudden death</w> would strike the major."
                            }
                        }
                    ],
                    "synonyms": [
                        "косая"
                    ],
                    "common_phrases": [
                        "ждать кондрашку"
                    ],
                    "traits": [
                        {
                            "trait_type": "colloquial",
                            "comment": "просторечное"
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "Sudden death."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "sudden death",
                                "target_lang_sense_clarification": "unexpected end of life"
                            }
                        ]
                    }
                }
            ]
        },
        {

{
    "word_family": [
        "ploeg",
        "ploegen",
        "ploeger"
    ],
    "entries": [
        {
            "pos": "verb",
            "senses": [
                {
                    "sense_id": "b9ce94b0-7150-3673-ab68-d4b5c1bee11d",
                    "sense_definition": "Een grote hoeveelheid tekst, informatie of werk met veel inspanning en aandacht helemaal doornemen.",
                    "learner_level": "C1",
                    "frequency": "Middle",
                    "semantic_group_id": "figurative_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "De student moest drie dikke boeken <w>doorploegen</w> voor het eindexamen.",
                            "target_lang_translations": {
                                "en": "The student had to <w>plough through</w> three thick books for the final exam."
                            }
                        },
                        {
                            "text": "Ik heb het hele archief <w>doorgeploegd</w> om dat ene document te vinden.",
                            "target_lang_translations": {
                                "en": "I <w>ploughed through</w> the entire archive to find that one document."
                            }
                        }
                    ],
                    "synonyms": [
                        "doorworstelen",
                        "uitspitten",
                        "bestuderen"
                    ],
                    "antonyms": [
                        "overslaan",
                        "globaal bekijken"
                    ],
                    "common_phrases": [
                        "dossiers doorploegen",
                        "de stof doorploegen",
                        "stapels werk doorploegen"
                    ],
                    "target_lang_definitions": {
                        "en": "To go through a large amount of text, information, or work with great effort and attention."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "plough through",
                                "target_lang_sense_clarification": "work through a large amount of material"
                            },
                            {
                                "target_lang_word": "wade through",
                                "target_lang_sense_clarification": "read or deal with something tedious or long"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "e88a11e3-ee14-389c-a1d8-021decc5b6d5",
                    "sense_definition": "Ondanks moeilijkheden, weerstand of vermoeidheid blijven doorgaan met een zware taak of tocht.",
                    "learner_level": "B2",
                    "frequency": "Low",
                    "semantic_group_id": "figurative_work",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "We moeten nog even <w>doorploegen</w> als we het project op tijd af willen hebben.",
                            "target_lang_translations": {
                                "en": "We need to <w>plough on</w> for a bit if we want to finish the project on time."
                            }
                        },
                        {
                            "text": "De wandelaars bleven stug <w>doorploegen</w> door de diepe modder.",
                            "target_lang_translations": {
                                "en": "The hikers kept stubbornly <w>ploughing</w> through the deep mud."
                            }
                        }
                    ],
                    "synonyms": [
                        "voortploeteren",
                        "doorzetten",
                        "volhouden"
                    ],
                    "antonyms": [
                        "opgeven",
                        "stoppen"
                    ],
                    "common_phrases": [
                        "stug doorploegen",
                        "blijven doorploegen"
                    ],
                    "target_lang_definitions": {
                        "en": "To continue with a heavy task or journey despite difficulties, resistance, or fatigue."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "plough on",
                                "target_lang_sense_clarification": "continue working despite difficulties"
                            },
                            {
                                "target_lang_word": "slog away",
                                "target_lang_sense_clarification": "work hard and steadily"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "4889f4bb-e6fd-3f49-8bd4-f3bc741557d3",
                    "sense_definition": "Met kracht ergens doorheen snijden of bewegen, vaak gezegd van schepen in het water of een ploeg in de aarde.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "physical_movement",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Het grote schip <w>doorploegde</w> de woeste golven van de oceaan.",
                            "target_lang_translations": {
                                "en": "The large ship <w>ploughed through</w> the fierce waves of the ocean."
                            }
                        },
                        {
                            "text": "De boer heeft de akker volledig <w>doorploegd</w> om het land vruchtbaar te maken.",
                            "target_lang_translations": {
                                "en": "The farmer has completely <w>ploughed</w> the field to make the land fertile."
                            }
                        }
                    ],
                    "synonyms": [
                        "doorklieven",
                        "omploegen"
                    ],
                    "common_phrases": [
                        "de zee doorploegen",
                        "de golven doorploegen"
                    ],
                    "target_lang_definitions": {
                        "en": "To cut or move through something with force, often said of ships in water or a plow in the earth."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "plough",
                                "target_lang_sense_clarification": "turn over earth with a plow"
                            },
                            {
                                "target_lang_word": "cleave",
                                "target_lang_sense_clarification": "cut through water (literary)"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
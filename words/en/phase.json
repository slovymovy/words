                {
                    "sense_id": "82e5d8ea-ebf5-3c82-8761-606a9b81e452",
                    "sense_definition": "A distinguishable part of a sequence, cycle, or process occurring over time.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "process_stage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The project is currently in its final <w>phase</w> of development.",
                            "target_lang_translations": {
                                "ru": "В настоящее время проект находится на завершающем <w>этапе</w> разработки."
                            }

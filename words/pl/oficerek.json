    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "55daac8e-e3de-3a49-980d-3840630b320f",
                    "sense_definition": "Zdrobnienie od słowa <w>oficer</w>, oznaczające młodego lub niższego stopniem oficera.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "people",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Do naszego sklepu przy kantynie przybiegali <w>oficerkowie</w> na przerwie podczas wykładów.",
                            "target_lang_translations": {
                                "ru": "В наш магазин при столовой прибегали <w>офицерики</w> во время перерыва между лекциями."
                            }
                        },
                        {
                            "text": "Ten młody <w>oficerek</w> nie miał jeszcze doświadczenia na polu walki.",
                            "target_lang_translations": {
                                "ru": "У этого молодого <w>офицерика</w> еще не было боевого опыта."
                            }
                        }
                    ],
                    "common_phrases": [
                        "młody oficerek"
                    ],
                    "traits": [
                        {
                            "trait_type": "form",
                            "comment": "diminutive"
                        }
                    ],
                    "target_lang_definitions": {
                        "ru": "Уменьшительная форма от слова <w>офицер</w>, обозначающая молодого или младшего по званию офицера."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "офицерик",
                                "target_lang_sense_clarification": "уменьшительно или пренебрежительно"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "02930f76-0c99-3864-8fda-8b4e75c7fb85",
                    "sense_definition": "Wysoki but skórzany z twardą cholewą, noszony przez wojskowych lub do jazdy konnej.",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "clothing",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Chyba pójdę na zbiórkę w <w>oficerkach</w>, bo mamy wieczorem apel przy pomniku.",
                            "target_lang_translations": {
                                "ru": "Наверное, я пойду на построение в <w>офицерских сапогах</w>, так как вечером у нас перекличка у памятника."
                            }
                        },
                        {
                            "text": "Do profesjonalnej jazdy konnej najlepiej kupić wygodne, skórzane <w>oficerki</w>.",
                            "target_lang_translations": {
                                "ru": "Для профессиональной верховой езды лучше всего купить удобные кожаные <w>сапоги</w>."
                            }
                        }
                    ],
                    "common_phrases": [
                        "czarne oficerki",
                        "skórzane oficerki",
                        "nosić oficerki"
                    ],
                    "target_lang_definitions": {
                        "ru": "Высокий кожаный сапог с жестким голенищем, который носят военные или используют для верховой езды."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "офицерский сапог",
                                "target_lang_sense_clarification": "обувь"
                            },
                            {
                                "target_lang_word": "сапог для верховой езды",
                                "target_lang_sense_clarification": "конный спорт"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
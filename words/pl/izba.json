                },
                {
                    "sense_id": "228d1d82-92cc-3d3b-84cf-755522fac8e9",
                    "sense_definition": "Dawna jednostka miary liczby futer, równa 40 sztukom.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "measurements",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Kupiec przywiózł ze wschodu całą <izbę> soboli na sprzedaż.",
                            "target_lang_translations": {
                                "ru": "Купец привез с востока целый <сорок> соболей на продажу."
                            }

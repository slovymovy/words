                {
                    "sense_id": "94197bf3-6958-34b3-8307-96cf1b6fa3b8",
                    "sense_definition": "Jedyny Bóg w religiach monoteistycznych, uznawany za stwórcę i władcę wszechświata",
                    "learner_level": "A1",
                    "frequency": "High",
                    "semantic_group_id": "monotheistic_god",
                    "name_type": "deity_or_religious_name",
                    "examples": [
                        {
                            "text": "Wierzę, że <w>Bóg</w> czuwa nad moją rodziną.",
                            "target_lang_translations": {
                                "ru": "Я верю, что <w>Бог</w> оберегает мою семью."
                            }
                        },
                        {
                            "text": "Dzięki <w>Bogu</w> nic złego się nie stało.",
                            "target_lang_translations": {
                                "ru": "Слава <w>Богу</w>, ничего плохого не случилось."
                            }
                        }
                    ],
                    "synonyms": [
                        "Pan",
                        "Stwórca",
                        "Najwyższy",
                        "Pan Bóg",
                        "Wszechmogący",
                        "Opatrzność",
                        "Bozia"
                    ],
                    "antonyms": [
                        "diabeł",
                        "lucyfer",
                        "belzebub"
                    ],
                    "common_phrases": [
                        "dzięki Bogu",
                        "broń Boże",
                        "Szczęść Boże",
                        "niech to Bóg broni",
                        "jak trwoga to do Boga"
                    ],
                    "target_lang_definitions": {
                        "ru": "Единый Бог в монотеистических религиях, признаваемый творцом и правителем вселенной"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "Бог",
                                "target_lang_sense_clarification": "Творец, Всевышний"
                            }
                        ]
                    }
                }
            ]
        },
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0715ab79-607f-37ea-9753-badcab397cb3",
                    "sense_definition": "W wierzeniach politeistycznych: istota nadprzyrodzona mająca wpływ na losy ludzi i zjawiska przyrody",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "polytheistic_deity",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Mars był rzymskim <w>bogiem</w> wojny.",
                            "target_lang_translations": {
                                "ru": "Марс был римским <w>богом</w> войны."
                            }
                        },
                        {
                            "text": "Starożytni Grecy wierzyli w wielu różnych <w>bogów</w>.",
                            "target_lang_translations": {
                                "ru": "Древние греки верили во множество разных <w>богов</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "bóstwo",
                        "bożyszcze",
                        "bożec"
                    ],
                    "common_phrases": [
                        "bóg wojny",
                        "bóg słońca",
                        "bóg płodności"
                    ],
                    "target_lang_definitions": {
                        "ru": "В политеистических верованиях: сверхъестественное существо, влияющее на судьбы людей и природные явления"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бог",
                                "target_lang_sense_clarification": "божество"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "0f27e660-bcc4-3c72-be1c-6fefa13e68a1",
                    "sense_definition": "Osoba niezwykle podziwiana, uwielbiana i otaczana czcią przez innych ludzi",
                    "learner_level": "C1",
                    "frequency": "Low",
                    "semantic_group_id": "metaphorical_idol",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Dla wielu nastolatków ten piosenkarz jest prawdziwym <w>bogiem</w>.",
                            "target_lang_translations": {
                                "ru": "Для многих подростков этот певец является настоящим <w>богом</w>."
                            }
                        },
                        {
                            "text": "Po wygranym meczu czuł się jak młody <w>bóg</w>.",
                            "target_lang_translations": {
                                "ru": "После выигранного матча он чувствовал себя как молодой <w>бог</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "idol",
                        "bożyszcze"
                    ],
                    "common_phrases": [
                        "czuć się jak młody bóg",
                        "wyglądać jak młody bóg"
                    ],
                    "target_lang_definitions": {
                        "ru": "Человек, которым чрезвычайно восхищаются, которого обожают и почитают другие люди"
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "бог",
                                "target_lang_sense_clarification": "кумир, идол"
                            }
                        ]
                    }
                }
            ]

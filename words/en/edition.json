    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "0f125e26-3417-343c-9aca-47935d9dfa9b",
                    "sense_definition": "A specific version or form in which a book, newspaper, or other text is published.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "publication_version",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "I decided to buy the paperback <w>edition</w> because it was much lighter to carry.",
                            "target_lang_translations": {
                                "ru": "Я решил купить <w>издание</w> в мягкой обложке, потому что его было гораздо легче носить."
                            }
                        },
                        {
                            "text": "The teacher told us to study the second <w>edition</w> of the textbook for the exam.",
                            "target_lang_translations": {
                                "ru": "Учитель сказал нам изучить второе <w>издание</w> учебника к экзамену."
                            }
                        }
                    ],
                    "synonyms": [
                        "version",
                        "issue"
                    ],
                    "common_phrases": [
                        "paperback edition",
                        "revised edition",
                        "digital edition",
                        "special edition"
                    ],
                    "target_lang_definitions": {
                        "ru": "Конкретная версия или форма, в которой публикуется книга, газета или другой текст."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "издание",
                                "target_lang_sense_clarification": "форма публикации"
                            },
                            {
                                "target_lang_word": "версия",
                                "target_lang_sense_clarification": "вариант исполнения"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ca449dc7-6d39-310d-8601-671d2f834aa1",
                    "sense_definition": "The total set of copies of a book or newspaper printed at the same time.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "publication_version",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "The first <w>edition</w> of the novel sold out within a few days.",
                            "target_lang_translations": {
                                "ru": "Первое <w>издание</w> романа было распродано за несколько дней."
                            }
                        },
                        {
                            "text": "Collectors are often willing to pay a high price for a rare <w>edition</w>.",
                            "target_lang_translations": {
                                "ru": "Коллекционеры часто готовы заплатить высокую цену за редкое <w>издание</w>."
                            }
                        }
                    ],
                    "synonyms": [
                        "printing",
                        "print run"
                    ],
                    "common_phrases": [
                        "first edition",
                        "limited edition",
                        "rare edition"
                    ],
                    "target_lang_definitions": {
                        "ru": "Совокупность всех экземпляров книги или газеты, отпечатанных одновременно."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "издание",
                                "target_lang_sense_clarification": "выпуск книги"
                            },
                            {
                                "target_lang_word": "тираж",
                                "target_lang_sense_clarification": "количество экземпляров"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "78a545af-285e-34a7-a948-829c7132d559",
                    "sense_definition": "A particular occurrence of a regular event, competition, or program.",
                    "learner_level": "B2",
                    "frequency": "Middle",
                    "semantic_group_id": "event_instance",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "This year's <w>edition</w> of the music festival attracted thousands of visitors.",
                            "target_lang_translations": {
                                "ru": "Музыкальный <w>фестиваль</w> этого года привлек тысячи посетителей."
                            }
                        },
                        {
                            "text": "The next <w>edition</w> of the tournament will take place in France.",
                            "target_lang_translations": {
                                "ru": "Следующий <w>розыгрыш</w> турнира состоится во Франции."
                            }
                        }
                    ],
                    "synonyms": [
                        "installment"
                    ],
                    "common_phrases": [
                        "annual edition",
                        "latest edition",
                        "current edition"
                    ],
                    "target_lang_definitions": {
                        "ru": "Отдельный случай проведения регулярного мероприятия, соревнования или программы."
                    },
                    "translations": {
                        "ru": [
                            {
                                "target_lang_word": "выпуск",
                                "target_lang_sense_clarification": "программы, шоу"
                            },
                            {
                                "target_lang_word": "розыгрыш",
                                "target_lang_sense_clarification": "спортивного турнира"
                            }
                        ]
                    }
                }
            ]
        }
    ]
}
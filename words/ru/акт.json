    "entries": [
        {
            "pos": "noun",
            "senses": [
                {
                    "sense_id": "f07fe4ee-b00d-37b9-b19b-fd5cc29d0941",
                    "sense_definition": "Единичное проявление какой-либо деятельности; отдельное действие или поступок.",
                    "learner_level": "B1",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Террористический <акт> вызвал панику среди населения.",
                            "target_lang_translations": {
                                "en": "The terrorist <act> caused panic among the population."
                            }
                        },
                        {
                            "text": "Это был настоящий <акт> милосердия по отношению к бездомным животным.",
                            "target_lang_translations": {
                                "en": "It was a true <act> of mercy towards the homeless animals."
                            }
                        }
                    ],
                    "synonyms": [
                        "действие",
                        "поступок"
                    ],
                    "common_phrases": [
                        "террористический акт",
                        "половой акт",
                        "акт доброй воли",
                        "акт агрессии"
                    ],
                    "target_lang_definitions": {
                        "en": "A single manifestation of an activity; a separate action or deed."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "act",
                                "target_lang_sense_clarification": "general action"
                            },
                            {
                                "target_lang_word": "deed",
                                "target_lang_sense_clarification": "action performed intentionally"
                            },
                            {
                                "target_lang_word": "action",
                                "target_lang_sense_clarification": "process of doing something"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "ff330ca5-abfb-333b-b516-37bed06cfd7d",
                    "sense_definition": "Отдельная законченная часть драматического произведения, театрального представления, оперы или балета.",
                    "learner_level": "B1",
                    "frequency": "Middle",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Зрители вышли в буфет после первого <акта>.",
                            "target_lang_translations": {
                                "en": "The audience went to the buffet after the first <act>."
                            }
                        },
                        {
                            "text": "В последнем <акте> пьесы раскрывается главная тайна сюжета.",
                            "target_lang_translations": {
                                "en": "The main secret of the plot is revealed in the last <act> of the play."
                            }
                        }
                    ],
                    "synonyms": [
                        "действие"
                    ],
                    "common_phrases": [
                        "первый акт",
                        "в последнем акте",
                        "пьеса в двух актах"
                    ],
                    "target_lang_definitions": {
                        "en": "A separate, completed part of a dramatic work, theatrical performance, opera, or ballet."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "act",
                                "target_lang_sense_clarification": "theater/drama"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "5fd611a4-a8d8-3451-ab55-8f57f18de8f2",
                    "sense_definition": "Официальный документ, имеющий юридическую силу, удостоверяющий что-либо или подтверждающий установленные факты.",
                    "learner_level": "B2",
                    "frequency": "High",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Комиссия подписала <акт> приема-передачи нового оборудования.",
                            "target_lang_translations": {
                                "en": "The commission signed the acceptance <certificate> for the new equipment."
                            }
                        },
                        {
                            "text": "Новый законодательный <акт> регулирует правила дорожного движения.",
                            "target_lang_translations": {
                                "en": "A new legislative <act> regulates traffic rules."
                            }
                        }
                    ],
                    "synonyms": [
                        "документ",
                        "постановление"
                    ],
                    "common_phrases": [
                        "законодательный акт",
                        "составить акт",
                        "нормативный акт",
                        "акт приема-передачи"
                    ],
                    "target_lang_definitions": {
                        "en": "An official document with legal force, certifying something or confirming established facts."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "act",
                                "target_lang_sense_clarification": "legislative/legal"
                            },
                            {
                                "target_lang_word": "certificate",
                                "target_lang_sense_clarification": "official record/verification"
                            },
                            {
                                "target_lang_word": "deed",
                                "target_lang_sense_clarification": "legal instrument"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "56fef94f-79ac-3f55-82a3-b5ca6d44c402",
                    "sense_definition": "Изображение обнажённого человеческого тела в живописи, графике или фотографии.",
                    "learner_level": "C2",
                    "frequency": "Low",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "На выставке современного искусства был представлен смелый <акт>.",
                            "target_lang_translations": {
                                "en": "A bold <nude> was presented at the modern art exhibition."
                            }
                        },
                        {
                            "text": "Студенты художественной академии учились рисовать <акт> с натуры.",
                            "target_lang_translations": {
                                "en": "Students of the art academy learned to draw <nudes> from life."
                            }
                        }
                    ],
                    "target_lang_definitions": {
                        "en": "A depiction of a naked human body in painting, graphic art, or photography."
                    },
                    "translations": {
                        "en": [
                            {
                                "target_lang_word": "nude",
                                "target_lang_sense_clarification": "art"
                            }
                        ]
                    }
                },
                {
                    "sense_id": "077dd9d5-2621-357b-904c-9c819067e1cf",
                    "sense_definition": "Торжественное собрание в учебных заведениях по поводу выпуска, вручения наград или других важных событий.",
                    "learner_level": "C2",
                    "frequency": "VeryLow",
                    "semantic_group_id": "",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Профессора и студенты собрались в зале на торжественный <акт>.",
                            "target_lang_translations": {
                                "en": "Professors and students gathered in the hall for the solemn <ceremony>."
                            }
                        },
                        {
                            "text": "Лучшим выпускникам вручили золотые медали во время годового <акта>.",
                            "target_lang_translations": {
                                "en": "The best graduates were awarded gold medals during the annual <assembly>."
                            }
                        }
                    ],
                    "common_phrases": [

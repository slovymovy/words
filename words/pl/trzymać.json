                },
                {
                    "sense_id": "5789090d-8ac4-3cf7-8c9d-06abb78bf238",
                    "sense_definition": "Przechowywać coś w określonym miejscu.",
                    "learner_level": "A2",
                    "frequency": "High",
                    "semantic_group_id": "storage",
                    "name_type": "no",
                    "examples": [
                        {
                            "text": "Wszystkie ważne dokumenty <trzymam> w zamkniętej szufladzie biurka.",
                            "target_lang_translations": {
                                "ru": "Все важные документы я <держу> в закрытом ящике стола."
                            }
                        },
                        {
                            "text": "Mleko i masło należy <trzymać> w lodówce, żeby zachowały świeżość.",
                            "target_lang_translations": {
                                "ru": "Молоко и масло нужно <хранить> в холодильнике, чтобы они оставались свежими."
                            }
                        }
                    ],
                    "synonyms": [
                        "przechowywać",
                        "chować"
